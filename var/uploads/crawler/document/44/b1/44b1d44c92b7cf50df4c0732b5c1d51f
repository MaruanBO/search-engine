<script type="text/javascript">

    var mapX = -10768000;
    var mapY = 4602000;
    var mapZoom = 2;

    $(function(){
        makeMap(true, 'compact');
        var loc = getCookie("loc");
        log("cookied loc: "+loc);
        var data=null;
        if (loc && loc.length)
        {
            data = JSON.parse(loc);
            if (data && data.x && data.y && data.z)
            {
                mapX=data.x; mapY=data.y; mapZoom=data.z;
                showMapLocation(mapX, mapY, mapZoom);
            }
        }
        else
        {
            showMapLocation(mapX, mapY, mapZoom);
            if (navigator.geolocation)
                navigator.geolocation.getCurrentPosition(showBrowserMapPosition, showBrowserMapPosition);
        }
    });
</script>
