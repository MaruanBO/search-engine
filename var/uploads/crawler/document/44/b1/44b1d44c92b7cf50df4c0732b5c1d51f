<script>
var carousel = {};
carousel.pos = -1;
carousel.data = new Array();carousel.data[0] = {text:"Matthes Crest Traverse - Sout..."};carousel.data[1] = {text:"Indian Peaks"};carousel.data[2] = {text:"Big Thompson Canyon"};carousel.data[3] = {text:"Gordon's Hangover (5.9+)"};carousel.data[4] = {text:"Horseshoe Canyon Ranch"};carousel.data[5] = {text:"Indian Creek"};carousel.data[6] = {text:"Snowpatch Spire"};carousel.data[7] = {text:"IRS Wall"};

function carouselMove(newPos)
{
    if (isUndefined(newPos))
    {
        newPos = (carousel.pos+1)%8;
        carousel.timer = setTimeout(carouselMove, 6000); // not set manually, iterate
    }
    else
        clearTimeout(carousel.timer);
    if (newPos == carousel.pos)
        return;
    $("#carouselImg_"+carousel.pos).fadeOut(1000);
    $("#carouselSel_"+carousel.pos).animate({"background-color":"transparent"}, 500);
    carousel.pos = newPos;
    $("#carouselImg_"+newPos).fadeIn(1000);
    $("#carouselSel_"+newPos).animate({"background-color":"#ccc"}, 500);
    $("#carouselPhotoText").fadeOut('500',
        function(){ $(this).html(carousel.data[newPos].text); }).fadeIn('500');

    var nextPos = (newPos+1)%8;
    $("#carouselImg_"+nextPos).attr("src", carousel.data[nextPos].img);
}
carouselMove();
</script>
