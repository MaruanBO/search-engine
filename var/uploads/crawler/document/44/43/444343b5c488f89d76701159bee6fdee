function showQuickView(pid)
{
	var pUrl = app.util.appendParamToURL("/on/demandware.store/Sites-HB-Site/default/Product-Show", "pid", pid);
	app.quickView.show({
		url: pUrl, 
		source: "cart",
		label: "Add to Bag"}
	 );
}	

jQuery(window).bind('scroll', function() {
    if (jQuery(window).scrollTop() > 178) {
    	jQuery('.mainnavigation, #minicart').addClass('fixed');
    }
    else {
    	jQuery('.mainnavigation, #minicart').removeClass('fixed');
    }
});
var Divs = jQuery(".imagecol");
Divs.each(function(i) {
      if (jQuery(this).find("img").length) {
            jQuery(this).parent(".dd-wrapper").css('width','374px');
      } else {
            jQuery(this).css("height","0");
            
     }
});

</script>	
