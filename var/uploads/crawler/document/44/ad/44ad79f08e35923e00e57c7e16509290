						    se.onload = function() {
						      checkChatOnlineStatus();
						    };
						  }
						  se.src = '//commondatastorage.googleapis.com/code.snapengage.com/js/173b3283-4b00-438a-8354-4c9ec65368be.js';
						  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(se, s);
						})();
						function checkChatOnlineStatus() {
						  SnapEngage.getAgentStatusAsync(function(online) {
						    if (online) {
						      jQuery('a.snap-engage-link .snap-engage-btn-copy').text("Questions? Chat Now");
						    }
						  });
						}
						function launchChat() {
						  SnapEngage.startLink();
						  SnapEngage.hideButton();
						  return false;
						}
            jQuery(document).ready(function(){
                jQuery('.snap-engage-link').click(function(e){
                    e.preventDefault();
                    SnapEngage.startLink();
                    SnapEngage.hideButton();
                });
            });
					</script>
					<!-- end SnapEngage code -->
		<h1>Predictive Intelligence</h1>
		<a class="btn btn-default rad trackable" data-action="cta_button_click" data-category="CTA" data-label="hero-rad-product-predictive-intelligence" href="http://www.exacttarget.com/request-a-demo">Request a Demo <span class="icon icon-arrow-right-filled"></span></a>
<a class="snap-engage-link btn btn-default hidden-xs trackable" href="http://www.exacttarget.com/" data-category="CTA" data-action="cta_button_click" data-label="hero-snapengage-product-predictive-intelligence"><span class="snap-engage-btn-copy">Contact Us</span> <span class="icon icon-arrow-right-filled"></span></a>
<a class="btn btn-default visible-xs hero-call-now trackable" href="tel:8663624538" data-category="CTA" data-action="cta_button_click" data-label="hero-callnow-product-email">Call Now <span class="icon icon-arrow-right-filled"></span></a>
</div>
	<div class="col-sm-6 hidden-xs">&nbsp;</div>
</div>
<p>&nbsp;</p>
 </div></div></div></div>
        </li>
          </ol>
