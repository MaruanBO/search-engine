<script type="text/javascript">
	$(function () {
		$("#ctl00_HeaderSearchForm_SearchTerm").blur(function () {
			if($("#ctl00_HeaderSearchForm_SearchTerm").val() === "") $("#ctl00_HeaderSearchForm_SearchTerm").val("Search Keyword or Item #");
		});
		$("#ctl00_HeaderSearchForm_SearchTerm").focus(function () {
			if ($("#ctl00_HeaderSearchForm_SearchTerm").val() === "Search Keyword or Item #") {
				$("#ctl00_HeaderSearchForm_SearchTerm").val("");
			}
		});
		$("#ctl00_HeaderSearchForm_SearchTerm").val("Search Keyword or Item #");

		$("#ctl00_HeaderSearchForm_SearchTerm").keypress(function (e) {
			if (e.which === 13) {
				e.preventDefault();
				$("#ctl00_HeaderSearchForm_HeaderSearchFormSubmit").click();
			}
		});
	});
</script>
