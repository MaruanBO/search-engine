<div id="native-ad" class="native-ad native-article-ad group">
<script type="text/javascript">
(function NativeAd($) {

    var AD_POSITION = 4;

    var adZone;
    var ord;
    var adExitsts;
    var $nativeArticleAd;
    var $sponsoredContent;
    var $adLocation;

    function init() {
        adZone = $('meta[name="DCSext.DartZone"]').attr('content');
        ord = window.ord || Math.floor(Math.random() * 1e16);

         adCall();
    }

    function adCall() {
        var item;
        var adsTest = '';
        var searchExists = window.location.search != '';
        var search = searchExists && window.location.search.split('?')[1];
        var params = search && search.split('&');

        if (params) {
            for (var i = 0, len = params.length; i < len; i++) {

                item = params[i].split('=');

                if(item[0] == 'adstest') {
                    adsTest = item[1];
                }
            }
        }

        document.write('<script type="text/javascript" src="http://ad.doubleclick.net/N4735792/adj/' + adZone + ';type=bi_content;sz=1x1;adstest=' + adsTest + ';ord=' + ord + '?"><\/script>');
    }

    function insertAd() {
        setUpAd();
        showAd();
    }

    function setUpAd() {
        $nativeArticleAd        = $('.native-article-ad');
        $sponsoredContent = $nativeArticleAd.find('.sponsored-article');
        $adLocation                = $($('#moreSectionNews').find('span[name=trackingEnabledModule]')[AD_POSITION - 1]);

        adExists = $sponsoredContent.length > 0 ? true : false;
    }

    function showAd() {
        if (adExists) {
            $sponsoredContent.insertAfter($adLocation);
        }
    }

    init();
    $(document).ready(insertAd);

}(jQuery));
</script>
