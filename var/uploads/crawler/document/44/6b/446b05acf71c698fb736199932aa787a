var ord = window.ord || Math.floor(Math.random() * 1e16);
document.write('<script type="text/javascript" src="http://ad.doubleclick.net/N4735792/adj/us.reuters/home;sz=1x1;type=bi_video;ord=' + ord + '?"><\/script>');
