<script type="text/javascript">
$(document).ready(function(){
		$('#carousel').flexslider({
			animation : "slide",
			controlNav : false,
			animationLoop : false,
			slideshow : false,
			itemWidth : 100,
			itemMargin : 1,
			asNavFor : '#slider'
		});

	$('#slider').flexslider({
	     animation : "slide",
	     animationLoop : true,
	     slideshow : true,
	     slideshowSpeed: 6000,
	     controlNav : true,
	     start : function(slider) {
	      $('body').removeClass('loading');
	     }
	    });
		$('#video_list').flexslider({
			animation : "slide",
			controlNav : false,
			animationLoop : false,
			slideshow : false,
			itemWidth : 80,
			itemMargin : 1,
			asNavFor : '#video_list_gallery'
		});
		$('#video_list_gallery').flexslider({
			animation : "slide",
			animationLoop : false,
			slideshow : false,
			sync : "#video_list",
			controlNav : false,
			start : function(slides1) {
				$('body').removeClass('loading');
			}
		});

	}); 
</script>


<script type="text/javascript">

$(function() {
    $(".radio_polls input").click(function() {
     	$(".radio_polls span").removeClass("active");
     	$(this).parent().addClass("active");
    });
});


	$(document).on('click','.vote_button',function(event){
		event.preventDefault();
		if($('.poll_block input[type="radio"]').is(':checked')){
			$.ajax({
				url:'/wps/pages/vote/'+$('#poll_hidden_id').attr('value')+'/'+$('#poll_hidden_title').attr('value')+'/nation_news',
				type:'POST',
				data:'poll_options_id='+$('.poll_type input[name="poll_options_id"]:checked').attr('value'),
				success:function(data){
					$('.poll_content').html(data);
					$('.vote_button').attr('disabled','disabled')
					$('.vote_button').attr('class','already_voted')
				}
			});
		}
	});

</script>
<noscript>
</noscript>
<script type="text/javascript">
$(".other-contact-dialog").click(function () {
$("#dialog").dialog({
        autoOpen: false
    });
$("#dialog").dialog('open');
});
 </script>


