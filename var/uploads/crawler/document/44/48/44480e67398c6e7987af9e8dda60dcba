  function _logEvent(category, action, label) {
    ga('send', 'event', category, action, label);
  }

  function logSocialOnly(action, label) {
    _logEvent('social', action, label);
    return true; /*allow hyperlink to complete standard behavior*/
  }

  function _logExternalLink(url, options, category, action, label) {
    _logEvent(category, action, label);
    window.open(url, '_blank', options);
    return false;
  }

  function logExternalLink(url, options, action, label) {
    var opt = options ? options : 'height=750,width=1150,resizable=1,toolbar=0,scrollbars=1';
    return _logExternalLink(url, opt, 'Outbound Link', action, label);
  }


  function logSocialLink(url, width, height, action, label) {
    var options = 'menubar=no,toolbar=no,resizable=yes,scrollbars=no,width=' + width + ',height=' + height + ',left=' + (screen.width/2 - width/2) + ',top=' + (screen.height/3 - height/2);
    return _logExternalLink(url, options, 'social', action, label);
  }

  function logTwitterShare(url, label) {
    return logSocialLink(url, 550, 420, 'twitter', label);
  }

  function logFacebookShare(url, label) {
    return logSocialLink(url, 650, 400, 'facebook', label);
  }

  function logGooglePlusShare(url, label) {
    return logSocialLink(url, 680, 480, 'google-plus', label);
  }

  function logPinterestShare(url, label) {
    return logSocialLink(url, 784, 314, 'pinterest', label);
  }

  function logLinkedInShare(url, label) {
    return logSocialLink(url, 850, 460, 'linkedIn', label);
  }

  function logEmailShare(url, label){
    ga('send', 'event', 'social', 'email', label);
//    var pTracker=_gat._getTrackerByName();
//    pTracker._trackEvent("social", "email", label);
    setTimeout(function(){window.location.href = url}, 500);/*todo determine work around for mailto: links cancelling ga requests*/
    return false;
  }


  


