            <div id="posts">
              <script type="text/javascript">
                var twitter_feed, tumblr_feed;
                $.when(
                  $.ajax({
                    type: "GET",
                    url: "http://admin.api.140proof.com/api/v1/ofpsocialfeed/twitter_feed.json",
                    //- url: "http://admin.localhost:3000/api/v1/ofpsocialfeed/twitter_feed.json",
                    data: twitter_feed,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) { twitter_feed = result; },
                    error: function (result) { }
                  }),
                  $.ajax({
                    type: "GET",
                    url: "http://admin.api.140proof.com/api/v1/ofpsocialfeed/tumblr_feed.json",
                    //- url: "http://admin.localhost:3000/api/v1/ofpsocialfeed/tumblr_feed.json",
                    data: tumblr_feed,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) {  tumblr_feed = result; },
                    error: function (result) { }
                  })).done(function(){
                    $("div.tweet > article > p").each( function (index){
                    //- get the text and replace it with what's returned in the ajax call
                    var text = twitter_feed["data"][index].text
                    var tweet_node = $(this);
                    var newText = tweet_node.text().replace("%%tweet_body%%", text+ " ");
                    tweet_node.text(newText);
                    //- get the urls we need to replace
                    
                    var urls = twitter_feed["data"][index]["entities"]["urls"];
                    
                    for(var i = 0; i < urls.length; i++){
                      var html = tweet_node.html();
                      var html_new = html.replace(urls[i]["url"], "<a href='" + urls[i]["expanded_url"] + "'> "+ urls[i]["display_url"]+ "</a> ");
                      //- var html_new = html.replace(urls[i]["url"], "<a href='" + urls[i]["expanded_url"] + "'> "+ urls[i]["expanded_url"]+ "</a> ");
                      tweet_node.html(html_new);
                    }
                    html = tweet_node.html();
                    var newhtml = "<em> via <a href='http://twitter.com/140proof/status/" + twitter_feed["data"][index]["id_str"] + "'> @140proof</a> ";
                    tweet_node.html(html + newhtml);
                  });
                  //- console.log(tumblr_feed);
                  $("li.tweet").css("visibility", "visible");
                  
                  $("#posts > ul > li.blog").each( function (index){
                    var tumblr_obj = tumblr_feed["data"][index];
                    var body = tumblr_obj["body"];
                    body = body.split("\n");
                    var imageRegex = /([^\s"]+(?=\.(jpg|gif|png))\.\2)/g;
                    var image_url = body[0].match(imageRegex)[0];
                    //- console.log(image_url);
                    var image_html = "<a href='" + tumblr_obj["post_url"] + "'> <img src='" + image_url + "' width ='100%'></a>";
                    var tumblr_node = $(this);
                    var title_html = "<h3><a href='"+tumblr_obj["post_url"] + "'>" + tumblr_obj["title"] + "</a></h3>"
                    var read_more = "<a class='read_more' href='" + tumblr_obj["post_url"] + "'> <p style='text-align:center; background-color:#f0f0f0; padding: 10px; border-radius: 3px;'> Read More </p> </a>";
                    var body_text = "<article>" + body[1] + body[2] + "</article>";
                    tumblr_node.html(image_html+title_html + body_text + read_more);
                  });
                  
                  $("li.blog").css("visibility", "visible");
                });
                
                
                
              </script>
