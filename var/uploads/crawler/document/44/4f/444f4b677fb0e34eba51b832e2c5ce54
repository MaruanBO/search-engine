            <script type="text/javascript">
              $(document).ready(function() {
                function onSuccessSend(data, status){
                  $("#contact_us_btn").text("Sent");
                }
                
                function onErrorSend(data, status){
                  //- console.log("error");
                }
                
                
                $("#contact_us_btn").unbind('click').bind('click', function(e){
                  e.preventDefault();
                  e.stopPropagation();
                  
                  //- console.log(e);
                  //- console.log("hi");
                  //- url: "http://admin.localhost:3000/api/v1/contact_us/send_message.json",
                  var formData = $("#contact_form").serialize();
                  
                  
                  var email = $(':input#email', $("form#contact_form"));
                  var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
                  var is_email_valid=re.test(email.val());
                  
                  var name = $(':input#name', $("form#contact_form"));
                  
                  var is_name_valid = !(name.val() == "Name");
                  if(is_email_valid && is_name_valid){
                    email.removeClass("contact_us_error");
                    name.removeClass("contact_us_error");
                    $.ajax({
                            type: "POST",
                            url: "http://admin.api.140proof.com/api/v1/contact_us/send_message.json",
                            cache: false,
                            data: formData,
                            success: onSuccessSend,
                            error: onErrorSend
                    });
                          return false;
                          
                  }else{
                    if(!is_email_valid){
                      email.addClass("contact_us_error");
                    }
                    if(!is_name_valid){
                      name.addClass("contact_us_error");
                    }
                    return false;
                  }
                });
              });
              
            </script>
