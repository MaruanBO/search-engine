		<script type="text/javascript">
		function get_object(id)
		{
			var object = null;

			if( document.layers )
			{
				object = document.layers[id];
			}
			else if( document.all )
			{
				object = document.all[id];
			}
			else if( document.getElementById )
			{
				object = document.getElementById(id);
			}
			return object;
		}
		function mktsumTab(tab)
		{
			var td = get_object('mktsum-default-'+tab);
			sCQ(td);

			var e = get_object('mktsum-'+tab);
			var l = get_object('mktsum-'+curMktSumTab);
			var t = get_object('mktsum-tbl-'+tab);
			var lt = get_object('mktsum-tbl-'+curMktSumTab);

			l.className = '';
			lt.style.display = 'none';
			e.className = 'active';
			t.style.display = 'block';
			curMktSumTab = tab;
		}
		function mktsumInfo(type)
		{
			var x = get_object('mktsuminfo');
			var t = get_object('mktsum-tbl-'+curMktSumTab);

			if(type==1)
			{
				t.style.display = 'none'
				x.style.display = 'block';
			}
			else
			{
				x.style.display = 'none';
				t.style.display = 'block'
			}
		}

		var qlink = 'qkquote';
		var clink = 'qkchart';

		function chartQuote(s, n, l, p, c, pc, d)
		{
			curMktSumSym = s;
			var x = get_object('cq').className = d;
			x = get_object('cq_title');

			if(curMktSumTab=='COMMODITIES')
				x.innerHTML = l;
			else
				x.innerHTML = n;

			x.title = l;
			x.href = 'p.php?pid='+qlink+'&symbol='+s;
			x = get_object('cq_a_chart');
			x.title = l+' chart';
			x.href = 'p.php?pid='+clink+'&symbol='+s;
			var oldimg = get_object('cq_chart');
			var img = oldimg.cloneNode(true);
			img.src = 'p.php?pid=staticchart&s='+s+'&p=0&t=50';
			img.title = img.alt = x.title = l + ' chart';
			x.replaceChild(img, oldimg);
			get_object('cq_p').innerHTML = p;
			x = get_object('cq_pc').innerHTML = c + ' ('+pc+') <img alt="'+d+'" src="http://images.advfn.com/imagesnew/2/us/'+d+'.gif"/>';
		}
		function sCQ(e)
		{
			if(curMktSumSym != e.getAttribute('sym'))
			{
				mktsumTmr = setTimeout( "chartQuote('"+e.getAttribute('sym')+"', '"+e.getAttribute('name')+"', '"+e.getAttribute('longname')+"', '"+e.getAttribute('price')+"', '"+e.getAttribute('price_change')+"', '"+e.getAttribute('percent_change')+"', '"+e.getAttribute('dir')+"')", 500);
			}
		}
		function ccQ()
		{
			clearTimeout(mktsumTmr);
		}

		var mktsumTmr;
		var curMktSumSym;
		var curMktSumTab = 'ASIA-PACIFIC';
		</script>


