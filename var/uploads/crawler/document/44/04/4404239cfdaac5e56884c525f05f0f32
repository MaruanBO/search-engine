  $(document).ready(function(){
    $(".tabs-area").weTabs({
      contentSelector:'.product-tab',
      tabSelector:'.tabset a',
      onInit:function(){
        $("a.close-demo").trigger("click");
      }
    });

    $(".quotations").weTabs({
      contentSelector:'.blockquote',
      tabSelector:'.switcher-dots a'
    });

    $('a.show-product-demo').bind('click', function(){
      $('.demo-window-container').remove();
      if($(this).hasClass("close-demo")){
        $(this).html($(this).attr("curr-text")).removeAttr("curr-text").removeClass("close-demo").removeClass("red").addClass("blue");
      }else{
        $(this).attr("curr-text", $(this).html()).html('&times; Close').addClass("close-demo").removeClass("blue").addClass("red");
        var selectedTab = this;
        var url = $(selectedTab).attr('href');
        var tabContentDiv = $(selectedTab).parents('.demo-content').get(0);
        $('<div></div>').css({
          position:'absolute',
          bottom:0,
          right:0,
          width:600,
          height:'100%'
        }).addClass('demo-window-container').appendTo(tabContentDiv).append($('<iframe>', {
          src: url,
          frameborder: 0,
          scrolling:'no',
          height:'100%',
          width:'100%',
          backgroundColor: 'transparent',
          allowTransparency:'true'
        }));
      }
      return false;
    });
  });
</script>
    </div>

