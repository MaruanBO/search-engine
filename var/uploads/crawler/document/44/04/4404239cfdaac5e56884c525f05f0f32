       <script type="text/javascript">
       	var ALERT = function() {
			var ALERT_BOX_SELECTOR = ".we-alert";
			var ALERT_BOX_CLOSE_SELECTOR = ".we-alert-close";
			var ALERT_SLIDE_SPEED = 1000;
			var ALERT_WAIT_PERIOD = 2000;
	
			/**********************/
			var init = function() {
				$(ALERT_BOX_CLOSE_SELECTOR).click(function() {
					$(this).parents(ALERT_BOX_SELECTOR).slideUp(ALERT_SLIDE_SPEED ,function(){
						$(ALERT_BOX_SELECTOR).hide();
						$(ALERT_BOX_SELECTOR).remove();
					});
				});
	
				$(ALERT_BOX_SELECTOR).slideDown(ALERT_SLIDE_SPEED , function(){
					if($(ALERT_BOX_SELECTOR).length > 0) {
						setTimeout(function() {
							$(ALERT_BOX_SELECTOR).slideUp(ALERT_SLIDE_SPEED ,function(){
								$(ALERT_BOX_SELECTOR).hide();
								$(ALERT_BOX_SELECTOR).remove();
							});
						}, ALERT_WAIT_PERIOD);
					}
				});
			};
	
			/**********************/
			return {
				init: init
			};
		}();
		ALERT.init();
       </script>

      

