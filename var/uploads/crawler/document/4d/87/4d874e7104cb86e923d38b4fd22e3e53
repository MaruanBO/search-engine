<div id="qvpop"></div>
<script>
var t1;
var t2;
$(".qvitem").mouseenter(function(){
clearTimeout(t2);
cell = $(this);
t1=setTimeout(function(){showquickview(cell)},1000);
}).mouseleave(function(){
clearTimeout(t1);
t2=setTimeout(function(){$("#qvpop").hide()},250);
});	
$("#qvpop").mouseenter(function(){
clearTimeout(t1);
clearTimeout(t2);
}).mouseleave(function(){
t2=setTimeout(function(){$("#qvpop").hide()},250);
});
$("#qvimage, #qvSkuCopy").live("click",function(){
location.href=$(this).parent().parent().parent().find("#qvsku").attr("qvhref");
ga('send', 'event', { eventCategory:'Quickview', eventAction: 'Click', eventLabel: $('h1[itemprop="name"]').text() + " - " +sku });
});	
$("#qvheaderclose").live("click",function(){
$("#qvpop").hide();
ga('send', 'event', { eventCategory:'Quickview', eventAction: 'Close', eventLabel: $('h1[itemprop="name"]').text() + " - " +sku });
});
function showquickview(obj){
cell = obj;
sku = cell.attr("sku");
if (right(sku,1) == "d"){
return false;
}
cellImg = cell.find("img").offset();
$.post("/includes/quickview/detail.cfm",{"skunum": sku},function(r){
$("#qvpop").html(r);	
$("#qvpop").show();	
$("#qvpop").offset({"top": cellImg.top-96, "left": cellImg.left+172});
if ($("#qvpop").offset().left+450>$(window).innerWidth()){
$("#qvpop").offset({"top": cellImg.top-96, "left": cellImg.left-494});
$("#qvarrow").removeClass("leftarrow");
$("#qvarrow").addClass("rightarrow");
}
ga('send', 'event', { eventCategory:'Quickview', eventAction: 'Pop', eventLabel: $('h1[itemprop="name"]').text() + " - " +sku });
});
}
