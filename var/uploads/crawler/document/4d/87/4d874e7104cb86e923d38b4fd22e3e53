<script>
$("#topbargsnum").keypress(function(e){
if(e.which==13){
$("#topbargsbutton").click();
}
});
$("#topbargsbutton").click(function(){
var button = $(this);
logevent("Event48");
$.post("/includes/goodsam/_validate.cfm",{gsnum:$('#topbargsnum').val()},
function(r){
if ($.trim(r)=="true"){
$.post("/includes/goodsam/member.cfm",
function(r){
button.parent().parent().html(r);
});
}
else{
alert("Unable to confirm membership at this time. Please try again later or call 1-888-626-7576.");
}
});
});
$("#topbargsjoin").on("click",function(){
logevent("Event48");
location.href="/goodsamclub/join/";
});
$("#topbargsrenew").on("click",function(){
logevent("Event50");
location.href="/goodsamclub/renew/";
});
function logevent(whichEvent){
$.ajax({
type: "POST",
url: "/includes/header/topbar/event.cfm",
data: {theEvent:whichEvent},
async: false,
success: function(r){
$("#topbargsevent").html(r);	
}
});	
}
</script>
