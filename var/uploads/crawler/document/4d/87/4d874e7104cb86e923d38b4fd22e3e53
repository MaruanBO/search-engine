<script>
(function($) {
$.fn.onVisible = function (callback) {
var self = this;
var selector = this.selector;
var timer;
if (self.is(":visible")) {
callback.call(self);
clearInterval(timer);
} else {
timer = setInterval(function() {
if ($(selector).is(":visible")) {
callback.call($(selector));
clearInterval(timer);
}
}, 50);
}
}	
}(jQuery));
</script>	
