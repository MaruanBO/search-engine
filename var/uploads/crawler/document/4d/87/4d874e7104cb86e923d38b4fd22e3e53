<script>
function loadWelcomeNew() {
var myleft = ($(window).width() - $('.welcomePopupNew').outerWidth()) / 2;
$.blockUI({
message: $('.welcomePopupNew'),
css: {
top: '25%',
width: '400px',
left: myleft + 'px',
cursor: 'default !important'
}
});
}
$(document).on('click', '.blockOverlay', function(ev) {
$.unblockUI();
ga('send', 'event', { eventCategory:'Newsletter', eventAction: 'Popup 10', eventLabel: 'Block Out'});
});
$(".welcomePopupNew > #emaildiagtext").click(function() {
if ($(this).val() == "Please enter your email address") {
$(this).val("");
}
}).keydown(function() {
if ($(this).val() == "Please enter your email address") {
$(this).val("");
}
});
$('.submitButton').click(function() {
$('.welcomePopupNew > #floatingCirclesG').show();
$.post("/includes/emaildiag/submit.cfm", {
email: $('.welcomePopupNew > #emaildiagtext').val()
}, function(r) {
var _r = $.trim(r);
$('.welcomePopupNew > #emaildiagerrs').text(_r);
//set the omniture event when submit gets clicked
$.post("/includes/emaildiag/gsevent.cfm", function(eventdata) {
$(".welcomePopupNew").append(eventdata);
});
if (_r.indexOf('error') < 0) {
$('.welcomePopupNew > #floatingCirclesG').hide();
SignMeUpNew();
setTimeout('$.unblockUI()', 4000);
}
});
});
$(document).keyup(function(e) {
if (e.keyCode == 27) {
$.unblockUI();
}
});
$('.welcomePopupNew > #emaildiagtext').keypress(function(e) {
if (e.which == 13) {
$('.welcomePopupNew > .submitButton').click();
}
});
</script>
<script>
function SignMeUpNew() {
ga('send', 'event', { eventCategory:'Newsletter', eventAction: 'Popup 10', eventLabel: 'Sign Up'});
}	
function NoThanksNew() {
ga('send', 'event', { eventCategory:'Newsletter', eventAction: 'Popup 10', eventLabel: 'No Thanks'});
}
function XoutNew() {
ga('send', 'event', { eventCategory:'Newsletter', eventAction: 'Popup 10', eventLabel: 'XOut'});
}
</script>
