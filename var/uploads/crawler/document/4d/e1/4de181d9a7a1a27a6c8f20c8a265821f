    <script type="text/javascript">
function Rotators(GUID, Title, Teaser, Image, Link, Alt)
{
this.GUID = GUID;
this.Title = Title;
this.Teaser = Teaser;
this.Image = Image;
this.Link = Link;
this.Alt = Alt;
}
var Rotator = new Array();
Rotator[0] = new Rotators("{0a82765c-588b-445c-8c5c-e49fd6c0c116}", "DIY Drying Rack for Pasta, Herbs and More", "Use your noodle to craft this collapsible rack that\u2019s perfect for drying homemade pasta, freshly cut herbs, dishes on a crowded counter, and so much more.", "/~/media/Images/MEN/Editorial/Articles/Magazine%20Articles/2015/02-01/DIY%20Drying%20Rack%20for%20Pasta%20Herbs%20and%20More/DIY-Drying-Rack-For-Pasta-H%20jpg.jpg", "/diy/diy-drying-rack-zm0z15fmzmar.aspx", "");
Rotator[1] = new Rotators("{c1d53314-950a-4f42-84bf-618670bca2cf}", "Homemade Bath Products: Shampoo, Deodorant and Toothpaste", "Save cash and shirk chemicals by whipping up your own toiletries from naturally effective ingredients.", "/~/media/Images/MEN/Editorial/Articles/Magazine%20Articles/2014/12-01/Homemade%20Bath%20Products%20Shampoo%20Deodorant%20and%20Toothpaste/Homemade-Bath-Products-Sham%20jpg.jpg", "/natural-health/homemade-bath-products-zm0z14djzkin.aspx", "");
Rotator[2] = new Rotators("{0c15bdf9-fba0-4cce-9be4-41b4f4dc9184}", "How to Source Truly High-Quality Seeds", "Seed experts and plant breeders explain seed genetics, a complicated seed industry, and how to find seeds to make yours a successful, resilient garden.", "/~/media/Images/MEN/Editorial/Articles/Magazine%20Articles/2014/12-01/How%20to%20Source%20Truly%20High%20Quality%20Seeds/How-To-Source-Truly-High-Qu%20jpg.jpg", "/organic-gardening/high-quality-seeds-zm0z14djzsto.aspx", "");
Rotator[3] = new Rotators("{dec431b2-edc2-4547-9440-17468883a453}", "Fossil Fuel Divestment: A Growing Movement", "In the past year, a hopeful number of organizations, townships, companies and universities have divested funds they once held in the fossil fuel industry.", "/~/media/Images/MEN/Editorial/Articles/Magazine%20Articles/2014/12-01/Fossil%20Fuel%20Divestment%20A%20Growing%20Movement/Fossil-Fuel-Divestment-A-Gr%20jpg.jpg", "/renewable-energy/fossil-fuel-divestment-zmgz14djzsto.aspx", "");
Rotator[4] = new Rotators("{167f804f-9a8a-440d-9a29-043285137c38}", "Easy Meals: One-Pot Recipes for Every Season", "Want to serve up convenient, well-balanced dishes without making a mess of your kitchen? This collection of single-pot meals will get you cookin\u2019.", "/~/media/Images/MEN/Editorial/Special%20Projects/Collection%20Pages/Easy%20Meals%20One%20Pot%20Recipes%20for%20Every%20Season/Easy-Meals-One-Pot-Recipes-%20jpg.jpg", "/real-food/easy-meals-one-pot-recipes-zl0z14djzmar.aspx", "");
function OnButtonMove(buttonNumber, prevButtonNumber)
{ 
document.getElementById('Link').href = Rotator[buttonNumber].Link;
document.getElementById('Title').innerHTML = Rotator[buttonNumber].Title;
document.getElementById('Teaser').innerHTML = Rotator[buttonNumber].Teaser;
document.getElementById('Image').src = Rotator[buttonNumber].Image;
document.getElementById('Image').alt = Rotator[buttonNumber].Alt;
document.getElementById('Image').title = Rotator[buttonNumber].Alt;
document.getElementById('imgLink').href = Rotator[buttonNumber].Link;
document.getElementById('Item' + buttonNumber).style.fontWeight = 'bolder';
document.getElementById('Item' + prevButtonNumber).style.fontWeight = 'normal';
}
var Button = parseInt(0);
var Rotate = true;
onload=setTimeout('SwitchDisplay()',7000);
function SwitchDisplay()
{
if (Rotate) {
switch (Button)
{
case 0:
OnButtonMove(1, 0);
setTimeout('SwitchDisplay()',7000);
Button = 1; 
break;
case 1:
OnButtonMove(2, 1);
setTimeout('SwitchDisplay()',7000);
Button = 2;
break;
case 2:
OnButtonMove(3, 2);
setTimeout('SwitchDisplay()',7000);
Button = 3;
break;
case 3:
OnButtonMove(4, 3);
setTimeout('SwitchDisplay()',7000);
Button = 4;
break;
case 4:
OnButtonMove(0, 4);
setTimeout('SwitchDisplay()',7000);
Button = 0;
break;
} } else { clearTimeout()} 
}
document.getElementById('Link').href = Rotator[0].Link;
document.getElementById('Title').innerHTML = Rotator[0].Title;
document.getElementById('Teaser').innerHTML = Rotator[0].Teaser;
document.getElementById('Image').src = Rotator[0].Image;
document.getElementById('Image').Alt = Rotator[0].Alt;
document.getElementById('Image').Title = Rotator[0].Alt;
document.getElementById('imgLink').href = Rotator[0].Link;
function OnButtonClick(buttonNumber, prevButtonNumber)
{ 
document.getElementById('Link').href = Rotator[buttonNumber].Link;
document.getElementById('Title').innerHTML = Rotator[buttonNumber].Title;
document.getElementById('Teaser').innerHTML = Rotator[buttonNumber].Teaser;
document.getElementById('Image').src = Rotator[buttonNumber].Image;
document.getElementById('Image').Alt = Rotator[buttonNumber].Alt;
document.getElementById('Image').Title = Rotator[buttonNumber].Alt;
document.getElementById('imgLink').href = Rotator[buttonNumber].Link;
document.getElementById('Item' + buttonNumber).style.fontWeight = 'bolder';
for (var i = 0; i < 5; i++)
 { 
document.getElementById('Item' + i).style.fontWeight = 'normal';
} 
document.getElementById('Item' + buttonNumber).style.fontWeight = 'bolder';
Rotate = false;}
</script>

</div>



        </div>
