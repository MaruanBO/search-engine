<script type="text/javascript" language="javascript">
    function LoadPopularTab(tabId)
    {
        visitedArticles = document.getElementById('MostPopularVisited');
        visitedTab = document.getElementById('mostPopular0');
        visitedTab.className = "most-popular-tab-off";
        visitedArticles.style.display = "none";
        if (tabId == 0) {
            visitedTab.className = "most-popular-tab-on";
            visitedArticles.style.display = "";
        }
        emailedArticles = document.getElementById('MostPopularEmailed');
        emailedTab = document.getElementById('mostPopular1');
        emailedTab.className = "most-popular-tab-off";
        emailedArticles.style.display = "none";
        if (tabId == 1) {
            emailedTab.className = "most-popular-tab-on";
            emailedArticles.style.display = "";
        }
    }
    LoadPopularTab(0);
</script>

