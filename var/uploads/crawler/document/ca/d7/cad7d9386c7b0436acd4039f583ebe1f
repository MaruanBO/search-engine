            <script type="text/javascript">
                jQuery('document').ready(function(){
                    var $mhdiv = $("#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 #" + mso +"-f7be0eba-82a5-48a2-8e99-ea20d219dba5");
                    var $child = $mhdiv.find(".hero-panel");
                    for (var i = 0; i < $child.length; i += 4) {
                        var $div = $("<div class='mini-hero-fourSlide'/>");
                        $child.slice(i, i + 4).wrapAll($div);
                    }
                    var slides = jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-fourSlide');
                    if(slides.length === 1){
                        jQuery('#mini-carousel-controls-f7be0eba-82a5-48a2-8e99-ea20d219dba5').addClass('hidden');
                    }else{
                        jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-fourSlide:gt(0)').addClass('dn');
                    }
                    jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-panels').find('.mini-panel-stacked:gt(3)').removeClass('dn');
                    jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-panels').find('.mini-panel-grid:gt(3)').removeClass('dn');
                    jQuery('.mini-hero-fourSlide .hero-panel:last-child').find('.mini-panel-stacked').removeClass('mrxxx'); //Remove left-margin from every 4th panel
                    jQuery('.mini-hero-fourSlide').find('.mini-panel-grid:eq(2)').addClass('clearl');
                });
            </script>
        
    
    
    
    
        <script type="text/javascript">
            //Remove the hidden class revealing the carousel.
            document.getElementById(mso+'-f7be0eba-82a5-48a2-8e99-ea20d219dba5').className ="";
            document.getElementById('carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5').className ="";
            jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-panels').find('.mini-panel-stacked:eq(3)').removeClass('mrxxx');
            jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-panels').find('.mini-panel-stacked:gt(3)').addClass('dn');
            jQuery('#carousel-f7be0eba-82a5-48a2-8e99-ea20d219dba5 .mini-hero-panels').find('.mini-panel-grid:gt(3)').addClass('dn');
        </script>
        
    
