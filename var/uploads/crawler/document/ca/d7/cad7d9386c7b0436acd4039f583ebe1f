<script type="text/javascript">
		var removeMSO = mso == 'twcc'?'bhn':'twcc';
		//Remove the hidden or dn class from the proper layout
		var e = document.querySelectorAll('.mso-' + mso);
		var i = e.length;
			while(i--){
				e[i].className="mso-" + mso;
			}

	jQuery('document').ready(function(){
		//If panel not in this MSO remove it
		jQuery('.msomarket:contains('+ removeMSO +')').parents('.hero-panel').empty().remove();
		jQuery('.mso-' + removeMSO).empty().remove();
	});
</script>
 
			
			
			
			
		
	
	






