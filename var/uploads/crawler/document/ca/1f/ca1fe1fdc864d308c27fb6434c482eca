<script>

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

strGACampaign = getParameterByName("utm_campaign");
strGAMedium = getParameterByName("utm_medium");
strGASource = getParameterByName("utm_source");
GAStrAppend = "";
if(strGACampaign.length>0) {
	GAStrAppend = "?utm_campaign="+strGACampaign+"&utm_medium="+strGAMedium+"&utm_source="+strGASource;
}

// show DC homepage
var homepageCurMonth = 2;
var homepageCurDay = 19;

if (homepageCurMonth >= 1 && homepageCurMonth <= 4)
{
	document.write('<iframe src="/includes/join/digital_cookie-recruit_home_page.asp'+GAStrAppend+'" width="970" height="620" frameBorder="0" scrolling="no"></iframe>');

} else {

	//show IE8 version for secure SSL since edge doesn't work 
	if ((document.location.protocol == 'https:') || ($.browser.msie && parseFloat($.browser.version) <= 8.0) || ($.browser.safari && parseFloat($.browser.version) <= 525.29)) {
		document.write('<iframe src="/includes/join/blue_trefoil/homepage_animation/recruit_home_page-with-code-ie8.asp'+GAStrAppend+'" width="970" height="620" frameBorder="0" scrolling="no"></iframe>');
	} else {
		document.write('<iframe src="/includes/join/blue_trefoil/homepage_animation/recruit_home_page-with-code.asp'+GAStrAppend+'" width="970" height="620" frameBorder="0" scrolling="no"></iframe>');
	}

}

//also set the cookie so that if you hit homepage and get to sub pages, you don't see the slide down banner
$.cookie("floatingBannerSlide", 1, {expires: 0, path:'/'});		// expires: 0 = session

</script>

