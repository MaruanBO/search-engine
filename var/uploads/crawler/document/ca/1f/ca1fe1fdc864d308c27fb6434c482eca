<script>
$(function(){
	$("#UpdateSignUpp").validate({
		rules: {
			email: {
				required: true,
				email: true
			}
		},
		messages: {
			required: "",
			email: ""
		},
		submitHandler: function() {
			submit_email();
		},
		errorLabelContainer: ".InvalidEmailMessage",
		wrapper: ""
	});
	
});

function submit_email()
{
	$.post('/includes/ajax_sendinfo_home.asp',{

		email:$('#email').val(),
		CORPHOME:$('#CORPHOME').val()
		
	}, function(txt){

		//submission page contains either welcome or thank you
		if(txt.search(/OK/i) == -1) {
			alert('Sorry, we are having a difficulty processing your request.\nPlease try again later.');
		} else {

			// show thank you 
			$('#email').val('');
			$('#SignUpSuccess').click();

		}
	});

	return false;

}

</script>

