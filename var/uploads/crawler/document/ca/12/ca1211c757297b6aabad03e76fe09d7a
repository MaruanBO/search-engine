<script>
$(function() {
	$('#j-carousel').presto_Carousel(1000, 525, {
		auto: true,
		animation: 1, 
		interval: 5000, 
		animationSpeed: 400,
		control: '.control'
	});
		
		
  $('#j-carousel .player-wrapper').hover(function() {
  	$(this).children('.close-player').show();
  }, function() {
  	$(this).children('.close-player').hide();
  });
	
$('#j-carousel .close-player').click(function(e) {
  e.preventDefault();
  jwplayer('j-carousel-player').stop();
  $('#j-carousel .player-wrapper').hide();
  $('.player-wrapper > div', '#j-carousel').remove();
  $('.player-wrapper', '#j-carousel').prepend('<div id="j-carousel-player" />');
  $('#j-carousel').getPRESTO_Carousel().play();
});
		
$('#j-carousel .play-video').click(function(e) {
	jwplayer('j-carousel-player').setup({
  		width: $('#j-carousel').outerWidth(),
  		height: $('#j-carousel').outerHeight(),
		file: $(this).attr('href'),
		provider: $(this).data('provider'),
		modes: [
			{ type: "flash", src: "/info/mediaplayer.swf" },
			{ type: "html5" }
		],
		autostart: true
  	});
  	e.preventDefault();
  	$('#j-carousel').getPRESTO_Carousel().stop();
	$('#j-carousel .player-wrapper').show();
}).hover(function() {
	$(this).css({'background-position': '0 -42px'});
}, function() {
	$(this).css({'background-position': '0 0'});
});
		

var initOverlayPlayer = function(id, src, width, height, provider) {
  jwplayer(id).setup({
    width: width,
    height: height,
    file: src,
    provider: provider,
    autostart: true,
    modes: [
      { type: "flash", src: "/info/mediaplayer.swf" },
      { type: "html5" }
    ]
      });
};



});
</script>

