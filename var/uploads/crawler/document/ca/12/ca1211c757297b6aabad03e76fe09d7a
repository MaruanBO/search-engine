<script>
$(function() {

var api = null,
panes = $('#ssp-tabs > .panes > .pane');

api = $('#ssp-tabs > .tabs').tabs($(panes), {
    current: 'active',
    initialIndex: 0,
    onBeforeClick: function(e, i) {
        $('#ssp-tabs > .tabs').removeClass('state' + (this.getIndex() + 1)).addClass('state' + (i + 1));
        $('#ssp-tabs > .panes > .tab_title').removeClass('active').eq(i).addClass('active');
    },
    onClick: function(e, i) {
        panes.eq(i).trigger('paneShown');
    }
}).data('tabConfig', { 'initialIndex': 0 }).data('tabs');


});
</script>

