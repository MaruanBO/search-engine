<script>
$(function() {

var api = null,
panes = $('#tradition-tabs > .panes > .pane');

api = $('#tradition-tabs > .tabs').tabs($(panes), {
    current: 'active',
    initialIndex: 0,
    onBeforeClick: function(e, i) {
        $('#tradition-tabs > .tabs').removeClass('state' + (this.getIndex() + 1)).addClass('state' + (i + 1));
        $('#tradition-tabs > .panes > .tab_title').removeClass('active').eq(i).addClass('active');
    },
    onClick: function(e, i) {
        panes.eq(i).trigger('paneShown');
    }
}).data('tabConfig', { 'initialIndex': 0 }).data('tabs');


var tabTitles = $('#tradition-tabs > .panes > .tab_title');
tabTitles.click(function(e) {
  e.preventDefault();
  if (typeof api !== 'undefined') {
    api.click(tabTitles.index(this));
  }
});


});
</script>
  <a href="http://www.gocrimson.com/video-gallery" class="video-link">Video</a>
