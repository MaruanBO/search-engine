<script>
(function($) {
$(function() {

$('[data-scrollable="true"]').each(function() {
  var slider = $(this).children('.slider');
  var controls = $(this).children('.controls');
  var circular = $(this).data('circular');
  var autoscroll = $(this).data('autoscroll');
  var delay = $(this).data('delay');

  var api = slider.scrollable({ circular: circular }).data('scrollable');
  if (autoscroll) {
    slider.autoscroll(delay);
  }

});

});
})(jQuery);
</script>









