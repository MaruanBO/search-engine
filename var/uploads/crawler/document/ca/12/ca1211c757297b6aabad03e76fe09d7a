        $('#social-tabs > .tabs').removeClass('state' + (this.getIndex() + 1)).addClass('state' + (i + 1));
        $('#social-tabs > .panes > .tab_title').removeClass('active').eq(i).addClass('active');
    },
    onClick: function(e, i) {
        panes.eq(i).trigger('paneShown');
    }
}).data('tabConfig', { 'initialIndex': 0 }).data('tabs');


var tabTitles = $('#social-tabs > .panes > .tab_title');
tabTitles.click(function(e) {
  e.preventDefault();
  if (typeof api !== 'undefined') {
    api.click(tabTitles.index(this));
  }
