<script>
(function($) {
  $(function() {

    $('#jump-to-sticky-nav').click(function(e) {
      e.preventDefault();
      $(this).toggleClass('active');
      $('#sticky-nav').toggleClass('active');
    });

    $('#sticky-nav .close').click(function(e) {
      $('#jump-to-sticky-nav').removeClass('active');
      $('#sticky-nav').removeClass('active');
      return false;
    });

        $('.has-submenu > a', '#sticky-nav').on('click', function() {
      $(this).parent().toggleClass('active');
      return false;
    });
  
    $(document).on('click', function(e) {
      $('.has-submenu.active').removeClass('active');
    });

  });
})(jQuery);
</script>

 

