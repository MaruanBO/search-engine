<script type="text/javascript">
$(function() {

var scroller = $('.schedule-slider .slider').scrollable({ circular: false, keyboard: false, mousewheel: false, touch: false }).data('scrollable');

var api = scroller;

$('.schedule-slider .pp').click(function() {
  if ($(this).hasClass('pause')) {

    $(this).removeClass('pause');
    $(this).addClass('play');
    api.stop();

  } else if ($(this).hasClass("play")) {

    $(this).removeClass('play');
    $(this).addClass('pause');
    api.play();

  };
});


});
</script>

