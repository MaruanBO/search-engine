<script>
(function() {
$(function() {

var container = $('#more-headlines');
var lazyLoaded = false

$('.pane', '.tabs-wrapper').on('paneShown', function() {
  if ($(this).has(container).length > 0 && !lazyLoaded) {
    setTimeout(function() {
      $('.thumb', container).lazyload({
        container: container,
        threshold : 100,
        effect : "fadeIn"
      });
      lazyLoaded = true;
      container.trigger('scroll');
    }, 200);
  }
});

});
})(jQuery);
</script>

