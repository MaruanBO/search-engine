<script type="text/javascript">
$(document).ready( function($) {
   var ajaxUrl = hideUrlFromBots('/GetSesCookies'
            + '?storeId=' + '6970'
            + '&catalogId=' + '11652'
            + '&langId=' + '-1'
            + '&noCacheDateMS=' + (new Date()).getTime()
            + '&noCacheRand=' + Math.floor((Math.random()*100)+1));
   if (typeof (ajaxUrl) != 'undefined' && ajaxUrl != null && ajaxUrl != "") {
      var SesCookies = getSesCookies();
      if (typeof (SesCookies) == 'undefined' || SesCookies == '') {
         $.ajax({
            type : 'GET',
            url : ajaxUrl,
            dataType : 'json',
            cache    : false,
            success  : function(result) {
               SesCookies = result.SesCookies;
               setSesCookies(SesCookies);
               $(document).trigger('SesCookiesChanged', ['SesCookiesCreated', SesCookies]);
            },
            error : function (xhr, ajaxOptions, thrownError) {
               if(window.console && console.error){
                  console.error('GetSesCookies error status: "' + xhr.status + '" message: "' + thrownError + '"');
               }
            }
         });
      }
   }
});
</script>

