<!-- Push global dimensions -->
<script type="text/javascript">// <![CDATA[

    // TO DO: parse content type
    var faContentType = function() {

        // Store URL for testing
        var sHref = window.location.href.toLowerCase();

        // Content type 1: Donation
        // Page with any of the following in the URL: 
        // donation=form1
        // ways-to-give
        // virtual_food_drive
        // GiftCatalog
        // GiveAMeal

        var bFoundContentType = 0;
        // first, check query string
        // Next, check URL - elements should always be lowercase
        var aUrlCheckTerms = ['ways-to-give','virtual_food_drive','giftcatalog','giveameal', 'donation=form1'];
        
        for (var i = 0; i < aUrlCheckTerms.length; i++) {

            if (sHref.indexOf(aUrlCheckTerms[i]) > -1) {
                bFoundContentType = 1;
            }
        }
        if (bFoundContentType) {
            return 'Donation';
        }

        // Content type 2: Conversion
        // Page with any of the following in the URL:
        // donation=complete
        // rthanks
        // EcommerceCheckout & CONFIRMATION=true

        var bFoundContentType = 0;
        // first, check query string
        if (sHref.indexOf('confirmation=true') > -1 && (sHref.indexOf('ecommercecheckout') > -1)) {
            // Check URL and query string - elements should always be lowercase
            bFoundContentType = 1;
        }
        else {
            // Next, check URL - elements should always be lowercase
            var aUrlCheckTerms = ['rthanks', 'donation=complete'];
            
            for (var i = 0; i < aUrlCheckTerms.length; i++) {

                if (sHref.indexOf(aUrlCheckTerms[i]) > -1) {
                    bFoundContentType = 1;
                }
            }
        }
        if (bFoundContentType) {
            return 'Conversion';
        }


        // Content type 3: Advocacy
        // Page with any of the following in the URL:
        // take-action
        // Advocacy
        // page=UserAction
        // or security category of Advocacy 1 == 1941

        var bFoundContentType = 0;
        if ('1' == '1941') {
            bFoundContentType = 1;
        }
        else {
            // Next, check URL - elements should always be lowercase
            var aUrlCheckTerms = ['take-action','advocacy','giftcatalog','giveameal', 'page=useraction'];
            
            for (var i = 0; i < aUrlCheckTerms.length; i++) {

                if (sHref.indexOf(aUrlCheckTerms[i]) > -1) {
                    bFoundContentType = 1;
                }
            }
        }
        if (bFoundContentType) {
            return 'Advocacy';
        }


        // Content type 4: Food Bank Locator
        // this will be applied to any page in the find-your-local-foodbank directory.

        var bFoundContentType = 0;
        // Next, check URL - elements should always be lowercase
        var aUrlCheckTerms = ['find-your-local-foodbank'];
        for (var i = 0; i < aUrlCheckTerms.length; i++) {

            if (sHref.indexOf(aUrlCheckTerms[i]) > -1) {
                bFoundContentType = 1;
            }
        }
        if (bFoundContentType) {
            return 'Food Bank Locator';
        }


        // Content type 5: Other
        // Any pages that doesn't meet the above requirements
        return 'Hunger Information';

    };


    // Determine first dataLayer.push() parameters / Setup object
    var oFaGaDataLayer = {};

    if ('' != '') {
        oFaGaDataLayer['visitorID'] = '';
    }
    if ('' != '') {
        oFaGaDataLayer['sourceCode'] = '';
    }
    // Catch referrer if CONVIO referrer is missing. Do this first because CONVIO may be undefined
    // Also, skip certain domains exist: convio.net, feedingamerica.org
    if ((document.referrer.indexOf("convio.net") == -1) 
        && (document.referrer.indexOf("feedingamerica.org") == -1)) {
            oFaGaDataLayer['referrer'] = document.referrer;
    }
    if (typeof CONVIO !== 'undefined') {
        if (typeof CONVIO.referrer !== 'undefined') {
            if (CONVIO.referrer.length > 0) {
                oFaGaDataLayer['referrer'] = CONVIO.referrer;
            }
        }
    }


    // get Content Type
    oFaGaDataLayer['contentType'] = faContentType();


    // TO DO: Only pass variables if present

    window.dataLayer = window.dataLayer || []; // This is required by trackTrans
    window.dataLayer.push(oFaGaDataLayer);

// ]]></script>
<!-- dataLayer -->
<script type="text/javascript">// <![CDATA[
 Y.use('jquery-noconflict','utils','yui2-yde', function() {

    var trackTrans = function() {
        // window.dataLayer = window.dataLayer || [];  // Runs earlier

        // TEST ########################################################
        window.dataLayer.push({
            'test_1': 'trackTrans() start'
        });
        // TEST ########################################################

        // Store URL for testing
        var sHref = window.location.href.toLowerCase();

        // Case sensitive
        var getQueryParam = function(label,value) {
            var vars = window.location.search.substring(1).split("&");

            for (var i = 0; i < vars.length; i++) {
                pair = vars[i].split("=");
                pair_label = pair[0];
                pair_value = pair[1];
                if (pair_label == label) {
                    if (value != undefined) {
                        if (pair_value.indexOf(value) > -1) {
                            return true;
                        }
                    }
                    else {
                        return pair_value;
                    }
                }
            }
            return (false);
        };

        var isNumber = function(n) {
            return !isNaN(parseFloat(n)) && isFinite(n);
        };

        if (sHref.indexOf('confirmation=true') > -1 && (sHref.indexOf('ecommercecheckout') > -1)) {
            //console.log("BF: init transaction tracking - eCommerce");

            var cartItem, itemPrice, quantity, prodCode, prodPrice, prodSku, total, shippingCost;
            var items = [];
            var amountTotal = 0;
            var orderId = jQuery('#tracking_code').html();

            var storeId = getQueryParam('store_id');
            if (!storeId && getQueryParam('amp;store_id')) {
                storeId = getQueryParam('amp;store_id');
            } else {
                storeId = '0000';
            }

            var storeAffiliation = storeId + '-' + ' Gift Catalog Ecommerce Store';
            var prod = {};

            //Get total from last row in table
            total = jQuery('table.ShoppingCart tr:last').find('td:nth-child(2) > p >strong').html();
            if (total) {
                total = total.substring(1, total.length); //Remove $ sign
            }

            //Get shipping cost from second last row in table
            shippingCost = jQuery('.ShoppingCart tr').eq(-2).find('td:nth-child(2) > p >strong').html();
            if (isNumber(shippingCost)) {
                shippingCost = shippingCost.substring(1, total.length);
            }

            jQuery('table.ShoppingCart tr').each(function() {
                cartItem = jQuery(this).find("span.CartItemName").html();
                itemPrice = jQuery(this).find('td:nth-child(3) > p').html();
                quantity = jQuery(this).find('td:nth-child(1) > p').html();

                if (cartItem) {
                    itemPrice = itemPrice.substring(1, itemPrice.length);
                    prodSku = storeId + '-' + cartItem;

                    prod = {
                        'sku': prodSku,
                        'name': cartItem,
                        'category': "Store",
                        'price': itemPrice,
                        'quantity': quantity
                    };

                    items.push(prod);
                }
            });
            var transactionData = {
                'event': 'donation',
                'subSourceCode' : '',
                'onsitePromo': '',
                'transactionId': orderId,
                'transactionstoreAffiliation': storeAffiliation,
                'transactionTotal': total,
                'transactionPaymentType': 'Credit Card',
                'transactionShipping': shippingCost,
                'transactionProducts': items
            };
            //console.log(transactionData);
            window.dataLayer.push(transactionData);

        } 

        // donation form tracking
        // TO DO: make this part of javascript above looking for "donation=complete" in URL
        else if (sHref.indexOf('donation=completed') > -1) {

            // window.dataLayer = window.dataLayer || []; // Runs earlier
            //console.log("BF: init transaction tracking - Donation form");
            window.dataLayer.push({   
                'event': 'donation', 
                'subSourceCode' : '',
                'onsitePromo': '',
                        'donationType': 'Payment Frequency',  
                        'transactionId': 'dc:transactionID',
                        'transactionstoreAffiliation': 'dc:giftType',
                        'transactionTotal': 0,
                            'transactionPaymentType': 'Credit Card',
                            'transactionProducts': [{
                            'sku': 'dc:donFormId-Donation Level-Payment Frequency',
                            'name': 'dc:donationFormName',
                            'category': 'Donation - Payment Frequency',
                            'price': 0,
                            'quantity': 1
                        }]
            });
        }

        // Team Raiser event registration tracking
        // TO DO: make this part of javscript above looking for "rthanks" in URL
        else if (window.location.href.indexOf('rthanks') > -1) {
            // window.dataLayer = window.dataLayer || []; // Runs earlier
            //console.log("BF: init transaction tracking - Teamraiser registration");
            window.dataLayer.push({
                'event': 'donation',
                'subSourceCode' : '',
                'onsitePromo': '',
                    'transactionId': '',
                    'transactionstoreAffiliation': 'dc:giftType',
                    'transactionTotal': 0,
                    'transactionPaymentType': 'Credit Card',
                        'transactionProducts': [{
                        'sku': '',
                        'name': '',
                        'category': 'Virtual Food Drive Registration',
                        'price': 0,
                        'quantity': 1
                }]
            });
        }

        else {
            //console.log("FA site.");
            // TEST ########################################################
            window.dataLayer.push({
                'test_page': 'unknown - no URL match'
            });
            // TEST ########################################################

        }

    } // end of trackTrans function.

    jQuery(function() {
        trackTrans();
    });

});
// ]]></script>

<!-- End Data Layer -->


