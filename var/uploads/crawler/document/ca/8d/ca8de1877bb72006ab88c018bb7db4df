<script type="text/javascript">
    $(document).ready(function() {
        var lightboxPromo = null;
        $('.lightbox-promo').each(function() {
            var box = $(this);
            if (getCookie('lightbox_promo_' + box.data('id')) != 'dismissed') {
                lightboxPromo = box;
                return false; // break
            }
        });
        if (lightboxPromo) {
            $.fancybox.open(lightboxPromo, {
                maxWidth    : 680,
                padding     : 20,
                scrolling   : 'visible',
                openEffect  : 'none',
                closeEffect : 'none',
                nextEffect  : 'none',
                prevEffect  : 'none',
                margin      : [20, 60, 20, 60],
                afterClose  : function() {
                    setCookie('lightbox_promo_' + lightboxPromo.data('id'), 'dismissed', 365);
                }
            });
        }
    });
</script>
