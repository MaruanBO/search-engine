 <script>
    (function () {
    // console.log('function execution');
  var takeoverID = 'cye';
  // var takeoverStart = new Date('12/02/14');
  var takeoverStart = new Date('12/26/2014');
        // console.log('takeoverStart: ' + takeoverStart);
  var takeoverEnd = new Date('01/01/2015');
        // console.log('takeoverEnd: ' + takeoverEnd);
  var cookieValue = readCookie('cye');
        // console.log('cookieValue: ' + cookieValue);
  var cookiereloadValue = readCookie('cyereload');
        // console.log('cookiereloadValue: ' + cookiereloadValue);
  var redirectUrl = 'http://help.feedingamerica.org/site/Donation2?df_id=20443&20443.donation=form1&s_onsite_promo=W14C31XTK';

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i <  ca.length; i++) {
          // console.log('read cookie loop');
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1,c.length);
          if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
        // console.log('cookie return null');
      }
      
    function newCookieExpireAtHourNextDayRedirect(name,value,hourNextDay /* 0 - 24 */) {
        // get current hour, add till midnight + 2 more
        if (hourNextDay) {
          var date = new Date();
          var minutesToRemove = date.getMinutes();
          var hoursToAdd = 24 - date.getHours() + hourNextDay;
          date.setTime(date.getTime()+((hoursToAdd*60-minutesToRemove)*60*1000));
          // var expires = "; expires="+date.toGMTString();
          // console.log('time: ' + date + minutesToRemove + hoursToAdd);
        }
        else var expires = "";
        document.cookie = name+"="+value+expires+"; path=/; host=http://feedingamerica.org/;";
      }

      today = new Date();
      //today.setHours(0, 0, 0, 0);
   
    $.browser.chrome = /chrom(e|ium)/.test(navigator.userAgent.toLowerCase());
 

        if (today.getTime() > takeoverStart.getTime() && takeoverEnd.getTime() > today.getTime() && cookieValue == null) {
          // console.log('time constraints met');
          newCookieExpireAtHourNextDayRedirect("cye", "true", 2);
          if (redirectUrl != null && redirectUrl != '') {
          // console.log('redirect is not null');

            // Beaconfire to update Google Analytics code?
            // _gaq.push(['_trackEvent', 'Redirect', 'Redirect']);
             redirectUrl = redirectUrl.replace(/&amp;/g, '&');
           if($.browser.chrome)
            {
                  setTimeout(function () { window.location.href = redirectUrl; },100);
            }
            else
            // console.log('redirect non-Chrome browsers');
              window.location = redirectUrl;
          }
          else {
            window.takeover = true;
            window['optimizely'] = window['optimizely'] || [];
            window['optimizely'].push(["customTag", "takeover", "displayed"]);
            window['optimizely'].push(["activate", takeoverID]);
          }
       }     
})();
    </script>



