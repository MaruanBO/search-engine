function isHandHeld(){
	var userAgentString = navigator.userAgent;
	if ( userAgentString.indexOf("Windows CE") != -1 ) {
        return true;
    }
    if ( userAgentString.indexOf("Palm") != -1 || userAgentString.indexOf("Blazer") != -1 ) {
        return true;
    }
	if ( userAgentString.indexOf("BlackBerry") != -1 ) {
    	return true;
    }
    return false;
}
