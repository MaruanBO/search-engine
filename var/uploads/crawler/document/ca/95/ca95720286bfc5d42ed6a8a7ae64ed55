<script type="text/javascript">
//<![CDATA[
$(document).ready(function() {
	//Show the paging and activate its first link
	$("#calloutLinks a:first").addClass("active");
	$active = $("#calloutLinks a:first");
	//Get size of the image, how many images there are, then determin the size of the image reel.
	var imageWidth = $("#calloutWindow").width();
	var imageSum = $("#calloutReel .reelItem").length;
	var imageReelWidth = imageWidth * imageSum;
	//Adjust the image reel to its new size
	$("#calloutReel").css({'width' : imageReelWidth});
	
	//Paging  and Slider Function
	rotate = function(){
		var triggerID = $active.attr("rel") - 1; //Get number of times to slide
		var image_reelPosition = triggerID * imageWidth; //Determines the distance the image reel needs to slide
	
		$("#calloutLinks a").removeClass('active'); //Remove all active class
		$active.addClass('active'); //Add active class (the $active is declared in the rotateSwitch function)
		
		if(triggerID <= 0){
			$(".calloutPrev").hide();
		} else {
			$(".calloutPrev").show();
		}
		if (triggerID == imageSum-1) {
			$(".calloutNext").hide();
		} else {
			$(".calloutNext").show();
		}	
		
		//Slider Animation
		$("#calloutReel").animate({
			left: -image_reelPosition
		}, 500 );
	}; 
	
	

	//Rotation  and Timing Event
	rotateSwitch = function(){
		play = setInterval(function(){ //Set timer - this will repeat itself every
			$active = $('#calloutLinks a.active').next(); //Move to the next paging
			if ( $active.length === 0) { //If paging reaches the end...
				$active = $('#calloutLinks a:first'); //go back to first
			}
			rotate(); //Trigger the paging and slider function
		}, 5000); //Timer speed in milliseconds
	};

	rotateSwitch(); //Run function on launch
	
	
	
//Hero onHover
	$(".heroArea").hover(function() {
		if(play){
		clearInterval(play); //Stop the rotation
		}
	}, function() {
		if(play){
		clearInterval(play); //Stop the rotation
		rotateSwitch(); //Resume rotation timer
		}
	});	
		
//Hero onClick
	$("#calloutLinks a").click(function() {
		if($active!=null){
		//Reset Timer
		clearInterval(play); //Stop the rotation
		$active = $(this); //Activate the clicked paging
		rotate(); //Trigger rotation immediately
		}
		return false; //Prevent browser jump to link anchor
	});
	$(".calloutPrev").on('click', function () {
		if($active!=null){
		//Reset Timer
		$active = $active.prev(); //Activate the clicked paging
		clearInterval(play); //Stop the rotation
		rotate(); //Trigger rotation immediately
		}
		return false; //Prevent browser jump to link anchor
	});
	$(".calloutNext").on('click', function () {
		if($active!=null){
		//Reset Timer
		$active = $active.next(); //Activate the clicked paging
		clearInterval(play); //Stop the rotation
		rotate(); //Trigger rotation immediately
		}
		return false; //Prevent browser jump to link anchor
	});
	
//Hover categories

	
//screen
	if ((window.screen.width==1024)) {
		$(".homepageInner").css({'margin':'-12px 0 0 -22px','padding':'0 19px 10px'});
		$(".heroArea").css({'width':'1010px'});
	}
	
	
	//Start product spot cycle...
	miniCycle(0, 10000);
	
	
	
	// ============= [ Attach Google Analytics events ] =============
	// Slide spots
	$('#calloutReel area').each(function(i, element) {
		$(this).click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'Slideshow Spot ' + (i + 1)]);
		});
	});
	
	// Slide subfeatures
	$('.heroArea a.secondaryCallout').each(function(i, element) {
		$(this).click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'Subfeature Spot ' + (i + 1)]);
		});
	});

	// Expanding category bar
	$('#homeCategories .productCatg').each(function(i, element) {
		$(this).find('a').click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'Expanding Category Spot ' + (i + 1)]);
		});
	});
	
	// Main product spots in homepage body
	$('#homeFeatured .product').each(function(i, element) {
		var $prodSpot_anchorTags = $(element).find('a');
		
		$prodSpot_anchorTags.click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'Product Spot ' + (i + 1)]);
		});
	});
	
	// Rich Relevance
	
	$('#rr_placement0 .rr_item').each(function(i, element) {
		var $rrSpot_anchorTags = $(element).find('a');
		
		$rrSpot_anchorTags.click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'RR Spot ' + (i + 1)]);
		});
	});	
	
	
	// Do-It-Yourself Corner
	$('#diyBlock .diyseo a').click(function(e) {
		_gaq.push(['_trackEvent', 'Homepage', 'click', 'DIY Corner - See all']);
	});
	$('#diyBlock .cell').each(function(i) {
		$(this).find('a').click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'DIY Corner Spot ' + (i + 1)]);
		});
	});
	
	// Bottom Promos
	$('.horizPromos a.hPromo').each(function(i) {
		$(this).click(function(e) {
			_gaq.push(['_trackEvent', 'Homepage', 'click', 'Bottom Promo Spot ' + (i + 1)]);
		});
    });
	// ============= [ /Attach Google Analytics events ] =============
});


// Homepage product spot banner cycling
function miniCycle(startFrom, speed) {
	var $mc_wrapper = $('.miniCycle');
	var $mc_slides = $mc_wrapper.children('a');
	var $mc_slideCount = $mc_slides.length;
	var $mc_targetSlide = $mc_slides.eq(startFrom);
	
	$mc_slides.each(function(i) { // Find the currently displayed banner and hide it
		var $this = $(this);
		if($this.is(':visible')) {
			$this.css('display','none');
		}
	});
	
	$mc_targetSlide.css('display','block'); // Show our new banner
	
	if (startFrom >= $mc_slideCount - 1) { // Check and reset the banner count
		startFrom = 0;
	} else {
		startFrom++;
	}
	
	setTimeout(function(){ // LOOP!
		miniCycle(startFrom, speed);
	}, speed);
}

//]]>
</script>



