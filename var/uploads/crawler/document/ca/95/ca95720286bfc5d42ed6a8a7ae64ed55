                <span class="userSignin" id="userSignin"></span>
				
				<script type="text/javascript" async>
					
					(function () {
												
						function customer () {

							this.isLoggedIn = false;
							this.isMemberStatusOn = false;
							this.isRestrictiveMAP = false;
							this.firstName = '';
							this.productCount = '0';
							this.cartTotal = '$0.00';

							try
							{
								this._init();
							}
							catch (e)
							{
							}							
						}

						customer.prototype._init = function () {

							var cookie = $.cookie('Cart');
							var pairs = cookie.split('&');

							for (i = 0; i < pairs.length; i++) 
							{
								var pair = pairs[i].split('=');

								if (pair.length != 2) continue;

								if (pair[0] === 'rNavProfileName' && pair[1])
								{
									this.isLoggedIn = true;
									this.firstName = pair[1].split(' ')[0];
								}
								else if (pair[0] === 'ic')
								{
									this.isMemberStatusOn = pair[1] === '1';
								}
								else if (pair[0] === 'TotalItems' && pair[1])
								{
									this.productCount = pair[1];
								}								
								else if (pair[0] === 'ProductTotal' && pair[1])
								{
									this.cartTotal = '$' + parseFloat(pair[1]).toFixed(2);
								}
								else if (pair[0] === 'RestrictiveMAP')
								{
									this.isRestrictiveMAP = pair[1] === '1';
								}
							}
						};

						customer.prototype.getCartTotal = function () {
							
							if (this.isRestrictiveMAP) return '';

							return this.cartTotal;
						};

						function showInfoInMastHead() {

							var c = new customer();

							// display customer first name
							if (c.isLoggedIn)
								$('.userGreeting').append('Hello, <strong class="userGreetingName">' + c.firstName + '</strong>');
							else
								$('.userGreeting').append('Hello');

							// display related phone number
							if (c.isLoggedIn && c.isMemberStatusOn)
								$('.livePhone').text('(800) 685-8888');
							else
								$('.livePhone').text('(800) 800-8300');

							// display login or logout links
							if(c.isLoggedIn)
							{
								var loggedInHTML = "";

								if (c.isMemberStatusOn)
									loggedInHTML = '<img src="http://images.highspeedbackbone.net/td/masthead_v2/masthead-innercircle.png" alt="Inner Circle" />&nbsp; | &nbsp;';
								
								loggedInHTML = loggedInHTML + '<a rel="nofollow" title="Sign out" href="https://www.tigerdirect.com/cgisec/logoff.asp">Sign out</a>'

								$('#userSignin').append(loggedInHTML);
							}							
							else
							{
								var notLoggedInHTML = '<a href="https://www.tigerdirect.com/secure/orderlogin.asp?PG=1&amp;cm_sp=Masthead-_-Signin-_-NA" rel="nofollow" title="Sign in">Sign in</a>' +
									' or <a rel="nofollow" href="https://www.tigerdirect.com/secure/subscribe2.asp?cm_sp=Masthead-_-NewCustomer-_-NA" title="New Customer">New Customer</a>'

								$('#userSignin').append(notLoggedInHTML);								
							}

							// display product count
							$("#cart_item_count").text(c.productCount);
							// display cart total based on restrictmap logic
							$("#cart_total").text(c.getCartTotal());
						}

						showInfoInMastHead();
					})();

				</script>
            </div>
