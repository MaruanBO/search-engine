    var sr_cartid  = ($.cookie('CartId')==undefined) ? "" : $.cookie('CartId');
	var sr_srccode = ($.cookie('SRCCODE')==undefined) ? "" : $.cookie('SRCCODE');  
	var sr_session = ($.cookie('SessionId')==undefined) ? "" : $.cookie('SessionId'); 

    _shoprunner_com.loginValidationURL = function(token, success, failure){
	  try{
		$.ajax("https://orders.tigerdirect.com/ShopRunner/Login?cartId=" + sr_cartid + "&srcCode=" + sr_srccode + "&sessionId=" + sr_session + "&srtoken=" + token,
			{dataType : "jsonp"})
			.done(function(data){
				
				if(data && data.cartId && data.cartDb){
					$.cookie("CartSave", "CartId=" + data.cartId + "&Cart=" + data.cartDb, { expires: 5, path: '/' });
					$.cookie("CartId", data.cartId, { path: '/' });
				}
				
				success();
			})
			.fail(function(){
				failure();
			});
		  }
		  catch(e)
		  {}
	}

	_shoprunner_com.checkout = new Object();
