//Otari 111319 for pages using this method
function showFreeShipTooltip(img) {
   var image = document.getElementById(img);
   if (image !=null)  {
	  if (image.offsetWidth > 55) {
		Tip('Free Econoship delivery on orders over $100 -- See details at bottom of the page');
	  } else {
		return true;
	  }
   }
}

function OpenChat()
{
	var chatURL = "http://www.tigerdirect.com/include/chatavailable.asp"
	$.get(chatURL, function(data){
		if(data.indexOf("Offline")==-1)
		{
		    window.open('https://chat.help.tigerdirect.com/chat.php?a=ee800&d=03','_blank','scrollbars=yes,width=570,height=690,toolbar=no,location=center,resizable=no'); 
		}
		else
		{
			window.location = "http://www.tigerdirect.com/sectors/Help/email.asp";	
		}
	});
}
</script>


