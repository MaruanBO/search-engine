		var dropdownMenu;
		if (typeof (DropdownMenu) == 'function')

		{
			dropdownMenu = new DropdownMenu(0.25);
		}

		function openMenu(depth, currentId, relativeId, vAlign, hAlign) {
			if (dropdownMenu) {
				dropdownMenu.open(depth, currentId, relativeId, vAlign, hAlign);
			}
		}

		function closeMenu(event) {
			if (dropdownMenu) {
				dropdownMenu.close(event);
			}
		}
	</script>

