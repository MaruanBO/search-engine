    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
            });
        });

        function CheckEmpty(elem, message) {

            if (jQuery.trim(elem.val()).length == 0) {
                elem.css('background', '#FEDFDF');
                alert(message);
                return false;
            }
            else {
                elem.css('background', '#FFFFFF');
                return true;
            }

        }

        function SendLead() {
            if (!CheckEmpty($('#yourname'), "Please enter your name")) {
                return;
            }
            if (!CheckEmpty($('#yourphone'), "Please enter your phone number")) {
                return;
            }
            if (!CheckEmpty($('#youremail'), "Please enter your email")) {
                return;
            }
            if (!CheckEmpty($('#yourmessage'), "Please tell us your site")) {
                return;
            }

            var yourname = $('#yourname').val();
            var yourphone = $('#yourphone').val();
            var youremail = $('#youremail').val();
            var yourmessage = $('#yourmessage').val();

            var url = 'http://www.queryly.com/ajax/publicajax.aspx?call=newlead';

            var pars = 'yourname=' + encodeURIComponent(yourname) + '&yourphone=' + encodeURIComponent(yourphone) + '&youremail=' + encodeURIComponent(youremail) + '&yourmessage=' + encodeURIComponent(yourmessage);
            $.ajax({
                async: false,
                type: "POST",
                url: url,
                data: pars,
                dataType: "text",
                success: function (data) {
                    $('#newlead').html("<div style='font-size: 1.4em;margin-top:8px;margin-bottom:50px;'>Thank you! Our dev team will reach out to you shortly.</div>");
                }

            });
        }


		</script>
