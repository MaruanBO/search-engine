        <script>

    /*
        omniture tracking
            s.pageName - get page name from tile
    */

    s.pageName = "public home page";

    /*
        traffic variable
            sProps1 - user type
            sProps4 - session compaign, tracked in omniture plugin
            sProps6 - user id only if user login
    */

    s.prop1 =   
                        "Guest"
                    
                ;

    s.prop4 = s.getAndPersistValue(s.prop4,'s_campaign',0);

    

    /*
        converstion variable
            eVar1 - why registered
            eVar2 - why registered Public profile specific click source
            eVar5 - why subscribed
            eVar19 - channel, tracking in the omniture plugin
            eVar22 - user id only if user login
    */

    
    /*
        success events
            event1 - reg init
            event2 - reg complete
            event3 - continue for free
            event5 - init landing
    */

    var s_code = s.t();
    if (s_code) {
        document.write(s_code);
    }

</script>




