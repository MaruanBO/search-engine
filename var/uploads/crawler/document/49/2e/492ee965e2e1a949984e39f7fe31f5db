  var theChosenPage = getQueryVariable('cat');
	if (theChosenPage == "ba"){
		swiperParent.swipeTo(1);
	}
	if (theChosenPage == "re"){
		swiperParent.swipeTo(2);
	}
	if (theChosenPage == "go"){
		swiperParent.swipeTo(3);
	}
	if (theChosenPage == "ed"){
		swiperParent.swipeTo(4);;
	}
	if (theChosenPage == "ta"){
		swiperParent.swipeTo(5);
	}
	if (theChosenPage == "vi"){
		swiperParent.swipeTo(6);
	}
	if (theChosenPage == "fa"){
		swiperParent.swipeTo(7);
	}

  
  
  
