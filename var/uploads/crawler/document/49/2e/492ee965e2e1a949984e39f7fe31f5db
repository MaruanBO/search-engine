					var tCount = $(".swiper-nested-7 .thumb").length;
					$(".prev7").css("background-color", origBGColor7);
					$(".prev7").css("cursor", "pointer");
					if(swiperNested7.activeIndex >= tCount - 6){
						$(".next7").css("background-color", grayColor7);
						$(".next7").css("cursor", "default");
					}else{
						$(".next7").css("background-color", origBGColor7);
						$(".next7").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
			
			});
				
			$(".prev" + i).bind("click", function(){
				var theID = $(this).attr("id");
				var lastChar = theID.substr(theID.length - 1);
				
				if (lastChar == 1){
					$(".next1").css("background-color", origBGColor1);
					$(".next1").css("cursor", "pointer");
					if(swiperNested1.activeIndex < 4){
						$(".prev1").css("background-color", "#666666");
						$(".prev1").css("cursor", "default");
					}else{
						$(".prev1").css("background-color", origBGColor1);
						$(".prev1").css("cursor", "pointer");
					}
				}
				
				if (lastChar == 2){
					$(".next2").css("background-color", origBGColor2);
					$(".next2").css("cursor", "pointer");
					if(swiperNested2.activeIndex < 4){
						$(".prev2").css("background-color", "#666666");
						$(".prev2").css("cursor", "default");
					}else{
						$(".prev2").css("background-color", origBGColor2);
						$(".prev2").css("cursor", "pointer");
					}
				}
				
				if (lastChar == 3){
					$(".next3").css("background-color", origBGColor3);
					$(".next3").css("cursor", "pointer");
					if(swiperNested3.activeIndex < 4){
						$(".prev3").css("background-color", "#666666");
						$(".prev3").css("cursor", "default");
					}else{
						$(".prev3").css("background-color", origBGColor3);
						$(".prev3").css("cursor", "pointer");
					}
				}
				
				if (lastChar == 4){
					$(".next4").css("background-color", origBGColor4);
					$(".next4").css("cursor", "pointer");
					if(swiperNested4.activeIndex < 4){
						$(".prev4").css("background-color", "#666666");
						$(".prev4").css("cursor", "default");
					}else{
						$(".prev4").css("background-color", origBGColor4);
						$(".prev4").css("cursor", "pointer");
					}
				}
				
				if (lastChar == 5){
					$(".next5").css("background-color", origBGColor5);
					$(".next5").css("cursor", "pointer");
					if(swiperNested5.activeIndex < 4){
						$(".prev5").css("background-color", "#666666");
						$(".prev5").css("cursor", "default");
					}else{
						$(".prev5").css("background-color", origBGColor5);
						$(".prev5").css("cursor", "pointer");
					}
