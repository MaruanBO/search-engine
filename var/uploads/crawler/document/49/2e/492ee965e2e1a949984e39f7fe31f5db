    <script type="text/javascript">
	
	function StartTour(){
			$('#grayOut').fadeIn();
			$('#tutVideo').fadeIn();
			$('#tutVideo').append('<div id="tvCloser" style="cursor:pointer;width:100%; height:30px; text-align:right;color:#fff;font-weight:800; font-size:16px; margin-bottom:10px;">close [x]</div><iframe width="853" height="480" src="//www.youtube.com/embed/BwWIS9qwPsg" frameborder="0" allowfullscreen></iframe>');
			
			$('#tvCloser').bind('click', function(){
				$('#grayOut').fadeOut();
				$('#tutVideo').fadeOut();
				$('#tutVideo').html('');
			});
	}
	
	$(document).ready(function(){
		
		//jquery smart banner for android devices
		$.smartbanner({
 		 title: 'IN.gov App',
 		 author: ' ',
		 inAppStore: 'On the App Store',
		 inGooglePlay: 'On Google Play',
		 icon: '/core/images/androidThumb.png'
		});
		//end smart banner for android
		
		$('#tvLaunch').bind('click', function(){
			$('#grayOut').fadeIn();
			$('#tutVideo').fadeIn();
			$('#tutVideo').append('<div id="tvCloser" style="cursor:pointer;width:100%; height:30px; text-align:right;color:#fff;font-weight:800; font-size:16px; margin-bottom:10px;">close [x]</div><iframe width="853" height="480" src="//www.youtube.com/embed/BwWIS9qwPsg" frameborder="0" allowfullscreen></iframe>');
			
			$('#tvCloser').bind('click', function(){
				$('#grayOut').fadeOut();
				$('#tutVideo').fadeOut();
				$('#tutVideo').html('');
			});
			
		});
		
		$('#grayOut').bind('click', function(){
			$('#grayOut').fadeOut();
			$('#tutVideo').fadeOut();
			$('#tutVideo').html('');
		});
		
		
		
	});
	
	</script>
 
