		 $("#lotto #prev").show();
		 $("#lotto #next").show();
	 }
	 if(swiperNested9.activeIndex == 4){
		 $("#lotto #prev").show();
		 $("#lotto #next").hide();
	 }
	 if(swiperNested9.activeIndex > 0 && swiperNested9.activeIndex < 4){
		 $("#lotto #prev").show();
		 $("#lotto #next").show();
	 }
  });
  
  
 
 
 
 
 
 
 var waitForFinalEvent = (function () {
               var timers = {};
                 return function (callback, ms, uniqueId) {
                   if (!uniqueId) {
                     uniqueId = "Don't call this twice without a uniqueId";
                   }
                   if (timers[uniqueId]) {
                     clearTimeout (timers[uniqueId]);
                   }
                   timers[uniqueId] = setTimeout(callback, ms);
                 };
               })();


$(window).resize(function() {
   waitForFinalEvent(function(){
	  var windowSize = $(window).width();
   	 if(windowSize < 880){
			//alert('doing this');
			var newWidth = $("#subpage-cta").width() - 32;
			$(".swiper-nested-1, .swiper-nested-2, .swiper-nested-3, .swiper-nested-4, .swiper-nested-5, .swiper-nested-6, .swiper-nested-7").attr('style', "width: " + newWidth + "px !important");
			
			swiperNested1.params.slidesPerGroup = 1;
			swiperNested1.params.slidesPerView = "auto";
        	swiperNested1.resizeFix(true);
            swiperNested1.reInit();
			swiperNested2.params.slidesPerGroup = 1;
			swiperNested2.params.slidesPerView = "auto";
			swiperNested2.resizeFix(true);
            swiperNested2.reInit();
			swiperNested3.params.slidesPerGroup = 1;
			swiperNested3.params.slidesPerView = "auto";
			swiperNested3.resizeFix(true);
            swiperNested3.reInit();
			swiperNested4.params.slidesPerGroup = 1;
			swiperNested4.params.slidesPerView = "auto";
			swiperNested4.resizeFix(true);
            swiperNested4.reInit();
			swiperNested5.params.slidesPerGroup = 1;
			swiperNested5.params.slidesPerView = "auto";
			swiperNested5.resizeFix(true);
            swiperNested5.reInit();
			swiperNested6.params.slidesPerGroup = 1;
			swiperNested6.params.slidesPerView = "auto";
			swiperNested6.resizeFix(true);
            swiperNested6.reInit();
			swiperNested7.params.slidesPerGroup = 1;
			swiperNested7.params.slidesPerView = "auto";
			swiperNested7.resizeFix(true);
            swiperNested7.reInit();
		}
		
		 if(windowSize > 880){
			//alert('doing this');
			var newWidth = $("#subpage-cta").width() - 32;
			$(".swiper-nested-1, .swiper-nested-2, .swiper-nested-3, .swiper-nested-4, .swiper-nested-5, .swiper-nested-6, .swiper-nested-7").attr('style', "width: 315px !important");
			
			
			for(i=1; i < 8; i++){
				$(".prev" + i).css("background-color", "#666666");
				$(".prev" + i).css("cursor", "default");
				var thumbCount = $(".swiper-nested-" + i + " .thumb").length;
				$(".swiper-nested-" + i).data("count", thumbCount);
				if (thumbCount <= 3){
					$(".next" + i).css("background-color", "#666666");
					$(".next" + i).css("cursor", "default");
				}
			}
			
			
			swiperNested1.params.slidesPerGroup = 3;
			swiperNested1.params.slidesPerView = 3;
        	swiperNested1.resizeFix(true);
            swiperNested1.reInit();
			swiperNested2.params.slidesPerGroup = 3;
			swiperNested2.params.slidesPerView = 3;
			swiperNested2.resizeFix(true);
            swiperNested2.reInit();
			swiperNested3.params.slidesPerGroup = 3;
			swiperNested3.params.slidesPerView = 3;
			swiperNested3.resizeFix(true);
            swiperNested3.reInit();
			swiperNested4.params.slidesPerGroup = 3;
			swiperNested4.params.slidesPerView = 3;
			swiperNested4.resizeFix(true);
            swiperNested4.reInit();
			swiperNested5.params.slidesPerGroup = 3;
			swiperNested5.params.slidesPerView = 3;
			swiperNested5.resizeFix(true);
            swiperNested5.reInit();
			swiperNested6.params.slidesPerGroup = 3;
			swiperNested6.params.slidesPerView = 3;
			swiperNested6.resizeFix(true);
            swiperNested6.reInit();
			swiperNested7.params.slidesPerGroup = 3;
			swiperNested7.params.slidesPerView = 3;
			swiperNested7.resizeFix(true);
            swiperNested7.reInit();
		}
		
	}, 500);
});

 
  
  
  
