    <script type="text/javascript">
    	var picStart = 'http://www.in.gov/core/images/photos/';
        picNum = Math.floor(Math.random() * 7) + 1;
        var picEnd = '.jpg';
        var picString = picStart + picNum + picEnd;
    	picString = String(picString);
    	
    	
    	$(window).load(function(){
        	$('.ieBG img').attr("src", picString);
    		$('.default').fadeIn();
        	$('.hover-info').fadeOut();
        
        	$('#background-cta').bind('hover', function(){
        		$('.default').fadeOut();
        		$('.hover-info').fadeIn();
        	});
        
        	$('#background-cta').bind('mouseleave', function(){
        		$('.default').fadeIn();
        		$('.hover-info').fadeOut();
        	});     
        });

    </script>
        
