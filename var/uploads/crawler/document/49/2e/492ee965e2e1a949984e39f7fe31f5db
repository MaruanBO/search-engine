function setCookie(cname,cvalue,exdays)
{
var d = new Date();
d.setTime(d.getTime()+(exdays*24*60*60*1000));
var expires = "expires="+d.toGMTString();
document.cookie = cname + "=" + cvalue + "; " + expires;
} 

var FooterDown = false;
var footCookieValue = "";

//change css (remove it) for :before attribute for .footer-cta-governor - change in the a tag of that class: padding top and padding left. 
function dropFooter(){
	$('footer').animate({height: 16}, 500);
	$('#footerHider').css("background-image", "url(/core/images/showFooterBtn.jpg)");
	$('#theGovBG').fadeIn();
	$('.name').html("Pence");
	$(".footer-cta-governor").animate({left: -20}, 500);
	$(".footer-cta-governor a").animate({paddingLeft: 113}, 500);
	FooterDown = true;		
}

function raiseFooter(){
	$('footer').animate({height: 75}, 500);
	$('#footerHider').css("background-image", "url(/core/images/hideFooterBtn.jpg)");
	$('#theGovBG').fadeOut();
	$('.name').html("Mike Pence");
	$(".footer-cta-governor").animate({left: 0}, 500);
	$("..footer-cta-governor a").animate({paddingLeft: 129}, 500);
	FooterDown = false;
}

function getCookie(cname)
{
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i=0; i<ca.length; i++)
  {
  var c = ca[i];
  if (c.indexOf(name)==0) return c.substring(name.length,c.length);
}
return "";
}





var lastScrollTop = 0;
var headerShould = $('body').width();

$(function(){
	$('#nav').data('size', 'big');
});

if (headerShould > 1015){
$(window).scroll(function(){
   var st = $(this).scrollTop();
   if (st >= lastScrollTop){
	   if($('#nav').data('size') == 'big'){
		   $('#nav').data('size', 'small');
		   $('#sliver').stop().animate({height: 0}, 500);
		   $('#home img').animate({marginTop: -16}, 500);
		   $('nav').animate({height: 60}, 500);
		   $('.page-trigger').animate({height: 60}, 500);
	   		$('.submenu').css('top', '60px');
	  		//lastScrollTop = st;
	   }
   } else {
	    if($('#nav').data('size') == 'small'){
		   $('#nav').data('size', 'big');
			$('#sliver').stop().animate({height: 37}, 500);
			$('#home img').animate({marginTop: 0}, 500);
		   $('nav').animate({height: 112}, 500);
		   $('.page-trigger').animate({height: 75}, 500);
		   $('.submenu').css('top', '112px');
		   //lastScrollTop = st;
		}
   }
   lastScrollTop = st;
});

}




$(document).ready(function() {
	
	
	$('#mySwipe').show();
	$('.content').show();
	
	randomstats();
	var timeout;
	
	$(".content").each(function(){
		$(".slide", this).eq(0).show();	
	});
	
	function swapStats(){
			$(".stats li").hide();
			var ran = Math.floor(Math.random()*4) + 1;
			$(".stats li:nth-child(" + ran + ")").fadeIn();
		}
	
	function randomstats(){
		var length = $(".stats li").length;
		var ran = Math.floor(Math.random()*4) + 1;
		$(".stats li:nth-child(" + ran + ")").show();
		timeout = setInterval(function(){swapStats()}, 5000);
	}; 
	
	if($('.county_info').length != 0) {
		var CIheight = $('.county_info').height();
		var MLBheight = $('.mylocal_box').height();
		//console.log("CIheight: " + CIheight);
		//console.log("MLBheight: " + MLBheight);
		if (CIheight >= MLBheight){
			$('.mylocal_box').css('height', (CIheight + 40) + 'px');
		}else{
			$('.county_info').css('height', (MLBheight + 40) + 'px');
		}
	}
	
	footCookieValue = getCookie("footDown");
	if(footCookieValue != ""){
		if(footCookieValue == "true"){
			dropFooter();	
		}
		if(footCookieValue == "false"){
			if(FooterDown == true){
				raiseFooter();
			}
		}
	}
	
	$("#footerHider").bind('click', function(){
		if(FooterDown == false){
			dropFooter();
			setCookie("footDown", "true", 10000);
		}else{
			raiseFooter();
			setCookie("footDown", "false", 10000);
		}
 	});
	
 //console.log('in here, doing this stuff');
