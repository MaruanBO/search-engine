 if(windowSize > 880){
	 

	 
	for(i=1; i < 8; i++){
		//var theColor = $(".prev" + i).css("background-color");
		//$(".prev" + i).data("bgColor", $(".prev" + i).css("background-color"));
		//alert("number " + i + " : " + $(".prev" + i).data("bgColor"));   -- this seems to be returning correctly(in rgb) - not sure if it is storing the data though.
		$(".prev" + i).css("background-color", "#666666");
		$(".prev" + i).css("cursor", "default");
		var thumbCount = $(".swiper-nested-" + i + " .thumb").length;
		$(".swiper-nested-" + i).data("count", thumbCount);
		if (thumbCount <= 3){
				$(".next" + i).css("background-color", "#666666");
				$(".next" + i).css("cursor", "default");
		}
		if (thumbCount > 3){
			
			$(".next" + i).bind("click", function(){
				var theID = $(this).attr("id");
				var lastChar = theID.substr(theID.length - 1);
				//alert($(".prev1").data("bgColor"));
				
				if (lastChar == 1){
					var tCount = $(".swiper-nested-1 .thumb").length;
					//alert("tcount = " + tCount);
					//alert("activeIndex = " + swiperNested1.activeIndex);
					$(".prev1").css("background-color", origBGColor1);
					$(".prev1").css("cursor", "pointer");
					if(swiperNested1.activeIndex >= tCount - 6){
						$(".next1").css("background-color", grayColor1);
						$(".next1").css("cursor", "default");
					}else{
						$(".next1").css("background-color", origBGColor1);
						$(".next1").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
				
				if (lastChar == 2){
					var tCount = $(".swiper-nested-2 .thumb").length;
					$(".prev2").css("background-color", origBGColor2);
					$(".prev2").css("cursor", "pointer");
					if(swiperNested2.activeIndex >= tCount - 6){
						$(".next2").css("background-color", grayColor2);
						$(".next2").css("cursor", "default");
					}else{
						$(".next2").css("background-color", origBGColor2);
						$(".next2").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
				
				if (lastChar == 3){
					var tCount = $(".swiper-nested-3 .thumb").length;
					$(".prev3").css("background-color", origBGColor3);
					$(".prev3").css("cursor", "pointer");
					if(swiperNested3.activeIndex >= tCount - 6){
						$(".next3").css("background-color", grayColor3);
						$(".next3").css("cursor", "default");
					}else{
						$(".next3").css("background-color", origBGColor3);
						$(".next3").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
				
				if (lastChar == 4){
					var tCount = $(".swiper-nested-4 .thumb").length;
					$(".prev4").css("background-color", origBGColor4);
					$(".prev4").css("cursor", "pointer");
					if(swiperNested4.activeIndex >= tCount - 6){
						$(".next4").css("background-color", grayColor4);
						$(".next4").css("cursor", "default");
					}else{
						$(".next4").css("background-color", origBGColor4);
						$(".next4").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
				
				if (lastChar == 5){
					var tCount = $(".swiper-nested-5 .thumb").length;
					$(".prev5").css("background-color", origBGColor5);
					$(".prev5").css("cursor", "pointer");
					if(swiperNested5.activeIndex >= tCount - 6){
						$(".next5").css("background-color", grayColor5);
						$(".next5").css("cursor", "default");
					}else{
						$(".next5").css("background-color", origBGColor5);
						$(".next5").css("cursor", "pointer");
					}
					//alert(swiperNested1.activeIndex);
				}
				
				if (lastChar == 6){
					var tCount = $(".swiper-nested-6 .thumb").length;
					$(".prev6").css("background-color", origBGColor6);
					$(".prev6").css("cursor", "pointer");
					if(swiperNested6.activeIndex >= tCount - 6){
