$(document).ready(function() {
	////RightNow FAQ Boxes Script
	var rnTimer;
	var rnTimeout = 0;
	function startCheck() {
		rnTimer = setInterval(checkLoaded, 1000);
	}
	function checkLoaded() {
		rnTimeout++;
		clearInterval(rnTimer);
		if($('#rn_ContentListskw_0 li a').length >= 42) {
			$('#rn_ContentListskw_0 li a').each(function() {
				if($(this).html().length > 80) {
					$(this).html($(this).html().substr(0, 80) + '...');
				}
			});
		} else {
			if(rnTimeout <= 4) {
				startCheck();
			}
		}
	}
	startCheck();


	////Homepage Category Search Radio Button Selections
	
	$('.ba .search-options input').click(function() {
		$('.ba form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.ba form #client').attr('value', 'default_frontend') : $('.ba form #client').attr('value', $(this).attr('value'));
	});
	$('.re .search-options input').click(function() {
		$('.re form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.re form #client').attr('value', 'default_frontend') : $('.re form #client').attr('value', $(this).attr('value'));
	});
	$('.go .search-options input').click(function() {
		$('.go form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.go form #client').attr('value', 'default_frontend') : $('.go form #client').attr('value', $(this).attr('value'));
	});
	$('.ed .search-options input').click(function() {
		$('.ed form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.ed form #client').attr('value', 'default_frontend') : $('.ed form #client').attr('value', $(this).attr('value'));
	});
	$('.tf .search-options input').click(function() {
		$('.tf form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.tf form #client').attr('value', 'default_frontend') : $('.tf form #client').attr('value', $(this).attr('value'));
	});
	$('.vp .search-options input').click(function() {
		$('.vp form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.vp form #client').attr('value', 'default_frontend') : $('.vp form #client').attr('value', $(this).attr('value'));
	});
	$('.fh .search-options input').click(function() {
		$('.fh form #site').attr('value', $(this).attr('value'));
		$(this).attr('value') == 'default_collection' ? $('.fh form #client').attr('value', 'default_frontend') : $('.fh form #client').attr('value', $(this).attr('value'));
	});

