 $("#mySwipePrev").bind('click', function(){
	var theIndex = swiperParent.activeIndex;
	swiperParent.swipePrev();
	changeActive(theIndex);
	//swiperParent.swipeTo(theIndex);
 });
 
 $("#mySwipeNext").bind('click', function(){
	var theIndex = swiperParent.activeIndex;
	//theIndex++;
	changeActive(theIndex);
	swiperParent.swipeNext();
 });
 
 $(".noClass").bind('click', function(){
	var theIndex = $(this).index();
	changeActive(theIndex);
	swiperParent.swipeTo(theIndex);
 });
 
  
 function changeActive(index){
	//alert('doingThis:' + index);
	/*if(index >= 9){
		index = 1; 
	}*/
	if(index <= 0) {
		$('.swipe-left').hide();
	}
	if(index >= 1) {
		$('.swipe-left').show();
	}
	if(index == 7) {
		$('.swipe-right').hide();
	}
	if(index <= 6) {
		$('.swipe-right').show();
	}
	$("#nav .active").removeClass("active");
	$("#nav .noClass").eq(index).addClass("active");
 }
  


