	////Online Services Homepage Boxes
	$('.service').each(function() {
		$(this).click(function(e) {
			//$('.service').removeClass('selected');
			$(this).closest('.services').find('.service').removeClass('selected');
			$('#' + $(this).attr('id')).addClass('selected');
			
			//$('.services-list').removeClass('selected');
			$(this).closest('.services').children('.services-list').removeClass('selected');
			$('#' + $(this).attr('id')+'-list').addClass('selected');
			
			e.preventDefault ? e.preventDefault() : e.returnValue = false;
		});
		/*$(this).hover(function(){
			$('.service a').each(function(){
				$(this).css('border-left', '0px');
			});
			$('a', this).css('border-left', '2px solid #F48221');
		});*/
	});
	
	$('input[type=search]').each(function() {
		//console.log('VAL: ' + $(this).val());
		if($(this).val() != '') {
			$(this).css('background-image', 'none');
		}
	});
	
	$('input[type=search]').bind('focus', function(){
		$(this).css('background-image', 'none');
	});
	
	$('input[type=search]').bind('blur', function(){
		if($(this).val() != '') {
			$(this).css('background-image', 'none');
		} else {
			$(this).css('background-image', 'url(/core/images/stateSearchBG.jpg)');
		}
	});
	
	if($('#weather-data').length != 0) {
		var conditionImage = "";
		var lat = 0;
		var lon = 0;
		function GetLocation(thelat, thelon) {
			lat = thelat;
			lon = thelon;
			doCalls();
		}

		navigator.geolocation.getCurrentPosition(function(position){
			GetLocation(location.coords.latitude, location.coords.longitude);
		});

		function doCalls(){
			$.ajax({
				url : "http://api.wunderground.com/api/1fb35e4f37107097/geolookup/conditions/q/" + lat + "," + lon + ".json",
				dataType : "jsonp",
				success : function(parsed_json) {
					var location = parsed_json['location']['city'];
					var temp_f = parsed_json['current_observation']['temp_f'];
					var theTemp = Number(parseFloat(temp_f).toFixed(0));
					var condition = parsed_json['current_observation']['weather'];
					var weatherImage = parsed_json['current_observation']['icon'];
					$("#theTempF").html(theTemp + "&deg; F");
					$('.weather-details-location').html(' in ' + location);
					$('.weather-image').html('<img src="http://icons-ak.wxug.com/i/c/i/' + weatherImage + '.gif" height="75" />');
				}
			});
			
			$.ajax({
				url : "http://api.wunderground.com/api/1fb35e4f37107097/forecast/q/" + lat + "," + lon + ".json",
				dataType : "jsonp",
				success : function(parsed_json) {
					var high = parsed_json['forecast']['simpleforecast']['forecastday'][0]['high']['fahrenheit'];
					var low = parsed_json['forecast']['simpleforecast']['forecastday'][0]['low']['fahrenheit'];
					var theHigh = Number(parseFloat(high).toFixed(0));
					var theLow = Number(parseFloat(low).toFixed(0));
					$("#weatherLow").append("Low Today " + theHigh + "&deg; F");
					$("#weatherHigh").append("High Today " + theLow + "&deg; F");
				}
			});
		}
	} 
	
});

  function getQueryVariable(variable) {
    var query = window.location.search.substring(1);
    var vars = query.split('&');
    for (var i = 0; i < vars.length; i++) {
        var pair = vars[i].split('=');
        if (decodeURIComponent(pair[0]) == variable) {
            return decodeURIComponent(pair[1]);
        }
    }
    //console.log('Query variable %s not found', variable);
	}
	

function mycarousel_initCallback(carousel) {
    // Disable autoscrolling if the user clicks the prev or next button.
    carousel.buttonNext.bind('click', function() {
        carousel.startAuto(0);
    });

    carousel.buttonPrev.bind('click', function() {
        carousel.startAuto(0);
    });

    // Pause autoscrolling if the user moves with the cursor over the clip.
	$('#Iam').mouseenter(function() {
        carousel.stopAuto();
	});
	$('#Iam').mouseleave(function() {
        carousel.startAuto();
    });
};


var prevVisible;

function mycarousel_itemVisibleInCallbackBeforeAnimation(carousel, item, idx, state) {
    $('#jcarousel-subitem-' + prevVisible).fadeToggle('fast')
};

function mycarousel_itemVisibleInCallbackAfterAnimation(carousel, item, idx, state) {
	//console.log(idx);
    $('#jcarousel-subitem-' + idx).fadeToggle('fast')
	prevVisible = idx;
};


(function(){
	$.ajax({
		type: "GET",
		url: "https://www.hoosierlottery.com/RSS/Jackpots.aspx",
		dataType: "xml",
		success: function(xml) {
		$(xml).find('rss').each(function(){
			$(this).find('channel').each(function(){
				$(this).find('item').each(function(){
					//console.log("I'm in your xml, all your base are belong to us!");
					var deJack = $(this).find("description").text();
					var deJackArray = deJack.split(": ");
					var when = deJackArray[0];
					var jackpot = deJackArray[1];
					var title = $(this).find("title").text();
					var theLink = $(this).find("link").text();
					var date = $(this).find("pubDate").text();
					
					date = date.replace('00:00:00 GMT', '');
					
					if(title.indexOf("5") > -1){
						$("#c5LottoJack").append(jackpot);
                    	$("#c5LottoDay").append(when);
						$("#c5LottoWhen").append(date);
					}
					
					if(title.indexOf("Hoosier") > -1){
						$("#hlLottoJack").append(jackpot);
                    	$("#hlLottoDay").append(when);
						$("#hlLottoWhen").append(date);
					}
					
					if(title.indexOf("Mega") > -1){
						$("#mmLottoJack").append(jackpot);
                    	$("#mmLottoDay").append(when);
						$("#mmLottoWhen").append(date);
					}
					
					if(title.indexOf("Powerball") > -1){
						$("#pbLottoJack").append(jackpot);
                    	$("#pbLottoDay").append(when);
						$("#pbLottoWhen").append(date);
					}
					
				});
			});
		});
		}
	});	
})(jQuery);


</script>

