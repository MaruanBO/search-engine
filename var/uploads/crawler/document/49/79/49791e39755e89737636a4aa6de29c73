<script src="/Common/JS/Core/global.js?v=20150218" type="text/javascript"></script><script type="text/javascript">
function getPrices(styles) {
var stylesString = "";
for (var i = 0; i < styles.length; i++) {
    stylesString += styles[i] + ",";
}
if (stylesString !== ""){
var myUrl = "/VOD/DynamicPricing.svc/" + stylesString;
$.ajax({
     type: "GET",
     url: myUrl,
     contentType: "text/xml; charset=\"utf-8\"",
     dataType: "xml",
     async: true,
     context: document.body,
     success: function(response) {
         $(response).find('StylePrice').each(function() {
         var style = $(this).find('ISISStyleCode').text();
         var price = $(this).find('Price').text();
         var divId = "rr_price_" + style;
         price = parseFloat(price).toFixed(2);
         $("p#" + divId).text("$" + price);
     });
     }
});
}
}
</script>
