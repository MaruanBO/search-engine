                    		<script type="text/javascript">
			$(document).ready(function(){
				setInterval( "window['increment_counter_gii_impact_calculator-3']();", 120000 );
			});

			window['increment_counter_gii_impact_calculator-3'] = function(){
				var cur_count = parseInt( $( '#gii_impact_calculator-3 .counter' ).html().replace( /,/g, "") );
				var new_count = cur_count + 1;
				new_count = new_count.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
				$( '#gii_impact_calculator-3 .counter' ).html( new_count );
			}
		</script>
		<div class="counter_widget" id="gii_impact_calculator-3">
			<div class="top_text">
