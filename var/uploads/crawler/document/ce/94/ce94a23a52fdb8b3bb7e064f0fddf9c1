<script language="JavaScript" type="text/javascript">var LHCallList = new Array();
function LHRegister(FuncObject)
{
  LHCallList[LHCallList.length] = FuncObject;
  return;
}
function LHValidate()
{
   var i;
   var retCode;
   for (i = 0; i < LHCallList.length; i++)
   {
      retCode = LHCallList[i].call();
      if (retCode == false)
         break;  // Stop testing
   }
   return retCode;
}
function LHSubmitting()
{
if (LHValidate)    {       if (LHValidate() == false)         return false;   }    if (this.submitted)
   {
      // Only prevent the 2nd click from submitting again; allow a "third"
      this.submitted = false;
      return false;
   }
   this.submitted = true;
   return true;
}
function limitText(limitField, limitCount, limitNum) {
if (limitField.value.length > limitNum)
{
    limitField.value = limitField.value.substring(0, limitNum);
} else
{
    if (limitCount != null)
       document.getElementById(limitCount).value = limitNum - limitField.value.length;
}
}
