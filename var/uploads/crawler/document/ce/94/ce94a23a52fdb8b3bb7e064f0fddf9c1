    $("td label,td.Answer,td.AnswerGray,td.AlternatingRow1,td.AlternatingRow2").click(function(event) {
        if ($(this).is("label")) {
        var input_elem = $($(this).parent().prev().children("input:radio")[0]);
        } else {
        var input_elem = $($(this).prev().children("input:radio")[0]);
        };
        if (input_elem.is("input:radio") == true) {
            input_elem.attr("checked", "checked");
        };
        if ($(this).is("label")) {
        var input_elem = $($(this).parent().prev().children("input:checkbox")[0]);
        } else {
        var input_elem = $($(this).prev().children("input:checkbox")[0]);
        };
        if (input_elem.is(":checked") == false) {
            input_elem.attr("checked", "checked");
        } else {
            input_elem.removeAttr("checked");
        };
    });
});
