<script type="text/javascript">
	var flipperCategory = "Crossover",
        doAnimation = true,
        toID,
        leftPos = 0,
        firstRow = true,
        adInitialized = false; // need this for incentives

	if (flipperCategory !== "" && flipperCategory.indexOf("/") > 0) {
		flipperCategory = flipperCategory.replace("/","-");
	}

	//should be called by the thought bubble ad server code
    function initCategoryAd() {
        adInitialized = true;
        $(document).ready(function () {

            var clickElement;
            var iFrm = $('#Ad-category-overlay iframe');

            if (iFrm.length > 0) {
                clickElement = $(iFrm[0].contentWindow.document);
            } else {
                clickElement = $("#Ad-category-overlay");
            }


	        if (flipperCategory == "") {
    			return;
    		}
    	
    		var overlayEl = $("#Ad-category-overlay"),
    			adCategoryIcon = $('#Browse-category .' + flipperCategory.toLowerCase());
    		// checks if bubble is inside of classifieds
            if( overlayEl.closest('#Search-wrapper').length > 0) 
                var thisIsClassifieds = true;
            var expandedEl = $('#Ad-Category-overlay-expanded-container');
    		/* only show the thought bubble when slp and show me cars tab is selected on classifieds(not for make/model tab)*/
    		if (!thisIsClassifieds || $("#Tab-no-mm.selected").length === 1) {
    			showAdCategoryBubble();
    		}

    		//$("#Ad-category-overlay").live("click", function () {
    		clickElement.live("click", function () {
    		    overlayEl.hide();
    		    if (typeof KBB !== "undefined" && KBB.gpt && typeof KBB.gpt.isInGptExperience === "function" && KBB.gpt.isInGptExperience()) {
    		        KBB.gpt.refreshAd("categoryoverlayexpanded");
    		    } else {
    		        var url = $('#Ad-category-overlay-expanded-src').val();
    		        $('#Ad-category-overlay-expanded-iframe').attr('src', url);
    		    }
    			expandedEl.show(); // this opens the larger box
   		    });

    		$('body, #classifiedCategoryAdOverlay .close').click(function () {
    			collapseCategoryAd();
    		});

    		$("#Ad-category-overlay-expanded").click(function (e) {
    			e.stopPropagation();
    		});

    		$('#Ad-category-overlay-expanded-iframe').load(function () {
    			// set iframe dimensions to those of the embedded object
    			var h = $(this).contents().find('object, embed').attr('height');
    			if (h !== undefined && h !== $(this).attr('height')) {
    				$(this).attr('height', h);
    			}
    			var w = $(this).contents().find('object, embed').attr('width');
    			if (w !== undefined && w !== $(this).attr('width')) {
    				$(this).attr('width', w);
    			}
    		});

	        // B-40163 - reposition the thought bubble correctly when 'By Category' tab is clicked - incentives can break this otherwise,
            // do it twice because sometimes it's an id, sometimes it's a class
    		$('.browse-category-toggle, #browse-category-toggle').on('click', function () {
    		    showAdCategoryBubble();
    		});

		});

    };
    
    //function to process category overlay
    var initCategoryOverlayHandler = function initCategoryOverlayHandler(e) {
    	if (typeof initCategoryAd === "function") {
    		initCategoryAd();
    	}
    };

    (function () {
    	var channelName = Utils.getMediatorChannel(Utils.eventTypes.showAd, "categoryOverlay");
    	//subscribe to the wallpaper ad from the ad server
    	Utils.getMediator().subscribe(channelName, initCategoryOverlayHandler);
    	//check if the category ad already fired earlier
    	var historyCheck = Utils.getShowAdHistory(channelName);
    	if (historyCheck) {
    		initCategoryOverlayHandler(historyCheck);
    	}
    })();

    // determines position of thought bubble for HP challengers
    function showAdCategoryBubbleHP() {
    	if (flipperCategory == "") {
    		return;
    	}
    	else if (flipperCategory.indexOf("/") > 0) {
    		flipperCategory = flipperCategory.replace("/", "-");
    	}
	    
    	var overlayEl = $("#Ad-category-overlay"),
		    adCategoryIcon = overlayEl.parent().find('li.' + flipperCategory.toLowerCase());
	    
    	setTimeout(function () {

    		if (adCategoryIcon.index() >= 6) {
    			firstRow = false;
    		}

    		adCategoryIcon.addClass('tb-ad');

    		if (overlayEl.length > 0) {
    			$('#Browse-category li').each(function (idx, val) {
    				// pushes down first row only
    				if (firstRow && idx < 6) {
    					$(this).css('margin-top', '50px');
    					//$(this).addClass('ad-row');
    				}
    					// pushes down second row 
    				else if (!firstRow && idx >= 6 && idx < 12) {
    					$(this).css('margin-top', '45px');
    					//$(this).addClass('ad-row');
    				}
    			});
    		}

    		overlayEl.show().css({
    			'left': adCategoryIcon.position().left + 20 + 'px',
    		    'top': adCategoryIcon.position().top - (!firstRow ? 10 : 5) + 'px'
    		}).mouseenter(function () {

    			if (leftPos == 0)
    				leftPos = parseInt(overlayEl.css("left").replace("px", ""));

    			if (doAnimation) {
    				overlayEl.stop(true, true).animate({ left: leftPos - 2 }, 200)
                    .animate({ left: leftPos + 2 }, 200)
                    .animate({ left: leftPos - 2 }, 200)
                    .animate({ left: leftPos + 2 }, 200)
                    .animate({ left: leftPos }, 200);
    			}

    		});
    	}, 1);
    }

    // determines position of thought bubble

    function showAdCategoryBubble() {
    	if (flipperCategory == "") {
    		return;
    	}
    	else if (flipperCategory.indexOf("/") > 0) {
    		flipperCategory = flipperCategory.replace("/", "-");
    	}

        //ABTest B31528
	    if (document.getElementById('browse-category-toggle') != null) {
		    showAdCategoryBubbleHP();
	    } else {
		    //HP Control and Classifieds
		    var overlayEl = $("#Ad-category-overlay"),
			    adCategoryIcon = overlayEl.parent().parent().find('ul.browse-category li.' + flipperCategory.toLowerCase());
		    setTimeout(function() {

			    if (adCategoryIcon.index(this) < 4)
				    firstRow = false;

			    adCategoryIcon.addClass('tb-ad').css('margin-top', (firstRow) ? 35 : 50 + 'px');

			    overlayEl.show().css({
				    'left': adCategoryIcon.position().left + 20 + 'px',
				    'top': adCategoryIcon.position().top - ((firstRow) ? 20 : 5) + 'px'
			    }).mouseenter(function() {

				    if (leftPos == 0)
					    leftPos = parseInt(overlayEl.css("left").replace("px", ""));

				    if (doAnimation) {
					    overlayEl.stop(true, true).animate({ left: leftPos - 2 }, 200)
						    .animate({ left: leftPos + 2 }, 200)
						    .animate({ left: leftPos - 2 }, 200)
						    .animate({ left: leftPos + 2 }, 200)
						    .animate({ left: leftPos }, 200);
				    }

			    });
		    }, 1);
	    }
    }

	function collapseCategoryAd() {
        var overlayEl = $("#Ad-category-overlay");
        var expandedEl = $('#Ad-Category-overlay-expanded-container');
		expandedEl.hide();
		
		if (overlayEl.closest('#Search-wrapper').length == 0 || $("#Tab-no-mm.selected").length === 1) {
            overlayEl.show();
        }
    }

</script>
