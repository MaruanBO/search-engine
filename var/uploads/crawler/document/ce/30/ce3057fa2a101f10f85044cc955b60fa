<script type="text/javascript">

	function showWallpaperContent(imagePath1, imagePath2, backgroundColor, gradientFilter) {
		var leftPanel = "",
			rightPanel = "",
			bgColor = "",
			wrapperElem = document.querySelectorAll(".nc-page-wrapper")[0] || document.querySelectorAll(".wallpaper-wrapper-right")[0];

		// check for valid hex color value
		if (backgroundColor.length == 3 || backgroundColor.length == 6) {
			bgColor = "#" + backgroundColor;
		}

		// for the issue with IBC/enhanced make page for firefox on a mac
		if ((s.pageName == "ymmpathmake_buynew_oem" || s.pageName == "ymmpathmake_none_oem") && document.querySelectorAll('.mac.firefox').length > 0) {
			document.body.style.background = bgColor;
			wrapperElem.style.background = bgColor;
		}
		else {
			if (typeof imagePath1 === "string" && imagePath1.indexOf("//") > 0) {
				leftPanel = "url('" + imagePath1 + "') no-repeat scroll top ";
			}
			if (typeof imagePath2 === "string" && imagePath2.indexOf("//") > 0) {
				rightPanel = "url('" + imagePath2 + "') no-repeat scroll top ";
			}

			document.body.style.background = leftPanel + bgColor;
			document.body.style.backgroundColor = bgColor; // separate background-color declaration to ensure certain browsers to auto fill background with "transparent"
			wrapperElem.style.background = rightPanel;

  		    if (gradientFilter === 'false') {
  		        if ((hasClass(document.documentElement, 'ie9')) || (hasClass(document.documentElement, 'ie9'))) {
  		            document.body.style.filter = 'progid:DXImageTransform.Microsoft.gradient(enabled = false)';
			    }
  		    }

			// only apply background changes if backgrounds exist
			if ((leftPanel.length > 0 || rightPanel.length > 0) && bgColor != "inherit") {
				adjustWPPos();

				$(window).resize(function () {
					adjustWPPos();
				});
			}
		}
	}
	//function to process wallpaper ad template
	var showWallpaperContentHandler = function showWallpaperContentHandler(e) {
		if (typeof showWallpaperContent === "function") {
			showWallpaperContent(e.leftImage, e.rightImage, e.bgColor, e.gradFilter);
		}
	};
	
	(function () {
		var channelName = Utils.getMediatorChannel(Utils.eventTypes.showAd, "wallpaper");
		//subscribe to the wallpaper ad from the ad server
		Utils.getMediator().subscribe(channelName, showWallpaperContentHandler);
		//check if the wallpaper ad already fired earlier
		var historyCheck = Utils.getShowAdHistory(channelName);
		if (historyCheck) {
			showWallpaperContentHandler(historyCheck);
		}
	})();
	
	function adjustWPPos() {
		var contentWidth = $(".page").outerWidth();
		var contentLeftPos = $(".page").offset() || 0;

		if (contentLeftPos && contentLeftPos.left)
			contentLeftPos = contentLeftPos.left;

		var rightPos = contentLeftPos + contentWidth;
		var leftPos = contentLeftPos - 250;
		document.body.style.backgroundPosition = leftPos + "px 0";
		if (document.querySelectorAll('.nc-page-wrapper').length > 0) {
			document.querySelectorAll('.nc-page-wrapper')[0].style.backgroundPosition = rightPos + "px 0";
		}
		else {
			document.querySelectorAll(".wallpaper-wrapper-right")[0].style.backgroundPosition = rightPos + "px 0";
		}

	}

    function hasClass(elem, className) {
        return new RegExp(' ' + className + ' ').test(' ' + elem.className + ' ');
    }

</script>
