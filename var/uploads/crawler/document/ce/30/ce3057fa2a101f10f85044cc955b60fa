<script type="text/javascript">
    function hideTabletTakeoverAd() {
        var $adTabletTakeover = $("#adTabletTakeover");
        $adTabletTakeover.css('display', 'none');
    }

    (function ($, window, document, undefined) {
        var $window = $(window),
            $html = $('html'),
            $adTabletTakeover,
            availWidth = window.screen.availWidth,
            initDelay=1500;

        //init position
        var setPosition = function() {
            $adTabletTakeover = $("#adTabletTakeover");
            if (availWidth <= 800 && $html.hasClass('android')) {
                //if 7inch android device, use position absolute and jquery to find position
                $adTabletTakeover.css('position', 'absolute');
                rePosition();
                //event listeners
                $window.scroll(function(){rePosition()}).resize(function(){rePosition()});
            } else {
            //or else use position fixed
                $adTabletTakeover.css({ 'position': 'fixed', 'bottom': 0 });
            }
        };

        //find new top position
        var rePosition = function() {
            var windowHeight = $window.height(),
                scrollTop = $window.scrollTop(),
                adHeight = $adTabletTakeover.height(),
                newTop = (windowHeight + scrollTop) - adHeight;
            $adTabletTakeover.css('top', newTop);
        };

        //delay until ad loads
        var t = setTimeout(function () {
            setPosition();
        }, initDelay);
    })(jQuery, window, document, undefined);
</script>

