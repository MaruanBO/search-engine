function toggleMembershipSummary(){var dropDownObj=$('#dashboardDropdownPopup');var loggedinCheck=$('#dashboardDropdownPopup form#formLogin');if(dropDownObj.hasClass('showNav')&&loggedinCheck.length==0){$('#membershipSummary').addClass('showNav');doDynamicResize(true);}
else{$('#membershipSummary').removeClass('showNav');doDynamicResize(false);}}
function doDynamicResize(arg){var dashboardTitleHeight=$('#dashboardTitle').height();var dashboardDropdownPopupTopFixedHeightVal=28;dashboardTitleHeight+=dashboardDropdownPopupTopFixedHeightVal-12;var dashboardTitleWidth=$('#dashboardTitle').width();var dashboardTitleWidthFixedWidthVal=44;dashboardTitleWidth+=dashboardTitleWidthFixedWidthVal;if(!arg){if($.browser.msie&&$.browser.version=="9.0"||$('html').hasClass('ie9')){$('#dashboardDropdownPopup').css('top',dashboardTitleHeight+1);}
else
{$('#dashboardDropdownPopup').css('top',dashboardTitleHeight);}
if(dashboardTitleWidth>=248){$('#dashboardDropdownPopup').css('width',dashboardTitleWidth);}else{$('#dashboardDropdownPopup').css('width','248px');}}else{$('#dashboardTitle').css('padding-bottom','0px')
dashboardTitleHeight-=10;$('#dashboardDropdownPopup').css('top',dashboardTitleHeight);$('#dashboardDropdownPopup').css('width','auto')}}});$('#dashboardTitle').hover(function(){$('#dashboardTitle').css('color','#333333');$('#membershipSummary').css('color','#666666');},function(){$('#dashboardTitle').css('color','#666666');})
