<script language="javascript">

	

	function _hbOnLink(a){

		if (a != null && 

			(a.href != null && a.href.indexOf("BuyProduct") > -1) || 

			(a.name != null && a.name.indexOf("NO_TRACK") > -1) ) {

          _hbHalt()

       }

	}

	

	function xrx_hc2() {

		var cmpTrax = document.location.search.match(/[?&](CMP|ppcseid)=([^&]+)/i);

		

		if (typeof(Cookie) != "undefined")

		{		

			var cookie  = new Cookie('TEKSESSION', '/', document.domain);



			if (cookie == null && cmpTrax == null) return '';



			var srcTrax   = document.location.search.match(/[?&]source=([^&]+)/i);

			var cmpCode   = cmpTrax != null ? cmpTrax[2].toUpperCase() : '';

			var isPromo   = cmpTrax != null && cmpCode.indexOf('ILC') != 0 && cmpCode.indexOf('OBC') != 0 ? true : cookie.get('is_promo') != null && cookie.get('is_promo') == '1';

			var hc2Source = srcTrax != null ? srcTrax[1] : cookie.get('source') != null ? cookie.get('source') : isPromo ? 'TRAX' : '';



			if ( cmpTrax != null ) {

				var source = srcTrax != null ? srcTrax[1] : null;



				update_cookie(isPromo, source);



				// doubly ensure we set the cookie...

				if ( typeof(jQuery) != 'undefined' ) {

					jQuery(window).unload(function(){update_cookie(isPromo, source)});

				}

			}



			return isPromo ? 'CMP_'+hc2Source : '';

		}

		

		return "";

	}

	function update_cookie(isPromo, source) 

	{

		if (typeof(Cookie) != "undefined")

		{

			var cookie = new Cookie('TEKSESSION', '/', document.domain);



			if (cookie == null) return;



			if ( source != null ) {

				cookie.set('cpid.origsrc', source);

				cookie.set('cpid.websrc',  source);

			}



			if (isPromo) cookie.set('is_promo', '1');



			document.cookie = cookie.bake();

		}

	}

</script>	

