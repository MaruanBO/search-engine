//clear field on focus
$(document).ready(function(){
	// clear input on focus for dates and search
	$('#search-advanced').focus(function(){
		if($(this).val()=="search"){
			$(this).val("");
		}
	});

	// if field is empty afterward, add text again
	$('#search-advanced').blur(function(){
		if($(this).val()==""){
			$(this).val("search");
		}
	});

	$('#search').focus(function(){
		if($(this).val()=="search"){
			$(this).val("");
		}
	});

	// if field is empty afterward, add text again
	$('#search').blur(function(){
		if($(this).val()==""){
			$(this).val("search");
		}
	});

	$('#date-range-from').focus(function(){
		if($(this).val()=="MM/DD/YYYY"){
			$(this).val("");
		}
	});

	// if field is empty afterward, add text again
	$('#date-range-from').blur(function(){
		if($(this).val()==""){
			$(this).val("MM/DD/YYYY");
		}
	});

	$('#date-range-to').focus(function(){
		if($(this).val()=="MM/DD/YYYY"){
			$(this).val("");
		}
	});

	// if field is empty afterward, add text again
	$('#date-range-to').blur(function(){
		if($(this).val()==""){
			$(this).val("MM/DD/YYYY");
		}
	});
});
</script>


