var resizeWithAspectRatio = function(maxWidth, maxHeight, width, height) {
    var difference = width / height,
        ratio;
    console.log(difference);
    ratio = Math.min(maxWidth / width, maxHeight / height);
    return { width: (width * ratio), height: ((width * ratio) / difference) };
};
	function a() {
		    $("iframe").each(function() {
		        var e = $(this),
		            d = e.data("proportion"),
		            b = e.attr("width"),
		            c = e.width();
		        if (!d) {
		            d = e.attr("height") / b;
		            e.data("proportion", d)
		        }
		        if (c != b) {
		            e.css("height", Math.round(c * d) + "px")
		        }
		        try {
			        if ($(window).width() <= 560) {
				        if (e.contents().find('.flash_content').length) {
				        	window.setTimeout(function(m) {
				        	try {
					        	e.contents().find('.jwplayer')[0].style.width = "100%";

