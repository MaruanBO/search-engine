<script language="JavaScript" type="text/javascript">
var slide = 1;
var slideRunning = false;
var sliderTimer = 0;
function setSlide(newSlide) {
	for(i=1; i <= 6; i++) {
		if(i == newSlide)
			$('#player_btn_sm_' + i).attr('src', "http://cdn3.stopagingnow.com/images/player_btn_sm_" + i + "_on.jpg");
		else
			$('#player_btn_sm_' + i).attr('src', "http://cdn3.stopagingnow.com/images/player_btn_sm_" + i + ".jpg");
	}
	slide = newSlide;
	var a_idx = 'a:eq(' + (slide-1) + ')';
	var imgName   = "http://cdn3.stopagingnow.com/images/player-20150130/player_flyer_" + newSlide + ".jpg";
	var imgNamAlt = $('#player_slides ' + a_idx + ' img').attr('alt');
	var imgMessage = $('#player_slides div:eq(' + (slide-1) + ')').html();
	var href =      $('#player_slides ' + a_idx).attr('href');
	$('#player_image').attr('src', imgName);
	$('#player_image').attr('alt', imgNamAlt);
	$('#player_link').attr('href', href);
	$('#player_note').html(imgMessage);
}
function nextSlide() {
	stopSlideTask();
	var newSlide = slide+1;
	if(newSlide > 6)
		newSlide = 1;
	setSlide(newSlide);
	return false;
}
function prevSlide() {
	stopSlideTask();
	var newSlide = slide-1;
	if(newSlide < 1)
		newSlide = 6;
	setSlide(newSlide);
	return false;
}
function pickSlide(newSlide) {
	stopSlideTask();
	setSlide(newSlide);
	return false;
}
function toogleSlideTask() {
	if(slideRunning)
		stopSlideTask();
	else
		startSlideTask();
	return false;
}
function stopSlideTask() {
	if(slideRunning) {
		$('#player_btn_sm_play').attr('src', "http://cdn3.stopagingnow.com/images/player_btn_sm_play.jpg");
		clearInterval(sliderTimer);
		sliderTimer = 0;
		slideRunning = false;
	}
	return false;
}
function startSlideTask() {
	if(!slideRunning) {
		$('#player_btn_sm_play').attr('src', "http://cdn3.stopagingnow.com/images/player_btn_sm_play_on.jpg");
		slideRunning = true;
		sliderTimer = setInterval(setSlideTask, 8000);
	}
	return false;
}
function setSlideTask() {
	var newSlide = slide + 1;
	if(newSlide > 6)
		newSlide = 1;
	setSlide(newSlide);
}
setSlide(1);
startSlideTask();
</script>
