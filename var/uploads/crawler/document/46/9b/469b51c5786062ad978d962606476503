<script type="text/javascript">
//open request a catalog dialog if user clicks "Request a Catalog" link
$("a#fancyBoxLink").fancybox({
	'href'   : '#fancyboxCatalog',
	'titleShow'  : false,
	'transitionIn'  : 'elastic',
	'transitionOut' : 'elastic'
});
//open request a catalog dialog if user chooses not to have email  
$('a#confirm_unsub_template').click(function(){
	openSubFancyBox("a#confirm_unsub_template");
}); 
//open request a catalog dialog if user chooses to have email
$('a#cancel_unsub_template').click(function(){
	document.getElementById('frm_offers_enews_optin_template').checked = true;
	openSubFancyBox("a#cancel_unsub_template");
});    

function openSubFancyBox(nameOfLink){
	$(nameOfLink).fancybox({
		'href'   : '#fancyboxCatalog',
		'titleShow'  : false,
		'transitionIn'  : 'elastic',
		'transitionOut' : 'elastic'
	});
}   
</script>
<script language="JavaScript" type="text/javascript">
function fix_checkboxes() {
	$('#emailCheckBoxes').toggle(!$('#frm_email1').val() == '');
}
function checkFormCatReqPopup() {
	var form = $('#freelitForm');
	$("#frm_email1").val($.trim($("#frm_email1").val()));
	focusField =  null;
	var error  = false;
	error |= checkField('frm_first_name', isBlank);
	error |= checkField('frm_last_name', isBlank);
	error |= checkField('frm_address', isBlank);
	error |= checkField('frm_city', isBlank);
	error |= checkField('frm_zip', isBlank);
	if(!isBlank($('#frm_email1').val()))
		error |= checkField('frm_email1', isNotEmail);
	if(!$('#frm_zip').val().match(/^\d{5}/)) {
		error = true;
		$('#frm_zip').addClass('inputError');
	} else {
		$('#frm_zip').removeClass('inputError');
	}
	if (focusField != null) {
		alert("Please correct all the highlighted fields");
		focusField.focus();
	}
	if(!error) {
		if($('#frm_state').val() == "") {
			alert("Please select a state");
			$('#frm_state').focus();
			error = true;
		}
	}		
	focusField =  null;
	if(!error) {
		$('#subButton').hide();
		$('#subSpinner').show();
		form.submit();
	}
	return false;
}
$(document).ready(function(){
	$('#frm_offers_enews_optin_template').on('change', function(){
		if(!$('#frm_offers_enews_optin_template').is(':checked')) {
			$.fancybox.open([{
				href: '#unsubscribe_dialog_template',
				fitToView	: false,
				width		: '515',
				height		: '335',
				autoSize	: false,
				closeClick	: false,
				openEffect	: 'none',
				closeEffect	: 'none',
				afterShow   : function(){
					$('#confirm_unsub').click(function(e){
						e.preventDefault();
						$.fancybox.close();
					});
					$('#cancel_unsub').click(function(e){
						e.preventDefault();
						$('#frm_offers_optin').prop('checked', true);
						$.fancybox.close();
					});
					$("#frequency_submit").one('click', function(e){
						e.preventDefault();
						if(!isEmail(document.frequency.frm_email.value)) {
							alert('Please enter a valid  Email address');
							return false;
						}
						$.ajax({
							async: false,
							type: "POST",
							url:  $('#frequency').attr('action'),
							data: $('#frequency').serialize(),
							cache: true,
							success: function(data){
								alert('Your email subscription frequency has been updated.');
								$.fancybox.close();
							},
							error: function(){
								alert("error");
							}
						});
						goToThisFunctionHere();
						return false;
					});
				},
				afterClose	:	function(){
					$.fancybox.open([{
						href: '#fancyboxCatalog',
						titleShow	: false,
						transitionIn	: 'elastic',
						transitionOut	: 'elastic'
					}], {});
				}
			}], {});
		}
	});
});

function goToThisFunctionHere(){
	document.getElementById('frm_offers_enews_optin_template').checked = true;
	$.fancybox.open([{
				href: '#fancyboxCatalog',
				titleShow	: false,
				transitionIn	: 'elastic',
				transitionOut	: 'elastic'
			}], {});
}
</script>
