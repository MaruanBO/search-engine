var cat_selected = "";
$('#navi_products a').each(function(i, o){
	var color = '#b3cfe6';
	var $a = $(o);
	var href = $a.attr('href');
	if(href == '') {
		$a.css('background-color', color);
		var $li = $($a.parent().get(0));
		$li.css('background-color', color);
	}
});
