<script>
	var attemptedLogin = false;
  window.fbAsyncInit = function() {
		
	
	    	FB.init({appId: '3119980345', cookie: true, xfbml: true, oauth: true});


	
						FB.Event.subscribe('auth.sessionChange', function(response) {
			    if (response.session) {
			      // A user has logged in, and a new cookie has been saved
					delayLogin();
			    } 
			  });

			FB.Event.subscribe('auth.statusChange', function(response) {
			    if (response.session) {

			      // A user has logged in, and a new cookie has been saveD
					delayLogin();
			    }
			  });
			
				FB.Event.subscribe('auth.login', function(response) {
					
					delayLogin();
				});
			
  };


  (function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());


	function delayLogin() {
		setTimeout("fbConnectLogin()",1000);
	}

	function fbConnectLogin() {
		if(attemptedLogin) return; // don't call this multiple times in a row
		attemptedLogin = true;
		
					var iframe = document.getElementById("fb-iframe");
			if(iframe) {
				iframe.src = "/user/login-lightbox?promptLightbox=true";
			} else {
				showLogin();
			}
			}
</script>
