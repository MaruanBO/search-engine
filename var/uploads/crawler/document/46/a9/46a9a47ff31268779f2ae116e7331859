<script type="text/javascript">

    $(document).ready(function () {
        if (path.indexOf("search")  > -1) {
            $(".testtitle").addClass("mobile-title");
        }
        else {
            $(".testtitle").removeClass("mobile-title");
        }

    });

    $(document).ready(function () {
        if ("census" == "tree") {
            $("#tree").show();
            $("#search").hide();
        }
        else
        {
            $("#tree").hide();
            $("#search").show();
        }

    });

    var url = "";

    function PostInfo() {
        url = "";

        $("input:visible, select:visible").each(function (i, e) {
            if ($(this).val() != $(this).attr("placeholder")) {
                url += "&" + $(this).attr("id") + "=" + $(this).val();
            }
        });

        if ("census" == "phone") {
            PostPhoneInfo();
        } else if ("census" == "address") {
            PostAddressInfo();

        } else if ("census" == "email") {
            PostEmailInfo();

        } else if ("census" == "geneadir") {
            PostGeneaInfo();
        }
        else if ("census" == "tree") {
            PostFamTree();
        }

        else if ("census" == "people" || "census" == "") {
            url = "/search/searchpreview.aspx?type=census" + url;
            window.location = url;
        }
        else {
            Genealogy();
        }
    }


    function PostEmailInfo() {
        $('#divValidationError').hide();

        var email = $('input.email:visible').val();

        if (email == "") {
            $('#divValidationError').show();
            return false;
        }


        if ("True".toLowerCase() == "true") {
            window.location = "/email/" + email;
        }
        else {
            window.location = "/search/searchpreview.aspx?type=email&q=" + email;
        }


        return false;
    }

    function PostGeneaInfo() {
        $('#divValidationError').hide();

        var fName = $('input.fNm:visible').val();
        var lName = $('input.lNm:visible').val();
        //               if (email == "") {
        //                   $('#divValidationError').show();
        //                   return false;
        //               }


        //               if ("True".toLowerCase() == "true") {
        //                   window.location = "/email/" + email;
        //               }
        //               else {
        window.location = "/directory/genealogy.aspx?q=" + email;
        // }


        return false;
    }

    function PostAddressInfo() {

        $('#divValidationError').hide();

        var addr = $('input.addr:visible').val();
        var city = $('input.city:visible').val();
        var state = $('select.state:visible').val();

        var house = "";
        var street = "";

        var arr = addr.split(' ');
        if (arr.length > 1) {
            house = arr[0];

            for (var i = 1; i < arr.length; i++) {
                street += arr[i] + " ";
            }

            street = $.trim(street);

        }

        street = $.trim(street);
        /*
        if (city == "" || state == "") {
        $('#divValidationError').show();
        return false;
        }
        */

        if ("True".toLowerCase() == "true") {
            window.location = "/address/" + house + "/" + street + "/" + city + "/" + state;
        }
        else {
            window.location = "/search/searchpreview.aspx?type=address&q=" + house + "/" + street + "/" + city + "/" + state;
        }

        return false;
    }

    function PostPhoneInfo() {

        var ac = $('input.phareacode:visible').val();
        var pre = $('input.phprefix:visible').val();
        var post = $('input.phsuffix:visible').val();

        var phone = ac + pre + post;

        var cleanphone = phone.replace(/[^\d]/g, "");

        if ("True".toLowerCase() == "true") {
            window.location = "/phone/" + cleanphone;
        }
        else {
            window.location = "/search/searchpreview.aspx?type=phone&q=" + cleanphone;
        }

        return false;
    }

    function Genealogy() {
        url = "";
        $("input:visible, select:visible").each(function (i, e) {
            if ($(this).val() != $(this).attr("placeholder")) {
                url += "&" + $(this).attr("name") + "=" + $(this).val();
            }
        });

        if ("True".toLowerCase() == "true") {
            window.location = "/census/ohmResults.aspx?" + url;
        }
        else {
            window.location = "/search/ohmResults.aspx?type=census" + url;
        }


        return false;
    }

    function PostFamTree() {
        var fn = $("#firstNm").val();
        var mn = $("#middleNm").val();
        var ln = $("#lastNm").val();
        var dob = $("#birthDate").val();
        var needsAccount = $("#needsAccount").val();
        var gender = $("#gender").val();

        if (!isEmpty(ln)) {
            window.location = "/FamilyTree/NewFamilyTree.aspx?fn=" + fn + "&mn=" + mn + "&ln=" + ln + "&dob=" + dob + "&needsAccount=" + needsAccount + "&gender=" + gender;
 
        }

        return false;
    }

    $(function () {
        $('.fn').val("");
        $('.mn').val("");
        $('.mi').val("");
        $('.ln').val("");
        $('.city').val("");
        $('.state').val("");
        $('.age').val("");
        $('.addr').val("");
        $('.email').val("");
        $('.phareacode').val("");
        $('.phprefix').val("");
        $('.phsuffix').val("");
        $('[name=fn]').val("");
        $('[name=mn]').val("");
        $('[name=mi]').val("");
        $('[name=ln]').val("");
        $('[name=city]').val("");
        $('[name=state]').val("");
        $('[name=age]').val("");
        $('[name=addr]').val("");
        $('[name=relative]').val("");
        $('[name=county]').val("");
        $('[name=dob]').val("");

        $(window).resize(function () {
            if ($(window).width() > 600) {
                $(".side2").css('display', 'inline-block');
//                $(".menu-drop").show();
                $(".SearchMenu").show();

                
            }
            else if ($(window).width() < 600) {
                $(".side2").css('display', 'block');
            }

        });
        /*$(window).resize(function () {
        var url = window.location.pathname;
        if (url = "search/searchpreview.aspx?") {

        if ($(window).width() < 480) {
        $(".form-inline").css("display", "none");
        $(".searchNav").css("display", "none");
        }

        else if ($(window).width() > 480) {
        $(".form-inline").css("display", "block");
        $(".searchNav").css("display", "block");
        }
        }
        });*/

        $(".prefilled").blur();

    });

    
</script>


         
