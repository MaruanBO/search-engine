<script type="text/javascript">
    (function (amc) {

        amc.require.now(function () {
            amc.user.info.init({ inlineDataId: 'user-info-inline-data' });

            var status = amc.user.info.loginStatus();

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({ 'loginStatus': status });

            if (status != 'Anonymous')
                document.getElementsByTagName('html')[0].className += ' authenticated' + (status == 'Loyalty' ? ' authenticated-with-loyalty' : '');
        });
    })(amc);
</script>

