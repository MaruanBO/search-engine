<!--  <script src="http://www.boots.com/wcsstore/ConsumerDirectStorefrontAssetStore/javascript/geotargeting/jquery-1.3.2.min.js" type="text/javascript"></script> -->
<script src="http://www.boots.com/wcsstore/ConsumerDirectStorefrontAssetStore/javascript/geotargeting/jquery.dd.js" type="text/javascript"></script>
<script src="http://www.boots.com/wcsstore/ConsumerDirectStorefrontAssetStore/javascript/geotargeting/geotargeting.js" type="text/javascript"></script>
<script language="javascript">
	jQuery(document).ready(function($){
	    $("#countries").msDropdown();
	    var geoOpts = getQueryStringParams('geoOpts');
	
		if (geoOpts == 'Y') {
			$("#showDialogue").click();
		} else {
			document.getElementById("geo_overlay").style.display = 'none';
		}
		var geoCode = getQueryStringParams('geoCode');
		if (typeof(geoCode) != 'undefined') {
			var cookieValue = geoCode;
			if(geoCode != "GB" && geoCode != "IE") {
				cookieValue = "INT:"+ geoCode;
			}
			if (typeof(cookieValue) != 'undefined') {
				var now = new Date();
				var time = now.getTime();
				time += 3600 * 1000 *24 * 365;
				now.setTime(time);
				document.cookie = 
	    			'cookiePrefDomain=' + cookieValue + 
	    			'; expires=' + now.toGMTString() + 
	    			'; path=/';
			}
		}
		document.cookie = "deepLinkVisited=true; path=/";
		
		$("#countries").change(function() {
	  		var host = window.location.hostname;
	    	var eSelect = document.getElementById('countries');
			var str = eSelect.value;
			var strArr = str.split("|");
			var countryCode = strArr[0];
			var url = strArr[1];
			var onw = strArr[2];
			if(onw == "*") {
				$("div#warning_text_spacer").show();
				$("div#warning_text").hide();
			} else {
				host = "http://"+host;
				if(host != url) {
					$("div#warning_text").show();
	  				$("div#warning_text_spacer").hide();
				}
			}
	  		return false;
  		});
	});
</script>


