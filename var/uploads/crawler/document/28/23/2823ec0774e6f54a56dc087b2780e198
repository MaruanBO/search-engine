</script><script>

	var pageName = "Home page (11051)";
	
	function reportClickEvent(href, linkName) {
		reportToCM(href,linkName,pageName);
		return false;
	
	}
	function reportClickEventAndRedirect(href, linkName,redirectURL) {
		try{
			reportToCM(href,linkName,pageName);
		}catch(e){;}	
		window.location = redirectURL;
		return false;
	}
	function reportFormClickEvent(href, linkName,button) {
		try{
			reportToCM(href,linkName,pageName);
		}catch(e){;}
		button.click();
		return false;		
	}
	
	function reportFormClickEventWithURL(href, linkName, button, url) {
		var orderType = 'false';
		try{
			if(false){
				orderType = '';
			}
			// only for Pharmacy Order type ( FRPS & OFPS ) - IMA cannot be updated.

			if (orderType == 'true') {
				// only for pharmacy
				document.getElementById("URL").value =  url;
			}
			reportToCM(href,linkName,pageName);
		}catch(e){;}
		button.click();
		return false;		
	}

	function reportToCM(href,linkName,pageName) {
		
			cmCreateManualLinkClickTag(href, linkName,pageName );
			//alert("cmCreateManualLinkClickTag("+href+", "+linkName+","+pageName+" ); ");

		
	}
	
	//uses a form object to set the action, URL and to submit
	function Add2WishListTaggedForm(form, catEntryId, productName) {
		if (!busy) {
			busy = true;
			createWishListLinkTag(productName);
			form.action = window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/InterestItemAdd";
			form.URL.value = "InterestItemDisplay";
			form.submit();
		}
	}
	
	//creates the tag and navigates to the Wish List page
	function Add2WishListTagged(catEntryId, productName) {
		var url = "";
		if (!busy) {
	      busy = true;
	      createWishListLinkTag(productName);
	      url = window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/InterestItemAdd?";
	      url = url + "catEntryId="+catEntryId+"&storeId=10052&langId=-1&catalogId=11051"+"&URL=InterestItemDisplay";
	      document.location=url;
	   }
	}
	
	//creates the tag and stays on the current page
	function Add2WishListTaggedReload(catEntryId, productName) {
		var url = "";
		if (!busy) {
			currUrl = escape(document.location);
			busy = true;
			createWishListLinkTag(productName);
    		url = window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/InterestItemAdd?";
    		url = url + "catEntryId="+catEntryId+"&storeId=10052&langId=-1&catalogId=11051"+"&errorViewName=ProductDisplayErrorView"+"&URL=" + currUrl;
    		document.location=url;
 		}
	}
	
	function createWishListLinkTag(productName){
		
			//alert("CMRealEstateReport: createPageTag(productName)");
			cmCreatePageviewTag(productName, "WISHLIST");
		
	}

</script><!--CMPageView End -->
	
</body>
</html>
