<script tye="text/javascript">
function cmCheckIgnoreImpression(sp,re,cr,me) {if (sp && sp.indexOf("supermenu-_-") > -1) {return false;}return true;}
function flashSupermenuClickEvent(hrefVal,linkname, spValue){
		var href = hrefVal.getAttribute("href");
		if(href!=null && href.indexOf("?")>0)
		{
			href+="&manual_cm_sp="+spValue
		}else{
			href+="?manual_cm_sp="+spValue
		}
		cmCreateManualLinkClickTag(href, linkname,pageName );

}

function flashSupermenuAndSetLinkDetail(hrefVal,linkname, spValue,LinkType,categoryName){
  flashSupermenuClickEvent(hrefVal,linkname, spValue);
  setLinkDetail(LinkType,categoryName);
}
</script>


