<SCRIPT LANGUAGE = JavaScript>

	function getCurrentCountry(locale,activeClass)
	{
		if(locale == "en_gov" || locale == "en_sb" || locale == "en_efx" )
		{
			locale = "en_us";
			activeClass = "US";
		}
		
		var auxChild= '';
		var parent = document.getElementById("countryDropDownList");
		var ddList = parent.getElementsByTagName("dd");
		var ddToRemove = new Array();
		var toRemoveCount = 0;
		for(var i=0;i<ddList.length;i++)
		{
			if (ddList[i].className == activeClass)
			{									
				if (ddList[i].id == locale)
				{
					auxChild=ddList[i];
				}
				else
				{
					ddToRemove[toRemoveCount]=ddList[i];
					toRemoveCount=toRemoveCount+1;
				}
			}			
		}
		for(var i=0;i<ddToRemove.length;i++)
		{
			parent.removeChild(ddToRemove[i]);
			parent.insertBefore(ddToRemove[i],parent.getElementsByTagName("dd")[0]);
		}
		if	(auxChild!='')
		{
			parent.removeChild(auxChild);
			parent.insertBefore(auxChild,parent.getElementsByTagName("dd")[0]);
		}
	}		
	getCurrentCountry('en_us','US');		
	
</SCRIPT>

