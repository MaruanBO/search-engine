<script>
  globalskycom.config.nav.host = 'assets.sky.com';
  globalskycom.config.nav.defaults.signUpUrl = "https://skyid.sky.com/signup/";
  globalskycom.config.nav.defaults.signInUrl = "https://skyid.sky.com/signin/";
  globalskycom.config.nav.defaults.signOutUrl = 'https://skyid.sky.com/signout?successUrl=' + globalskycom.pageUrl;
