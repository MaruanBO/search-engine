            },
            tracking: {
                site: "skycom",
                section: "home/hp-main/uk",
                headline: "",
                contentType: "home",
                contentId: "",
                account: "bskybhp,bskybnetworkglobal,bskybeexperienceglobal",
                
                mpaccount: "bskybhp,bskybmpprod,bskybeexperienceglobal",
                loadVariables: {'testAndTarget': hp.trackingMastheadType()}
            },
            neverMiss: {
                cancelUrl: "http://www.sky.com/hp/nevermiss/cancel",
                successUrl: "http://www.sky.com/hp/nevermiss/success",
                api: "http://epgservices.sky.com/8.0.1/api/2.1/nevermiss/json",
                title: "signin"
            },
            urls: {
                signIn: "https://skyid.sky.com/signin/?appearance=compact&successUrl=&cancelUrl="
            }
        };

        
            if(hp.sequenceCarousel()) {
                window.hpConfig.tracking.loadVariables['SequencingCarousel'] = 'homepage-sequenceCarousel'
            }
        

        
        

        var SKY_TRACKING = window.hpConfig.tracking;

