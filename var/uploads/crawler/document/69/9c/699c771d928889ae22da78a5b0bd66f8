		//remove all empty p tags
		$('.teasersBlock p:empty').remove();
			
		//scan all teaser blocks
		$(".teasersBlock").find("p:first").each(function(){
				
			//grab current height
			var current_number = $(this).height();
				
			//assign highest number to global var
			if(current_number >= highest_num){
					
				highest_num = current_number;
					
			}
				
		});
			
		//once we have our max number set all divs to that height
		$(".teasersBlock").each(function(){
				
			//set height
			$(this).find("p:first").css('height', highest_num);
				
		});	
			
	}

});
}(jQuery));

//--><!]]>
</script>    </div>
</div>    </div>
  
  
  
  
  
  
</div>        </div>
