<script type="text/javascript">
	var search_button_submit_check = function( search_value )
	{
		search_value = search_value.toLowerCase();
	
		var search_term_array = new Array(
			'recherche',	// BE
			'busca',	// BR
			'vyhledat', 	// CZ
			'søg',		// DK
			'search', 	// EN
			'buscar',	// ES
			'haku',		// FI
			'søk',		// NO
			'sök',		// SE
			'搜尋',		// TW
			
			''		// NULL
		);
	
		var result = $.inArray( search_value, search_term_array );
	
		// If search_value was not in the array, it's safe
		if( result == -1 )
		{
			return true;
		}
		
		return false;
	}
</script>

