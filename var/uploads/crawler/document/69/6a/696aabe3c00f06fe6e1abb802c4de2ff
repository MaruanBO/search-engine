<script>
$(function() { 
	$('a[href^="#"][href*="@"]').not('ul.tabs a[href^="#"]').bind('click', function(e){
	
		var pieces = $(this).attr('href').split( '@' );
		var tab_id = pieces[0];
		var accordian_index = parseInt( pieces[1] );
		$("ul.tabs").data( "tabs" ).click( tab_id );
			 $("div.tab-content").each(function (index, domEle) {
			        // domEle == this
			        
			        if ($(this).attr('id') == tab_id) {
			          $("section.accordion", this).each(function (index, domEle) {
	
			        	  $(this).data( "tabs" ).click(accordian_index);
			        	  }) 
			          }
			      });
		return false;		
	});
});

</script>

