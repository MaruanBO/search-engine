	$(document).ready(function() {
	
	$("#slidetabs").tabs(".items > div", {
	
		// enable "cross-fading" effect with 'fade'
		effect: 'horizontal',
		fadeOutSpeed: "slow",
	
		// start from the beginning after the last tab
		rotate: true
	
	// use the slideshow plugin. It accepts its own configuration
	}).slideshow({clickable:false, autoplay: true, interval: 6500});
	
	});
</script>

<script>
var check = $.cookie( 'show_alert_box' );
var day   = '01';
var month = '01';
var year  = '2010';
if (typeof expiry_date !== 'undefined'){
	day   = expiry_date.substring(0,2);
	month = expiry_date.substring(3,5);
	year  = expiry_date.substring(6,10);
}
var myDate = new Date(year, month - 1, day);
var today = new Date();
if (myDate > today) {
    if( check == 'true' ){
	  $("aside#alert").hide();
	}
	else{
	  	// Display the alert bar
		$("#alert-close").click(
			function () {
				$('aside#alert').slideUp("fast");
	  			$.cookie( 'show_alert_box', 'true', { expires: 7, path: '/' } );
			}
		);
	}
}
else {
    $("aside#alert").hide();
}
</script>


<script type="text/javascript">
var Open_Close_Fix = function( button_classname, open_text, close_text )
{
	this.open_state = false;
	this.open_text = '.' + open_text;
	this.close_text = '.' + close_text;
	this.button_classname = '.' + button_classname;
	
	// Setup a pointer back to this class for use in events
	var self = this;
	
	// Bind the click event on our buttons to this class's click() function
	$(this.button_classname).unbind( 'click' ).bind( 
		'click', 
		function() {
			self.click( open_text, close_text );
		}
	);
	
}

Open_Close_Fix.prototype.click = function( open_text, close_text )
{
	if( !this.open_state )
	{
		this.classname = '.' + 'pane';
		this.open_all_tabs();
		this.classname = '.' + 'pane-title';
		this.open_all_titles();
		var allp = document.getElementsByTagName('p');
		var text = "";
		var pchildren; 
		for (var i=0; i<allp.length; i++) {
			pchildren = allp[i].innerHTML;
			if (pchildren == open_text) {
				document.getElementsByTagName('p')[i].innerHTML = close_text;
			}
		}
	}
	else
	{
		this.classname = '.' + 'pane';
		this.close_all_tabs();
		this.classname = '.' + 'pane-title';
		this.close_all_titles();
		var allp = document.getElementsByTagName('p');
		var text = "";
		var pchildren; 
		for (var i=0; i<allp.length; i++) {
			pchildren = allp[i].innerHTML;
			if (pchildren == close_text) {				
				document.getElementsByTagName('p')[i].innerHTML = open_text;
			}
		}		
	}
	
	this.open_state = !this.open_state;
}


Open_Close_Fix.prototype.open_all_tabs = function()
{
	$(this.classname).css( 'display', 'block' );
}

Open_Close_Fix.prototype.open_all_titles = function()
{
	$(this.classname).addClass("current");
}

Open_Close_Fix.prototype.close_all_tabs = function()
{
	$(this.classname).css( 'display', 'none' );
}
Open_Close_Fix.prototype.close_all_titles = function()
{
	$(this.classname).removeClass("current");

}
</script>

<script type="text/javascript">
$(document).ready( 
	function() {
	var open_close_fix = new Open_Close_Fix( 'show_all_button', open_all_fix, close_all_fix );
	} 
);
</script>




<!-- additional scripts -->
<script>
    var current = $();

    $("dt, dd").each(function() {
        var $this = $(this);
        if ($this.is('dt')) {
            doTheWrap(current);
            current = $();
        }
        current = current.add(this);
    });
    doTheWrap(current);

    function doTheWrap(d) {
        d.wrapAll('<article></article>');
    }
</script>



<script language='JavaScript1.1' src='http://pixel.mathtag.com/event/js?mt_id=120179&mt_adid=100655&v1=&v2=&v3=&s1=&s2=&s3='></script>

<script>
    // -------------
    // sets a cookie
    // -------------
    function setCookie(cookieName, cookieValue, nDays) {
        var today = new Date();
        var expire = new Date();
        if (nDays == null || nDays == 0) {
            nDays = 1;
        }
        expire.setTime(today.getTime() + 3600000 * 24 * nDays);
        document.cookie = cookieName + "=" + escape(cookieValue) + ";expires=" + expire.toGMTString();
    }

    // --------------
    // Gets a cookie
    // --------------

    function getCookie(cookie_name) {
        var i, x, y;
        var cookies = document.cookie.split(";");
        for (i = 0; i < cookies.length; i++) {
            x = cookies[i].substr(0, cookies[i].indexOf("="));
            y = cookies[i].substr(cookies[i].indexOf("=") + 1);
            x = x.replace(/^\s+|\s+$/g, "");
            if (x == cookie_name) {
                return unescape(y);
            }
        }

        return -1;
    }

    // -------------------------------------------
    // Checks user agent. If device is android, it 
    // calls setCookie method and redirects
    // -------------------------------------------
    function checkAndroid() {
        var ua = navigator.userAgent.toLowerCase();
        var isAndroid = ua.indexOf("android") > -1; //&& ua.indexOf("mobile");

        if (isAndroid) {

            if (getCookie('tm_home_android')) {
                return;
            }

            var cookieName = 'tm_home_android';
            var cookieValue = 'active';
            var expire = 14;
            setCookie(cookieName, cookieValue, expire);
            window.location = 'http://housecall.trendmicro.com/us/android-device/';
        }
    }

    window.onload = checkAndroid();
</script>
