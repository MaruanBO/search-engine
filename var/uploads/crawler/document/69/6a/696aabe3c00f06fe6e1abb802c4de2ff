<!-- Floating share bar js -->
<script>
$(document).ready(function() {
	if($("#sharebox").length) {
		var $sidebar = $("#sharebox"),
		$window = $(window),
		offset = $sidebar.offset(),
		topPadding = 100;
		
		$window.scroll(function() {
			if ($window.scrollTop() > (offset.top-topPadding)) {
				$sidebar.addClass("fixed");
			} else {
				$sidebar.removeClass("fixed");
			}
		});
	}
});
</script>

