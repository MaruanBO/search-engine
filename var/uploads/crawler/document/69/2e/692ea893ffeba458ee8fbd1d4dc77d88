<script>
//ga('send', 'pageview', {'page': '/goal/land/ezrf2/reg0'});
var err='';
var tmr1=null;

function is_taken(v){
	var u="?page=helpers/ajax_username_taken&login="+escape(v);
	if(document.F.email.value.length>2) u+=("&alt="+document.F.email.value);
	else{
		var c='.'+String.fromCharCode(Math.floor(Math.random()*26+97))+String.fromCharCode(Math.floor(Math.random()*26+97));
		u+=("&alt="+c);
	}
	document.getElementById('usr_taken').src=u;
}
function visual_is_taken(y,alt){
	var msg=['',"Display name/Username is taken","All ready in use please enter a different one",
		 		"Too short","Invalid - Use letter and digits only"];
	var vis=document.getElementById('display_login');
	if (vis) {
		if (y) {
			if(alt && alt.length>0) {
				msg[y]+="<div class='alt_logins'>Available:&nbsp; ";
				for(i=0;i<alt.length;i++){
					if(i) msg[y]+='&nbsp; '; 
					msg[y]+="<u onclick=\"document.F.login.value='"+alt[i]+"';visual_is_taken(0)\">"+alt[i]+"</u>";
				}
				msg[y]+="</div>";
			}
			vis.innerHTML="<div id=e>"+msg[y]+"</div>";
		}
		else vis.innerHTML="(This will appear on your profile; no spaces)";
	}
}
function is_all_empty(F){
	if(F.login && F.email && F.pass && 
		(F.login.value.length<4 || F.email.value.length<2 || F.pass.value.length<2)) {
		alert("An error has occurred\n=================\n\nIn order to register, you must complete the form!");
		return false;
	}
	return true;
}

function submit_r1(){
	// try to validate or do a bit of ajax
	err='';
	var y=$('input[name="agree"]');
	if(!Fr1.agree.checked) {
		err="You have to agree to our terms<br>"; 
		y.css('box-shadow','red 0 0 5px'); 
	} else y.css('box-shadow','none');
	
	y=$('input[name="login"]');
	if(y.val().length<3) {
		err+="Display name is too short<br>"; 
		y.addClass("ebox") 
	} else y.css('box-shadow','none');
	
	y=$('input[name="pass"]');
	if(y.val().length<3) {
		err+="Password - minimum 6 chars<br>"; 
		y.addClass("ebox") 
	} else if(/\s/.test(y.val())) {
		err+="Password - No spaces allowed<br>"; 
		y.css('box-shadow','red 0 0 5px'); 
	} else y.css('box-shadow','none');
	 
	y=$('input[name="email"]');
	if(y.val().length<3) {
		err+="No email... not good!<br>"; 
		y.addClass("ebox")
	} else y.css('box-shadow','none');
	if (!err){
		$('#Fr1').submit();
	} else 
		$('div.msg').html("<div id=e>"+err+"</div>");
}
function check_year(){
	var y=$('select[name="year"]');
	if(y.val()<10) {
		err="Please select a year"; 
		y.css('border','2px solid #ff0000'); 
	} else { 
		err=''; 
		y.css('box-shadow','none'); 
		update_view(4); 
	}
}
function update_view(question){
	$('.myfooter .login').hide();
	$("div.qSelects").hide();
	$("#q"+question+"box").fadeIn();
	if(question==7){
		$('#q1-content').hide();
		setTimeout(function(){ $('.answers').show(); $('.myload').hide(); },2000);
	} else if(question>7) $('#q1-content').hide(); 
}

$(document).ready(function(){
	$(".mybutton, .mybutton3").click(function(){
		if(err) return;
		var parent=$(this).closest('.qSelects');
		var question=$(this).attr('rel');
		if(!question) question=parent.attr('rel');
		parent.find('.mybutton').removeClass('mybOn');
		$(this).addClass('mybOn');
		update_view(question);
	});
});
