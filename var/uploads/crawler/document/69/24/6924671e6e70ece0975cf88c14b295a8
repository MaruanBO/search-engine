<script>
$(document).ready(function() {
    $(window).scroll(function() {
        if($(this).scrollTop() > 300){
            $('#goTop').stop().animate({
                bottom: '100px'    
                }, 500);
        }
        else{
            $('#goTop').stop().animate({
               bottom: '-100px'    
            }, 500);
        }
    });
    $('#goTop').click(function() {
        $('html, body').stop().animate({
           scrollTop: 0
        }, 500, function() {
           $('#goTop').stop().animate({
               bottom: '-100px'    
           }, 800);
        });
    });
}); 
</script>






