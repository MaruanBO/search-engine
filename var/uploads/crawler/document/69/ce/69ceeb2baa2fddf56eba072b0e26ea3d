	$(document).on('ready', function(){
		$('.select-recruit').on('click', function(){
			$('.tmj-enterprise').addClass('recruit').removeClass('build').removeClass('hire');
		});
		$('.select-build').on('click', function(){
			$('.tmj-enterprise').removeClass('recruit').addClass('build').removeClass('hire');
		});
		$('.select-hire').on('click', function(){
			$('.tmj-enterprise').removeClass('recruit').removeClass('build').addClass('hire');
		});

		var scrollerItems = $('.tmj-enterprise .scroller-item');
	    window.autoRotate = false;
	    var currentScrollerItem = 0;
		$(window).on('scroll', function(){
			if(window.autoRotate == true) return false;
		    var scrollerOffset = $(document).scrollTop() - $('.tmj-enterprise').offset().top;
		    console.log('on scroll', scrollerOffset, $(document).scrollTop(), $('.tmj-enterprise').offset().top);

		    function scrollerAutoRotate(){
		    	if(window.autoRotate){
		    		$(scrollerItems)[currentScrollerItem].click();
		    		window.clearTimeout(window.scrollerTimeout);
		    		window.scrollerTimeout = window.setTimeout(function(){
		    			scrollerAutoRotate();
		    		}, 6000);
		    		currentScrollerItem += 1;
		    		if(currentScrollerItem >= scrollerItems.length){
		    			currentScrollerItem = 0;
		    		}
		    	}
		    }

		    if(scrollerOffset > -160){
		      window.autoRotate = true;
		      scrollerAutoRotate();
		      $(scrollerItems).on('mouseover', function(){
		      	window.autoRotate = false;
		      });
		    }
		 });
		if(($(document).scrollTop() - $('.tmj-enterprise').offset().top) > -160){
	      window.autoRotate = true;
	      scrollerAutoRotate();
	      $(scrollerItems).on('mouseover', function(){
	      	window.autoRotate = false;
	      });
	    }

	    $('.company-scroller-items>li').each(function(ind, el){
	      var companyName = $(el).attr('data-company').replace(/[\/\? ]+/g, '-');
	      var location = '';
	      var link = $('<a href="/clients"><img class="thumb" src="'+$(el).attr('data-img-url')+'" alt="'+$(el).attr('data-company')+' Jobs"/></a>');
	      $(el).append(link);
	      link.on('click', function(){
	        _gaq.push(['_trackEvent', window.pageCategory, 'Featured Company - ' + companyName, window.pageLabel]);
	      })
	    });

	});

	</script>

