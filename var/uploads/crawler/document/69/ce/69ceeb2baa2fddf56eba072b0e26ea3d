            $.ajax({
                url: '/api/logger',
                data: { message: window.AdditionalInfo.Message || "JS Debug", additionalInfo: JSON.stringify(window.AdditionalInfo) },
                type: 'POST'
            });
        };

        window.onerror = function(msg, url, line) {
            window.AdditionalInfo.Message = msg;
            window.AdditionalInfo.Url = url;
            window.AdditionalInfo.LineNumber = line;
            if (url && url.indexOf('tweetmyjobs') > 0) {
                window.LogError();
            }else {
                console.log('skipping logging external js error', msg, url, line);
