<script>
 $(document).ready(function() {

	window.refresh_captcha = function (id)
    {
     $(id).attr('src', '/register/captcha_image?='+Math.random());
    }
$( "#poll_vote" ).click(function() {
	var selected = $("input[type='radio'][name='button']:checked");
		if (selected.length > 0) {
		var selectedVal = selected.val();
		} else {
		$('#poll_message').html('<span style=color:red;>Please Select an option.</span>');
		return false;
		}
		if($("input[name ='magicword']").val() == '')
		{
		$('#poll_message').html('<span style=color:red;>Please enter text showing in the image.</span>');
		return false;
		}	
	var magicword = $("input[name ='magicword']").val();
	var poll_id = $("input[name ='poll_id']").val();
		$.ajax({
			type: "POST",
			url: 'http://www.beckett.com/home/poll_insert',
			cache: false,
			data: "magicword="+magicword+"&selected_id="+selectedVal+"&poll_id="+poll_id,
			beforeSend: function(){
			},
		success : function(data) {
		if(data == 'dattampt'){
		$('#poll_message').html('<span style=color:red;>Double attempt are not allowed.</span>');
		$("input[name ='magicword']").val("");
		$('input:radio[name=button]:checked').prop('checked', false);
		return false;
		}
		else if(data == 'false')
		{
		$('#poll_message').html('<span style=color:red;>Wrong image code.</span>');
		$("input[name ='magicword']").val("");
		$('#captcha_img').attr('src', '/register/captcha_image?='+Math.random());
		return false;
		}
		else if(data == 'success'){
		$('#captcha_img').attr('src', '/register/captcha_image?='+Math.random());
		$('#poll_message').html('<span style=color:green;>Thanks for your vote.');
		$("input[name ='magicword']").val("");
		$('input:radio[name=button]:checked').prop('checked', false);
		}
		else {
		$('#poll_message').html('<span style=color:red;>Error found. Please try after some time.');
		$("input[name ='magicword']").val("");
		$('input:radio[name=button]:checked').prop('checked', false);
		}
	},
			complete: function (data){
			},
			error: function (jqXHR, textStatus, errorThrown)
			{
				site_notification.show_only_this_message('Error: Error found. Please try after some time.','error');
			}
			
		});
});
    });
    function calendar_up(url) {
        $.ajax({
            dataType: 'html',
            url: url,
            type: "POST",
            cache: false,
            async: false,
            success: function(messages) {
                $('.calendar-content').html(messages);
            },
            complete: function() {

            },
            error: function() {

            }
        });
    }
	
</script>

<script>
  $(document).ready(function(){
  	var windowheight = $(window).height()-140;
	//alert(windowheight);
      $('div.giftBox').hide();
		setInterval(function(){
        $('div.giftBox').show();
		$('div.giftBox').animate({top:windowheight},2000);
        },7000);
  });
</script>
