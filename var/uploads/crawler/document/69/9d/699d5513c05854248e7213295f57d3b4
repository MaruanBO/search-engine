    <script>
    $(document).ready(function() {
        $("#newsletterforms").validate();
        $('#errormes2').hide();
        $("#inlinecontent").dialog({
            autoOpen: false,
            height: 300,
            width: 550,
            modal: true
        });
        $(".subscribe").click(function() {
            if (!validateEmail($.trim($("#subscribe_email").val()))) {
                $('#errormes').html("Invalid Email Address");
                $('#errormes').show();
                $('#newsletterforms').hide();
                $("#subscribe_email").val(" ");
                $('#subscriber_email').val(" ");
                //$(".subscribe").colorbox.close();
            } else {
                //newsletter/checkemail_aleadyexists
                $.ajax({
                    dataType: 'html',
                    url: '/newsletter_subscriber/checkemail_aleadyexists/' + $.trim($("#subscribe_email").val()),
                    type: "POST",
                    cache: false,
                    async: false,
                    beforeSend: function() {
                        $('#subscriber_email').val(" ");
                        $('#inline_content').show();
                        $('#errormes').html(" ");
                       
                    },
                    success: function(messages) {
                        if ($.trim(messages) == 1) {
                            $('#subscriber_email').val(" ");
                            $('#errormes').html("This email address already exists !");
                            $('#errormes').show();
                            $('#newsletterforms').hide();
                          
                            // $(".subscribe").colorbox.close();
                        } else {
                            $('#subscriber_email').val($.trim($("#subscribe_email").val()));
                            $('#errormes').html(" ");
                            $('#errormes').hide();
                            $('#newsletterforms').show();
                            $("#inlinecontent").dialog("open");
                          
                        }
                    },
                    complete: function() {

                    },
                    error: function() {

                    }
                });
            }

        });
    });

    function validateEmail(sEmail) {
        var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        if (filter.test(sEmail)) {
            return true;
        }
        else {
            return false;
        }

    }

    function validat_form() {
        // && $.trim($("#list_3").attr("value"))=="" && $.trim($("#list_4").attr("value"))=="" && $.trim($("#list_5").attr("value"))==""
        if (document.getElementById("list_6").checked==false && document.getElementById("list_2").checked ==false) {
            $('#errormes2').html("Atleast choose one offer ");
            $("#errormes2").show();
            return false;
        }
        $('#errormes2').html(" ");
        $("#errormes2").hide();
        return true;
    }
</script>

