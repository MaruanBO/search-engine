<script>
    $(document).ready(function () {
        var firstLevelNav = $('#navigation > ul > li > a');
        var secondLevelNav = firstLevelNav.parent().find('> ul > li > a');
        firstLevelNav.on('mouseenter', function () {
            firstLevelNav.parent().find('> ul').hide();
            $(this).parent().find('> ul').show();
            $(this).parent().find('> ul > li:first > div').show();
            //$(this).parent().find('ul').find('li').each(function(e){$(this).removeClass('activeli');});
            if ($(this).parent().find('> ul > li.activeli').length == 0) {
                $(this).parent().find('> ul > li:first').addClass('activeli');
                //$(this).parent().find('> ul > li:first').css('font-weight','bold');
            }
            //$(this).parent().find('> ul > li:first').css('font-weight','bold');
        });
        firstLevelNav.on('focusin', function () {
            firstLevelNav.parent().find('> ul').hide();
            $(this).parent().find('> ul').show();
            $(this).parent().find('> ul > li:first > div').show();
            //$(this).parent().find('ul').find('li').each(function(e){$(this).removeClass('activeli');});
            $(this).parent().find('> ul > li:first').addClass('activeli');
        });
        $('#navigation > ul > li > ul > li').hover(function () {
            $('#navigation ul li ul li.activeli').removeClass('activeli');
            //$('#navigation ul li ul li').css('font-weight','normal');
            $(this).addClass('activeli');
            //$(this).css('font-weight','bold');
            //$(this).parent().children('>ul>li:first').removeClass('activeli');
        }, function () {
            $(this).addClass('activeli');
            //$('#navigation > ul > li > ul > li:first').removeClass('activeli');
            //$(this).css('font-weight','bold');
        });
        secondLevelNav.on('mouseenter', function () {
            secondLevelNav.parent().find('> div').hide();
            $(this).parent().find('> div').show();
        });
        $('#navigation').on('mouseleave', function () {
            firstLevelNav.parent().find('> ul').hide();
            secondLevelNav.parent().find('> div').hide();
        });
        secondLevelNav.on('focusin', function () {
            secondLevelNav.parent().find('> div').hide();
            $(this).parent().find('> div').show();
        });
//$('#navigation ul li.activeli a').hover(function(e){
//$('#navigation ul li ul li').removeClass('activeli');
//$(this).parent('li').addClass('activeli');
//});
    });
</script>
<script>
    $(document).ready(function () {
        $('.last-or').hover(function () {
            $('#collectorhub').slideDown();
        });

        $('.clickme').click(function () {
            $('#nav_mob').slideToggle();
        });
        /*
         $('#home-one').click(function(){
         $('#nav_mob').slideToggle();
         });*/

        $('#nav_mob > li > a').click(function () {
            if ($(this).attr('class') != 'active') {
                $('#nav_mob > li > ul').slideUp();
                $(this).next().slideToggle();
                $('#nav_mob > li > a').removeClass('active');
                $(this).addClass('active');
            }
            else {
                $(this).removeClass('active');
                $('#nav_mob li ul').slideUp();
            }
        });

        $('#nav_mob > li > ul > li > a').click(function () {
            if ($(this).attr('class') != 'active') {
                $('#nav_mob > li > ul > li > ul').slideUp();
                $(this).next().slideToggle();
                $('#nav_mob > li > ul > li > a').removeClass('active');
                $(this).addClass('active');
            }
            else {
                $(this).removeClass('active');
                $('#nav_mob li ul li ul').slideUp();
            }
        });

        $('#nav_mob > li > ul > li > ul > li > a').click(function () {
            if ($(this).attr('class') != 'active') {
                $('#nav_mob > li > ul > li > ul > li > ul').slideUp();
                $(this).next().slideToggle();
                $('#nav_mob > li > ul > li > ul > li > a').removeClass('active');
                $(this).addClass('active');
            }
            else {
                $(this).removeClass('active');
                $('#nav_mob li ul li ul li ul').slideUp();
            }
        });

        $myinput = $('#siteSearchQuery').autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: 'http://www.beckett.com/searchresult/',
                    datatype: "json",
                    data: {
                        q: request.term,
                        type: $("select[name='type']").val(),
                        maxRows: 15
                    },
                    beforeSend: function () {
                        //alert("yes sending");

                    },
                    success: function (data) {
                        //alert("dfgdfgfdgfdgfdg");
                        response($.map(data, function (item) {
                            return {
                                label: item,
                                value: item
                            }
                        }))
                    }
                })
            },
            select: function (event, ui) {
                $('#siteSearchQuery').val(ui.item.value);
            },
            open: function () {
                var position = $("#siteSearchQuery").position(),
                        left = position.left,
                        top = position.top;
                $(this).autocomplete("widget")
                        .appendTo("#results_auto")
                        .css({"position": "absolute",
                            "left": (left) + "px",
                            "top": (top + 30) + "px",
                            "width": "355px",
                            "background": "#fff;"});
            }
        });
        $myinput.data('autocomplete').menu.element.css('min-width', '355px');

    });
</script>
