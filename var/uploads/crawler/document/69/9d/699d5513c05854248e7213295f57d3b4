<script>
    (function (cash)
    {
        $(document).ready(function () {
            if ($('#search_sport').val() == '-1') {
            }
            $('form#siteSearchFrm').submit(function (e) {
                if ($('#search_sport').val() < 0) {
                    site_notification.show_only_this_message('Please select a category first', 'alert error alert-danger');
                    return false;
                }
                return true;
            });

            $('input:radio[name="type"]').click(function () {
                if ($(this).val() == 'coin' || $(this).val() == 'marketplaceCoin') {
                    location.href = 'http://www.beckett.com/coins';
                }
            });
            if (jQuery(window).width() > 767) {
                var elementPosTop = $('#jq4u-sidebar-ads').position().top;
                $(window).scroll(function () {
                    var wintop = $(window).scrollTop(), docheight = $(document).height(), winheight = $(window).height();
                    //if top of element is in view
                    if (wintop > elementPosTop) {
                        //always in view
                        $('#jq4u-sidebar-ads').css({"position": "fixed", "z-index": "1000", "width": "100%", "box-shadow": "0px 2px 2px #888888", "min-height": "61px"});
                        $('.main-nav-top').css({"display": "none"});
                        $('.notificationMsg').css({"display": "none"});
                        $('.topbarHeader').css({"display": "none"});
                        $('#fixedheader').addClass('marginTop94');
                        $('.logosize').css({'width': '106px'});
                        $('.search-top').css({'margin-top': '0px'});
                        $('.logo-home').css({'padding': '2px 4px', 'background': 'transparent'});
                        $('#home-one .header-bg .search-top select').css({'height': '28px'});
                        $('#home-one .header-bg .search-top input[type="text"]').css({'height': '28px'});
                        $('.search-top .btn').css({'padding': '2px 12px'});
                        $('#home-one .header-bg .top-border').css({'background': 'none'});
                        $('.workday').css({'display': 'none'});

                    } else {
                        //reset back to normal viewing
                        $('#jq4u-sidebar-ads').css({"position": "inherit", "box-shadow": "0px 0px 0px #9a9a9a"});
                        $('.main-nav-top').css({"display": "block"});
                        $('.notificationMsg').css({"display": "block"});
                        $('.topbarHeader').css({"display": "block"});
                        $('#fixedheader').removeClass('marginTop94');
                        $('.logosize').css({'width': '194px'});
                        $('.logo-home').css({'padding': '15px 30px', 'background': '#000'});
                        $('#home-one .header-bg .search-top select').css({'height': '38px'});
                        $('#home-one .header-bg .search-top input[type="text"]').css({'height': '37px'});
                        $('.search-top .btn').css({'padding': '6px 12px'});
                        $('#home-one .header-bg .top-border').css({'background': 'url("../../images/rs_opg/top-border.jpg") repeat-x 0 0'});
                        $('.workday').css({'display': 'block'});
                        //$('.search-top').css({'margin-top': '25px'});
                    }
                });
            } else {
                $('#jq4u-sidebar-ads').css({"position": "inherit", "box-shadow": "0px 0px 0px #9a9a9a"});
                //alert('else condition');
            }
        });
    })(jQuery);
</script>
<script type="text/javascript">
    $(document).ready(function (e) {
        if ($(window).width() < 767) {
            $('#quicklinknav').hide()
            $('.quicklinkmob').show();
            $('.quicklinkmob').click(function () {
                $('#quicklinknav').slideToggle();
            })
        }
        else {
            $('#quicklinknav').show()
            $('.quicklinkmob').hide();
        }
        $('#showhidecartbtn').hover(function ()
        {
            $('.arrow_box').show();
            //alert('hi');
        }, function () {
            $('.arrow_box').hide();
        });
    });
</script>
