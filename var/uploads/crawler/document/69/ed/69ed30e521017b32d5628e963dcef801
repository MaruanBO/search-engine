        <script>
		var displayTime = "10000";
		
		var fx = "scrollHorz";
		var initialLoad = true;
		var offsetWidth = 15;
		var itemWidth = 22;
		
		jQuery.extend(jQuery.easing, {
			easeOutSine: function(x, t, b, c, d) {
				return c * Math.sin(t/d * (Math.PI/2)) + b;
			}
		});
		
		$(document).ready(function(){
			
			// add the slideshow controls
			$('#slideshow').after('<div id="slideshow_controls">');
			
			// options for LazySlides
			var options = {
				container: '#slides',
				slides: [{link:{url:"http://www.liberty.edu/aboutliberty/index.cfm?PID=31874",target:null},image:{src:"/archives/public/photos/2015-Flames-Baseball-Outlook.jpg",alt:"2015 Flames Baseball Outlook"},minDelay:null},{link:{url:"http://www.liberty.edu/aboutliberty/index.cfm?PID=31843",target:null},image:{src:"/archives/public/photos/Journey-FM.jpg",alt:"Journey FM"},minDelay:null},{link:{url:"http://www.liberty.edu/since1971",target:null},image:{src:"/archives/public/photos/LU-Since-1971.jpg",alt:"Training Champions Since 1971"},minDelay:null}]
			};
		
			var LazySlides = new lu.LazySlides(options);
			
			$('#slides').cycle({
				fx: fx, 
				speed: 500, 
				timeout: displayTime, 
				sync: true, 
				easing: 'easeOutSine',
				delay:-1500, 
				pause: 1,
				pager: "#slideshow_controls",
				before: LazySlides.onBefore
			});
			
			$(".default_slide").hover(
				function() {
					$(this).css("background-position", "bottom right");
				}, 
				function() {
					$(this).css("background-position", "top right");
				}
			);
			
			$("#pic_of_the_day_launcher").fancybox({
				'width'				: 600,
				'height'			: 400,
				'autoScale'			: false,
				'transitionIn'		: 'elastic',
				'transitionOut'		: 'elastic',
				'type'				: 'iframe',
				'scrolling'			: 'no',
				'overlayOpacity'	: .5,
				'onStart'			: picDayStart,
				'onClosed'			: picDayClosed
			});
			
		});
		
		function picDayStart(){
			$('#slides').cycle('pause');
		}
		
		function picDayClosed(){
			$('#slides').cycle('resume');
		}
		
		</script>
        
