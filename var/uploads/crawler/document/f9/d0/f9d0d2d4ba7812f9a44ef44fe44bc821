			<script type='text/javascript'>

				initGoogleTags = function () {
					var displaySelector = '.desktop';
					if (window.innerWidth < mobileBreakPoint) {
						displaySelector = '.mobile';
					}

					/**
					 * Here define the Out Of Page Slots.
					 *
					 * It will search all divs with the class .dfp-container and .dfp-oop, will iterate over this elements
					 * will read the parameters from the element attributes and will define the slots.
					 */
					jQuery('.dfp-container.dfp-oop' + displaySelector).each(function (index, element) {
						var dfp = jQuery(element);
                        var adUnit = '/' + siteToken;
                        if (dfp.attr('data-zone') != '') {
                          adUnit += '/' + dfp.attr('data-zone');
                        }
						googletag.defineOutOfPageSlot(adUnit, dfp.attr('id'))
							.setTargeting("pos", dfp.attr('data-position'))
							.addService(googletag.pubads());
					});

					/**
					 * Here we define all the Slots.
					 *
					 * It will search all divs with the class .dfp-container and .dfp-slot, will iterate over this elements
					 * will read the parameters from the element attributes and will define the slots.
					 */
					ybotq.push(function () { //Dfp push wrapped inside yieldbot
						googletag.cmd.push(function () { //all the slots inside cmd push
							jQuery('.dfp-container.dfp-slot' + displaySelector).each(function (index, element) {
								var dfp = jQuery(element);
                                var adUnit = '/' + siteToken;
                                if ( dfp.attr('data-zone') != '') {
                                  adUnit += '/' + dfp.attr('data-zone');
                                }
								var slot = googletag.defineSlot(adUnit, jQuery.parseJSON(dfp.attr('data-sz')), dfp.attr('id'))
									.setTargeting("pos", dfp.attr('data-position'))
									.addService(googletag.pubads());
								//alow only slots for some slots
								if (dfp.attr('id') == "dfp-tag-horizontal-position-1" || dfp.attr('id') == "dfp-tag-rightrail-position-2") {
									//dynamically insert `setTargeting` for Yieldbot custom criteria
									yieldbot.setSlotTargeting(dfp.attr('id'), slot);
								}
								//Saving the Slot instance for future handling in the JavaScript side.
								var slot_info = {
									tag: dfp.attr('id'),
									slot: slot
								};

								googletag.slots.push(slot_info);
							});
							googletag.pubads().enableAsyncRendering(); //Async only for Desktop, mobile Sync.
							googletag.pubads().enableSingleRequest();
							googletag.pubads().collapseEmptyDivs();
							googletag.enableServices();
						});
					});
					ybotq.push(function () { //Dfp push wrapped inside yieldbot
						googletag.cmd.push(function () { //all the slots inside cmd push
							jQuery(googletag.slots).each(function (index, slot) {
								googletag.display(slot.tag); //Display for compatibility.
							});
						});
					});
				}

				jQuery(document).ready(function () {
					//Will do push only for async ads, for mobile we use sync so we don't have to do push
					ybotq.push(function () { //Dfp push wrapped inside yieldbot
						googletag.cmd.push(initGoogleTags);
					});
				});
			</script>
