<script>
  //<![CDATA[
    $(function() {
      $("form#user_login_zapper").bind('ajax:success', function(evt, data, status, xhr) {
        if(data.success == true) {
          window.location.href = data.redirect;
        } else {
          hide_ajax_loading_layer();
          var error_message = data.errors[0];
          error_message_length = error_message.length;
          //console.log(error_message_length);
          required_lines = Math.ceil(error_message_length / 33);
          //console.log(required_lines);
          error_message_height = required_lines * 16 + 14;
          //console.log(error_message_height);
          container_height = 260 + error_message_height;
          //console.log(container_height);
          $('#login_wrapper').animate({
              height: [container_height,'easeInOutCirc']
            }, 150, function() {
              if ($('div#user_login_zapper_error').css('display') != 'block'){
                $('#login_wrapper').effect('shake', { times: 2 }, 100);
                $('div#user_login_zapper_error').hide().html(data.errors[0]).slideDown('slow');
              }
              else {
                $('div#user_login_zapper_error').hide().html(data.errors[0]);
                var zapper_height = $('#zapper_wrapper').height();
                $('#login_wrapper').css('top',zapper_height);
              }
          });
        }
      });
      $("form#user_login_zapper").bind('ajax:before', show_ajax_loading_layer);
    });
  //]]>
</script>
