    $(document).ready(function() {
      // If user agent is IE, we use separate hover image
      if ($('body').attr('class').match(/msie/)) {
        $('.big_play_button').hover(function() {
          $(this).find('img').attr('src','http://dwtrd9cmgca2m.cloudfront.net/files/assets/whitelabel/popmog/static/play_button_hover.png');
        }, function() {
          $(this).find('img').attr('src','http://dwtrd9cmgca2m.cloudfront.net/files/assets/whitelabel/popmog/static/play_button.png');
        });
      }
      // Or else we use vertical flip class via css
      else {
        $('.big_play_button').hover(function() {
          $(this).find('img').addClass('img_flip');
        }, function() {
          $(this).find('img').removeClass('img_flip');
        });
      }
    })
