<script>

function blogrollFeaturedHTML(brpos) {

	var rethtml;

	if ( brpos == -1 ) {
		rethtml = "<div id=\"BR-feat-auth\">\n<img src=\"" +
			"br-default.jpeg" +
			"\" height=\"150\" width=\"150\" class=\"" +
			"BR-default\" alt=\"" +
			"\" />\n</div>\n<div id=\"BR-feat-content\">\n<h3 class=\"BR-feat-header\">" +
			"Title Slug" +
			"</h3><span class=\"BR-feat-byline\">By <a href=\"" +
			"#" +
			"\">" +
			"Author Slug" +
			"</a></span>\n<p class=\"BR-feat-content-style\">" +
			"Excerpt Slug.  Should probably do something nicer when you clear a blogroll entry." +
			"<em><a href=\"" +
			"#" +
			"\"> Read more...</a></em></p>\n</div>";
	} else {
		rethtml = "<div id=\"BR-feat-auth\"><a href=\"/d/br/author/" +
			brInfo[brpos][6] +
			"/\"><img src=\"" +
			brInfo[brpos][2] +
			"\" height=\"150\" width=\"150\" class=\"" +
			brInfo[brpos][3] +
			"\" alt=\"" +
			brInfo[brpos][0] +
			"\" /></a></div>\n<div id=\"BR-feat-content\">\n<h3 class=\"BR-feat-header\">" +
			"<a href=\"/d/br/author/" +
			brInfo[brpos][6] +
			"/\">" +
			brInfo[brpos][4] +
			"</a></h3><span class=\"BR-feat-byline\">By <a href=\"/d/br/author/"
			+ brInfo[brpos][6] +
			"/\">" +
			brInfo[brpos][0] +
			"</a></span>\n<p class=\"BR-feat-content-style\">" +
			brInfo[brpos][5] +
			"<em> <a href=\"/d/br/author/" +
			brInfo[brpos][6] +
			"/\">Read more...</a></em></p>\n</div>";
	}
	return rethtml;
}
      
function blogrollAuthorHTML(brpos) {

	var rethtml;

	if ( brpos == -1 ) {
		rethtml = "<div class=\"BR-auth\">\n<img src=\"" +
			"br-default.jpeg" +
			"\" height=\"100\" width=\"100\" class=\"" +
