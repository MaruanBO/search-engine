		<script type="text/javascript">
			//-----------------------------------------------------------------------
			function checkSearch() {
				/*
					First, test if autocomplete is up.  If so, select the first item.
					If it is not or does not have a first item, then send to geocoder.
					If geocoder returns good info, use it for the search.
					If it does not, then send them on their way with a blank search.
				*/
				if ($(".ac_results:visible ul li:first-child").length) { $(".ac_results:visible ul li:first-child").trigger("click"); }
				else {
					var geocoder = new google.maps.Geocoder();
					if ($("#pageTopperSearch").val().trim().length) {
						geocoder.geocode({address: $("#pageTopperSearch").val().trim()}, handleSearchAddress);
					}
				}
			}
			function handleSearchAddress(result, status) {
				var searchObj = handleGeocodeAddress(result, status);
				if (searchObj.success) {
					var baseURL = $("#landForSale").attr("action");
					switch (searchObj.searchType) {
						case "zip" : var t = setTimeout("window.location.href = '" + baseURL + "?hs=1&Search=zip&zip=" + searchObj.searchPart + "&location=" + searchObj.searchLocation + "'", 250);
						break;
						case "city" : var t = setTimeout("window.location.href = '" + baseURL + "?hs=1&Search=city&location=" + searchObj.searchLocation + "'", 250);
						break;
						case "county" : var t = setTimeout("window.location.href = '" + baseURL + "?hs=1&Search=county&location=" + searchObj.searchLocation + "'", 250);
						break;
						case "state" : var t = setTimeout("window.location.href = '" + baseURL + "?hs=1&Search=state&location=" + searchObj.searchLocation + "'", 250);
						break;
						default :
							$("#pageTopperSearch").val('');
							$("#landForSale")[0].submit();
						break;
					}
				}
				else {
					var foundProp = false;
					var valCheck = $("#pageTopperSearch").val().trim();
					// check if it is a property id
					if ($.isNumeric(valCheck)) {
						$.ajax({
							type: "POST",
							url: '/webservices/remote.cfc?method=validatePropertyID&returnFormat=json',
							async: false,
							data: {
								propID: valCheck
							},
							dataType: 'json',
							success: function( checkresponse, request, settings ){
								foundProp = checkresponse.PROPFOUND;
							}
						});
					}

					if (foundProp) {
						var t=setTimeout("window.location.href = '/listing/" + valCheck + "'", 250);
					}
					else {
						// send to top searches
						$("#pageTopperSearch").val('');
						$("#landForSale")[0].submit();
					}
				}
			}
			//-----------------------------------------------------------------------
			
			
			$(document).ready(function() {
				try {
					// Set up the data filter to remove leading // from ajax responses.
					$.ajaxSetup({
					    dataFilter: function(data, type){
					    	if( data.substring( 0, 2 ) == '//' ) {
						        return data.substring(2, data.length);
					    	} else {
					    		return data;
					    	}
					    }
					});

					if($().autocomplete){
						$("#pageTopperSearch").autocomplete("/shared/ajax/locationFeeder.cfm", {
							minChars: 3
							, dataType: "json"
							, cacheLength: 20
							, matchSubset: false
							, selectOnly: true
							, maxItemsToShow: 50
							, delay: 200
							, width: '325px'
							, scroll: true
							, scrollHeight: 300
							, parse: parseData
							, formatItem: formatAutocomplete
						}).result(handleResult);
					}
				}
				catch (err) {}
				//-----------------------------------------------------------------------
				$('#pageTopperSearch').on({
					"focus": function(){
						$(this).data('placeholder', $(this).attr('placeholder'));
						$(this).attr('placeholder', '');

			
			
					},
					"blur": function(){
						$(this).attr('placeholder', $(this).data('placeholder'));
						$(this).data('placeholder', '');
					},
					"keyup": function(){
						if ($("#searchSubmit").text().indexOf("CONTINUE") >= 0) {
							$("#searchSubmit").html($("#searchSubmit").html().replace("CONTINUE LAST SEARCH", "SEARCH"));
						}
					}
				});// END $('#pageTopperSearch').on({

				// run for initial sizing
				checkTopLinkColumns();

				// setup window listener
				$(window).resize(function() { checkTopLinkColumns(); });
			}); // END $(document).ready(function(){
			//-----------------------------------------------------------------------
			var formatAutocomplete = function (data, i, n, value) {
				var ret = 'Invalid search';
				switch(data.T) {
					case 'inv':
						ret = ''
						//Dealing with an inventory object
						if(data.IMG != ''){ret += '<img src="' + data.IMG + '" style="height: 50px; width: 77px; float: left; margin: 4px 8px 4px 0;" />';}
						ret += 'Property ID: ' + data.ID + '<br />';
						ret += data.ACRES + ' acres<br />' + data.COUNTY + ' ' + data.CLABEL + ', ' + data.STATE;
						break;
					default:
						ret = data.L;
						break;
				}
				return ret;
			}
			var handleResult = function(e, row){
				if(row){
					rc.saveHomeSearch(row);
					var searchDir = rc.getSearchTypeDirectory();
					var locURL = $("#locURL").attr('value');
					// console.log(locURL);
					switch(row.T){
						case 'inv':// INVENTORY
							var t=setTimeout("window.location.href = '/listing/" + row.ID + "'", 250);
							return false;
							break;
						case 'zip':// ZIP CODE
							var t = setTimeout("window.location.href = '/" + locURL + searchDir + "/?hs=1&search=zip&zip=" + row.ZIP + "&state_id=" + row.SID + "&location=" + row.L + "'", 250);
							return false;
							break;
						case 'ct':// CITY
							var t = setTimeout("window.location.href = '/" + locURL + searchDir + "/?hs=1&search=city&city_id=" + row.ID + "&location=" + row.L + "'", 250);
							return false;
							break;
						case 'cy':// COUNTY
							var t = setTimeout("window.location.href = '/" + locURL + searchDir + "/?hs=1&search=county&county_id=" + row.ID + "&location=" + row.L + "'", 250);
							return false;
							break;
						case 'lk':// LAKE
							var t = setTimeout("window.location.href = '/" + locURL + searchDir + "/?hs=1&search=lake&lake_id=" + row.ID + "&location=" + row.L + "'", 250);
							return false;
							break;
						case 's':// STATE
							var t = setTimeout("window.location.href = '/" + locURL + searchDir + "/?hs=1&search=state&state_id=" + row.ID + "&location=" + row.L + "'", 250);
							return false;
							break;

					}
				}
				else{
					alert('We couldn\'t find the location you entered. Please re-enter the name, and choose the location from the list.');
				}
			}
			var parseData = function(data) {
				return $.map(data, function(row) {
					switch(row.T){
						case 'inv': return { data: row, result: row.ID += '' };
						break;
						default: return { data: row, result: row.L };
						break;
					}
				});
			}
			var toggleTopLinks = function() {
				var linkContainer = $('#land-for-sale-container');
				var linkContainerLink = $("#link-container-showAll a");
				var hideLink = (linkContainer.css("display") == "none");
				
				if (hideLink) { linkContainerLink.hide(); }
				linkContainer.slideToggle(500, function() {
					
					if (!hideLink) {
						linkContainerLink.show();
						//linkContainer.hide();
						$('html, body').scrollTop(0);
					}
				});
				return false;
			}

			function checkTopLinkColumns() {
				var newW = $(window).width();
				var newNumCols = 3;

				// the top-links
				var colsTop = $('.top-link-container ul.top-links-top');
				var numColsTop = colsTop.length;

				// the all list
				var colsMain = $('.top-link-container ul.top-links-main');
				var numColsMain = colsMain.length;

				// the all-region list
				var colsRegion = $('.top-link-container ul.top-links-region');
				var numColsRegion = colsRegion.length;

				// default is three columns
				// 1280 = 4 columns
				if ((newW >= 1280) && (newNumCols != 4)) {
					newNumCols = 4;
				}

				// set each to new number of columns
				if ((numColsTop > 0) && (numColsTop != newNumCols)) {
					colsTop.cols(newNumCols);
				}
				if ((numColsMain > 0) && (numColsMain != newNumCols)) {
					colsMain.cols(newNumCols);
				}
				if ((numColsRegion > 0) && (numColsRegion != newNumCols)) {
					colsRegion.cols(newNumCols);
				}

			}
			//-----------------------------------------------------------------------
		</script>
