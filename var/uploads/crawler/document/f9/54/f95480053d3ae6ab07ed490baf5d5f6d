   var setItemDataOnForm_vKW2DoPt0MMd = function(itemNumber, item) {
        $('#item-'+itemNumber+'-header-_vKW2DoPt0MMd').val(item.header);
        $('#item-'+itemNumber+'-description-_vKW2DoPt0MMd').val(item.description);
        $('#item-'+itemNumber+'-accent-color-_vKW2DoPt0MMd').val(item.accentColor);
        $('#item-'+itemNumber+'-dest-url-_vKW2DoPt0MMd').val((item.destURL == '' || item.destURL == undefined) ? '' : decodeURIComponent(item.destURL));
        $('#item-'+itemNumber+'-bg-mode-_vKW2DoPt0MMd option[value="'+item.bgMode+'"]').attr("selected", "selected");
        $('#item-'+itemNumber+'-bg-image-_vKW2DoPt0MMd').val((item.bgImage == '' || item.bgImage == undefined) ? '' : decodeURIComponent(item.bgImage));
        $('#item-'+itemNumber+'-bg-media-_vKW2DoPt0MMd').val((item.bgMedia == '' || item.bgMedia == undefined) ? '' : decodeURIComponent(item.bgMedia));
        setItemFormForBgMode_vKW2DoPt0MMd(item.bgMode, itemNumber);
   }

   var setItemFormForBgMode_vKW2DoPt0MMd = function(bgMode, itemNumber) {
        if(bgMode == 'image' || bgMode == undefined) {
            $('.item-'+itemNumber+'-bg-image-_vKW2DoPt0MMd').show();
            $('.item-'+itemNumber+'-bg-media-_vKW2DoPt0MMd').hide();
        } else {
            $('.item-'+itemNumber+'-bg-image-_vKW2DoPt0MMd').hide();
            $('.item-'+itemNumber+'-bg-media-_vKW2DoPt0MMd').show();
        }
   }

   $(document).ready(function() {
        var numberOfItems_vKW2DoPt0MMd = '4';
        numberOfItems_vKW2DoPt0MMd = numberOfItems_vKW2DoPt0MMd == '' ? '1' : numberOfItems_vKW2DoPt0MMd;
        $('#number-items-select-_vKW2DoPt0MMd option[value="'+numberOfItems_vKW2DoPt0MMd+'"]').attr("selected", "selected");
        showItemsAsNeeded_vKW2DoPt0MMd();
        setItemsOnForm_vKW2DoPt0MMd();

        $('#number-items-select-_vKW2DoPt0MMd').change(function() {
             showItemsAsNeeded_vKW2DoPt0MMd();
        });

        var colCSSClass = "col-sm-12";
        switch(parseInt(numberOfItems_vKW2DoPt0MMd)) {
            case 2: colCSSClass = "col-sm-6";
            break;
            case 3: colCSSClass = "col-sm-4";
            break;
            case 4: colCSSClass = "col-sm-3";
            break;
        }

        for(var idx=0;idx<numberOfItems_vKW2DoPt0MMd;idx++) {
            var html = '<div id="hero-content-item-_vKW2DoPt0MMd-'+idx+'" class="'+colCSSClass+'">';
            
            if(items_vKW2DoPt0MMd[idx].bgMode == 'image') {
                html += '<div class="hero-content-container">';
                html += '<div class="bg-section" style="background: url('+ decodeURIComponent(items_vKW2DoPt0MMd[idx].bgImage)+') 100%; background-size: cover;"></div>';
            } else {
                html += '<div class="hero-content-container" style="background-color: #555;">';
