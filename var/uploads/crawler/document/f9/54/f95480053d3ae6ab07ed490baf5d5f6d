<script>
    var setDisplayHelp = function() {
        var showStaticHelp = "Search box will always been displayed.";
        var showDynamicHelp = "Users will be able to toggle the search box visibility.";
        var selectedDisplayType = $('#display-type-select-_e9IycP1cKSf1').val();
        if(selectedDisplayType == 'static') {
            $('#display-type-select-help-_e9IycP1cKSf1').html(showStaticHelp);
        } else {
            $('#display-type-select-help-_e9IycP1cKSf1').html(showDynamicHelp);
        }
    }
    
    var validateSubmitSearchForm = function(e) { 
        // Validate if text was inputted before submitting
        var searchText = $("#cs-search-field-_e9IycP1cKSf1").val().trim();
        if(searchText != "" && searchText != undefined) {
            // remove the has error
            $('#cs-search-view-form-_e9IycP1cKSf1 .input-group').removeClass('has-error');
            // sanitize the field before submitting 
            if($.sanitize) $("#cs-search-field-_e9IycP1cKSf1").val($.sanitize(searchText));
            $('#cs-search-view-form-_e9IycP1cKSf1').submit();
        } else {
            e.preventDefault();
            // add has-error class to the form
            $('#cs-search-view-form-_e9IycP1cKSf1 .input-group').addClass('has-error');
            $("#cs-search-field-_e9IycP1cKSf1").focus();
        }
    }

   $(document).ready(function() {
       var displayMode = 'static';
       displayMode = displayMode == '' ? 'static' : displayMode;
       $('#display-type-select-_e9IycP1cKSf1 option[value="'+displayMode+'"]').attr("selected", "selected");

        setDisplayHelp();

        $('#display-type-select-_e9IycP1cKSf1').change(function() {
             setDisplayHelp();
        });

       $('.cs-search-view-dynamic').click(function() {
            if($('.down-search-arrow').is(':visible')) {
                $('.down-search-arrow').hide();
                $('.up-search-arrow').show();
                $('#cs-search-view-form-_e9IycP1cKSf1').slideDown(300);
            } else {
                $('.down-search-arrow').show();
                $('.up-search-arrow').hide();
                $('#cs-search-view-form-_e9IycP1cKSf1').slideUp(300);
            }
       });
       
       // checks whether the pressed key is "Enter", and performs search if so
        $("#cs-search-field-_e9IycP1cKSf1").keypress(function(e) {
            if(e.which == 13) { 
                validateSubmitSearchForm(e);
            }
        });

      $('#cs-search-field-_e9IycP1cKSf1 + span').click(function(e) { 
            validateSubmitSearchForm(e);
      });
       
       
   });
</script>

	
	

					
				
			
			
		
	
	



