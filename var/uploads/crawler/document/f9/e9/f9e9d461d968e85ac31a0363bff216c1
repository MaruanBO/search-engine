<script type="text/javascript">
/* <![CDATA[ */	
globalCount=0;
var recipeCount;
function searchstring(doCallAjax) {
		var searchstring = "";
		var s = $('#rezept_form').serializeArray();
		var count_gerichteIDs_lm = new Number(0);
      		$('#gerichteIDs_lm').find('input[name="gerichteIDs_lm"]:checked').each(function(){
	        	if (count_gerichteIDs_lm == 0) {
	      				searchstring += ' AND gerichteIDs_lm:('+this.value+')';
	      		}
	      		else if (count_gerichteIDs_lm > 0) {
	      				searchstring = searchstring.slice(0,-1);
	      				searchstring += ' AND '+this.value+')';
	      		}
	      		count_gerichteIDs_lm++;	  	
    		});
      	jQuery.each(s,function(i,s){
      		if (this.name == "recipesearch" && this.value.length > 0 && this.value!="Zutat oder Stichwort eingeben (mind. 2 Zeichen)") {		
      				this.value=$.trim(this.value.toLowerCase());
					var replacerRegExp = "[^a-zA-Z0-9 ßÄÖÜäöüÃÀàÂâÆæÇçÈèÉéÊêËëÎîÏïÔôôŒœÙùÛûŸÿØøÁáçÍíÑñÓóÚúüªº»«¸]";
 					var replacer = new RegExp(""+replacerRegExp,"g");
      				var searchWord = "";
      				var searchWordOhne = ""; 
      				var items = this.value.replace(/-/g, " ");
 					items = items.replace(replacer, "");
 					var item = items.trim().split(' ');
 					if (item.length	> 1 ) {
 						searchWord += "*";
 					}
 					else {
 						searchWord += "*";
 					}
 					for (var ii in item) { 
 						searchWord += ''+item[ii].toLowerCase()+'';	
 						if (item.length	> 1 && ii < item.length-1) {
 						 	searchWord += '* AND *';
 						}	
 					}
					if (item.length	> 1 ) {
 						searchWord += "*";
 					}
 					else {
 						searchWord += "*";
 					}
      			searchWordOhne = searchWord;
 				while(searchWordOhne.indexOf('*') > -1){
 					searchWordOhne = searchWordOhne.replace("*","");
 				}	
					searchstring += 'AND (titel_t:('+searchWord+') OR (titel_t:('+searchWordOhne+'))^4.5 OR kurzbeschreibung_t:('+searchWord+') OR (kurzbeschreibung_t:('+searchWordOhne+'))^1,0 OR zutatenNamePlural_txt:('+searchWord+') OR (zutatenNamePlural_txt:('+searchWordOhne+'))^1,0 OR zutatenNameSingularList_txt:('+searchWord+') OR (zutatenNameSingularList_txt:('+searchWordOhne+'))^1,5)';
      		}    	
      		if (this.name == "schwierigkeit") {
      				//alert("Schwierigkeit: " + this.value);
      			if (this.value == 1) {	
      				searchstring += ' AND schwierigkeitsgradName_t:(einfach)'; 
      			}
      			else if (this.value == 2) {
      				searchstring += ' AND schwierigkeitsgradName_t:(mittel)';
      			}
      			else if (this.value == 3) {
      				searchstring += ' AND schwierigkeitsgradName_t:(anspruchsvoll)';
      			}
      		}
      		if (this.name == "zeit") {
      				//alert("Zeit: " + this.value);
      			if (this.value == 1) {	
      				searchstring += ' AND zubereitung_min_i:[* TO 15] AND -(zubereitung_std_i:[1 TO *] OR zubereitung_std_i:[* TO *])'; 
      			}
      			else if (this.value == 2) {
      				searchstring += ' AND zubereitung_min_i:[* TO 30] AND -(zubereitung_std_i:[1 TO *] OR zubereitung_std_i:[* TO *])';
      			}
      			else if (this.value == 3) {
      				searchstring += ' AND zubereitung_min_i:[* TO 60] AND -(zubereitung_std_i:[1 TO *] OR zubereitung_std_i:[* TO *])';
      			}
      			else if (this.value == 4) {
      				searchstring += ' AND zubereitung_std_i:[1 TO *]';
      			}
      		}
      		if (this.name == "kalorien") {
      			if (this.value == 250 || this.value == 500 || this.value == 750) {
      				searchstring += ' AND kcal_d:[* TO '+this.value+']';
      			}
      			else if (this.value > 750) {
      				searchstring += ' AND kcal_d:[750 TO *]';
      			}
      		}
		});
		$('#querySent').val(searchstring);
		if (typeof doCallAjax == 'undefined' ) {
			callAjax(searchstring);
		} else if (doCallAjax) {
			callAjax(searchstring);
		}	
	}
	function getUserChanges() {
		var stringForm = "";
			 if (typeof $('#rezept_form').val() != 'undefined' ){
				 $.each($('#rezept_form').serializeArray(), function() {
				 	stringForm += "&" + this.name + "=" +encodeURIComponent(this.value);
				 	return stringForm;
				 });
			 }
			 if (typeof paging != 'undefined' ) {
			 	stringForm += "&paging.ActHitsPerPage="+ paging.getActHitsPerPage();
			 	stringForm += "&paging.startPage="+ paging.startPage;
			 	stringForm += "&paging.start="+ paging.start;
			 	stringForm += "&paging.getActSort="+ encodeURIComponent(paging.getActSort());
			 }
			 if (typeof $('#queryList').val() != 'undefined' ){
			 	stringForm += "&queryList="+ encodeURIComponent($('#queryList').val());
			 }
		return	encodeURIComponent(stringForm.substring(1)); 
	}
	function callAjax(query) {
			var rows  = new Number(3000);
			var regionShort = "EZ";
			if(query.length > 0) {
				$.ajax({
      					url: '/ts/rezepte/rcn.jsp',
						dataType: 'json',
						data: {'start': '0' , 'rows': ''+rows+'' , 'indent' : 'off' , 'omitHeader' :'true',
						'hl' : 'false',
						'wt' : 'json',
						'fl' : ' gerichteIDs_lm, tcmIDs_lm',
						'q' : 'indexName:b2cRezepteIndex AND regionNamesShort_txt:('+regionShort+')' +query+'',
						'form' : ''+getUserChanges()			
						},
						type: 'POST',
						cache: false,
						success: function(json){
							var listCatIDs = [];
							$.each(json.response.docs, function(key, value) {	
					      		if (value.gerichteIDs_lm) {
					      			$.each(value.gerichteIDs_lm, function(key, value) {
					      				if (jQuery.inArray(value, listCatIDs) == -1) {
					      					 listCatIDs.push(value);
					      		  		}
					      		  	});
					      		}
							});
							if (query.length > 0) {   
								$(':checkbox').attr('disabled', 'disabled').next().addClass("disabled");
								recipeCount = json.response.numFound;
								recipeCounter(json.response.numFound);
							}
							$.each(listCatIDs, function(key, val) {
								if (val==1) {  $(':checkbox[value=00001]').removeAttr('disabled').next().removeClass("disabled"); }
					      		 $(':checkbox[value='+val+']').removeAttr('disabled').next().removeClass("disabled"); 
					      		 if ($(':checkbox[value='+val+']').attr('checked')) { 
								 	$(':checkbox[value='+val+']').addClass("checked");
								 }
					      	});
						},
						error: function(jqXHR, textStatus, errorThrown) {
								return ; 
						}
					})
			}
			if(query.length == 0) {
				$(".countbutton").html("<span class=\"disabled\">Rezepte anzeigen</span>");
				$("a.countbutton").addClass("disabled");
				$("#sliderSchwierigkeitsgrad").slider({value: 0},{range: "min"}).find('a > span').remove();
				$("#sliderZubereitungsdauer").slider({value: 0},{range: "min"}).find('a > span').remove();
				$("#sliderKalorien").slider({value: 0},{range: "min"}).find('a > span').remove();
			}
			else {
				if( !$('a.ui-slider-handle').children().hasClass("tooltip")  ) {
						$( "#sliderSchwierigkeitsgrad" ).find('a.ui-slider-handle').append('<span class="tooltip"></span>');
						$( "#sliderZubereitungsdauer" ).find('a.ui-slider-handle').append('<span class="tooltip"></span>');
						$( "#sliderKalorien" ).find('a.ui-slider-handle').append('<span class="tooltip"></span>');
				}
			}
	}
	function recipeCounter(count) {
		globalCount=new Number(count);
		$(".tooltip").text(count + " gefundene Rezepte");
		if(count == 0) {
			$(".countbutton").html("<span class=\"disabled\">"+count + " Rezepte anzeigen</span>");
			$("a.countbutton").addClass("disabled");
		}
		else {
				$("a.countbutton").removeClass("disabled");
				$(".countbutton").html("<span>"+count + " Rezepte anzeigen</span>");
			}
	}
$(document).ready(function() {
formcheck = 'false'; 
		$(function() {
			var actValue1 = 0;
			
			$( "#sliderSchwierigkeitsgrad" ).slider({
					range: "min",
					value: actValue1,
					min: 0,
					max: 3,
					step: 1,
					create: function(event, ui) { $(event.target).find("a.ui-slider-handle").append('<span class="tooltip"></span>'); },
					slide: function( event, ui ) {
						$('.hidden2').attr('value', ui.value).attr('name', 'schwierigkeit');
						searchstring();	
					}
			});
		});
		$(function() {
			var actValue2 = 0;
			
			$( "#sliderZubereitungsdauer" ).slider({
				range: "min",
				value: actValue2,
				min: 0,
				max: 4,
				step: 1,
				create: function(event, ui) { $(event.target).find("a.ui-slider-handle").append('<span class="tooltip"></span>'); },
				slide: function( event, ui ) {
					$('.hidden3').attr('value', ui.value).attr('name', 'zeit');
					searchstring();
				}
			});
		});
	$(function() {
		var actValue5 = 0;
				
		$( "#sliderKalorien" ).slider({
				range: "min",
				value: actValue5,
				min: 0,
				max: 1000,
				step: 250,
				create: function(event, ui) { $(event.target).find("a.ui-slider-handle").append('<span class="tooltip"></span>'); },
				slide: function( event, ui ) {
					$('.hidden5').attr('value', ui.value).attr('name', 'kalorien');
					searchstring();
				}
		});
	});
	$('.ajax_change').change(function(){
		searchstring();
	});
	$('.ajax_click').click(function(){
		searchstring();
	});
	$(function() {
		var query = "";
		var regionShort = "EZ";
	var acAppendToID = null;
	if ($.browser.msie  && parseInt($.browser.version, 10) === 8)
	{
		acAppendToID = "#recipesearchList2s";
	}
	else
	{
		acAppendToID = null;
	}
	$( ".recipesearch" ).autocomplete({
			search: function() {
				 query = this.value.toLowerCase();
				 querySent = $('#querySent').val();	
			},
			source: function(request, response) {
				$.ajax({
					url: '/search.xml',
					dataType: "json",
					data: { 'indent' : 'true' ,
					'facet' : 'on',
					'hl' : 'false',
					'rows' : '0',
					'q' : 'indexName:b2cRezepteIndex AND regionNamesShort_txt:('+regionShort+') '+querySent+'',
					'facet.field' : 'rezeptNameSPAutoCList_txtat',
					'facet.prefix' : ''+query+'',
					'facet.mincount' : '1',
					'facet.limit': '5'	
					},
					type: 'POST',
					cache: false,
					isAutoComplete: true,
					success: function(json){ 
						if (json!=null && json.response !=null){
							recipeCounter(json.response.numFound); 
						} else {
							recipeCounter(0);
						}
						var complete = []; 
						$.each(json.facet_counts.facet_fields.rezeptNameSPAutoCList_txtat, function(key, value) {
								if (key%2 == 0) {
									complete.push(value); 
								}
						});
						response(complete);
					}
				});
			},
			minLength: 2,
			close: function() { 
 				searchstring();
 			},
 			appendTo: acAppendToID,
			delay:200	
		});
	});
	 function validateRecipeInput(){
			var s = $('#rezept_form').serializeArray();
			var check= new Boolean(false);
			jQuery.each(s,function(i,s){
					if ( ((this.name == "schwierigkeit" || this.name == "zeit" || this.name == "kosten" || this.name == "kalorien" || this.name == "personen") && this.value!='0'  ) || this.name == "gerichteIDs_lm" || this.name == "getraenkeIDs_lm" || this.name == "saisonIDs_lm" || this.name == "anlassIDs_lm" || this.name == "laenderIDs_lm" || this.name == "gesundheitsaspekteIDs_lm" || this.name == "gesundheitsaspekte_sw_IDs_lm" || this.name == "tcmIDs_lm") {
							check=true;
					}				
			});
			if(check==true) {			
				return true;
			}
			else{
				if($('#recipesearch').val().length < 2) {
	 				$('#recipesearch').val('Zutat oder Stichwort eingeben (mind. 2 Zeichen)');
	 				return false;
	 			}
	 			else {
	 				return true;
	 		}
		}
	 } 
	 $('#rezept_form').submit(function() {		
		if(formcheck=="rezeptsuche") {	
		 	if (validateRecipeInput()) {
			 		searchstring();
			 		return true;
			 	}
			 	else {
			 		return false;
			 	}
		 }
		 else {
		 	return false;
		 }
	 });
	$('.rezeptsuchen').click(function(e) {
		formcheck = 'rezeptsuche';
		$('.search_modus').attr('value', 'rezept');
		$('#tabActive').attr('value', 'rezept');
	 	if(!$(this).hasClass('disabled')) {
		 	$('#rezept_form').submit();
		} 
		else {
			$('#recipesearch').val('Zutat oder Stichwort eingeben (mind. 2 Zeichen)');
		}
		e.preventDefault();	
	});
	$('#rezept_form').keyup(function(e) {
		if(e.keyCode == 13 && !$('#tabRecipes').hasClass('ui-tabs-hide')) {
			formcheck = 'rezeptsuche';
			$('.search_modus').attr('value', 'rezept');
			$('#tabActive').attr('value', 'rezept');
			var s = $('#rezept_form').serializeArray();
			var check= new Boolean(false);
			var test = "Zutat oder Stichwort eingeben (mind. 2 Zeichen)";					
			jQuery.each(s,function(i,s){
				if ( ((this.name == "schwierigkeit" || this.name == "zeit" || this.name == "kosten" || this.name == "kalorien" || this.name == "personen") && this.value!='0'  ) || this.name == "gerichteIDs_lm" || this.name == "getraenkeIDs_lm" || this.name == "saisonIDs_lm" || this.name == "anlassIDs_lm" || this.name == "laenderIDs_lm" || this.name == "gesundheitsaspekteIDs_lm" || this.name == "gesundheitsaspekte_sw_IDs_lm" || this.name == "tcmIDs_lm" || this.name == "video_i") {
						check=true;
				}				
			});
			if( ($('#recipesearch').val().length < 2 && check==true) || ($('#recipesearch').val().length >= 2 && ( $('#recipesearch').val() != test || check==true) )  ) {
				 	$('#rezept_form').submit();
			}
			else {
				$('#recipesearch').val('Zutat oder Stichwort eingeben (mind. 2 Zeichen)');
				e.preventDefault();
			}
		}
		if(e.keyCode == 13 && !$('#tabIngredients').hasClass('ui-tabs-hide') ) {
			formcheck = 'zutaten';
			$('.search_modus').attr('value', 'zutaten');
			$('#tabActive').attr('value', 'zutaten');
			if(!$('#zutatensuche span').hasClass('disabled')) {
				$('#rezept_form').submit();
			}
		}
 	});
});
var rezeptsuche = {
		init: function () {
			$("#recipesearch").focusin(rezeptsuche.onFocusIn);
			$("#recipesearch").focusout(rezeptsuche.onFocusOut);
			$("#recipesearch").keyup(function(e) { rezeptsuche.onChange(e); });
			$(".conRezeptsuche .head").click(rezeptsuche.onHeadClick); 
			$(".conRezeptsuche input[type=checkbox]").change(rezeptsuche.onCheckboxChange);
			$(".conRezeptsuche input[type=checkbox]:checked").parents('.option').addClass('checked');
		},
		onChange: function(ev) {
			searchstring(true);
		},
		onFocusIn: function() {
			if($(this).val()=="Zutat oder Stichwort eingeben (mind. 2 Zeichen)") {
				$(this).val('');
			} 
			return;
		},
		onFocusOut: function() {
			if($(this).val().length < 1) {
				$(this).val('Zutat oder Stichwort eingeben (mind. 2 Zeichen)');
				return;
			}	
			if($(this).val().length > 1) {
				searchstring(false);
			}
		},
		onHeadClick: function (ev) {
			if ($(this).parents(".cat").hasClass("opened")) {
				$(".cat").removeClass("opened");
			}
			else {
				$(".cat").removeClass("opened");
				$(this).parents(".cat").addClass("opened");
			}
			ev.preventDefault();
		},
		onCheckboxChange: function (ev) {		
			if( $(ev.target).attr('checked')){
				$(this).parents('.option').addClass("checked");
			} else {
				$(this).parents('.option').removeClass("checked");
			}
		}
	}
$(document).ready(function() {
	rezeptsuche.init();
	$("#sliderSchwierigkeitsgrad").slider({value: 0},{range: "min"}).find('a > span').remove();
	$("#sliderZubereitungsdauer").slider({value: 0},{range: "min"}).find('a > span').remove();
	$("#sliderKalorien").slider({value: 0},{range: "min"}).find('a > span').remove();
});
