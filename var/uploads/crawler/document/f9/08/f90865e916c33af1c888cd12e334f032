 <script>
var trackLink = function (e) {
		var source = jQuery(e.currentTarget);
		var nav = source.closest('nav');
		var category = 'unknown';
 
		if(nav.length) {
			var parent = nav.parent();
			category = parent.attr('id') || parent.attr('class');
		} else if(source.is('a[target=\'_blank\']')) {
			if(/.*\.pdf/i.test(source.attr('href'))) {
				category = 'PDF';
			} else {
				category = 'external';
			}
		} else {
			var module = source.closest('.module');
			category = module.attr('class').split(' ')[0];
			
			if(category === '' || category === 'module') {
				category = module.attr('id');	
			}
		}
 
		var link = source.is('a') ? source : source.parents('a:first');
		var href = link.attr('href');
		var target = link.attr('target');
 
		category = 'winemag-' + category;
	 
		if (window.ga) {
			if (target !== '_blank') {
				e.preventDefault();
				e.stopPropagation();
				ga('winemag.send', 'event', category, href, { 'hitCallback': function() { window.location = href; }});
			} else {
				ga('winemag.send', 'event', category, href);
			}
		}
	};
 
	jQuery('a[target=\'_blank\']').click(trackLink);
	jQuery('.module').on('click', 'a', trackLink);
	jQuery('nav').on('click', 'a', trackLink);
</script>


