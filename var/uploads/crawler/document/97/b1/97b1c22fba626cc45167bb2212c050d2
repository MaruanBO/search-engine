        <script>
        /* WRITES IN THE PROPER VIEWPORTY META TAG -- NECESSARY FOR OLDER ANDROID BROWSERS.
           THis script has been modified from the original. In addition to extra line break removal, it used 640 as a default width. 
           640 has been replaced with 320 everywhere except in the getScaleWithScreenwidth function, where it was replaced with 1280.
           In the Firefox option, getScaleWithScreenwidth()/2 was changed to getScaleWithScreenwidth()*4 to give the correct reesults.  */
            var ua = navigator.userAgent;
            var viewportChanged = false;
            var scale = 0;
            if (ua.indexOf("Android") >= 0 && ua.indexOf("AppleWebKit") >= 0) {
                var webkitVersion = parseFloat(ua.slice(ua.indexOf("AppleWebKit") + 12));
                // targets android browser, not chrome browser (http://jimbergman.net/webkit-version-in-android-version/)
                if (webkitVersion < 535) {
                    viewportChanged = true;scale = getScaleWithScreenwidth();
                    document.write('<meta name="viewport" content="width=320, initial-scale=' + scale + ', minimum-scale=' + scale + ', maximum-scale=' + scale + '" />');
                }
            }
            if (ua.indexOf("Firefox") >= 0) {
                viewportChanged = true;scale = (getScaleWithScreenwidth()*4);
                document.write('<meta name="viewport" content="width=320, user-scalable=false, initial-scale=' + scale + '" />');
            }
            if (!viewportChanged) {document.write('<meta name="viewport" content="width=320, user-scalable=false" />');}
            if (ua.indexOf("IEMobile") >= 0) {document.write('<meta name="MobileOptimized" content="320" />');}
            document.write('<meta name="HandheldFriendly" content="true"/>');
            function getScaleWithScreenwidth() {var viewportWidth = 1280;var screenWidth = window.innerWidth;return (screenWidth / viewportWidth);}
        </script>
