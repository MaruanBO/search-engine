<script type="text/javascript" language="javascript">

    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regexS = "[\\?&]" + name + "=([^&#]*)";
        var regex = new RegExp(regexS);
        var results = regex.exec(window.location.href.toLowerCase());
        if (results == null)
            return "";
        else
            return decodeURIComponent(encodeURIComponent(results[1].replace(/\+/g, " ")));
    }

    $(document).ready(function () {
        var exdate = new Date();
        exdate.setTime(exdate.getTime() + (24 * 60 * 60 * 1000));
        var checkcookie = getCookie("ACQIntercept");
        if (window.location.href.toLowerCase().indexOf('offers') == -1) {
            if (getParameterByName("rid") != "" || window.location.href.toLowerCase().indexOf('profile') != -1) {
                    setCookie("ACQIntercept", "yes", exdate); //cookie valid for 24hours                   
                }
                else if ((checkcookie == null || checkcookie == "")) {
                    var overlayDelay = $(".OverlayDelay").text();
                    var sh = $(window).height() / 2;
                    var sw = $(window).width() / 2;
                    var pboxh = $(".registerform").height() / 2;
                    var pboxw = $(".registerform").width() / 2;
                    $("#maskregOverlay").delay(overlayDelay).fadeIn("slow").css({ 'z-index': '10000' });

                    $(".registerform").delay(overlayDelay).fadeIn("slow", callWaterMark()).css({ 'left': '125px', 'top': '210px', 'z-index': '100005', 'width': '736px' });
                    var omni = s_gi(s_account);
                    omni.templtv = s.linkTrackVars;
                    omni.templte = s.linkTrackEvents;
                    omni.linkTrackVars = 'pageName';
                    omni.channel = "RightAtHome";
                    omni.pageName = "ACQ_INTERCEPT_LANDING";
                    var s_code = omni.t();

                }            
        }

        $("#maskregOverlay").click(function () {
            $("#maskregOverlay, .registerform").fadeOut();
            setCookie("ACQIntercept", "yes", exdate); //cookie valid for 24hours
        });

        $('#ctl00_OverlayControl_SubmitButton').click(function () {
            if (Page_IsValid) {

                var omni = s_gi(s_account);
                omni.templtv = s.linkTrackVars;
                omni.templte = s.linkTrackEvents;
                omni.linkTrackVars = 'pageName';
                omni.channel = "RightAtHome";
                omni.pageName = "ACQ_INTERCEPT_THANK-YOU";
                var s_code = omni.t();
            }

        });

    });


    function setCookie(c_name, value, exhours) {
        var c_value = escape(value) + ((exhours == null) ? "" : "; expires=" + exhours.toUTCString()) + "; path=/";
        document.cookie = c_name + "=" + c_value;
    }

    function getCookie(c_name) {
        var i, x, y, ARRcookies = document.cookie.split(";");
        for (i = 0; i < ARRcookies.length; i++) {
            x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
            y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
            x = x.replace(/^\s+|\s+$/g, "");
            if (x == c_name) {
                return unescape(y);
            }
        }
    }
   
</script>

