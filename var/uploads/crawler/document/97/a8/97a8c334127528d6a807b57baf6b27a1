	<script type="text/javascript">
	    $(document).ready(function () {
	        // Set default open/close settings
	        $('.accordian_Content').hide(); // Hide/close all containers

	        //On Click
	        $('.acchead').click(function () {
                // Check if current element is already active
	            if ($(this).hasClass('active')) {
	                $(this).toggleClass('active').next().animate({ height: 'toggle', opacity: 'toggle' });
	                return;
	            }

	            $('.acchead.active').removeClass('active').next().animate({ height: 'toggle', opacity: 'toggle' });
	            $(this).toggleClass('active').next().slideDown('fast');

	        });
	    });
	</script>
