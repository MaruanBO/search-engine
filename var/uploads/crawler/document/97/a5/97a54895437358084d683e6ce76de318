					function setCMlinks(){
						 var width,height;
						 width = $('#container').width();
						 height = Math.ceil(width * .4341142 )
						 var t_margin = 554 - height ;
						 
						$('#section_peak').css('top',height+82 + 'px');
						$('#social_and_CMlinks').css('margin-top',t_margin*-1);
						 if(width<=1008){
							 $('#social_and_CMlinks').css('margin-top',"40px");
							$('#section_peak').css('top',675 + 'px');
						 }if(width<=740)
							 $('#social_and_CMlinks').css('margin-top',"25px");
						 if(width<=553){
							 $('#social_and_CMlinks').css('margin-top',"-40px");
							$('#section_peak').css('top',620 + 'px');
						}
						$('#section_peak').show();
					}

					$(document).ready(function(){
						$("select[name='g'] option").each(function(){
								var text = $(this).text(),
								fixed_text = 'I am a ' + text;
								$(this).text(fixed_text);
						});
						
						$("input[name='z']").attr("placeholder", "Enter zip code");
						$("input[name='e']").attr("placeholder", "Enter e-mail");
						$('#submit').val('Browse Free Now!');
						
						// use a placeholder shim for all ie versions
						if ($.browser.msie) {
							$('[placeholder]').focus(function() { //ie 9 and lower  html5 placeholder shim
								var input = $(this);
								if (input.val() == input.attr('placeholder')) {
									input.val('');
									input.removeClass('placeholder');
								}
							}).blur(function() {
								var input = $(this);
								if (input.val() == '' || input.val() == input.attr('placeholder')) {
									input.addClass('placeholder');
									input.val(input.attr('placeholder'));
								}
							}).blur();
							$('#start_signup_form').submit(function(event) {
								if ($("input[name='z']").val() == $("input[name='z']").attr("placeholder")) {
									$("input[name='z']").val('');
								}
								if ($("input[name='e']").val() == $("input[name='e']").attr("placeholder")) {
									$("input[name='e']").val('');
								}
								return true;
							});
						}
						
						/*
						if (navigator.userAgent.indexOf('Safari') != -1 || navigator.userAgent.indexOf('Chrome') != -1){
							$('select').css('background','white url(/images/select_arrow.png) 93%  no-repeat');
						}
						if ( $.browser.mozilla == true && $.browser.version > '28.0' ) {
