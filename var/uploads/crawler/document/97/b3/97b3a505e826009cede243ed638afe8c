 <script type="text/javascript">
  $(document).ready(function() {
   var lParameters = { 'bcove' : {'width' : 480, 'height' : 270, 'margin' : 0, 'padding' : 0},
    'youtube' : {'width' : 680, 'height' : 385, 'margin' : 0, 'padding' : 2},
    'reserve' : {'width' : 260, 'height' : 407, 'margin' : 0, 'padding' : 0},
    'gmap' : {'width' : 940, 'height' : 600, 'margin' : 0, 'padding' : 0},
    'default' : {'width' : 712, 'height' : 500, 'margin' : 0, 'padding' : 2}};
   $("a.fancyBoxLink").each(function(){
    var lParameter = lParameters['default'];
    var lMedia = $(this).attr('href');
    if(lMedia.match(/bcove.me/i) || lMedia.match(/brightcove/i))
    {
     lParameter = lParameters['bcove'];
    }
    else if (lMedia.match(/youtube/i))
    {
     lParameter = lParameters['youtube'];
    }
    else if (lMedia.match(/reserve/i))
    {
     lParameter = lParameters['reserve'];
    }
    else if (lMedia.match(/gmap/i))
    {
     lParameter = lParameters['gmap'];
    }

    $(this).fancybox({
     'width' : lParameter.width,
     'height' : lParameter.height,
     'margin' : lParameter.margin,
     'padding' : lParameter.padding,
     'showNavArrows' : false,
     'autoScale' : false,
     'transitionIn' : 'none',
     'transitionOut' : 'none',
     'scrolling' : 'no',
     'type' : 'iframe',
     'overlayOpacity' : 0.8,
     'overlayColor' : '#000',
     'titleShow' : false
    });
   });

   $("#shadow").css("height", $(document).height()).hide();
  });
 </script>
