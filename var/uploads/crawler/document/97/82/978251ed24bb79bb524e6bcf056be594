<script type="text/javascript">
$(document).ready(function(){

  $('.video-button').on("click", function() {
    var element = $(this).parent().parent().next().children();
    var vidId = $(element).data("vid-src");
    var youtube = '<iframe width="560" height="315" src="//www.youtube.com/embed/'+ vidId +'?autoplay=1&showinfo=0&modestbranding=1&rel=0&autohide=1" frameborder="0" allowfullscreen></iframe>';
    $(element).prepend(youtube);
    $(element).parent().fadeIn();
  });

  $('.vidplay button').on("click", function() {
    var iframe = $(this).prev();
    var vid = $(this).parent().parent();
    iframe.remove('iframe');
    vid.hide();
  });
});
</script>
	
