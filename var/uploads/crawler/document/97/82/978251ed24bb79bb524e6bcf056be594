	var redirectURL = "http://www.otterbox.asia";
	var countryName = "Singapore";
	var link = jQuery("<a/>", {
		'href': redirectURL,
		'text': redirectURL
	});
	
	$("#geoIpDialog #geolocation").text(countryName);
	$("#geoIpDialog #geolocation_link").append(link);
	
	var dialogTitle = (typeof geoipdialog_title === 'undefined') ? "Information" : geoipdialog_title;
	
	var buttons = [
	    {
			text: 'OK',
			click: function() {
				$(this).dialog("close");
				window.location = redirectURL;
			}
	    },
	    {
			text: 'Cancel',
			'class': 'confirm',
			click: function() {
				$(this).dialog("close");
			}
	    },
	];
	
	jQuery("#geoIpDialog").dialog({
	  	bgiframe: true,
		autoOpen: true,
		modal: true,
