<script type="text/javascript">	
		$( ".submitEmail" ).unbind('click').on( "click", function( event ) {
			event.preventDefault();
			
                        // Close the veggieburger to show the dialog if on mobile
                        if($('.veggieburger.active').length)
                                    $('.veggieburger-icon').click();
                        

			// validate the entry
			var email = $(this).siblings(".newsletterEmail").val();
			if ( !validateEmail($(this).siblings(".newsletterEmail").get(0) ) ) {
				$.fancybox(
					'<h2 style="color: #007987; font-size:20px; text-transform: uppercase; padding: 0 20px; margin-top: 0;">Invalid Email Address</h2><p style="padding: 10px 20px 0; font-size: 14px; font-weight: bold; line-height: 18px; color: #666;">Please enter a valid email address.</p>',
					{
	        				'autoDimensions'	    : true,
	        				'maxWidth'				: 600,
						'hideOnContentClick'    : true,
						'overlayOpacity'        : 0.9,
						'overlayColor'          : '#000'
					}
				);
				return false;
			}
			
			<!--_gaq.push(['_trackEvent', 'Footer Newsletter Signup', 'signup']); -->
			$.ajax({
				url: "http://www.otterbox.com/on/demandware.store/Sites-otterbox_us_v2-Site/default/Press-NewsLetterSignup?context=footer",
				data: $(this).parent(".newslettersignup").serialize(),
				dataType: "json",
				type: "POST"
			})
			.done(function(jsondata) {
				if (jsondata.success) {
					$(this).siblings(".newsletterEmail").val("");
					$.fancybox(
						'<h2 style="color: #007987; font-size:20px; text-transform: uppercase; padding: 0 20px; margin-top: 0;">Thanks For Signing Up</h2><p style="padding: 10px 20px 0; font-size: 14px; font-weight: bold; line-height: 18px; color: #666;">Your email sign-up went through successfully.</p><p style="padding: 10px 20px 0; font-size: 12px; font-style: italic; line-height: 15px; color: #666;">OtterBox.com adheres to a strict privacy policy. We do not share, lend, sell or otherwise disclose our subscriber&rsquo;s names. If  you would like to manage your subcription to our emails <a href="/email-unsubscribe">click here</a>.</p>',
						{
	        					'autoDimensions'	    : true,
	        					'maxWidth'				: 600,
							'hideOnContentClick'    : true,
							'overlayOpacity'        : 0.9,
							'overlayColor'          : '#000'
						}
					);
				} else {
					$.fancybox(
						'<h2 style="color: #007987; font-size:20px; text-transform: uppercase; padding: 0 20px; margin-top: 0;">Error Signing Up</h2><p style="padding: 10px 20px 0; font-size: 14px; font-weight: bold; line-height: 18px; color: #666;">Your email sign-up failed. (' + jsondata.error + ')</p>',
						{
	        					'autoDimensions'	    : true,
	        					'maxWidth'				: 600,
							'hideOnContentClick'    : true,
							'overlayOpacity'        : 0.9,
							'overlayColor'          : '#000'
						}
					);
				}
			})
			.fail(function() {
				$.fancybox(
					'<h2 style="color: #007987; font-size:20px; text-transform: uppercase; padding: 0 20px; margin-top: 0;">Error Signing Up</h2><p style="padding: 10px 20px 0; font-size: 14px; font-weight: bold; line-height: 18px; color: #666;">Your email sign-up failed.</p><p style="padding: 10px 20px 0; font-size: 12px; font-style: italic; line-height: 15px; color: #666;">OtterBox.com adheres to a strict privacy policy. We do not share, lend, sell or otherwise disclose our subscriber&rsquo;s names. If  you would like to manage your subcription to our emails <a href="/email-unsubscribe">click here</a>.</p>',
					{
	        			'autoDimensions'	    : true,
	        			'maxWidth'				: 600,
						'hideOnContentClick'    : true,
						'overlayOpacity'        : 0.9,
						'overlayColor'          : '#000'
					});
			});
		});
</script>
</div>
		</div> <!-- End content-asset -->
	


	



		




	
		






	
		
