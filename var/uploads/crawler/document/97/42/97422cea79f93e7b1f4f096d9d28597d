<!--Fix for share box on all kitchen pages-->
<script>
    $(document).ready(function () {
        if(!window.location.href.match(/kitchen-collections_(ice-collection-design|kitchen-ice-collection|ice-collection-innovation|kitchen-stainless-collection|stainless-collection-design|stainless-collection-innovation)/g)){
	         var share_link = $('<a onmouseout="return addthis_close();">share</a>');
        
	         share_link.attr('href', '//www.addthis.com/bookmark.php?v=250&amp;pubid=ra-5009b7c36c29ca4d');
	         share_link.addClass('addthis_button share');

	         $('#shareContact li:first-child').replaceWith($('<li></li>').append(share_link));

	         $('#shareContact li:first-child a').live('mouseover', function () {
            addthis_open(this, '', '[URL]', '[TITLE]');
            });

            console.log(' - ShareBox Fix for kitchen pages. Applied - on header_p2');
        }
    });
    
    $(window).load(function(){
        
        var spacer = 30;
        
        var oneLeft = spacer + $(".everydaycare").width();
        $(".topMenuListDividerOne").css("left", oneLeft);

        var twoLeft = spacer + oneLeft + $(".laundry").width();
        $(".topMenuListDividerTwo").css("left", twoLeft);

        var threeLeft = spacer + twoLeft + $(".kitchen").width();
        $(".topMenuListDividerThree").css("left", threeLeft);

        var fourLeft = spacer + threeLeft + $(".wholehome").width();
        $(".topMenuListDividerFour").css("left", fourLeft);

        var fiveLeft = spacer + fourLeft + $(".accessories").width();
        $(".topMenuListDividerFive").css("left", fiveLeft);
    
    });
</script>
