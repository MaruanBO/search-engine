<script type="text/javascript"><!--
var bgArray = new Array("url('/images/content/masthead_bg_1.jpg') no-repeat scroll center -1px transparent",
  "url('/images/content/masthead_bg_2.jpg') no-repeat scroll center -1px transparent",
  "url('/images/content/masthead_bg_3.jpg') no-repeat scroll center -1px transparent",
  "url('/images/content/masthead_bg_4.jpg') no-repeat scroll center -1px transparent");

  changeHomeBackground(0);


function changeHomeBackground(currSlide) {
  $("body").css("background", bgArray[currSlide]);

  switch(currSlide) {
    case 0:
      $("#legal_footer_content p#slide1").css("display","block");
      $("#legal_footer_content p#slide2a").css("display","none");
      $("#legal_footer_content p#slide2b").css("display","none");
      $("#legal_footer_content p#slide4").css("display","none");
      break;
    case 1:
      $("#legal_footer_content p#slide1").css("display","none");
      $("#legal_footer_content p#slide2a").css("display","block");
      $("#legal_footer_content p#slide2b").css("display","block");
      $("#legal_footer_content p#slide4").css("display","none");
      break;
    case 2:
      $("#legal_footer_content p#slide1").css("display","none");
      $("#legal_footer_content p#slide2a").css("display","none");
      $("#legal_footer_content p#slide2b").css("display","none");
      $("#legal_footer_content p#slide4").css("display","none");
      break;
    case 3:
      $("#legal_footer_content p#slide1").css("display","none");
      $("#legal_footer_content p#slide2a").css("display","none");
      $("#legal_footer_content p#slide2b").css("display","none");
      $("#legal_footer_content p#slide4").css("display","block");
      break;
  }
}

function seeItWorkPopup(urlToOpen, windowName, windowParams) {
   window.open (urlToOpen, "seeitworkwindow", windowParams);
}
