<script>
$( document ).ready(function() {
	jQuery('.horizontalCarousel').jcarousel();

	jQuery('.jcarousel-prev').on('jcarouselcontrol:active', function() {
	    $(this).removeClass('inactive');
	}).on('jcarouselcontrol:inactive', function() {
	    $(this).addClass('inactive');
	}).jcarouselControl({
	    target: '-='+scrollInt
	});

	jQuery('.jcarousel-next').on('jcarouselcontrol:active', function() {
	    $(this).removeClass('inactive');
	}).on('jcarouselcontrol:inactive', function() {
	    $(this).addClass('inactive');
	}).jcarouselControl({
	    target: '+='+scrollInt
	});
});
</script>
