      var ajaxfinished = false;

      function enableFormSubmit() {
            ajaxfinished = false;
      }

      function updateCustomerCookie(emailaddress, formname) {
            if (emailaddress != null && emailaddress != '') {
                  var url = "/on/demandware.store/Sites-Gardeners-Site/default/Account-CustomerCookie";
                  url+="?email="+emailaddress;
                  //var url = "on/demandware.store/Sites-Gardeners-Site/default/Customer-CustomerCookie?email="+emailaddress;
                                 
                  jQuery.ajax({
                        url: url,
                  type: "GET",
                  success: function(res){
                        //jQuery('.requestresult').empty().append('Success! '+res);
                        if (formname != null && formname != '' && ajaxfinished == false) {
                              setTimeout('enableFormSubmit()', 2000);
                              ajaxfinished = true;
                              jQuery("form[name=" + formname + "]").submit();
                        }
                  },
                  error: function(res){
                        //jQuery('.requestresult').empty().append('Failure!'+res);
                  }
                  });               
            }     
      }

