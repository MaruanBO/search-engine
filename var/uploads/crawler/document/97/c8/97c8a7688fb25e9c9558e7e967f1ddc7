	<script type="text/javascript">
	//<![CDATA[
	if ($('quick_search_type_id').value == 'zip') {
		$('oar-zip').show();
		$('oar-email').hide();
	} else {
		$('oar-zip').hide();
		$('oar-email').show();
	}

	var dataForm = new VarienForm('oar_widget_orders_and_returns_form');

	function showIdentifyBlock(id) {
		if (id == 'zip') {
			$('oar-zip').show();
			$('oar-email').hide();
		} else if (id == 'email') {
			$('oar-zip').hide();
			$('oar-email').show();
		}
		return false;
	}
	//]]>
	</script>
