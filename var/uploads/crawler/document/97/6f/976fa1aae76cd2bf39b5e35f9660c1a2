<script>
    (function ($) {
        function initMobileNavigation() {
            var btn, menu, content;
            btn = $('.hamburger');
            menu = $('#mobile-navigation');
            content = $('#page');

            if(btn.length > 0 && menu.length > 0 && content.length > 0) {
                var mobNav = new MobileNavigation({
                    btn: btn,
                    menu: menu,
                    content: content
                });
            }
        }
        function improveNavigation() {
            var items = window.location.pathname.split('/');

            for ( var i = 0; i < items.length; i += 1 ) {
                var item = items[i];
                if ( item ) {
                    item = item.toLowerCase();
                    $('#nav, #sub-nav').find('.' + item).addClass('active');
                    $('#sub-nav').removeClass('hidden');
                }
            }

            if ( BASE_URL == location.href ) {
                $('#nav .home').addClass('active');
            }
        }
        $(document).on('ready', function () {
            initMobileNavigation();
            improveNavigation();
        });

    })(jQuery);
</script>
