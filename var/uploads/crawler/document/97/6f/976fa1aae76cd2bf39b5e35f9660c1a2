    <script>
        // jQuery wrapper
        (function($){
            // Document is ready
            $(document).on('ready', function(){
                // Prepare carousel
                var $carousel = $('.carousel');
                $carousel.find('.item').eq(0).addClass('active');
                $carousel.find('.carousel-indicators li').eq(0).addClass('active');

                // Set carousel options
                $carousel.carousel({
                    interval: 5000
                });

                // Append actions to events
                $carousel.find('.carousel-indicators li').on('click', function(){
                    var $this = $(this);
                    var index = $this.index();
                    $this.parents('.carousel').carousel(index);
                });
                $carousel.find('.carousel-control').on('click', function(){
                    var $this = $(this);
                    var direction = $this.hasClass('left') ? 'prev' : 'next';
                    $this.parents('.carousel').carousel(direction);
                });
            })
        })(jQuery)
    </script>


