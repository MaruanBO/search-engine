    jQuery(function($){
        var url = 'http://www.petfooddirect.com/ajax/block/';
        var data = {  'block_type': 'page/html_welcome'}
        var blockPromise = Pet360.Ajax.call(url,data);
        blockPromise.done(function (response) {
            if (response != '-1')
            {
                $('#welcome-msg').html(response);
            }
        });
    });
</script>    <div class="container-fluid topBar">
        
