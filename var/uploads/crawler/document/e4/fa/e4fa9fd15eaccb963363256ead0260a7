      var d = new Date(), dateBits = [], dow, diff, gadsCore, cdnPath;
      d.setUTCHours(d.getUTCHours() - 9);
      dow = d.getUTCDay();
      diff = ((dow < 2) ? 2 : 5) - dow;
      diff = (diff < 1) ? diff+4 : diff;
      d.setUTCDate(d.getUTCDate() + diff);
