<script type="text/javascript">
   //<![CDATA[


   var smartSuggestLogging = {
      firstChangeTime: null,
      lastLoggedText: null,
      textBoxHistory: []
   };


   function processSearchText()
   {
      setTimeout(function ()
      {
         //If a model number token is included in the search context, add "searchTextImageBackgroundKeyword" class.
         var hasModelToken = $(".searchToken[data-token-type='3']").length > 0;

         if ($("#searchText").val() !== "" || $(".searchToken").length > 2)
         {
            $("#searchText").removeClass("searchTextImageBackgroud" + (hasModelToken ? " searchTextImageBackgroundKeyword" : ""));
            $("#searchText").addClass("searchTextWhiteBackground");
         }
         else
         {
             $("#searchText").addClass("searchTextImageBackgroud" + (hasModelToken ? " searchTextImageBackgroundKeyword" : ""));
            $("#searchText").removeClass("searchTextWhiteBackground");
         }

         var st = $("#searchText");
         if (smartSuggestLogging.firstChangeTime == null) {
            smartSuggestLogging.firstChangeTime = getMSTimestamp();
         }
         if (st.val() != smartSuggestLogging.lastLoggedText) {
            smartSuggestLogging.textBoxHistory[smartSuggestLogging.textBoxHistory.length] = { text: st.val(), time: getMSTimestamp() - smartSuggestLogging.firstChangeTime };
            smartSuggestLogging.lastLoggedText = st.val();
            
         }
      }, 0);
   }


   if (!Date.now)
   {
      Date.now = function () { return new Date().getTime(); }
   }

   function getMSTimestamp()
   {
      return Math.floor(Date.now());
   }

            

   $(document).ready(function ()
   {
      processSearchText();
      $("#searchText").bind("keydown cut paste input", function (e)
      {
         processSearchText();
      });

      //Is this a touch device?
      if ("ontouchstart" in document.documentElement || (window.navigator && window.navigator.msMaxTouchPoints && window.navigator.msMaxTouchPoints > 0)) {
         $(".drop").click(function () {
            //First hide any open dropdowns, then display the one the user clicked on, only if the dropdown clicked on isn't already open.
            if ($("ul:visible", this).length == 0) {
               $(".drop ul").hide();
               $(".drop span.arrowToggleUp").removeClass("arrowToggleUp");
               $("ul", this).show();
               $("span.arrowToggle", this).addClass("arrowToggleUp");
            } else {
               $(".drop ul").hide();
               $(".drop span.arrowToggleUp").removeClass("arrowToggleUp");
            }
         });
      } else {
         $(".drop").hoverIntent({
            over: function () {
               $("ul", this).show();
               $("span.arrowToggle", this).addClass("arrowToggleUp");
            },
            out: function () {
               $("ul", this).hide();
               $(".drop span.arrowToggleUp").removeClass("arrowToggleUp");
            },
            sensitivity: 25
         });
      }

      $(".drop").hoverIntent({
         over: function () {
            $("a.arrowToggle", this).addClass("arrowToggleUp");
            $("ul", this).show();
         },
         out: function () {
            $(".drop a.arrowToggleUp").removeClass("arrowToggleUp");
            $("ul", this).hide();
         },
         sensitivity: 25
      });


      
   });
//]]>
</script>

