<script type="text/javascript">
   var mainMenuModelSizes = { defaultWidth: 240, defaultHeight: 180, largeWidth: 640, largeHeight: 360 };
</script>
       
<script type="text/javascript">
   var mainMenuModelPlayer;
   function initializeMainMenuPlayer(appVideoId)
   {
      if ( youtubeIframeAPIReady )
      {
         $("#mainMenuModelPlayerCtn").html("").append("<div id=\"mainMenuModelPlayer\"></div>");
         if (_.isUndefined(mainMenuModelPlayer) == false && _.isUndefined(mainMenuModelPlayer.destroy) == false)
         {
            mainMenuModelPlayer.destroy();
         }
         mainMenuModelPlayer = new YT.Player("mainMenuModelPlayer", {
            videoId: appVideoId ? appVideoId : "24_1KAi94fk",
            playerVars: { 'autoplay': 0, 'format': 1 },
            height: '180',
            width: '240'
         });
         mainMenuModelPlayer.addEventListener("onStateChange", "onMainMenuPlayerStateChange");
   }
   }
   function mainMenuModelShowModelHelp(appTypeId, appVideoId)
   {
      initializeMainMenuPlayer();
      $("#mainMenuModelImages").html($("#mainMenuModelProductContent" + appTypeId).html());
      $("#mainMenuModelImages .shownModelImage").attr("src", $("#mainMenuModelImages .shownModelImage").attr("data-imgsrc"));

      if (!_.isUndefined(appVideoId) && appVideoId !== "")
      {
         loadMainMenuModelVideo(appVideoId);
         $("#mainMenuModelNocontent").show();
         if ($("#mainMenuModelFindModelNumber").offset() != null)
         {
            $(document).scrollTop($("#mainMenuModelFindModelNumber").offset().top);
         }
      }
      else
      {
         $("#mainMenuModelNocontent").hide();
      }
   }
   function loadMainMenuModelVideo(appVideoId)
   {

      initializeMainMenuPlayer(appVideoId);
   }
   function onMainMenuPlayerStateChange(e)
   {
      //
      //Are they clicking play for the first time? If yes, then expand the video player.
      //
      if (e.data != -1 && e.data != 5 && e.data != 2 && $("#mainMenuModelPlayer").width() == mainMenuModelSizes.defaultWidth && $("#mainMenuModelPlayer").height() == mainMenuModelSizes.defaultHeight)
      {
         mainMenuModelSizeVideoCtn(true);
         mainMenuModelPlayer.setSize(mainMenuModelSizes.largeWidth, mainMenuModelSizes.largeHeight);
         mainMenuModelPlayer.setPlaybackQuality("medium");
      }
   }
   function mainMenuModelResetVideo()
   {
      mainMenuModelPlayer.pauseVideo();
      mainMenuModelSizeVideoCtn(false);
      mainMenuModelPlayer.setSize(mainMenuModelSizes.defaultWidth, mainMenuModelSizes.defaultHeight);
   }
</script>

<script type="text/javascript">
   function mainMenuSwitchHelpModelImage(imgPath, e)
   {
      $("#mainMenuModelTxtMiddle #mainMenuModelImages .mainMenuModelTextWrapper p a").removeClass("modelHelpDataAndClickedLink").addClass("modelHelpDataAndVideodLink");
      $(e).removeClass("modelHelpDataAndVideodLink").addClass("modelHelpDataAndClickedLink");
      $("#mainMenuModelTxtMiddle #mainMenuModelImages .shownModelImage").attr("src", imgPath);
   }
   function mainMenuModelSizeVideoCtn(makeBigger)
   {
      if (makeBigger)
      {
         $("#mainMenuModelTxtMiddle p,#mainMenuModelTxtMiddle img.shownModelImage").hide();
         $(".mainMenuModelHeader:visible + .mainMenuModelShrinkVideo").show();
         $("#mainMenuModelPlayerCtn").css("margin", "0px 0 10px 40px");
      }
      else
      {
         $("#mainMenuModelTxtMiddle p,#mainMenuModelTxtMiddle img.shownModelImage").show();
         $(".mainMenuModelHeader:visible + .mainMenuModelShrinkVideo").hide();
         $("#mainMenuModelPlayerCtn").css("margin", "37px 0 0 10px");
      }
   }
</script>
<script type="text/javascript">
   //<![CDATA[
   function showModelHelpIfProductChosen()
   {
      if ($(".navColumn > ul > li > a.productMenuHighlight:visible").length > 0)
      {
         $("#mainMenuModelNocontent").show();
      }
      else
      {
         $("#mainMenuModelNocontent").hide();
      }
   }
   function mainMenuProcessProductLink(t)
   {
      $(".nav > .productNavWrap:visible").css("display", "none");
      $("." + t.attr("data-submenu")).css("display", "block");
      $(".productMenuLink:visible.productMenuHighlight").removeClass("productMenuHighlight");
      t.addClass("productMenuHighlight");
      $("#mainMenuModelNocontent").show();
      mainMenuModelShowModelHelp(t.attr("data-appid"), t.attr("data-ytid"));
   }
   function wirePartCatMenuClick()
   {
      $(".partCatMenuLink,.partCatMenuLinkSeeAll").hover(function ()
      {
         if ($(".brandMainMenuLink:visible.brandMenuHighlight").length > 0)
         {
            var brandLink = $(".brandMainMenuLink:visible.brandMenuHighlight")[0];

            var matchRegEx = $(this).hasClass("partCatMenuLinkSeeAll") ? /(\/shop-for-parts\/show-all\/a\d+)(?:b\d+)?(r\d+t\d+.+)/i : /(\/shop-for-parts\/a\d+)(?:b\d+)?(c\d+.+)/i;
            var match = $(this).attr("href").match(matchRegEx);

            if (!_.isNull(match) && !_.isUndefined(match))
            {
               $(this).attr("href", match[1] + "b" + $(brandLink).attr("data-brandid") + match[2]);
            }
         }
         else
         {
            var matchRegEx = $(this).hasClass("partCatMenuLinkSeeAll") ? /(\/shop-for-parts\/show-all\/a\d+)(?:b\d+)?(r\d+t\d+.+)/i : /(\/shop-for-parts\/a\d+)(?:b\d+)?(c\d+.+)/i;
            var match = $(this).attr("href").match(matchRegEx);

            if (!_.isNull(match) && !_.isUndefined(match))
            {
               $(this).attr("href", match[1] + match[2]);
            }
         }
      });
   }
   $(document).ready(function ()
   {
      $(document).click(function (e)
      {
         var visibleMenu = $(".nav:visible");
         if (visibleMenu.length == 1)
         {
            if (e.target.id !== "#outerMainMenuWrapper" && $(e.target).parents("#outerMainMenuWrapper").length == 0)
            {
               visibleMenu.slideUp();
               $("#menuIndicatorWrapper").hide();
               $(".topMenuDown").removeClass("topMenuDown").addClass("topMenuUp");
               $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuDown").addClass("topMenuUp");
               $("#mainMenuModelNocontent").hide();
            }
         }
         
         if (!("ontouchstart" in document.documentElement || window.navigator.msPointerEnabled)) {
            var visibleHeaderMenu = $(".drop ul:visible");
            if (visibleHeaderMenu.length > 0 && e.button == 0 && e.target && !$(e.target).hasClass(".drop") && $(e.target).parents(".drop").length == 0) {
               $(visibleHeaderMenu).siblings("span.arrowToggleUp").removeClass("arrowToggleUp");
               visibleHeaderMenu.hide();
            }
         }
      });
      $(".brandMainMenuLink").hoverIntent({
         over: function ()
         {
            $(".brandMainMenuLink:visible.brandMenuHighlight").removeClass("brandMenuHighlight");
            $(this).addClass("brandMenuHighlight");
          
         },
         sensitivity: 25,
         out: function () { }
      });
      $(".productMenuLink").hoverIntent(
      {
         over: function () { mainMenuProcessProductLink($(this)); },
         sensitivity: 25,
         out: function () { }
      }).click(function () {
         trackEventWithTimeSincePageLoad('Mega Menu Time Since', 'Click Mega Menu Option');
      });
      $(".topMainMenuLink").click(function (e)
      {
         e.preventDefault();
         var visibleMenu = $(".nav:visible");
         if (visibleMenu.length == 1)
         {
            if (visibleMenu[0].id == $(this).attr("data-menu"))
            {
               visibleMenu.slideUp();
               $("#menuIndicatorWrapper").hide();
               $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuDown").addClass("topMenuUp");
               $("#mainMenuModelNocontent").hide();
            }
            else
            {
               visibleMenu.insertAfter("#" + $(this).attr("data-menu"));
               $("#" + $(this).attr("data-menu")).slideDown();
               visibleMenu.slideUp();
               $("#mainMenuCurrentIndicator").css("left", $(this).attr("data-indicator"));
               $(".topMenuDown").removeClass("topMenuDown").addClass("topMenuUp");
               $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuDown").addClass("topMenuUp");
               showModelHelpIfProductChosen();
            }
         }
         else
         {
            $("#" + $(this).attr("data-menu")).slideDown();
            $("#mainMenuCurrentIndicator").css("left", $(this).attr("data-indicator"));
            $("#menuIndicatorWrapper").show();
            $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuUp").addClass("topMenuDown");
            showModelHelpIfProductChosen();
         }
         trackEventWithTimeSincePageLoad('Mega Menu Time Since', 'Click Mega Menu');
      });
      $(".topMainMenuLink").hoverIntent(function ()
      {
         var visibleMenu = $(".nav:visible");
         if (visibleMenu.length == 1)
         {
            if (visibleMenu[0].id != $(this).attr("data-menu"))
            {
               visibleMenu.hide();
               $("#" + $(this).attr("data-menu")).show();
               $("#mainMenuCurrentIndicator").css("left", $(this).attr("data-indicator"));
               $(".topMenuDown").removeClass("topMenuDown").addClass("topMenuUp");
               $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuUp").addClass("topMenuDown");
            }
         }
         else
         {
            $("#" + $(this).attr("data-menu")).slideDown();
            $("#mainMenuCurrentIndicator").css("left", $(this).attr("data-indicator"));
            $("#menuIndicatorWrapper").show();
            $("#" + $(this).attr("data-menuupdown")).removeClass("topMenuUp").addClass("topMenuDown");
         }
         showModelHelpIfProductChosen();
         closeHeaderDropDowns();
         trackEventWithTimeSincePageLoad('Mega Menu Time Since', 'Hover Mega Menu');
      }, function () { });
      $(".mainMenuSeeAll").hoverIntent(
      {
         over: function () {
            $(".productMenuHighlight,.brandMenuHighlight", $(this).parents("ul")).removeClass("productMenuHighlight brandMenuHighlight");
            if ($(this).hasClass("productMenuLink")) {
               $(".nav > .productNavWrap:visible").css("display", "none");
            }
         },
         sensitivity: 25,
         out: function () { }
      });
   });
   //]]>
</script>

