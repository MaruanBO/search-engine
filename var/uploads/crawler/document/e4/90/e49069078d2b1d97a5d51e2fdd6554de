   <script type="text/javascript">
      var isClickedAndHolding = false;
      var mouseClickPos = null;
      $(document).ready(function () {
         rc_GetCookie("HasSeenSuggestedLiveChatMsg");

         $("#suggestLiveChatContent").disableTextSelect();
         $("#suggestLiveChatContent").bind("mousedown", function (e) {
            if (e &&
                e.target &&
                ((e.target.attributes &&
                e.target.attributes.src &&
                e.target.attributes.src.value &&
                _.str.contains(e.target.attributes.src.value, ".rcchatjpg")) ||
                e.target.id == "suggestClose")) {
               return;
            }

            isClickedAndHolding = true;
            mouseClickPos = {
               y: e.offsetY === undefined ? e.layerY : e.offsetY,
               x: e.offsetX === undefined ? e.layerX : e.offsetX
            };

            $(window).mousemove(function (e) {
               if (isClickedAndHolding) {
                  //Handle situation where window is dragged through the bottom of the page
                  var heightBeforeFold = $(window).scrollTop() + $(window).height();

                  var isBelowFold = heightBeforeFold < (e.clientY + $(window).scrollTop());
                  var isAboveFold = (e.clientY + $(window).scrollTop()) < 1;
                  var isFarLeft = e.clientX + $(window).scrollLeft() < 1;
                  var isFarRight = $(window).width() > e.clientX;

                  if (isBelowFold ||
                      isAboveFold ||
                      isFarLeft) {
                     e.preventDefault();
                     return false;
                  }

                  $("#suggestLiveChatContent").css({
                     top: (e.pageY - mouseClickPos.y) - $(window).scrollTop(),
                     left: (e.pageX - mouseClickPos.x) - $(window).scrollLeft(),
                     margin: 0
                  });

                  $("body")
                     .disableTextSelect()
                     .css("cursor", "move");
               }
            }).bind("mouseup", function (e) {
               if (isClickedAndHolding) {
                  isClickedAndHolding = false;

                  $("#suggestLiveChatContent .iframeMask").remove();
                  $(this).unbind("mousemove");
                  $("body").enableTextSelect().css("cursor", "default");

                  repositionWindowIfHidden(e);
               }
            });
         });

         function repositionWindowIfHidden(e) {
            //The origin is used to determine if the mouse is on the left, right, top, or bottom.
            var origin = { x: $(window).width() / 2, y: $(window).height() / 2 };
            var windowPos = $("#suggestLiveChatContent").position();
            var slopValue = 5;

            var isHiddenLeft = (Math.abs(windowPos.left) + slopValue) > $("#suggestLiveChatContent").width() && e.clientX < origin.x;
            var isHiddenRight = (windowPos.left + slopValue) > $(window).width() && e.clientX > origin.x;

            //If hidden left or right, bump it out
            if (isHiddenLeft || isHiddenRight) {
               if (e.clientX < origin.x) {
                  $("#suggestLiveChatContent").css({ left: windowPos.left + (slopValue * 4) });
               } else {
                  $("#suggestLiveChatContent").css({ left: windowPos.left - (slopValue * 6) });
               }
               return;
            }

            //If it's hidden in the top, or bottom bump it out a bit
            var isHiddenTop = windowPos.top + slopValue < slopValue && e.clientY < origin.y;
            var slopValue = slopValue * 3;
            var isHiddenBottom = windowPos.top + slopValue > $(window).height() && e.clientY > origin.y;

            if (isHiddenTop || isHiddenBottom) {
               if (e.clientY < origin.y) {
                  $("#suggestLiveChatContent").css({ top: 0 });
               } else {
                  $("#suggestLiveChatContent").css({ top: windowPos.top - (slopValue * 2) });
               }
               return;
            }
         }

         //Suggest live chat after unsuccessful text searches
         var messageDisplayed = false;

         
            var searchCountCookie = rc_GetCookie("MultipleSearchTextCount");

            //Check that they are over the limit
         if (searchCountCookie && searchCountCookie >= '3') {
               suggestedLiveChatSource = "Text Search";
               suggestLiveChat("HasSeenSuggestedLiveChatMsg", "3", "accountID%3d1%26siteID%3d1%26queueID%3d4", "1", isNaN(searchCountCookie) ? "" : searchCountCookie);
               messageDisplayed = true;
            }
         
            //Setup live chat for inactivity, if the message hasn't been displayed
            var hasSeenMessageCookie = rc_GetCookie("HasSeenSuggestedLiveChatMsg");
            if (!messageDisplayed && !hasSeenMessageCookie) {
               setChatInterval();

               //If a link is clicked, reset idle time.
               $("a").click(function(){
                  idleTimeLiveChat = 0;
                  clearChatInterval();
                  setChatInterval();
               });
            }
         
      });

      var idleTimeLiveChat = 0;
      var idleTimeIntervalLiveChat = 1000;
      var idleChatIntervalId = null;

      function incrementTimeForLiveChat() {
         idleTimeLiveChat += idleTimeIntervalLiveChat;
         if (idleTimeLiveChat >= '45000') {
            suggestedLiveChatSource = "Idle";
            suggestLiveChat("HasSeenSuggestedLiveChatMsg", "3", "accountID%3d1%26siteID%3d1%26queueID%3d4", "2", idleTimeLiveChat);

            clearChatInterval();
         }
      }

      function setChatInterval() {
         idleChatIntervalId = setInterval(incrementTimeForLiveChat, idleTimeIntervalLiveChat);
      }

      function clearChatInterval() {
         if (idleChatIntervalId) {
            clearInterval(idleChatIntervalId);
         }
      }

      var suggestedLiveChatSource = null
      function acceptedChat() {
         closeSuggestLiveChatContent();
         var e = window.open('https://www.rcappliancepartsimages.com/PreChatSurvey.aspx?accountID=1&siteID=1&queueID=4&suggest=1', 'chatWindow', 'width=490,height=404,resizable=0,scrollbars=no,menubar=no,status=no');

         $.rcJsonPost(
         {
            type: "POST",
            url: "/General/AcceptedSuggestedLiveChat",
            success: function () {
               ga('send', 'event', "Suggest Live Chat", "Accepted Live Chat", suggestedLiveChatSource);
            },
            error: function (e) {
               console.error(e);
            }
         });
      }
   </script>
