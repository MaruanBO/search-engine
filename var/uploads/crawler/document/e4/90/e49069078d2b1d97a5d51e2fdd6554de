<script type="text/javascript">
//<![CDATA[

   $('#emailFooter').mousedown(function () {
      if ($("#emailFooter").val() == 'Enter email address')
         $("#emailFooter").val('');
   });

   $(document).ready(function () {
      ValidateFooterEmail();
      rc_PreLoadImage("btnSave", "/images/btn/btnSave.jpg");
      checkCcrGpHoldMessage();
   });

   function checkCcrGpHoldMessage(){
      
   }

   
      $(document).keydown(function(e){
         if($("#emailFooter").is(":focus") && !optionsShown && e.keyCode == "13"){
            e.preventDefault();
            TrackFooterEmailAndPost(true);
         }
      });
   

   var optionsShown = false;
   function TrackFooterEmailAndPost(isSave){
      if(isSave){
         ga('send', 'event', 'Footer NL Signup', 'Saved');
     }else{
         ga('send', 'event', 'Footer NL Signup', 'Clicked');
     }

     if(optionsShown){
         $("#newsletterForm").submit();
     }else{
         $.rcJsonPost(
         {
            url: "/Email/" + ("GetMailingLists"),
            data: { email: $("#emailFooter").val() },
            success: function(data) { 
               if(data == "validation"){
                  $("#mailingLists").html("");
                  $("#emailFootValidationMsg").addClass("formFieldError").html("Please enter a valid email address.").show();
               }
               else if(data == "error" || optionsShown){
                  $("#emailFootValidationMsg").addClass("formFieldError").html("An error has occured").show();
               }
               else{
                  optionsShown = true;
                  $("#emailFootValidationMsg").hide();
                  $("#mailingLists")
                     .html("<ul>");
                  for(var i = 0; i < data.length; i++){
                     $("#mailingLists ul").append("<li id='mailingListOption" + i + "'>");
                     $("#mailingLists ul li#mailingListOption" + i)
                        .append("<input class='noBorder' name='email.MailingLists' id='MailingLists" + i + "' type='checkbox' value='" + data[i].ListId + "' " + (data[i].OptInDefault ? "checked='checked'" : "") + " />")
                        .append("<label for='MailingLists" + i + "'>" + data[i].Description + "</label>");
                  }
                  $("#saveNewsletter").removeClass("none");
                  $("#siteFooter").height($("#siteFooter").height() + $("#mailingLists").height());
                  window.scrollTo(0,$(document).height());
               }
            }
         });
      }
   }

   function ValidateFooterEmail() {
     
     var result = '';

      if (result != null && result != "") {
         if (result == "1") {
            $("#emailFootValidationMsg").addClass("validationFooterNewsletterSuccess").html("Thank you for joining our email list.");
            window.scrollBy(0, $(document).height());
      }
         else if (result == "2") {
            $("#emailFootValidationMsg").addClass("formFieldError").html("Please enter a valid email address.");
            window.scrollBy(0, $(document).height());
         }
         else if (result == "3") {
            $("#emailFootValidationMsg").addClass("formFieldError").html("Please choose one or more mailing lists to receive emails for discounts and free-money saving tips.");
            window.scrollBy(0, $(document).height());
         }
     }
     }

   function GoHDStore()
   {
	   var hWnd = window.open("http://www.homedepot.com/webapp/wcs/stores/servlet/StoreFinderView?langId=-1&storeId=10051&catalogId=10053","Window","width=473,height=608,resizable=yes,scrollbars=yes");
	   if ((document.window != null) && (!hWnd.opener))
		   hWnd.opener = document.window;
   }

