        $(".diyLogoSprite,.affiliateLogoSprite,.youtubeGraySprite,.facebookGraySprite,.twitterGraySprite,.googlePlusGraySprite,.pinterestGraySprite,.instagramGraySprite,.linkedinGraySprite")
        .find("span")
