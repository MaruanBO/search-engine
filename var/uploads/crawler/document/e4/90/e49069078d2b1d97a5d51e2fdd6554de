   <script type="text/javascript">
      $(document).ready(function () {
         $("#searchText").focus();
         hr.heroImageRotation();
      });

      var hr = new function() {
         var i = 0;
         var count = 3 - 1;
         var interval = null;
         var timeInterval = 5000;

         this.heroImageRotation = function () {
            i = transitionTo(i);

            $("#contentOverHeros a").mouseover(function (e) {
               clearHeroInterval();
               $("#contentOverHeros a").addClass("hover");
            }).mouseout(function (e) {
               setHeroInterval();
               $("#contentOverHeros a").removeClass("hover");
            });

            $(".selector").click(function (e) {
               var selectedImage = $(this).attr("data-ix");
               i = transitionTo(selectedImage);
            });

            $("#overHeroLinks").mouseover(function (e) {
               clearHeroInterval();
            }).mouseout(function (e) {
               setHeroInterval();
            });
         }

         var transition = function () {
            i = transitionTo(i);
         }

         var transitionTo = function (ix) {
            clearHeroInterval();

            if ($("#heroImg" + ix).is(":hidden")) {
               $(".heroImages,.promoText").fadeOut();
               $("#heroImg" + ix + ",#promoText" + ix).fadeIn();

               $("#promoSelector .selector").removeClass("active");
               $("#promoSelector #selector" + ix).addClass("active");

               if (ix >= count) {
                  ix = 0;
               } else {
                  ix++;
               }               
            } else {
               if (ix >= count) {
                  ix = 0;
               } else {
                  ix++;
               }
            }

            setHeroInterval();
            return ix;
         }

         var setHeroInterval = function () {
            interval = setTimeout(transition, timeInterval);
         }

         var clearHeroInterval = function () {
            if (interval) {
               clearInterval(interval);
            }
         }
      };
   </script>
