<script type="text/javascript">
jQuery(document).ready(function() {	
	jQuery( "#textresizer a" ).textresizer({
         		target: "#content",
         		type: "cssClass",
         		sizes: ["small","medium", "large"],
         		selectedIndex: 0
	});
	jQuery.fn.cycle.updateActivePagerLink = function(pager, currSlideIndex) { 
    		jQuery(pager).find('li').removeClass('activeLI') 
       		 .filter('li:eq('+currSlideIndex+')').addClass('activeLI'); 	
	}; 
	jQuery('#main_cycle').cycle({
		fx: 'fade',
		timeout: 3000,
		pager:  '#nav',
		pauseOnPagerHover: true,		
		pause: 1,
		pagerAnchorBuilder: function(idx, slide) {
	           		return '#nav <span></span>';
	        	}
	});
	jQuery('#mycarousel').jcarousel({
        		vertical: true,
		size: 7,
		scroll: 1,
		wrap: 'last',
		auto: 7,
		initCallback: mycarousel_initCallback,
		itemFirstInCallback: mycarousel_itemFirstInCallback,
		buttonNextHTML: null,
		buttonPrevHTML: null
	});
function mycarousel_initCallback(carousel) {
	carousel.clip.find('li a').click(function() {
		carousel.stopAuto();
	});
	
	carousel.clip.find('li a').mouseout(function() {
		carousel.startAuto();
	});
	
};
function mycarousel_itemFirstInCallback(carousel, item, idx, state) {
};
function mycarousel_itemVisibleInCallback(carousel, item, idx, state) {
};
});
</script>
<script src="/bhcv2/bhcadmin.nsf/images/jquery.history/$file/jquery.history_remote.pack.js" type="text/javascript"></script>
<script type="text/javascript">
	jQuery(document).ready( function() {
  			
		var tabContainers = jQuery('div.healthyrecipes > div');
		tabContainers.hide().filter(':first').show();
				
		jQuery('div.healthyrecipes ul.tabs a').click(function(){
			
			tabContainers.hide().filter(this.hash).show();
			jQuery('div.healthyrecipes ul.tabs a').removeClass('active');
			jQuery(this).addClass('active');		
			return false;
		});
		
		jQuery('div.healthyrecipes ul.tabs a').filter(':first').click();
	});
</script>
