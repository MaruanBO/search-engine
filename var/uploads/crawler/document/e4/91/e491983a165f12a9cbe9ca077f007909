<script>
            function set_topbar(){
                var width = jQuery(window).width();
 var left_start = (width/2) + 420;
 jQuery('#top-floating-bar').css('visibility', 'visible');
jQuery('#topbar_close').css('margin-left', left_start+"px");
 jQuery('#top-floating-bar').slideDown('slow');
            }
function close_topbar(){
 jQuery('#top-floating-bar').click(function(){
	jQuery(this).slideUp('slow');
 });
}
</script>
<script>
jQuery(document).ready(function(){	
	jQuery( "#textresizer a" ).textresizer({
         		target: "#content",
         		type: "cssClass",
         		sizes: ["small","medium", "large"],
         		selectedIndex: 0
		});
		for(i=1;i<4;i++){		
			CreateMenu(i);  			
		}
		if (jQuery.browser.msie) {
			jQuery("#megaanchor1,#megaanchor2,#megaanchor3").hover(function(){
  		   		jQuery("#mlsProfessionalsSelected").css({ visibility: "hidden" });
		   	},function(){
			    jQuery("#mlsProfessionalsSelected").css({ visibility: "visible" });
			});
		}
	});
	
	function CreateMenu(k){
		jQuery.ajax({
	  			url: "/megamenucont"+k+".htm",
	  			cache: false,
	  			success: function(html){
    				jQuery('#megamenu'+k).append(html);
	  			}
		});
	}
</script>
