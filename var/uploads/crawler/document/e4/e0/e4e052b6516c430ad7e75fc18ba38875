<script type="text/javascript">
    var _gaq = _gaq || [];
    
    $('#financingCenterUpsell a').hover(function ()
	{
		$(this).css({
			'text-decoration': 'none',
			'color': '#000'
		});
	}, function()
	{
		$(this).css({
			'text-decoration': 'none',
			'color': '#505050'
		});
	});
	
	$('#financingCenterUpsell a b').hover(function ()
	{
		$(this).css(
			'text-decoration',
			'underline');
	}, function ()
	{
		$(this).css(
			'text-decoration',
			'none');	
	});
	
	function cookieAlert()
	{
		$.cookie(
			'bbsfcABclosed',
			'1',
			{
				path: '/',
				domain: '.bizbuysell.com',
				expires: 5*365
			});
	}
	
	if (!$('body').hasClass('hideFinancingCenterUpsell'))
	{
		var userClosedFCAlert = ($.cookie('bbsfcABclosed') || '');

		if (userClosedFCAlert == '0' || userClosedFCAlert == '')
		{
			$('#financingCenterUpsell').fadeIn("slow", function()
			{
				$('#financingCenterUpsell').css('display', 'block');
			});
		}
		
		$('#financingCenterUpsellClose').executeBeforeNavigation(
		    function ()
		    {
		        _gaq.push(['_trackEvent', 'financeCenter', 'alertBarClose', '/default.aspx']);
		        cookieAlert();
		        $('#financingCenterUpsell').fadeOut("slow");
		    });
		
		$('#financingCenterAlertLink').executeBeforeNavigation(
		    function ()
		    {
		        _gaq.push(['_trackEvent', 'financeCenter', 'alertBar', '/default.aspx']);
		        cookieAlert();
		    });
	}
</script>       
	</div>
</div>
