	<script type="text/javascript">
		$(document).ready(function()
	    {
	        Geo.SetDefaultText({ SelectStateText: 'Select County' });
	        Geo.LinkStateAndCountyDropDowns('ddlStates', 'ddlCounties');
	        Geo.LinkStateAndCountyDropDowns('ddlMobileStates', 'ddlMobileCounties');
	        Geo.LinkStateAndCountyDropDowns('phoneBrokerStates', 'phoneBrokerCounties', { UseRegionID: true });
	        Geo.LinkStateAndCountyDropDowns('brokerStates', 'brokerCounties', { UseRegionID: true });
	    });

	    $(document).bind(
	        'OnMyBBSLoginSuccessful',
	        function(
	            evt,
	            registrationType,
	            brokerData)
	        {
	            brokerData = (
	                brokerData ||
	                {
	                    IsBroker: false,
	                    OnBrokerLoginSuccessfulRedirectUrl: ''
	                });

	            var isBroker = !!brokerData.IsBroker;
	            var onBrokerLoginSuccessfulRedirectUrl = (brokerData.OnBrokerLoginSuccessfulRedirectUrl || '');

	            if (isBroker)
	            {
	                if (onBrokerLoginSuccessfulRedirectUrl != '')
	                {
	                    window.location = onBrokerLoginSuccessfulRedirectUrl;
	                }
	                else
	                {
						window.location = 'http://www.bizbuysell.com/brokers/';
	                }
	            }
	            else
	            {
					window.location = 'http://www.bizbuysell.com/mybbs/';
	            }
	        });

	    // MAIN SEARCH FORM TOGGLE
		$('#myTabDesktop li').bind('click', function(e)
	    {
	        e.preventDefault();
	        $('#myTabDesktop li').removeClass('active');
	        $(this).addClass('active');
	        var datatarget = $(this).attr('data-target');
	        $('.mainform').hide();
	        $(datatarget).fadeIn('fast');
	    });

	    $('#myTab a').click(function(e)
	    {
	        e.preventDefault();
	        $(this).tab('show');
	    });

	    $('.btn-cookie-location').click(function()
	    {
	    	var state = $('#ddlStates').val();
	    	if (state != 'XX')
	    	{
				$.cookie('LastSelected_StateLegacyCode', state, { domain: '.bizbuysell.com', path: '/', expires: 5*365 });

	    		//cleaning LastSelected_RegionID
				$.cookie('LastSelected_RegionID', '', { domain: '.bizbuysell.com', path: '/', expires: 5*365 });
	    	}
	    });
	    
	    $(function()
	    {
	    	$('.accordionTabs').on('show', function(e)
	    	{
	    		$(e.target).prev('.accordion-heading').find('.accordion-toggle').addClass('active');
	    	});

	    	$('.accordionTabs').on('hide', function(e)
	    	{
	    		$(this).find('.accordion-toggle').not($(e.target)).removeClass('active');
	    	});
	    });
	</script>
