    <div id="jcarousel-control">
        <a href="#" class="current" data-carousel-index="1"><img src="http://bio-img.s3.amazonaws.com/circle-pagination-active.png" border="0" /></a>
        <a href="#" data-carousel-index="4"><img src="http://bio-img.s3.amazonaws.com/circle-pagination-next.png" border="0" /></a>
        <a href="#" data-carousel-index="7"><img src="/images/circle-pagination-next.png" border="0" /></a>
  
        <a href="#" data-carousel-index="10"><img src="/images/circle-pagination-next.png" border="0" /></a>
  
        <!--REMOVE COMMENT TAGS TO ADD ANOTER DOT/PAGE TO CAROUSEL
        <a href="#" data-carousel-index="13"><img src="/images/circle-pagination-next.png" border="0" /></a>-->
    </div>

<script type="text/javascript">
  jQuery(function() {
    jQuery('#first-carousel').jcarousel({
      scroll: 3,
      visible: 3,
      initCallback: carouselInitCallback
    });

    jQuery('.jcarousel-next').live('click', function() {
      var $currentItem = jQuery('#jcarousel-control').find('.current')
      if (!$currentItem.is(':last-child')) {
        $currentItem.removeClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-next.png')
          .parent()
          .next()
          .addClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-active.png');
      }
    });

    jQuery('.jcarousel-prev').live('click', function() {
      var $currentItem = jQuery('#jcarousel-control').find('.current')
      if (!$currentItem.is(':first-child')) {
        $currentItem.removeClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-next.png')
          .parent()
          .prev()
          .addClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-active.png');
      }
    });

    function carouselInitCallback(carousel) {
      jQuery('#jcarousel-control').find('a').click(function() {
        var $this = jQuery(this);

        $this.addClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-active.png');
        $this.siblings()
          .removeClass('current')
          .find('img')
          .attr('src', 'http://bio-img.s3.amazonaws.com/circle-pagination-next.png');

        carousel.scroll(jQuery.jcarousel.intval($this.attr('data-carousel-index')));

        return false;
        });

        jQuery('')
        }
    });
</script>
    <div id="scroller">
            <ul id="first-carousel" class="jcarousel-skin-tango">
            <li>
