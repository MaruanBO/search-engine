			<script type='text/javascript'>
				function ClearBox(box_id, box_prompt)
				{
					var box = document.getElementById(box_id);
					if (box.value == box_prompt)
						box.value = '';
				}
				function UnClearBox(box_id, box_prompt)
				{
					var box = document.getElementById(box_id);
					if (box.value == '')
						box.value = box_prompt;
				}
				</script>
