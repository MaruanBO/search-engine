<!--//--><![CDATA[//><!-->
function iP(num) {
var result = 1;
if (isNaN(num) || num < 0) {
result = 0;
}
if (num == 1 || num == 2) {
result = 1;
}
for (var i = 2; i < num; i++) {
if (num % i == 0) {
result = 0;
break;
}
}
return result;
}
function CriaArray(n) {
this.length = n
for (var i = 1; i <= n; i++) {
this[i] = 1
}
}
function infP(npSize) {
var i, k;
var m;
if (npSize <= 0) {
return false;
}
var flags = new CriaArray(npSize);
for (i = 2; i <= flags.length; i++) {
if (flags[i]) {
for (k = i + 1; k <= flags.length; k += i) {
flags[k] = 0;
}
}
}
if (npSize > 2) {
m = 2 + ",";
}
for (i = 0; i <= flags.length; i++) {
if (flags[i] == 0) {
m += i + ",";
}
}
return m;
}
function cC(nn, vv, dd) {
var ex;
if (dd) {
var dte = new Date();
dte.setTime(dte.getTime() + (dd * 24 * 60 * 60 * 1000));
ex = "; expires=" + dte.toGMTString();
} else {
ex = "";
}
document.cookie = nn + "=" + vv + ex + "; path=/";
}
function rC(nY) {
var nYEQ = nY + "=";
var ca = document.cookie.split(';');
for (var i = 0; i < ca.length; i++) {
var c = ca[i];
while (c.charAt(0) == ' ') c = c.substring(1, c.length);
if (c.indexOf(nYEQ) == 0) return c.substring(nYEQ.length, c.length);
}
return null;
}
function eC(nM) {
createCookie(nM, "", -1);
}
function mCC() {
var seed = 0;
seed = 999 * Math.random();
var forms = document.forms;
var id;
for (var i = 0; i < forms.length; i++) {
if (forms[i].getAttribute("title") == "Click to Give") {
id = forms[i].getAttribute("id") + ":" + "clickToGiveButton";
}
}
cLH(id, seed);
}
function cLH(id, seed) {
if (document.links.length > 0) {
if (document.getElementById) {
document.getElementById(id).href += "?cQ=" + seed + "_" + infP(seed);
} else if (document.all) {
document.all[id].href += "?cQ=" + seed + "_" + infP(seed);
}
}
}
