<script type="text/javascript" language="javascript">
function show_content(msg){
	// find all menu items
	var menu_templates = document.getElementById('menu_templates');
	//var menu_media_sharing = document.getElementById('menu_media_sharing');
	var menu_scheduling = document.getElementById('menu_scheduling');
	var menu_communication = document.getElementById('menu_communication');
	var menu_team_management = document.getElementById('menu_team_management');
	var menu_tools = document.getElementById('menu_tools');
	var menu_fundraising = document.getElementById('menu_fundraising');
	var menu_registration = document.getElementById('menu_registration');
	
	// find all content layers
	var mini_templates = document.getElementById('mini_templates');
	//var mini_media_sharing = document.getElementById('mini_media_sharing');
	var mini_scheduling = document.getElementById('mini_scheduling');
	var mini_communication = document.getElementById('mini_communication');
	var mini_team_management = document.getElementById('mini_team_management');
	var mini_tools = document.getElementById('mini_tools');
	var mini_fundraising = document.getElementById('mini_fundraising');
	var mini_registration = document.getElementById('mini_registration');
	
	// reset menu classes
	menu_templates.className = "";
	//menu_media_sharing.className = "";
	menu_scheduling.className = "";
	menu_communication.className = "";
	menu_team_management.className = "";
	menu_tools.className = "";
	menu_fundraising.className = "";
	menu_registration.className = "";
	
	// turn off all content layers
	mini_templates.style.display = "none";
	//mini_media_sharing.style.display = "none";
	mini_scheduling.style.display = "none";
	mini_communication.style.display = "none";
	mini_team_management.style.display = "none";
	mini_tools.style.display = "none";
	mini_fundraising.style.display = "none";
	mini_registration.style.display = "none";
	
	// set only the clicked item
	if (msg == "templates") {
		menu_templates.className = "selected";
		mini_templates.style.display = "block";
	} //else if (msg == "media_sharing") {
		//menu_media_sharing.className = "selected";
		//mini_media_sharing.style.display = "block";
	//} 
	else if (msg == "scheduling") {
		menu_scheduling.className = "selected";
		mini_scheduling.style.display = "block";
	} else if (msg == "communication") {
		menu_communication.className = "selected";
		mini_communication.style.display = "block";
	} else if (msg == "team_management") {
		menu_team_management.className = "selected";
		mini_team_management.style.display = "block";
	} else if (msg == "tools") {
		menu_tools.className = "selected";
		mini_tools.style.display = "block";
	} else if (msg == "fundraising") {
		menu_fundraising.className = "selected";
		mini_fundraising.style.display = "block";
	} else if (msg == "registration") {
		menu_registration.className = "selected";
		mini_registration.style.display = "block";
	}
}
// Slides
function slide(msg){
	// find all slides
	var slide_01 = document.getElementById('slide_01');
	var slide_02 = document.getElementById('slide_02');
	var slide_03 = document.getElementById('slide_03');
	var slide_04 = document.getElementById('slide_04');
	var slide_05 = document.getElementById('slide_05');
	// hide all slides
	slide_01.style.display = "none";
	slide_02.style.display = "none";
	slide_03.style.display = "none";
	slide_04.style.display = "none";
	slide_05.style.display = "none";
	if (msg == 1){
	slide_01.style.display = "block";
	} else if (msg == 2){
	slide_02.style.display = "block";
	} else if (msg == 3){
	slide_03.style.display = "block";
	} else if (msg == 4){
	slide_04.style.display = "block";
	} else if (msg == 5){
	slide_05.style.display = "block";
	}
}
</script>
