<script type="text/javascript" language="javascript">
var count = 1;
var total_photos = 4;
var int=self.setInterval("switch_photos(1)",5000);
function switch_photos(msg){
	count += msg;
	if (count > total_photos){
		count = 1;
	} else if (count <= 0) {
		count = total_photos;
	}
	
	set_photo(count);
}
function set_photo(msg){
	count = msg;
	var photo = document.getElementById('template_photos');
	//photo.innerHTML = count;
	
	var dot_1 = document.getElementById('dot_1');
	var dot_2 = document.getElementById('dot_2');
	var dot_3 = document.getElementById('dot_3');
	var dot_4 = document.getElementById('dot_4');
	
	dot_1.className = "";
	dot_2.className = "";
	dot_3.className = "";
	dot_4.className = "";
	
	if (count == 1) {
		dot_1.className = "dot_selected";
		photo.className = "photo_1";
	} else if (count == 2) {
		dot_2.className = "dot_selected";
		photo.className = "photo_2";
	} else if (count == 3) {
		dot_3.className = "dot_selected";
		photo.className = "photo_3";
	} else if (count == 4) {
		dot_4.className = "dot_selected";
		photo.className = "photo_4";
	}
}
</script>
