<script language="javascript">
var loc = document.location.toString();
var index = loc.indexOf(":");
var url = loc.substring(index,loc.length);
if (index == "4") {
   secureUrl = "https" + url;
   location.replace(secureUrl); // get rid of current page in history
   location.href = secureUrl;
}

var dt = new Date(); 
dt =dt.getFullYear(); 
document.all("footer").innerHTML="&nbsp;&nbsp; &copy;"+dt+" Xerox State & Local Solutions, Inc. All Rights Reserved.";
document.all("programselect").style.display = "none";
function selState(val,obj){
	var state = document.all("state");
	var defaultText = "Select your State and Program:";
	var selText = "Select your State and Program to log in:";
	if(val==""){
		state.innerHTML = defaultText;
		document.all("programselect").style.display = "none";
		document.all("stateName").value = "";
		document.all("stateChosen").value = "";
	}else{
		var val = val;
		var arr = val.split("&");
		var stateVal = val.split("=")[1];
		var programVal = "";
		
		if(arr.length>1){
			stateVal = arr[0].split("=")[1];
			programVal = arr[1].split("=")[1];
		}
		var icon= programVal==null || programVal == "" ?"":"_"+programVal;
		icon = stateVal + "" + icon;
		state.innerHTML = selText;
		document.all("programselect").style.display = "block";
		document.all("programselectpic").src = "images/icon/"+icon+".gif";
		document.all("stateChosen").value=programVal;
		document.all("stateName").value=stateVal;
	}
}

function go(){
	var state = document.all("stateName").value;
	if(state.indexOf("okedc")>=0){
		document.all("state_menu").action="/"+state+"client/index.jsp";
	}else{
		document.all("state_menu").action="/"+state+"client/setupPortal.recip";
	}
	
	document.all("state_menu").submit();
}

function init(){
	document.all("statesselect").options[0].selected = true;
}
$(function(){
$(".statesselect").resize(function() {
	//$(".statesselect").removeClass("statesselect").addClass("statesselect");
	window.location.reload();	
})
})


