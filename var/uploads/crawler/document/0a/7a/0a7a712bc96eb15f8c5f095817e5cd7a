<script type="text/javascript" language="JavaScript">
    var isIE;
    if (navigator.appName == "Microsoft Internet Explorer")
    { isIE = true; }

    $(document).ready(function () {
        $("#ctl00_uxMainHeader_uxSearchInput").keypress(function (e) {
            var keyCode;

            if (isIE) {
                keyCode = window.event.keyCode;
            }
            else {
                keyCode = e.which;
            }

            if (keyCode == 13) {

                $("#ctl00_uxMainHeader_uxGoSearch").click();
                return false;

            } else return true;

        });
        if (navigator.appVersion.indexOf("webkit") !== -1)
        {
            //$('#FreeTextDiv').css('left:900');
        }
        if (navigator.appVersion.indexOf("mozilla") !== -1) {
            //$('#FreeTextDiv').css('left:300px');
        }

});


//    var SignInLink = document.getElementById("ctl00_uxMainHeader_uxSignInLink");
    //    var SignOutLabel = document.getElementById("ctl00_uxMainHeader_uxSignOutLabel");
    //    var SignOutLink = document.getElementById("ctl00_uxMainHeader_uxSignOutLink");
    //    SignOutLabel.style.visibility="hidden";
    //    SignOutLink.style.visibility="hidden";
    function showPopup() {
        $find('ModalBehavior').show();
    }
    function hidePopup() {
        $find('ModalBehavior').hide();
    }
    function SlideError() {
        {
            jQuery("#errordiv").slideDown("slow").delay(5000).slideUp("slow");
        }
    }


    function getButton() {
        //Capture Netscape events
        if (!isIE) { document.captureEvents(Event.KEYDOWN); }
        document.onkeydown = submitText;
    }

    function submitText(evt) {
        var theButtonPressed;

        if (isIE) {
            theButtonPressed = window.event.keyCode;
        }
        else {
            theButtonPressed = evt.which;
        }

        if (theButtonPressed == 13) {
            $("#ctl00_uxMainHeader_uxGoSearch").click();
        }
    }
    function getImgSize(imgSrc) {
        // gets the image size as it is loaded.
        var newImg = new Image();
        newImg.src = imgSrc;
        var height = newImg.height;
        var width = newImg.width;
        p = $(newImg).ready(function () {
            return { width: newImg.width, height: newImg.height };
        });
        return p[0]['width'] + "|" + p[0]['height'];
    }

    function resizeImage(MyImage) {
        // Call getImgSize to get the true image size as the image is loaded
        // If we wait until after it has been loaded, javascript may return
        // skewed results based on the images container or CSS properties
        var MySize = getImgSize(MyImage.src);
        // Parse returned string
        var pipeLocation = MySize.indexOf("|");
        var image_width = parseInt(MySize.substr(0, pipeLocation));
        var image_height = parseInt(MySize.substr(pipeLocation + 1));
        MyImage.style.width = image_width + 'px';
        MyImage.style.height = image_height + 'px';
        MyImage.width = image_width + 'px';
        MyImage.height = image_height + 'px';
        //alert('width=' + image_width + ' height=' + image_height);
        var window_height = 115;
        var window_width = 65;
        var height_ratio = image_height / window_height;
        var width_ratio = image_width / window_width;
        if ((image_width > window_width) && (image_height > window_height)) {
            // If both dimensions are out of range, depending on which is worse, set the appropriate 
            // height or width attribute and set the opposite one to "auto" to preserve aspect ratio
            if (height_ratio > width_ratio) {
                MyImage.style.width = "auto";
                MyImage.style.height = window_height + 'px';
                MyImage.width = "auto";
                MyImage.height = window_height + 'px';
            }
            else {
                MyImage.style.width = window_width + 'px';
                MyImage.style.height = "auto";
                MyImage.width = window_width + 'px';
                MyImage.height = "auto";
            }
        }
        else {
            if ((image_width > window_width) || (image_height > window_height)) {
                // If only one dimension is out of range, depending on which is incorrect, set the appropriate 
                // height or width attribute to 100% and set the opposite one to "auto" to preserve aspect ratio
                if (height_ratio > width_ratio) {
                    MyImage.style.width = "auto";
                    MyImage.style.height = "100%";
                    MyImage.width = "auto";
                    MyImage.height = "100%";
                }
                else {
                    MyImage.style.width = "100%";
                    MyImage.style.height = "auto";
                    MyImage.width = "100%";
                    MyImage.height = "auto";
                }
            }
            else {
                //no resize done
            }

        }
    }

    function AnimateCart(btn) {

        var $u = jQuery('#divHeader');
        var pleft = $u.offset().left + 747;
        $('#shoppingcartList').css("left", pleft);
        $('#shoppingcartList').css("z-index", 103);
        //$('#shoppingcart-animate').css({position:'absolute', left:pleft, top:126});

        //        $('#shoppingcart-animate').css("left", left);
        //        $('#shoppingcart-animate').css("top", 126);
        // Stop basket from retreating to a hidden state
        $('#shoppingcart-animate').stop(true, false);
        // Grab the DOM ID from the button passed to the function for later use
        var myid = btn.id;
        var Page_Control_ID_Base = myid;
        // Get a control reference to the basket text field located in the main header 
        var uxBasketText = document.getElementById("ctl00_uxMainHeader_uxMainMenu_uxBasketText");
        // The following variables are used in determining the location and value of the product ID
        // Here we declare and initialize them for later use
        var ProductID_Index = 0;
        var ProductID_End_Index = 0;
        var ProductID_Value = 0;
        // Below we declare and initialize the variables used in searching for various information returned from 
        // the add to basket routine such as error messages or returned quantities and dollar amounts
        var Separator_Location = 0;
        var Message_Location = 0;
        // Determine if we can find the part of the button ID relating to all controls on the page
        // We will call the the Page_Control_ID_Base
        var Page_Control_Index = myid.indexOf("uxProductList_ctl");
        var CurrentURL = document.location.href.toLowerCase();
        var AddToBasketLocation = "";
        var ViewBasketLocation = "";

        if (CurrentURL.indexOf("/frontend/") > 0) {
            AddToBasketLocation = "/frontend";
            ViewBasketLocation = "/frontend";
        }
      
        AddToBasketLocation = AddToBasketLocation + "/shop/AddToBasket.aspx";
        ViewBasketLocation = AddToBasketLocation + "/shop/ViewBasket.aspx";
        // If we were able to find the control base, extract it and put it in a variable so that 
        // we have a starting place to look for the rest of the variables
        if (Page_Control_Index > 0) {
            Page_Control_ID_Base = Page_Control_ID_Base.substring(0, Page_Control_Index);
        }
        // Get references to Cart items and hidden items
        // Get reference to the part of the control that relates specifically to the LIST of items 
        // within the product list page
        var Datalist_Control_ID_Base = myid.replace("uxAddToCartBtn", "");
        // Derive references to all pertinent controls on the current page NOT in the item list
        var uxCartItemPurchaseCount = document.getElementById(Page_Control_ID_Base + "uxCartItemPurchaseCount");
        var uxCartItemDesc = document.getElementById(Page_Control_ID_Base + "uxCartItemDesc");
        var uxCartItemPrice = document.getElementById(Page_Control_ID_Base + "uxCartItemPrice");
        var uxAdditionalInfo = document.getElementById(Page_Control_ID_Base + "uxAdditionalInfo");

        var uxCartItemRegularPrice = document.getElementById(Page_Control_ID_Base + "uxCartItemRegularPrice");
        var uxCartNumberItems = document.getElementById(Page_Control_ID_Base + "uxCartNumberItems");
        var uxCartTotalValue = document.getElementById(Page_Control_ID_Base + "uxCartTotalValue");
        var uxIconForCart = document.getElementById(Page_Control_ID_Base + "uxIconForCart");

        // Derive references to all pertinent controls on the current page IN the item list
        var uxCartItemPurchaseCountInterface = document.getElementById(Datalist_Control_ID_Base + "uxCartItemPurchaseCountInterface");
        var uxCartItemDescInterface = document.getElementById(Datalist_Control_ID_Base + "uxCartItemDescInterface");
        var uxCartItemRegularPriceInterface = document.getElementById(Datalist_Control_ID_Base + "uxCartItemRegularPriceInterface");
        var uxCartItemPriceInterface = document.getElementById(Datalist_Control_ID_Base + "uxCartItemPriceInterface");
        var uxItemAddedLabel = document.getElementById(Datalist_Control_ID_Base + "uxItemAddedLabel");
        var uxCartItemProductID = document.getElementById(Datalist_Control_ID_Base + "uxCartItemProductID");
        // Once we have all the references, we can assign values as needed
        // Assign values to cart fields using the hidden field interface values
        uxCartItemPurchaseCount.innerHTML = uxCartItemPurchaseCountInterface.value;
        uxCartItemDesc.innerHTML = uxCartItemDescInterface.value;
        uxCartItemPrice.innerHTML = " " + uxCartItemPriceInterface.value;
        uxCartItemRegularPrice.innerHTML = uxCartItemRegularPriceInterface.value;

        // Handle ClubBev prices in a special manner if they differ from original price
        if (uxCartItemPriceInterface.value == uxCartItemRegularPriceInterface.value) {
            // Price is the same so do not show ClubBev price
            uxCartItemRegularPrice.style.textDecoration = 'none';
            uxCartItemPrice.style.visibility = "hidden";
            uxAdditionalInfo.style.visibility = "hidden";
        }
        else {
            // Price is different so show ClubBev price and show original with a line through it
            uxCartItemPrice.style.visibility = "visible";
            uxAdditionalInfo.style.visibility = "visible";
            uxAdditionalInfo.innerHTML = "<br /> ClubBev: ";
            uxCartItemRegularPrice.style.textDecoration = 'line-through';
            uxCartItemPrice.style.color = "red";
        }

        // Get image link and product details link so that we can modify them
        var IconImage = document.getElementById(Datalist_Control_ID_Base + "uxProductIconImage");
        var ProductLink = document.getElementById(Datalist_Control_ID_Base + "uxProductLink");
        ProductID_Index = ProductLink.href.indexOf("ProductID=");
        // Get product id from product link
        if (uxCartItemProductID.value.length > 0) {
            ProductID_Value = uxCartItemProductID.value;
        }
        else
        {
            //We don't have a ProductID so we can't add it to the basket.
            return;
        }

        // Assign cart icon image and resize to fit the cart
        uxIconForCart.src = IconImage.src;
        resizeImage(uxIconForCart);
        // Prepare to do cart animation using jQuery
        $(function () {
            $(document).ready(function () {
                if (CurrentURL.indexOf("/shop/") > 0) {
                    if (CurrentURL.indexOf("/frontend/") > 0) {
                        ViewBasketLocation = "/frontend/shop/ViewBasket.aspx";
                    } else {
                        ViewBasketLocation = "/shop/ViewBasket.aspx";
                    }
                }
                // Request to add product to cart
                $.get(AddToBasketLocation + "?ProductID=" + ProductID_Value + "&ProductQTY=1", function (response) {
                    // Declare and initialize return variables
                    var TotalAmount = "0.00";
                    var ItemCount = "0";
                    var ErrorMessage = "";
                    // Check to see if more than one piece of information was returned (separator exists)
                    Separator_Location = response.indexOf(":");
                    // Check to see if a returned error message exists (square bracket exists)
                    Message_Location = response.indexOf("[");
                    $('.loadingspinner').hide();
                    if (Message_Location >= 0) {
                        // Error message detected, item not added
                        ErrorMessage = response.replace("[", "").replace("]", "");
                        document.getElementById('ctl00_uxMainHeader_uxErrorLabel').innerHTML = ErrorMessage;
                        SlideError();
                        uxItemAddedLabel.style.visibility = "hidden";
                        // If you want to do something with the error message, do so here...
                    }
                    else {
                        // Values returned if a separator detected
                        if (Separator_Location > 0) {
                            // Assign values to display on the basket
                            TotalAmount = response.substring(0, Separator_Location);
                            ItemCount = response.substring(Separator_Location + 1);
                        }

                        // set cart return values from addtobasket routine
                        uxCartNumberItems.innerHTML = ItemCount + "&nbsp;";
                        uxCartTotalValue.innerHTML = TotalAmount;
                        uxBasketText.innerHTML = "You have " + ItemCount + " item(s) in your ";

                        //recalculate position when the window resizes
                        $(window).resize(function (event) {
                            //                            $('#shoppingcart-animate').stop(true, false);
                            //                            viewPntLeft = pleft;
                            //                            var $u = jQuery('#divHeader');
                            //                            var pos = $u.offset();
                            //                            var pleft = $u.offset().left + 747;
                            //                            $('#shoppingcartList').css("left",pleft);
                            //if (event.target.id.indexOf('uxAddToCartBtn') == -1) {
                            //$(window).unbind('scroll');	   
                            $('#shoppingcart-animate').hide().stop();
                            $('#shoppingcartList').css("z-index", -1);
                            $('#shoppingcart-animate').stop().animate({ 'marginTop': '-210px' }, 'fast', function () {
                                $('#shoppingcart-animate').hide().stop();
                                $('#shoppingcartList').css("z-index", -1);
                            });
                            //}
                        });
                        // Animate and auto scroll cart using jQuery
                        $(window).scroll(function (event) {

                            $('#shoppingcart-animate').stop(true, false);

                            viewPntTop = $(window).scrollTop();

                            if (viewPntTop > 126) {
                                $('#shoppingcartList').animate({ 'top': viewPntTop + 'px' }, { duration: 500, queue: false });
                                $("#shoppingcart-animate").stop(true, false).animate({ 'marginTop': '0px' }, 1800);
                            } else if (viewPntTop == 0) {
                                $('#shoppingcartList').animate({ 'top': '126px' }, { duration: 300 });
                                $("#shoppingcart-animate").stop(true, false).animate({ 'marginTop': '-23px' }, 1800);
                            } else {
                                $('#shoppingcartList').animate({ 'top': '126px' }, { duration: 300 });
                                $('#shoppingcart-animate').stop(true, false).animate({ 'marginTop': '-23px' }, 1800);
                            }
                        });

                        viewPointTop = $(window).scrollTop();

                        if (viewPointTop > 126) {
                            $('#shoppingcartList').animate({ 'top': viewPointTop + 'px' }, { duration: 500 });
                            $("#shoppingcart-animate").show().stop(true, false).animate({ 'marginTop': '0px' }, 1800);
                        } else {
                            $('#shoppingcartList').animate({ 'top': '126px' }, { duration: 300 });
                            $('#shoppingcart-animate').show().stop(true, false).animate({ 'marginTop': '-23px' }, 1800);
                        }
                        // An area out of the basket is clicked the animate the basket off the screen                      	                    
                        $(document).bind("click", function (event) {
                            if (event.target.id.indexOf('uxAddToCartBtn') == -1) {
                                $(window).unbind('scroll');
                                $('#shoppingcart-animate').stop().animate({ 'marginTop': '-210px' }, 'slow', function () {
                                    $('#shoppingcart-animate').hide().stop();
                                    $('#shoppingcartList').css("z-index", -1);
                                });
                            }
                        });
                    }
                });
            });
        });
        // If the basket links on the bottom of the persistent basket are clicked, hide the basket and redirect
        $(function () {
            $('#gotobasket').click(
              function () {
                  $('#shoppingcart-animate').hide();
                  window.location.href = ViewBasketLocation;
              }
            );
        })
    }


    //Like AnimateCartDetail but parameterized in a more sensible way.
    /*
    productID: Id of the product to add to cart
    quantity: amount of the product to add to cart
    productThumbnail: the URL of the icon that should be used for the product thumbnail
    price: price to display for the item in the add to cart success dropdown
    clubBevPrice: reduced price to display for the item in the add to cart success dropdown (only displayed if different from price)
    productName: Name of the product to display in the add to cart success dropdown
    */
    function AnimateCartDetailGiftList(productID,quantity,productThumbnailURL,price,clubBevPrice,productName) {
        var $u = jQuery('#divHeader');
        var pos = $u.offset();
        var pleft = $u.offset().left + 747;
        $('#shoppingcart').css("left", pleft);
        $('#shoppingcart').css("z-index", 103);
        // Stop basket from retreating to a hidden state
        $(function () {
            $('#shoppingcart-animate').stop(true, false);
        });

        // Get a control reference to the basket text field located in the main header 
        var CurrentURL = document.location.href.toLowerCase();
        var AddToBasketLocation = "";
        var ViewBasketLocation = "";

        if (CurrentURL.indexOf("/frontend/") > 0) {
            AddToBasketLocation = "/Frontend";
            ViewBasketLocation = "/Frontend";
        }

        ViewBasketLocation = ViewBasketLocation + "/Shop/ViewBasket.aspx";
        AddToBasketLocation = AddToBasketLocation + "/Shop/AddToBasket.aspx";
        

        var uxCartItemPurchaseCount = document.getElementById("uxCartItemPurchaseCount");
        var uxCartItemDesc = document.getElementById("uxCartItemDesc");
        var uxCartItemPrice = document.getElementById("uxCartItemPrice");
        var uxAdditionalInfo = document.getElementById("uxAdditionalInfo");
        var uxCartItemRegularPrice = document.getElementById("uxCartItemRegularPrice");
        var uxCartNumberItems = document.getElementById("uxCartNumberItems");
        var uxCartTotalValue = document.getElementById("uxCartTotalValue");
        var uxIconForCart = document.getElementById("uxIconForCart");
        var uxBasketText = document.getElementById("ctl00_uxMainHeader_uxMainMenu_uxBasketText");

        // Assign values to cart fields using the hidden field interface values
        uxCartItemPurchaseCount.innerHTML = quantity;
        uxCartItemDesc.innerHTML = "&nbsp;" + productName;
        uxCartItemPrice.innerHTML = "&nbsp;" + clubBevPrice;
        uxCartItemRegularPrice.innerHTML = price;
        // Handle ClubBev prices if they differ from original
        if (clubBevPrice == price) {
            // Price is the same so do not show ClubBev price
            uxCartItemPrice.style.visibility = "hidden";
            uxAdditionalInfo.style.visibility = "hidden";
        }
        else {
            if (uxCartItemRegularPrice.innerHTML != 'undefined') {
                // Price is different so show ClubBev price and show original with a line through it
                uxCartItemRegularPrice.style.textDecoration = 'line-through';
                uxAdditionalInfo.innerHTML = "<br /> ClubBev: ";
                uxCartItemPrice.style.color = "red";
            }
            else {
                uxCartItemRegularPrice.innerHTML = uxCartItemPrice.innerHTML;
                uxAdditionalInfo.style.visibility = "hidden";
                uxCartItemPrice.style.visibility = "hidden";
            }
        }

        // Get image link and product ID link
        // Assign cart icon image and resize to fit the cart
        uxIconForCart.src = productThumbnailURL;
        resizeImage(uxIconForCart);
        // Prepare to do cart animation using jQuery
        //debugger;
        $(function () {
            $(document).ready(function () {
                if (CurrentURL.indexOf("/frontend/") > 0) {
                    if (CurrentURL.indexOf("/shop/") > 0) {
                        ViewBasketLocation = "/frontend/shop/ViewBasket.aspx";
                    }
                } else {
                    if (CurrentURL.indexOf("/shop/") > 0) {
                        ViewBasketLocation = "/shop/ViewBasket.aspx";
                        AddToBasketLocation = "/shop/AddToBasket.aspx";
                    } 
                }
                // Request to add product to cart
                $.get(AddToBasketLocation + "?ProductID=" + productID + "&ProductQTY=" + quantity, function (response) {
                    // Declare and initialize return variables
                    var TotalAmount = "0.00";
                    var ItemCount = "0";
                    // Check to see if more than one piece of information was returned (separator exists)
                    Separator_Location = response.indexOf(":");
                    // Check to see if a returned error message exists (square bracket exists)
                    Message_Location = response.indexOf("[");
                    if (Message_Location >= 0) {
                        // Error message detected, item not added
                        ErrorMessage = response.replace("[", "").replace("]", "");
                        document.getElementById('ctl00_uxMainHeader_uxErrorLabel').innerHTML = ErrorMessage
                        SlideError();
                        uxItemAddedLabel.style.visibility = "hidden";
                        // If you want to do something with the error message, do so here...
                    }
                    else {
                        // Values returned if a separator detected
                        if (Separator_Location > 0) {
                            // Assign values to display on the basket
                            TotalAmount = response.substring(0, Separator_Location);
                            ItemCount = response.substring(Separator_Location + 1);
                        }

                        // set cart return values from addtobasket routine
                        uxCartNumberItems.innerHTML = ItemCount + "&nbsp;";
                        uxCartTotalValue.innerHTML = TotalAmount;
                        uxBasketText.innerHTML = "You have " + ItemCount + " item(s) in your ";

                        //recalculate position when the window resizes
                        $(window).resize(function () {
                            $('#shoppingcart-animate').stop(true, false);
                            $('#shoppingcart-animate').hide().stop();
                            $('#shoppingcart-animate').stop().animate({ 'marginTop': '-210px' }, 'fast', function () {
                                $('#shoppingcart-animate').hide().stop();
                                $('#shoppingcart').css("z-index", -1);
                            });
                            $('#shoppingcart').css("z-index", -1);
                        });

                        // Animate the cart and set bindings on click for basket and outside of basket
                        $('#shoppingcart-animate').click(function () {
                            //event.stopPropagation();
                        });
                        $('#shoppingcart-animate').show().stop(true, false).animate({ 'marginTop': '-22px' }, 1800);
                        //                        $('#shoppingcart-animate').stop().animate({'marginTop':'-210px'}, 'slow', function() { 
                        //			                        $('#shoppingcart-animate').hide().stop();  
                        //			                        $('#shoppingcart').css("z-index", -1);
                        //			                        }); 
                        //			                }
                        //$('#shoppingcart').delay(1000).css("z-index", -1);
                        $(document).bind("click", function () {
                            $('#shoppingcart-animate').delay(500).animate({ 'marginTop': '-210px' }, 'slow', function () {
                                $('#shoppingcart-animate').hide().stop();
                                $('#shoppingcart').css("z-index", -1);
                            });
                            //}//);

                        });

                    }
                });
            });
        });
        // If the basket links on the bottom of the persistent basket are clicked, hide the basket and redirect
        $(function () {
            $('#gotobasket').click(
              function () {
                  $('#shoppingcart-animate').hide();
                  window.location.href = ViewBasketLocation;
              }
            );
        })

    }

    function AnimateCartDetail(btn) {
        
        var $u = jQuery('#divHeader');
        var pos = $u.offset();
        var pleft = $u.offset().left + 747;
        $('#shoppingcart').css("left", pleft);
        $('#shoppingcart').css("z-index", 103);
        // Stop basket from retreating to a hidden state
        $(function () {
            $('#shoppingcart-animate').stop(true, false);
        });

        // Grab the DOM ID from the button passed to the function
        var myid = btn.id;
        var Page_Control_ID_Base = myid;
        // Get a control reference to the basket text field located in the main header 
        var uxBasketText = document.getElementById("ctl00_uxMainHeader_uxMainMenu_uxBasketText");
        // The following variables are used in determining the location and value of the product ID
        // Here we declare and initialize them for later use
        var ProductID_Index = 0;
        var ProductID_End_Index = 0;
        var ProductID_Value = 0;
        // Below we declare and initialize the variables used in searching for various information returned from 
        // the add to basket routine 
        var Separator_Location = 0;
        var CurrentURL = document.location.href.toLowerCase();
        var AddToBasketLocation = "";
        var ViewBasketLocation = "/Shop/ViewBasket.aspx";

        if (CurrentURL.indexOf("/frontend/") > 0) {
            AddToBasketLocation = "/frontend";
        }

        AddToBasketLocation = AddToBasketLocation + "/shop/AddToBasket.aspx";

        // extract the control base and put it in a variable
        var Page_Control_ID_Base = myid.replace("uxAddToCartButton", "");
        if (Page_Control_ID_Base == myid) {
            Page_Control_ID_Base = myid.replace("uxAddCaseButton", "");
        }

        // Derive references to all pertinent controls on the current page
        var uxCartItemPurchaseCount = document.getElementById("uxCartItemPurchaseCount");
        var uxCartItemDesc = document.getElementById(Page_Control_ID_Base + "uxCartItemDesc");
        var uxCartItemPrice = document.getElementById(Page_Control_ID_Base + "uxCartItemPrice");
        var uxAdditionalInfo = document.getElementById(Page_Control_ID_Base + "uxAdditionalInfo");
        var uxCartItemRegularPrice = document.getElementById(Page_Control_ID_Base + "uxCartItemRegularPrice");
        var uxCartNumberItems = document.getElementById(Page_Control_ID_Base + "uxCartNumberItems");
        var uxCartTotalValue = document.getElementById(Page_Control_ID_Base + "uxCartTotalValue");
        var uxIconForCart = document.getElementById(Page_Control_ID_Base + "uxIconForCart");

        var uxCartItemPurchaseCountInterface = document.getElementById(Page_Control_ID_Base + "UxQtyToAdd");
        var uxCartItemDescInterface = document.getElementById(Page_Control_ID_Base + "uxItemDesc");
        var uxCartItemRegularPriceInterface = document.getElementById(Page_Control_ID_Base + "uxRegularPriceInterface");
        var uxCartItemPriceInterface = document.getElementById(Page_Control_ID_Base + "uxItemSalesPrice");

        // Assign values to cart fields using the hidden field interface values
        try {
            if (uxCartItemPurchaseCountInterface.value != null) {
                uxCartItemPurchaseCount.innerHTML = uxCartItemPurchaseCountInterface.value;
            }
        } catch (e) {
            //do nothing, just swallow the error.
        }
        try {
            if (uxCartItemDescInterface.value != null) {
                uxCartItemDesc.innerHTML = "&nbsp;" + uxCartItemDescInterface.value;
            }
        } catch (e) {
            //do nothing, just swallow the error.
        }
        try {
            if (uxCartItemPriceInterface.value) {
                uxCartItemPrice.innerHTML = "&nbsp;" + uxCartItemPriceInterface.value;
            }
        } catch (e) {
            //do nothing, just swallow the error.
        }
        try {
            if (uxCartItemRegularPriceInterface.value) {
                uxCartItemRegularPrice.innerHTML = uxCartItemRegularPriceInterface.value;
            }
        } catch (e) {
            //do nothing, just swallow the error.
        }
        // Handle ClubBev prices if they differ from original
        if (uxCartItemPriceInterface.value == uxCartItemRegularPriceInterface.value) {
            // Price is the same so do not show ClubBev price
            uxCartItemPrice.style.visibility = "hidden";
            uxAdditionalInfo.style.visibility = "hidden";
        }
        else {
            if (uxCartItemRegularPrice.innerHTML != 'undefined') {
                // Price is different so show ClubBev price and show original with a line through it
                uxCartItemRegularPrice.style.textDecoration = 'line-through';
                uxAdditionalInfo.innerHTML = "<br /> ClubBev: ";
                uxCartItemPrice.style.color = "red";
            }
            else {
                uxCartItemRegularPrice.innerHTML = uxCartItemPrice.innerHTML;
                uxAdditionalInfo.style.visibility = "hidden";
                uxCartItemPrice.style.visibility = "hidden";
            }
        }

        // Get image link and product ID link
        var IconImage = document.getElementById(Page_Control_ID_Base + "uxThumbNailImage");
        var ProductIDHiddenField = document.getElementById(Page_Control_ID_Base + "uxProductID");
        ProductID_Value = ProductIDHiddenField.value;
        // Assign cart icon image and resize to fit the cart
        uxIconForCart.src = IconImage.value;
        resizeImage(uxIconForCart);
        // Prepare to do cart animation using jQuery
        //debugger;
        $(function () {
            $(document).ready(function () {
                if (CurrentURL.indexOf("/frontend/") > 0) {
                    if (CurrentURL.indexOf("/shop/") > 0) {
                        ViewBasketLocation = "/frontend/shop/ViewBasket.aspx";
                    }
                }else {
                    if (CurrentURL.indexOf("/shop/") > 0) {
                        ViewBasketLocation = "/shop/ViewBasket.aspx";
                        AddToBasketLocation = "/shop/AddToBasket.aspx";
                    }                
                }

                // Request to add product to cart
                $.get(AddToBasketLocation + "?ProductID=" + ProductID_Value + "&ProductQTY=" + uxCartItemPurchaseCountInterface.value, function (response) {
                    // Declare and initialize return variables
                    var TotalAmount = "0.00";
                    var ItemCount = "0";
                    // Check to see if more than one piece of information was returned (separator exists)
                    Separator_Location = response.indexOf(":");
                    // Check to see if a returned error message exists (square bracket exists)
                    Message_Location = response.indexOf("[");
                    $('.loadingspinner').hide();
                    if (Message_Location >= 0) {
                        // Error message detected, item not added
                        ErrorMessage = response.replace("[", "").replace("]", "");
                        document.getElementById('ctl00_uxMainHeader_uxErrorLabel').innerHTML = ErrorMessage
                        SlideError();
                        uxItemAddedLabel.style.visibility = "hidden";
                        // If you want to do something with the error message, do so here...
                    }
                    else {
                        // Values returned if a separator detected
                        if (Separator_Location > 0) {
                            // Assign values to display on the basket
                            TotalAmount = response.substring(0, Separator_Location);
                            ItemCount = response.substring(Separator_Location + 1);
                        }

                        // set cart return values from addtobasket routine
                        uxCartNumberItems.innerHTML = ItemCount + "&nbsp;";
                        uxCartTotalValue.innerHTML = TotalAmount;
                        uxBasketText.innerHTML = "You have " + ItemCount + " item(s) in your ";

                        //recalculate position when the window resizes
                        $(window).resize(function () {
                            $('#shoppingcart-animate').stop(true, false);
                            $('#shoppingcart-animate').hide().stop();
                            $('#shoppingcart-animate').stop().animate({ 'marginTop': '-210px' }, 'fast', function () {
                                $('#shoppingcart-animate').hide().stop();
                                $('#shoppingcart').css("z-index", -1);
                            });
                            $('#shoppingcart').css("z-index", -1);
                        });

                        // Animate the cart and set bindings on click for basket and outside of basket
                        $('#shoppingcart-animate').click(function () {
                            //event.stopPropagation();
                        });
                        $('#shoppingcart-animate').show().stop(true, false).animate({ 'marginTop': '-22px' }, 1800);
                        //                        $('#shoppingcart-animate').stop().animate({'marginTop':'-210px'}, 'slow', function() { 
                        //			                        $('#shoppingcart-animate').hide().stop();  
                        //			                        $('#shoppingcart').css("z-index", -1);
                        //			                        }); 
                        //			                }
                        //$('#shoppingcart').delay(1000).css("z-index", -1);
                        $(document).bind("click", function () {
                            $('#shoppingcart-animate').delay(500).animate({ 'marginTop': '-210px' }, 'slow', function () {
                                $('#shoppingcart-animate').hide().stop();
                                $('#shoppingcart').css("z-index", -1);
                            });
                            //}//);

                        });

                    }
                });
            });
        });
        // If the basket links on the bottom of the persistent basket are clicked, hide the basket and redirect
        $(function () {
            $('#gotobasket').click(
              function () {
                  $('#shoppingcart-animate').hide();
                  window.location.href = ViewBasketLocation;
              }
            );
        })

    }
    function unloadFunction() {
        //var modalPopupBehaviorCtrl = document.getElementById("");
        // modalPopupBehaviorCtrl.show();
    }
    function getHiddenSignOutLink() {
        return document.getElementById("ctl00_uxMainHeader_uxHiddenSignOut");
    }
    function getSignInLabel() {
        return document.getElementById("ctl00_uxMainHeader_uxSignInLabel");
    }
    function getSignInLink() {
        return document.getElementById("ctl00_uxMainHeader_uxSignInLink");
    }
    function getSignOutLabel() {
        return document.getElementById("ctl00_uxMainHeader_uxSignOutLabel");
    }
    function getSignOutLink() {
        return document.getElementById("ctl00_uxMainHeader_uxSignOutLink");
    }

    function AddCase(btn) {
        var myid = btn.id;
        var Page_Control_ID_Base = myid.replace("uxAddCaseButton", "");
        var uxCartItemPurchaseCount = document.getElementById(Page_Control_ID_Base + "UxQtyToAdd");
        uxCartItemPurchaseCount.value = '12';
    }
    function RemoveCase(btn) {
        var myid = btn.id;
        var Page_Control_ID_Base = myid.replace("uxAddCaseButton", "");
        var uxCartItemPurchaseCount = document.getElementById(Page_Control_ID_Base + "UxQtyToAdd");
        uxCartItemPurchaseCount.value = '1';
    }
</script>



    

