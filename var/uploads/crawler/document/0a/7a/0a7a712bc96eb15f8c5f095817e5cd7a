                       <img id="fulfillmentloading" src="/media/images/ui/spinner_red.gif" alt="loading..." style="float:right;display:none;" />
                        <input type="image" name="ctl00$uxMainHeader$uxFulFillmentSelect$uxSelectFulfillmentButton" id="ctl00_uxMainHeader_uxFulFillmentSelect_uxSelectFulfillmentButton" src="Media/Images/UI/selectbutton.png" onclick="$(&#39;#fulfillmentloading&#39;).show();UpdateFulfillment();&#32;return&#32;false;" border="0" />
