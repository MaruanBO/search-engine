        <div id="fulfillmentMask"></div>    
        <input type="hidden" name="ctl00$uxMainHeader$uxFulFillmentSelect$uxFulfillmentdesc" id="ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentdesc" value="Please&#32;Select" />
        <input type="hidden" name="ctl00$uxMainHeader$uxFulFillmentSelect$uxFulfillmentRequired" id="ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentRequired" />   





<script type="text/javascript">
$(document).ready(function() {	
    //recalculate position when the window resizes
    	$(window).resize(function(){
        if( $('div#fulfillment-form').is(':hidden') ){

        }
        else
        {
            $('#fulfillment-form').hide();
            $('#fulfillmentMask, .window').hide();
            var fullfilmentMethodID = 0;
            var fullfilmentLocationID = 0;
            var error = 'There was an error when trying to save your selection. Please try again.';

            var webservicePath ='/Misc/FulfillmentSelectDropDown.aspx'; //'http://www.bevmo.com/Misc/FulfillmentSelectDropDown.aspx';
            
            if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
                fullfilmentMethodID = 1;            
                fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentPickupLocation")[0].value;
                error = 'Oops! You need to pick a store before proceeding.';     
            } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
                fullfilmentMethodID = 2;            
                fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation")[0].value;
                error = 'Please pick a state';
            } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
                fullfilmentMethodID = 3;            
                fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentDeliveryLocation")[0].value;
                error = 'Please choose a delivery store';
            }

            if ((fullfilmentMethodID < 1) && (fullfilmentLocationID < 1)){
                fullfilmentMethodID = 2; //Ship 
                fullfilmentLocationID = 8; //CA

                $.ajax({
                    type: "GET",
                    url: webservicePath,
                    data: 'fullfilmentMethod=' + fullfilmentMethodID + '&fullfilmentLocation=' + fullfilmentLocationID,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: OnFulfillmentSuccessDefault,
                    complete: OnFulfillmentSuccessDefault,
                    failure: function(response) {
                        $('#fulfillment-form').slideToggle();
                        $('#fulfillmentMask, .window').hide();
                    }
                });
            }
        }            
    });

    if (navigator.appVersion.indexOf("webkit") !== -1)
	{
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').width('22px');
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').width('22px');
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').width('22px');
	    $('#radioPickupDiv').css('padding','0px 0px 0px 2px');
	    $('#radioShipDiv').css('padding','0px 0px 0px 2px');
	    $('#radioDeliverDiv').css('padding','0px 0px 0px 2px');
	}
	
    if (navigator.appVersion.indexOf("mozilla") !== -1)
	{
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').width('22px');
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').width('22px');
	    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').width('22px');
	    $('#radioPickupDiv').css('padding','0px 0px 5px 2px');
	    $('#radioShipDiv').css('padding','0px 0px 5px 2px');
	    $('#radioDeliverDiv').css('padding','0px 0px 5px 2px');
	    $('#findastores').css('padding','1px 0px 0px 3px');
	    
	}
    var isiPhone = navigator.userAgent.toLowerCase().indexOf("iphone");
    var currentPage = 'http://www.bevmo.com/default.aspx';

    //Setup the change event for the Pickup radio button.
	$('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').change(function() {
		ShowPickupFulfillment();
	});

	//Setup the change event for the Shipping radio button.
	$('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').change(function() {
		ShowShippingFulfillment();
	});

	//Setup the change event for the Delivery radio button.
	$('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').change(function() {
		ShowDeliveryFulfillment();
	});

    //Add the current Fulfillment information to the Filfillment dropdown.
    $('#dropdownFulfillment').text($("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentdesc")[0].value);

	//Setup the click event for the fulfillment dropdown div.
	$('#dropdownFulfillment').click(function () {		
	    if (isiPhone  > -1 ){
            window.location=CleanFulfillmentQueryString('/Mobile/FullfilmentSelect.aspx' +'?BackURL='+ currentPage);
        }
        else
        {    
            openFulfillmentDropdown();
        }
	});		
   
	//Setup the click event for the fulfillment dropdown Image Button.
	$('#dropdownFulfillmentButton').click(function () {		        
	    if (isiPhone  > -1 ){
            window.location=CleanFulfillmentQueryString('/Mobile/FullfilmentSelect.aspx'  +'?BackURL='+  currentPage);
        }
        else
        {    
            openFulfillmentDropdown();
        }
   	});
	
    //Setup the click event for the fulfillment dropdown mask (the greyed out area around the dropdown).
	$('#fulfillmentMask').click(function () {		        
        var fullfilmentMethodID = 0;
        var fullfilmentLocationID = 0;
        if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
            fullfilmentMethodID = 1;            
            fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentPickupLocation")[0].value;
        } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
            fullfilmentMethodID = 2;            
            fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation")[0].value;
        } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
            fullfilmentMethodID = 3;            
            fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentDeliveryLocation")[0].value;
        }

        if ((fullfilmentMethodID > 0) && (fullfilmentLocationID > 0)){
            openFulfillmentDropdown();
        } else {
            if (fullfilmentMethodID > 0) {
                closeFulfillmentDropdown();
            } else {
                var fulfillmenterror = 'You forgot to pick a fulfillment method. Were not mad, but please pick one.';
                $('#fulfillmenterrors').text(fulfillmenterror);  
                $('#fulfillmenterrors').show();              
            }
        }
	});
	
	var sIsFulfillmentRequired = $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentRequired')[0].value;	
	if (sIsFulfillmentRequired == "True"){
        openFulfillmentDropdown();
	}	
	
});

function CleanFulfillmentQueryString(pageURL)
{
    if (pageURL.search(/%26Fulfillment=[1-2]/i) > -1)  //The url has fulfillment query string needs to be removed before the page reloads
    {
        pageURL = reloadPage.replace(/%26Fulfillment=[1-2]%7C[0-9]+/i,"");
    }
    else if (pageURL.search(/&Fulfillment=[1-2]/i) > -1)
    {
        pageURL = pageURL.replace(/&Fulfillment=[1-2]%7C[0-9]+/i,"");
    }
    else if (pageURL.search(/\?Fulfillment=[1-2]/i) > -1) //The url has fulfillment query string needs to be removed before the page reloads
    {
        pageURL = pageURL.replace(/\?Fulfillment=[1-2]%7C[0-9]+/i,"");
    }
    return pageURL;
}

function uxOKStopButton_ClientClick(sender, e) 
{ 
   __doPostBack(sender,e);
}

//Update the fulfillment information collected from the form.
function UpdateFulfillment() {

    var fullfilmentMethodID = 0;
    var fullfilmentLocationID = 0;
    var fulfillmenterror = 'There was an error when trying to save your selection. Please try again.';
    var webservicePath ='/Misc/FulfillmentSelectDropDown.aspx'; //'http://www.bevmo.com/Misc/FulfillmentSelectDropDown.aspx';
       
    //See which radio button was sellected and update the appropriate variables.
    if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
        fullfilmentMethodID = 1;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentPickupLocation")[0].value;
        fulfillmenterror = 'Oops! You need to pick a store before proceeding. ';     
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
        fullfilmentMethodID = 2;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation")[0].value;
        fulfillmenterror = 'Please pick a state';
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
        fullfilmentMethodID = 3;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentDeliveryLocation")[0].value;
        fulfillmenterror = 'Please choose a delivery store';
    }
    
    //Check to make sure the Fulfillment Method was updated.
    if (fullfilmentMethodID < 1)
    {
        var fulfillmenterror = 'You forgot to pick a fulfillment method. Were not mad, but please pick one.';
    }    

    //If we have all the information we need, then send it the web service that will update the cookie and DB.
    if ((fullfilmentMethodID > 0) && (fullfilmentLocationID > 0)){
        $.ajax({
            type: "GET",
            url: webservicePath,
            data: 'fullfilmentMethod=' + fullfilmentMethodID + '&fullfilmentLocation=' + fullfilmentLocationID,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            cache:false,
            success: OnFulfillmentSuccess,
            error: function(jqXHR, textStatus, errorThrown) {
                var error = 'There was an error when trying to save your selection. Please try again.';
                $('#fulfillmenterrors').text(error);  
                $('#fulfillmenterrors').show();  
            }
        });
    } else {
        $('#fulfillmenterrors').text(fulfillmenterror);  
        $('#fulfillmenterrors').show();  
    }
}

//If successfull then undate the display.
function OnFulfillmentSuccess(response) {
	
    var reloadPage = 'http://www.bevmo.com/default.aspx';
	var FulfillmentDesc = 'Please Select';
	
	//Update the dropdown display.
	if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
        FulfillmentDesc = 'Store Pickup in ' + $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentPickupLocation option:selected").text();
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
        FulfillmentDesc = 'Shipping to ' + $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation option:selected").text();
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
        FulfillmentDesc = 'Delivery from ' + $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentDeliveryLocation option:selected").text();
    }
    
	//Close the form.
	$('div#fulfillment-form').slideToggle();
    $('#fulfillmentMask, .window').hide();
				
    $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentdesc").val(FulfillmentDesc);
    $('#dropdownFulfillment').text(FulfillmentDesc);
    _gaq.push(['_trackEvent', 'Home', 'Click', 'FulfillmentSelect']);
    //Certian pages have to be reloaded because the information on the page has to be updated.
    if ((reloadPage.search(/Default.aspx/i) > -1) || (reloadPage.search(/\/shop\/viewbasket.aspx/i) > -1) || (reloadPage.search(/\/Misc\/Events.aspx/i) > -1) || (reloadPage.search(/\/Shop\/ProductList.aspx/i) > -1) || (reloadPage.search(/\/Shop\/ProductDetail.aspx/i) > -1) || (reloadPage.search(/\/Shop\/LandingPage.aspx/i) > -1) || (reloadPage.search(/Department.aspx/i) > -1)) {
        window.location = CleanFulfillmentQueryString(reloadPage);
    }
}

function ShowPickupFulfillment() {
	// show or hide pick up drop-down menu
	$('#shipping_states').hide();
	$('#delivery_stores').hide();
	$('#pickup_stores').fadeIn();
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStateValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStoreValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDeliveryStoreValidator'), false);
    $('#fulfillmenterrors').hide();  			
}

function ShowShippingFulfillment() {
	// show or hide shipping drop-down menu
	$('#pickup_stores').hide();
	$('#delivery_stores').hide();
	$('#shipping_states').fadeIn();
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStoreValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStateValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDeliveryStoreValidator'), false);
    $('#fulfillmenterrors').hide();  			
}

function ShowDeliveryFulfillment() {
	// show or hide delivery drop-down menu
	$('#pickup_stores').hide();
	$('#shipping_states').hide();
	$('#delivery_stores').fadeIn();
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStoreValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentStateValidator'), false);
	ValidatorEnable(document.getElementById('ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDeliveryStoreValidator'), false);
    $('#fulfillmenterrors').hide();  			
}

function openFulfillmentDropdown(){

    var $u = jQuery('#dropdownFulfillment');
    var pos = $u.offset();
    if (navigator.appVersion.indexOf("webkit") !== -1)
	{
        var pleft = $u.offset().left;// + 298;
        $('#fulfillment-form').css("left",pleft);
    }
    if (navigator.appVersion.indexOf("mozilla") !== -1)
    {
        var pleft = $u.offset().left;// + 299.5;
        $('#fulfillment-form').css("left",pleft);
        $('#fulfillment-form').css("top",171);
    }
    if($.browser.msie)
    {
        var pleft = $u.offset().left;// + 295;
        $('#fulfillment-form').css("left",pleft);
    }
	var pageTracker = _gat._createTracker("UA-4948601-1");
	pageTracker._initData();	
	var _gaq = _gaq || [];
	_gaq.push(['_setAccount', 'UA-4948601-1']);
	_gaq.push(['_setDomainName', 'bevmo.com']);
	_gaq.push(['_trackEvent', 'Home', 'Load', 'Fulfillment']);
     
    //open and display the dropdown form.
    if( $('div#fulfillment-form').is(':hidden') ) {
        $('#pickup_stores').hide();
        $('#shipping_states').hide();
        $('#delivery_stores').hide();
                          
        if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
            $('#pickup_stores').show();            
            $('#fulfillmentMask').show();            
        } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
            $('#shipping_states').show();            
            $('#fulfillmentMask').show();            
        } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
            $('#delivery_stores').show();
            $('#fulfillmentMask').show();            
        } else {
            $('#fulfillmentMask').css('background-color','#555555');

            //display the grey area.     
            $('#fulfillmentMask').fadeIn();    
            $('#fulfillmentMask').fadeTo("slow",0.8);       
        }

        $('#fulfillment-form').slideToggle();
	} else {
        var isiPhone = navigator.userAgent.toLowerCase().indexOf("iphone")
        if (isiPhone  < 0 ){
     	    closeFulfillmentDropdown();
        }	
    }			

    if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':visible')) {
        $('#fulfillment-form').animate({height:'318px'}, 500); 
    }        
} 

//Close the Dropdown.
function closeFulfillmentDropdown() {

    var fullfilmentMethodID = 0;
    var fullfilmentLocationID = 0;
    var error = 'There was an error when trying to save your selection. Please try again.';

    var webservicePath ='/Misc/FulfillmentSelectDropDown.aspx';
    
    if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentPickup').is(':checked')) {
        fullfilmentMethodID = 1;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentPickupLocation")[0].value;
        error = 'Oops! You need to pick a store before proceeding.';     
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').is(':checked')) {
        fullfilmentMethodID = 2;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation")[0].value;
        error = 'Please pick a state';
    } else if ($('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentDelivery').is(':checked')) {
        fullfilmentMethodID = 3;            
        fullfilmentLocationID = $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentDeliveryLocation")[0].value;
        error = 'Please choose a delivery store';
    }

    //If the user clicked close and it was their first time to the site the set Fulfillment to the Default settings.
    if ((fullfilmentMethodID < 1) && (fullfilmentLocationID < 1)){
        fullfilmentMethodID = 2; //Ship 
        fullfilmentLocationID = 8; //CA

        $.ajax({
            type: "GET",
            url: webservicePath,
            data: 'fullfilmentMethod=' + fullfilmentMethodID + '&fullfilmentLocation=' + fullfilmentLocationID,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnFulfillmentSuccessDefault,
            error: function(response) {
                $('#fulfillment-form').slideToggle();
                $('#fulfillmentMask, .window').hide();
            }
        });
    } else {
        if ((fullfilmentMethodID > 0) && (fullfilmentLocationID > 0)){
            $('#fulfillment-form').slideToggle();    
            $('#fulfillmentMask, .window').hide();
        } else {
            fullfilmentMethodID = 2; //Ship 
            fullfilmentLocationID = 8; //CA

            $.ajax({
                type: "GET",
                url: webservicePath,
                data: 'fullfilmentMethod=' + fullfilmentMethodID + '&fullfilmentLocation=' + fullfilmentLocationID,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: OnFulfillmentSuccessDefault,
                error: function(response) {
                    $('#fulfillment-form').slideToggle();
                    $('#fulfillmentMask, .window').hide();
                }
            });
        }
    }
}

//Update the default fulfillment settings
function OnFulfillmentSuccessDefault(response) {
	
    var reloadPage = 'http://www.bevmo.com/default.aspx';
	var FulfillmentDesc = 'Shipping to California';
	var $u = jQuery('#divHeader');
    var pos = $u.offset();
    var pleft = $u.offset().left + 300
    ;
    $('#fulfillment-form').css("left", pleft);   
	$('div#fulfillment-form').slideToggle();
    $('#fulfillmentMask, .window').hide();

	ShowShippingFulfillment();
    
    $('#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentShipping').attr('checked', true);
    $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulFillmentStateLocation").val("8");  			
				
    $("#ctl00_uxMainHeader_uxFulFillmentSelect_uxFulfillmentdesc").val(FulfillmentDesc);
    $('#dropdownFulfillment').text(FulfillmentDesc);
    
    if ((reloadPage.search(/Default.aspx/i) > -1) || (reloadPage.search(/\/shop\/viewbasket.aspx/i) > -1) || (reloadPage.search(/\/Misc\/Events.aspx/i) > -1) || (reloadPage.search(/\/Shop\/ProductList.aspx/i) > -1) || (reloadPage.search(/\/Shop\/ProductDetail.aspx/i) > -1) || (reloadPage.search(/\/Shop\/LandingPage.aspx/i) > -1) || (reloadPage.search(/Department.aspx/i) > -1)) {
        window.location = CleanFulfillmentQueryString(reloadPage);
    }
}

