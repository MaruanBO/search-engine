		    function loadOpenGraph(component) {
		    	$(component).each(function(){
		    		var object = $(this).attr('id');
		    		$.ajax({
		        		url: 'http://graph.facebook.com/'+$(this).data('link'),
		        		dataType: 'jsonp',
		        		success: function(data) {
		        			var stats = ['comment','shares'];
		        			$.map(stats, function(element) {
		        				$('#'+object+' .'+element+'-track').animate({theVal: element}, {
		        					duration: 10000,
		        					easing:'swing',
		        					step: function() {
		        						var value = (element=='comment') ? ((!data.comments) ? 0 : data.comments) : ((!data.shares) ? 0 : data.shares);
		        						$('#'+object+' .'+element+'-track').text(Math.ceil(value));
		        					}
		        				});
		        			});
		        		}
		        	});
		        });
		    }
