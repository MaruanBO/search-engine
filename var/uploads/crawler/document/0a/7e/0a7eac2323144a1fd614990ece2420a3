<script type="text/javascript">
	$(document).ready(function(){
		
			var min = "0px";
			var max = $('#home-hideable-container').height() + "px";
			  	
			
			$("#home-hideable-container").css("height", min);
			
			$(".wsod-bottom-graph").hide();
			$("#right-rail-container").hide();
			$("#bottom-section-par").hide();
							
			$('.mm_view_more_control').live('click', function() {
				var el = $(this);
				if(el.hasClass('mm_view_less')){
					$('#home-hideable-container').animate({height: min}, 500, 'linear', function() {
						el.removeClass('mm_view_less').addClass('mm_view_more');
						$("#mm_view_more_link").html("View More<i class=\"fa fa-chevron-down\"></i>");
						$(".wsod-bottom-graph").hide();
						$("#right-rail-container").hide();
						$("#bottom-section-par").hide();
						$("#hideable-top").focus();
						$("#hideable-top").html("Here is some text that we will not publish out today");
					});
				}else{
					$(".wsod-bottom-graph").show();
					$("#right-rail-container").show();
					$("#bottom-section-par").show();
					$('#home-hideable-container').animate({height: max}, 500, 'linear', function() {
						el.removeClass('mm_view_more').addClass('mm_view_less');
						$("#mm_view_more_link").html("View Less<i class=\"fa fa-chevron-up\"></i>");
						$("#hideable-top").focus();
						$("#hideable-top").html("Here is some text that we will not publish out today-View Less");
					});
				}
			});   		
		
    });
</script>
    
