    var tlbWatermark = "Search Keywords or Symbol";
    jQuery(".search-box input.ForAutofill").focus(function() {
        if (jQuery(this).val() === tlbWatermark && jQuery(this).hasClass('watermark')) jQuery(this).val('').removeClass('watermark');
