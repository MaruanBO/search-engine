    <script type="text/javascript">
 // Use jQuery to create Tabs. Address replacement is done in order to bring up
 // the correct tab when page is bookmarked. Underscore replacement is to 
 // alleviate scrolling issues when address is replaced.
    var selectedTab = 0;
    $('#_tab li a').each(function(index) {
        if ($(this).attr('href').replace("#_", "#") == document.location.hash) {
            selectedTab = index;
        }
    });
    $('#_tab').tabs({
        'select': function(event, ui){
                document.location.hash = ui.panel.id.slice(1); 
            },
        'selected' : selectedTab
    });
    </script>


