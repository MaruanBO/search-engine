<script type="text/javascript">
var token = "";
var id = "";
window.onload = function() {
	if ('safari' in window && 'pushNotification' in window.safari) {
		checkPerms();
	}
};
function checkPerms() {
	var pResult = window.safari.pushNotification.permission('web.com.rushlimbaugh.notify');
	if(pResult.permission === 'default') {
		//request permission
		requestPermissions();
	}
	else if(pResult.permission === 'granted') {
		token = pResult.deviceToken;
	}
	else if(pResult.permission === 'denied') {
	}
}

function requestPermissions() {
	window.safari.pushNotification.requestPermission('https://notify.rushlimbaugh.com', 'web.com.rushlimbaugh.notify', {"id":id}, function(c) {
		if(c.permission === 'granted') {
			token = c.deviceToken;
		}
		else if(c.permission === 'denied') {
		}
	});
}
</script>



