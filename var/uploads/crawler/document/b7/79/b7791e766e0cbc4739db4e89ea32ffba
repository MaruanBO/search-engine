<div id="wall" style="overflow:visible;"></div>
<script>
(function () {
    var Collection = Livefyre.require("streamhub-sdk/collection");
    var WallView = Livefyre.require("streamhub-wall");

    var opts = {
        "network": "timeout.fyre.co",
        "siteId": "347566",  // site id change this if it's not london
        "articleId": "custom-1396538399153", //collection id change this
    };

    var wallView = window.view = new WallView({
        initial: 5,
        showMore: 5,
        el: document.getElementById("wall"),
        columns: 3
    });

    var collection = new Collection(opts);
    collection.pipe(wallView);
}())

</script>
