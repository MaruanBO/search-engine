	<script type="text/javascript">
		window.onload = function(e) {
			// footer site index
			var btn_site_index = document.getElementById('js-btn-index'),
				site_index = jQuery('#js-site-index'),
				isIndexOpen = true,
				isSearch = false,
				isMenuOpen = true,
				btn_search = jQuery('.js-search'),
                search_box = jQuery('.js-searchbox');

            
			// IE8 Safe
			addEvent('click', btn_site_index, toggleIndex);

			function toggleIndex(e) {
				preventDefault(e);
				if(!isIndexOpen) {
					site_index.animate({ height: '400px' }, "slow");
					jQuery('html, body').animate({ scrollTop: jQuery(document).height() }, "slow");
				} else {
					site_index.animate({ height: '0' }, "slow");
				}
				isIndexOpen = !isIndexOpen;
			}

			btn_search.click(function(e) {
                e.preventDefault();
                if (!isSearch) {
                    search_box.fadeIn(); 
                } else {
                    search_box.fadeOut(); 
                }
                isSearch = !isSearch;
            });

			// store locator
			var scrollObject = jQuery('html, body');

			var scrollTo = function (el) {
			var i = 0;
			(function loop() {
			  if (!el.is(':visible') && i < 50) {
			    i += 1;
			    setTimeout(loop, 100);
			  } else if (el.is(':visible')) {
			    scrollObject.stop().animate({
			      scrollTop: el.offset().top - 100
			    }, 2000);
			  }
			}());
			};

			jQuery('.show-map').on('click touchstart', function(e) {
				e.preventDefault();
				scrollTo(jQuery('#map'));
			});

			// home rotator
			if(document.getElementById("js-rotator")) {
				var seconds = 5000,
					current_slide = 0,
					total_slides = jQuery('.js-rotator-slide').length,
					rotator_prev = jQuery('.js-rotator-prev'),
					rotator_next = jQuery('.js-rotator-next'),
					bullet = jQuery('.js-rotator-bullet'),
					timer = setInterval(function() { autoRotate() }, seconds);

				bullet.click(function() {
					current_slide = jQuery(this).index();
					showSlide();
					return false;
				});

				rotator_prev.click(function() {
					current_slide--;
					showSlide();
					return false;
				});

				rotator_next.click(function() {
					current_slide++;
					showSlide();
					return false;
				});

				function showSlide() {
					checkSlide();

					// update bullets
					bullet.removeClass('active');
					jQuery('.js-rotator-bullet:eq('+current_slide+')').addClass('active');

					// show next slide
					jQuery('.js-rotator-slide').hide();
					jQuery('.js-rotator-slide:eq('+current_slide+')').fadeIn('slow');

					resetRotateTimer();
				}

				function autoRotate() {
					current_slide++;
					showSlide();
				}

				function checkSlide() {
					// reset slide to beginning if last
					if(current_slide == total_slides) current_slide = 0;

					// go to last slide
					if(current_slide < 0) current_slide = total_slides - 1;
				}

				function resetRotateTimer() {
					clearInterval(timer);
					timer = setInterval(function() { autoRotate() }, seconds);
				}

        var aSpotHoverTime = new Date();

        function pauseTimer() {
          clearInterval(timer);
          aSpotHoverTime = new Date();
        }

        jQuery('#js-rotator').mouseenter(function() {
          pauseTimer();
        }).mouseleave(function(){
          if (((new Date().getTime() - aSpotHoverTime.getTime()) / 1000 ) > 2) {
            autoRotate();
            resetRotateTimer();
          } else {
            resetRotateTimer();
          }
        });

			}

			// catering detail
			if(document.getElementById("js-menus")) {
				var menu_index  = 0,
					menu_item = jQuery('.js-menu-item'),
					menu_anchor = jQuery('.js-menu-anchor'),
					menu_content = jQuery('.js-menu-content'),
					hash = window.location.hash.split('?');

				if(window.location.hash != '') {
					switch(hash[0]) {
						case '#kosher':
							menu_index = 1;
							showMenuTab();
							break;
						case '#instructions':
							menu_index = 2;
							showMenuTab();
							break;
						default:
							break;
					}
				}

				updateContactDetails();

				menu_anchor.click(function() {
					menu_index = jQuery(this).parent().index();
					location.hash = jQuery(this).attr('href');
					showMenuTab();
					updateContactDetails();
					return false;
				});
			}

			function showMenuTab() {
				menu_content.hide();
				jQuery('.js-menu-content:eq('+menu_index+')').show();
				menu_item.removeClass('nav-menu-active');
				jQuery('.js-menu-item:eq('+menu_index+')').addClass('nav-menu-active');
			}

			function updateContactDetails() {
				var container = jQuery('.contact-info'),
					index     = menu_index > 1 ? 0 : menu_index;

				if (index < window.cateringContacts.length) {
					jQuery('.fn', container).text(window.cateringContacts[index].name);
					jQuery('.tel', container).text(window.cateringContacts[index].phone);
					jQuery('.contact-email', container)
						.text(window.cateringContacts[index].email)
						.attr('title', window.cateringContacts[index].name)
						.attr('href', 'mailto:'+window.cateringContacts[index].email);
				}
			}

			// mobile app page
			if(document.getElementById("js-mobile-app")) {
				var overlay_full = jQuery('.js-overlay-full'),
					btn_close_overlay = jQuery('.js-close-overlay'),
					phone = jQuery('.js-phone'),
					overlay_anchor = jQuery('.js-overlay-anchor'),
					screen_anchor = jQuery('.js-screen-anchor'),
					screen_group = jQuery('.js-screen-group'),
					app_features = jQuery('.js-app-features'),
					app_feature = jQuery('.js-app-feature'),
					video_container = jQuery('.js-video-embed'),
					video_iframe = jQuery('<iframe width="853" height="480" src="//www.youtube.com/embed/-ZoYEcdLsOw?autoplay=1" frameborder="0" allowfullscreen></iframe>'),
					feature_index = 0;

				//video_container.empty();

				overlay_anchor.click(function() {
					var id = jQuery(this).attr('href');
					jQuery(''+id+'').fadeIn('fast', function() {

					});
					if(id == '#js-screen-overlay') {
							screen_group.addClass('show');
						} else {
							video_container.append(video_iframe);
						}
					return false;
				});

				screen_anchor.click(function() {
					var imgsrc = jQuery(this).find('.screen-img').attr('src');
					phone.attr('src', imgsrc);
					closeOverlay();

					// update active state on feature
					feature_index = jQuery(this).parent().index();
					app_feature.removeClass('active');
					jQuery(app_feature.get(feature_index)).addClass('active');
					return false;
				});

				btn_close_overlay.click(function() {
					closeOverlay();
					return false;
				});

				function closeOverlay() {
					if(screen_group.hasClass('show'))
						screen_group.removeClass('show');

					overlay_full.fadeOut('fast', function() {
						video_container.empty();
					});
				}
			}

			// store detail page
			if(document.getElementById('js-select-store')) {
				var select_store = jQuery('#js-select-store');

				select_store.change(function() {
					document.location.href = '/store/' + jQuery(this).val();
				});
			}

			// magazine page
			if(document.getElementById("js-magazine")) {
				jQuery(".js-issuu").fancybox({
					fitToView	: false,
					width		: '90%',
					height		: '90%',
					autoSize	: false,
					closeClick	: false,
					openEffect	: 'none',
					closeEffect	: 'none'
				});
			}

			// text to win page
			if(document.getElementById("js-ttw")) {
				var instructions = jQuery('.js-instructions'),
					select_store = jQuery('.js-ttw-store'),
					phrase = jQuery('.js-phrase');

				select_store.change(function() {
					phrase.html(jQuery(this).val());
					instructions.show();
				});
			}
		};
	</script>
