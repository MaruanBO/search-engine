	<script type="text/javascript">
		jQuery(document).ready(function() {
			var list = jQuery('.js-blog-list');
			jQuery.ajax({
	            type: "GET",
	            url: "http://blog.fairwaymarket.com/get-latest",
	            dataType: "jsonp",
	            jsonpCallback : 'rssCallback'
	        }).done(function( data ) {
	            jQuery.each(data.articles, function( index, value ) {
	            	var t = '<li class="col span_4">' +
				                '<a href="'+value.permalink+'" title="'+value.title+'" target="_blank" class="blog-image"><img src="'+value.image+'" alt="'+value.title+'" class="img-full"></a>' +
				                '<h1 class="blog-title"><a href="'+value.permalink+'" title="'+value.title+'" target="_blank">'+value.title+'</a></h1>' +
				                '<a href="'+value.permalink+'" title="Read More" class="read-more" target="_blank">Read More <span class="bg-circle">&rsaquo;</span></a> ' +
				            '</li>';
				    list.append(t);
	            });
	        });
		});
	</script>
