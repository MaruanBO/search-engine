<script>
    Event.observe(window, 'load', function () {
        $('newsletter-subscribe').observe('click', function (e) {
            if (newsletterSubscriberFormDetail.validator.validate()) {
                var newsletter_email = $('newsletter-email').getValue();
                var url = 'http://www.annaslinens.com/foa/ajax/customer_subscribe';
                var parameters = {email:newsletter_email};
                var newsletter_block = $('newsletter-block');

                newsletter_block.update('<div class="white-bg"><div class="loading-ajax">&nbsp;</div></div>');

                new Ajax.Request(url, {
                    method:'post',
                    parameters:parameters,
                    onSuccess:function (transport) {
                        //foa_block_number.update(transport.status);
                        if (transport.status == 200) {
                            //  var response = transport.responseText.evalJSON();

                            newsletter_block.update('<div class="white-bg">' + transport.responseText + '</div>');
                        }
                    }
                });
            }
        });
    });
</script>
