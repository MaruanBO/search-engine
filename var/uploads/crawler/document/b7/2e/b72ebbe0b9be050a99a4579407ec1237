jQuery('.act').append('<div class="act-arr"></div>');
jQuery('.tabscroller li').each(function(i) {
      jQuery(this).click(function(){
        jQuery('.act-arr').remove();
        jQuery(this).addClass('act').siblings().removeClass('act').parents('.tabscroller').find('.tabs').fadeOut(700).end().find('.tabs:eq('+i+')').fadeIn(700);
        jQuery('.act').append('<div class="act-arr"></div>');
        clearInterval(auto);
    });
  });
  var k =1;
  var auto  = setInterval(function(){
        if(k<4){
        jQuery('.act-arr').remove();
        jQuery('.tabscroller li:eq('+k+')').addClass('act').siblings().removeClass('act').parents('.tabscroller').find('.tabs').fadeOut(700).end().find('.tabs:eq('+k+')').fadeIn(700);
        jQuery('.act').append('<div class="act-arr"></div>');
        k++;
        }
        else{k = 0; return false;}
    },5000);

