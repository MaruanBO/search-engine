</div><div id="tncms-block-490775" class="tncms-block"><iframe src="http://www.facebook.com/plugins/likebox.php?href=http://www.facebook.com/pages/The-Daily-News/115458415536&amp;width=300&amp;colorscheme=light&amp;show_faces=1&amp;stream=1&amp;header=1" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:300px; height:610px;" allowTransparency="true"></iframe></div><div id="tncms-block-504671" class="tncms-block"><script type="text/javascript">
/*
    TN Comment API JS
    Last Update: 9/28/2012
    Added confirm.
*/

jQuery(document).ready(function($)
{
	/* build thread */
	$('#comment-list li').each(function(){
		if( $(this).hasClass('has-replies') ){
			var tID = $(this).attr('id');
			var pID = $(this).attr('title');
			/* add child list */
			if( $('#'+pID+' .replies').length == 0 && tID != pID ) $('#'+pID).append('<ol id="replies-'+pID+'" class="replies"></ol>');
			/* inject comment to parent */
			$('#replies-'+pID).append( '<li id='+tID+'>'+$(this).html()+'</li>' );
			/* remove default */
			$(this).remove();
		}
	});
	
	/* expand comments */
    $('.expand-comments').click(function(){
        $('.comment-view-btn').hide();
        $('#comment-container').slideDown();
        $('#loadComments').scrollTo(1000);
        track_comment_expand();
        return false;
    });
    
    function addComment(){
       var postAction = $('#postAction').attr('title');
	  
	   $('#addComment').submit(function(){
            // the comment
            var aComment = encodeURIComponent( $('#commentText').val() );
            var pUUID = $('#reply-uuid').val();
            var commentsDiv = 'loadComments';
            
            // stop and alert if no comment set
            if(!aComment){
                formResponse('comment-posted-error','Please Enter a Comment!','alert-message warning');
        
            } else {
                // try and prevent multiple posts
                submitButton('hide');
    
                $.ajax({
                    type: "POST",
                    url: postAction,
                    data: "format=json&action=comments:post&comment="+aComment+"&parent_uuid="+pUUID,
                    error: function(result){ formResponse('comment-posted-error','An Error Occured!','alert-message error'); submitButton('show'); },
                    success: function(result){
                        if(result.success) {
                            formResponse('comment-posted-ok','<strong>Comment Posted!</strong> A moderator will review your submission.','alert-message success');
                            $('#commentText').val('');
                            submitButton('show');
                        
                            if($('#'+commentsDiv+' li:first').length==0){
                                $('#comment-top-response').html('You posted the first comment!');
                                $('#'+commentsDiv+' #comment-count').after('<ol><li style="display: none;"></li></ol>');
                            }
                        
                            // add comment to list
                            $('#'+commentsDiv+' li:first').before('<li class="featured moz-border"><dl><dt></dt><dd>'+ decodeURIComponent( aComment ) +'</dd></dl>');
                            submitButton('show');
                        } else {
                            var errorMsg = 'An Error Occured!';
                            if(result.error) { errorMsg = result.error; }
                            formResponse('comment-posted-error',errorMsg,'alert-message error'); submitButton('show');
                        }
                    }
                });
            }
            
            return false;
        });
    }
        // unbind previous submit events
    $('#addComment').unbind('submit')
	// re-attach submit event
    addComment();
    
    function reportAbuse(commentID,event){
        var postAction = $('#postAction').attr('title');
        var reportConfirm = confirm("Are you sure you want to report this comment for being abusive?");
        if (reportConfirm==true){
	        $.ajax({
	            type: "POST",
	                url: postAction,
	                data: "format=json&action=comments:abuse&comment_id="+$(commentID).attr('title'),
	                error: function(result){ alert('Unable to report comment! Try again later.'); },
	                success: function(result){
	                    alert('Thank You! This comment has been reported.');
	                }
	            });
        }
        event.preventDefault()
        return false;
    }
    $('.report-comment').unbind('click');
    $('.report-comment').click(function(event){
    	reportAbuse(this,event);
    });
    
    function submitButton(state)
    {
        if(state=='hide'){
            $('#addCommentButton').attr('disabled','disabled');
            $('#addCommentButton').attr('value','Please Wait...');
        } else {
            $('#addCommentButton').removeAttr('disabled','disabled');
            $('#addCommentButton').attr('value','Submit Comment');
        }
    }
    
    // form messages
    function formResponse(div,message,className)
    {
        // attach message
        $('#comment-form-response').html('<div id="'+div+'" class="'+className+'">'+message+'</div>');
        // fade out and clear message
        $('#'+div).delay(5000).fadeOut();
        $('#comment-form-response').html();
    }
    
    function countChars(commLength)
    {
        $("#commentText").keyup(function(){
            var maxChars = parseInt(commLength);
            var commentVal = $("#commentText").val();
            var charsLeft = ( maxChars-commentVal.length );
                
            if(charsLeft > -1){
                $('#countCommentChars').css('color','#333333');
                if(charsLeft == 0){
                    $('#countCommentChars').css('color','#cc0000');
                    $('#countCommentChars').html('<strong>No more characters remaining.</strong>');
                } else {
                    $('#countCommentChars').html('<strong>'+charsLeft+'</strong> characters remaining.');
                }                
            } else {
                $('#commentText').val(commentVal.substr(0, maxChars));
            }
        });
    }
    // attach comment count if active
    var commLength = $('#comment-settings #comment-max-length').attr('title');
    
    if(commLength!='false'){
        countChars(commLength);
        $('#countCommentChars').html('<strong>' + commLength + '</strong> Characters Remaining').show();
    }
    
    $('.quote-comment').click(function(){
        $('#commentText').val(null);
            
        var qID = $(this).attr('id').replace('quote-','');
        var qText = $('#tn-comment-'+qID).html().replace(/<(\w+)[^>]*>((?:(?!<\/?\1\b[^>]*>)[\S\s])*)<\/\1>/g, '').replace('<br>','');
        var qUser = $('#tn-user-'+qID).attr('title');
        
        $('#commentText').val('[quote]'+ qUser + ' said: "' + qText + '"[/quote]\n\n');
        $('#commentText').focus();
        
        return false;
    });
    
	function cancelReply(){
		$('#cancel-reply').click(function(){
			var cForm = $('#comment-form-container').html();
			$('#comment-form-container').remove();
			$('#comment-container-org').html( '<div id="comment-form-container">'+cForm+'</div>' );
			$('#reply-uuid').val('');
			$('#cancel-reply').hide();
			reAttachCommentEvents();
		});
	}
	
	$('.reply-comment').click(function(){
		var parent = $(this).attr('title');
		$('#reply-uuid').val(parent);
		
		var pLI = $(this).parent().parent();
		var cForm = $('#comment-form-container').html();
		
		$('#comment-form-container').remove();
		$(pLI).append( '<div id="comment-form-container">'+cForm+'</div>'  );
		$('#cancel-reply').show();
		reAttachCommentEvents();
                
		return false;
	});
	
	function reAttachCommentEvents(){
		// re attach events
		addComment();
		// attach comment count if active
		var commLength = $('#comment-settings #comment-max-length').attr('title');
		if(commLength!='false'){
			countChars(commLength);
			$('#countCommentChars').html('<strong>' + commLength + '</strong> Characters Remaining').show();
		}
		cancelReply();
	}
        
        /* scroll to form */
        var goToCommentForm = getParam('comment_form');
        if(goToCommentForm) $('#loadComments').scrollTo(1000);
});
</script></div></div>
    
    
    
