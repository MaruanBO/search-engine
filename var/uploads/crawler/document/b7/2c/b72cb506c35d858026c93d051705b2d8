<script type="text/javascript">
    function OmnTrackCustomLink(name)
    {
        s.linkTrackVars = 'eVar17,eVar18,eVar19';
        s.eVar17 = s.getTimeParting('h', '-8'); // Set hour 
        s.eVar18 = s.getTimeParting('d', '-8'); // Set day
        s.eVar19 = s.getTimeParting('w', '-8'); // Set Weekend / Weekday
        s.tl(this,'o',name);
    }	
	
    function OmnTrackExitLink(name)
    {
        s.linkTrackVars = 'eVar17,eVar18,eVar19';
        s.eVar17 = s.getTimeParting('h', '-8'); // Set hour 
        s.eVar18 = s.getTimeParting('d', '-8'); // Set day
        s.eVar19 = s.getTimeParting('w', '-8'); // Set Weekend / Weekday
        s.tl(this,'e',name);
    }	
	
    var campaignId = 0;
    var cmpre = /[&?#]cmpid=([^&?#=]*)/;
    var res = cmpre.exec(window.location);
    if (res && res[1]) { campaignId = res[1]; }
	
    var refDomain = "";
    if (! document.referrer.match(/crackle\.com/))
    {
        var domMa = /(?:http:\/\/)?(?:https:\/\/)?([^/]*)/i.exec(document.referrer)
        if (domMa && domMa[1])
            refDomain = domMa[1];
    }
    Crackle.stats.SetCampaignCookiesAndLogUniques (campaignId, refDomain);

    var strQuery = GetQueryFromAfterPound (window.location.href);
    var qs = ParseQueryString (strQuery);

    //SiteCatalyst code version: H.19.3.
    //Copyright 1997-2009 Omniture, Inc. More info available at
    //http://www.omniture.com -->

    /* You may give each page an identifying name, server, and channel on
    the next lines. */
    s.pageName="crackle.com:home page"
        s.server=""
        s.channel=""
        s.pageType=""
        s.prop1="home"
    s.prop2="home"
    s.prop3="home"
    s.prop6=""
    
    s.prop7="Logged Out"
    
    s.prop40=window.location.href;
    if (qs && qs ["q"])
        s.prop6 = qs["q"];
    /* Conversion Variables */
    s.campaign= "";
    try 
    {
        if (campaignId && campaignId > 0)
            s.campaign = campaignId;
    } catch (e) {}

    s.state = "";
    s.zip = "";
    //s.events="";
    s.products = "";
    s.purchaseID = "";
    s.eVar1 = "";
    s.eVar8 = "";
    s.eVar10 = getPrettyDomain(document.domain.toLowerCase());
    s.prop50=s.eVar10;
    
    s.eVar5 = "Home Site";
    
    s.eVar39 = "crackle";
    

    if (qs["utm_source"])
        s.eVar51 = qs["utm_source"];
    if (qs["utm_medium"])
        s.eVar52 = qs["utm_medium"];
    if (qs["utm_campaign"])
        s.eVar53 = qs["utm_campaign"];
    if (qs["utm_term"])
        s.eVar54 = qs["utm_term"];
    if (qs["utm_content"])
        s.eVar55 = qs["utm_content"];

    var pcmp = Crackle.stats.GetPcmpObFromCookies()

    s.eVar14 = Crackle.stats.GetDaysSinceFirstVisit(pcmp,-8).toString();
    s.eVar15 = s.getDaysSinceLastVisit('s_lv');
    s.eVar32 = s.getReferrer();
    s.eVar58 = Crackle.stats.GetMonthOfFirstVisit(pcmp,-8);

    

    if(qs && qs["regsuccess"]) {
        s.events =  "event3,event20";
    }
    else {
        s.events = "";
    }

    // Optimizely SiteCatalyst Integration
    window.optimizely = window.optimizely || [];
    window.optimizely.push("activateSiteCatalyst");

    /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
    var s_code=s.t();if(s_code)document.write(s_code) //--></script>
<!--/DO NOT REMOVE/-->
