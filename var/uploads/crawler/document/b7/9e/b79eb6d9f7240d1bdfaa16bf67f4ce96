	function AC_AddExtension(src,ext)
	{if(src.indexOf('?')!=-1)
	return src.replace(/\?/,ext+'?');else
	return src+ext;}
	function AC_Generateobj(objAttrs,params,embedAttrs)
	{var str='';if(isIE&&isWin&&!isOpera)
