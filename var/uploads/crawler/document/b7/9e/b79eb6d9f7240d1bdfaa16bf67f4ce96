	function generateFlash(_id, _url, _opts){
		var a_height = (_opts==null || _opts.height == null) ? "100%" : _opts.height;
		var a_width = (_opts==null || _opts.width == null) ? "100%" : _opts.width;
		
		var sWmode="opaque";
		if(null!=_opts && _opts.wmode){
			sWmode=_opts.wmode;
		}
		if(_url.indexOf(".swf")!=-1){
			_url=_url.replace(".swf","");
		}
		var ss=AC_FL_RunContent(
		        "src", _url,
		        "FlashVars", window.location.search.substring(1),
		        "width", a_width,
		        "height", a_height,
		        "id", _id,
		        "name", _id,
		        "align", "middle",
		        "quality", "high",
		        "allowScriptAccess","always",
		        "type", "application/x-shockwave-flash",
		        "pluginspage", "http://www.adobe.com/go/getflashplayer",
		        "wmode", sWmode
		    );
		
	    if(null!=_opts && _opts.nowrite){
	    	return ss;
	    }else{
	    	document.write(ss);
	    }
	}
