<script type="text/javascript">
	function load_1732993562() {
        var autocompleteBilling_1732993562 = new google.maps.places.Autocomplete(document.getElementById('location_search_value_1732993562'), {types: ['(regions)']});
        autocompleteBilling_1732993562.setComponentRestrictions({'country': 'us'}); //United States only for now
        google.maps.event.addListener(autocompleteBilling_1732993562, 'place_changed', function () {
        	var place = autocompleteBilling_1732993562.getPlace();

        	var city = '';
                var state = '';
                var hasCity = false;
                for (var i = 0; i < place.address_components.length; i++) {
                    if (place.address_components[i].types[0] == 'administrative_area_level_1') {
                        state = place.address_components[i]['short_name'];
                    }
                    if (place.address_components[i].types[0] == 'sublocality_level_1') {
                        city = place.address_components[i]['short_name'];
                        hasCity = true;
                    }
                    if (place.address_components[i].types[0] == 'locality' && !hasCity) {
                        city = place.address_components[i]['short_name'];
                    }
                }

            document.getElementById('location_search_value_1732993562').value = city + ' ' + state;
        });
    };
    addListener('load',window,load_1732993562);

</script>
