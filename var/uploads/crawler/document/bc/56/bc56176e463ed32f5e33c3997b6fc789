<script type="text/javascript">
	function addListener(event, obj, fn) {
	    if (obj.addEventListener) {
	        obj.addEventListener(event, fn, false);   // modern browsers
	    } else {
	        obj.attachEvent("on"+event, fn);          // older versions of IE
	    }
	}

	function checkAutoComplete() {
		if($$('.header_store_current input[type=text]:focus').length > 0){
			document.getElementsByClassName("header_store_current")[0].addClassName("hover");
		} else {
			document.getElementsByClassName("header_store_current")[0].removeClassName("hover");
		}
	}

	function showHide() {
		if(document.getElementsByClassName("header_store_current")[0].classList.contains('hover')){
			document.getElementsByClassName("header_store_current")[0].removeClassName("hover");
		} else {
			document.getElementsByClassName("header_store_current")[0].addClassName("hover");
		}
	}

	var formElement;
	function validateAddress(parent) {
		formElement = parent;
		var address = jQuery(parent).find("input[type=text]").val();
		var service = new google.maps.places.AutocompleteService();
  		service.getPlacePredictions({ input: address, componentRestrictions: {country: 'us' }, types: ['(regions)'] }, callback);
		/*var obj = {};
		obj['address'] = jQuery(parent).find("input[type=text]").val();
		jQuery.ajax( {
		    url  : 'http://www.fivebelow.com/store-locator/index/checkIfValidAddress/',
		    data: obj,
		    success : function( data) {
		        if (data == "OK") {
		        	$(parent).submit();
		        } else {
		        	alert("Please enter a valid city, state or zip.");
		        }
		    },
		    error : function( data, textStatus ) {
		       	alert("Something went wrong. Please try again.");
		    }
		} );		*/
	}

	function callback(predictions, status) {
		if (status == "OK") {
			$(formElement).submit();
        } else if (status == "ZERO_RESULTS") {
        	jQuery(".valid").show();
        	jQuery(".error").hide();
        	showValidateOverlay();
        } else {
        	jQuery(".valid").hide();
        	jQuery(".error").show();
        	showValidateOverlay();
        }
	}

	document.addEventListener('DOMNodeInserted', function(event) {
        var target = event.target;
        if (target.classList) {
	        if (target.classList.contains('pac-item')) {
	            target.innerHTML = target.innerHTML.replace(", United States","");

	        	if (target.innerHTML.indexOf(" County") != -1) {
	        		target.style.display = "none";
	        	}
	        }
    	}
    });
</script>

