<script type="text/javascript">
	function load_190378626() {
        var autocompleteBilling_190378626 = new google.maps.places.Autocomplete(document.getElementById('location_search_value_190378626'), {types: ['(regions)']});
        autocompleteBilling_190378626.setComponentRestrictions({'country': 'us'}); //United States only for now
        google.maps.event.addListener(autocompleteBilling_190378626, 'place_changed', function () {
        	var place = autocompleteBilling_190378626.getPlace();

        	var city = '';
                var state = '';
                var hasCity = false;
                for (var i = 0; i < place.address_components.length; i++) {
                    if (place.address_components[i].types[0] == 'administrative_area_level_1') {
                        state = place.address_components[i]['short_name'];
                    }
                    if (place.address_components[i].types[0] == 'sublocality_level_1') {
                        city = place.address_components[i]['short_name'];
                        hasCity = true;
                    }
                    if (place.address_components[i].types[0] == 'locality' && !hasCity) {
                        city = place.address_components[i]['short_name'];
                    }
                }

            document.getElementById('location_search_value_190378626').value = city + ' ' + state;
        });
    };
    addListener('load',window,load_190378626);

</script>
