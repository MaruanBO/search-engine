<script type="text/javascript">
    function hideValidateOverlay() {
        $("address_overlay").setStyle({
        display: "none"
        });
        $("address_overlay_message").setStyle({
            display: "none"
        });
    }

    function showValidateOverlay() {
        $("address_overlay").setStyle({
        	display: "block"
        });
        $("address_overlay_message").setStyle({
            display: "block"
        });
    }

    $("address_overlay").observe("click", function(e) {
        hideValidateOverlay();
    });
    $("address_overlay_close").observe("click", function(e) {
        hideValidateOverlay();
        e.preventDefault()
    });
    $("address_overlay_x").observe("click", function(e) {
        hideValidateOverlay();
        e.preventDefault()
    });
</script>

