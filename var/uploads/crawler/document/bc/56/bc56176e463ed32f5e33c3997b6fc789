	<div class="arrow fl next">
		<img src="http://cdn.fivebelow.com/skin/frontend/default/fiveBelow/images/redesign/nextArrow.png" alt="">
	</div>
</ul>
</div>
<script type="text/javascript">
	var itemCount = 5;
	var speed = 100;

	jQuery(document).ready(function(){
		jQuery('.slider img').css('width','100%').css('height','100%').css('max-height','377px');
		console.log();
			});

	var bannerInterval = setInterval(function(){changeBanner("next", false);}, 7000);
	jQuery(".prev").click(function () {
		changeBanner("prev", false);
		clearInterval(bannerInterval);
	});
	
	jQuery(".next").click(function () {
		changeBanner("next", false);
		clearInterval(bannerInterval);
	});
	
	changeBanner("next", false);
	
	jQuery(window).resize(function () {
		changeBanner("next", true);
	});


	var swiped = false;
    jQuery(".trendingListing .item").touchwipe({
        wipeRight: function() {
            // Previous
			clearInterval(bannerInterval);
			changeBanner("prev", false);
			swiped = true;
        },
        wipeLeft: function() {
            // Next  
			clearInterval(bannerInterval);
			changeBanner("next", false);
			swiped = true;
        },
        preventDefaultEvents: false
    });

	jQuery(".trendingListing .item").hover(
		function(){
			if(jQuery('.product_list.hover_ready').length){
				jQuery('.halfCircle', this).attr('src',"http://cdn.fivebelow.com/skin/frontend/default/fiveBelow/images/redesign/halfCircleActive.png");
				clearInterval(bannerInterval);
			}
		},
		function(){
			jQuery('.halfCircle', this).attr('src',"http://cdn.fivebelow.com/skin/frontend/default/fiveBelow/images/redesign/halfCircle.png");
			if(!swiped)
				bannerInterval = setInterval(function(){changeBanner("next", false);}, 7000);
		}
	);
	
	function changeBanner(direction, reset){
		var totalItems = jQuery(".slidingBanner").children(".item").length;

		itemCount = 5;
		if (window.innerWidth < 884) {
			itemCount = 2;
			jQuery(".slidingBanner .prev").hide();
			jQuery(".slidingBanner .next").hide();
			jQuery(".slidingBanner").children(".item").css('width','148px');
			//jQuery(".halfCircle").css('width','148px');
			jQuery('.product_list').removeClass('hover_ready');
			speed = 250;
		}
		else if (window.innerWidth < 988) {
			itemCount = 3;
			jQuery(".slidingBanner .prev").show();
			jQuery(".slidingBanner .next").show();
			jQuery(".slidingBanner").children(".item").css('width','196px');
			//jQuery(".halfCircle").css('width','148px');
			jQuery('.product_list').removeClass('hover_ready');
			speed = 250;
		} 
		else if (window.innerWidth < 1181) {
			itemCount = 4;
			jQuery(".slidingBanner .prev").show();
			jQuery(".slidingBanner .next").show();
			jQuery(".slidingBanner").children(".item").css('width','196px');
			//jQuery(".halfCircle").css('width','148px');
			jQuery('.product_list').removeClass('hover_ready');
			speed = 200;
		}
		else{
			jQuery(".slidingBanner .prev").show();
			jQuery(".slidingBanner .next").show();
			jQuery(".slidingBanner").children(".item").css('width','196px');
			//jQuery(".halfCircle").css('width','148px');
			jQuery('.product_list').addClass('hover_ready');
			speed = 600;
		}
		
		var currentIndex = jQuery(".slidingBanner .item").index(jQuery(".slidingBanner").find(".item:visible:first"));
		
		if(direction == "next"){

			jQuery(".slidingBanner .item").removeClass("visible",speed);
						
			var start = currentIndex + itemCount;
			if (currentIndex == -1 || start >= totalItems || reset) {
				start = 0;
			}
			
			var end = start + itemCount - 1;
			if (end > totalItems) {
				end = totalItems - 1;
			}
			jQuery(".slidingBanner").children(".item").css('left','100%');

			for (i = end; i >= start; i--) {
				jQuery(".slidingBanner").children(".item")[i].setAttribute('class', 'item fl visible');
				jQuery(".slidingBanner").children(".item.visible").animate({"left":"0px"},speed);
			}
			//if on last row and doesn't fill, include first row
			if((end - start)+1 < itemCount)
				for (i = itemCount-(end-start+2); i >= 0; i--) {
					jQuery(".slidingBanner").children(".item")[i].setAttribute('class', 'item fl visible');
					jQuery(".slidingBanner").children(".item.visible").animate({"left":"0px"},speed);
				}

		}
		
		if(direction == "prev"){

			jQuery(".slidingBanner .item").removeClass("visible",speed);
			
			var start = currentIndex - itemCount;
			if (currentIndex == -1) {
				start = itemCount - 1;
			}
			if (start < 0) {
				start = totalItems - itemCount ;
			}
			var end = start + itemCount - 1;
			if (end > totalItems) {
				end = totalItems - 1;
			}
			jQuery(".slidingBanner").children(".item").css('left','-100%');
			
			for (i = start; i <= end; i++) {
				jQuery(".slidingBanner").children(".item")[i].setAttribute('class', 'item fl visible');
				jQuery(".slidingBanner").children(".item.visible").animate({"left":"0px"},speed);
			}
			//if on last row and doesn't fill, include first row
			if((end - start)+1 < itemCount)
				for (i = 0; i <= itemCount-(end-start+2); i++) {
					jQuery(".slidingBanner").children(".item")[i].setAttribute('class', 'item fl visible');
					jQuery(".slidingBanner").children(".item.visible").animate({"left":"0px"},speed);
				}

		}
	}
</script>
				<div class="clr"></div>
			</div><!--end box1-->
