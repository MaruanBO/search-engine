    (function(){ 
        /* IE8 Compatibility View kludge, even though production doesn't use it */
        if(!browser.ie8 && !browser.ie7) return;
            var trouble = document.getElementsByTagName("a");
            for(var i = 0; i < trouble.length; i++){
                if(/(^|\b)lvl2($|\b)/.test(trouble[i].className) && /(^|\b)lvl2($|\b)/.test(trouble[i].parentNode.className)){
                    switch(trouble[i].getAttribute("title")){
                        case "Men": {trouble[i].style.width = "32px"; break; }
                        case "Women": {trouble[i].style.width = "58px"; break; }
                        case "Kids": {trouble[i].style.width = "31px"; break; }
                        case "Customize": {trouble[i].style.width = "81px"; break; }
                        case "Timberland Pro": {trouble[i].style.width = "129px"; break; }
                        case "Sale": {trouble[i].style.width = "33px"; break; }
                    }
                    trouble[i].style.paddingLeft = "20px";
                    trouble[i].style.paddingRight = "20px";
                }
            }
    })();

</script>
 
    
