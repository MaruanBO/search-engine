$(document).observe('dom:loaded', function(e){
	if($$('.ext-links')){

	var extUl = $$('.ext-links');
		for (var x=0; x < extUl.length; x++){
			extUl[x].observe("click", function(e) {
				var clickedLi = e.findElement('li');
				var liLink = clickedLi.down(0);
				if (liLink.innerHTML.indexOf("About") > -1 || liLink.innerHTML.indexOf("Job") > -1) {
					var extURL = liLink.readAttribute("href").toString();
				if(isIE && ieversion<6){
					e.preventDefault();
					window.open(extURL, "_self");
				}else{
					e.preventDefault();
					window.open(extURL, "_self");
				}
				}
			});
		};
	}
});
