			$$('.button').each(function(el) {
				if( ( el.getStyle( 'backgroundImage' ).include('gradient_buttons.jpg') ) && ( el.tagName != 'A' ) ) {
					Event.observe( el, 'mouseover', function() {
						var newBgPos = parseInt( el.getStyle( 'background-position-y') ) - 36 + "px";
