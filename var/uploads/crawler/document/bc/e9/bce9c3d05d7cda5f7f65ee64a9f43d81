<script type="text/javascript">

jQuery.noConflict();

jQuery(document).ready(function($){
	var selectedTab = Cookie.get('lastSelectedTab', 0);
	selectedTab = parseInt(selectedTab) + 1;
	if ($('.tabcont > div:nth-child(' + selectedTab + ')').size() == 0) {
		selectedTab = 1;
	}
	$('.tabcont > div:nth-child(' + selectedTab + ')').show();
	$('.tabmenu li:nth-child(' + selectedTab + ')').addClass('active');

	$('.tabmenu li a').click(function(){
		$('.tabmenu li').removeClass('active');
		$(this).parent().addClass('active');
		var currentTab = $(this).attr('href');
		$('.tabcont > div').hide();
		$(currentTab).show();
		var lastSelectedTab = $('.tabmenu li a').index(this);
		Cookie.set('lastSelectedTab', lastSelectedTab, 1000*60*60*24*30, '.nicovideo.jp', '/');
		return false;
	});
});

</script>




