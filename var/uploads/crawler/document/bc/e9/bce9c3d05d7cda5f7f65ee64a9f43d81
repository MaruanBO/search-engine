      <div id="Blomaga_tab" class="simple_frame">


<script type="text/javascript">

jQuery.noConflict();

jQuery(document).ready(function($){
	var selectedTab = Cookie.get('lastSelectedTabBlomaga', 2);
	selectedTab = parseInt(selectedTab) + 1;
	if ($('.blomaga_tab_cont > div:nth-child(' + selectedTab + ')').size() == 0) {
		selectedTab = 1;
	}
	$('.blomaga_tab_cont > div:nth-child(' + selectedTab + ')').show();
	$('.blomaga_tab_menu li:nth-child(' + selectedTab + ')').addClass('active');

	$('.blomaga_tab_menu li a').click(function(){
		$('.blomaga_tab_menu li').removeClass('active');
		$(this).parent().addClass('active');
		var currentTab = $(this).attr('href');
		$('.blomaga_tab_cont > div').hide();
		$(currentTab).show();
		var lastSelectedTabBlomaga = $('.blomaga_tab_menu li a').index(this);
		Cookie.set('lastSelectedTabBlomaga', lastSelectedTabBlomaga, 1000*60*60*24*30, '.nicovideo.jp', '/');
		return false;
	});
});

</script>


