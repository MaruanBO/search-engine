<script type="text/javascript">
	$('get_code').observe('click', respondToClick);
    function in_array(needle, haystack) {
        for(var i in haystack) {
            if(haystack[i] == needle) return true;
        }
        return false;
    }
	function detectCustomerStatus() {
		var uri = 'http://www.inkcartridges.com/ldpcheckout/cart/customerStatus/';
    	new Ajax.Request(uri, {
    		method: 'get',
            asynchronous: true,
    		onSuccess: function (transport) {
    			updateCustomerStatus(parseInt(transport.responseText));
    		},
            onComplete: function (transport) {
                detectMarketingParams();
            }
    	});
	}
	function updateCustomerStatus(status) {
		var login = '<a href="http://www.inkcartridges.com/customer/account/login/">Login</a>';
		var logoff = '<a href="http://www.inkcartridges.com/customer/account/logout/">Logout</a>';
		$('nav')
			.childElements()
			.each(function(e) {
				if (e.className != undefined && e.className == 'last') {
					if (status == 1) {
						e.innerHTML = logoff;
					} else {
						e.innerHTML = login;
					}
				}
			});
        if (typeof(PageInfo) !== 'undefined'){
            PageInfo.login = status;
        }
	}
	function respondToClick(event) {
		var element = Event.element(event);
		new Ajax.Request('http://www.inkcartridges.com/servicecode/?url=http%3A%2F%2Fwww.inkcartridges.com%2Fcatalogsearch%2Fresult%2F%3Fq%3D42416', {
			method:'get',
			onSuccess: function(transport){
				var response = transport.responseText || "error";
                if (!response.match(/html/i)) {
                    element.update("Service code: " + response);
                } else {
                    element.update("Error with generating service code!");
                }
			}
		});
	}

	function queryStringArgs()
	{
	    var qs = (location.search.length > 0 ? location.search.substring(1) : "");
	    var args = {};

	    var items = qs.split("&");
	    var item = null, name = null, value = null;

	    for (var i = 0; i < items.length; i++)
	    {
	        item = items[i].split("=");
	        if (item[0])
	        args[decodeURIComponent(item[0].toLowerCase())] = decodeURIComponent(item[1]);
	    }

	    return args;
	}

    function detectMarketingParams() {
        var currentUrl = window.location.href.split('?')[0];
        var getargs = queryStringArgs(); // from js.js
        var uri = "http://www.inkcartridges.com/ldpmarketing/ajax/params/?";
        var requestParams = [];
        var config = ["xid","aid","kid","sid","coupon","pid","cid"];
        for (var param in getargs) {
            param = param.toLowerCase();
            if (getargs[param] != 'undefined' && in_array(param, config)) {
                requestParams.push(encodeURIComponent(param) + "=" + encodeURIComponent(getargs[param]));
            }
        }
        if (requestParams.length > 0) {
            //requestParams.push('source=' + encodeURIComponent(currentUrl));
            new Ajax.Request(uri + requestParams.join('&'), {
                method: 'get',
                asynchronous: true,
                onSuccess: function (transport) {
                    // do something there
                },
                onComplete: function (transport){
                    retrieveProductData();
                }
            });
        }else{
            retrieveProductData();
        }
    }

    function retrieveProductData() {
        var _productIds = [];
        if (typeof currentProductId === 'undefined') {
            currentProductId = '';
        }
        if (typeof moduleController === 'undefined') {
            moduleController = '';
        }
        var allowedModuleController = ['catalog|category|view','catalog|product|view','catalogsearch|result|index'];
        var url = 'http://www.inkcartridges.com/ldpcatalog/ajax/retrieveProductData/';

        if (moduleController
            && in_array(moduleController, allowedModuleController)) {
            jQuery('.price').each(function (e) {
                if (this.id.match(/^product-price/)) {
                    _productIds.push(this.id.split('-')[2]);
                }
            });
        }
        if (_productIds.length > 0) {
            new Ajax.Request(url, {
                method: 'get',
                asynchronous: false,
                parameters: {'ids': _productIds.join(),'module': moduleController,'product_id': currentProductId},
                onSuccess: function (transport) {
                    var result = transport.responseText.evalJSON();
                    for (var i =0 ; i < _productIds.length; i++) {
                        productId = _productIds[i];
                        if (result[productId] != undefined) {
                            if (result[productId]['price_block'] != undefined && $('product-priceblock-' + productId) != undefined) {
                                $('product-priceblock-' + productId).innerHTML = result[productId]['price_block'];
                            }
                            // Ink & Toner regular price
                            if (result[productId]['price'] != undefined
                                && $('old-price-' + productId) != undefined) {
                                $('old-price-' + productId).innerHTML = '$' + result[productId]['price'];
                            }

                            // Ink & Toner sale price
                            if (result[productId]['special_price'] != undefined
                                && result[productId]['is_office_product'] == false
                                && $('product-price-' + productId) != undefined) {
                                $('product-price-' + productId).innerHTML = '$' + result[productId]['special_price'];
                            }
                            // Office product regular price
                            /* DON'T UPDATE THIS IN INKCARTRIDGES
                            if (result[productId]['price'] != undefined
                                && result[productId]['is_office_product'] == true
                                && $('product-price-' + productId) != undefined) {
                                $('product-price-' + productId).innerHTML = '$' + result[productId]['price'];
                            }
                            */

                            // Office product sale price
                            if (result[productId]['tier_price_short'] != undefined
                                && $('tierprice-short-' + productId) != undefined) {
                                $('tierprice-short-' + productId).innerHTML = result[productId]['tier_price_short'];
                            }
                            // Office product sale price
                            if (result[productId]['tier_price_full'] != undefined
                                && $('tierprice-full-' + productId) != undefined) {
                                $('tierprice-full-' + productId).innerHTML = result[productId]['tier_price_full'];
                            }

                            if (result[productId]['name'] != undefined
                                && $('product-name-' + productId) != undefined) {
                                $('product-name-' + productId).innerHTML = result[productId]['name'];
                            }

                            if (result[productId]['long_name'] != undefined
                                && $('product-longname-' + productId) != undefined) {
                                $('product-longname-' + productId).innerHTML = result[productId]['long_name'];
                            }

                            if (result[productId]['info_line'] != undefined
                                && $('product-infoline-' + productId) != undefined) {
                                $('product-infoline-' + productId).innerHTML = result[productId]['info_line'];
                            }

                            if (result[productId]['add_to_cart'] != undefined
                                && $('addtocart-' + productId) != undefined) {
                               $('addtocart-' + productId).innerHTML = result[productId]['add_to_cart'];
                            }

                            if (result[productId]['add_to_cart'] != undefined
                                && result[productId]['is_sellable'] == false
                                && $('product-status-' + productId) != undefined) {
                                $('product-status-' + productId).innerHTML = result[productId]['add_to_cart'];
                            }
                            if (result[productId]['is_sellable'] == true
                                && $('product-status-' + productId) != undefined
                                && $('product-status-' + productId).innerHTML.length > 0) {
                                $('product-status-' + productId).innerHTML = '';
                            }
                            if (result[productId]['in_stock'] != undefined
                                && $('instock-' + productId) != undefined) {
                                $('instock-' + productId).innerHTML = result[productId]['in_stock'];
                            }
                            if (result[productId]['is_sellable'] == false
                                && $('crosssell_addtocart_form'+productId) != undefined) {
                                try{
                                    jQuery('#crosssell_addtocart_form'+productId ).replaceWith( result[productId]['add_to_cart']);
                                    jQuery('div.add-to-cart > button.button').replaceWith( result[productId]['add_to_cart']);
                                } catch(e){
                                //catch and just suppress error
                                }
                            }
                        }
                    }
                }
            })
        }
    }

    function checkCartQty() {
        var doNotAllowedModuleController = ['checkout|cart|index','checkout|onepage|index', 'checkout|onepage|success'];
        if (typeof moduleController === 'undefined') {
            moduleController = '';
        }
        if ($('item_in_cart') && !in_array(moduleController, doNotAllowedModuleController)) {
            new Ajax.Request('http://www.inkcartridges.com/ldpcheckout/cart/qty/',
            {
                method: 'get',
                asynchronous: true,
                onSuccess: function(transport) {
                    var qty = parseInt(transport.responseText);
                    if (!isNaN(qty)) {
                        $('item_in_cart').innerHTML = qty;
                    }
                },
                onComplete: function(transport) {
                    detectCustomerStatus();
                }
            });
        }else{
            detectCustomerStatus();
        }
    }

