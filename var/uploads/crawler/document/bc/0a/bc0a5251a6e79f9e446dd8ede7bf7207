<script type="text/javascript">
//<![CDATA[
function reset_options() {
    document.getElementById('topCategories').selectedIndex = 0;
    return true;
}
window.onload=reset_options;

var catalogUrl = 'http://www.inkcartridges.com/ldpcatalog/ajax/view/';
var urlMappingTable = {};
Event.observe($('topCategories'), 'change', function() {
	if (this.value != '') {
		resetShoppingEasy($('subCategories'));
		resetShoppingEasy($('groupedProducts'));
		new Ajax.Request(catalogUrl + 'id/' + this.value, {
			method: 'get',
			asynchronous: false,
			onSuccess: function(transport) {
				var data = transport.responseText.evalJSON();
				if (data) {
					addDataToShippingEasy(data);
				}
			}
		})
	}
});

Event.observe($('findSubmit'), 'click', function(e) {
	Event.stop(e);
	var topCat = document.getElementById('topCategories');
	var subCat = document.getElementById('subCategories');
	var groupedP = document.getElementById('groupedProducts');
	if (!topCat.value) {
		topCat.style.color = "#f00";
	}
	if (!subCat.value) {
		subCat.style.color = "#f00";
	}
	if(groupedP.value) {
		window.location = groupedP.value
	} else {
		groupedP.style.color = "#f00";
		setTimeout(function() {
			topCat.style.color = "#000";
			subCat.style.color = "#000";
			groupedP.style.color = "#000";
		}, 1000);
	}
})

Event.observe($('subCategories'), 'change', function() {
	if (this.value != '') {
		if (urlMappingTable[this.value]) {

			var sub = this;
			var data = urlMappingTable[this.value];
			var prod = $('groupedProducts');
			resetShoppingEasy(prod);
			for (var i in data) {
				if (data[i]) {
					var opt = document.createElement('option');
					opt.value = data[i].url;
					opt.text = data[i].name;
					try {
						prod.add(opt, null);
					} catch (ex) {
						prod.add(opt);
					}
				}
			}
		}
	}
	sortOptions('groupedProducts');
});

Event.observe($('groupedProducts'), 'change', function() {
	if (this.value != '') {
		document.location = "http://www.inkcartridges.com/" + this.value;
	}
});

function resetShoppingEasy(el) {
	if (el != undefined) {
		for (i = el.options.length - 1; i > 0; i--) {
			if (i > 0) {
				el.remove(i);
			}
		}
	}
}

function addDataToShippingEasy(data)
{
	urlMappingTable = {};
	var sub = $('subCategories');
	sub.disabled = true;
	if (data) {
		for (var i in data) {
			var opt = document.createElement('option');
			opt.value = i;
			opt.text = data[i].name;
			if (data[i].sub) {
                try {
                    sub.add(opt, null);
                } catch (ex) {
                    sub.add(opt);
                }
				var map = data[i].sub;
				urlMappingTable[i] = {};
				for (var j in map) {
					if (map[j].id)
						urlMappingTable[i][j] = map[j];
				}

			}
		}
	}
	sub.disabled = false;
	sortOptions('subCategories');
    //sortSubcats();
}
// sortOptions defined on incart-functions.js

//]]>
</script>

