// cms::index::index// http

var ld_pixels = [], ld_noscripts = [];

(function (w) {
    var old = w.onload;
    w.onload = function () {
        if (typeof old === 'function') {
            old();
        }
        setTimeout(loadImagePixels, 50);
        function loadImagePixels() {
            var body = document.body;
            try {
                var pixelHTML = '', noscriptHTML = '';
                for (var i = 0, len = ld_pixels.length; i < len ; i++) {
                    pixelHTML += ld_pixels.join(' ');
                }

                for (var i = 0, len = ld_noscripts.length; i < len ; i++) {
                    noscriptHTML += ld_noscripts.join(' ');
                    //pixelHTML += '<noscript>' + noscriptHTML + '</noscript>';
                }

                // Append no-script pixel to body
                if (noscriptHTML) {
                    var noScriptPixelContainer = document.createElement('noscript');
                    noScriptPixelContainer.innerHTML = noscriptHTML;
                    body.appendChild(noScriptPixelContainer);
                }

                // Append image pixel to body
                if (pixelHTML) {
                    var pixelContainer = document.createElement('div');
                    pixelContainer.innerHTML = pixelHTML;
                    for (var i = 0; i < pixelContainer.childNodes.length; i++) {
                        body.appendChild(pixelContainer.childNodes[i]);
                    }
                }
            } catch (e) {

            }
        }
    }
}(window));

function loadInteractivePixels() {
    try {
        $LAB
        .script("//upfront.thefind.com/scripts/main/utils-init-ajaxlib/upfront-badgeinit.js")
.wait(function() { thefind.upfront.init('tf_upfront_badge', '31db606d73f0d7d460a87fc98b287a39');});
    } catch (e) {
    }
}
document.observe("dom:loaded", loadInteractivePixels);

</script>
