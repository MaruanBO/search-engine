    <script type="text/javascript">

        $(document).ready(function () {
            LoadBroadcast();
            LoadContentRotator();
        });

        function LoadBroadcast() {
            $.ajax({
                url: "/ajax/HomePage/Broadcast.ashx",
                type: "GET",
                complete: function (req) {
                    if (req.status == 200) { //success

                        var responses = req.responseText.split("|");

                        if (responses.length > 0) {
                            $('#homePageBroadcastTitle').text(responses[0]);
                            $('#homePageBroadcastDescription').html(responses[1]);
                            $('.homePageBroadcastLink').attr('href', responses[2]);
                        }
                    } 
//                    else {
//                        alert(req.responseText);
//                    }
                }
            });
        }
        
        function LoadContentRotator() {
            $.ajax({
                url: "/ajax/HomePage/ContentRotator.ashx",
                type: "GET",
                complete: function (req) {
                    if (req.status == 200) { //success

                        var response = req.responseText;

                        $('#adContainer').html(response);

                        window.api = $("div.scrollable").scrollable({ size: 1, clickable: false, circular: true }).navigator().autoscroll({ steps: 1, interval: 7000, autoplay: true, autopause: true, api: true });
                        $("#scrollableContainer").mouseenter(function () { api.pause(); });
                        $("#itemsContainer").children().mouseenter(function () { api.pause(); });
                        $("#navigationWrapper").click(function () { api.stop(); });

                    }
//                    else {
//                        alert(req.responseText);
//                    }
                }
            });
        }
    
    </script>


    

<script type="text/javascript">
//<![CDATA[
window.IsUserAuthenticated = false;Sys.Application.initialize();
//]]>
</script>
