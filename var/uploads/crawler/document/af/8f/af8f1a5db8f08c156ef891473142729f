	CSWxManager.ready(function(wx) {  
		if (!wx.hasData()) { return; }
		
		var i = -1;
		function rotate() {
			++i;
			if (i >= cityNames.length) {
				i = 0;
			}
		
			$wn('#csCol4Wx #csWx1 .wxIcon').css('background-image', wx.fetch('iconcss'));
			$wn('#csCol4Wx #csWx1 .wxTemp').html(wx.fetch('temperature') + '&deg;');
			$wn('#csCol4Wx #csWx1 .wxCity').html(cityNames[i]);
			$wn('#csCol4Wx #csWx1 .wxFeels').html(wx.fetch('feelslike') + '&deg;');
			
