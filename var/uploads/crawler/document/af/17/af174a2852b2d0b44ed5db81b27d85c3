    </div>
    <script>
        (function($) {

            var wrapper = $("#hero-video");

            $("#hero-video a.fancybox").fancybox({
            
            }); 

            var overlay = $("#-overlay");
            var container = $("#");

            $(".atvi-age-gate-overlay.display").live('click', function(){
                 $(".atvi-age-gate-overlay.display").removeClass("display");
                 $(".atvi-age-gate-light-container.display").removeClass("display");
            });

            $("#hero-video .iframe").click(function() {

                //Create properties for age gate
                var ageGateConfig = {
                        ageGateWrapper: wrapper.find(".atvi-age-gate-light-container"),   
                        disableAgeGate: false,
                        minimumAge : 18.0
                };

                var linkEl = this;

                var showLightbox = function() {

                    $.fancybox({
                        'type' : 'iframe',
                        // hide the related video suggestions and autoplay the video
                        'href' : linkEl.href.replace(new RegExp('watch\\?v=', 'i'), 'embed/') + '?rel=0&wmode=transparent&autoplay=1#t=' + 0,
                        'overlayShow' : true,
                        'centerOnScroll' : true,
                        'speedIn' : 100,
                        'speedOut' : 50,
                        'width' : 800,
                        'height' : 600,
                        

                    });
                };

                //If agegate isn't disabled
                if(!ageGateConfig.disableAgeGate) {

                    // show agegate div
                    overlay.addClass('display');
                    container.addClass('display');

                    /*container.css("top", Math.max(0, (($(window).height() - container.outerHeight()) / 2) + 
                                                $(window).scrollTop()) + "px");*/
                    container.css("left", Math.max(0, (($(window).width() - container.outerWidth()) / 2) + 
                                                $(window).scrollLeft()) + "px");

                    wrapper.find("#");
                    ATVI.components.agegate.init(function() {

                        // hide agegate div
                        overlay.removeClass('display');
                        container.removeClass('display');

                        showLightbox();
                    }, ageGateConfig);

                // agegate is disabled, just show video
                } else {
                    showLightbox();
                }

                return false;
            });

        })(jQuery);
    </script></div>
</div>

