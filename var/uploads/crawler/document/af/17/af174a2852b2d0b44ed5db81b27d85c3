                        <h2>Enter Your <span>Date of Birth</span></h2>
                        <fieldset id="dob-fields" class="clearfix">
                            <input type="text" id="age-gate-month" name="month" size="2" maxlength="2" required placeholder="MM">
                            <input type="text" id="age-gate-date" name="date" size="2" maxlength="2" required placeholder="DD">
                            <input type="text" id="age-gate-year" name="year" size="4" maxlength="4" required placeholder="YYYY">
