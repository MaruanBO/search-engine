<script type="text/javascript">
$(document).ready(function(){
$.ajax({
                type: "GET",
                url: "/ajax/ppv.ashx",
                data: { "_action": "getPPVByCatalog" },
                cache: false,
                dataType: "json",
                success: function (data) 
                {
                     var take5 = 0
                     var stringBuilder = "";
                     for(var x = 0; x < data.length; x++)
                    {
                           if(data[x].category_id == 1)
                           {
                                take5 ++;
                                stringBuilder += "<div><img alt=\"\" src=\"https://my.dish.com/customer/images/payperview/dynamicload/"+data[x].image_name+"\" /><br />"+((data[x].title.length>16)?(data[x].title.substring(0, 16)+"..."):(data[x].title))+"<br /><br /><a class=\"button red\" href=\"https://my.dish.com/customercare/payperview/getPPVMovieList.do?&amp;hdSelected=Y&amp;progType=MOVIE&amp;csgEventId="+data[x].eid+"\">Order Now</a></div>";
                           }
                           if(take5 == 5)
                           {
                                break;
                           }
                    }
                    $("#featured").append(stringBuilder);
                }
           });
});
