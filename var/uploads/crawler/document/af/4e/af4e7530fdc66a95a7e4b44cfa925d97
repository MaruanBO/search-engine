        //var fields_to_be_highlighted = "null";
        //if (fields_to_be_highlighted != null) {
        //    highlightErrorFields(fields_to_be_highlighted);
        //}
        $(document).ready(function () {
            //addclosebutton();
            //monitorSessionTimeOut();
            //showPhoneNumber();
            //addDefaultButton();


        });
        /* ---Now I have two generic Approach for toggleClass. we can use anyone */
        if (document.getElementById('toggleleft') !== null) {
            document.getElementById('toggleleft').onclick = function toggleLeft() {
                $("#rowoffcanvas").removeClass("row-offcanvas-right");
                $("#rowoffcanvas").addClass("row-offcanvas-left");
                window.setTimeout($(".row-offcanvas-left").toggleClass("active"), 10);
                $("#rightmenu").hide();
                $("#leftmenu").show();
                showSubNav('my-account');
                window.setTimeout(backToFixedLeft, 300);
                $('.mainmenu').each(function () {
                    $(this).removeClass('activeMainMenu');
                });
                $('.mainmenuDefault').each(function () {
                    $(this).addClass('activeMainMenu');
                });
            }
        }
        if (document.getElementById('toggleright') !== null) {
            document.getElementById('toggleright').onclick = function toggleRight() {
                $("#rowoffcanvas").removeClass("row-offcanvas-left");
                $("#rowoffcanvas").addClass("row-offcanvas-right");
                $("#rightmenu").show();
                $("#leftmenu").hide();
                window.setTimeout($(".row-offcanvas-right").toggleClass("active"), 10);
                window.setTimeout(backToFixedRight, 300);
            }
        }

        // #Second Approach toggleClass in jquery does not support Mobile so I have change it to javascript in order to support both :) 
        function toggleClass(elem, className) {
            var newClass = ' ' + elem.className.replace(/[\t\r\n]/g, " ") + ' ';
            if (hasClass(elem, className)) {
                while (newClass.indexOf(" " + className + " ") >= 0) {
                    newClass = newClass.replace(" " + className + " ", " ");
                }
                elem.className = newClass.replace(/^\s+|\s+$/g, '');
            } else {
                elem.className += ' ' + className;
            }
        }

        function backToFixedLeft() {
            if ($(".row-offcanvas-left").hasClass("active")) {
                backToFixed(true);
            } else {
                backToFixed(false);
            }
        }

        function backToFixedRight() {
            if ($(".row-offcanvas-right").hasClass("active")) {
                backToFixed(true);
            } else {
                backToFixed(false);
            }
        }

        function backToFixed(isMenuActive) {
            if (isMenuActive) {
                $("#rowoffcanvas").addClass("row-offcanvas-static");
                $("#rowoffcanvas").removeClass("row-offcanvas-fixed");
                $("#navbar-default").addClass("navbar-static-top");
                $("#navbar-default").removeClass("navbar-fixed-top");
            } else {
                $("#rowoffcanvas").removeClass("row-offcanvas-static");
                $("#rowoffcanvas").addClass("row-offcanvas-fixed");
                $("#navbar-default").removeClass("navbar-static-top");
                $("#navbar-default").addClass("navbar-fixed-top");
            }
        }
        var displayOverlayCounter = 0;
        if (displayOverlays.length > 0) {
            showNextOverlay();
        } else {
            $(".overlayDataParsing").each(function () {
                $(this).hide();
            });
            $(".overlayBlurDiv").each(function () {
                $(this).hide();
            });
        }
        backToFixedRight(); backToFixedRight();
</script>
    

    
    
    


    
    
    
    
    
    <script type="text/javascript">

        if (top != self) {
            top.location = self.location;
        }


        $(document).on("click", "a[href^=http\\:\\/\\/www\\.mydish\\.com]", function () {
            this.href= this.href.replace('http://www.mydish.com/', '\/');
        });

        function ajaxRatePage() {
            var url = window.location.pathname;
            var rtg = $("#ratingContent input[type='radio']:checked").val();
            var ttl = document.title;
            //alert("titl: " + ttl);
            if (rtg != null) {
                $.ajax({
                    type: "POST",
                    url: "/Ajax/RatePage.ashx",
                    data: ({ ID: url, Rating: rtg, Title: ttl, Action: "RatePage" }),
                    cache: false
                });
                $("#ratingContent p, #ratingContent .red_button, #RadioButtonList").toggle();
            }
            else {
                alert("Please select a rating before clicking Submit.");
            }
        }
        function ajaxFeedback() {
            var url = window.location.pathname;
            var ttl = document.title;
            var sub = "Support Feedback";
            var msg = $("#ctl00_ctl00_TextBoxMessage").val();
            if (sub != "" && msg != "") {
                //alert("url: " + url + " sub: " + sub + " msg: " + msg);
                $.ajax({
                    type: "POST",
                    url: "/Ajax/RatePage.ashx",
                    data: ({ ID: url, Subject: sub, Message: msg, Title: ttl, Action: "Feedback" }),
                    cache: false
                });
                $("#feedbackdialog p").text("Thanks for your feedback!");
                $("#popupContentArea .row, #popupContentAreaMessage").toggle();
            }
            else {
                alert("Please fill out all fields before clicking Submit.");
            }
        }
        var tfn = '';
        var kbid = '';
        $(document).ready(function () {
            tfn = '';
            kbid = '';
            if(tfn)
            {
                var newtel = "tel:+" + tfn;
                $('a[href^="tel"]').not('.statictfn').attr('href', newtel).text(tfn);
            }
        });
        
        function getQueryVariable(variable) {
            var query = window.location.search.substring(1);
            var vars = query.split("&");
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == variable) { return pair[1]; }
            }
            return (false);
        }

    </script>


