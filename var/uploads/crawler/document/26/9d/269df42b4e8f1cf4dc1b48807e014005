		<script type="text/javascript">

			$(document).ready(function () {

				// Gets Domain
			    var s = window.location.hostname.split('.');
			    var domain = s[s.length-2]+'.'+s[s.length-1];

			    // Modifies External Links To Open In A New Window, Excluding DoubleClick
			    $('a').filter(function() {
			        if (this.hostname && this.hostname != 'ad.doubleclick.net') {
			            var t = this.hostname.split('.');
			            var test = t[t.length-2]+'.'+t[t.length-1];
			            if (test != domain)
			                return true;
			            else
			                return false;
			        }
			    }).attr('target', '_blank');

			    // Call Twitter Bootstrap v3.0 Dropdown in Footer
			    // http://getbootstrap.com/
				$('.network-drop .dropdown-toggle').click(function() {
					$(this).dropdown('toggle');
				});
				$('.network-drop .dropdown-menu').focusout(function() {
					$('.network-drop .dropup').removeClass('open');
				});

				$('#PageContainer').click(function() {
					$('.network-drop .dropup').removeClass('open');
				});

			    // Lazy load images via Unveil jQuery plugin
			    // http://luis-almeida.github.io/unveil/
			    $('img.unveil').unveil(0, function() {
				  $(this).load(function() {
				    this.style.opacity = 1;
				  });
				});

			});

			// http://stackoverflow.com/questions/12393667/undefined-appearing-lots-of-time-in-my-404-error-log
			window.suggestmeyes_loaded = true;

		</script>

