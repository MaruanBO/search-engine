	if($(this).parent().parent().find(".span").length){
		$(this).toggleClass("active").parent().parent(".proSelcet").toggleClass("hideNav");
		if(!$(this).hasClass("active")){window.location.href = url;}
		}else{window.location.href = url;}
		return false;
	})
	$(".proSelcet .psbtn em").click(function(){
		$(this)	.parent().parent(".proSelcet").toggleClass("hideNav");
	})
}else{
	$(".proSelcet").hover(function(){
		$(this).toggleClass("hideNav");
	})		
}
</script>


