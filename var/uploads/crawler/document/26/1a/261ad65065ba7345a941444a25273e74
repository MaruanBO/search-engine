<script type="text/javascript">
function getQueryString(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
    var r = window.location.search.substr(1).match(reg);
    if (r != null) return unescape(r[2]); return null;
}
function Jump(utm_source,src){
	if(utm_source || src){
		if(utm_source && utm_source=='nl'){
			return true;
		}
		if(src){
		 	return true;
		}
	}
	return false;
}

var url_param = window.location.search;

if (url_param.indexOf("?") != -1) {//if there are arguments
	var utm_source = getQueryString('utm_source');
	var src = getQueryString('src'); 
	var is_jump = Jump(utm_source,src);
	if(!is_jump){
		window.location.href = 'http://www.wondershare.com/';
	}
}
</script>

