<script type="text/javascript">
function inputFocus(field, defaultValue, cssName) {
    if (field.id != null) {
        field = field.id;
    }
    if (document.getElementById(field).value == defaultValue) {
        document.getElementById(field).value = "";
        document.getElementById(field).className = cssName;
    }
}

function inputBlur(field, defaultValue, cssName) {
    if (field.id != null) {
        field = field.id;
    }
    if (document.getElementById(field).value == "") {
        document.getElementById(field).value = defaultValue;
    }
    if (document.getElementById(field).value == defaultValue) {
        document.getElementById(field).className = cssName;
    }
}
</script>

