<script language="javascript" type="text/javascript">

var decodedAlid;
var uid;
                
    $j('#Profilesign').click(function(){
        $j('#dialogHolder').toggle();
        CallProfileResponse(decodedAlid, uid);
    });

    function DisplayWelcomeMessage(searchControlID)
    {
        var uCookie = GetCookie("secuser");
        
        if (uCookie != null || uCookie != '' || uCookie != undefined){
            var uarr = '';
            if (uCookie.indexOf("&") != -1) {
                uarr = uCookie.split("&");
            }
            
            if (uarr != null || uarr != undefined){
               if (uarr.length > 0){
                   uid = uarr[0].split("=")[1];
               }
            }
        }
        
        var regCookie = GetCookie("reg");
        if (regCookie != null || regCookie != '' || regCookie != undefined)
        {
            var arr = '';
            if (regCookie.indexOf("&") != -1) {
                arr = regCookie.split("&");
            }
            
            if (arr != null || arr != undefined)
            {
               if (arr.length > 0)
               {
                    var alid = arr[2].split("=")[1];
                    // TT: #00101
                    if ($j.trim(alid).length != 0){
                        decodedAlid = decode64(alid);                        
                    }
                    else{
                        decodedAlid = "-1";
                    }
                    
                    var firstname = arr[0].split("=")[1];
                    var lastname = arr[1].split("=")[1];
                    if (firstname != '' && $j.trim(firstname).length != 0 && lastname != '' && $j.trim(lastname).length != 0){
                        var decodedFName = decode64(arr[0].split("=")[1]);
                        var decodedLName = decode64(arr[1].split("=")[1]);
                        ConstructDialogHeader(decodedFName, decodedLName); 
                    }
               }
            }
        }
    }
    
  function CallProfileResponse(alid, uid){
     var currentProtocolVal = location.protocol;
     var currentPage = currentProtocolVal + "//" + window.location.hostname + "/layouts/Controls/RegistrationPopup/SignOutDialogHandlerV1.ashx?alid=" + escape(alid) + "&uid=" + escape(uid) + "&rsd=" + Date();
     $j.ajax({
            url: currentPage,
            //data: {'alid':alid, 'uid':uid},
            async: false,
            cache: true,
            success: function(data) {
                 $j('#dialogHolder').html(data);
            }
        });
  }
    
    function ConstructDialogHeader(firstName, lastName){
       $j('#firstNameSpan').html(firstName); 
       $j('#lastNameSpan').html(lastName); 
       $j('#signedintoolbar').css('display', 'block'); 
    }
    
    // function to close the signout dialog when clicking out of the welcome div
    $j(document).click(function(e){
        var myTarget = $j("#ProfileNameDiv");
        var clicked = e.target.className;
        
        if (e.target.id == 'firstNameSpan' || e.target.id == 'lastNameSpan' || e.target.id == 'signoutImg'){
            myTarget = e.target.parentNode;
            clicked = myTarget.className;
        }
        
        if (e.target.id.indexOf('imgPhoto') > 0){
            myTarget = e.target.parentNode.parentNode.parentNode.parentNode.parentNode;
            clicked =  myTarget.className;
        }
        
//        if($j.trim(myTarget) != '') {
            if (myTarget != ''){
                if(myTarget.attr('class') != clicked) {
                    $j("#dialogHolder").slideUp('slow').hide();
                }
            }
//        }
          
    });
    
    function GetCookie(c_name) {
    if (document.cookie.length > 0) {
        c_start = document.cookie.indexOf(c_name + "=");
        if (c_start != -1) {
            c_start = c_start + c_name.length + 1;
            c_end = document.cookie.indexOf(";", c_start);
            if (c_end == -1) c_end = document.cookie.length
            return unescape(document.cookie.substring(c_start, c_end));
        }
    }
    return ""
}


function mylogout()
{
    var currentProtocolVal = location.protocol;
    var hostNameValue = window.location.host;
    var logoutURL = currentProtocolVal + "//" + hostNameValue + "/layouts/controls/registrationpopup/signout.aspx";
    
    $j.post(logoutURL, null, function(res, sta, xhr){            
            if (location.protocol.indexOf('https') != -1)
            {
                currentProtocolVal = 'http:';
            }
            
            var currentHost = currentProtocolVal + "//" + window.location.hostname;
            var currentPage = currentHost + location.pathname;
            window.location.href = currentPage.split("?")[0];
    });
}
        

function decode64(input) {
var keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
var output = "";
	     var chr1, chr2, chr3 = "";
	     var enc1, enc2, enc3, enc4 = "";
	     var i = 0;
	 
	     // remove all characters that are not A-Z, a-z, 0-9, +, /, or =
	     var base64test = /[^A-Za-z0-9\+\/\=]/g;
	     if (base64test.exec(input)) {
	        alert("There were invalid base64 characters in the input text.\n" +
	              "Valid base64 characters are A-Z, a-z, 0-9, '+', '/',and '='\n" +
	              "Expect errors in decoding.");
	     }
	     input = input.replace(/[^A-Za-z0-9\+\/\=]/g, "");
	 
	     do {
	        enc1 = keyStr.indexOf(input.charAt(i++));
	        enc2 = keyStr.indexOf(input.charAt(i++));
	        enc3 = keyStr.indexOf(input.charAt(i++));
	        enc4 = keyStr.indexOf(input.charAt(i++));
	 
	        chr1 = (enc1 << 2) | (enc2 >> 4);
	        chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
	        chr3 = ((enc3 & 3) << 6) | enc4;
	 
	        output = output + String.fromCharCode(chr1);
	 
	        if (enc3 != 64) {
	           output = output + String.fromCharCode(chr2);
	        }
	        if (enc4 != 64) {
	           output = output + String.fromCharCode(chr3);
	        }
	 
	        chr1 = chr2 = chr3 = "";
	        enc1 = enc2 = enc3 = enc4 = "";
	 
	     } while (i < input.length);
	 
	     return unescape(output);
	  }
</script>

