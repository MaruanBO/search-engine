 <script type="text/javascript">
     $(document).ready(function() {
         $(window).load(function() {
             $('ul.ui-autocomplete:last').addClass('secondul');
             //Code changes added for 9561

             var geoLocCookie = getCookie("geoloc");
             if (geoLocCookie == null) {
                 geoLocCookie = getCookie("geoloccode1"); // change to correct valusgeoloccode1
                 parseGeoLocCodeCookie(geoLocCookie);
             }
             else {
                 parseGeoLocCookie(geoLocCookie);
             }
         });
     });
     function parseGeoLocCookie(locValue) {        
     //ip_address=198.185.18.207&city=Miamisburg&state=Ohio&state_code=OH&country=United States&country_code=US&latitude=39.6374&longitude=-84.27182&area_code=937&postal_code=45342&time_zone=-5
         if (locValue.indexOf("&") == -1) {
             arrCookieValues = locValue.split(",");
         }
         else {
             arrCookieValues = locValue.split("&");
         }

         if (arrCookieValues.length == 1) {
             arrCookieValues = locValue.split(',');
         }
        
         var cityDetails = arrCookieValues[1].split('=');
         var stateDetails = arrCookieValues[2].split('=');
         var stateCodeDetails = arrCookieValues[3].split('=');
         var countryDetails = arrCookieValues[4].split('=');
         displayDefaultValue(countryDetails[1], stateCodeDetails[1], stateDetails[1], cityDetails[1]);
         
     }
     function parseGeoLocCodeCookie(locValue) {
         //US5&country_code=US&city=MIAMISBURG&state=OH&state_Code=OH&country=US&country_code=US&latitude=39.6429&longitude=-84.2867&county=MONTGOMERY&fips=39113&timezone=EST&network=None&network_type=None&area_code=937
         var arrCookieValues = locValue.split('&');
         var cityDetails = arrCookieValues[2].split('=');
         var stateDetails = arrCookieValues[3].split('=');
         var stateCodeDetails = arrCookieValues[4].split('=');
         var countryDetails = arrCookieValues[5].split('=');
         displayDefaultValue(countryDetails[1], stateCodeDetails[1], stateDetails[1], cityDetails[1]);
     }

     function displayDefaultValue(country, statecode, state, city) {
         
         var displayFormat = '';
         City = city;
         displayFormat = city;
         Tier = '';
         Country = '';
         State = '';
         ZipCode = '';
         defaultLocationValue = '';              
         if (country.toLowerCase() == "us")
             country = "U.S.A.";
         if (country.toLowerCase() == "united states" || country.toLowerCase() == "canada" || country.toLowerCase() == "u.s.a." || country.toLowerCase() == "u.s.a.") {
             Country = country;
             State = state;
             if (statecode == '') {
                 for (i = 0; i < us.length; i++) {
                     if (us[i][1].toLowerCase() == state.toLowerCase()) {
                         statecode = us[i][0];
                         break;
                     }
                 }
             }
             if (displayFormat.length > 0 && statecode.length > 0) {
                 displayFormat = displayFormat + ", " + statecode;
             }
             else {
             if(city.length == 0 && state.length>0)
                 displayFormat = state + ", " + country;             
             }
             Tier = '1';
             defaultLocationValue = 'true';
             document.getElementById('location').value = displayFormat;
         }
         else {
             Country = country;
             if (displayFormat.length > 0 && country.length > 0) {
                 displayFormat = displayFormat + ", " + country;
             }             
             Tier = '1';
             defaultLocationValue = 'true';
             document.getElementById('location').value = displayFormat;
         }
     }
     var us = [["AL", "Alabama"], ["AK", "Alaska"], ["AZ", "Arizona"], ["AR", "Arkansas"], ["CA", "California"], ["CO", "Colorado"], ["CT", "Connecticut"], ["DE", "Delaware"], ["DC", "District of Columbia"], ["FL", "Florida"], ["GA", "Georgia"], ["GU", "Guam"], ["HI", "Hawaii"], ["ID", "Idaho"], ["IL", "Illinois"], ["IN", "Indiana"], ["IA", "Iowa"], ["KS", "Kansas"], ["KY", "Kentucky"], ["LA", "Louisiana"], ["ME", "Maine"], ["MD", "Maryland"], ["MA", "Massachusetts"], ["MI", "Michigan"], ["MN", "Minnesota"], ["MS", "Mississippi"], ["MO", "Missouri"], ["MT", "Montana"], ["NE", "Nebraska"], ["NV", "Nevada"], ["NH", "New Hampshire"], ["NJ", "New Jersey"], ["NM", "New Mexico"], ["NY", "New York"], ["NC", "North Carolina"], ["ND", "North Dakota"], ["OH", "Ohio"], ["OK", "Oklahoma"], ["OR", "Oregon"], ["PA", "Pennsylvania"], ["RI", "Rhode Island"], ["SC", "South Carolina"], ["SD", "South Dakota"], ["TN", "Tennessee"], ["TX", "Texas"], ["UT", "Utah"], ["VT", "Vermont"], ["VA", "Virginia"], ["WA", "Washington"], ["WV", "West Virginia"], ["WI", "Wisconsin"], ["WY", "Wyoming"]];
</script>
