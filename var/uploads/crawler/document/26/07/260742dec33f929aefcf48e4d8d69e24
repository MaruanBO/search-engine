<script type="text/javascript">
    $(document).ready(function(){
        var $subscribeModalInput = $('.subscribe-modal input[type="text"]');
        showModal = function() {
            $('.subscribe-screenlock, .subscribe-modal').fadeIn();
        },
            hideModal = function() {
                $('.subscribe-screenlock, .subscribe-modal').fadeOut();
            };
        $('.subscribe-link').bind('click', function(e){
            $linkHref = $(this).attr('href');
            e.preventDefault();
            showModal();
        });
        $('.subscribe-screenlock').bind('click', function(){
            hideModal();
        });
        $('.subscribe-modal form').on('submit', function(e){
            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/,
                $subscribeInputVal = $subscribeModalInput.val();
            if(!emailReg.test($subscribeInputVal)) {
                e.preventDefault();
                $('.subscribe-modal-error').html('Type correct email');
            } else {
                $('.subscribe-modal-error').html('');
                window.open($linkHref);
            }
        });
    });
</script>
