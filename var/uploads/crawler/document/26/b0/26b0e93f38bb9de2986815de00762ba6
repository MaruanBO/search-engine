<script type="text/javascript">
    var intCurrentIndex = 1;
    var intPanelCount = 6;
    var intTimerInterval = 10000;
    var intTimer = window.setInterval("moveNextNews();", intTimerInterval);

    function moveNextNews() {
        var intPrevIndex;
        intPrevIndex = intCurrentIndex;

        if (intCurrentIndex == intPanelCount) {
            intCurrentIndex = 1;
        } else {
            intCurrentIndex = intCurrentIndex + 1;
        }

        showNewsPanel(intPrevIndex, intCurrentIndex);
    }

    function showNewsPanel(intPrevIndex, intNewIndex) {
         $j("#landingNewsAlertORD" + intPrevIndex).hide("slide", { direction: "right" }, 1000, function() {
            $j("#landingNewsAlertORD" + intNewIndex).show("slide", { direction: "left" }, 1000);
         });
         
         $j("#landingNewsAlertMDW" + intPrevIndex).hide("slide", { direction: "right" }, 1000, function() {
            $j("#landingNewsAlertMDW" + intNewIndex).show("slide", { direction: "left" }, 1000);
         });
    }

    window.onunload = function () { }; 
    $j.ajaxSetup({
      cache: false
    });

    sfHover = function() {
	var sfEls = document.getElementById("chooseAirport").getElementsByTagName("LI");
	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmouseover=function() {
			this.className+=" over";
		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" over\\b"), "");
		}
	}
    }


    $j(document).ready(function () {

        if (window.attachEvent) window.attachEvent("onload", sfHover);


        $j(".siteSectionLink").click(function () {
            var $link = $j(this);

	         var $dialog = $j('<div></div>')
			.load('/controls/LandingPage/LandingModuleModal.aspx?url=' + $link.attr('href') + '&siteSection=' + $link.attr('name'))
			.dialog({
			    autoOpen: false,
			    modal: true,
			    draggable: false,
			    resizable: false,
			    title: $link.attr('title'),
			    width: 440,
			    zIndex: 1000000,
			    open: function () {
			        jQuery('.ui-widget-overlay').bind('click', function () {
			            jQuery($dialog).dialog('close');
			        })
			    },
                close: function (event, ui) {                        
                        $dialog.empty();
                }
			});

            $dialog.dialog('open');
            return false;
        });
    });
    
    function SearchOhareFlight() {
		var strFlightNumber;
		strFlightNumber = $j('input[id$=txtORDFlightNumber]').val();
		strFlightNumber = encodeURIComponent(strFlightNumber);

		var relativeURL;
		relativeURL = "/OHare/EN/MyFlight/default.aspx?FlightNumber=" + strFlightNumber ;

		document.location.href = relativeURL;

        //		var $dialog = $j('<div></div>')
