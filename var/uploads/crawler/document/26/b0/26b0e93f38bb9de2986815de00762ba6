            $j(document).ready(function () {
                var box1 = $j("#clip1");
                var box2 = $j("#clip2");
                clipTween1 = TweenLite.from(box1, 1, { clip: "rect(0px 400px 400px 400px)", paused: true });
                clipTween2 = TweenLite.from(box2, 1, { clip: "rect(0px 400px 400px 400px)", paused: true });
                box1.parent().mouseenter(function () {
                    clipTween1.play();
                });
                box1.parent().mouseleave(function () {
                    clipTween1.reverse();
                });
                box2.parent().mouseenter(function () {
                    clipTween2.play();
                });
                box2.parent().mouseleave(function () {
                    clipTween2.reverse();
                });
