    function HandleFlightSearchORDBoxKeyPress(event) {
        if (event.keyCode == 13) {
            searchOnKeyPress = 'Flight';
            SearchOhareFlight();
            return false;
        }
        else
            return true;
    }

    function HandleFlightSearchMDWBoxKeyPress(event) {
        if (event.keyCode == 13) {
            searchOnKeyPress = 'Flight';
            SearchMidwayFlight();
            return false;
        }
        else
            return true;
    }
    
