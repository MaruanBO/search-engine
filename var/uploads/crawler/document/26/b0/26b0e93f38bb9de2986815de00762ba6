<script type="text/javascript">

detectmob();
function detectmob() { 
 if (document.location.href.indexOf("business") > 0)
    return;
 if( navigator.userAgent.match(/Android/i)
 || navigator.userAgent.match(/webOS/i)
 || navigator.userAgent.match(/iPhone/i)
 || navigator.userAgent.match(/iPad/i)
 || navigator.userAgent.match(/iPod/i)
 || navigator.userAgent.match(/BlackBerry/i)
 || navigator.userAgent.match(/Windows Phone/i)
 || navigator.userAgent.match(/Touch/i)
 ){
    if(document.referrer){
    	if(document.location.href.indexOf("landingpage.aspx")>-1)
    		return window.location.replace("http://www.flychicago.com/m");
    		
    	var temp = document.referrer;
        if (temp.slice(0, 25) != "http://www.flychicago.com")
        {
        	checkurl()
        	/*if(document.URL.toLowerCase().indexOf('midway')>-1)
        		return window.location.replace("http://www.flychicago.com/m/midway/en/default.aspx");
			if(document.URL.toLowerCase().indexOf('ohare')>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/default.aspx");

			return window.location.replace("http://www.flychicago.com/m");*/
		}
		else
		{
			//full site
		}
    }
    else{
		checkurl()
 	}
  }
 else {
 	console.log(document.referrer);
    return false;
  }
}
function checkurl(){
			var tempUrl=location.href;
			
			tempUrl = tempUrl.toLowerCase();
			tempUrl = tempUrl.replace('pages/', '');
			
		if(tempUrl.indexOf("ohare")>-1){
			if(tempUrl.toLowerCase().indexOf("parking/international")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/parking/default.aspx#international");
			else if(tempUrl.indexOf("parking/main")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/parking/default.aspx#main");
			else if(tempUrl.indexOf("parking/economy")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/parking/default.aspx#economy");
			else if(tempUrl.indexOf("parking/valet")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/parking/default.aspx#valet");
			else if(tempUrl.indexOf("parking/")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/parking/default.aspx");
			else if(tempUrl.indexOf("curb-to-gate")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/gettingtofrom/directions/default.aspx");
			else if(tempUrl.indexOf("myflight/airline/")>-1)	
				return window.location.replace("http://www.flychicago.com/m/ohare/en/myflight/default.aspx#airlines");
			else if(tempUrl.indexOf("myflight/")>-1)	
				return window.location.replace("http://www.flychicago.com/m/ohare/en/myflight/default.aspx");
			else if(tempUrl.indexOf("connect")>-1)
				return window.location.replace("http://www.flychicago.com/m/ohare/en/contact/default.aspx");
			//default
			return window.location.replace("http://www.flychicago.com/m/ohare/en/home/default.aspx");
			
		}
		if(tempUrl.indexOf("midway")>-1){
			if(tempUrl.toLowerCase().indexOf("parking/international")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/gettingtofrom/parking/");
			else if(tempUrl.indexOf("parking/main")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/gettingtofrom/parking/default.aspx#main");
			else if(tempUrl.indexOf("parking/economy")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/gettingtofrom/parking/default.aspx#economy");
			else if(tempUrl.toLowerCase().indexOf("parking/")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/gettingtofrom/parking/default.aspx");
			else if(tempUrl.indexOf("curb-to-gate")>-1)	
				return window.location.replace("http://www.flychicago.com/m/midway/en/gettingtofrom/directions/default.aspx");
			else if(tempUrl.indexOf("myflight/airline/")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/myflight/default.aspx#airlines");
			else if(tempUrl.indexOf("myflight/")>-1)
				return window.location.replace("http://www.flychicago.com/m/midway/en/myflight/default.aspx");
			else if(tempUrl.indexOf("connect")>-1)	
				return window.location.replace("http://www.flychicago.com/m/midway/en/contact/default.aspx");
			//default
			return window.location.replace("http://www.flychicago.com/m/midway/en/home/default.aspx");
		}
		
		return window.location.replace("http://www.flychicago.com/m");
}

</script>


