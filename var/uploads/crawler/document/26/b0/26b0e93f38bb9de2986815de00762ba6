<script type="text/javascript">

    var strSiteSearchDefaultInputTextBoxText = "Search site";
    var siteSection = getCurrentSiteSection();

    function showSearchPopupDiv() {
        var src;
        //src = $j("#ctl00_ctl00_ctl18_g_f85ffdfd_e15c_4ac1_909d_346e752f635f_ctl02_ctl00_ibtnSearchSite").attr("src").replace("btn_hdr_search.png", "btn_hdr_search_focus.png");
        $j("#ctl00_ctl00_ctl18_g_f85ffdfd_e15c_4ac1_909d_346e752f635f_ctl02_ctl00_ibtnSearchSite").attr("src", src);
        $j("#searchFormFullDiv").attr("class", "searchOpen");
    }

    function hideSearchPopupDiv() {
        var src;
        //src = $j("#ctl00_ctl00_ctl18_g_f85ffdfd_e15c_4ac1_909d_346e752f635f_ctl02_ctl00_ibtnSearchSite").attr("src").replace("btn_hdr_search_focus.png", "btn_hdr_search.png");
        $j("#ctl00_ctl00_ctl18_g_f85ffdfd_e15c_4ac1_909d_346e752f635f_ctl02_ctl00_ibtnSearchSite").attr("src", src);
        $j("#searchFormFullDiv").attr("class", "search");
    }

    function HandleSearchBoxKeyPress(event) {
        if ((event.keyCode) == 13) {
                HandleSearchBtnClick();
                return false;
            }
    }

    function HandleSearchBtnClick() {
        if ((searchOnKeyPress == '') && ((document.activeElement.id.indexOf('SiteSearch') != -1) || (document.activeElement.id.indexOf('SearchSite') != -1))) {
            var strSiteCollection = "";

	    if ($j('input[id$=txtSiteSearch]').val() == strSiteSearchDefaultInputTextBoxText) {
                    return;
            }            

	    if (document.getElementById('chkboxSiteSearchPopupBusiness').checked) {
                strSiteCollection = "BUS";
                strSiteSearchResultURL = "/Business/EN/search/Pages/default.aspx";
            }
            if (document.getElementById('chkboxSiteSearchPopupMidway').checked) {
                strSiteCollection = strSiteCollection + ",MDW";
                strSiteSearchResultURL = "/Midway/EN/search/Pages/default.aspx";
            }
            if (document.getElementById('chkboxSiteSearchPopupOHare').checked) {
                strSiteCollection = strSiteCollection + ",ORD";
                strSiteSearchResultURL = "/OHare/EN/search/Pages/default.aspx";
            }
            if (strSiteCollection == "") {
                if ($j('input[id$=txtSiteSearch]').val() == strSiteSearchDefaultInputTextBoxText) {
                    showSearchPopupDiv();
                }
                $j("#errMsgSearch").show();
            }
            else {
                if (strSiteCollection.charAt(0) == ',') {
                    strSiteCollection = strSiteCollection.substr(1);
                }
                var encodedSiteCollection;
                encodedSiteCollection = encodeURIComponent(strSiteCollection);

                var encodedSearchTerms;
                var strSearchTerms;
                strSearchTerms = $j('input[id$=txtSiteSearch]').val();
                if (strSearchTerms == strSiteSearchDefaultInputTextBoxText)
                    strSearchTerms = "";
                encodedSearchTerms = encodeURIComponent(strSearchTerms)

                var relativeURL;
                relativeURL = strSiteSearchResultURL + "?SiteCollection=" + encodedSiteCollection + "&SearchTerms=" + encodedSearchTerms;

                document.location.href = relativeURL;
            }
        }
        else {
            searchOnKeyPress = '';
        }
    }

    function hideErrorMessage() {
        $j("#errMsgSearch").hide();
    }

    $j(document).ready(function () {
        if (siteSection == 'ORD') {
            document.getElementById('chkboxSiteSearchPopupOHare').checked = true;
            document.getElementById('chkboxSiteSearchPopupOHare').previousSibling.style.backgroundPosition = "0 -" + checkboxHeight * 2 + "px";
        }
        else if (siteSection == 'MDW') {
            document.getElementById('chkboxSiteSearchPopupMidway').checked = true;
            document.getElementById('chkboxSiteSearchPopupMidway').previousSibling.style.backgroundPosition = "0 -" + checkboxHeight * 2 + "px";
        }
        else if (siteSection == 'BUS') {
            document.getElementById('chkboxSiteSearchPopupBusiness').checked = true;
            document.getElementById('chkboxSiteSearchPopupBusiness').previousSibling.style.backgroundPosition = "0 -" + checkboxHeight * 2 + "px";
        }
		else {
			document.getElementById('chkboxSiteSearchPopupOHare').checked = true;
			document.getElementById('chkboxSiteSearchPopupMidway').checked = true;
			document.getElementById('chkboxSiteSearchPopupBusiness').checked = true;
		}

        $j('.checkbox').click(function () {
            $j("#errMsgSearch").hide();
        });

        var mouseIsInsideDrop;
        var mouseIsInsideTextbox;
        var mouseIsInsideButton;
        $j(".drop").hover(function () {
            mouseIsInsideDrop = true;
        }, function () {
            mouseIsInsideDrop = false;
        });

        $j(".searchBox").hover(function () {
            mouseIsInsideTextbox = true;
        }, function () {
            mouseIsInsideTextbox = false;
        });

        $j(".searchSubmit").hover(function () {
            mouseIsInsideButton = true;
        }, function () {
            mouseIsInsideButton = false;
        });

        $j("body").mouseup(function () {
            if (!mouseIsInsideDrop && !mouseIsInsideTextbox && !mouseIsInsideButton) {
                hideSearchPopupDiv();
            }
        });
    });

</script>
    
