function updateItemFromCart(action, basketItemPk) {
	var _data = $("#updateFromGlobalCartForm").serialize();
	var actionUrl;
	
	if(action == 'update') {
		actionUrl = "/basket.do?method=updateFromGlobalCart&r="+Math.random();
	} else if (action == 'remove'){
		actionUrl = "/basket.do?method=updateFromGlobalCart&removeItems="+basketItemPk+"&r="+Math.random();
	}
	jQuery.ajax({
		type: "POST",
		url: actionUrl,
		data: _data,
		success: function(data){
			jQuery("#myBagLi").html(data);
			//window.setTimeout(function() {
					MarketLive.GlobalCart.showGlobalBasket(true);
			//}, 300);
			MarketLive.GlobalCart.adjustGlobalCartLayout();
		},
		error: function(e){
			window.console && console.log('Edge case, which is not handled while manipulating basket from global cart');
		}
	});
}


   			   
