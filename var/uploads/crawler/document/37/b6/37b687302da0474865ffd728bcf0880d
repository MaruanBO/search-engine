															if(buttonOffName == buttonOnName){
																document.getElementById(buttonOnNameClick).style.visibility = "visible";
																document.getElementById(buttonOnNameClick).style.display = "block";
																document.getElementById(buttonOnName).style.visibility = "hidden";
																document.getElementById(buttonOnName).style.display = "none";
															}
															else{
																document.getElementById(buttonOffNameClick).style.visibility = "hidden";
																document.getElementById(buttonOffNameClick).style.display = "none";
