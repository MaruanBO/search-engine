<script>
	function WTM_Class_Change(varWtmTab){var varWtmClass=document.getElementById("wtm_pick").value; document.getElementById('wtm_letter').value=varWtmTab; if(varWtmClass == 0){document.getElementById('wtm_pick').selectedIndex=1; varWtmClass=document.getElementById("wtm_pick").value;}document.getElementById('wtm_form').submit();}
	function opt_disabled(i){wtm_div_hide(); document.getElementById("wtm_pick_0").style.display='none'; document.getElementById("wtm_pick_0").style.visibility='hidden'; document.getElementById('wtm_pick_'+i).style.display='block'; document.getElementById('wtm_pick_'+i).style.visibility='visible';}
	function wtm_div_hide(){document.getElementById("wtm").style.display='block'; document.getElementById("wtm").style.visibility='visible';document.getElementById("wtm_pick_2").style.display='none'; document.getElementById("wtm_pick_2").style.visibility='hidden';document.getElementById("wtm_pick_1").style.display='none'; document.getElementById("wtm_pick_1").style.visibility='hidden';document.getElementById("wtm_pick_16").style.display='none'; document.getElementById("wtm_pick_16").style.visibility='hidden';document.getElementById("wtm_pick_15").style.display='none'; document.getElementById("wtm_pick_15").style.visibility='hidden';document.getElementById("wtm_pick_11").style.display='none'; document.getElementById("wtm_pick_11").style.visibility='hidden';document.getElementById("wtm_pick_18").style.display='none'; document.getElementById("wtm_pick_18").style.visibility='hidden';document.getElementById("wtm_pick_17").style.display='none'; document.getElementById("wtm_pick_17").style.visibility='hidden';}
	wtm_div_hide();
</script>
