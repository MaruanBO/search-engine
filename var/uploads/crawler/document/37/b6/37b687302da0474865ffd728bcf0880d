													function changeHotTopic12(hotTopicName,hotTopicId,hotTopicHoverImage,hotTopicButtonImage){
													
															var buttonOnNameClick = 'hot_topics_buttons_'+hotTopicName+'_click';
															var buttonOnName = 'hot_topics_buttons_'+hotTopicName;
															var buttonOffNameClick = 'hot_topics_buttons_Quellin_click';
