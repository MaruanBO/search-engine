		<script>
			var myvar;
			function menuinit()
			{
				
					document.getElementById('m1').style.display = 'none';
					/* handles the open/close text */
					var s=document.getElementById('s_m1');
					var p=document.createElement('p_m1');
					var txt='open +';
					p.appendChild(document.createTextNode(txt));
					s.innerHTML="";
					s.appendChild(p);
					/* // handles the open/close text */
				
					document.getElementById('m2').style.display = 'none';
					/* handles the open/close text */
					var s=document.getElementById('s_m2');
					var p=document.createElement('p_m2');
					var txt='open +';
					p.appendChild(document.createTextNode(txt));
					s.innerHTML="";
					s.appendChild(p);
					/* // handles the open/close text */
				
					document.getElementById('m3').style.display = 'none';
					/* handles the open/close text */
					var s=document.getElementById('s_m3');
					var p=document.createElement('p_m3');
					var txt='open +';
					p.appendChild(document.createTextNode(txt));
					s.innerHTML="";
					s.appendChild(p);
					/* // handles the open/close text */
				
			}
			function menuexpand (i)
			{
				var listElementStyle=document.getElementById(i).style.display;
				if (listElementStyle == 'block')
				{
					document.getElementById(i).style.display = 'none';
					/* handles the open/close text */
					var s=document.getElementById('s_' + i);
					var p=document.createElement('p_' + i);
					var txt='open +';
					p.appendChild(document.createTextNode(txt));
					s.innerHTML="";
					s.appendChild(p);
					/* // handles the open/close text */
					myvar = '';
				}
				else
				{
					document.getElementById(i).style.display = 'block';
					/* handles the open/close text */
					var s=document.getElementById('s_' + i);
					var p=document.createElement('p_' + i);
					var txt='close -';
					p.appendChild(document.createTextNode(txt));
					s.innerHTML="";
					s.appendChild(p);
					/* // handles the open/close text */
					myvar = i;
				}
			}
			function firstmenuinit()
			{
				
				
						document.getElementById('m1').style.display = 'block';
						/* handles the open/close text */
						var s=document.getElementById('s_m1');
						var p=document.createElement('p_m1');
						var txt='close -';
						p.appendChild(document.createTextNode(txt));
						s.innerHTML="";
						s.appendChild(p);
						/* // handles the open/close text */
					
						document.getElementById('m2').style.display = 'block';
						/* handles the open/close text */
						var s=document.getElementById('s_m2');
						var p=document.createElement('p_m2');
						var txt='close -';
						p.appendChild(document.createTextNode(txt));
						s.innerHTML="";
						s.appendChild(p);
						/* // handles the open/close text */
					
						document.getElementById('m3').style.display = 'block';
						/* handles the open/close text */
						var s=document.getElementById('s_m3');
						var p=document.createElement('p_m3');
						var txt='close -';
						p.appendChild(document.createTextNode(txt));
						s.innerHTML="";
						s.appendChild(p);
						/* // handles the open/close text */
					
			}
		</script>
		
		
