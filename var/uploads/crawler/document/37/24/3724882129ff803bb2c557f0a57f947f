<script type="text/javascript">

(function($) {
 
$(document).ready(function(){
  
 var containerelem = $('#slideshow_reel');
 
 if($('.slide').length > 1) {
  
  containerelem.cycle({fx:'fade', activePagerClass: 'activenav', slideExpr: '.slide',pager:'#nav',timeout:'4000'});
 
  var size = $('#nav a').length;
  var width = size*16;
  $("#slideshow_navigation").css('display', 'block');
  $('#nav').css({'width':width , 'display':'block'});
 
  //Stop the slideshow when the navigation is
  $('#nav a, .cta, .quickview').click(function(){
   containerelem.cycle('pause');
  });
  
  //Add swipe functionality
  if(!$.browser.msie) {
   $(".slide").touchwipe({
     wipeLeft: function() { $('#slideshow_reel').cycle('pause'); containerelem.cycle('next'); },
     wipeRight: function() { $('#slideshow_reel').cycle('pause'); containerelem.cycle('prev'); }
   });
  } 
  
 }
 
});
 
})(jQuery);

