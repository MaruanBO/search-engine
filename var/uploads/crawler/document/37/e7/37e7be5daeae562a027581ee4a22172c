				<script>
					var isPrivate = false;
					var storage = localStorage;
					try {
					  storage.setItem('sTest', 'test');
					  storage.removeItem('sTest');
					} catch (e) {
					  if (e.code == DOMException.QUOTA_EXCEEDED_ERR && storage.length == 0) {
						isPrivate = true;
					  } else {
						console.log(e);
					  }
					}
					var mobileDevice = {
					 isIphone :  (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i)) != null,
					 isIpad : navigator.userAgent.match(/iPad/i) != null,
					 isAndroid : navigator.userAgent.match(/Android/i) != null
					} 
					var desktopBookmark = {
						verifyBookMark : function(){
							if (mobileDevice.isIpad || mobileDevice.isIphone || mobileDevice.isAndroid){
								
								if (isPrivate === false && localStorage.getItem('bookMark') === null){
										desktopBookmark.build();
									} else {
										return false
									}
							} else {
								return false
							}
						},
						build : function (){
						
							var body = document.getElementsByTagName('body')[0];
							var bookmarkContainer = document.createElement('div');
								bookmarkContainer.setAttribute('role','dialog');
								bookmarkContainer.setAttribute('id','addHome');
								bookmarkContainer.setAttribute('alt','Capital One 360');
								bookmarkContainer.setAttribute('aria-live','rude');
								if (mobileDevice.isIphone || mobileDevice.isAndroid){
									bookmarkContainer.className = "bookmarkMsgBtm";
								} else if (mobileDevice.isIpad){
									bookmarkContainer.className = "bookmarkMsgTop";
								} else {
									return false
								} 
								
								body.appendChild(bookmarkContainer);
										
							var logo = document.createElement('img');
								logo.setAttribute('area-hidden','true');
								logo.setAttribute('src','https://images.capitalone360.com/images/secure/home-screen-icon-popup-2x.png');
								logo.width = '57';
								logo.className = 'logo';
								bookmarkContainer.appendChild(logo);

							
								
								
							var para = document.createElement('p');	
								para.setAttribute('id','bookMarkLabel');
								if (mobileDevice.isIpad || mobileDevice.isIphone){
									var nav = navigator;
									var OSVersion = nav.appVersion.match(/OS (\d+_\d+)/i);
									OSVersion = OSVersion && OSVersion[1] ? +OSVersion[1].replace('_', '.') : 0;
									var imgIconSrc = 'https://images.capitalone360.com/home/ipad-share-icon.png';
									var imgIconWidth = '18';
									if (OSVersion >= 7){
										imgIconSrc = "data:image/png;base64,";
										imgIconWidth = '11';
									}
									para.innerHTML = 'Add our page to your home screen - tap on the arrow (<img aria-hidden="true" class="share" src="' + imgIconSrc + '" width="' + imgIconWidth + '">) and choose <strong>\'Add to Home Screen\'</strong>';		
								} else if (mobileDevice.isAndroid) {
									para.innerHTML = 'Add us as a bookmark or a shortcut to your home screen for easy access to banking on the go.';	
								}
								bookmarkContainer.appendChild(para);
								
								var btn = document.createElement('button');
								btn.addEventListener('click', desktopBookmark.writeData, false);
								bookmarkContainer.appendChild(btn);
								var closeBtn = document.createElement('img');
								closeBtn.setAttribute('aria-label','Close Dialog');
								closeBtn.setAttribute('src','https://images.capitalone360.com/home/shared-lightbox-close-gray.png');
								closeBtn.setAttribute('alt','Close');
								btn.appendChild(closeBtn)
								bookmarkContainer.appendChild(btn);
								
								js.urchin('/homepage/add_to_homescreen');
						},
						writeData : function() {					
							js.urchin('/homepage/add_to_homescreen_close');
							localStorage.setItem('bookMark', true);
							document.getElementById('addHome').style.display = 'none';

						}
					}
					desktopBookmark.verifyBookMark();
				</script>
