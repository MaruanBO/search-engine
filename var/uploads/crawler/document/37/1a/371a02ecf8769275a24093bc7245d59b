	    } else {
	      window['__gcs_sayt'] = sayt;
	      completionObject = sayt['completionController'];
	    }
	  };
	// Autocomplete options
	  var autoCompletionOptions = {
	    'maxCompletions' : 5,
	    'styleOptions' : {
	       xAlign: 'left'
	     }
	  };
	 // this adds the calls to the sites input box for SAYT
	 //SAYT_box is the id of the input box
	 //searchForm is the id of the form SAYT_box belongs to
	 if( typeof(setupSayt) !== 'undefined') {
		setupSayt(cx, key, 'q', 'searchform', setSaytCallback, autoCompletionOptions, sayt_params);
	   }
	 
	 </script>
	 <![endif]>	
	 <script type="text/javascript">
	 
	 //SiteCatalyst analytics.  this is done here by hyjacking the submit because of
	 //site search redirects.  theres another piece on the landing page.
	 $("#searchform").submit(function(event){
		 if($("#q").val()){
			if(typeof(mcgia_o) == "object"){
				mcgia_o.searchterm($("#q").val());
				mcgia_o.search();
				s.events = "event64";
				s.tl();
			}
		 }
		 else{
			mcgia_o.searchterm(null);
			}
		 return true;
		 
	 });
	</script>
	






</div>  
				</div>
			</div>
		</div>
	</div>
