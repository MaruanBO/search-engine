<script>

//*****Left Buttons Logic*****

//Variables
var newDiv;
var anonTag;
var newImg;
var itemWrapper;
var leftButtons;
var wishlistInitilized = false;

//Request buttons
//Test Code
leftButtons = [{
	"title":"Recently Viewed Items",
	"blue_icon_url":"http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/images/icon_recentlyviewed.png",
	"white_icon_url":"http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/images/icon_recentlyviewed_white.png"
}];

//*****Right Buttons Logic*****
var wishListPageSize = 5;
var mfwishlist = {"WishListProducts":"0"};
var mfwsnpages = 1;

//********* Wish List *********

//Build Wish List, and update pagination
$(document).ready(function() {
	//Display buttons
	for (var i=0; i<leftButtons.length; i++) {
		newDiv = document.createElement("div");
		newDiv.id = "magicFooterLeftButton" + i;
		var imgblue = document.createElement("img"); imgblue.id = "magicFooterLeftButton" + i + "BlueIcon"; imgblue.src=leftButtons[i].blue_icon_url;imgblue.className = "magicFooterLeftIcon";
		var imgwhite = document.createElement("img"); imgwhite.id = "magicFooterLeftButton" + i + "WhiteIcon"; imgwhite.src=leftButtons[i].white_icon_url; imgwhite.className = "magicFooterLeftIcon magicFooterHiddenIcon";
		newDiv.appendChild(imgblue);
		newDiv.appendChild(imgwhite);
		newDiv.innerHTML += leftButtons[i].title;
		newDiv.className = "magicFooterLeftButton magicFooterButton";
	
		anonTag = document.createElement("img"); anonTag.src = "http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/images/icon_arrow_up.png"; anonTag.className = "magicFooterRightIcon";

		if (document.getElementById('magicFooterMenuBar') != null) {	
			document.getElementById('magicFooterMenuBar').appendChild(newDiv);
			document.getElementById("magicFooterLeftButton" + i).appendChild(anonTag);
		}
	}
	//Some variation of this will need to be called every time any button is opened
	//Click JSON Button 0: open content
	$('#magicFooterLeftButton0').click(function() {
		toggleActive(".magicFooterContent", "#magicFooterLeftButton0");
	});

	//Quick State check/init stuff
	if (state == undefined || state == "menu" || state == "") {
		state = "menu";
		$("#magicFooterMenuBar").removeClass("magicFooterClosed");
		$('#magicFooterArrowUp').addClass('magicFooterHiddenIcon');
		$('#magicFooterArrowDown').removeClass('magicFooterHiddenIcon');		
	}
	if (msieversion() > 10 || msieversion() == 0) {hasCSS3 = true;}
	else {$(".magicFooterTransition").css("transition", "none");}
});

function initWS() {
	$.getJSON("/on/demandware.store/Sites-jtv-Site/default/Serialize-WishList", function(list) {
		mfwishlist = list;
		WishListPage(1);
		mfwsnpages = Math.ceil(mfwishlist.WishListProducts.length / wishListPageSize);
		
		//Pagination magic
		if (mfwishlist.WishListProducts.length > wishListPageSize) {
			for (i=2; i<=mfwsnpages; i++) {
				var opt = document.createElement("option");
				opt.id = 'mFWLSO' + i;
				opt.innerHTML = i;
				if (document.getElementById("mFWLS") != null) {
					document.getElementById("mFWLS").appendChild(opt);
				}
			}
			$("#mFWLPT").html("of " + mfwsnpages);
		}
	});
}

//Navigate to Send Wish List To A Friend page
function sendWL2Friend() {
var x=0;
	window.location.href=mfwishlist.WishListShowOther;
}

//Builds items in the Wish List
//Do not call this until mfwishlist has been defined!
function WishListPage(Page) {
	Page--;
	var itemStart = Page * wishListPageSize;
	var itemEnd = itemStart + wishListPageSize;
	
	//Empty Wish List
	$("#magicFooterWishListPopUpMain").empty();
	
	//Loop over all items in the Wish List and attach them to the footer
	var limit = (mfwishlist.WishListProducts.length > itemEnd ? itemEnd : mfwishlist.WishListProducts.length); 
	
	for (var i=itemStart; i<limit; i++) {
		//Build & Append HR
		if (i!=itemStart) {
			var mfClearDiv = document.createElement("div"); mfClearDiv.className = "clear";
			var mfHR = document.createElement("hr"); mfHR.className = "magicFooterPopUpHR";
			mfClearDiv.appendChild(mfHR);
			if (document.getElementById('magicFooterWishListPopUpMain') != null) {
				document.getElementById('magicFooterWishListPopUpMain').appendChild(mfClearDiv);
			}
		}		
	
		newDiv  = document.createElement("div");
		anonTag = document.createElement("div");
		newImg  = document.createElement("img");
		itemWrapper = document.createElement("div"); itemWrapper.className = "magicFooterItemWrapper";
		
		//Build Img
		newDiv.className = "magicFooterPopUpItem";
		newImg.src = mfwishlist.WishListProducts[i].thumbnail;
		newImg.alt = mfwishlist.WishListProducts[i].name;
		
		//Append Img Then Build & Append Rating Img
		anonTag.appendChild(newImg);
		newDiv.appendChild(anonTag);
		newDiv.appendChild(anonTag);
		anonTag = document.createElement("div");
		newImg = document.createElement("img");
		if (mfwishlist.WishListProducts[i].ratingImageURL) {
			newImg.src = mfwishlist.WishListProducts[i].ratingImageURL;
		}
		anonTag.appendChild(newImg);
		newDiv.appendChild(anonTag);
		itemWrapper.appendChild(newDiv);
		
		//Build & Append Table
		var mfTable = document.createElement("table");
		anonTag = document.createElement("tr");
		//Name
		var mfTD = document.createElement("td"); mfTD.className = "magicFooterPopUpDesc";
		mfTD.innerHTML = mfwishlist.WishListProducts[i].name;
		anonTag.appendChild(mfTD);
		//Quantity
		mfTD = document.createElement("td"); mfTD.className = "magicFooterPopUpQty";
		mfTD.innerHTML = "1";
		anonTag.appendChild(mfTD);
		//Price
		mfTD = document.createElement("td"); mfTD.className = "magicFooterPopUpPrice";
		mfTD.innerHTML = "$" + mfwishlist.WishListProducts[i].price;
		anonTag.appendChild(mfTD);
		mfTable.appendChild(anonTag);
		itemWrapper.appendChild(mfTable);
		
		//Build & Append Button
		var mfButton = document.createElement("button"); mfButton.className = "AddToCartBtn green_button";
		mfButton.name = mfwishlist.WishListProducts[i].pid;
		anonTag = document.createElement("div");
		anonTag.innerHTML = "Add To Cart";
		mfButton.appendChild(anonTag);
		itemWrapper.appendChild(mfButton);
		
		if (document.getElementById('magicFooterWishListPopUpMain') != null) {
			document.getElementById('magicFooterWishListPopUpMain').appendChild(itemWrapper);
			//NOTE: This is SUPER ugly hack to get IE7 to actually display the table.  I'd love to change this to something more intelligent.  This is both the solution and source of my problems in IE7....
			$("#magicFooterWishListPopUpMain").children().last().html($("#magicFooterWishListPopUpMain").children().last().html());
		}
	}
	
	//Add To Cart Wrapper
	$(".AddToCartBtn").click(function() {
		window.location.href = '/on/demandware.store/Sites-jtv-Site/default/Cart-AddProduct?pid=' + this.name;
	});
}

//Wish List Selection Change
$("#mFWLS").change(function() {
	WishListPage ($("#mFWLS").val());
});

//Wish List Navigation
function MFNav (direction) {
	var mFNum = parseInt($("#mFWLS").val());
	var mFTarget = "#mFWLSO" + mFNum; 
	$(mFTarget).removeAttr('selected');
	mFNum = mFNum + direction;
	mFTarget = "#mFWLSO" + mFNum;
	$("#mFWLS").val(mFNum);
	$(mFTarget).attr('selected','selected');
	WishListPage (mFNum);
}

//Button Navigation Left
$("#mFWLNL").click(function() {
	if ($("#mFWLS").val() > 1) {
		MFNav(-1);
	}
});

//Button Navigation Right
$("#mFWLNR").click(function() {
	if ($("#mFWLS").val() < mfwsnpages) {
		MFNav(1);
	}
});

/* Gets the cookie value named c_name */
var getCookie = function(c_name) // from w3c
{
	var i, x, y, ARRcookies = document.cookie.split(";");
	for (i = 0; i < ARRcookies.length; i++) {
		x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
		y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
		x = x.replace(/^\s+|\s+$/g, "");
		if (x == c_name) {
			return unescape(y);
		}
	}
}

/*
 * Sets the cookie named c_cookie with value and exipration exdays from
 * today
 */
var setCookie = function(c_name, value, exdays) // from w3c
{
	var exdate = new Date();
	exdate.setDate(exdate.getDate() + exdays);
	var c_value = escape(value)
			+ ((exdays == null) ? "" : "; expires=" + exdate.toUTCString())
			+ ";path=/";
	document.cookie = c_name + "=" + c_value;
}

//*****Open and collapse logic******
var active = "none";
var activeParent = "none";
var animationSpeed = "slow";
var hasCSS3 = false;
var state = (getCookie("state") == undefined || getCookie("state") == "" ? "menu" : getCookie("state"));

function msieversion() {
	var ua = window.navigator.userAgent;
	var msie = ua.indexOf ("MSIE ");

	if (msie > 0) {      // If Internet Explorer, return version number
		return parseInt (ua.substring (msie+5, ua.indexOf (".", msie )));
	} else {             // If another browser, return 0
		return 0;
	}
}

function animateOpen (target) {
	$(target).removeClass("magicFooterClosed");
	if (hasCSS3 == false) {
		$(target).slideDown(animationSpeed, function() {});
	}	
}

function animateClose (target) {
	if (hasCSS3 == false) {
		$(target).slideUp(animationSpeed, function() {});//$(target).addClass("magicFooterClosed");
	} else {
		$(target).addClass("magicFooterClosed");
	}
}

function giveButtonFocus (target) {
	$(target).addClass('magicFooterActiveParent');
	$(target + 'BlueIcon').addClass('magicFooterHiddenIcon');
	$(target + 'WhiteIcon').removeClass('magicFooterHiddenIcon');
}

function removeButtonFocus (target) {
	$(target).removeClass('magicFooterActiveParent');
	$(target + 'WhiteIcon').addClass('magicFooterHiddenIcon');
	$(target + 'BlueIcon').removeClass('magicFooterHiddenIcon');
}

function toggleActive (caller, callerParent) {
	if (active == "none") {
		$(caller).css("z-index", "1");
		animateOpen (caller);
		giveButtonFocus(callerParent);
		state = "active";
		active = caller;
		activeParent = callerParent;
		s.prop27 = "Full State View";
		s.tl();
		if (caller == ".magicFooterCartPopUp") {//Analytics for when people view the cart
			mcgia_o.fireevent("scView");
			mcgia_o.writeEvents();
		}
	} else {
		animateClose (active);
		$(active).css("z-index", "-1");
		removeButtonFocus(activeParent);
		
		if (caller != "none" && caller != active) {
			animateOpen (caller);
			$(caller).css("z-index", "1");
			giveButtonFocus (callerParent);
			state = "active";
			active = caller;
			activeParent = callerParent;
		} else {
			state = "menu";
			active = "none";
			activeParent = "none";
		}
	}
}

//Click arrow logic
$('.magicFooterArrow').click(function() {
	//Logic: If (its open) Then {close everything} else {open menu bar}
	switch (state) {
	case "closed":
		animateOpen ('#magicFooterMenuBar');
		$('#magicFooterArrowUp').addClass('magicFooterHiddenIcon');
		$('#magicFooterArrowDown').removeClass('magicFooterHiddenIcon');
		state="menu";
		break;
	case "menu":
		animateClose ('#magicFooterMenuBar');
		$('#magicFooterArrowDown').addClass('magicFooterHiddenIcon');
		$('#magicFooterArrowUp').removeClass('magicFooterHiddenIcon');
		state="closed";
		break;
	case "active":
		toggleActive ("none");
		break;
	}
	setCookie("state", state, 1);
});

//View Cart
$('#magicFooterCart').click(function() {
	toggleActive(".magicFooterCartPopUp", "#magicFooterCart");
});
//Close Button
$('#magicFooterCartClose').click(function() {
	toggleActive(".magicFooterCartPopUp", "#magicFooterCart");
});

//View Wish List
$('#magicFooterWishList').click(function() {
	if (!wishlistInitilized) {
		initWS();
		wishlistInitilized = true;
	}
	toggleActive(".magicFooterWishListPopUp", "#magicFooterWishList");
});
//Close Button
$('#magicFooterWishListClose').click(function() {
	toggleActive(".magicFooterWishListPopUp", "#magicFooterWishList");
});

function RefreshCart() {
	$.get("/on/demandware.store/Sites-jtv-Site/default/Cart-FooterRefresh", function (cartHTML) {
		$("#magicFooterCartPopUpMain").html(cartHTML);
	});
	$.getJSON("/on/demandware.store/Sites-jtv-Site/default/Serialize-CartInfo", function(cart) {
		$("#magicFooterCartText").text(cart.ItemCount + " Item(s)");
		$("#mfcpubrEstimate").text(cart.Estimate);
		$("#mfcpubrTotal").text(cart.Total);
	});
}

//***Analytics! Hooray!***
function gotoCheckout () {
	mcgia_o.fireevent("scCheckout");
	mcgia_o.writeEvents();
	window.location.href="https://www.jtv.com/on/demandware.store/Sites-jtv-Site/default/Cart-MiniCheckout"
}

function gotoCart () {
	window.location.href="https://www.jtv.com/cart"
}

