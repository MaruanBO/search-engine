<script type="text/javascript">
	/* Use of the cdata tag helps this script validate, since it lives inside an xml document */
	/* <![CDATA[ */
	function navBarGetChildText(elem) { //strips html tags and character codes within the given element, returning text
		var text, tagsRegex, codesRegex;
		tagsRegex = /<.*?[^>]>/g;
		codesRegex = /&.+;/g;
		text = elem.html().replace(tagsRegex, '').replace(codesRegex, '');
		return text;
	}

	jQuery("#navBar").find("li a.navHead").click(
			function(obj) {
				var a;
				a = jQuery(obj.target);
				navBarReportLink(obj.target, navBarGetChildText(a),
						navBarGetChildText(a));
			});

	jQuery("#navBar").find("li div.nav_content a").click(
			function(obj) {
				var elem, a;
				a = jQuery(obj.target);
				elem = a.parents("li.navMenuItem").find("a.navHead");
				navBarReportLink(obj.target, navBarGetChildText(elem),
						navBarGetChildText(a));
			});

	function navBarReportLink(target, linkText, headerText) {
		var s, lt, targetLink;
		targetLink = (target.nodeName.toLowerCase() == 'a') ? target.href
				: jQuery(target).parents('a').attr('href');
		s = s_gi(s_account)
		s.linkTrackVars = 'eVar13,prop26';
		s.eVar13 = headerText;
		s.prop26 = linkText + ": " + headerText;
		lt = (targetLink) ? s.lt(targetLink) : '';
		if (!lt) {
			s.tl(target, 'o', 'Header Link');
		}
	}
	/* ]]> */
	
	$(document).ready(function() {
		//Find every middle align
		$("#navBarContainer").find(".nav_malign").each(function(index) {
			var left_offset=0;
			var right_offset=0;
			var max_adjust=0;
			var adjustmentNeeded=0;
			//calculate total left offset from the navBarContainer
			$(this).parentsUntil("#navBarContainer").each(function() {
				left_offset += $(this).position().left;
			});
			
			//Find the right offset
			right_offset = $("#navBarContainer").width() - (left_offset + $(this).width());
			adjustmentNeeded = ((right_offset + left_offset)/2)-left_offset;
			max_adjust = $(this).parent().width() - $(this).width();
			if (adjustmentNeeded > 0) { //Guarantee its not pushed too far to the right
				$(this).removeClass("nav_malign");
				$(this).addClass("nav_lalign");
			} else if (adjustmentNeeded < max_adjust) { //Guarantee its not pushed too far to the left
				$(this).removeClass("nav_malign");
				$(this).addClass("nav_ralign");  
			} else {
				$(this).css("margin-left", adjustmentNeeded);
			}
		});
	});

</script>

