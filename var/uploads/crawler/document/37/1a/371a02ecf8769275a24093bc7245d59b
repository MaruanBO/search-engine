 <script type="text/javascript"> 	
	var jsPath = "http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/js";
	var libPath = "http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/lib";
	var fcPath = "http://demandware.edgesuite.net/aaby_prd/on/demandware.static/Sites-jtv-Site/-/default/v1424377004718/foresee";
	var loadedLibs = new Array();
	
	//Load a browser object in the jQuery object to keep functionality similar to deprecated $.browser
	if (jQuery){
		var browser = {			
			msie: null,
			version: 0
		}
		browser.msie = navigator.userAgent.match(/MSIE/i) === null ? false : true;
		if(browser.msie){
			browser.version = navigator.userAgent.match(/MSIE [0-99]+\.*[0-99]*/i);
			browser.version = browser.version[0].substr(5, browser.version[0].length);
		}
		
		jQuery.browser = browser;
	}
	
	if (!Array.prototype.indexOf) { 
	    Array.prototype.indexOf = function(obj, start) {
	         for (var i = (start || 0), j = this.length; i < j; i++) {
	             if (this[i] === obj) { return i; }
	         }
	         return -1;
	    }
	}
	
	function load_js_lib(lib){		
		try {
			var library = {
					jqueryui : '//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js',
					jcarousel : jsPath + '/jcarousel/0.2.9/lib/jquery.jcarousel.min.js',
					blockui : jsPath + '/jquery.blockUI.js',
					qtip : jsPath + '/jquery.qtip-1.0.0-rc3.min.js',
					lightbox : jsPath + '/lightbox/js/jquery.lightbox-0.5.js',
					mcgzoom: jsPath + '/jquery.mcgZoom.js',
					mcgia : jsPath + '/mcgianalytics.js',
					validate: libPath + '/js/jquery-validate/jquery.validate.min.js',
					swfobject : jsPath + '/swfobject_2_2/swfobject.js',
					certonamanager : jsPath + '/certonaManager.js',			
					foresee: fcPath + '/foresee-trigger.js',
					flyout: jsPath + '/flyout_menus.js',
					jtvscripts: jsPath + '/jtvScripts.js',
					mtagconfig: jsPath + '/mtagconfig.js',
					app: jsPath + '/app.js',
					redesign: jsPath + '/redesign.js',
					dotdotdot: jsPath + '/jquery.dotdotdot-1.5.9.min.js',
					modernizr: jsPath + '/modernizr.js'
			}
			
			
			
			if (typeof jQuery === "undefined"){		
				loadJS(library.jquery);
			}	
			
			if (typeof lib !== "undefined" && typeof lib !== "jquery")
				lib = lib.toLowerCase();
			else
				return;

			if(loadedLibs.indexOf(lib) >= 0) return;
			switch (lib){
				case 'jcarousel':
					if (typeof jQuery.jcarousel === "undefined"){			
						deferJS(library.jcarousel);
					}
					break;
					
				case 'blockui':
					if (typeof jQuery.blockUI === 'undefined'){
						deferJS(library.blockui);
					}
					break;		
				case 'qtip':
					if (typeof jQuery.qtip === 'undefined'){
						deferJS(library.qtip);
					}
					break;					
				case 'lightbox':
					if (typeof jQuery.lightBox === 'undefined'){
						deferJS(library.lightbox);
					}
					break;
					
				case 'mcgzoom':
					if (typeof jQuery.mcgZoom === 'undefined'){
						deferJS(library.mcgzoom);
					}
					break;
				case 'validate':
					if (typeof jQuery.validate === 'undefined'){
						deferJS(library.validate);
					}
					break;
					
				case 'swfobject':
					if (typeof swfobject === 'undefined'){
						deferJS(library.swfobject);
					}
					break;		
				case 'tooltip':
					if (typeof jQuery.tooltip === 'undefined'){
						deferJS(library.tooltip);
					}
					break;		
				case 'certonamanager':
					if (typeof certonaInfoManager === 'undefined'){
						loadJS(library.certonamanager);
					}
					break;
				case 'dotdotdot':
					if (typeof jQuery.dotdotdot === 'undefined'){
						deferJS(library.dotdotdot);
					}
					break;						
				case 'app':
					loadJS(library.app);
					break;
				case 'mcgia':
					loadJS(library.mcgia);
					break;
				default:
					deferJS(library[lib]);
					break;
			}
			

		}catch(e) {
			
		}
		loadedLibs[loadedLibs.length] = lib;
	}
	
	function deferJS(scriptLocation) {
		 (function(d) {
			 var element = document.createElement("script");
			 element.id = scriptLocation.replace(/^.*[\\\/]/, '');
			 element.type = 'text/javascript';
			 element.async = true;
			 element.src = scriptLocation;
			 d.getElementsByTagName('head')[0].appendChild(element);
		 })(document);
	}
	
	function loadJS(scriptLocation) {
		document.write('<script id="' + scriptLocation.replace(/^.*[\\\/]/, '')  + '" type="text/javascript" src="' + scriptLocation + '"></'+'script>');
	}
</script>
