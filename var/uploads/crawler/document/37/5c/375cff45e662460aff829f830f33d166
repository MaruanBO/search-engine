<script>
$(document).on(shcWidgets.EVENTS.gnfLoaded, function() {
	if ($(window).width() <= 1200 && $('.hdrv3').length === 0) {
		$('#home #shcContent').css({ /* Force non-gutter layout */
			'float': 'none',
			'width': 'auto',
			'padding-left': '3px'
		});
		$('.span20').next() /* Mark OAS container for special CSS handling and shift position in the DOM */
			.attr('id','oas-container')
			.find('script').remove().end()
			.insertBefore('.span20');
		setTimeout(function() { /* This was done via a deptNav complete() callback, but it no longer works */
				$('#oas-container')
					.children(':first-child').offset(function(){ 
						var nav = $('#gnf_dept_tree_nav>ul, #deptNav'); /* Apparently either of these might be valid */ 
						return nav.length > 0? {top: nav.offset().top + nav.height() + 20} : {};
					}).end()
					.css('display','block');
			}, 450);
	}
})
</script>
