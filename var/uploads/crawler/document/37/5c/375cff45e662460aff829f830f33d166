<script type="text/javascript">
mboxCreate("kmart_homepage_memberappreciation");
// hide default mbox content; hiding just the outer div doesn't work, it will be shown again a bit later
var defaultDivs = document.getElementById("content-shc-kmart-en_us-par-bootstraprow-par1-start_0").getElementsByTagName("div");
for(var i=0; i<defaultDivs.length; i++) {
    defaultDivs[i].style.visibility="hidden";
}
// show default mbox content if still there
function showDefaultkmart_homepage_memberappreciation() {
    var defaultContent = document.getElementById("content-shc-kmart-en_us-par-bootstraprow-par1-start_0");
    if (defaultContent) {
        CQ_Analytics.TestTarget.ignoreNextUpdate('kmart_homepage_memberappreciation');
        var defaultDivs = defaultContent.getElementsByTagName("div");
        for(var i=0; i<defaultDivs.length; i++) {
            defaultDivs[i].style.visibility="visible";
        }
    }
}
// wait 2s for campaign content to arrive before showing default content
setTimeout("showDefaultkmart_homepage_memberappreciation()", 2000);
</script></div>
