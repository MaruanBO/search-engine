function buildLoginMessage(targetElement){
//init user status
var signedIn = '';
var isMember = false;

//check login 
signedIn = getRAInfo().userType;

isMember = getSYWRPersonalization().isMbr ;

///////////////

//set links based on login status
var signIn = !(signedIn =='R')?'<a class="usrLogin">sign in</a> or ':'';
var register = !(signedIn =='R')?'<a class="usrRegister">join now for FREE</a>':'<a class="usrJoin">join Shop Your Way</a>';
var shopNowLink = '';

//add content based on login status

var contentToAdd = (isMember.toLowerCase()=='true')?shopNowLink:signIn + register;

$('.shcSYWMessage .'+targetElement).html(contentToAdd);


//click for modal

	$('.shcSYWLink a').on( "click", function() {
		var modAction = $(this).attr('class');
		switch (modAction) {
		case 'usrLogin':
			shcWidgets.Factory.get('profile').login()
			break;
		case 'usrRegister':
			shcWidgets.Factory.get('profile').register()
			break;
		case 'usrJoin':
			shcWidgets.Factory.get('profile').join()
			break;
		}
	
	});
}

if ($('.billboard').length>0){
	buildLoginMessage('shcSYWLink')
}
	
$(document).on(shcWidgets.EVENTS.dataSYWR, function(e, data) {
	buildLoginMessage('shcSYWLink')
});

</script> 

