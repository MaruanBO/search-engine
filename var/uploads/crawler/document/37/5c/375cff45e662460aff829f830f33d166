function showDefaultkmart_homepage_taxfree() {
    var defaultContent = document.getElementById("content-shc-kmart-en_us-par-bootstraprow-par1-start");
    if (defaultContent) {
        CQ_Analytics.TestTarget.ignoreNextUpdate('kmart_homepage_taxfree');
