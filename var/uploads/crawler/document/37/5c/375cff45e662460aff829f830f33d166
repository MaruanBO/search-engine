//shc.isOutlet=true;

//$( document ).ready(function() {

//check for images

    if(''.length > 0){

        var bbHeroImg=('' && shc.isOutlet)?'':'';
		bbHeroImg=bbHeroImg.replace('http://s.shld.net/is/image/Sears/','http://s.shld.net/is/image/Sears/');
        var bbWidth=(bbHeroImg.indexOf('wid=')>0 || bbHeroImg.indexOf('$cq_width') >0)?'':'$cq_width_1000$'
        var bbQlt=(bbHeroImg.indexOf('qlt=')>0)?'':'qlt=60';

        (bbWidth.length > 0 && bbQlt.length > 0)?bbHeroImg+'?'+ bbWidth +'&'+bbQlt:bbHeroImg;
        var bbHeroLink=(''&& shc.isOutlet)?'':'';
        var bbHeroTxt=(''&& shc.isOutlet)?'':'';
