		<script type="text/javascript">
			dojo.require('wc.widget.WCRibbonBase');
		
			/*
			 * Ribbon code
			 */
			dojo.addOnLoad(function() {
				var ribbons = dojo.query('.ibm-ribbon .ibm-container-body'); 
				if(ribbons.length > 0) {
					dojo['require']("wc.widget.WCRibbon");
					dojo.addOnLoad(function() {
						dojo.forEach(ribbons, function(item) {
									
							// make sure this ain't leadspace ribbon
							if(dojo.hasAttr(item, 'id')){
								if(item.id == 'ibm-leadspace-body' ){
									// there can always be only 1 such ribbon per page
									// therefore it makes no sense to bust memory with widgets,
									// insteadl, we will make it a simple function call
									wc.widget.WCRibbonLeadspace.init({width:935, height:425, imageMap: {
	"1": "/wcsstore/AuroraPlus/Attachment/HomeRow1_Content/HomeRow1_Content_Jan01_B3G2.jpg",
	"2": "/wcsstore/AuroraPlus/Attachment/HomeRow1_Content/HomeRow1_Content_Feb05_LunarNewYear.jpg"
}, bidi: false });
								}
								else {
									var s = new wc.widget.WCRibbon({ id: dojo.attr(item, 'id'),srcNodeRef: item, bidi: false });
									s.startup();
								}
							} else {
								var s = new wc.widget.WCRibbon({ srcNodeRef: item, bidi: false });
								s.startup();
							}
						});
					});
				}
			});
		</script>
	
