			function omnitureStrip(a) 
			{
				//alert
				a = a.split("|").join("");
				a = a.split("/").join("");
				a = a.split("!").join("");
				a = a.split("&").join(" and ");
				a = a.split("'").join("");
				a = a.split("#").join("");
				a = a.split("$").join(" dollars ");
				a = a.split("%").join(" percent ");
				a = a.split("^").join("");
				a = a.split("*").join("");
				a = a.split(":").join("");
				a = a.split("~").join("");
				a = a.split(";").join("");
				a = a.split(" ").join("+");
				a = a.toLowerCase();
			return a;  
			}
	   
			
