var locpath = window.location.pathname.toString();
var regex = /\/C[0-9]+/;
if (regex.test(loc)) {
loc = loc.replace(regex,'');
}
if (regex.test(locpath)) {
locpath = locpath.replace(regex,'');
}
$(".custServiceTemplate ul.contNav li a").each(function() {
if(this.href == loc || this.href == document.location.protocol + "//" + window.location.hostname + locpath) {
