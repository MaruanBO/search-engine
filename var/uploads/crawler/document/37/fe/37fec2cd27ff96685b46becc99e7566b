<script type="text/javascript">
    var buttonA = $('#js-featured-button-1'),
        buttonB = $('#js-featured-button-2'),
        elementA = $('#js-decks'),
        elementB = $('#js-subjects');

    buttonA.addClass('selected');
    elementB.hide();

    buttonA.on('click', showContent);
    buttonB.on('click', showContent);

//    buttonB.addClass('selected standalone');
//
    function showContent(e) {
        if($(this).attr('id') == buttonA.attr('id')) {
            elementA.show();
            elementB.hide();
            buttonA.addClass('selected');
            buttonB.removeClass('selected');
        } else {
            elementB.show();
            elementA.hide();
            buttonB.addClass('selected');
            buttonA.removeClass('selected');
        }
    }
</script>
