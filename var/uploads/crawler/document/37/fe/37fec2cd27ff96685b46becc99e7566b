<script type="text/javascript">
    var signin = false,
        mainNav,
        navList;

    $(document).ready( function() {
        var isHttps = (("https:" == document.location.protocol) ? "https://" : "http://");
        $("#sign-in").on('click', showLogin);

        mainNav = $('.main-nav');
        navList = $(mainNav.find('ul:hidden'));
        mainNav.on('click', showMenu);
    });

    function showLogin(e){
        e.preventDefault;

        if(signin) {
            $("#sign-in").removeClass('sign-in-open');
            $("#signup-popup").hide();
            signin = false;
        } else {
            $("#sign-in").addClass('sign-in-open');
            $("#signup-popup").show();
            signin = true;
        }
    }

    function showMenu() {
        if(navList.is(':hidden')) {
            navList.show();
        } else {
            navList.hide();
        }
    }

    function gupSB( name ) {
        name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
        var regexS = "[\\?&]"+name+"=([^&#]*)";
        var regex = new RegExp( regexS );
        var results = regex.exec( window.location.href );
        if( results == null )
            return "";
        else
            return results[1];
    }

    function recordOutboundLink(link, action) {
        try{
            var _gaq = _gaq || [];
            var gaAccount = (window.location.host.indexOf('127.0.0.1') > -1 || window.location.host.indexOf('localhost') > -1 || window.location.host.indexOf('staging.studyblue.com') > -1 || window.location.host.indexOf('dev.studyblue.com') > -1) ? gaAccount = "UA-2252691-5" :  gaAccount = "UA-2252691-6";
            _gaq.push(['_setAccount', gaAccount]);
            _gaq.push(['_trackEvent', 'outbound_links', action]);

            // redirect for new apps

                            var seo = "http://itunes.apple.com/us/app/studyblue/id323887414?mt=8";
                        // TapStream redirect to allow

            if(action=="iphone_app"){ setTimeout(document.location = seo, 100); }
            else if(action=="ipad_app"){ setTimeout(document.location = seo, 100); }
            else if(action=="android_app"){ setTimeout('document.location = \"https://market.android.com/details?id=com.studyblue\"', 100); }
        } catch(err) {}
    }

</script>

