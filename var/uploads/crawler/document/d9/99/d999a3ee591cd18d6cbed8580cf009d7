<script>
	for (var i=2;i<=5;i++)
	{
		$('#feature_image_' + i).fadeOut('fast');
	}
	
	var featureLinks = new Array();
	var featureTarget = new Array();
	var featureName = new Array();
	
		
		featureLinks[1] = '/play.do?id=18802';
		featureTarget[1] = '';
		featureName[1] = 'Gold Glove';
	
		
		featureLinks[2] = '/play.do?id=18746';
		featureTarget[2] = '';
		featureName[2] = 'Multiplayer Pool Sharks';
	
		
		featureLinks[3] = '/play.do?id=18829';
		featureTarget[3] = '';
		featureName[3] = 'Candystand U.S. Open Challenge';
	
		
		featureLinks[4] = '/play.do?id=18791';
		featureTarget[4] = '';
		featureName[4] = 'Final Ninja Zero';
	
		
		featureLinks[5] = '/play.do?id=18794';
		featureTarget[5] = '';
		featureName[5] = 'Galactic Gems';
	
	
	function nextFeature()
	{
		var newFeature = Number(Number(selected_box)+1);
		if(newFeature==6) newFeature = 1;
		updateFeature(newFeature,featureLinks[newFeature],featureTarget[newFeature],featureName[newFeature])
	}
	
	var featureTimeout=setTimeout("nextFeature()",15000);
</script>


		
		

		














