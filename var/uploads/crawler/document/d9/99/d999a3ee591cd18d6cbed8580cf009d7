// load rss feed for Meebo
var RSS = {};
RSS.load = function load(url, onComplete) 
{
	$.ajax({
		type: "GET",
		url: url,
		dataType: "xml",
		success: onComplete
	});
}

function onTopTenComplete(xml) {
	var channel = $('channel', xml);
	var data = [];
    $(channel).find('item').each( function() {
    	data.push({
    		value:$(this).find('link').text(),
    		text:$(this).find('title').text(),
			description:$(this).find('description').text(),
    		icon:$(this).find('icon').text()
    	});	    	   
	});
    if(data.length > 0) RSS.data = data;
    if(domReady) Meebo('domReady');
    setTimeout( function() {
    	//createMeeboMenuButton('TopTenGames', '/assets/images/meebo/games_joystick.png', 'Top 10 Games', data , onTopTenSelected);
        createMeeboActionButton('Register', '', 'Register', openReg);
		createMeeboWidgetButton('TopTenGames','/assets/images/meebo/games_joystick.png','Top 10 Games',data);
    }, 3000);
}

function createTopTenGamesWidget(data)
{
	var widget_container = document.createElement('div');
	$(widget_container).attr('id',"topTenGamesWidget");
	$(widget_container).css({'position':'relative',
							'width':'200px',
							'height':'695px',
							'padding-top':'5px',
							'padding-bottom':'5px',
							'overflow':'hidden',
							'background':'url("/assets/images/meebo/widget_bg.gif") no-repeat'});
		
	//var breaker_div = $('<div class="breaker" style="width:196px; height:10px; background-image:url(/assets/images/modules/widgets/news_breaker.gif); background-repeat:repeat-x; background-position: center center; ">&nbsp;</div>');
	
	for(var i=0; i < data.length; i++) 
	{
		var breaker_div = document.createElement('div');
		$(breaker_div).attr({'class':'widget_breaker'});
		$(breaker_div).css({'background':'url("/assets/images/meebo/widget_breaker.gif") repeat-x',
							'width':'190px',
							'height':'1px',
							'margin-left':'5px',
							'margin-top':'5px',
							'margin-bottom':'5px'});
	
		$(breaker_div).text(' ');

		var game_image = document.createElement('img');
		var overlay_image = document.createElement('img');
		$(game_image).attr({'id':'img'+(i+1),
							'src':data[i].icon});
		$(game_image).css({'float':'left',
						   'width':'80px',
						   'height':'60px'});

		$(overlay_image).attr({'id':'overlay_img'+(i+1),
							   'src':"/assets/images/meebo/widget_overlays/"+(i+1)+".png"});
		$(overlay_image).css({'position':'absolute',
						   'width':'80px',
						   'left':'0px',
						   'margin-left':'5px',
						   'height':'60px',
						   'display':'inline',
						   'visibility':'hidden',
						   'z-index':'9999'});
		var game_link = document.createElement('a');
		$(game_link).attr({'id':"game"+i,
							'class':'games',
							'href':data[i].value,
						   'name':""+(i+1)+""});
		
		$(game_link).text(data[i].text);
		$(game_link).css({'display':'block',
						  //'text-indent':'-9999em',
						  'width':'190px',
						  'height':'60px',
						  'margin-left':'5px',
						  'overflow':'hidden'});
		
		var game_item = document.createElement('div');
		
		$(game_item).css({'width':'190px','height':'60px', 'overflow':'hidden'});
				
		var game_title = document.createElement('div');
		$(game_title).css({'color':'#0097E0',
						   'margin-bottom':'5px',
						   'width':'90px',
						   'font-family':'Tahoma, sans-serif',
						   'font-size':'10px',
						   'font-weight':'bold'});
		
		$(game_title).text(data[i].text);
		
		var game_description = document.createElement('div');
		
		$(game_description).text(data[i].description);
		$(game_description).css({'color':'#000000',
								 'height':'60px',
								 'width':'90px',
								 'font-family':'Tahoma, sans-serif',
								 'font-size':'9px',
								 'font-weight':'normal'});
		
		var game_text_wrapper = document.createElement('div');
		$(game_text_wrapper).css({'margin-left':'90px','width':'90px','height':'60px','overflow':'hidden'});
		
		$(game_text_wrapper).append(game_title);
		$(game_text_wrapper).append(game_description);
		
		$(game_item).append(game_image);
		$(game_item).append(overlay_image);
		$(game_item).append(game_text_wrapper);
		$(game_link).prepend(game_item);
		
		$(game_link).click(function() {
		trackThis({type:'event', label:'Meebo Bar|Top 10 - Position '+$(this).attr('name')});
		});
		$(game_link).hover(
		function() {
		
		$(this).find('#overlay_img'+this.name).css({'visibility':'visible'});
		
		$(this).css('background-color','#DDDDDD');
		}, 
		function() {

			$(this).find('#overlay_img'+this.name).css({'visibility':'hidden'});
			
			

			$(this).css('background-color','#F6F6F6');
			

		});
		
						 

		// /*		var child_game_link = $('<a href="'+data[i].value+'" style="display:block"> <div class="game_link_div" style="width:190px; height:60px; margin-bottom:10px;"><img class="game_icon" src="'+data[i].icon+'" style="float:left"/> <div class="title_div">'+data[i].text'</div><div class="description">'+data[i].description+'</div></div></a>'); */
		$(widget_container).append(game_link);
		$(widget_container).append(breaker_div);
	}
	
	return widget_container;
}


function createMeeboMenuButton(id, icon, label, items, callback) 
{
     return Meebo('addButton', {
          id: id, 
          type: 'menu', 
          icon: icon,
          label: label,
          items: items,
          onSelect: callback
     });  
}

function createMeeboActionButton(id, icon, label, callback) 
{
     return Meebo('addButton', {
          id: id, 
          type: 'action', 
          icon: icon,
          label: label,
          onClick: callback
     });  
}

function onTopTenSelected(value) {
    parent.location.href = value;
}

RSS.load("/rss/topgames", onTopTenComplete);
var domReady = false;
$(document).ready(function() {
     domReady = true;
     if(typeof RSS.data !== 'undefined') {
          Meebo('domReady');
     } 
});

function createMeeboWidgetButton(id, icon, label, data)
{
	return Meebo('addButton', {
        id: id,
        type: "widget",
        icon: icon,
        label: label,
        width: 220,
        height: 212,
        onCreate: function(widget, element){ 
            widget.square = createTopTenGamesWidget(data); 
			element.appendChild(widget.square);
		}  
	});
}

