	<script language="JavaScript" type="text/javascript">
	
		var firstId;
		var currentId;
		var jsonString = '[{"id":18699,"isNew":false,"name":"Electric Box 2","plays":"776 Plays","publicGamePlayUrl":"/play/electric-box-2?pre","shortName":"Electric Box 2","thumbnailUrl":"/assets/images/game/thumbs/electric_box_2.jpg"},{"id":18795,"isNew":false,"name":"Amazing Sheriff","plays":"1 Plays","publicGamePlayUrl":"/play/amazing-sheriff?pre","shortName":"Amazing Sheriff","thumbnailUrl":"/assets/images/game/thumbs/amazing_sheriff.jpg"},{"id":17831,"isNew":false,"name":"Billiards","plays":"495 Plays","publicGamePlayUrl":"/play/billiards?pre","shortName":"Billiards","thumbnailUrl":"/assets/images/game/thumbs/billiards.jpg"},{"id":18765,"isNew":false,"name":"Momma&39;s Pizza","plays":"155 Plays","publicGamePlayUrl":"/play/mommas-pizza?pre","shortName":"Momma&39;s Pizza","thumbnailUrl":"/assets/images/game/thumbs/mommas_pizza.jpg"},{"id":18462,"isNew":false,"name":"Monster Trucks 2","plays":"2,119 Plays","publicGamePlayUrl":"/play/monster-trucks-2?pre","shortName":"Monster Trucks 2","thumbnailUrl":"/assets/images/game/thumbs/monster_trucks_2.jpg"},{"id":18775,"isNew":false,"name":"Tri-Peaks Solitaire","plays":"1,458 Plays","publicGamePlayUrl":"/play/tri-peaks-solitaire?pre","shortName":"Tri-Peaks Solitaire","thumbnailUrl":"/assets/images/game/thumbs/tripeaks_solitaire.jpg"},{"id":17882,"isNew":false,"name":"Candy Drops","plays":"63 Plays","publicGamePlayUrl":"/play/candy-drops?pre","shortName":"Candy Drops","thumbnailUrl":"/assets/images/game/thumbs/candy_drops.jpg"},{"id":18778,"isNew":false,"name":"Bingo","plays":"0 Plays","publicGamePlayUrl":"/play/bingo?pre","shortName":"Bingo","thumbnailUrl":"/assets/images/game/thumbs/bingo.jpg"},{"id":18281,"isNew":false,"name":"Vector TD 2","plays":"198 Plays","publicGamePlayUrl":"/play/vector-td-2?pre","shortName":"Vector TD 2","thumbnailUrl":"/assets/images/game/thumbs/vectortd2.jpg"},{"id":17927,"isNew":false,"name":"Air Hockey Extreme","plays":"224 Plays","publicGamePlayUrl":"/play/air-hockey-extreme?pre","shortName":"Air Hockey Extreme","thumbnailUrl":"/assets/images/game/thumbs/air_hockey.jpg"}]';
		var topTenDetail = jQuery.parseJSON(jsonString);
	
		$(document).ready(function() {
			var links = $("a.topTenLink");
			for (var i = 0; i < links.length; i++) {
				$(links[i]).mouseover(topTenOver);
			}
		});
	
		function topTenOver(event) {
			var e = $(this).get(0);
			if (!e.id) return;
			
			var id = e.id.substring(e.id.lastIndexOf("_") + 1, e.id.length); 
			currentId = id;

			writeJsonResponse(id);
			
		}
	
		function writeJsonResponse(currentId) {

			for (var i = 0; i < topTenDetail.length; i++) {
				
				var jo = topTenDetail[i];
				if (jo.id == currentId) {
					$("#thumb_new_container").removeClass("thumb_new");

					$("#top10_detail_thumb").get(0).src = jo.thumbnailUrl;
					$("#top10_gamename").html(jo.shortName.replace("&39;", "'"));
					$("#top10_detail_link").get(0).href = jo.publicGamePlayUrl;
					$("#top10_gamename").get(0).href = jo.publicGamePlayUrl;
					$("#top10_detail_link").get(0).title = 'Play ' + (jo.shortName.replace("&39;", "'"));
					$("#top10_gamename").get(0).title = 'Play ' + (jo.shortName.replace("&39;", "'"));
					$("#top10_detail_thumb").get(0).alt = (jo.shortName.replace("&39;", "'"));
					
					if (jo.isNew) {
						$("#thumb_new_container").addClass("thumb_new");
					}

					break;
									
				}
			}
	
		}
		
	</script>
	
	
