	<script type="text/javascript">

		var currentTrophy = 0;

		function navigateToTrophy(dir) {

			if (dir == "next") {
				currentTrophy++;
			} else {
				currentTrophy--;
			}

			if (currentTrophy > 4) {
				currentTrophy = 0;
			} else if (currentTrophy < 0) {
				currentTrophy = 4;
			}

			$("div.latest_trophy_content").hide();
			$("#latest_trophies_" + currentTrophy).show();

		}
	
	</script>

