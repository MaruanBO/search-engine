<script type="text/javascript">
		
	YUI(_CBC_LOADER).use(function(Y) {
						
		Y.use("yql", "node", "event", "cookie", function(Y){
			Y.on('domready', function(){

				var yp_ip = '72.196.47.22',
				yp_query,
				yp_nav = Y.one("#yp_nav"),
				yp_content = Y.one("#yp_content"),
				yp_more_results = Y.one("#yp-more-results-container"),
				nocache = new Date(),
				yp_tab1 = 'restaurants',
				yp_tab2 = 'dentists',
				yp_tab3 = 'lawyers',
				yp_tab4 = 'carrepairs',
				yp_tab5 = 'physicians',
				yp_tab6 = 'beautysalons',
				window_pathname = window.location.pathname,
				yp_region_regional = '(none)',
				yp_region = getRegion(),
				yp_regions = {
					'bc': {
						'city': 'British Columbia',
						'prov': 'BC'
					},
					'calgary': {
						'city': 'Calgary',
						'prov': 'AB'
					},
					'edmonton': {
						'city': 'Edmonton',
						'prov': 'AB'
					},
					'hamilton': {
						'city': 'Hamilton',
						'prov': 'ON'
					},
					'kamloops': {
						'city': 'Kamploops',
						'prov': 'SK'
					},
					'kitchener-waterloo': {
						'city': 'Kitchener-Waterloo',
						'prov': 'ON'
					},
					'manitoba': {
						'city': 'Manitoba',
						'prov': 'MB'
					},
					'montreal': {
						'city': 'Montreal',
						'prov': 'QC'
					},
					'nb': {
						'city': 'New Brunswick',
						'prov': 'NB'
					},
