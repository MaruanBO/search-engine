	window.ClickTaleSettings.CheckAgentSupport = function (defaultHandler, agent) {
		if (agent.t == agent.IE && (typeof XDomainRequest == 'function' || typeof XDomainRequest == 'object')) {
			// don't use XDR but allow ajax
			agent.XDR = false;
			window.ClickTaleSettings.XHRWrapper.AllowWithGet = true; // allow over get
		}
		return defaultHandler(agent);
	};
	window.ClickTaleSettings.XHRWrapper.RequestFilter = function (method, url) {
		if (url && url.indexOf("programguide/ProgramListFeed.do") > -1) {
			return false;
		}
		return true;
	};
