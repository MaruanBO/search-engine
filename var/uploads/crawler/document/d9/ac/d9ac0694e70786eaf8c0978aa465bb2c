						'city': 'Newfoundland Labrador',
						'prov': 'NL'
					},
					'north': {
						'city': 'North',
						'prov': 'North'
					},
					'ns': {
						'city': 'Nova Scotia',
						'prov': 'NS'
					},
					'ottawa': {
						'city': 'Ottawa',
						'prov': 'ON'
					},
					'pei': {
						'city': 'PrinceEdwardIsland',
						'prov': 'PE'
					},
					'sask': {
						'city': 'Saskatchewan',
						'prov': 'SK'
					},
					'saskatoon': {
						'city': 'Sasatoon',
						'prov': 'SK'
					},
					'sudbury': {
						'city': 'Sudbury',
						'prov': 'ON'
					},
					'thunderbay': {
						'city': 'Thunder Bay',
						'prov': 'ON'
					},
					'toronto': {
						'city': 'Toronto',
						'prov': 'ON'
					},
					'windsor': {
						'city': 'Windsor',
						'prov': 'ON'
					}
				};
				
				var truncate = function(trunc_var,char_limit){
					if(trunc_var.length > char_limit){
						var trunc = trunc_var.slice(0,(char_limit-3))+'&#8230;';
						return trunc;
					} else {
						var trunc = trunc_var;
						return trunc;
					}
				};

				function load_yp_feed(){

					yp_content.set("innerHTML", '<h4>Loading <a href="http://www.yellowpages.ca" target="_blank">YellowPages.ca<span>&#8482;</span></a> listings&#8230;</h4>');

					yp_query = Y.YQL('select * from xml where url="http://api.yellowapi.com/FindBusiness/?what='+yp_tab1.replace(' ','')+'&where='+yp_regions[yp_region].city.replace(' ','')+'&apikey=ru3uzhujs5w5gsydnnsyjm7u&fmt=XML&UID='+yp_ip+'&sort=WEB&pgLen=3&ypid=cbc&'+nocache.getTime()+'"', function(o){

						var tab1 = o.query.results.SearchResults.Summary.Search.Parameter[0].content,
						regiontext = o.query.results.SearchResults.Summary.Search.Parameter[1].content;

						var listings1 = o.query.results.SearchResults.Listings.Listing;

						output = '',
						output_content = '',
						output_more_results = '';

						output += '<li class="first active"><a href="#">'+yp_tab1+'</a></li>';
						output += '<li><a href="#">'+yp_tab2+'</a></li>';
						output += '<li><a href="#">'+yp_tab3+'</a></li>';
						if(yp_region_regional != "(none)"){
							if(yp_region_regional != "hamilton"){
								output += '<li><a href="#">'+yp_tab4.replace('carrepairs','Car Repairs')+'</a></li>';
							}
						}
						if(yp_region_regional != "(none)"){
							if(yp_region_regional == "north"){
								output += '<li><a href="#">'+yp_tab5+'</a></li>';
								output += '<li><a href="#">'+yp_tab6.replace('beautysalons','Beauty Salons')+'</a></li>';
							}
						}

						for (var j=0; j<listings1.length; j++){

							if(listings1[j].Address.City != null){
								listings1[j].Address.City.toLowerCase();
							} else {
								listings1[j].Address.City = regiontext;
							}

							if(listings1[j].Prov != null){
								listings1[j].Prov.toLowerCase();
							} else {
								listings1[j].Prov = yp_regions[yp_region].prov;
							}

							switch(j){
								case 0:
									output_content += '<li class="first">';
									break;
								case listings1.length-1:
									output_content += '<li class="last">';
									break;
								default:
									output_content += '<li>';
									break;
							}

							output_content += '<header><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp&ypid=cbc" target="_blank" title="Yellow Pages"></a></header>';

							if(yp_region_regional == "north"){
								output_content += '<section><a class="yp_name" href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">'+listings1[j].Name+'</a>';
							} else {
								output_content += '<section><a class="yp_name" href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">'+truncate(listings1[j].Name,30)+'</a>';
							}

							output_content += '<aside><div class="yp_deals"><p>'+listings1[j].Address.City+', '+listings1[j].Prov+'</p>';

							output_content += '<span class="yp_stars ';
							switch (listings1[j].Ratings){
								case "0.0":
									output_content += 'yp_stars_0_0';
									break;
								case "0.5":
									output_content += 'yp_stars_0_5';
									break;
								case "1.0":
									output_content += 'yp_stars_1_0';
									break;
								case "1.5":
									output_content += 'yp_stars_1_5';
									break;
								case "2.0":
									output_content += 'yp_stars_2_0';
									break;
								case "2.5":
									output_content += 'yp_stars_2_5';
									break;
								case "3.0":
									output_content += 'yp_stars_3_0';
									break;
								case "3.5":
									output_content += 'yp_stars_3_5';
									break;
								case "4.0":
									output_content += 'yp_stars_4_0';
									break;
								case "4.5":
									output_content += 'yp_stars_4_5';
									break;
								case "5.0":
									output_content += 'yp_stars_5_0';
									break;
								default:
									output_content += 'yp_stars_0_0';
									break;
							}
							output_content += '"></span></div>';

							if(listings1[j].Deal != null){
								output_content += '<span class="yp_deals_tag"></span><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">';

								/* for cbc.ca page */
								if(yp_region_regional == "(none)"){
									output_content += truncate(listings1[j].Deal.content,15);
								} else if(yp_region_regional == "north"){
									output_content += listings1[j].Deal.content;
								} else {
									output_content += truncate(listings1[j].Deal.content,20);
								}
	
								output_content += '<span class="yp_deals_plus"></span></a>';
							}

							output_content += '<footer><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">More info</a></footer></aside>';

							output_content += '</section></li>';
						} /* for j */
							
						output_more_results = '<a id="yp-more-results" href="http://www.yellowpages.ca/search/si/1/'+tab1+'/'+regiontext.toLowerCase()+'?showDD=true&tdcam=cbc&tdchn=web_widget_serp&ypid=cbc" target="_blank">See more results >></a>';
	
						//Output the string to the page:
						yp_nav.set("innerHTML", output);
						yp_more_results.set("innerHTML", output_more_results);
						yp_content.set("innerHTML", output_content);

					}); //yp_query

				} // load_yp_feed

				function click_yp_feed(yp_tab){

					yp_content.set("innerHTML", '<h4>Loading <a href="http://www.yellowpages.ca" target="_blank">YellowPages.ca<span>&#8482;</span></a> listings&#8230;</h4>');

					yp_query = Y.YQL('select * from xml where url="http://api.yellowapi.com/FindBusiness/?what='+yp_tab.replace(' ','')+'&where='+yp_regions[yp_region].city.replace(' ','')+'&apikey=ru3uzhujs5w5gsydnnsyjm7u&fmt=XML&UID='+yp_ip+'&sort=WEB&pgLen=3&ypid=cbc&'+nocache.getTime()+'"', function(o){
	
						var tab = o.query.results.SearchResults.Summary.Search.Parameter[0].content,
						regiontext = o.query.results.SearchResults.Summary.Search.Parameter[1].content;

						var listings1 = o.query.results.SearchResults.Listings.Listing;
	
						output_content = '',
						output_more_results = '';

						for (var j=0; j<listings1.length; j++){

							if(listings1[j].Address.City != null){
								listings1[j].Address.City.toLowerCase();
							} else {
								listings1[j].Address.City = regiontext;
							}

							if(listings1[j].Prov != null){
								listings1[j].Prov.toLowerCase();
							} else {
								listings1[j].Prov = yp_regions[yp_region].prov;
							}

							switch(j){
								case 0:
									output_content += '<li class="first">';
									break;
								case listings1.length-1:
									output_content += '<li class="last">';
									break;
								default:
									output_content += '<li>';
									break;
							}

							output_content += '<header><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank" title="Yellow Pages"></a></header>';

							if(yp_region_regional == "north"){
								output_content += '<section><a class="yp_name" href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">'+listings1[j].Name+'</a>';
							} else {
							output_content += '<section><a class="yp_name" href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">'+truncate(listings1[j].Name,30)+'</a>';
							}

							output_content += '<aside><div class="yp_deals"><p>'+listings1[j].Address.City+', '+listings1[j].Prov+'</p>';

							output_content += '<span class="yp_stars ';
							switch (listings1[j].Ratings){
								case "0.0":
									output_content += 'yp_stars_0_0';
									break;
								case "0.5":
									output_content += 'yp_stars_0_5';
									break;
								case "1.0":
									output_content += 'yp_stars_1_0';
									break;
								case "1.5":
									output_content += 'yp_stars_1_5';
									break;
								case "2.0":
									output_content += 'yp_stars_2_0';
									break;
								case "2.5":
									output_content += 'yp_stars_2_5';
									break;
								case "3.0":
									output_content += 'yp_stars_3_0';
									break;
								case "3.5":
									output_content += 'yp_stars_3_5';
									break;
								case "4.0":
									output_content += 'yp_stars_4_0';
									break;
								case "4.5":
									output_content += 'yp_stars_4_5';
									break;
								case "5.0":
									output_content += 'yp_stars_5_0';
									break;
								default:
									output_content += 'yp_stars_0_0';
									break;
							}
							output_content += '"></span></div>';

							if(listings1[j].Deal != null){
								output_content += '<span class="yp_deals_tag"></span><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">';

								/* for cbc.ca page */
								if(yp_region_regional == "(none)"){
									output_content += truncate(listings1[j].Deal.content,15);
								} else if(yp_region_regional == "north"){
									output_content += listings1[j].Deal.content;
								} else {
									output_content += truncate(listings1[j].Deal.content,20);
								}
	
								output_content += '<span class="yp_deals_plus"></span></a>';
							}

							output_content += '<footer><a href="'+listings1[j].MerchantUrl+'&tdcam=cbc&tdchn=web_widget_mp" target="_blank">More info</a></footer></aside>';

							output_content += '</section></li>';
						} /* for j */
							
						output_more_results = '<a id="yp-more-results" href="http://www.yellowpages.ca/search/si/1/'+tab+'/'+regiontext.toLowerCase()+'?showDD=true&tdcam=cbc&tdchn=web_widget_serp&ypid=cbc" target="_blank">See more results >></a>';

						//Output the string to the page:
						yp_more_results.set("innerHTML", output_more_results);
						yp_content.set("innerHTML", output_content);

					}); //yp_query

				} // click_yp_feed

				load_yp_feed();

				/* YP tabs click handler */
				function yp_tab_click(e){
					var yp_tabs = Y.all('#yp_nav li');

					if(this.ancestor('li').hasClass('active') == false){
						
						for(var i=0; i<yp_tabs.size(); i++){
							yp_tabs.item(i).removeClass('active');
						}
						this.ancestor('li').addClass('active');

						click_yp_feed(this.getContent());

					}
					e.preventDefault();
				}

				Y.on("click", yp_tab_click, "#yp_nav li a");

				/* Get region cookie value */
				function getRegion(){
					if(yp_region_regional == "(none)"){
						if(window_pathname != "/hamilton/economy/"){
							var key = '',
						    regionCookie = Y.Cookie.get("cbclocal");
							if (regionCookie != null){
							    regionCookie = Y.Cookie.get("cbclocal").split('&');
							    for(var i=0; i<regionCookie.length; i++){
									key = regionCookie[i].split('=');
									if(key[0] == 'region' && key[1].length > 0){
										return key[1];
										break;
									}
							    }
							} else {
								return "windsor";
							}
						} else {
							return "hamilton";
						}
					} else {
						return yp_region_regional;
					}
				} // getRegion

			}); // domready
		}); // use

	});
</script>
