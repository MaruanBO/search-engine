	<script type="text/javascript">
		function getQueryEmail() {
			var q=window.location.search;
			if (q.length>0) {
				q=q.substring(1);
				qs=q.split("&");
				for (i=0; i<qs.length; i++) {
					t=qs[i].split('=');
					if (t[1].indexOf(".")>0 && t[1].indexOf("@")>0) {
						cy.Custom1="Guest";
						cy.UserId=unescape(t[1]);
						cy.FunnelLevel="0";
						cy_getImageSrc();
						break;
					}
				}
			}
		}		
		getQueryEmail();
  </script>
   
		
		
		


