					if ($('chat-btn-shoedog') != undefined){
					            $('chat-btn-shoedog').src = kanaProtocol+'www.roadrunnersports.com/rrs/img/header/KanaClear.png';
					            $('chat-btn-shoedog').setAttribute('style', '');
					}
					if ($('chat-btn-content') != undefined){
					            $('chat-btn-content').src = kanaProtocol+'www.roadrunnersports.com/rrs/img/header/KanaClear.png';
					            $('chat-btn-content').setAttribute('style', '');
					}
					if ($('chat-btn-cart') != undefined){
					            $('chat-btn-cart').src = kanaProtocol+'www.roadrunnersports.com/rrs/img/header/KanaClear.png';
					            $('chat-btn-cart').setAttribute('style', '');
					}
					
	            }
	        }
	      });
      
      		function openChat() { chat.startChat(); }
      
    	</script>
    
   
    
    
    
    
     
		
				
		
				
				
					
					
				
					
					
							
									
							

