<script>
	var timeoutId;
	var searchbarId;
	function AWeberSubscribePop() {
		var email2 = document.getElementById('stemailp');
		var durl = 'http://www.sliptalk.com/wp-content/themes/jarida';
		var load = document.getElementById('aloaderp');
		durl += '/addSub.php';
		load.style.display = 'block';

		if(email2.value != "" && email2.value != "Enter Your Email Address.") {

			var request = jQuery.ajax({
			  url: durl,
			  type: "POST",
			  data: {email : email2.value},
			  dataType: "html"
			});

			request.done(function(msg) {
			  var c = document.getElementById('fContentp');
			  var f = document.getElementById('thanksp');

			  c.style.display = 'none';
			  load.style.display = 'none';
			  f.style.display = 'block';
			});
		} else {
			load.style.display = 'none';
		}
	}

	function MegaOn(id) {
		if (!timeoutId) {
				timeoutId = window.setTimeout(function() {
					timeoutId = null; // EDIT: added this line
					HideAll();
					jQuery(".relMenu").fadeIn();
					jQuery("#mega_" + id).fadeIn();
			   }, 500);
			}
	}
	function MegaOut(id) {
		if (timeoutId) {
				window.clearTimeout(timeoutId);
				timeoutId = null;
			}
	}
	function SearchOut() {
		if (searchbarId) {
				window.clearTimeout(searchbarId);
				searchbarId = null;
			}
	}
	function ShowSearchBar() {
		if (!searchbarId) {
				searchbarId = window.setTimeout(function() {
					searchbarId = null; // EDIT: added this line
					jQuery("#searchBar").fadeIn();
			   }, 500);
			}
	}
	function HideSearchBar() {
		jQuery("#searchBar").fadeOut();
	}
	function HideMegaMenu(id) {
		jQuery(".relMenu").fadeOut();
	}
	
	function HideAll() {
		var div = document.getElementById("newNavLinks");
		var divs = div.getElementsByTagName("div");
		for(var x = 0; x < divs.length; x++) {
			if(divs[x].id.substring(0,5) == "mega_") {
				divs[x].style.display = 'none';
			}
		}
	}
</script>
