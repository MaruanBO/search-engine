<style>
    .shogun-widget.image-map .gw-cf-style-bg {
        background-position:center center;
        background-repeat:no-repeat;
    }
    #image-map-ns_1X9A4QPEBTBET0CFV7Z9_7404_ .cropped-image-map-size {
	position:relative;
	overflow:hidden;
	width:100%;
	height:170px;
}
