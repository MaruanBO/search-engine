<script type="text/javascript">
function getPrices(data) { 
    console.log(data);
    return data; 
}

function getSpots() { 
    if(typeof($j) == "undefined")
        $j = jQuery;

    if(window.location.protocol == 'https:')
        var port = 4343;
    else 
        var port = 8383;


    $j.ajax({ 
        type: "GET",
            url: "//bulliongoldprices.com/jsonp_spots",
//            url: "//bulliongoldprices.com:" + port + "/jsonp_spots",
//            url: "//www.usgoldbureau.com/spots.php?rand="+Math.floor(Math.random()*1111111111111111111111),
//            url: "//localhost:8383/jsonp_spots",
            //url: "https://invest.usgoldbureau.com:8383/spots?rand="+Math.floor(Math.random()*1111111111111111111111),
            dataType: "jsonp",
            success: function(response) {
                data = new Object();
                data = JSON.parse(response);

//                console.log(data);
                $j('.goldAsk').html("$" + data.goldAsk.toFixed(2));
                $j('.goldChange').html("$" + data.goldChange.toFixed(2));
                
                if(data.goldChange > 0) {
                    $j('#goldArrow').removeClass('downarrow').addClass('uparrow');
                    $j('.goldArrow').removeClass('downarrow').addClass('uparrow');
                } else {
                    $j('#goldArrow').removeClass('uparrow').addClass('downarrow');
                    $j('.goldArrow').removeClass('uparrow').addClass('downarrow');
                }

                $j('.silverAsk').html("$" + data.silverAsk.toFixed(2));
                $j('.silverChange').html("$" + data.silverChange.toFixed(2));
                
                if(data.silverChange > 0) {
                    $j('#silverArrow').removeClass('downarrow').addClass('uparrow');
                    $j('.silverArrow').removeClass('downarrow').addClass('uparrow');
                } else {
                    $j('#silverArrow').removeClass('uparrow').addClass('downarrow');
                    $j('.silverArrow').removeClass('uparrow').addClass('downarrow');
                }

                $j('.platinumAsk').html("$" + data.platinumAsk.toFixed(2));
                $j('.platinumChange').html("$" + data.platinumChange.toFixed(2));

                if(data.platinumChange > 0) {
                    $j('#platinumArrow').removeClass('downarrow').addClass('uparrow');
                    $j('.platinumArrow').removeClass('downarrow').addClass('uparrow');
                } else {
                    $j('#platinumArrow').removeClass('uparrow').addClass('downarrow');
                    $j('.platinumArrow').removeClass('uparrow').addClass('downarrow');
                }

                $j('.palladiumAsk').html("$" + data.palladiumAsk.toFixed(2));
                $j('.palladiumChange').html("$" + data.palladiumChange.toFixed(2));
                
                if(data.palladiumChange > 0) {
                    $j('#palladiumArrow').removeClass('downarrow').addClass('uparrow');
                    $j('.palladiumArrow').removeClass('downarrow').addClass('uparrow');
                } else {
                    $j('#palladiumArrow').removeClass('uparrow').addClass('downarrow');
                    $j('.palladiumArrow').removeClass('uparrow').addClass('downarrow');
                }

                $j('.update').html(data.timestamp + " CT");

            },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    console.log("error " + textStatus);
                    console.log(errorThrown);
                }
    });
}
getSpots();
setInterval(getSpots, 5000);

</script>
        </div>
