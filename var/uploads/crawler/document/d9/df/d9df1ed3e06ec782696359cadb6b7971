<script type="text/javascript">
jQuery(document).ready(function(){

		jQuery("#mynxx-contactFormContainer").show();

                jQuery('a[title="Log In"]').click(function(e){
                    e.preventDefault();
                    if (jQuery("#mynxx-contactForm").is(":hidden")){
                        jQuery("#mynxx-contactForm").slideToggle("normal"),
			jQuery("#mynxx-login-content").fadeIn("slow");
                    }
                    else{
                        jQuery("#mynxx-contactForm").slideUp("normal"),
			jQuery("#mynxx-login-content").fadeOut("fast");
                    }
                });

									var reflink = jQuery('a[title="Log In"]', this).attr('href').split("login/");
					var reflink = reflink[1]; var reflink = "https://invest.usgoldbureau.com/customer/account/loginPost/" + reflink;
					jQuery("#mynxx-login-content form").attr('action',reflink);
				
            });
</script>

