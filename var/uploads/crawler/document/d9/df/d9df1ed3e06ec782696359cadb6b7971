    $j(function(){
            $j("li.level1, li.level2, li.level3").mouseenter(function(){
                $j(this).stop().prepend('<div class="hoverbg"></div>');
            }),
            $j("li.level1, li.level2, li.level3").mouseleave(function(){
                $j("div.hoverbg").stop().fadeOut(300, function() {
                    $j(this).remove();
                });
            })
        });
	if (jQuery.browser.msie) {
        if(parseInt(jQuery.browser.version) == 6) {
			$j("div.hoverbg").remove();
			};
	};
});
</script>
