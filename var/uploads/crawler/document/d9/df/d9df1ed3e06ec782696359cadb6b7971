        <script type="text/javascript">
 var $j = jQuery.noConflict();
// Truncate
$j(function() {
      $j(".truncate_slider_title").truncate({max_length: 60 });
      $j(".truncate_slider_desc").truncate({max_length: 250 });
 });
</script>
                
                
