		function tempChanged(valueTemp,obj){
		obj.checked = true;
		if(valueTemp=='temp'){
				document.getElementById("stopforwarding").style.display = 'block';
			}else{
				document.getElementById("stopforwarding").style.display = 'none';
				document.moveInfoForm.disconnectMoveDate.value='';
			}					
		}
		function checkMoveType(obj){
			if(obj[1].checked){
				document.getElementById("stopforwarding").style.display = 'block';
			}else{
				document.getElementById("stopforwarding").style.display = 'none';
				document.moveInfoForm.disconnectMoveDate.value='';
			}
		}
		function checkMoveWithType(moveWith,obj){
		 	obj.checked = true;
			if(moveWith == 'Business'){
				document.getElementById("businessDiv").style.display = '';
			}else{
				document.getElementById("businessDiv").style.display = 'none';
				document.moveInfoForm.businessName.value='';
			}
		}
		function checkMoveWith(obj){
			if(obj[2].checked){
				document.getElementById("businessDiv").style.display = '';
			}else{
				document.getElementById("businessDiv").style.display = 'none';
				document.moveInfoForm.businessName.value='';
			}
		}
		var isNN = (navigator.appName.indexOf("Netscape")!=-1);
		function autoTab(input,len, e) {
			var keyCode = (isNN) ? e.which : e.keyCode; 
			var filter = (isNN) ? [0,8,9] : [0,8,9,16,17,18,37,38,39,40,46];
			if(input.value.length >= len && !containsElement(filter,keyCode)) {
			input.value = input.value.slice(0, len);
			input.form[(getIndex(input)+1) % input.form.length].focus();
		}
		function containsElement(arr, ele) {
			var found = false, index = 0;
			while(!found && index < arr.length)
			if(arr[index] == ele)found = true;else  index++;
			 return found;}
		     function getIndex(input) {var index = -1, i = 0, found = false;while (i < input.form.length && index == -1)
		     if (input.form[i] == input)index = i;else i++;	
		     return index;
		     }
		     return true;
		 }
		 function showPopUp(popupObj,windowObj){
			 document.getElementById(popupObj).style.display='block';
			 document.getElementById(windowObj).style.display='block';
			 document.getElementsByTagName('html')[0].style.height = '100%';
			 document.getElementsByTagName('html')[0].style.width = '100%';
			 document.getElementsByTagName('html')[0].style.overflow = 'hidden';
		 }
		 function hidePopUp(popupObj,windowObj){
			 document.getElementById(popupObj).style.display='none';
			 document.getElementById(windowObj).style.display='none';
			 document.getElementsByTagName('html')[0].style.overflow = '';
		 }
		 function editOrder(){
			 if(document.userLoginForm.email.value == ""){
				 alert("Please enter email address.");
				 document.userLoginForm.email.focus();
				 return false;
			 }
			 if(document.userLoginForm.password.value == ""){
				 alert("Please enter your confirmation code.");
				 document.userLoginForm.password.focus();
				 return false;
			 }
			 return true;

		 }

		 function trim(s)
		 {
		 	return rtrim(ltrim(s));
		 }

		 function ltrim(s)
		 {
		 	var l=0;
		 	while(l < s.length && s[l] == ' ')
		 	{	l++; }
		 	return s.substring(l, s.length);
		 }

		 function rtrim(s)
		 {
		 	var r=s.length -1;
		 	while(r > 0 && s[r] == ' ')
		 	{	r-=1;	}
		 	return s.substring(0, r+1);
		 }
		 		 

		 function DoCustomValidation()
		 {
		 var frm = document.forms["moveInfoForm"];
		 frm.confirmEmail.value = trim(frm.confirmEmail.value);
		 frm.email.value = trim(frm.email.value);
		 if(frm.confirmEmail.value != frm.email.value)
		 {
		 	alert("Please confirm your email");
		 	return false;
		 }
		 else
		 {
		 	return true;
		 }
		 
		 }
		 function DoCustom_realestate_Validation(){
			 if(document.moveInfoForm.real_estate_contact.checked){
					if(document.moveInfoForm.realesatetimeframe.value=="-1"){
						alert("please select time frame");
						return false;
					}
					else if(document.moveInfoForm.realesatetype.value=="-1"){
						alert("please select state type");
						return false;
					}
					else if(document.moveInfoForm.realesatepricerange.value=="-1"){
						alert("please select price range");
						return false;
					}
					else if(document.moveInfoForm.realesatepurchasetype.value=="-1"){
						alert("please select purchase type");
						return false;
					}
					else{
						return true;
					}
				}
			 else{
				 return true;
			 }
			 
		 }

function addressLookup(addressType){

	var address;
	var city;
	var state;
	var zipcode;
	var aType;
	if(addressType == 'old'){
		$('#lookupResold').html("");
		address = document.moveInfoForm.oldAddress.value;
		city  = document.moveInfoForm.oldCity.value;
		state = document.moveInfoForm.oldState.value;
		zipcode = document.moveInfoForm.oldZip.value;
	}else{
		$('#lookupResnew').html("");
		address = document.moveInfoForm.newAddress.value;
		city  = document.moveInfoForm.newCity.value;
		state = document.moveInfoForm.newState.value;
		zipcode = document.moveInfoForm.newZip.value;
	}
	
	if(zipcode){
		jQuery.ajax({
			 url: "addresslookup.jsp",
			 global: false,
			 dataType: 'json',
		     type: "POST",
			 data: ({addressType:addressType,address:address, city:city, state: state, zipcode : zipcode}),
		     success: function(data){
		         var json = data;		         
		         if(json.hasOwnProperty('addresstype')){
		        	 aType = json['addresstype'];
		         }
		         
		         if(aType == 'old'){
			    	 if(json.hasOwnProperty('address')){
			    		document.moveInfoForm.oldAddress.value =  json['address'];
			    	 }
			    	 
			    	 if(json.hasOwnProperty('city')){
				    		document.moveInfoForm.oldCity.value =  json['city'];
				     }
			    	 if(json.hasOwnProperty('state')){
				    		document.moveInfoForm.oldState.value =  json['state'];
				     }
			    	 
			    	 if(json.hasOwnProperty('zipcode5')){
				    		document.moveInfoForm.oldZip.value =  json['zipcode5'];
				     }
			    	 
 			    	 if(json.hasOwnProperty('returnText')){
 			    		 $('#lookupResold').html(json['returnText']);
 				     }
			    	 
		         }else{
		       	 	if(json.hasOwnProperty('address')){
			    		document.moveInfoForm.newAddress.value =  json['address'];
			    	 }
			    	 
			    	 if(json.hasOwnProperty('city')){
				    		document.moveInfoForm.newCity.value =  json['city'];
				     }
			    	 if(json.hasOwnProperty('state')){
				    		document.moveInfoForm.newState.value =  json['state'];
				     }
			    	 
			    	 if(json.hasOwnProperty('zipcode5')){
				    		document.moveInfoForm.newZip.value =  json['zipcode5'];
				     }
			    	 
			    	 if(json.hasOwnProperty('returnText')){
 			    		 $('#lookupResnew').html(json['returnText']);
 				     }
		         }
		   }
		});
	}
}

	
