function dosubmit(){
	 
// 	document.moveInfoForm.email.value = trim(document.moveInfoForm.email.value);
// 	document.moveInfoForm.confirmEmail.value = trim(document.moveInfoForm.confirmEmail.value);
	document.moveInfoForm.real_estate_contact.value=document.moveInfoForm.real_estate_contact.checked; 
	
	if($('#lookupResold').text() == 'Note: Apt, suite, or box number is missing.'){
		alert("Please add an apartment/unit number to old address");
		document.moveInfoForm.oldAddress.focus();
		return false;
	}else if($('#lookupResnew').text() == 'Note: Apt, suite, or box number is missing.'){
		alert("Please add an apartment/unit number to new address");
		document.moveInfoForm.newAddress.focus();
		return false;
	}
}

