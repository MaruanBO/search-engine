			//popup
$('a[name=modal]').click(function(e) {
		//Cancel the link behavior
		e.preventDefault();
		
		//Get the A tag
		var id = $(this).attr('href');
	
		//Get the screen height and width
		var maskHeight = $(document).height();
		var maskWidth = $(window).width();
	
		//Set heigth and width to mask to fill up the whole screen
		$('#mask').css({'width':maskWidth,'height':maskHeight});
		
		//transition effect		
		$('#mask').fadeIn(1000);	
		$('#mask').fadeTo("slow",0.8);	
	
		//Get the window height and width
		var winH = $(window).height();
		var winW = $(window).width();
              
		//Set the popup window to center
		$(id).css('top',  winH/2-$(id).height()/2);
		$(id).css('left', winW/2-$(id).width()/2);
	
		//transition effect
		$(id).fadeIn(2000); 
	
	});
	
	//if close button is clicked
	$('.window .close').click(function (e) {
		//Cancel the link behavior
		e.preventDefault();
		
		$('#mask').hide();
		$('.window').hide();
	});		
	
	//if mask is clicked
	$('#mask').click(function () {
		$(this).hide();
		$('.window').hide();
	});		
	
	$('#oldAddress').bind('input', function(){
		$('#lookupResold').html('');
	});

	$('#newAddress').bind('input', function(){
		$('#lookupResnew').html('');
	});
			
});
		
		function gotoUSPS(){						
			if(document.moveInfoForm.firstName.value !='' && document.moveInfoForm.email.value !='' && false){
				var frm = document.forms["moveInfoForm"];	
				var moveTypeVal = "Permanent";
				var moveTypeWithVal = "Individual";
				var disconnectMoveDateVal = "";
				var businessNameVal = "";
				if(frm.moveType[1].checked){
					moveTypeVal = "Temporary";
					disconnectMoveDateVal = frm.disconnectMoveDate.value;
				}
				if(frm.moveTypeWith[1].checked){
					moveTypeWithVal = "Family";
				}else if(frm.moveTypeWith[2].checked){
					moveTypeWithVal = "Business";
					businessNameVal = frm.businessName.value;
				}
				$.post("saveinfoforusps.jsp", {
					 moveType: moveTypeVal,
					 moveTypeWith: moveTypeWithVal,
					 disconnectMoveDate: disconnectMoveDateVal,
					 businessName: businessNameVal,
					 firstName: frm.firstName.value,
					 lastName: frm.lastName.value,		
					 middleName: frm.middleName.value,
					 email: frm.email.value,		 
					 moveDate: frm.moveDate.value,
					 oldAddress: frm.oldAddress.value,
					 oldCity: frm.oldCity.value,
					 oldState: frm.oldState.value,
					 oldZip: frm.oldZip.value,
					 newAddress: frm.newAddress.value,
					 newCity: frm.newCity.value,
					 newState:  frm.newState.value,
					 newZip: frm.newZip.value,
					 oldPhone1: frm.oldPhone1.value,
					 oldPhone2: frm.oldPhone2.value,
					 oldPhone3: frm.oldPhone3.value,
					 newPhone1: frm.newPhone1.value,
					 newPhone2: frm.newPhone2.value,
					 newPhone3: frm.newPhone3.value
					
				}, function(data) {
					if (data == null) {
						message(container, "Data Not Saved...");
					} else {		
						window.location ="https://moversguide.usps.com/icoa/icoa-main-flow.do?execution=e1s1";
					}
				});
				
			}else{
				var newtab = window.open();
				newtab.location ="https://moversguide.usps.com/icoa/icoa-main-flow.do?execution=e1s1";
			}			
			
		}
		
		 function validateForm(frm){
			 	if(trim(frm.firstName.value)==""){
					alert("Please enter first name.");
					frm.firstName.focus();
					return false;
				}else if(trim(frm.lastName.value)==""){
					alert("Please enter last name.");
					frm.lastName.focus();
					return false;
				}else if(trim(frm.email.value)==""){
					alert("Please enter valid email address.");
					frm.email.focus();
					return false;
				}else if(trim(frm.oldAddress.value)==""){
					alert("Please enter a old Address.");
					frm.oldAddress.focus();
					return false;
				}else if(trim(frm.oldCity.value)==""){
					frm.oldCity.focus();
					alert("Please enter old City.");
					return false;
				}else if(trim(frm.oldZip.value)==""){
					frm.oldZip.focus();
					alert("Please enter a valid old zip code.");
					return false;
				}else if(trim(frm.oldPhone1.value)==""){
					frm.oldPhone1.focus();
					alert("Please enter a valid old phone number.");
					return false;
				}else if(trim(frm.oldPhone2.value)==""){
					frm.oldPhone2.focus();
					alert("Please enter a valid old phone number.");
					return false;
				}else if(trim(frm.oldPhone3.value)==""){
					frm.oldPhone3.focus();
					alert("Please enter a valid old phone number.");
					return false;
				}else if(trim(frm.newAddress.value)==""){
					frm.newAddress.focus();
					alert("Please enter a new address.");
					return false;
				}else if(trim(frm.newCity.value)==""){
					frm.newCity.focus();
					alert("Please enter a new City.");
					return false;
				}else if(trim(frm.newZip.value)==""){
					frm.newZip.focus();
					alert("Please enter a valid new zip code.");
					return false;
				}else {
					return true;
				}
				
			}
		
		
