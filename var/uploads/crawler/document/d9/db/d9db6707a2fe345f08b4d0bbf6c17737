    <script type="text/javascript">
        usingNamespace("Biz.HomePage2013")["MarketplaceSpecial"] = function(){
            var config={},
                $Isloop= true;
                
            config.CoremetricData ="";
            config.DomID="MarketplaceSpecial";

            var container=new Swiper("#"+config.DomID,{
                pagination:$Isloop?"#"+config.DomID+" .pagination":"",
                loop:$Isloop,
                simulateTouch:$Isloop,
                onSlideChangeEnd: function () {
                    if (config.CoremetricData != "") {
                        var activeSlideIndex = $Isloop ? container.activeSlide + 1 : container.activeSlide;
                        var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));

                        if (config.CoremetricData && config.CoremetricData.length > 0) {
                            Biz.Common.Coremetric.send(currentID, config.CoremetricData,0);
                            Biz.Common.SiteCatalyst.sendCache("list1", "dynamic recommendation impression");
                        }
                    }
                }
            });

            jQuery(function () {
                var activeSlideIndex=$Isloop?container.activeSlide+1 :container.activeSlide;
                var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));
                if (config.CoremetricData != "") {
                    Biz.Common.Coremetric.send(currentID, config.CoremetricData,0);
                }
            });

            if($Isloop){
                jQuery("#"+config.DomID+" .arrow-left").click(function (e) {
                    e.preventDefault();
                    container.swipePrev();
                });

                jQuery("#"+config.DomID+" .arrow-right").click(function (e) {
                    e.preventDefault();
                    container.swipeNext();
                });
            }
        }();
    </script>
