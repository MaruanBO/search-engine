        usingNamespace("Biz.HomePage2013")["Spotlight"] = function(){
            var config={},
                $Isloop= true;
                
            config.CoremetricData =[{"position":"Spotlight1","dataList":[{"cMData":"Spotlight-_-9SIA4642ET6147-_-02192015_1","itemNumber":"9SIA4642ET6147"}]},{"position":"Spotlight2","dataList":[{"cMData":"Spotlight-_-9SIA29R1M70204-_-02192015_2","itemNumber":"9SIA29R1M70204"}]},{"position":"Spotlight3","dataList":[{"cMData":"Spotlight-_-9SIA5HJ2K36929-_-02192015_3","itemNumber":"9SIA5HJ2K36929"}]}];
            config.DomID="Spotlight";

            var container=new Swiper("#"+config.DomID,{
                loop:$Isloop,
                simulateTouch:$Isloop,
                onSlideChangeEnd: function () {
                    //idangeor's bug when loop is true
                    var activeSlideIndex=$Isloop?container.activeSlide+1 :container.activeSlide;
                    var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));

                    if (config.CoremetricData && config.CoremetricData.length > 0) {
                        Biz.Common.Coremetric.send(currentID, config.CoremetricData);
                    }
                }
            });

            jQuery(function () {
                //idangeor's bug when loop is true
                var activeSlideIndex=$Isloop?container.activeSlide+1 :container.activeSlide;
                var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));

                Biz.Common.Coremetric.send(currentID, config.CoremetricData);
            });

            if($Isloop){
                jQuery("#"+config.DomID+" .arrow-left").click(function (e) {
                    e.preventDefault();
                    container.swipePrev();
                });

                jQuery("#"+config.DomID+" .arrow-right").click(function (e) {
                    e.preventDefault();
                    container.swipeNext();
                });
            }
        }();
    </script>
</div>


    <div class="callToAction">
        <a href="http://www.newegg.com/marketplace/deals" title="What's hot in Marketplace"></a>
    </div>


</div>

            </div>

        </div>
    </div>
    
