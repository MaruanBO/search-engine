    <script type="text/javascript">
        usingNamespace("Biz.HomePage2013")["DailyDeal"] = function(){
            var config={},
                $Isloop= true;
                
            config.CoremetricData =[{"position":"DailyDeal1","dataList":[{"cMData":"DailyDeal-_-28-731-005-_-Homepage_1","itemNumber":"28-731-005"}]},{"position":"DailyDeal2","dataList":[{"cMData":"DailyDeal-_-0WX-003B-002P9-_-Homepage_2","itemNumber":"0WX-003B-002P9"}]},{"position":"DailyDeal3","dataList":[{"cMData":"DailyDeal-_-12-400-548-_-Homepage_3","itemNumber":"12-400-548"}]},{"position":"DailyDeal4","dataList":[{"cMData":"DailyDeal-_-22-178-296-_-Homepage_4","itemNumber":"22-178-296"}]},{"position":"DailyDeal5","dataList":[{"cMData":"DailyDeal-_-24-005-589-_-Homepage_5","itemNumber":"24-005-589"}]}];
            config.DomID="DailyDeal";

            var container=new Swiper("#"+config.DomID,{
                loop:$Isloop,
                simulateTouch:$Isloop,
                onSlideChangeEnd: function () {
                    //idangeor's bug when loop is true
                    var activeSlideIndex=$Isloop?container.activeSlide+1 :container.activeSlide;
                    var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));

                    if (config.CoremetricData && config.CoremetricData.length > 0) {
                        Biz.Common.Coremetric.send(currentID, config.CoremetricData);
                    }
                }
            });

            jQuery(function () {
                //idangeor's bug when loop is true
                var activeSlideIndex=$Isloop?container.activeSlide+1 :container.activeSlide;
                var currentID = jQuery.trim(container.getSlide(activeSlideIndex).getAttribute("data-cm-groupid"));

                Biz.Common.Coremetric.send(currentID, config.CoremetricData);
            });

            if($Isloop){
                jQuery("#"+config.DomID+" .arrow-left").click(function (e) {
                    e.preventDefault();
                    container.swipePrev();
                });

                jQuery("#"+config.DomID+" .arrow-right").click(function (e) {
                    e.preventDefault();
                    container.swipeNext();
                });
            }
        }();
    </script>
</div>


