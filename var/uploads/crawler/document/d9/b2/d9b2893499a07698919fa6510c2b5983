<script>
  (function($) {
      showArrowFor($('.category').html());
    $(document).ready(function() {
      $('.pinterest-button').css('visibility', 'visible');
      var active_category = $('.active-category');
      $('.tips-hover').mouseenter(function() {
        $('.active-category').removeClass('active-category');
        $('.green-background:first').addClass('active-category');
        showArrowFor('tips');
      });
      $('.camera-hover').mouseenter(function() {
        $('.active-category').removeClass('active-category');
        $('.aqua-background').addClass('active-category');
        showArrowFor('camera');
      });
      $('.postprod-hover').mouseenter(function() {
        $('.active-category').removeClass('active-category');
        $('.post-production-background').addClass('active-category');
        showArrowFor('postprod');
      });
      $('.header-menus').mouseleave(function() {
        hideArrows();
        showArrowFor($('.category').html());
        hideCategories();
        active_category.addClass('active-category');
      });
    });

    $(window).load(function() {
      //$('.bar-ad div').css('display') != 'none'
      if ( $('.bar-ad iframe img').length > 1 ) {
        $('.search-bar.desktop').animate({width: '167px'}, 1000, function() { $('.bar-ad').fadeIn(); });
        $('.search-bar.desktop input').css('width', '127px');
      };
    });
  })(jQuery);
</script>

