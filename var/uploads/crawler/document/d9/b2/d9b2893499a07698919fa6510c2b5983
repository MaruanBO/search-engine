    <script type="text/javascript">
      jQuery( window ).load( function() {
        var windowWidth = jQuery( window ).width();

        googletag.pubads().refresh( all_slots );
        jQuery( '.pb-ad-all' ).show();

        if ( windowWidth >= 1024 ) {
          googletag.pubads().refresh( desktop_slots );
          jQuery( '.pb-ad-desktop' ).show();
        } else if ( windowWidth > 320 ) {
          googletag.pubads().refresh( small_slots );
          googletag.pubads().refresh( tablet_slots );
          jQuery( '.pb-ad-tablet' ).show();
        } else {
          googletag.pubads().refresh( small_slots );
          googletag.pubads().refresh( mobile_slots );
          jQuery( '.pb-ad-mobile' ).show();
        }
      } );
    </script>
