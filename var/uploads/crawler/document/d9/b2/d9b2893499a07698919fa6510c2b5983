      jQuery(window).load(function(){
        var elem = jQuery('#social-carousel li');
        elem.css('visibility', 'visible');
        elem.hide();
        jQuery('#social-carousel li.current').show();
        if (!jQuery.support.leadingWhitespace) {
          jQuery('#social-carousel li:nth-child(3)').remove();
          jQuery('div.gplus-button').remove();
        }
        setInterval(function() { changeSocialButton(); }, 7500);
      });
    </script>

    <script>
      var $ = jQuery;
      $(document).ready( function() {
        var msie = $.support.leadingWhitespace;

        if(!msie) {
          $('.article-container article').hover(function() {
            $(this).find('.overlay').css('opacity', 0.8);
          }, function() {
            $(this).find('.overlay').css('opacity', 0);
          });
        } else {
          $('.article-container article').hover(function() {
            $(this).find('.overlay').css('opacity', 1);
          }, function() {
            $(this).find('.overlay').css('opacity', 0);
          });
        }
      })
    </script>

