        <a href="#close" id="usm_close">Close</a>
      </div>
    </div>
    <div class="usm_open" style="z-index: 1000;"><a href="#close" id="usm_close">Close</a></div>
    <script>
    var current_url = "http://digital-photography-school.com";
    var timerID     = 0;

    if (document.cookie.indexOf('hidebar=true') != -1) {
          jQuery('.usm_bar_wrapper').css('display', 'none');
          jQuery('.usm_bar').css('display', 'none');
         }
      jQuery(document).ready(function() {
        if(jQuery('.usm_message_list').children().length == 0) {
          jQuery('.usm_bar_wrapper').remove();
          jQuery('.usm_open').remove();
        } else if(jQuery('.usm_message_list').children().length == 1) {
          return;
        }

        if (document.cookie.indexOf('hidebar=true') != -1) {
          jQuery('.usm_bar_wrapper').css('display', 'none');
          jQuery('.usm_bar').css('display', 'none');
         } else {
            if(jQuery('.usm_message_list').children().length > 1)
              timerID = start_timer();
         }
      });

      function start_timer() {
        restart_loop();
        var i = 1;
        var total = jQuery('.usm_message_list').children().length;
        var continueloop = true;
        var theme = jQuery('.usm_message_list li:nth-child(' + i + ')').attr('data-theme');
        var wait = jQuery('.usm_message_list li:nth-child(' + i + ')').attr('data-wait');
        var id = self.setInterval(function() {
          if(continueloop) {
              jQuery('.usm_message_list li:nth-child(' + i + ')').fadeOut(400, function() {
              continueloop = false;
              i++;
              theme = jQuery('.usm_message_list li:nth-child(' + i + ')').attr('data-theme');
              wait = jQuery('.usm_message_list li:nth-child(' + i + ')').attr('data-wait');
              if(theme == 'light') {
                jQuery('.usm_bar_wrapper').removeClass('dark');
                jQuery('.usm_bar_wrapper').addClass('light');
              } else {
                jQuery('.usm_bar_wrapper').addClass('dark');
                jQuery('.usm_bar_wrapper').removeClass('light');
              }
              jQuery('.usm_message_list li:nth-child(' + i + ')').fadeIn();
              setTimeout(function() {continueloop = true;} , 1000);
            });
            jQuery('.usm_message_list li:not(:nth-child(' + i + '))').css('display', 'none');
            if ( i >= total ) i = 0;
          }
        }, wait*1000);
        return id;
      }

      function restart_loop() {
        jQuery('.usm_message_list').children().each(function () {
          jQuery(this).css('display', 'none');
        });
        jQuery('.usm_message_list').children().first().css('display', 'block');
      }

      function stop_timer() {
        clearInterval(timerID);
      }

      jQuery(window).blur(function() {
        stop_timer();
      });

      jQuery(window).focus(function() {
        if(jQuery('.usm_message_list').children().length > 1)
         timerID = start_timer();
      })

      jQuery('a#usm_close').click(function (e) {
        e.preventDefault();
        jQuery('.usm_bar_wrapper').slideToggle('slow');
        jQuery('.usm_bar').slideToggle('slow', function() {
          if(jQuery('.usm_bar').css('display') == 'none') {
            stop_timer();
          } else {
            if(jQuery('.usm_message_list').children().length > 1)
             timerID = start_timer();
          }
        });

        if (document.cookie.indexOf('hidebar=true') == -1) {
          var time = 1000*60*60*24*15*999;
          var expires = new Date((new Date()).valueOf() + time);
          document.cookie = "hidebar=true; path=/;expires=" + expires.toUTCString();
        } else {
          document.cookie = "hidebar=true; path=/;expires=Thu, 01 Jan 1970 00:00:01 GMT";
        }
      });

      // (function(a){(jQuery.browser=jQuery.browser||{}).mobile=/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows (ce|phone)|xda|xiino/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.su