      <script>
      jQuery( document ).ready( function() {
        var slideyHome = jQuery("#header-slider").unslider({
          width: 100,
          height: 580,
          fluid: true,
          delay: false,
          complete: function() {
            var slider        = slideyHome.data('unslider');
            var img_element   = jQuery(slider.items[slider.current]).find('img');
            var photoTitle    = img_element.attr('title');
            var photoSubtitle = img_element.attr('subtitle');
            var photoAuthor   = img_element.attr('author');
            var photoLink     = img_element.attr('author-link');
            var photoSection  = img_element.attr('section');
            var postLink      = img_element.attr('permalink');

            jQuery('.title-highlight').html('<a href="' + photoLink + '">' + photoTitle + '</a>');
            jQuery('.title-highlight a').attr('href', postLink);
            jQuery('#subtitle').html(photoSubtitle);
            jQuery('#author').html(photoAuthor);
            jQuery('#photograph-section').html(photoSection);
            jQuery('.photo-author-image a').attr('href', photoLink);

            var section_icon   = jQuery('#section-icon');
            var category_label = jQuery('#category-label');
            var photo_section  = jQuery('.photo-section a');

            switch(photoSection) {
              case 'Tips & Tutorials':
                section_icon.removeClass();
                section_icon.addClass('photography-icon').addClass('section-header');
                category_label.removeClass();
                category_label.addClass('green-photo-section');
                photo_section.attr('href', 'http://digital-photography-school.com/tips');
                break;
              case 'Cameras & Equipment':
                section_icon.removeClass();
                section_icon.addClass('camera-icon').addClass('section-header');
                category_label.removeClass();
                category_label.addClass('aqua-photo-section');
                photo_section.attr('href', 'http://digital-photography-school.com/cameras');
                break;
              case 'Post Production':
                section_icon.removeClass();
                section_icon.addClass('screen-icon').addClass('section-header');
                category_label.removeClass();
                category_label.addClass('blue-photo-section');
                photo_section.attr('href', 'http://digital-photography-school.com/post-production');
                break;
            }
          }
        });
        var data = slideyHome.data('unslider');
        data.stop();
        jQuery('.unslider-home-arrow').click(function(e) {

          var fn   = this.className.split(' ')[1];
          var func = fn.split('-')[1];
          data[func]();

          e.preventDefault();
        });
        jQuery('#header-slider').css('display', 'block');
        jQuery('.slider').css('background-image', 'none');
      });

      unique_track = new Image();
      unique_track.src = "http://forms.aweber.com/form/displays.htm?id=TOwsbAwMrGyM";

    </script>

