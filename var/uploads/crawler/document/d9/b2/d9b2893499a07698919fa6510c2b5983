<script>
  (function($) {
    $('ul.tabs li').click(function() {
      var tab_id = $(this).attr('data-tab');
      var section = $(this).attr('data-section');

      $('.article-container').css('display', '');

      $(section+' ul.tabs li').removeClass('active');
      if (section == '.red') {
        $(section+' .resources').removeClass('active');
      } else if (section == '.green' || section == '.aqua' || section == '.blue') {
        $(section+' .article-container').removeClass('active');
      }

      $(this).addClass('active');
      $(section+" #"+tab_id).addClass('active');
      adjustHeight(tab_id, section);
    });

    $(window).load(function() {

     jQuery('.legacy-comments-count').each(function() {
      var element      = jQuery(this);
      var permalink    = element.parent().find('.st_sharethis_custom').attr('st_url');
      var legacy_count = parseInt(element.text());
      var disqus       = element.parent().find('.st_comments a:first');
      var word         = element.parent().find('.comment-word');
      var disqus_count = parseInt(disqus.text());
      disqus_count     = ( isNaN(disqus_count) ) ? 0 : disqus_count;

      var total = legacy_count + disqus_count;
      disqus.text(total);
      if(total == 1) {
        word.text('Comment');
      }

      jQuery.getJSON('http://graph.facebook.com/' + permalink, function(data) {
        var facebook_int = data.shares ? parseInt( data.shares ) : 0;
        var shares_elem  = element.parent().find('.st_sharethis_custom .chicklet-count');
        var shares       = parseFloat( shares_elem.text() );
        shares           = ( isNaN( shares ) ) ? 0 : shares;
        shares           = ( shares % 1 != 0 ) ? shares * 1000 : shares;
        var total_shares = facebook_int + shares;
        var total_shares = ( total_shares == 0 ) ? 'New' : total_shares;
        shares_elem.text( ( total_shares ) );

        if(shares_elem.text() == 'New') {
          shares_elem.parent().text('Share This');
        }
      });

    });

    });

    $(window).resize(function() {
      var width = $(window).width();
      if(width > 320) {
        if($('.green #tab-1').is(':visible')) {
          adjustHeight('tab-1', '.green');
        }

        if($('.aqua #tab-1').is(':visible')) {
          adjustHeight('tab-1', '.aqua');
        }

        if($('.blue #tab-1').is(':visible')) {
          adjustHeight('tab-1', '.blue');
        }
      }

      if(width > 1000)
        $('.article-container').css('display', '');
    });

    $(document).ready(function() {
      if ($(window).width() >= 1000) {
        $('ul.tabs .active').click();
      } else {
        adjustHeight('tab-1', '.green');
        adjustHeight('tab-1', '.aqua');
        adjustHeight('tab-1', '.blue');
      }
    });
  })(jQuery);
</script>
    <script type="text/javascript">
    // <![CDATA[
        var disqus_shortname = 'digitalphotographyschool';
        (function () {
            var nodes = document.getElementsByTagName('span');
            for (var i = 0, url; i < nodes.length; i++) {
                if (nodes[i].className.indexOf('dsq-postid') != -1) {
                    nodes[i].parentNode.setAttribute('data-disqus-identifier', nodes[i].getAttribute('rel'));
                    url = nodes[i].parentNode.href.split('#', 1);
                    if (url.length == 1) url = url[0];
                    else url = url[1]
                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + disqus_shortname + '.disqus.com/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script>
