    (function($) {
      if (document.cookie.indexOf('newsletter-popup=1') == -1 && jQuery(window).width() >= 800) {
        setTimeout(function() {
          var time = 1000*60*60*24*15*999;
          var expires = new Date((new Date()).valueOf() + time);
          document.cookie = "newsletter-popup=1; path=/;expires=" + expires.toUTCString();
          showOldNewsletter();
        }, 15000);
      }
    })(jQuery);
