<script language="javascript">

    // Mini cart code
    

    $(document).ready(function () {
        // Close minicart if user click outside of it
        $(document).mouseup(function (e) {
            var $quickcart = $("#quickcart-container");
            if (!$quickcart.is(e.target) // if the target of the click isn't the container...
                && $quickcart.has(e.target).length === 0) { // ... nor a descendant of the container
                $quickcart.slideUp('medium');
            }
        });

        $("#quickcart-container").mouseenter(function(){
            //console.log('mouse enter - cleared time out ' + $(this).data('timeoutId'));
            clearTimeout($(this).data('timeoutId'));
        }).mouseleave(function(){
            var $quickcart = $(this);
            var timeoutId = setTimeout(function(){
                $quickcart.slideUp('medium');
            }, 3000);
            $quickcart.data('timeoutId', timeoutId); 
            //console.log('mouse leave - set time out ' + timeoutId);
        });
    });

    function refreshOrAddToMiniCart(addToCartLink, scrollTop, autoOpen, autoClose) {

        var $quickcart = $('#quickcart-container');
        if($quickcart.data('isMiniCartRefreshing')) { return true; }

        var originalUrl = $(addToCartLink).prop('href');
        var querystring = (originalUrl != null && originalUrl.indexOf('?') >= 0) ? originalUrl.substr(originalUrl.indexOf('?') + 1) : '';
        var sli_sessionId = $('#Sli_SessionId').html();
        var url = querystring != '' 
            ? ('//www.puritan.com/mvc/shoppingcart/add?' + querystring + '&IsMiniCart=true&SessionId=' + sli_sessionId)
            : ('//www.puritan.com/mvc/shoppingcart/?isMiniCart=true&sessionId=' + sli_sessionId);

        $quickcart.block({ message: null });

        if(scrollTop) {
            $('html,body').animate({ scrollTop: 0 }, 'slow', function() {
                if(autoOpen) {
                    $quickcart.slideDown('medium', function() {
                        miniCart(url, autoClose);
                    });
                } else {
                    miniCart(url, autoClose);
                }
            });
        } else if(autoOpen) {
            $quickcart.slideDown('medium', function() {
                miniCart(url, autoClose);
            });
        } else {
            miniCart(url, autoClose);
        }

        return true;
    }
    
    function miniCart(url, autoClose) {
    
        var $quickcart = $('#quickcart-container');
        if($quickcart.data('isMiniCartRefreshing')) { return true; }
        $quickcart.data('isMiniCartRefreshing', true);

        clearTimeout($quickcart.data('timeoutId'));
        if(autoClose) {
            var timeoutId = setTimeout(function(){$quickcart.slideUp('medium')}, url.indexOf('add?')==0 ? 6000 : 3000);
            $quickcart.data('timeoutId', timeoutId);
            //console.log('mini cart: set time out ' + timeoutId);
        }

        $quickcart.block();
        $.ajax({
            'url': url,
            'type': 'GET',
            'dataType': 'html',
            'cache': false,
            'success': function (data) {
                $quickcart.data('isMiniCartRefreshing', false);

                $quickcart.unblock();
                $quickcart.html(data);
                numberOfItems = $('#miniCartItemCount', $quickcart).html();
                numberOfItems = numberOfItems == null ? 0 : numberOfItems;
                updateSliCookie(numberOfItems);
                $('#cartcount').html(numberOfItems + ' Item(s)');
            },
            'error': function (jqXr, textStatus, errorThrown) {
                $quickcart.data('isMiniCartRefreshing', false);
                
                if(originalUrl != null) {
                        window.location = originalUrl;
                    }
                
            }
        });
        return true;
    }

    function addToMiniCart(e, link) {
        clearTimeout($('#quickcart-container').data('timeoutId'));
        if(refreshOrAddToMiniCart(link, true, true, true)) {
            e.preventDefault();
            return false;
        }
    }

    function displayMiniCart(refreshCart) {
        // Do not do anything is cart is empty or if minicart is already visible
        if($('#cartcount').html().indexOf('0') == 0 || $('#quickcart-container').is(":visible") ) {
            return;
        }
        if($('#quickcart-container').html().indexOf('quickcart-placeholder') > 0 || refreshCart) {
            refreshOrAddToMiniCart(null, refreshCart, true, true);
        } else {
            // Remove ITEM ADDED TO CART message
            $('#quickcart-container h3.minicart-title').html('My Shopping Cart');
            // Open cart, close after 6 seconds
            var $quickcart = $('#quickcart-container');
            $quickcart.slideDown('medium');
            clearTimeout($quickcart.data('timeoutId'));
            var timeoutId = setTimeout(function(){$quickcart.slideUp('medium')}, 6000);
            $quickcart.data('timeoutId', timeoutId);
            //console.log('display mini cart: set time out ' + timeoutId);
        }
    }

    function updateSliCookie(newItemCount) {

        var sliCookie = $.cookie('SLICookie');
        if(sliCookie == null) return;

        var truncatedSliCookie = sliCookie.substr(sliCookie.indexOf('item_count=') + 11);
        var itemCount = truncatedSliCookie.indexOf('&') >=0 
           ? truncatedSliCookie.substring(0, truncatedSliCookie.indexOf('&'))
           : truncatedSliCookie;
        sliCookie = sliCookie.replace('item_count=' + itemCount, 'item_count=' + newItemCount);
        $.cookie('SLICookie', sliCookie, { domain: '.puritan.com', path: '/' });
    }

</script>
<span id="Sli_SessionId" style="display: none"></span>


