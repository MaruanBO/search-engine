  //homepage global : attach hover event handler to categories to show counts
  function handleHccDiscHover() {
    var hccDiscEnter = function() {
      $(this).find('.hcc_category_item').hide();
      $(this).find('.hcc_category_item_hover').show();
    };
    var hccDiscLeave = function() {
      $(this).find('.hcc_category_item_hover').hide();
      $(this).find('.hcc_category_item').show();
    };
    $('.cause_carousel li').on({
      mouseenter: hccDiscEnter,
      mouseleave: hccDiscLeave
    });
  }

  // the following is for the callout box carousel, and you can set the duration of the
  // animation, whether you can loop back to the beginning or not, size (number of boxes to show),
  // number of boxes to move when the arrow is clicked, and whether to show icons indicating
  // where in the stack one is
  $(document).ready(function() {

    // get causes and counts
    formData = decodeURI("?l=Singapore, Singapore&categories");
    $.get("/search/async", formData , searchResultsCallback);

    $('#slides').jcarousel({
      wrap: 'circular',
      visible: 4,
      scroll: 2
    });

    $('#slides').show();

    //attach click event handler to categories to execute search
    $('.hcc_category_container').click(
      function(){
        window.location="/search/"+decodeURI("?l=Singapore, Singapore&categories="+$(this).attr('category_id'));
      }
    );

    //add classes to next and prev buttons to record google analytics
      $('.jcarousel-next-horizontal').addClass("sprite-homepage, gaCategoryCarouselNextClick");
      $('.jcarousel-prev-horizontal').addClass("sprite-homepage, gaCategoryCarouselPrevClick");

      $('.gaCategoryClick').click(function() {
        trackAnalyticsEvent("HomepageCategoryClick","");
      });

      $('.gaCategoryCarouselNextClick').click(function() {
        trackAnalyticsEvent("HomepageCategoryCarouselNextClick","");
      });

      $('.gaCategoryCarouselPrevClick').click(function() {
        trackAnalyticsEvent("HomepageCategoryCarouselPrevClick","");
      });

      //attach hover event handler to categories to show counts
      handleHccDiscHover();

  });

  function searchResultsCallback(data) {

    //get id and opp counts from results and load into html
    $('#all_result_group_counts', data).children().each(function() {
      if ($(this).attr('id').indexOf("all_count_category_") !== -1) {
        var id = $(this).attr('id').replace('all_count_category_','');
        var count = $(this).attr('value');
        $('#hcc_category_listitem_'+id).attr("count", count);
        $('#hcc_category_listitem_'+id).children('.hcc_category_item_hover').html(count);
      }
    });
  }



