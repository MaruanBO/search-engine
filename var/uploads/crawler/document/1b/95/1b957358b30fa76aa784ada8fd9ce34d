<script type="text/javascript">
  $('#search_widget_loc_anchor').click(function(){toggleSearchLocationInput(true)});

  var toggleSearchLocationInput = function(isShow, locationExcerpt, resultSize){

    if(locationExcerpt){
      $('#locAnchorText').text(locationExcerpt);
    }

    if(isShow){
      $('#search_widget_loc_anchor').hide();
      $('#oppsearch').fadeIn(500).select();
      $('#search_location').select();
    } else {
      $('#search_widget_loc_anchor').show();
      $('#oppsearch').hide();
    }
  }
</script>

