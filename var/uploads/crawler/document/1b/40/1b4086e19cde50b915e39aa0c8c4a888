cyljq(function vdcModuleTabbedModule_widgetStyle($){
	var ACTIVE_CLASS = "cyl-isChecked",
		currInput = $(),
		currForm = $();
	
	function updateChecked(ev){
		if(this.checked && !currInput.is(this)){
			currInput.parent().removeClass(ACTIVE_CLASS);
			currInput = $(this).closest("label").addClass(ACTIVE_CLASS).end();
			
			currForm.hide();
			currForm = $(document.getElementById(this.value)).show();
			//If we have a click, fire an event to let others know
			if(ev instanceof cyljq.Event && ev.type != 'fakeclick'){
				currInput.trigger('flexibleMultiWidgetClicked', [ev]);
				
				//If we have analytics, let's track the event
				if(typeof fed !== 'undefined' && fed.analytics){
    				var label = cyljq.trim(currInput.closest("label").text()).toLowerCase();
					
		    		fed.analytics.trackEvent({event: 'click:Search Widget Tab', ga_category: 'widget', ga_label: label, nonInteraction: false});
    			}
			}
		}
	}
	
	$("input[name='RxUMI_14b992a998f42cfbecbbdee']")
	.on("touchstart.FlexibleMultiWidget click.FlexibleMultiWidget fakeclick.FlexibleMultiWidget", updateChecked)
	.each(function(i, el){
		$('#' + this.value).hide();
		updateChecked.call(this);
	});
	$(document).trigger("tabbedModuleInitialized",[207827]);
});
