<script class="rx-layout_vdc_topNavigation" type="text/javascript">
	cyljq(function topNavScripts(jq){
		
		function toggleTitleAttr(e){
			jq(this).attr('title', function(idx, oldTitle){
				var ret = jq(this).data('oldTitle') || '';
				jq(this).data('oldTitle', oldTitle);
				return ret;
			});
		}
		
		function fixIOSTouchNav(e){
			jq(document).one('touchstart', function(we){
				jq('.isActive', '#cylTopNav').removeClass('isActive');
			});
			var jTopLink = jq(e.currentTarget).addClass('isActive').find('ul[data-topnav="subnav"]').siblings('a');
			//var jTopLink = jq(e.currentTarget).addClass('isActive').children().children('[data-topnav="subnav"]').siblings('a');
			if ( jTopLink.has(e.target).length || jTopLink.is(e.target) ) {
				e.preventDefault();
			}
		}
		
		jq('#cylTopNav')
		.on('touchstart touchend', '[data-topnav="subwrap"]', fixIOSTouchNav)
		.on('hover', '[title]', toggleTitleAttr);
		
		try { // Change parent location when not coming from or managed by vegas.com
			if(top.location.hostname.indexOf('vegas.com') <= 0 && jq.cookie('controller') != 'vegas') {
				parent.location = self.location.href;
			}
		} catch(e){}
	});
</script>
