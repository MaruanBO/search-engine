	var CURRENT_LANGUAGE = 'en';
	var CURRENT_LANGUAGE_NAME = '';
	var CURRENT_COUNTRY = '';
	var COUNTRY_LANGS_OBJ = [];
	var COUNTRY_LANGS = [];
	
	
	$.each(COUNTRY_LANGS_OBJ, function(i, o){
		COUNTRY_LANGS.push(o.lang);
	});
	
	function getCurrentLangName()
	{
		$(LANGUAGES).each(function(i, o) {
			if (o.lang == CURRENT_LANGUAGE) {
				CURRENT_LANGUAGE_NAME = o.langName;
			}
		});
	}
	
	$(document).ready(function(){
		getCurrentLangName();
	});
	
	
    </script>
