    <script type="text/javascript">
        $(document).ready(function(){
            $('.maxText').each(maxText);

            var items = $('.testimonials .s');
            var idx = 1;

            setFeaturesTitleHeight();

            setInterval(function(){
                $('.testimonials .s').removeClass('show');
                $(items[idx]).addClass('show');

                if (idx < items.length - 1) {
                    idx++;
                }
                else {
                    idx = 0;
                }
            }, 4000);

        });

        $(window).resize(function(){
            if (!LOGGED) {
                repositionCont();
                headerButtonShowHide();
            }

            $('.maxText').each(maxText);
        });

        if (!LOGGED) {
            repositionCont();

            $(window).scroll(function(){
                headerButtonShowHide();
            });
            // hack pt pozitionarea contentului in debug mode - cand se apeleaza prima data repositionCont(),
            // headerul nu are inca toate stilurile aplicate si continutul e pozitionat mai jos decat ar trebui
            
                        
            // end hack debug mode
        }

        if (mobileDevice) {
            setInterval(function(){
                headerButtonShowHide();
            }, 1000);
        }

        function headerButtonShowHide()
        {
            if ($(window).scrollTop() >= (parseInt($('#transparentbutton').offset().top) - $('#UA-header').height() + $('#transparentbutton').height())) {
                $('#UA-header .UA-hmenu').stop(true, true).fadeOut(300, function(){
                    $('#UA-header .floatingbtn').fadeIn();
                });
            }
            else {
                $('#UA-header .floatingbtn').stop(true, true).fadeOut(300, function(){
                    $('#UA-header .UA-hmenu').fadeIn();
                });
            }
        }

        function repositionCont()
        {
            $('.UA-mainContent').css({
                paddingTop: $('#UA-header').height() + 'px'
            });
        }

        function setFeaturesTitleHeight()
        {
            var featuresConts = $('.pS3 .fCont .tl table td h3');
            var featuresContsMax = 34;

            featuresConts.each(function(){
                var h = $(this).height();
                if (h > featuresContsMax) {
                    featuresContsMax = h
                }
            });

            $('.pS3 .fCont .tl table td').css({
                height: featuresContsMax + ''
            });
        }

    </script>
