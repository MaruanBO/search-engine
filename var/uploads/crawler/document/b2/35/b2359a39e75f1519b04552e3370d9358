        $(function () {
            $('textarea.autosize').autosize();
            $('.btn').button();

            $('#accounttype label').click(function () {
                var type = $(this).children('input').attr('value');

                if (type == 'advertiser') {
                    $('#form-brands').show();
                    $('#form-sites').hide();
                } else {
                    $('#form-sites').show();
                    $('#form-brands').hide();
                }
            });

            $('#btn-advertiser').click();
        });
