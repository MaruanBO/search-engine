<!-- Link to the javascript file we will create -->
<script type="text/javascript">
    $(document).ready(function(){
		//Hide the 'What are you Interested in?' dropdown
		$('#what-are-interests').hide();
		//If they have more than 15 employees show the 'What are you Interested in?' dropdown
		$('#numberOfEmployees').blur(function() {
			if($(this).val() >= 15){
				$('#what-are-interests').show();
			}
		});
        // Test for placeholder support
        $.support.placeholder = (function(){
            var i = document.createElement('input');
            return 'placeholder' in i;
        })();
        // Hide labels by default if placeholders are supported
        if($.support.placeholder) {
            $('.form li').each(function(){
                $(this).addClass('js-hide-label');
            });  
         
            $('.form li').find('input, textarea, select').on('keyup blur focus', function(e){     
                // Cache our selectors
                var $this = $(this),$parent = $this.parent();

                // Add or remove classes
                if (e.type == 'keyup') {
                    if( $this.val() == '' ) {
						//Keep the label hidden until user starts typing
                        $parent.addClass('js-hide-label'); 
                    } else {
						//Show the label
                        $parent.removeClass('js-hide-label'); 
                    }                   
                } 
                else if (e.type == 'blur') {
					$this.removeClass('grey-bg'); 
                    if( $this.val() == '' ) {
                    	$parent.addClass('js-hide-label');
                	}else{
                    	$parent.removeClass('js-hide-label').addClass('js-unhighlight-label');
                	}
                } 
                else if (e.type == 'focus') {
					$this.addClass('grey-bg'); 
                    if( $this.val() !== '' ) {
                        $parent.removeClass('js-unhighlight-label');
                    }
                }
            });
        }
    });
	
	//If the referral source is already selected, show the label
	if($("#whereHear").val() != ""){
		$(".whereHearLabel").css("opacity","1"); 
	}
	
    var phone, zip;
    //Changes the selected dropbox color from grey to black
    var firstPar, secPar, dash, space, zipDash = false;
    function colorToBlack(id){
        document.getElementById(id).style.color="#000";	
    }		
    function displayServices(){
        document.getElementById('service-dropdown').style.display="block";	
    }
    
    //Hides the client code text box until user select that they are a client
    document.getElementById('clientCode').style.display="none";	
    //Hides the available services until user selects that they want to specify their interests
    document.getElementById('service-dropdown').style.display="none";	
	
	
</script>
</div>
