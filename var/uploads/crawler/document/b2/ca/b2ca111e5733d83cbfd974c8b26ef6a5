	<script language="javascript">
		var useUrlRewrite = "true";
		
			var keyword_search_type = "solr";
		

		function trim(str, chars) {
		    return ltrim(rtrim(str, chars), chars);
		}

		function ltrim(str, chars) {
		    chars = chars || "\\s";
		    return str.replace(new RegExp("^[" + chars + "]+", "g"), "");
		}

		function rtrim(str, chars) {
		    chars = chars || "\\s";
		    return str.replace(new RegExp("[" + chars + "]+$", "g"), "");
		}

		function submitSearchFormOnTop() {
				var actionVal = 'http://www.totalwine.com';
				var keyword = j$.trim (j$('#searchTerm').val() );

				keyword = keyword.replace( /(\s|'|’|&|!)/g, "_" );
				keyword = keyword.replace( ":", "_" );
				actionVal = actionVal+'/eng/search/'+ escape(keyword);
				location.replace(actionVal);
	
			}

		

			j$(function(){

				j$('#searchTerm').keypress(function(event){
								
					var keycode = (event.keyCode ? event.keyCode : event.which);
					if(keycode == '13' && j$.trim ( j$('#searchTerm').val() ).length > 1){
						j$('#srcGo').click();
					}
				});
				
			});
			
	
	</script>
	
	
