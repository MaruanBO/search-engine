<script type="text/javascript">

CommunityFeed = function() {
	// Hook up all the collapse links
	var links = YD.getElementsByClassName('collapse', 'a', 'communityfeed');
	for (var i in links) {
		YE.on(links[i], 'click', function (e) {
			YE.stopEvent(e);
			var event = this.href.split('#')[1]; // remove the anchor to get the ID
			var collapsed_els = YD.getElementsByClassName(event + " collapsed", 'li', 'communityfeed');
			YD.setStyle(collapsed_els, 'display', '');
			YD.removeClass(collapsed_els, 'collapsed');
		});
	}
}

EXTENSIONS.push(CommunityFeed);
</script>

	

    
