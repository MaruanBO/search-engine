<script type="text/javascript">
EXTENSIONS.push(function () {
    var dismiss_links = YD.getElementsByClassName('dismiss_alert');
    for (i in dismiss_links) {
        link = dismiss_links[i];
        YE.on(link, 'click', function(e) {
            YE.stopEvent(e);
            // TODO: Get the URL from the dismiss link, and scrap the "id" in the dismiss link
            YC.asyncRequest('POST', HOST_NAME + '/alerts/' + e.target.id.split("_")[2] + '/dismiss/', {
                    success: function(o){
                        $("alert_" + e.target.id.split("_")[2]).style.display = 'none';
                    },
                    timeout: 6000
                }
            );
        });
    }
});
</script>


			


	

	
