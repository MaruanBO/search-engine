    <script>menusGo()</script>
    <script>
    if (!ez_NN4) {window.onresize = function() {displayMenu();}} 
    function displayMenu(menu, element, xPos, yPos) {
        if(!element)
            return false;
        var imgPosition = imgXY(element);
        /* showPanel(menu, imgPosition.x + xPos, imgPosition.y + yPos); removed for 2012 per kt 2.27.2012 */
        showPanel(menu, imgPosition.x + xPos, imgPosition.y + (yPos+15));

    }
    function imgXY(imgID) {
      /* This function returns the upperleft x,y coordinates of the specified image */
      var XY = new Object();
      var imgObj = document.images[imgID];
      if (document.layers) {
        XY.x = eval(imgObj).x;
        XY.y = eval(imgObj).y;
      } else {
        var x,y, tempEl;
        x = eval(imgObj).offsetLeft;
        y = eval(imgObj).offsetTop;
        tempEl = eval(imgObj).offsetParent;
          while (tempEl != null) {
              x += tempEl.offsetLeft;
            y += tempEl.offsetTop;
              tempEl = tempEl.offsetParent;
          }
        XY.x = x;
        XY.y = y;
      }
      return XY;
    }
    </script>
 

<!-- ////// END CON_JS ///// -->



