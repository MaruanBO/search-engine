<script src="//www.google.com/jsapi" type="text/javascript"></script>
<script type="text/javascript"> 
 google.load('search', '1');
      function inputFocus() {
            document.getElementById('query-input').style['background'] = '';
      }
      function inputBlur() {
            var queryInput = document.getElementById('query-input');
            if (!queryInput.value) {
            queryInput.style['background'] =
            'white url(http://www.alz.org/images'
            + 'google_custom_search_watermark_empty.gif) no-repeat 0% 50%';
                     }
      }
      function init() {
          //reset myCX to always be national - needed for autocomplete fill in
          var myCX = '010566669151724466353:7vnkstnw2hs';
            google.search.CustomSearchControl.attachAutoCompletion(
            myCX,
            document.getElementById('query-input'),
            'two-page-form');
            inputBlur();
      }
    function submitQuery() {
    var queryTerm = encodeURIComponent(document.getElementById('query-input').value);
    var n=queryTerm.replace(/'/g,"");
    window.location = 'http://www.alz.org/search/results.asp?q=' +  n + '&as_dt=i';
    return false;
    }
      google.setOnLoadCallback(init);
</script>
<!--END NEW SEARCH SCRIPT -->
 
 
 
