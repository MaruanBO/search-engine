        $('a._is_active').removeClass('_is_active').find("b").remove();
        $(this).addClass('_is_active').find("span.screen-reader-only").prepend("<b>Currently selected, </b>");
        var panelTarget = $(this).attr('rel');
        $($tab_panel_body).removeClass('_is_active').attr("aria-expanded", "false").attr("aria-hidden", "true").find("a").attr("tabindex","-1");
        $($tab_panel_body).find("p").attr("tabindex","-1");
        $(panelTarget).addClass('_is_active').attr("aria-expanded", "true").attr("aria-hidden", "false").find("a").attr("tabindex","0");
        $(panelTarget).find("p").focus();
        return false;
    });
