<script type="text/javascript">
	$(document).ready(
		function(){		
			var bopisFlag = 'ENABLE_ALL',
			 sfsFlag = 'ENABLE_ALL',
		 prefCookie = Target.controller.header.cookie.read("Pref"),
		 userStatus=Target.controller.headerNew.getUserStatus();
		var guestVisit = 'N';
		if(prefCookie == null){
		 prefCookie = "F=N|Sub=N" ;
		}else{
			guestVisit = 'Y';
		}
	    if (bopisFlag == 'ENABLE_ALL' ) {
	    	if(prefCookie.indexOf('F=N') !== -1){
	    	console.log("in enable_all and F= N");
	    		prefCookie = prefCookie.replace('F=N','F=Y');	
	    		Target.controller.header.cookie.erase("Pref");
	    	}
	    }else if((bopisFlag == 'ENABLE_FOR_SELECT_GUESTS' ) ){
			    	if(guestVisit == 'N' &&  prefCookie.indexOf('F=Y') !== -1){
			    		console.log("in select guest and F= Y");
				    	prefCookie = prefCookie.replace('F=Y','F=N');
			 			Target.controller.header.cookie.erase("Pref");
			    	}
	    	
 						
						
 		}else { 
	    			
	    		prefCookie = prefCookie.replace('F=Y','F=N');
			 	Target.controller.header.cookie.erase("Pref");
	    	
	    	
	    }
	    /* Start Added for Global SFS functionality Defect 23595*/
        if (Target.globals.sfsFlag == 'ENABLE_ALL'){
            if(prefCookie.indexOf('SFS=') == -1) {                                     
                            prefCookie = prefCookie+'|SFS=Y';
            }
            else if(prefCookie.indexOf('SFS=N') != -1){
                            prefCookie = prefCookie.replace('SFS=N','SFS=Y');
                            }
            else if(prefCookie.indexOf('SFS=Y') != -1){
                            prefCookie = prefCookie;
                            }              
            Target.controller.header.cookie.erase("Pref");
		}else if(((Target.globals.sfsFlag == 'ENABLE_FOR_SELECT_GUESTS' && userStatus != 'Logged in') || Target.globals.sfsFlag == 'DISABLE_ALL')&& prefCookie.indexOf('SFS=Y') !== -1){                              
            prefCookie = prefCookie.replace('SFS=Y','SFS=N');
            Target.controller.header.cookie.erase("Pref");
        }
    
        /* END Added for Global SFS functionality */
	    Target.controller.header.cookie.create("Pref" , prefCookie , 14);
	});
</script>

