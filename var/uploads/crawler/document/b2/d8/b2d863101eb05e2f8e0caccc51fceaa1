		$(document).ready(function () {
			$("body").delegate("a#OpenLoginPopup", "click", function () {
				var	baseURL = window.location.href,
				    currentURL = baseURL.split('/'),
				    reFormURL = "";
					currentURL.shift();
					currentURL.shift();
					currentURL.shift();
					reFormURL = currentURL.join("/");
					Target.controller.header.cookie.create('basePageURLForPOPup',reFormURL);
