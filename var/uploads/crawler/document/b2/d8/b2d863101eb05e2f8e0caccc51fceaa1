<script type="text/javascript" charset="utf-8">
$(document).bind('rr-ready', function(event) {
	if($(".pdp-rightrail").length > 0){$(".pdp-rightrail").lazy();}
	var i = 0, j=0;
		if(typeof rr_recs.placements.length != "undefined" && rr_recs.placements.length > 0){				
			$.each(rr_recs.placements, function(key, val) {		
				if(val.html == ""){
					try{
						var recomendationJsonString = JSON.stringify(RR.data.JSON.placements[i]);
						if(JSON.parse(recomendationJsonString).context != undefined && JSON.parse(recomendationJsonString).context != ''){	
							
							var placementName =  JSON.parse(recomendationJsonString).context.split(";")[0].split(",")[1];
							if( placementName != undefined && placementName.indexOf('plp') != -1){
									
								jsonCallback(RR.data.JSON.placements[i],"200", "Message");
								
							}
						}
					}catch(e){
						console.log(e);
					}
					if(typeof RR.data.JSON.placements[i].items != "undefined" && RR.data.JSON.placements[i].items.length > 0){
						var  jsonObj = RR.data.JSON.placements[i].items,
							 strat_msg = RR.data.JSON.placements[i].strat_msg,
							 placmentId  = val.placementType.replace( ".", "_"),
							 pagePlacmentId  = val.placementType.replace( ".", "-");
						 if (pagePlacmentId.match(/horizontal/g) == "horizontal"){
							 $( "#pdpBottomLazyLoadContent" ).prepend('<div id="'+pagePlacmentId+'"></div>');
						 }								
						 else{
							 if(pagePlacmentId == "item_page-pdp_vertical_3_1")
								 $( ".pdpRightRailRR" ).prepend('<div id="'+pagePlacmentId+'" componentname="vertical_3_1_component"></div>');
							 else if(pagePlacmentId == "item_page-pdp_vertical_5_1")
								 $( ".pdpRightRailRR" ).prepend('<div id="'+pagePlacmentId+'" componentname="vertical_5_1_component"></div>');
							 else if(pagePlacmentId == "item_page-pdp_vertical_6_1")
								 $( ".pdpRightRailRR" ).prepend('<div id="'+pagePlacmentId+'" componentname="vertical_6_1_component"></div>');
							 else
							 	$( ".pdpRightRailRR" ).prepend('<div id="'+pagePlacmentId+'"></div>');
						 }	
						 if (window.location.protocol === "https:") {
							var protocall =  "https://";
						} else {
							var protocall =  "http://";
						}
						$("#pdp-rr-placement").tmpl('',{componentId: val.placementType, componentClass: placmentId, strat_msg: strat_msg, counter:j+1}).appendTo("#"+pagePlacmentId);
						$("#component_group").tmpl(jsonObj,{componentId: val.placementType, protocall:protocall,
							getSplittedString: function(title){var title = $("<div/>").html(title).text(); return jQuery.trim(title).substring(	0, 30).split(" ").slice(0, -1).join(" ") + "...";}
						}).appendTo("."+placmentId);
					}
				}
				i++;	
				if(val.placementType == "item_page.pdp_vertical_3_1"){
					$("#item_page-pdp_vertical_3_1").length?$("#item_page-pdp_vertical_3_1 li").first().find("input").attr("checked","checked"): ""; 
				    $("#item_page-pdp_vertical_3_1 input").click(function() {
				          var arr = new Array();
				           $.each($("#item_page-pdp_vertical_3_1 input:checked"), function () {
				             arr.push($(this).val());
				           });
				           (arr.length==0)? $("#rr_shopthislook").addClass("state-disabled").append("<span class='screen-reader-only'> disabled</span>") : $("#rr_shopthislook").removeClass("state-disabled");
				           arr = new Array();
				    }); 					
				}
				
				if(val.placementType == "item_page.pdp_vertical_6_1"){ 
					$("#item_page-pdp_vertical_6_1 ul li:last-child").css("border-right", "none");
					$("#item_page-pdp_vertical_6_1 ul li:first-child").css("border-top", "none");
					$( "#item_page-pdp_vertical_6_1 ul li:nth-child(2)" ).css("border-top", "none");
					$( "#item_page-pdp_vertical_6_1 ul li:nth-child(2)" ).css("border-right", "none");
				}
				if(val.placementType == "item_page.pdp_horizontal_2" || val.placementType == "item_page.pdp_horizontal_3"){
					$("#item_page-pdp_horizontal_2").length?$("#item_page-pdp_horizontal_2 li").first().find("input").attr("checked","checked"): ""; 
					$("#item_page-pdp_horizontal_3").length?$("#item_page-pdp_horizontal_3 li").first().find("input").attr("checked","checked"): ""; 
					$("#item_page-pdp_horizontal_2").length?$('#item_page-pdp_horizontal_2 li:first').after('<li class="middle-container"><img alt="buy together" src="http://xcc-uat01.target.com/wcsstore/TargetSAS/images/plus.jpg"></li>'):"";
					if( $( "#item_page-pdp_horizontal_2 li" ).size() > 4 ){
						$('#item_page-pdp_horizontal_2 li:last').remove();
					}
					$("#item_page-pdp_horizontal_2 input, #item_page-pdp_horizontal_3 input").click(function() { 
				          var arr1 = new Array();
				           $.each($("input:checked"), function () {
				             arr1.push($(this).val());
				           }); 
						   if(arr1.length==0){
								$("#rr_addtocart").addClass("state-disabled").append("<span class='screen-reader-only'>&nbsp;disabled</span>")
						   }else{
								$("#rr_addtocart").removeClass("state-disabled");
								$("#rr_addtocart .screen-reader-only").remove();
						   }
				           //(arr1.length==0)? $("#rr_addtocart").addClass("state-disabled").append("<span class='screen-reader-only'> disabled</span>") : $("#rr_addtocart").removeClass("state-disabled");
				           arr1 = new Array();
				    }); 					
				}
			});
		}
		
		var headingSet = $(".hpW3Carousel.recmm.fromrr.evolution");
		headingSet.each(function(){
		        var busHeadline = $(this).find(".youmaylike .title"),
		        rrHeadline =$(this).find(".component.component-set.w3").find(".headline");
		        $(this).find(".youmaylike").append(rrHeadline);
		        if(busHeadline.length == 0){
		                $(this).find(".youmaylike").find('.headline').removeClass('headline').addClass('title');
		        } else {
		                $(this).find(".youmaylike").find('.headline').removeClass('headline').addClass('subhead');
		        }
		});
		
	    Target.controller.globalUtils.checkIfCarousal(".component-carousel-1");
        Target.controller.globalUtils.checkIfCarousal(".component-carousel-3");
        Target.controller.globalUtils.checkIfCarousal(".component-carousel-4");
        Target.controller.globalUtils.checkIfCarousal(".component-carousel-5");
        Target.controller.globalUtils.expandDescComponent();
		Target.controller.globalUtils.RRAddToCart();
		var siteSkinPlacementID = 'home_page.home_siteskin_1';					
		if(siteSkinPlacementID != 'null' && rr_recs.placements.length > 0){
	        $.each(rr_recs.placements,function(){
	        	if( this.placementType == siteSkinPlacementID){
	           		$("body").addClass("globalMessaging");
	           		 return false; 	       
	        	}
       		});
    	 };
      		
      	setTimeout(function(){
      		var container = document.querySelector('#flexible_grid_container'), cont = $("#flexible_grid_container");
			if (cont.length != 0) {
      		msnry = new Masonry(
					container, {
						columnWidth : 1
					})
			}
		    }, 1000);
		
});
</script>

 
		

