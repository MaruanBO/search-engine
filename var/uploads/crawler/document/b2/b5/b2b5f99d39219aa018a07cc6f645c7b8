    <script type="text/javascript" data-type="init Quantcast">
    Mendel._quantsegs = null;
    Mendel._getQsegs = function() {
        if ( Mendel._quantsegs != null ) { return Mendel._quantsegs; }
        var cn = '__qseg';
        var c=document.cookie;if(!c)return '';
        var i=c.indexOf(cn+"=");if(-1==i)return '';
        var len=i+cn.length+1;
        var end=c.indexOf(";", len);
        var value = c.substring(len,end<0?c.length:end);
        var quantSegs = [];
        var _qsegs = value.split('|');
        for (i=0;i < _qsegs.length;i++) { var qArr=_qsegs[i].split("_"); if (qArr.length>1) { quantSegs.push( qArr[1] ); } }
        Mendel._quantsegs = quantSegs.join(',');
        return Mendel._quantsegs;
    }
