<script type="text/javascript">
var player; 
var experienceModule; 
var contentModule;
var videoCount;
function onPlayerLoaded(id) { 
	player = brightcove.getExperience(id); experienceModule = player.getModule(APIModules.EXPERIENCE);
	experienceModule.addEventListener(BCExperienceEvent.CONTENT_LOAD, onContentLoad); experienceModule.addEventListener(BCExperienceEvent.ENTER_FULLSCREEN, onEnterFullscreen);
	experienceModule.addEventListener(BCExperienceEvent.EXIT_FULLSCREEN, onExitFullscreen);	}
function onContentLoad(event) {	
	contentModule = player.getModule(APIModules.CONTENT);
	var playlists = contentModule.getAllMediaCollections("playlist");
	videoCount = playlists[0].videoCount;hidePlaylist();}
function onEnterFullscreen(event) {
	hidePlaylist();}
function onExitFullscreen(event) { if (videoCount == 1){hidePlaylist();}else if(videoCount > 1){showPlaylist();}}
function hidePlayer(){		document.getElementById("videoPlayerDiv").style.display = "none"; }
function hidePlaylist() {	experienceModule.getElementByID("playlistCanvas").setVisible(false); }
function showPlaylist() {	experienceModule.getElementByID("playlistCanvas").setVisible(true); }
brightcove.createExperiences();
</script>

	
