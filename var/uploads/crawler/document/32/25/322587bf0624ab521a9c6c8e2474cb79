<script type="text/javascript">

function loadMonth(dt) {
	$('#calendarSmall').css('box-shadow','0 0 10px #ccc');
	$('#monthTitle').text('Loading...');	
	$('.dy').css('opacity','0.6');	
	$('.cD').css('background-color','#ccc').css('color','#aaa');
	calendarURL='/customcf/calendar-small.cfm?processdate=' + dt;	
	$('#thisMonth').load(calendarURL);
}

</script>


