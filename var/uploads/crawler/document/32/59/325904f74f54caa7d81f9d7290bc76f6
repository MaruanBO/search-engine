<script type="text/javascript">
    $(document).ready(function() {
        
        // Check to see if the user is currently logged in.  If so, we display
		// a welcome message; otherwise, we display the login box.
		var isDev = (window.location.hostname.indexOf("d-") === 0);
		var devPrefix = "";
		if(isDev) { devPrefix = "d-"; }
		$.getJSON('https://' + devPrefix + 'secure.hslda.org/authenticateduserInfoJson.aspx?callback=?',null,
		function(userInfo) {
			if(userInfo.IsAuthenticated){
                $('#member_welcome_name').text(userInfo.Addressee);
                $('#member_welcome').show();
			} else {
				$('#member_login_wrapper').show();
			}
		});
    });
</script>

