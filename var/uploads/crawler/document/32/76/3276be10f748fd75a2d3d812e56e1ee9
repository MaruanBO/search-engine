		$('#header-secondary-outer ul#social li').clone().prependTo('#header-secondary-outer ul.sf-menu').addClass('addedMenu');

		$('<div class="hotline-small"><a class="nectar-button small extra-color-1" href="http://boxsupport.com/connect/"><span>Connect</span></a></div>').prependTo('header#top div.col_last').hide();

		$( window ).resize(function() {
			var smallHotline = $('.hotline-small');
			var bigHotline = $('li.connect-btn');

			if ($(window).width() < 1000) {
				smallHotline.show();
				bigHotline.hide();
			} else {
				smallHotline.hide();
				bigHotline.show();
			};
		});


	}(jQuery));

</script>
<!-- END // Custom jQuery Code -->


