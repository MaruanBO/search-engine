		<script type="text/javascript">
		    var $message = $('p.message');
		    if (!TRAILS.settings.user.isLoggedIn) {
		        $message.append('For full access, <a href="' + TRAILS.settings.urls.login + '">login</a> or ');

		        $signup = jQuery('<a id="BannerCreateAccountLink" href="' + TRAILS.settings.urls.signup + '?oc=4687">start a free account</a>');
		        $overlay = jQuery('<a id="BannerCreateAccountOverlay" style="cursor:pointer;display:none;text-decoration:underline;">start a free account</a>');
		        $overlay.click(function () {
		            var frame = jQuery(document.createElement('iframe'));
		            frame.attr('id', 'reg-frame').addClass('reg-rounded').attr('src', TRAILS.settings.urls.signup.replace('subscribe', 'registration-iframe'));

		            jQuery('#reg-container').append(frame);
		            jQuery('#reg-dialog-container').css('display', 'block');
		        });

		        if (typeof (window['SHOW_REGISTRATION_OVERLAY']) != 'undefined' && window['SHOW_REGISTRATION_OVERLAY']) {
		            $signup.css('display', 'none');
		            $overlay.css('display', 'inline');
		        }

		        $message.append($signup);
		        $message.append($overlay);
		        $message.append('. <a rel="http://www.trails.com/subscription_overview.aspx">Learn more</a>&hellip;')
		    }
		    else if (!TRAILS.settings.user.isSubscribed && !TRAILS.settings.user.isExpired) {
		        if (TRAILS.settings.user.isEligibleForTrial)
		            $message.append('<a href="' + TRAILS.settings.urls.signup + '?oc=4687">Begin your 14-Day Free Trial</a>.');
		    }
		    else if (TRAILS.settings.user.isTrial)
		        $message.append('<a href="https://ssl.trails.com/myaccount/subscription_start.aspx">Begin Your Full Subscription Now!</a>');
		    else if (TRAILS.settings.user.isExpired)
		        $message.append('<a href="https://ssl.trails.com/myaccount/subscription_order.aspx">Start Your Subscription Now!</a>');
		    else
		        $message.append('Current Subscription: ' + TRAILS.settings.user.subscripionPlan);
		</script>
	</p>

</div>

				    
			    </div>
			    <div class="bottomGrad"></div>
			
			    <script type="text/javascript">
			    //<![CDATA[
				    var menuitem = null;
				    function tlmOpen() {  
					    tlmClose();
					    menuitem = $(this).find('div').css('display', 'block');
					    $(this).addClass('selected');
					    $(this).find('a:eq(0)').addClass('selected');
				    }
				    function tlmClose() {  
					    if(menuitem) menuitem.css('display', 'none');
					    $(this).removeClass('selected');
					    $(this).find('a:eq(0)').removeClass('selected');
				    }
				    $(document).ready(function() {  	
					    $('#SiteNavigation > li:lt(8)').bind('mouseover', tlmOpen)
					    $('#SiteNavigation > li:lt(8)').bind('mouseout',  tlmClose)
				    });
			    //]]>
			    </script>
			    
		    </div>
        </div>

