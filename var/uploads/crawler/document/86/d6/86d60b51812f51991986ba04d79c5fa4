	<li class="marginTop3">
		<input  omni="HomePage : Todays Quiz : Pos 1  : Answer" class="middle left" id="answer-44493" name="answer" onclick="location.href='http://people.howstuffworks.com/culture-traditions/tv-and-culture/popular-commercial-slogans-quiz.htm'.replace('.htm', '') + '1.htm?answerId=44493&ref=1';" type="radio" />
