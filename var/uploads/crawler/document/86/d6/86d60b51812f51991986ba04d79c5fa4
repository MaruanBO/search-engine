<script>
    function searchClear(item)
    {
        item.value="";
    }

    function fillSearch(item)
    {
        if (navigator.appName == 'Microsoft Internet Explorer')
        {
            item.value="Search HowStuffWorks";
        }

    }

    $("#facebookDropdown").live("hover",
        function() {
            $("#facebookIframe").toggle();
            $(this).toggleClass("activeIframe");
            $('.facebookSocialIcon').toggleClass("activeIcon");

        });

    /* IE is not wanting to play nice with Youtube subscribe iframe on hover */
    if($.browser.msie){
        $('#utubeDropdown').live('mouseenter',function() {
            $(this).addClass('hover');
        });
        $('#utubeIframe').live('hover',function() {
            $("#utubeDropdown").addClass('hover');
        });
        $("#utubeDropdown").live('mouseleave',function() {
            $(this).removeClass('hover');
        });
    }

    $("#twitterDropdown").live("hover",
        function() {
            $("#twitterIframe").toggle();
            $(this).toggleClass("activeIframe");
            $('.twitterSocialIcon').toggleClass("activeIcon");

        });

    $("#subscribeDropdown").live("hover",
        function() {
            $("#subscribeIframe").toggle();
            $(this).toggleClass("activeIframe");
            $('.subscribeSocialIcon').toggleClass("activeIcon");

        });
</script>

