        if(jQuery.browser.webkit) {
            setTimeout(function(){
                jQuery('#carousel_1424365771e17867ec1').animate({
                    height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-1').outerHeight(true)
                });
           },1000);
        } else {
            jQuery('#carousel_1424365771e17867ec1').animate({
                height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-1').outerHeight(true)
            });
        }
    }
	jQuery('#jcarousel-next-horizontal_1424365771e17867ec1').live('click', function(){
		var slide = parseInt(jQuery('#slide_1424365771e17867ec1').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1424365771e17867ec1').html(slide);
		if(jQuery('#carousel_1424365771e17867ec1').outerHeight(true) < jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1424365771e17867ec1').animate({
				height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1424365771e17867ec1').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1424365771e17867ec1').animate({
			height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1424365771e17867ec1').live('click', function(){
		var slide = parseInt(jQuery('#slide_1424365771e17867ec1').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1424365771e17867ec1').html(slide);
		if(jQuery('#carousel_1424365771e17867ec1').outerHeight(true) < jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1424365771e17867ec1').animate({
				height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1424365771e17867ec1').children('li:last').detach();
		jQuery('#carousel_1424365771e17867ec1').css({
			marginLeft: 0-300
		}).prepend(a).animate({
			marginLeft: 0
		}, 300);
		jQuery('#carousel_1424365771e17867ec1').animate({
			height: jQuery('#carousel_1424365771e17867ec1 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
</script>
