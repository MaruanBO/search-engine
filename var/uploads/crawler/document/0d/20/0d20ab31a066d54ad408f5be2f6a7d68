var max = -1;
if(offset > 1){
$("#content-body-710402").children().slice(0, offset-1).remove();
}
if(max > -1){
$("#content-body-710402").children().slice(max).remove();
}
// Show dateline field at the beginning of the first paragraph
var showDateline = true;
var datelineStr = "";
if(offset <= 1 && showDateline == true && datelineStr != "") {
$("#content-body-710402 p:first").prepend("<span>"+ datelineStr + "- </span>");
}
});
</script>
</section>
</div>
