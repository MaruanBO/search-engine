<div class="heading"><a href="http://www.calientesemanal.com/">Caliente Semanal</a></div>
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">
google.load("feeds", "1");
function initialize() {
var content = "";
var feed = new google.feeds.Feed("http://www.calientesemanal.com/100/index.xml");
feed.setNumEntries(4);
feed.setResultFormat(google.feeds.Feed.MIXED_FORMAT);
feed.load(function(result) {
if (!result.error) {
for (var i = 0; i < result.feed.entries.length; i++) {
var entry = result.feed.entries[i];
var thumb = entry.xmlNode.getElementsByTagName("enclosure")[0].getAttribute("url");
content += '<article class="media"><div class="teaser">';
content += '<div class="img-container pull-left picture" style="width:30%;">';
content += '<a href="'+entry.link+'">';
content += '<img src="'+thumb+'" class="img-responsive">';
content += '</a>';
content += '</div>';
content += '<div class="headline element_spacing_small">';
content += '<h4 class="title"><a href="'+entry.link+'">'+entry.title+'</a></h4>';
content += '</div>';
content += '</div></article>';
$("#calientefeed").html('<section id="trending">'+content+'</section>');
}
}
});
}
google.setOnLoadCallback(initialize);
</script>
<div id="calientefeed"></div>
