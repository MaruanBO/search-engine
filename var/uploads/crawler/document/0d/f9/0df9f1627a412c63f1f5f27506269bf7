<script type="text/javascript">
PLATFORM.EventMan.registerToEvent(function() {
	$("#wnHideParent").parents(".wnDVUtilityBlock").hide();
	var tsActive = 0;
	var $tsrTop = $("#WNTSWContent .wnDS-20");
	
	// FIX VIDEO INCLUDED AND DATE //
	fixDate($("#WNTSWContent #WNDS-20 .wnDVSummary .wnDate"));
	$("#WNCol2 .wnDS-20 .wnDVSummary:lt(4) .wnDate").each(function(){
		fixDate($(this));
	});
	
	$("#WNCol2 .wnDS-20 .wnDVSummary:lt(4)").each(function(){
		$("#WNTSWContent #WNDS-20 .wnDSItems-standard").append($(this));	
	});
	
	
	$("#WNTSWContent #WNDS-20 .wnDSItems-standard .wnDVSummary").each(function(){
		if(!$(this).find(".wnImage").length){
			$(this).prepend("<div class='wnImage wnImageLeft wnImageWidth-180'><a href='"+ $(this).find("h4 a").attr("href") +"'><img src='"+ tsrBackupImg +"' alt='' title='' border='0'></a></div>");
		}
		$(this).find("h4").insertAfter($(this).find(".wnImage"));
		$(this).find(".wnImage a").append("<div class='gradient'></div>");
		$(this).find(".wnImage").nextAll().wrapAll("<div class='wrap'></div>");
		$(this).find(".wnVideoIncluded").insertAfter($(this).find("p"));
		$(this).find(".wnSummaryLink").text("Read Full Story");
	});
	
	
	function fixDate($el){
		var tDate = $el;
		$(tDate).find("script").remove();
		$(tDate).find("noscript").remove();
		$(tDate).html($(tDate).text());
	}
	$("#WNTSWContent #WNDS-20").append("<div class='thumbnails'></div>");
	$("#WNTSWContent #WNDS-20 .wnDSItems-standard").clone().appendTo($(".thumbnails"));
	$("#WNTSWContent #WNDS-20 .wnDSItems-standard:first .wnDVSummary:first").addClass("on");
	$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary:first").addClass("on");
	$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary:last").addClass("last");

	function tsaSwap(){
		$("#WNTSWContent #WNDS-20 .wnDSItems-standard:first .wnDVSummary.on").fadeOut(300,function(){
			$(this).removeClass("on");
			$("#WNTSWContent #WNDS-20 .wnDSItems-standard:first .wnDVSummary").eq(tsActive).fadeIn().addClass("on");
			$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary.on").removeClass("on");
			$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary").eq(tsActive).addClass("on");				
		});	
	}

	var iTx;

	$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary").mouseover(function(){
		var tmp = $("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary").index(this);
		if (tsActive != tmp){
			iTx = setTimeout(function(){
				clearInterval(autoTx);
				tsActive = tmp;
				tsaSwap();
			}, 200);
		}
	});
	$("#WNTSWContent #WNDS-20 .thumbnails .wnDVSummary").mouseout(function(){
		clearTimeout(iTx);
	});
	$("#WNTSWContent #WNDS-20 .wnDSItems-standard:first .wnDVSummary").mouseover(function(){
		clearInterval(autoTx);
	});	
	
	var autoTx;
	
	if (autoRotate || autoRotate == "true") {
		$(document).ready(function(){
			autoTx = setInterval(function(){
				tsActive++;
				if (tsActive >= $("#WNTSWContent .thumbnails .wnDVSummary").length) {
					tsActive = 0;
				}
				tsaSwap();
			}, 5000);
		});
	} else {
		tsaSwap();	
	}		
}, 'WNCol23done');
</script></div><script>Worldnow.Helpers.debug('UB 2012 - Top Story Rotator ONLY(221383)', 'UB_221383');</script></div><!--END wnDSItems-->
