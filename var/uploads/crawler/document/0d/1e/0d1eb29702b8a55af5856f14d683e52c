		$(window).scroll(function() {
			//animation for inbox checker
			$('.home-ic-frame li').each(function(){
				var imagePos = $(this).offset().top;

				var topOfWindow = $(window).scrollTop();
					if (imagePos < topOfWindow+400) {	
						$(this).addClass("fadeIn");
					}
			});

			//animation for phone devices
			var y_scroll_pos = window.pageYOffset;
        	var scroll_pos_test = $('.home-responsive');

        	if(scroll_pos_test) {
	            $(".portrait .mobile-container").stop(true, true).animate({ scrollTop: 0 }, 900).delay(2000).animate({ scrollTop: 540 }, 900).delay(3000).animate({ scrollTop: 1100 }, 900);     	             
	            $(".portrait .ball").stop(true, true).animate({ top: 150 }, 900).delay(2000).animate({ top: 700 }, 900).delay(3000).animate({ top: 1255 }, 900);  
	            
	            $(".landscape .mobile-container").stop(true, true).animate({ scrollTop: 0 }, 900).delay(3500).animate({ scrollTop: 880 }, 900).delay(3500).animate({ scrollTop: 1590 }, 900);
	            $(".landscape .ball").stop(true, true).animate({ top: 100 }, 900).delay(3500).animate({ top: 980 }, 900).delay(3500).animate({ top: 1690 }, 900);   	            
	        }    

                //android show inbox thumbnails
	        var ua = navigator.userAgent.toLowerCase();
			var isAndroid = ua.indexOf("android") > -1; //&& ua.indexOf("mobile");
			if(isAndroid) {				
				$('.home-ic-frame > ul li').css('visibility', 'visible');
			}  

		});
	</script>


