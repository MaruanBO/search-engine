<script>
require(['jquery'], function($) {
    var $slider = $('#wbw-54e657b012c2f');

    var hasCarousel = typeof $.fn.carousel === 'function';
    if (hasCarousel) {
        $slider.carousel({
            interval: 10000
        });
    } else {
        return;
    }

    var supportsTouch = 'ontouchstart' in window || navigator.msMaxTouchPoints;
    var hasTouchwipe  = typeof $.fn.touchwipe === 'function';

    if (supportsTouch && hasTouchwipe) {
        $slider.touchwipe({
            wipeLeft: function(e) {
                e.preventDefault();
                $slider.carousel('next');
            },
            wipeRight: function(e) {
                e.preventDefault();
                $slider.carousel('prev');
            },
            preventDefaultEvents: false
        });
    }
});
</script>
