		function openActivityModal() {
			$(".activityModalContainer").css({ "z-index" : getMaxZ() + 1 }).fadeIn("fast");
			// $(".banner").addClass("hiddenBanner");
		}
		
		function closeActivityModal() {
			$(".activityModalContainer").fadeOut("fast");
			// $(".banner").removeClass("hiddenBanner");
		}
		
		function getMaxZ() {
		    var maxZ = Math.max.apply(null,$.map($('body > *'), function(e,n){
		           if($(e).css('position') !== 'static')
		                return parseInt($(e).css('z-index'))||1 ;
		           })
		    );
		    return maxZ ;
		}

		$(document).ready(function(){
			$(".popClose").click(closeActivityModal);
			
			$(".activityModalContainer").mousedown(closeActivityModal);
	
			
			$(".activityModal").click(function(e) {
				e.stopImmediatePropagation();
			});
			
			$(".activityModal").mousedown(function(e) {
				e.stopImmediatePropagation();
			});
		});
	</script>

