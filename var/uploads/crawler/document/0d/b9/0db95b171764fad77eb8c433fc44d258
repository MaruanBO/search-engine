			    $(this).qtip({ 
			        content: {
			            text: $(this).data("hovertip")
			        },
			        position: {
			        	my: 'bottom center',
			        	at: 'top center'
			        },
			        style: {
			        	classes: 'qtip qtip-default qtip-light qtip-rounded qtip-shadow qtip-pos-bc qtip-focus'
			        }
			    });
			});

		})();
	</script>







