	<script>
		function sbPromptForLogin() {
			openActivityModal();
		}
	
	
		$(".like").live('click',function(e){
			e.stopImmediatePropagation();
			var currentUser = "";
			var postId = $(this).attr('data-postId');
			var blogId = $(this).attr('data-blogId');
			if (!currentUser) {
				sbPromptForLogin();
			} else {
				if (!$(this).hasClass('selected')) {
					$(this).addClass('selected');
					$(".displease" + blogId + "_" + postId).removeClass('selected');
					console.log(".displease" + blogId + "_" + postId);
					$(".number" + blogId + "_" + postId).html('' + (parseInt($(".number" + blogId + "_" + postId).html()) + 1));
				}
				$.get(
					"/util/vote/" + blogId + "/" + postId,
					{
						vote : "up"
					},
					function(data) {
						console.log("Vote response: " + data);
					}
				)
			}
			return false;
		});
		$(".displease").live('click',function(e){
			e.stopImmediatePropagation();
			var currentUser = "";
			var postId = $(this).attr('data-postId');
			var blogId = $(this).attr('data-blogId');
			if (!currentUser) {
				sbPromptForLogin();
			} else {
				if (!$(this).hasClass('selected')) {
					$(this).addClass('selected');
					console.log(".like" + blogId + "_" + postId);
					$(".like" + blogId + "_" + postId).removeClass('selected');
					$(".number" + blogId + "_" + postId).html('' + (parseInt($(".number" + blogId + "_" + postId).html()) - 1));
				}
				$.get(
					"/util/vote/" + blogId + "/" + postId,
					{
						vote : "down"
					},
					function(data) {
						console.log("Vote response: " + data);
					}
				)
			}
			return false;
		});
	</script>

	<script>
		$(".tabset a").click(function(){
			var target = $(this).attr('href');
			$(".tabset a").removeClass('active');
			$(this).addClass('active');
			var height = $('.tab-content > .active').height();
			if (height < 1000) {
				height = 500;
			}

			$('.tab-content > div').removeClass('active').hide();
			$(target).addClass('active').show();
			
			if ($(target).attr('data-url')) {
				$(target).html($('.tabsetLoading').html());
				$(target).css('min-height',height + "px");
				initSameHeight();
				$.get($(target).attr('data-url'),function(data) {
					$(target).html(data);
					$(target).css('min-height',"0px");
					initSameHeight();
				});
			} else {
				$(target).css('min-height',"0px");
				initSameHeight();
			}
			
			return false;
		});
		$('.more').live('click',function(){
			var parent = $(this).parent();
			var url = $(this).attr('href');

			$(parent).html($('.tabsetLoading').html());
			initSameHeight();
			$.get(url,function(data) {
				$(parent).before(data);
				$(parent).remove();
				initSameHeight();
			});
			return false;
		});
			</script>
		
