<script>
	$("#sidebar .dl-menu a").click(function(e) {
		e.stopImmediatePropagation();
		
		if ($(this).parents("li").first().hasClass("topNewsLi")) {
			$("#sidebar .dl-menu>li").removeClass("active");
			$(this).parents("li").first().addClass("active");
			$($(this).data("subnav")).hide();
			window.location = $(this).attr("href");
		}

		// check if in tablet mode
		if ($(".tabletLi").css("position") === "relative") {
			if ($(this).parents("li").first().hasClass("active")) {
				$($(this).data("subnav")).toggle();
			} else {
				if ($(this).hasClass("topCat")) {
					$("#sidebar .dl-menu>li").removeClass("active");
					$(this).parents("li").first().addClass("active");
					$($(this).data("subnav")).show();
					$(".tabletSubnav").not($(this).data("subnav")).hide();
				}
			}
		} else {
			// non-tablet
			$("#sidebar .dl-menu>li").removeClass("active");
			$(this).parents("li").first().addClass("active");
			window.top.location = $(this).attr("href");
		}
		
		return false;
	});
</script>

