<script type="text/javascript" language="javascript">
/* Load Video */
function DPVideo(file, target, width, height) {
    if (document.browser && document.browser.H264 == true) {
        var video = document.createFullElement('video', {
            'width': width,
            'height': height,
            'id': 'TestingVideo',
            'controls': 'controls',
            'src':file
        });
        document.getElementById(target).appendChild(video);
    } else {
        var video = document.createFullElement('embed', {
            'id': 'JCPlayer',
            'name': 'JCPlayer',
            'classid': 'clsid:d27cdb6e-ae6d-11cf-96b8-444553540000',
            'width': width,
            'height': height,
            'src': 'http://uturn.liquidus.net/flash/jcplayer.swf?videoURL=' + escape(file) + '&amp;autoPlay=false&amp;backgroundColor1=0xe7e7e7&amp;backgroundColor2=0xc5c5c50&amp;inkColor=0x737570&amp;highlightColor=0xffbb03',
            'quality': 'high',
            'bgcolor': '#ffffff',
            'name': 'JCPlayer',
            'align': 'middle',
            'salign': 'TL',
            'scale': 'noScale',
            'allowscriptaccess': 'sameDomain',
            'allowfullscreen': 'true',
            'type': 'application/x-shockwave-flash',
            'pluginspage': 'http://www.macromedia.com/go/getflashplayer'
        });
        document.getElementById(target).appendChild(video);
    }
}
</script>



  
