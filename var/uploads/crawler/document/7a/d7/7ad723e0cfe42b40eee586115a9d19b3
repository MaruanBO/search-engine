function setSearchTarget(theSearch) {
    if (document.getElementById('sT2').checked == true) {
	document.getElementById('frmHeadSearch').action = 'http://www.lionbrand.com/cgi-bin/sqlsearch.fcgi';
    }
}

if (document.location.href.indexOf("sqlsearch") > -1) {
    document.getElementById('sT2').checked=true;
} else {
    document.getElementById('sT').checked=true;
}
