$(document).ready(function(){
	$('.slider_menu a').mouseover(function() {
		if (!$(this).hasClass("menu_on")) {
			$(".slider_menu a").removeClass("menu_on");
			$(this).addClass("menu_on");
			var activeTab = $(this).attr("rel");
			$(".slide_item").slideUp();
			$(activeTab).slideDown();
			return false;
		}
	});
});
</script>
