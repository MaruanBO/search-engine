<script type="text/javascript" language="javascript">
	var ltrValue = '';
	
	$(document).ready(function() {
	   
	   var listeners = new Array("a","form");
	   
	   for(x in listeners)
	   {
	   
		   $(listeners[x]).each(function()
		   {
		   
				if(ltrValue.length > 0)
				{
				
					if(x == 1)
					{
						var thisLink = $(this).attr("action");
					}else
					{
						var thisLink = $(this).attr("href");
					}
					
					var novaPattern = new RegExp("(^\/){1,1}|^http://([A-Za-z0-9\.]+)novadevelopment.com|\/nova2\/");
					
					if(novaPattern.test(thisLink))
					{
					
						//alert(thisLink);
						if(thisLink.search("[?/&]{1,1}ltr=")<0)
						{
							if(thisLink.indexOf("?") >= 0)
							{
								thisLink = thisLink + "&ltr=" + ltrValue;
							}else
							{
								thisLink = thisLink + "?ltr=" + ltrValue;
							}
						
							if(x == 1)
							{
								$(this).attr("action",thisLink);
							}else
							{
								$(this).attr("href",thisLink);
							}
						
						}
					
					}
				
				}
				
		   });
	   
	   }
	   
	});
</script>
