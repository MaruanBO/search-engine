<script>
 $(document).ready(function() {

		$("a").click(function() {
			//make sure the link is in the 'footer links'
			if($(this).parent().attr('id') == 'footer_topCat')
			{
				var linkHref = $(this).attr("href").replace(/(<([^>]+)>)/ig,"");
				
				// if the link href is not relative
				if(linkHref.charAt(0) != '/')
				{
					//create the base url
					var baseURI = location.protocol + '://' + location.hostname;
				
										
					// fetermine whether or not the clicked on link is external or not
					if(!linkHref.match("^" + baseURI))
					{
						//return false;
						
						var analyticsAccountNo = 'UA-223533-11';
						
  						try
  						{
    						var pageTracker=_gat._getTracker(analyticsAccountNo);
    						pageTracker._trackEvent('Outbound Links', linkHref);
    						setTimeout('document.location = "' + linkHref + '"', 100);
    						return false;
  						}catch(err){}
						
					}
				
				}
			}
		});

        $(".next").click(function () {
			//alert('working');
			var menuPossitem = $(this).parents('.menu_paging').attr('id');
			//alert('menuPossitem ='+menuPossitem);
			menuPossitem = menuPossitem.replace('menu_paging_','');
			//alert('menuPossitem ='+menuPossitem);
			var itemsMax = $(this).parents('.menu_paging').attr('rel');
			//alert('itemsMax ='+itemsMax);
			var count = $('#next_'+menuPossitem).attr('rel');
			//alert('count ='+count);
			if (count < itemsMax)
			{
					$('#next_'+menuPossitem).removeClass('transBtn');
					$('#prev_'+menuPossitem).removeClass('transBtn');
				//alert(count);
			  $(".menu_list_"+menuPossitem+"_"+count).fadeOut(120, function () {
				$(this).next().fadeIn(80);
				count++;
				if (count >= itemsMax)
				{
					$('#next_'+menuPossitem).addClass('transBtn');
					//$('#prev_'+menuPossitem).removeClass('transBtn');
				}
				$('#menu_paging_count_'+menuPossitem).text(count+'/'+itemsMax);
				//alert(count);
				$('#next_'+menuPossitem).attr('rel',count);
				$('#prev_'+menuPossitem).attr('rel',count);
			  });
		}
          return false;
        }); 
        $(".prev").click(function () {
			//alert('working');
			var menuPossitem = $(this).parents('.menu_paging').attr('id');
			//alert('menuPossitem ='+menuPossitem);
			menuPossitem = menuPossitem.replace('menu_paging_','');
			//alert('menuPossitem ='+menuPossitem);
			var itemsMax = $(this).parents('.menu_paging').attr('rel');
			var itemsMin = 1;
			var count = $('#prev_'+menuPossitem).attr('rel');
			if (count > itemsMin)
			{
				$('#prev_'+menuPossitem).removeClass('transBtn');
				$('#next_'+menuPossitem).removeClass('transBtn');
			  $(".menu_list_"+menuPossitem+"_"+count).fadeOut(120, function () {
				$(this).prev().fadeIn(80);
				count--;
				if (count <= itemsMin)
				{
					$('#prev_'+menuPossitem).addClass('transBtn');
				}
				$('#menu_paging_count_'+menuPossitem).text(count+'/'+itemsMax);
				//alert(count);
				$('#next_'+menuPossitem).attr('rel',count);
				$('#prev_'+menuPossitem).attr('rel',count);
			  });
		  }
          return false;
        }); 
	  }); 
</script>

