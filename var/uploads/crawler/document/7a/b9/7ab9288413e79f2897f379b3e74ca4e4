    <script type="text/javascript">
    function screenshiftFix() {
      var header = jQuery("header.region.region-header");
      if (header.hasClass("small")) {
	header.css("position","fixed");
	jQuery("#main").css("margin-top","58px");
      } else {
	header.css("position","static");
	jQuery("#main").css("margin-top","0");
      }
    }
    jQuery("header.region.region-header").append("<div style='clear:both'></div>");
    jQuery(window).scroll(screenshiftFix);
    jQuery(window).resize(screenshiftFix);
    screenshiftFix();
    </script>
