function checkadd() {
	 var valid = true, output = '';
		if (document.getElementById('zipcode').value.length < 5) {
		valid = false;
		output += 'Please enter a valid zipcode\n';
	}
	 if (!valid) {
		alert(output);
		return false;
	  } else {
		document.getElementById('pwaitrates').style.display = "block";
		document.getElementById('srchRates').style.display = "none";
		return true; }
}
	
