<script>
$(document).ready(function(){
    if($('#wallpaper').length > 0) {
        if($('#wrapper').length > 0) {
            $('#wallpaper').insertBefore($('#wrapper'));
            $('#wrapper').css({
                'position':'absolute',
                'left':'50%',
                'z-index':'2',
                'width':'960px',
                'margin-left':'-480px',
                'top':'0'
            });
        } else if ($('#container').length > 0) {
            $('#wallpaper').insertBefore($('#container'));
            $('#container').css({
                'position':'absolute',
                'left':'50%',
                'z-index':'2',
                'width':'960px',
                'margin-left':'-480px',
                'top':'0'
            });
        }
        var imgWidth = $('#wallpaper img').width()/2*-1 + 'px';
        $('#wallpaper').css({
            'position':'fixed',
            'left':'50%',
            'z-index':'0',
            'margin-left':imgWidth,
            'top':'0'
        });
    }
});
</script>
