$(document).ready(function() {
   $('<span style="font-size:10px;">F</span>').appendTo('#temp');

  $('select option').each(function() {
        if($(this).text() == "Calendar")
        {
            $(this).text("Select One");
        }
    });
    
    
   

    var aPos = [];
    var sSection = '/';
    var pos = sSection.indexOf("/");

    while (pos > -1) {
        aPos.push(pos);
        pos = sSection.indexOf('/', (pos+1));
    }

    var sSection2 = sSection.substr( (aPos[0]+1) , (aPos[1]-1) );
    if(sSection2 == 'sports')
    {
       if(!$('.fnp-news').children().length > 0){
            $('.fnp-news').css('display','none')
        } 
    }
    
    
   $('#blox-user-panel #uUser').text("Login or Signup below.");
   
    
});
</script>

