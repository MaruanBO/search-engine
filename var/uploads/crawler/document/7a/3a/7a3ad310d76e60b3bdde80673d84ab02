    <script type="text/javascript">
      enableVideoSwapping = false;
      jQuery(function(){
    enableVideoSwapping = true;
        jQuery("#simple-playerpagebottom").parents(".blox-multimedia").find(".blox-multimedia-text a.load-video").bind("click",function(){
        if(!jQuery(jQuery(this).parents(".item")[0]).hasClass("playing")){
if(enableVideoSwapping){
            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"this should be running the swap video script",application:"videoplayer"});}
            swapVideo("#simple-playerpagebottom",this);
        } }
        });
      });

if(typeof(swapVideo)=="undefined"){

function swapVideo(id, element){

jQuery(id).addClass("loading");

mediaPlayerId = id.replace("#","");
if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"swapping video",style:"divider top",application:"videoplayer"});}
      jQuery(element).parents(".item-list").find(".item").removeClass("playing");
            jQuery(jQuery(element).parents(".item")[0]).addClass("playing");

      if(jQuery(jQuery(element).parents(".item")[0]).find(".video-text").find("p").html()!=""){
          jQuery(element).parents(".blox-multimedia").find(".blox-multimedia-video").find(".blox-video-box").find(".blox-video-description").html(jQuery(jQuery(element).parents(".item")[0]).find(".video-text").find("p").html()).show(); 
      }else{ jQuery(element).parents(".blox-multimedia").find(".blox-multimedia-video").find(".blox-video-box").find(".blox-video-description").hide(); }
      jQuery(element).parents(".blox-multimedia").find(".blox-multimedia-video").find(".blox-video-box").find(".blox-video-title").html(jQuery(jQuery(element).parents(".item")[0]).find(".title").html());

    jQuery(id).html("");


var vidType = jQuery(jQuery(element).parents(".item")[0]).find(".video-type").text();
if(typeof(ninjaLog)!="undefined"){
    ninjaLog.Update({text:"determining video type",application:"videoplayer"});
    ninjaLog.Update({text:"video type is: "+vidType});
}
    if(vidType=="video"||vidType=="syndicaster"){
        var previewImage = false;
        if(typeof(jQuery(jQuery(element).parents(".item")[0]).find("img").attr("src"))!="undefined"){
            previewImage = jQuery(jQuery(element).parents(".item")[0]).find("img").attr("src");
        }

if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Player: Normal Video Asset",application:"videoplayer"});}

var useHTML5 = false;
var fileName = jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text();
if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"fileName: "+fileName});}

var fileType = "flv";
if(typeof(fileName)=="string"){ fileType = fileName.split(".")[fileName.split(".").length-1];}
var sources = "";

// DETECT APPLE DEVICE
usingAppleDevice = false;
if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPad/i)) || (navigator.userAgent.match(/iPod/i))) usingAppleDevice = true;
if((typeof(ninjaLog)!="undefined")&&usingAppleDevice){ninjaLog.Update({text:"Using Apple iOS device"});}

if(usingAppleDevice && fileType != "flv"){
// html5 support for internal videos
if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Using HTML5 mode",application:"videoplayer"});}
if(typeof(_gaq)!="undefined"){_gaq.push(['_trackEvent', 'Videos', 'Using HTML5: '+jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text()]);}
var useHTML5 = true;

jQuery(id).html("");if((fileType == "mp4" || fileType == "m4v") && (!jQuery.browser.mozilla)){ sources +="<source src=\""+jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text().trim()+"\" type=\"video/mp4;\" codecs=\"avc1.4D401E, mp4a.40.2\">"; }

if(sources != ""){
jQuery(id).html("<video poster=\""+jQuery(jQuery(element).parents(".item")[0]).find("img").attr("src")+"\" style=\"width:100%; height:100%;\" controls>\
"+sources+"\
    <p class=\"videoPlayerFail\">We Apologize for the Inconvenience</p>\
</video>");

}else { useHTML5 = false; ninjaLog.Update({text:"falling back to flash video"}); }

}

if(!useHTML5){
if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"type is video",application:"videoplayer"});}

        showAd = false;
        showPost = false;
        if(typeof(vo)!="undefined"){
            if(typeof(vo.embed[0])!="undefined"){
                showAd = true;
            
                automaticSwapFromAd = false;
            }

            if(typeof(vo.embedPost[0])!="undefined"){
                showPost = true;                    
            }
        }

        pl = new Array({url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:false,autoBuffering:false,bufferLength:15});
        if(showAd && showPost){ pl = [vo.embed[0].playlistItem,{url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:false,autoBuffering:true,bufferLength:15},vo.embedPost[0].playlistItem];
        } else if(showAd){ pl = [vo.embed[0].playlistItem,{url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:true,autoBuffering:false,bufferLength:15}]; }

        if(previewImage){
            pl = [{url: previewImage},{url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:false,autoBuffering:false,bufferLength:15}];

            if(showAd && showPost){ pl = [{url: previewImage},vo.embed[0].playlistItem,{url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:true,autoBuffering:false,bufferLength:15},vo.embedPost[0].playlistItem];
            }else if(showAd){ pl = [{url: previewImage},vo.embed[0].playlistItem,{url: jQuery(jQuery(element).parents(".item")[0]).find(".video-url").text(),autoPlay:true,autoBuffering:false,bufferLength:15}]; }
        }

var gacheck = false; if(typeof(_gaq)!="undefined"){ gacheck = true; }
flowplayer(mediaPlayerId, "http://bloximages.newyork1.vip.townnews.com/fredericknewspost.com/content/tncms/live/components/core_external_flowplayer/resources/flash/flowplayer-3.2.7.swf?_dc=1418921941",{
    wmode: 'opaque',
    clip: { autoPlay: true,
        onStart:function(clip){
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
           
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'play', "asset: " + clipTitle + " url: " + clip.url]);
            }
            }
        },
        onResume:function(clip){
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
           
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'resume', "asset: " + clipTitle + " url: " + clip.url]);
                }
            }
           
            if(typeof(vo.embed)!="undefined"){ if(typeof(ninjaLog)!="undefined"){ ninjaLog.Update({text:"Video - Resume - found vo.embed"}); }
                vo.embed[0].playerId = "mediaPlayerId";
                vo.embed[0].playerElement = jQuery("#mediaPlayerId")[0];
            }
        },
        
        onPause: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'pause', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
        },

        onStop: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'stop', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
        },

        onFinish: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'finish', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
            
        },
        
        
        scaling:'fit'
    },
    plugins:{
        controls: {
            progressColor: '#666666',
            durationColor: '#888888',
            buttonColor: '#888888',
            bufferColor: '#444444',
            tooltipTextColor: '#ffffff',
            timeBgColor: 'rgb(0, 0, 0, 0)',
            timeSeparator: ' ',
            volumeColor: '#888888',
            timeColor: '#888888',
            volumeSliderGradient: 'none',
            progressGradient: 'none',
            backgroundColor: '#222222',
            timeBorder: '0px solid rgba(0, 0, 0, 0.3)',
            backgroundGradient: [0.0,0,0.0],
            sliderColor: '#000000',
            borderRadius: '0px',
            sliderGradient: 'none',
            bufferGradient: 'none',
            autoHide: 'never',
            tooltipColor: '#000000',
            sliderBorder: '1px solid rgba(128, 128, 128, 0.7)',
            buttonOverColor: '#cccccc',
            volumeBorder: '1px solid rgba(128, 128, 128, 0.7)',
            volumeSliderColor: '#000000',
            height: 24,
            opacity: .8
        }
    },
    playlist:pl
});



}

}else{ if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"type is youtube or other",application:"videoplayer"});}
    showAd = false;
    automaticSwapFromAd = false;
    if(typeof(vo)!="undefined"){if(typeof(vo.embed[0])!="undefined"){if(!vo.embed[0].watched){
        showAd = true;
        automaticSwapFromAd = true;

        

vo.embed[0].playlistItem = {
    url:vo.embed[0].videoUrl,
    autoPlay:false,
    autoBuffering:false,
    onStart: function(clip){
        jQuery("#"+this.id()).addClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'play', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }
    
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Start Event: "+clip.url});}
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Playing: "+clip.url});}
                
    },
    
    onResume: function(clip) {  
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'resume', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
            }
    },

    onPause: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'pause', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }

        if(typeof(vo.embed[0].videoClickUri)!='undefined'){
            window.open(decodeURIComponent(vo.embed[0].videoClickUri));
        }
    },

    onStop: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'stop', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
            }
    },

    onFinish: function(clip) {
        jQuery("#"+this.id()).removeClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'finish', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }
                
        vo.embed[0].watched = true;
        vo.embed[0].lock = false;

            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"finished playing ad video"});}
        
        var next = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").first();
        if(automaticSwapFromAd && next.find(".video-type").text()!="video"){
            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"attempting to swap to active video in to "+this.id()});}
            swapSubstituteHTML("#"+this.id(),jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing a.load-video").first());

        }

        automaticSwapFromAd = false;
        vo.embed.shift();
        
    }
}


if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"attempting to push id and element into vo",application:"videoplayer"});}

        vo.embed[0].playerId = id;
        vo.embed[0].playerElement = element;

        var previewImage = false;
        if(typeof(jQuery(jQuery(element).parents(".item")[0]).find("img").attr("src"))!="undefined"){
            previewImage = jQuery(jQuery(element).parents(".item")[0]).find("img").attr("src");
        }

        if(previewImage){
            pl = [{url: previewImage},vo.embed[0].playlistItem];

        } else { pl = [vo.embed[0].playlistItem];}



var gacheck = false; if(typeof(_gaq)!="undefined"){ gacheck = true; }
flowplayer(mediaPlayerId, "http://bloximages.newyork1.vip.townnews.com/fredericknewspost.com/content/tncms/live/components/core_external_flowplayer/resources/flash/flowplayer-3.2.7.swf?_dc=1418921941",{
    wmode: 'opaque',
    clip: { autoPlay: true,
        onStart:function(clip){
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
           
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'play', "asset: " + clipTitle + " url: " + clip.url]);
            }
            }
        },
        onResume:function(clip){
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
           
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'resume', "asset: " + clipTitle + " url: " + clip.url]);
                }
            }
           
            if(typeof(vo.embed)!="undefined"){ if(typeof(ninjaLog)!="undefined"){ ninjaLog.Update({text:"Video - Resume - found vo.embed"}); }
                vo.embed[0].playerId = "mediaPlayerId";
                vo.embed[0].playerElement = jQuery("#mediaPlayerId")[0];
            }
        },
        
        onPause: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'pause', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
        },

        onStop: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'stop', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
        },

        onFinish: function(clip) {
            if(gacheck){
                var clipType = clip.url.split(".")[clip.url.split(".").length-1];
                var clipAdvert = false; if(clip.url.match(/asset/)==null){ clipAdvert=true; }
                var clipTitle = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").find(".title").first().text();
                
                if((clipType!="jpg")&&(!clipAdvert)){
                    _gaq.push(['_trackEvent', 'videos', 'finish', "asset: " + clipTitle + " url: " + clip.url]);
                }
                }
            
        },
        
        
        scaling:'fit'
    },
    plugins:{
        controls: {
            progressColor: '#666666',
            durationColor: '#888888',
            buttonColor: '#888888',
            bufferColor: '#444444',
            tooltipTextColor: '#ffffff',
            timeBgColor: 'rgb(0, 0, 0, 0)',
            timeSeparator: ' ',
            volumeColor: '#888888',
            timeColor: '#888888',
            volumeSliderGradient: 'none',
            progressGradient: 'none',
            backgroundColor: '#222222',
            timeBorder: '0px solid rgba(0, 0, 0, 0.3)',
            backgroundGradient: [0.0,0,0.0],
            sliderColor: '#000000',
            borderRadius: '0px',
            sliderGradient: 'none',
            bufferGradient: 'none',
            autoHide: 'never',
            tooltipColor: '#000000',
            sliderBorder: '1px solid rgba(128, 128, 128, 0.7)',
            buttonOverColor: '#cccccc',
            volumeBorder: '1px solid rgba(128, 128, 128, 0.7)',
            volumeSliderColor: '#000000',
            height: 24,
            opacity: .8
        }
    },
    playlist:pl
});

  
    } else { swapSubstituteHTML(id,element); } } else{ swapSubstituteHTML(id,element); } } else { swapSubstituteHTML(id,element); }

} jQuery(id).removeClass("loading"); }



function swapSubstituteHTML(id,element){
    if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"swapping active video"});}
    var playURL = $($(element).parents(".item")[0]).find(".video-url");
    var width = $(id).width();
    var height = $(id).height();
    
    if(playURL.length){
        $(id).html("");
        
        $(id).html(playURL.html().replace(/<!--\(TEMPLATE|\/TEMPLATE\)-->/g,'').replace('"','\"'));
        
        $(id).css({"height":height,"width":width});

        $(id).find("embed").css({"height":height,"width":width});
        $(id).find("embed").attr({"height":height,"width":width});   
    }
}

}

    </script>

</div>
