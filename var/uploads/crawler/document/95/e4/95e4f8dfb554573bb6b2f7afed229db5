<script>
    $('.bxslider').bxSlider();
    /* recipe tile actions */
    $(document).on("click", ".btn-add-grocery", function() {
        var userId =  -1;
        var recipeUrl = unescape($(this).parent().parent().parent().parent().parent().parent().parent().attr('data-url'));
        if ((userId == null) || (userId <= 0)) {
            showLoginDialog(recipeUrl);
            return;
        }
        var rid = $(this).attr('data-rid');
        var servings = $(this).attr('data-servings');
        $(this).fadeTo(250, 0.2, null);
        window.location = "/grocery/addrecipe/" + rid;
    });

    $(document).on("click", ".btn-add-menu", function() {
        var userId =  -1;
        var recipeUrl = unescape($(this).parent().parent().parent().parent().parent().parent().parent().attr('data-url'));
        if ((userId == null) || (userId <= 0)) {
            showLoginDialog(recipeUrl);
            return;
        }
        var rid = $(this).attr('data-rid');
        var servings = $(this).attr('data-servings');
        $(this).fadeTo(250, 0.2, null);
        addToPlan(rid, -1, servings);
    });

    $(document).on("click", ".try-btn", function() {
        var userId =  -1;
        var recipeUrl = unescape($(this).parent().parent().parent().parent().attr('data-url'));
        if ((userId == null) || (userId <= 0)) {
            showLoginDialog(recipeUrl+"?act=addTrySoon");
            return;
        }
        var username = '';
        var bOpenGraph = false;
        var rid = $(this).parent().parent().parent().parent().attr('data-rid');
        var servings = $(this).parent().parent().parent().parent().attr('data-servings');
        $(this).children().first().fadeTo(250, 0.2, null);

        if ($(this).children().first().hasClass('try-btn-attach-on')) {
            removeFromTrySoon(rid, userId, username, recipeUrl, bOpenGraph);
            $(this).children().first().removeClass('try-btn-attach-on');
            $(this).children().first().fadeTo(250, 1, null);

        } else {
            addToTrySoon(rid, userId, username, recipeUrl, bOpenGraph);
            $(this).children().first().addClass('try-btn-attach-on');
            $(this).children().first().fadeTo(250, 1, null);

        }
    });


    $(document).on("click", ".fa-heart", function() {
        var userId =  -1;
        var recipeUrl = unescape($(this).parent().parent().parent().parent().parent().parent().parent().attr('data-url'));

        if ((userId == null) || (userId <= 0)) {
            showLoginDialog(recipeUrl+"?act=addFavorite");
            return;
        }
        var username = '';
        var bOpenGraph = false;
        var rid = $(this).parent().parent().parent().parent().parent().parent().parent().attr('data-rid');
        var servings = $(this).parent().parent().parent().parent().parent().parent().parent().attr('data-servings');



        $(this).fadeTo(250, 0.2, null);

        if ($(this).hasClass('favorite-on')) {
            removeFromFavorites(rid, userId, username, recipeUrl, bOpenGraph);
            $(this).removeClass('favorite-on');
            $(this).fadeTo(250, 1, null);

        } else {
            addToFavorites(rid, userId, username, recipeUrl, bOpenGraph);
            $(this).addClass('favorite-on');
            $(this).fadeTo(250, 1, null);
            $(this).fadeTo(250, 1, null);
        }
    });

    initPunchUpPunchDown();
</script>

            </div>
        </div> 


        

