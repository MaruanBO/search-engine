<script>
    function setStateQE() {
        var jsonFavorites = [];
        var jsonTrySoons = [170816,26526,785281,160487,284374,912216,158447,741353,977816,559499,170412,190371,174609,176226,298781,677420,175487,791434,342614,163911,177857,285314,572533,163877,586714,572507,399944,93087,94147,736873,41059,387370,215537,593926,176854,738154,226851,572685,811769,332246,1125087,165203,221019,951409,1080534,380152,180446,633364,182212,1057342,175261,163703,237799,239314,168264,216824,764001,127415,998649,1071135,194423,976055,176977,168695,161425];
        $(".recipe-tile-full").each(function(index, el) {
            var rid = Number($(this).attr("data-rid"));
            if ($.inArray(rid, jsonFavorites, 0) >= 0) {
                $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.fa-heart').addClass("favorite-on");
            }
            if ($.inArray(rid, jsonTrySoons, 0) >= 0) {
                $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.try-btn-attach').addClass("try-btn-attach-on");
                var countText = $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.upvote-counter').html();
                countText = "You + " + countText.replace("You + ","");
                $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.upvote-counter').html(countText);
            }
        });
    }

    $.ajax(
        {
            url: '/utils/homerecipes?type=qe&uid=-1',
            type: 'GET',
            success:
                function(data) {
                    $("#tile1").html(recipeHtmlFromAPIRecipeInfoV2(data[0], false, false, false, false));
                    $("#tile2").html(recipeHtmlFromAPIRecipeInfoV2(data[1], false, false, false, false));
                    $("#tile3").html(recipeHtmlFromAPIRecipeInfoV2(data[2], false, false, false, false));
                    $("#tile4").html(recipeHtmlFromAPIRecipeInfoV2(data[3], false, false, false, false));
                    setStateQE();
                },
            error:
                function() {}
        }
    );


</script>
