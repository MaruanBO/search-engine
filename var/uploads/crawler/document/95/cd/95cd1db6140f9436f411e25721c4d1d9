<script type="text/javascript"> //START A NEW JAVASCRIPT BLOCK TO TRY TO PROTECT javascript from compilation errors elsewhere

//=============================[ START WINDOW EVENT SUPPORT ]======
	if( window.addEvent == null ){
		window.addEvent = function(obj, eventName, func)
		{
		      if(obj.addEventListener)
		            return obj.addEventListener(eventName, func, true);
		      else if(obj.attachEvent)
		      {
		            obj.attachEvent("on" + eventName, func);
		            return true;
		      }
		      return false;
		}
	}
//=============================[ END WINDOW EVENT SUPPORT ]======


//=============================[ START IE6 background image fix ]======
	function fixIE6BackgroundImages(){
		var agt = navigator.userAgent.toLowerCase();
		var appVer = navigator.appVersion.toLowerCase();
		    // *** BROWSER VERSION ***
		var is_konq = false;
		var kqPos = agt.indexOf('konqueror');
		if (kqPos != -1) {
		    is_konq = true;
		}
		var is_safari = ((agt.indexOf('safari') != -1) && (agt.indexOf('mac') != -1));
		var is_khtml = (is_safari || is_konq);
		var is_opera = (agt.indexOf("opera") != -1);
		var iePos = appVer.indexOf('msie');
		var is_ie = ((iePos != -1) && (!is_opera) && (!is_khtml));
		if (is_ie)
		{
		    try {
		        document.execCommand('BackgroundImageCache', false, true);
		    } catch(e) {
		        // this will fail for IE 6 prior to SP1.
		    }
		}
	}
	try{
		fixIE6BackgroundImages();
	}catch(e){}
//=============================[ END IE6 background image fix ]======
</script>


<script type="text/javascript"> //START A NEW JAVASCRIPT BLOCK TO TRY TO PROTECT javascript from compilation errors elsewhere
//=============================[ START WEBTRENDS ANALYTICS ]======

	function doWebtrendsTracking(){ 
	
    
	    if (typeof docInfo == 'undefined')
	    {
	    	var docInfo = "";
	    }
	    
	    if (typeof docUri == 'undefined')
	    {
	    	docUri = "\x2FRxSolWeb\x2Fmvc\x2Fhome.do";
	    }
	
		if (typeof thisTitle == 'undefined') 
		{
			if (docInfo == "")
			{
				docInfo = document.title;
			}
			if(docUri == ""){
				docUri = "/" + document.title;
			}
		}
		else
		{
			if (docInfo == "")
			{
				docInfo = thisTitle;
			}
			if(docUri == ""){
				docUri = "/" + thisTitle;
			}
		}

		// this hack fixes the webtrends issue with payment-info.ep for last 3 checkout tabs
		if (docUri.indexOf("payment-info.ep") >= 0)
		{
			var noSpaceDocInfo = docInfo.replace("OptumRx.com - ", "");
			noSpaceDocInfo = noSpaceDocInfo.replace(/ /gi, "_");
			docUri += "/" + noSpaceDocInfo;
		}

		try{
			var debugLoc = window.location.href;
			if( debugLoc && debugLoc.indexOf("debug=webtrends") >= 0 ){
				alert("DCS Site: " + sitename + " :: dcsUri: " + docUri + "  :: WT.ti: " + docInfo);
			}
		}catch(e){}
		
		//alert("DCS Site: " + sitename + " :: dcsUri: " + docUri + "  ::  dcsInfo: " + docInfo);
		dcsMultiTrack('DCS.dcssip',siteWebTagUrl,'DCS.dcsuri',docUri,'WT.ti',docInfo,'WT.cg_n','CONSUMER','WT.si_n','','WT.si_p','','WT.si_x','','WT.seg_1','','WT.vhseg_1','','WT.seg_2','','WT.vhseg_2','','WT.pn_sku','','WT.tx_u','','WT.tx_s','','DCSext.client','default')
	}
	

	$jq(document).bind("ready", doWebtrendsTracking); 

//	window.addEvent(window,"load", doWebtrendsTracking);
	
//=============================[ END WEBTRENDS ANALYTICS ]======

</script>

