<script type="text/javascript">
  	var thisTitle = html_entity_decode("OptumRx"); // browser title for this page
	var minTabHeight = 0;
	var maxTabHeight;
	var rotation;
	var rotation_on = 0;
	jQuery.noConflict();
	jQuery(document).ready(function(){

	jQuery('link[rel*=style][title]').each(function(i)
	{
		if(this.disabled == false)
		{
			if (this.getAttribute('title') == 'medium')
			{
				jQuery('#switch2').attr('style','display:visible');
				jQuery('#switch3').attr('style','display:none');
				jQuery("div.footerLine2").css('margin-left','40px');
				if (jQuery.browser.msie) 
 				{
 				    jQuery('#register').attr('style','height:67Px !important;');
 					
 				}
			}
			else if (this.getAttribute('title') == 'large')
			{
				jQuery('#switch3').attr('style','display:visible');
				jQuery('#switch2').attr('style','display:none');
				jQuery("div.footerLine2").css('margin-left','20px');
				if (jQuery.browser.msie) 
 				{
 					jQuery('#register').attr('style','height:69Px !important;');
 				}
			}
		}
	});
	jQuery('#switch3').click(function(){
		jQuery('#switch2').attr('style','display:visible');
        jQuery(this).attr('style','display:none');
		jQuery("div.footerLine2").css('margin-left','40px');
		if (jQuery.browser.msie) 
 		{
 			jQuery('#register').attr('style','height:67Px !important;');
 		}
	});
	jQuery('#switch2').click(function(){
		jQuery('#switch3').attr('style','display:visible');
        jQuery(this).attr('style','display:none');
		jQuery("div.footerLine2").css('margin-left','20px');
		if (jQuery.browser.msie) 
 		{
 			jQuery('#register').attr('style','height:69Px !important;');
 		}
	});



      	jQuery("#trademark").html("&#8482;");
		jQuery("#our_awards").css('display','block');



				jQuery('#slides').slides({
					preload: true,
					preloadImage: 'https://www.optumrx.com/rxsol/chp/html/ps/img/default/home/loading2.gif',
					play: 5000,
					pause: 2800,
					hoverPause: true
			});


		});



	    $jq(document).ready(function(){
	     $jq("img").each(function(){
            if($jq(this).attr("src").indexOf("cmsStaticContentPath")>=0){
             $jq(this).attr("src",$jq(this).attr("src").replace("cmsStaticContentPath","https://www.optumrx.com/vgnlive"));

            }
      });
	    });
</script>


