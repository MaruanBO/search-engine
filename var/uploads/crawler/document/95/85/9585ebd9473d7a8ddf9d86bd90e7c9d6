<script type="text/javascript">
	prodid = [], prodName = [],	price = [],	catId = [];
	function addslashes(str) {
		return (str + '').replace(/[\\"']/g, '\$&').replace(/\u0000/g, '\\0');
	}
	var array_elements = (''.replace('[', '').replace(']', '')).split(",");
	var pageType = ('homepage' != "") ? 'homepage' : 'home';
	if(pageType == 'cart'){
		$('td.info').each(function(){
			prodName.push(addslashes($(this).find('a.item-name').html().replace(/^\s+|\s+$/g, '')));
			prodid.push(($(this).find('span.item-number').html().replace(/^\s+|\s+$/g, '')) + '-' + $(this).find('input.cart-sku-id').val());
			catId.push($(this).find('input.cart-item-id').val());
		});
		$('td.col-price').each(function(){
			price.push(parseFloat($(this).find('div.cell-inner').html().replace(/^\s+|\s+$/g, '').replace('$','')));
		});
		$('span.item-number').each(function(){
			
		});
	}
	else if(pageType == "review checkout-process"){
		$('p.product-name').each(function(){
			prodName.push(addslashes($(this).html()));
		});
		$('td.col-price').each(function(){
			price.push(parseFloat($(this).find('div.cell-inner').html().replace(/^\s+|\s+$/g, '').replace('$','')));
		});
		$('td.col-item-num').each(function(){
			prodid.push(($(this).find('div.cell-inner').html().replace(/^\s+|\s+$/g, '')) + '-' + $(this).find('input.review-sku-id').val());
			catId.push($(this).find('input.removal-item-id').val());
		});
	}else{
		for ( var intt = 0; intt < array_elements.length; intt++) {
			prodid[intt] = (array_elements[intt].replace(/^\s+|\s+$/g, '')).replace('sku:', '' + '-');
		}
		prodName.push(addslashes(""));
	}
	var google_tag_params = {
		pagetype : pageType,
		prodid : prodid,
		pvalue : ('' != "") ? parseFloat('') : price,
		pcat : ('' != "") ? '' : catId,
		pname : prodName,
		use_case : 'retail'
	};
</script>
