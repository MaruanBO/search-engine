    <script language="javascript" type="text/javascript">
        function activateTeamTab(el, sport) {
           $(".tmtabs .tab a").removeClass('active'); $("#" + el).addClass('active');
           $("#tmTabBox div").hide(); $("#tmTab" + sport).show();
       }
      </script>
      
       
            <script language="javascript" type="text/javascript">
                $(document).ready(function () {
                    var user_pref = $.cookie('news_home');
                    if (user_pref != null) {
                        if (user_pref.toString().toUpperCase() != 'NONE') {
                            switch (user_pref.toString().toUpperCase()) {
                                case "MLB": activateTeamTab("tmtabHyp" + user_pref.toUpperCase(), user_pref.toUpperCase(), false); break;
                                case "NFL": activateTeamTab("tmtabHyp" + user_pref.toUpperCase(), user_pref.toUpperCase(), false); break;
                                case "NBA": activateTeamTab("tmtabHyp" + user_pref.toUpperCase(), user_pref.toUpperCase(), false); break;
                                case "NHL": activateTeamTab("tmtabHyp" + user_pref.toUpperCase(), user_pref.toUpperCase(), false); break;
                                default: break;
                            }
                        }
                    }
                });
           </script>
       
       

