	    <!-- START SURVEY TOOLTIP -->
	    <script type="text/javascript">
			var surveyWindow; 
			var today = new Date();
			var surveyCookieName = "2Q2013SurveyCookie";        // Cookie name here

			function Get_Cookie(name) {

			var start = document.cookie.indexOf(name+"=");
			var len = start+name.length+1;

			if ((!start) && (name != document.cookie.substring(0,name.length))) return null; 

			if (start == -1) return null;

			var end = document.cookie.indexOf(";",len);

			if (end == -1) end = document.cookie.length;

			return unescape(document.cookie.substring(len,end));
			}

			// This function should be run at the end of the page to enable the Survey.
			function showSurvey(event) {
			if (!Get_Cookie(surveyCookieName)) {
			showTooltip('surveyTooltip',325,event,this);
			}
			}

			//var expires = new Date(today.getTime() + (60000));  //expires in one minute
			//var expires = new Date(today.getTime() + (365 * 86400000));  //expires in one year
			var expires = new Date(today.getTime() + (365 * 21600000));  //expires in one quarter

			function Set_Survey_Cookie(name,value,expires,path,domain,secure) {
			document.cookie = name + "=" +escape(value) +
			( (expires) ? ";expires=" + expires.toGMTString() : "") +
			( (path) ? ";path=" + path : "") + 
			( (domain) ? ";domain=" + domain : "") +
			( (secure) ? ";secure" : "");
			}

			function openSurveyWindow() {
			Set_Survey_Cookie(surveyCookieName,'1',expires);
			var surveyWindow = window.open("http://www.gesurveys.net/survey/133379/163b/","surveyWindow","width=780,height=480,status,menubar,scrollbars,resizable");
			
			}
	    </script>

	    <style type="text/css">
			#surveyTooltip {
