<script>
miAppControler.prototype.googleadsense = 1;
switch (miAppControler.prototype.googleadsense)
{
case 0:
    // noop disabled - for the moment, we just won't show it;
    break;
case 1:
    // enable for preview and live
    document.write('<div id="googleAdSense">');
    mictxt_DrawGoogleAd();
    document.write('</div>');
    break;
case 2:
    // enable for preview only
    // How do I check to see if I'm on preview?
    break;
case 3:
    // present on preview and live with display hidden
    document.write('<div id="googleAdSense" style="display:none">');
    mictxt_DrawGoogleAd();
    document.write('</div>');
    break;
default:
    //noop should never get here, but should be same as case 0 for now change to case 1 after launch;
}
function mictxt_DrawGoogleAd()
{
    document.write('<sc'+'rip'+'t async src="http://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></sc'+'rip'+'t>');
    document.write('<center><ins class="adsbygoogle" '+'style="display:inline-block;width:300px;height:250px" '+'data-ad-client="ca-pub-7642884967523832" '+'data-ad-slot="1082539098"></ins></center>');
    document.write('<sc'+'rip'+'t>(adsbygoogle = window.adsbygoogle || []).push({});</sc'+'rip'+'t>');
}
</script>
