<script type="text/javascript">
var dsqPolling, dsqPollCount = 1, dsqTarget, insiteSource;
// an element generated by Disqus that proves its existence on the page
dsqTarget = '.dsq-request-user-logout';
insiteSource = "#pluckLogOut";
function dsqReplaceLink() {
   var target = $( dsqTarget );
   var source = $( insiteSource );
   // Make sure Disqus is loaded before doing anything else
   if( target !== null && source !== null && 'Guest' != account_user_name ) {
	// Disqus is loaded, kill polling for it
	clearInterval( dsqPolling );
	$(target).attr('href', $(source).attr('href'));
   }
   else {
	// Time boxing counter
	dsqPollCount = dsqPollCount + 1;	
	// If time box counter >=5 stop looking for Disqus
	if( dsqPollCount >= 5 ) {
		clearInterval( dsqPolling );
	}
   }
}
// Start looking for Disqus when the page loads
$(document).ready( function() {
	dsqPolling = setInterval( "dsqReplaceLink()", 3000 );
});
</script>
    
