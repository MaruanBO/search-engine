<script>
$(document).ready(function() {

	$('#state_donate_simple').focus(function(){
		$('#donate_err-donor-volunteer-form').hide();	
	});

	$('#form_donate_simple .mktInput input').each(function(){
		
		var default_value = $(this).val();
			
		if(default_value == 'Keywords' || default_value == 'Charity Name')
		{	
			$(this).css('color','#797979');					
		}
		else
		{
			$(this).css('color','#000');
		}
							
			$(this).focus(function(){
				$('#donate_err-donor-volunteer-form').hide();
				if($(this).val()=='Keywords' || $(this).val()=='Charity Name' || $(this).val()=='')
				{
					$(this).css('color','#000');
					$(this).val('');					
				}								
				});
			
			$(this).blur(function(){
				if($(this).val()=='')
				{
	
					if( $(this).attr('id') == 'keywords_donate_simple')
					{
						$(this).css('color','#797979');
						$(this).val('Keywords');
					}
					if( $(this).attr('id') == 'orgname_donate_simple')
					{
						$(this).css('color','#797979');
						$(this).val('Charity Name');
					}
					
				}								
				});	
		
		
		});
	
	
		
});

function formSubmitValidate()
{
var	err = 0;	
	if($('#keywords_donate_simple').val() == '' || $('#keywords_donate_simple').val() == 'Keywords')
	{
		err ++;
	}
	if($('#orgname_donate_simple').val() == '' || $('#orgname_donate_simple').val() == 'Charity Name')
	{
		err ++;
	}
	if($('#state_donate_simple').val() == '')
	{
		err ++;											
	}
	
	if(err == 3 )
	{
		$('#donate_err-donor-volunteer-form').show();
	}
	else
	{
		if($('#keywords_donate_simple').val() == 'Keywords')
		{
			$('#keywords_donate_simple').val('');
		}
		if($('#orgname_donate_simple').val() == 'Charity Name')
		{
			$('#orgname_donate_simple').val('');
		}

// Submit the form
		$("#form_donate_simple").get(0).target = "_blank";
		if (validateMain($("#form_donate_simple").get(0),1)) 
		{			
			$("#form_donate_simple").submit();

			if($('#keywords_donate_simple').val() == '')
			{
				$('#keywords_donate_simple').val('Keywords');
			}
			if($('#orgname_donate_simple').val() == '')
			{
				$('#orgname_donate_simple').val('Charity Name');
			}

			
		}

		
	}
}
</script>                	  <div id="block-views-homepage_social_proof-block_1" class="clear-block block block-views">


