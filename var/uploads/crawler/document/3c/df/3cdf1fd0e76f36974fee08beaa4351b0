<script>

    $(document).ready(function () {
        $('.btn-viewonta').click(function (e) {
            e.preventDefault();
            var linkUrl = $(this).parent().children('div').find('a').attr('href');
            window.open(linkUrl, "_tab");
        });
        $('.location-loc-tripadvisor .TA_logoWidget div a').each(function (i, u) {
            var linkurl = $(this).attr('href');
            $(this).parent().parent().parent().find('a.btn-viewonta').attr('href', linkurl);
        });
    });
