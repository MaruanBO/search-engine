  function randString() {
    var textArr = [];
    var possible = "abcdefghijklmnopqrstuvwxyz0123456789-";

    for( var i=0; i < 37; i++ ) {
      textArr.push(possible.charAt(Math.floor(Math.random() * possible.length)));
    }

    return textArr.join("");
  }

