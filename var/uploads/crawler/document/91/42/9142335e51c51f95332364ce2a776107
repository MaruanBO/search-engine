<script>
$(document).ready(function(e) {
    var spotlightHeight = $('#spotlight').outerHeight(),
	spotlightTopOffset = $('#spotlight').offset().top,
	ebHomeExist = $('#ebHome').length,
	bannerExist = $('.showBanner').length,
	wingsHeight = spotlightTopOffset + spotlightHeight;

if(ebHomeExist > 0){
	$('.fullsitewrapper').prepend('<div class="addWings"></div>');
	$('.addWings').height(wingsHeight + 'px');
}
});
</script>

