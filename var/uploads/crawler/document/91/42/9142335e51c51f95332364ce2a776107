     <script type="text/javascript">
        $('.slide_controls ul li').css('width',(100/$('.slide_controls ul li').length)+'%');
        var spotlight = $('#spotlight').spotlight({
            transition:'slide', 
            intervalLength: '5', 
            rotate:true, 
            mouseAction:'hover', 
            onSnap: function() {
                if ($('.slide5 ').hasClass('selected')) {
                    cmCreateElementTag('Hover', 'Slot6Appeared');
                }
            }  
        });
        
        $('#top_sellers').productseb({skuList:'16730108,05149001,42199402,09048092',Rpp:'4',grpSize:'4',imageSize:['large','l'],linkPDP:true,showBubbles:true},setupCM);
        $('#new_arrivals').productseb({skuList:'6238B,05005002,44700903,77249',Rpp:'4',grpSize:'4',imageSize:['large','l'],linkPDP:true,showBubbles:true},setupCM);
        $('#youtube_vid').youtube({id:'GmnAp2QVDbo'});	
        //collectionDisplay.init();

        function getcmstring(term) {
            var searchTerm = $.trim(term.toString().toLowerCase());
            searchTerm = searchTerm.replace(/[/]/ig,'');
            searchTerm = searchTerm.replace(/[^a-z0-9-+ ]/ig,'');
            searchTerm = searchTerm.replace(/ +/ig,'_');
            searchTerm = searchTerm.replace(/-+/ig,'');
            searchTerm = searchTerm.replace(/_+/ig,'_');

            return searchTerm;
        }
        function setupCM() {
            $('.product_cell').unbind('click');
            $('.product_cell').bind('click', function() {
               if($(this).parent('span').attr('class') == 'product_cell_wrapper'){
                var cm = $(this).parents('li').first().parent('ul').parent('div').attr('id') + "-_-" + (parseInt($(this).parents('li').first().find('a').index(this))+1) + "-_-" + getcmstring($(this).attr('title'));

               }else{
                var cm = $(this).parent('li').parent('ul').parent('div').attr('id') + "-_-" + (parseInt($(this).parent('li').children('a').index(this))+1) + "-_-" + getcmstring($(this).attr('title'));
               }
                cmCreateManualLinkClickTag('www.eastbay.com/?cm_re='+cm,getcmstring($(this).attr('title')));
            });


        }

    </script>
