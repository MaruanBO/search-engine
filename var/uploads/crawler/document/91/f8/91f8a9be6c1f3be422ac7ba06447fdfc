<script type="text/javascript">

    (function($) {

        $(document).ready( function() {
            /* mp */
            var html = "<ol class=\"mp-list\">";
            var mpi = 2;
            var c = 0;
            var grp = 1;

            $.getJSON("//s3.amazonaws.com/cdn.wbur.org/published/json/ga/wbur.json", function(data) {
                $.each(data, function(key, val) {
                    link = val.url.replace("http://","//");
                    html = html + "<li class=\"clearfix mp-group-"+grp+"\"><a href=\"" + link + "\" class=\"google-event\" data-ga-category=\"Most Popular\" data-ga-action=\"Click\" data-ga-label=\"" + val.title + "\">" + val.title + "</a></li>";
                    c++;
                    grp = (c % 10 == 0) ? grp+1 : grp;
                });
                html += "</ol>";
                $("#mp-list-wrap").html(html);
            });

            $('.mp-more').click(function() {
                $('.mp-list LI.mp-group-'+mpi).slideDown();
                $('.mp-more').fadeOut();
            });
        });

    })(jQuery);

</script>
