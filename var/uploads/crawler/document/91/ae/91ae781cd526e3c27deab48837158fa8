<script language="javascript">
    function doSearch() 
    {
        var sterm = document.getElementById('searchTerm');
        if (sterm != null) 
        {
            sterm = escape(sterm.value);
            window.location.href = "http://search.moaa.org/search?site=default_collection&client=default_frontend&output=xml_no_dtd&proxystylesheet=default_frontend&proxycustom=%253CHOME%2F%253E&q=" + sterm;
        }
    }

    function blockEnter(evt) 
    {
        evt = (evt) ? evt : event;
        var charCode = (evt.charCode) ? evt.charCode : ((evt.which) ? evt.which : evt.keyCode);
        if (charCode == 13 || charCode == 3) 
        {
            doSearch();
            return false;
        }
        else 
        {
            return true;
        }
    }

</script>

