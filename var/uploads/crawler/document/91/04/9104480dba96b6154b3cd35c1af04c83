        tlium.endsWithAndClean = function (str, suffix) {
            if (str.indexOf(suffix, str.length - suffix.length) !== -1){
                return str.substring(0, str.length - suffix.length);
            } else {
                return str;
            }
        };
        tlium.GetSiteProduct = function(urlHost) {
            try {
                if (urlHost) {
                    urlHost = urlHost.toLowerCase();

                    if (urlHost.indexOf("order") != -1 || urlHost.indexOf("accounts") != -1 || window.location.pathname.indexOf("Cart.aspx") > -1) {
