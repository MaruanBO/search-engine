                        if (defaultDelivery == "true") {
                            gf_odt = ":date_f";
                        } else if ("" != "" && "" != "undefined") {
                            gf_odt = ":date_t";
                        }
                        if ("" != "" && "" != "undefined") {
                            gf_zip = ":zip_t";
                        }
                        if ((gf_odt + gf_opg + gf_zip).toString().indexOf("_t") > -1) {
                            returnVal = "widget" + gf_zip + gf_odt + gf_opg;
                        }
                    } 
                    else 
                    {
                        if ("" != "" && "" != "undefined") 
                        {
                            switch ("") 
                            {
                                case "D":
