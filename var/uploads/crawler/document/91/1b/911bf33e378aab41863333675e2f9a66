<script type="text/javascript">
            window.fbAsyncInit = function() {
                FB.init({
                    appId: '373770139443958', status: true, cookie: true, xfbml: true
                });
            
                /* All the events registered */
                FB.Event.subscribe('auth.login', function(response) {
                    // do something with response
                    login();
                });
                FB.Event.subscribe('auth.logout', function(response) {
                    // do something with response
                    logout();
                });
            
                FB.getLoginStatus(function(response) {
                    if (response.session) {
                        // logged in and connected user, someone you know
                        login();
                    }
                });
            };
            (function() {
                var e = document.createElement('script');
                e.type = 'text/javascript';
                e.src = document.location.protocol +
                    '//connect.facebook.net/en_US/all.js';
                e.async = true;
                document.getElementById('fb-root').appendChild(e);
            }());
            
            function login(){
                FB.api('/me', function(response) {
                    document.getElementById('login').style.display = "block";
                    if(response.name){
                        document.getElementById('login').innerHTML = response.name + " successfully logged in!";
            
                        var status = 'Email registered : ' + response.email + '<br>';
                            
                        $('#status_div').html(status);
            
            $('#IBNSubscribeEmail1').val(response.email);
            $( "#IBNSubscribeButton1" ).trigger( "click" );
                    }
                });
            }
            function logout(){
                document.getElementById('login').style.display = "none";
            }
            
            function setStatus(status1){
                FB.api(
                  {
                    method: 'status.set',
                    status: status1
                  },
                  function(response) {
                    if (response == 0){
                        alert('The status is not set.');
                    }
                    else{
                        alert('The status has been set successfully.');
                    }
                  }
                );
            }
