<script type="text/javascript">
$("document").ready(function (){
    if (document.cookie.indexOf('emailregistered=true') == -1){
     if (window.location.href.indexOf('email') <= 0){
	       // load the overlay
        if (document.cookie.indexOf('visited=true') == -1) {
			var fifteenDays = 1000*60*60*24*15;
			var expires = new Date((new Date()).valueOf() + fifteenDays);
			document.cookie = "visited=true;path=/;expires=" + expires.toUTCString();
			setTimeout(function () {
			$.colorbox({width:"580px", inline:true, href:"#subscribe_popup"});
			},10000);
		}
 } else {
 			var year = 1000*60*60*24*365;
			var expires = new Date((new Date()).valueOf() + year);
			document.cookie = "emailregistered=true;path=/expires=" + expires.toUTCString();
 }}
 });
</script>
<!--end popup Script-->
