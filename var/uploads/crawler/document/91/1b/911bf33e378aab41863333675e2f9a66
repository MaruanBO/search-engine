function isValidEmailAddress(emailAddress) {

   var pattern = /^([a-zA-Z0-9])(([a-zA-Z0-9])*([._+-])*([a-zA-Z0-9]))*@(([a-zA-Z0-9-])+(.))+([a-zA-Z]{2,4})+$/;

   return pattern.test(emailAddress);

}



(function($) {

$.fn.EmailCampaignsSubscribe1 = function(client_id, lists, opt){

   var defaults = {

       form: "IBNSubscribeForm",

       email: "IBNSubscribeEmail",

       msg: "IBNStatusMessage",

       button: "IBNSubscribeButton",

       successMsg: "<div id='successMsg'><text>Thank you</text></div>",

       emailValidation: true,

       submitForm: false,

       errorMsg: "Please check your email"

   };

   

   var subscribeFunction = function(){

       var valid = true;

       if(opt.emailValidation && !isValidEmailAddress($("#"+opt.email).val())){

           valid = false;

       }

   

       if(valid) {

           var data = $("#"+opt.form).serialize();

           var img = "";

           for (i in lists) {

                img += "<img style='display:none;' src='http://server1.emailcampaigns.net/autoadd/?c="+client_id+"&"+data+"&lid="+lists[i]+"'>";

           }

           $("#"+opt.form).html(opt.successMsg + img);
		    var year = 1000*60*60*24*365;
			var expires = new Date((new Date()).valueOf() + year);
			document.cookie = "emailregistered=true;path=/;expires=" + expires.toUTCString();
                       _gaq.push(['_trackEvent', 'SubscribeEmail','Click', 'UnderNewscastRegistered'])
	   
       } else{

           $("#"+opt.msg).html(opt.errorMsg);

       }

   };

   

   var opt = $.extend(defaults, opt);

   $("#"+opt.button).click( subscribeFunction );

   $("#"+opt.form).submit( function(e) {

       if ( ! opt.submitForm ) {

           e.preventDefault();

           e.stopPropagation();

       }

       

       subscribeFunction();

   });

};

})(jQuery);

</script> 

