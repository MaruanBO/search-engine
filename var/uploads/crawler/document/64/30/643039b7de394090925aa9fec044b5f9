    <script>
      $(window).load(function(){
        // FORCING ALL LINK EXTERNAL TO ARDUINO.CC TO BE TARGET _BLANK
        $('a').each (function () {
          href = $(this).attr ('href');
          if (href.substring (0, 4) == 'http' && href.indexOf ('arduino.cc') == -1)
            $(this).attr ('target', '_blank');
        });
      });
    </script>

