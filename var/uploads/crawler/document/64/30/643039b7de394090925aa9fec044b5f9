    <!-- BACK TO TOP -->
    <div>
      <a href="#" class="go-top"><img src="../new_home/assets/icon-arrow.svg"></a>
      <script>
        $(document).ready(function() {
          // Show or hide the sticky footer button
          $(window).scroll(function() {
            if ($(this).scrollTop() > 200) {
              $('.go-top').fadeIn(200);
            } else {
              $('.go-top').fadeOut(200);
            }
          });

          // Animate the scroll to top
          $('.go-top').click(function(event) {
            event.preventDefault();

            $('html, body').animate({scrollTop: 0}, 300);
          })

          var fromNewsletterConfirmBrokenLink = "false";
          if(fromNewsletterConfirmBrokenLink == "true") {
            $('#newsletterModal').find('.modal-header-main').hide();
            $('#newsletterModal').find('.modal-header-alt').show();
            $('#newsletterModal').foundation('reveal', 'open');
          }

        });
      </script>
    </div>

