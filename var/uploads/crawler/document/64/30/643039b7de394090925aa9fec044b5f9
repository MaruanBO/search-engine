    <script src="new_home/js/vendor/modernizr.js"></script>
    <script src="http://arduino.cc/js/foundation.min.js"></script>
    <script src="http://arduino.cc/js/foundation.topbar.custom.js"></script>
    <!-- <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.min.js" type="text/javascript"></script> -->

    <script type="text/javascript">
    // LAZY LOAD
      $(document).foundation();
      $("img").unveil(200);

      function fill_banner (adv, data) {
        $(adv).find ('img').attr ('src', data.image).attr ('alt', data.alt);
        $(adv).parent ().attr ('href', data.link);
        if (data.text != '') {
          var textLinesArray = data.text.split("\n");
          var htmlLines = '';
          for (var i=0; i<textLinesArray.length; i++) {
            htmlLines =  htmlLines + ['<p style="margin:0">',
                                           '<span style="background-color: '+data.color+'; padding: 3px 5px 3px 5px; margin-left: -10px">',
                                             textLinesArray[i],
                                           '</span>',
                                       '</p>'].join("\n"); 
          }
          $(adv).append('<div class="banneroverlay"><div class="bannercontents">' + htmlLines + '</div></div>');
          
        }
          
      }

      function populate_banner (data) {
        if (data.main.length != 0) {
          m = data.main [0];
          fill_banner ($('.adv:eq(0)'), m);
        }

        for (i = 0; i < data.random.length; i++)
          fill_banner ($('.adv:eq(' + (i + 1) + ')'), data.random [i]);
      }

      // LOADING "BANNERS" FROM EXTERNAL SOURCE
      $.getJSON ('/tools/homecontrol/publish/img/?id=' + (Math.random() * 9999999999), function (data) {
        populate_banner (data);
      });

    // WHAT IS ARDUINO BOX ANIMATION
      $(function()
      {
          var expanded = false;
          $('.sidebar_what').click(function(){
              if (!expanded)
              {
                  $('#main_wrapper').animate({'margin-top':'500px'}, {
                    duration: 900,
                    easing: 'easeOutBack'
                  });
                  $('#ardu_what').animate({'z-index':'10'});
                  expanded = true;
              }
          });
          $('#close').click(function(){
              if (expanded)
              {
                  $('#ardu_what').animate({'z-index':'-1'}, 80);
                  $('#main_wrapper').animate({'margin-top':'50px'}, {
                    duration: 800,
                    easing: 'easeInOutCirc'
                  });
                  expanded = false;
              }
          });
      });
    </script>

    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-22581631-3']);
