                                                 <script type="text/javascript" charset="utf-8">
    jQuery(document).ready(function(){
        /* JQuery UI autocomplete */
        var obj = jQuery("#search_string_top")
            .focus()
            .autocomplete({
                source: "/search/autocomplete",
                minLength: 3,
                delay: 150,
                focus: function(event, ui) {
                    // Set the store ID. Future version should determine store / vs product & set product ID / store ID
                    jQuery("#search_store_id_top").val(ui.item.id);
                },
                select: function(event, ui) {
                    // Set the store ID. Future version should determine store / vs product & set product ID / store ID
                    jQuery("#search_store_id_top").val(ui.item.id).trigger('change');
                    window.location.href = "/store/" + ui.item.value;
                }
            });

            obj.data("ui-autocomplete")._renderItem = function(ul, item) {
                var row = $("<a></a>")
                    //.append('<img src="http://cdn0.resellerratings.com/CDN-1424377914/store/thumbnail/' + item.value + '.jpg" class="store-thumb-small">')
                    .append(item.label);
                return $("<li>")
                    .addClass('autocomplete-entry')
                    .append(row)
                    .appendTo(ul);
            };
    });
    function urlencode(str) {
        return encodeURIComponent(str);
    }
    function searchClick_top()
   {
       var queryString = urlencode(jQuery('#search_string_top').val());
       var url = jQuery('#search_url_top').val();
       window.location = url + 'search/' + queryString;
       return false;
   }
</script>
