$.tools.validator.fn('input[required]', 'Please provide this mandatory field', function(input, value) {
  value = $.trim(value);
  return (value != '');
});
$.tools.validator.fn('select[required]', 'Please provide this mandatory field', function(input, value) {
  return (input.find('option:selected').length != 0);
});
$.tools.validator.fn('textarea[required]', 'Please provide this mandatory field', function(input, value) {
  value = $.trim(value.replace(/\<br[ \/]*\>/gi,''));
  return (value != '');
});
$.tools.validator.fn('input[data-url]', 'Please provide this mandatory field', function(input, value) {
  value = $.trim(value);
  var regex = /^([A-Za-z]+:\/+)?([A-Za-z0-9\.\-]+)\.([A-Za-z0-9\.]{2,})(\/[^#]*)?(#.*)?$/;
  return (value == '' || regex.test(value));
});
$.tools.validator.fn('input[data-email]', 'Please provide this mandatory field', function(input, value) {
  value = $.trim(value);
  var regex = /^[A-Za-z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[A-Za-z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[A-Za-z0-9](?:[A-Za-z0-9-]*[A-Za-z0-9])?\.)+[A-Za-z0-9](?:[A-Za-z0-9-]*[A-Za-z0-9])?$/;
  return (value == '' || regex.test(value));
});
$.tools.validator.fn('input[data-equal]', 'Value not equal with the previous field', function(input, value) {
  value = $.trim(value);
  var name = input.attr('data-equal');
  field = this.getInputs().filter('input[name='+name+']');
  return (value == $.trim(field.val()));
});
function submitform(form,formdata,successhandler,progresshandler,multipart){
  var dataformpercent=false;
  var remotepercent=false;
  var callprogress=function(){if(multipart&&remotepercent){var percent=(dataformpercent?(dataformpercent+remotepercent)/2.0:remotepercent);progresshandler.call(form,percent);}else if(dataformpercent)progresshandler.call(form,dataformpercent);};
  var formaction='\/';
  var interval=false;
  if(multipart){
    formaction='\/upload';
    var uuid='';
    for(var i=0;i<32;i++)
      uuid+=Math.floor(Math.random()*16).toString(16);
    formaction+=(formaction.indexOf('?')!=-1?'&':'?')+'X-Progress-ID='+uuid;
    $(form).attr('action',formaction);
    interval=window.setInterval(function(){
      $.ajax({url:'/progress',data:'X-Progress-ID='+uuid,type:'GET',timeout:30000,async:true,cache:false,dataType:'json',processData:true,contentType:'application/x-www-form-urlencoded; charset=UTF-8',context:this}).done(function(data){if(!data)data={'state':'error'};switch(data.state){case 'uploading':var percent=(data.received?data.received:0.0)/(data.size?data.size:1.0)*100.0;if(!remotepercent||remotepercent<percent){remotepercent=percent;callprogress();}break;case 'done':case 'error':window.clearTimeout(interval);break;}});
    },1000);
  }
  $(form).ajaxSubmit({url:formaction,data:$.extend({},{language:window.contentlanguage?window.contentlanguage:'KiaLEJq2fBR'},formdata),dataType:'html',success:function(data,textStatus,jqXHR){if(interval)window.clearTimeout(interval);successhandler.call(form,data);},uploadProgress:function(event,position,total,percentComplete){dataformpercent=position/total*100.0;callprogress();},error:function(){if(interval)window.clearTimeout(interval);$(form).each(function(index){this.submit();});},forceSync:true});
}
window.historyKey=0;window.historyData=false;function pophistory(key,params){var oldkey=window.historyKey;var oldstate=window.historyData[oldkey];var newkey=key;var newstate=window.historyData[newkey];dialogclose();if(newkey+1==oldkey&&oldstate&&typeof(oldstate.clickhandler)=='function'){oldstate.clickhandler();if(typeof(newstate.clickhandler)=='function')newstate.clickhandler();}else if(oldkey+1==newkey&&newstate&&typeof(newstate.clickhandler)=='function'){if(typeof(oldstate.clickhandler)=='function')oldstate.clickhandler();newstate.clickhandler();}else{if(oldstate&&typeof(oldstate.closer)=='function')oldstate.closer();if(newstate&&typeof(newstate.loader)=='function')newstate.loader();else loadpage(params||'');}window.document.title=(newstate&&newstate.title)?newstate.title:'gloria.tv - the more catholic the better';window.historyKey=newkey;}if(history.pushState){window.onpopstate=function(event){if(window.historyData===false){if(!event.state)return;window.historyData=[];}var key=event.state?event.state.key:0;var params=event.state?event.state.params:null;pophistory(key,params);};}else{$(window).on('hashchange',function(){var hash=$.trim(window.location.hash.replace(/^#/,''));var key=hash.length!=0?parseInt(hash.replace(/~.*$/,'')):0;if(key==window.historyKey)return;var params=hash.replace(/^.*~/,'');pophistory(key,params);});}
function pushhistory(params,uri,clickhandler){
  var createItem=function(clickhandler){var loaded=false;var title=false;var scrolltop=false;var navigation=false;var highdefinition=false;var container=false;return{closer:function(){if(!loaded)return;$('.animator').finish();title=window.document.title;scrolltop=$(document).scrollTop();var mediaelements=$('#hdscreen,#container').find('video,audio,object,embed');if(mediaelements.length){loaded=false;mediaelements.filter('video,audio').each(function(){try{this.pause();this.src='';this.load();}catch(e){}});}else{navigation=$('#navigation').children().clone();highdefinition=$('#hdscreen').children().detach();container=$('#container').children().detach();}},loader:function(){if(loaded){$('#hdscreen').children().detach();$('#container').children().detach();substitute(title,navigation,highdefinition,container);$(window.pageYOffset!=null?'html':'body').animate({scrollTop:scrolltop+'px'},800);}else if(!clickhandler)loadpage(params);},done:function(){loaded=true;},remove:function(){if(clickhandler)return;if(navigation)navigation.remove();if(highdefinition)highdefinition.remove();if(container)container.remove();},clickhandler:clickhandler?clickhandler:false};};if(window.historyData===false){var item=createItem();item.done();window.historyData=[item];}var key=window.historyKey;if(!clickhandler&&window.historyData[key]&&typeof(window.historyData[key].closer)=='function')window.historyData[key].closer();window.historyKey=(key+=1);for(var i=key;i<window.historyData.length;i++)window.historyData[i].remove();window.historyData.splice(key,window.historyData.length);var item=createItem(clickhandler);window.historyData[key]=item;if(history.pushState){var state={'key':key,'params':params,'uri':uri};history.pushState(state,null,uri);}else{window.historyKey=key;window.location.hash='#'+key+'~'+encodeURIComponent(params);}return item;}
function inject(container,html){
  if(html.length==0){container.empty();return;}var animator=$('<div>').addClass('animator');if(!$.support.leadingWhitespace)animator.css('position','relative');var scrolldown=container.parents('.letterhistory').length?container.parents('.letterhistory'):null;if(scrolldown)container.append(animator);else container.prepend(animator);animator.append(html).hide();transformelements(animator,false);var scroll=animator.height();animator.animate({opacity:'show',height:'show',paddingTop:'show',paddingBottom:'show',marginTop:'show',marginBottom:'show'},800,function(){transformelements(animator);if(document.activeElement&&$.contains(this,document.activeElement))return;if($(this).find('video,audio,object,embed').length)return;$(this).children().unwrap();});if(scrolldown)scrolldown.animate({scrollTop:'+='+scroll},800);
}
function substitute(title,navigation,hd,container){
  if(title&&title.length)window.document.title=title;if(navigation&&navigation.length){navigation.each(function(index){var id=this.id;var button=$(this);if($('#'+id).is(':visible')&&button.css('display')=='none')$('#'+id).animate({opacity:'hide',height:'hide',paddingTop:'hide',paddingBottom:'hide',marginTop:'hide',marginBottom:'hide'},'slow');else if(!$('#'+id).is(':visible')&&button.css('display')!='none'){$('#'+id).replaceWith(button.hide());$('#'+id).animate({opacity:'show',height:'show',paddingTop:'show',paddingBottom:'show',marginTop:'show',marginBottom:'show'},'slow');}else $('#'+id).replaceWith(button);});}if(hd&&hd.length){var hdscreen=$('#hdscreen').empty().show();inject(hdscreen,hd);}if(container&&container.length)inject($('#container'),container);
}
