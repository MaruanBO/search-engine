<script type="text/javascript">
$(document).ready(function() {
	var searchInputUnselectedText = "Search Local Businesses";
	if(searchInputUnselectedText != $("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").val() )
	{
		$("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").addClass("localedge_focus");
	}
	
	$("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").focus( function() {
		if(searchInputUnselectedText == $(this).val())
		{ 
			$(this).val("");
			$(this).addClass("localedge_focus");
		}
	});
	$("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").blur( function() {
		if("" == $(this).val())
		{ 
			$(this).val(searchInputUnselectedText);
			$(this).removeClass("localedge_focus");
		}
	} );
	//don't submit form if they didn't type anything
	$("div#localedge_integratedSearch .localedge_submit").click(function (event) {
		var currentValue = $("div#localedge_integratedSearch div.localedge_header span.localedge_buttons input.localedge_searchInput").val();
		if (currentValue == searchInputUnselectedText) {
			event.preventDefault();
		}
	});
});
</script>  </div>

  
