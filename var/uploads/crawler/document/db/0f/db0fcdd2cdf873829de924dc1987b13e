<script type="text/javascript">

(function($){
    var load_slider = function() {
      var is_mode_slideshow = $('.js-CollectionListSlider .CollectionListSlider-slide').length > 1;
      $('.flexslider').flexslider({
          animation: "slide",
          controlNav: "thumbnails",
          directionNav: false,
          touch: false,
          slideshow: is_mode_slideshow,
          start: function() {
              $('.flexslider .slides li').addClass('is-visible');
              $(".flexslider ol li").removeClass("activeFlexLi");
              $(".flexslider ol li img.flex-active").parent().addClass("activeFlexLi");
          },
          after: function() {
              $(".flexslider ol li").removeClass("activeFlexLi");
              $(".flexslider ol li img.flex-active").parent().addClass("activeFlexLi");
          }
      });

      if (is_mode_slideshow) {
        $('.js-CollectionListSlider').addClass('is-mode-slideshow');
      }
    };

       $('.js-CollectionListSlider .CollectionListSlider-image:eq(0)')
    .one('load', load_slider)
    .each(function() {
      if(this.complete) $(this).load();
    });


}(jQuery));
</script>


