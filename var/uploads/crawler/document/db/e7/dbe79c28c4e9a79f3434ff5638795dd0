<script>
    $(document).ready(function () {
        videoTop.bind('play', function () {
            videoBottom.pause();
        });

        videoBottom.bind('play', function () {
            videoTop.pause();
        });
    });

    (function () {
        var app = angular.module('app', []);
        app.controller('SubscriberController', SubscriberController);

        SubscriberController.$inject = ['$http', '$scope', '$window'];

        function SubscriberController($http, $scope, $window) {
            $scope.subscribe = function (subscriber) {
                $http({
                    url: '/infrastructure/services/Subscribe.php',
                    method: 'POST',

                    data: $.param({
                        userDetails: subscriber,
                    }),

                    headers: {
                        'Content-Type': 'application/x-www-form-urlencoded',
                    },

                }).success(function (response) {
                    $window.location.href = '/bridge1a.php?ba810subemail=' + subscriber.email + '&ba810subname=' + subscriber.name + '&bid=' + '';
                    $scope.submitSuccess = true;
                    $scope.submitError = false;
                }).error(function (reason) {
                    $scope.errorMessage = reason.error;
                    $scope.submitError = true;
                });
            }

            $scope.access = function () {
                $http({
                    url: '/members.php',
                    method: 'POST',
                    params: {
                        accessCode: $scope.accessCode,
                    },
                }).success(function (response) {
                    $window.location.href = '/members/index.php';
                }).error(function (reason) {
                    $('#modalTrial').modal('show');
                });
            }
        }


        app.controller('ImagesController', ImagesController);

        ImagesController.$inject = [];

        function ImagesController() {
            var vm = this;
            vm.limitLg = 7;
            vm.limitMd = 7;
            vm.limitSm = 5;
            vm.limitXs = 2;
            vm.addMoreLg = addMoreLg;
            vm.addMoreMd = addMoreMd;
            vm.addMoreSm = addMoreSm;
            vm.addMoreXs = addMoreXs;
            vm.allPicturesShownLg = false;
            vm.allPicturesShownMd = false;
            vm.allPicturesShownSm = false;
            vm.allPicturesShownXs = false;
            var path = '/images/salespage/userimages';

            function addMoreLg() {
                vm.limitLg += 7;
                if (vm.limitLg == 28) {
                    vm.allPicturesShownLg = true;
                }
            };

            function addMoreMd() {
                vm.limitMd += 7;
                if (vm.limitMd == 28) {
                    vm.allPicturesShownMd = true;
                }
            };

            function addMoreSm() {
                vm.limitSm += 5;
                if (vm.limitSm == 25) {
                    vm.allPicturesShownSm = true;
                }
            };

            function addMoreXs() {
                vm.limitXs += 2;
                if (vm.limitXs == 28) {
                    vm.allPicturesShownXs = true;
                }
            };






            vm.images = [
                path + '/1.jpg',
                path + '/2.jpg',
                path + '/3.jpg',
                path + '/4.jpg',
                path + '/5.jpg',
                path + '/6.jpg',
                path + '/7.jpg',
                path + '/8.jpg',
                path + '/9.jpg',
                path + '/10.jpg',
                path + '/11.jpg',
                path + '/12.jpg',
                path + '/13.jpg',
                path + '/14.jpg',
                path + '/15.jpg',
                path + '/16.jpg',
                path + '/17.jpg',
                path + '/18.jpg',
                path + '/19.jpg',
                path + '/20.jpg',
                path + '/21.jpg',
                path + '/22.jpg',
                path + '/23.jpg',
                path + '/24.jpg',
                path + '/25.jpg',
                path + '/26.jpg',
                path + '/27.jpg',
                path + '/28.jpg',
            ];
        };





    }());
</script>


