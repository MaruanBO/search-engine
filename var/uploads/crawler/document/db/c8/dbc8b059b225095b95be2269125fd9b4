    <script type="text/javascript">
        (function() {
            jQuery(document).on('ready', function() {

                // Menu Controller

                var $container = jQuery('#complex-menu');
                var main_active = false;
                var is_animating = false;

                var open_main = function(callback) {
                    var $main_menu = jQuery('#complex-menu .main-menu');

                    if( ! $main_menu.length || $main_menu.hasClass('open') ) return;

                    // Make sure the page is scrolled far enough so the topbar is fixed to top
                    var topbar_height = jQuery('#complex-navbar').outerHeight();
                    var $target = jQuery('#cmn_wrap');

                    is_animating = true;
                    main_active = true;

                    // toggle open class
                    $main_menu.addClass("open");

                    jQuery('body').addClass('menu-open');

                    // slide menu
                    $main_menu.animate({
                        left: '0px'
                    }, 250, 'swing', function() {
                        is_animating = false;

                        if( callback && typeof callback === 'function' ) {
                            callback();
                        }
                    }.bind(this));

                    // GA-Event
                    /////////////sendPageEvent('Channel Menu', 'Open menu', 'Menu Icon');
                };

                var close_main = function(callback) {
                    var $main_menu = jQuery('#complex-menu .main-menu');

                    if( ! $main_menu.length || ! $main_menu.hasClass('open') ) return;

                    // toggle open class
                    $main_menu.removeClass("open");

                    // Slide the topbar logo back
                    jQuery('body').removeClass('menu-open');

                    is_animating = true;

                    // slide menu
                    $main_menu.animate({
                        left: -$main_menu.width()
                    }, 250, 'swing', function() {
                        main_active = false;
                        is_animating = false;

                        if( callback && typeof callback === 'function' ) {
                            callback();
                        }
                    }.bind(this));
                };


                // Button Controller
                var menuClickHandler = function(event) {
                    event.preventDefault();

                    if( is_animating ) return;

                    // toggle
                    if( main_active ) {

                        close_main();
                        ////////////////sendPageEvent('Channel Menu', 'Close menu', 'Close Icon');

                        // Stop listening
                        jQuery(document).off('click.complex-menu');
                    }
                    else {

                        open_main();

                        // Listen for a click on the document (to close the menu)
                        setTimeout( function() {
                            // Wait a bit until the menu animates in
                            jQuery(document).on('click.complex-menu', documentClickHandler);
                        }, 250);
                    }
                };

                // Enable menu button
                jQuery('#complex-navbar .slideout-menu-toggle').on('click', menuClickHandler);

                var documentClickHandler = function(event) {

                    // If the target of the click is not the menu, nor a descendant of the menu
                    if( ! $container.is(event.target) && $container.has(event.target).length === 0 ) {

                        close_main();
                        /////////////////sendPageEvent('Channel Menu', 'Close menu', 'Outside click');

                        // Stop listening
                        jQuery(document).off('click.complex-menu');
                    }
                }

                // Manually scroll the menu if possible to disable body scrolling
                jQuery('#complex-menu .scroll-wrapper').on('mousewheel DOMMouseScroll', function(event) {

                    var scrollTo = null;

                    if( event.type == "mousewheel" ) {
                        scrollTo = event.originalEvent.wheelDelta * -1;
                    }
                    else if( event.type == "DOMMouseScroll" ) {
                        scrollTo = 40 * event.originalEvent.detail;
                    }

                    if( scrollTo ) {
                        event.preventDefault();

                        jQuery(this).scrollTop( scrollTo + jQuery(this).scrollTop() );
                    }
                });

            });
        })();
    </script>

