var downNavMenu;
	var downNavConClose=function(){
		if(downNavMenu){downNavMenu.css('display', 'none');}
	}
	var downNavConOpen=function(){
		//alert("aaa");
		downNavMenu = $(this).find('ul').eq(0).css('display', '');
	}
	$(document).ready(function(){
		$(".downNav>li").bind("mouseover",downNavConOpen);
		$(".downNav>li").bind("mouseout",downNavConClose);
	});
	document.onclick = downNavConClose;
</script>
