<script type="text/javascript">

	document.getElementById('secondPoll').style.display = 'none';
	document.getElementById('thirdPoll').style.display = 'none';
function display() {
		var radios = document.getElementsByName('firstChoice');
		if(radios[0].checked ) {
			document.getElementById('secondPoll').style.display = 'block';
			document.getElementById('thirdPoll').style.display = 'block';

		}
	  
		else {
			document.getElementById('secondPoll').style.display = 'none';
			document.getElementById('thirdPoll').style.display = 'none';	

			
			var radios = document.getElementsByName('secondChoice');
			for(var i = 0, length = radios.length; i < length; i++) {
				radios[i].checked = false;
			}
		}
}



</script>


