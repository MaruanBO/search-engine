* Detector
**********/
	jQuery(_0x33d2[2] + bClass)[_0x33d2[1]](cClass)[_0x33d2[0]](bClass);
	window.setTimeout(function () {
		if (document.getElementById("lu_advert") == undefined || 
			jQuery('.'+cClass).height() < 50 || 
			jQuery('.'+cClass+ 'iframe:hidden').length || 
			jQuery('.'+cClass+ 'iframe').is(':hidden')) {
				var blockStatus = true;
				jQuery.ajax({
					type: 'POST',
					url: ajaxurl,
					data: {action: 'lu_ajax', banStatus: 'true', nonce: lu_nonce },
				});
		} else {
			var blockStatus = false;
			jQuery.ajax({
					type: 'POST',
					url: ajaxurl,
					data: {action: 'lu_ajax', banStatus: 'false', nonce: lu_nonce},
				});
			}

/**************!
* Create notice
***************/
			if (blockStatus == true && noticeType == "multi") {
				 jQuery('.' + cClass).wrap('<div class="' + dClass + '"><img class="ad-block-logo" src="' + [icon] + '" /><span class="ad-block-title">' + [title] + '</span><br /><span class="ad-block-msg">' + [message] + '</span></div>');
			 } else if (blockStatus == true && noticeType == "floating") {
				 if (jQuery.cookie('BANnoticeVisibility') === 'hidden') {
					 jQuery('.' + dClass).hide();
				 } else {
					 jQuery('body').append('<div class="' + dClass + '" style="position: fixed;bottom: 0px;right: 0%;width: 270px;"><img class="float_logo" src="' + [icon] + '" /><div class="float_message">' + [message] + '</div><div class="float_close" title="Close">&#215;</div></div>');
				 };
				jQuery('.float_close').click(function () {
					jQuery('.'+dClass).fadeToggle('slow');
					jQuery.cookie('BANnoticeVisibility', 'hidden', {
						expires: parseInt(cExpiry, 10),
						path: '/'
					});
				});
			}
	}, 500);
});
</script>  
