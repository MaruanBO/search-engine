var dClass		= 'kf08wskodljy2pe67m1cgthra3nu5bi4qvxz9';		
var ajaxurl		= 'http://patriotupdate.com/wp-admin/admin-ajax.php';	 	// define ajax url
var lu_nonce	= '541dfe40f1';	// define nonce for ajax security
var _0x33d2 = ["\x72\x65\x6D\x6F\x76\x65\x43\x6C\x61\x73\x73", "\x61\x64\x64\x43\x6C\x61\x73\x73", "\x2E"];

