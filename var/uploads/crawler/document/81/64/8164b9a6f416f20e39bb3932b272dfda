            <script type="text/javascript">
              var value1 = $j('input:hidden[name=gsafilter]').val();

              $j("#btnG").click(function() {
                u = "/search?q=" + $j("input#srch-term").val() + ((typeof (value1) === 'undefined' || value1 == "") ? "" : "&" + value1);
                window.location.replace(u);
              });

              $j('#srch-term').bind('keypress keydown keyup', function(e) {
                if (e.keyCode == 13 && $j(this).val()) {
                  e.preventDefault();
                  u = "/search?q=" + $j("input#srch-term").val() + ((typeof (value1) === 'undefined' || value1 == "") ? "" : "&" + value1);
                  window.location.replace(u);
                }
              });

