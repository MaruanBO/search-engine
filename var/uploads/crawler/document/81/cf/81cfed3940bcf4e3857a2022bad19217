<script src="/../v2/script/tvears_v2.js"></script>
<script type="text/javascript">

    $(document).ready(function () {
        $('#subscriptForm').dialog({
            autoOpen: false,
            width: 215,
            height: 230
        });
        $(".subscribeBtn").on("click", function () {
            $('#subscribe-field').text('');
            $("#firstname").val($("#subFirstName").val());
            $("#lastname").val($("#subLastName").val());
            $("#email").val($("#subEmail").val());
            $("#globalsubscriptionstatus").val(1);
            $("#submitter").trigger("click");
        });
        $('body').keypress(function (e) {
            if (e.which == 13) {
                if ($("#subscribe-field").is(":focus")) {
                    e.preventDefault();
                    $('#subscribe-button').click();
                }
                else
                    return true;
            }
        });
    });

    /*
    * Calls the WebHelper with the email and it will update or create a contact
    */
    function subscribeEmail() {
        var email = $('#subscribe-field').val();
        if (email) {
            $.ajax({
                type: 'POST',
                url: tvears.Settings.Services.WebHelper + '&task=subscribe&email=' + email,
                dataType: 'jsonp',
                error: function (req, status, error) {
                    $('#subscribe-field').text('');
                },
                success: function (output) {
                    if (output.IsValid == true) {
                        $('#subscribe-field').text('');
                        $("#firstname").val(output.FirstName);
                        $("#lastname").val(output.LastName);
                        $("#email").val(output.Email);
                        $("#globalsubscriptionstatus").val(1);
                        $("#submitter").trigger("click");
                    }
                    else {
                        $("#subEmail").val(output.Email);
                        $("#subscriptForm").dialog("open");
                    }
                }
            });
        }
    };
</script>
