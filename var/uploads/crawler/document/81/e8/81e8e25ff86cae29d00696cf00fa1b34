  <script>
    $.noConflict();

    (function($, document) {
        function supports_html5_storage() {
          try {
            return 'localStorage' in window && window['localStorage'] !== null;
          } catch (e) {
            return false;
          }
        }

        // Start Newsletter Popup
        $('#myModal')
          .on('click', 'button.js-join', function(e) {
            e.preventDefault()
            $(this).prop('disabled', true)

            var $form = $(this).closest('form'),
                $email = $form.find('.input-control'),
                url,
                query
            if ($email.val() == '') {
              $email.addClass('required')
              return
            } else {
              $email.removeClass('required')
            }
            query = $form.serialize()

            url = 'http://www.naturallycurly.com/site/newsletterJsonp/?callback=window.do_newsletter&' + query;

            $('script').eq(0).after($('<script />')
              .attr({
                async: true,
                type: 'text/javascript',
                src: url
              }))
          });

        var threeDays,
          now = new Date(),
          nlDate,
          key = 'CN_NEWSLETTER',
          showPopup = true,
          delay = 10;

        if (supports_html5_storage())
        {
          threeDays = new Date()
          threeDays.setDate(now.getDate() + 3)
          nlDate = localStorage.getItem(key)

          if (nlDate === null) {
            localStorage.setItem(key, threeDays.getTime())
          } else {
            if (now.getTime() > nlDate) {
              localStorage.setItem(key, threeDays.getTime())
            } else {
              showPopup = false
            }
          }
        }

        if (showPopup) {
          setTimeout(function() {
            _gaq.push(['_trackEvent', 'email-popup','display', "time", now.getTime()]);
            $('#myModal').modal();
          }, delay * 1000);
        }

        window.do_newsletter = function(data) {
          if (data.error != null)
          {
            alert(data.error)
          } else {
            var $holder = $('#myModal')
            $holder.find('.modal-content.the-form').hide()
            $holder.find('.modal-content.thank-you').show()

            setTimeout(function() {
              $holder.modal('hide')
            }, 5 * 1000)
          }
        }

        // End Newsletter Popup
        
        // Begin click events
        $('body').on('click', function(e) {
            var $el, action, category, label, value, _path, _ref, _slug;
            $el = $(e.target);
            label = $el.attr('data-track-label') || $el.text();
            _path = window.location.pathname;
            _slug = _path.match(/^\/(?:curl)?([^\/]+).+$/) || [0, (_path === "/" ? "homepage" : window.location.path)];

            if ($el.is('.global-click-track') || $el.parents().is('.global-click-track')) {
                return true;
            }
            if ($el.is('.global-no-track,[data-no-track]') || $el.parents().is('.global-no-track,[data-no-track]')) {
                return true;
            }

            if ($el.is('img')) {
                label = $el.attr('src').match(/^.+\/([^.]+\..+)$/)[1];
            }
            if (!$el.is('a') && $el.parents('a:first')) {
                $el = $el.parents('a:first');
            }
            if ($el.text() === '' && $el.is('a') && label === '') {
                label = $el.attr('href');
            }

            category = $el.attr('data-track-category') || _slug[1];
            action = $el.attr('data-track-action') || 'Clicked';
            value = $el.attr('data-track-value') || null;
            label = label.indexOf("\n") > -1 ? $el.attr('href') || "page" : label;

            _gaq.push(['_trackEvent', category, action, label, value]);
            
        }).on('nc-deliverd', function(e) {
            var action, category, label, _path, _slug;
            _path = window.location.pathname;
            _slug = _path.match(/^\/(?:curl)?([^\/]+).+$/) || [0, (_path === "/" ? "homepage" : window.location.path)];
            category = _slug[1];
            action = 'Delivered';
            label = _path;
            
            _gaq.push(['_trackEvent', category, action, label, null]);
            
        });
        $(document).on('ready', function(e) {
            setTimeout(function() {
                $('body').trigger('nc-deliverd');
            }, 20 * 1000);
        });

    })(jQuery, document);
  </script>

