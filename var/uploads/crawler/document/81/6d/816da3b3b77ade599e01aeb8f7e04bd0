<script type="text/javascript">
  var showHumanVerificationQuestion = function () {
    $('#cardnumberLoginButton').overlay($.extend({}, standardOverlaySettings, {load:true}));
  };

  $(document).ready(function () {
	
    // init cardnumber field
    $('#card_card_number').onlyAllowStrictNumerics();

    // load the captcha view
    $("#humanVerificationContainer").load('/welcome/are_you_human');

    // captcha click
    $('.humanImage').live('click', function () {
      $('#areYouHumanAnswer').val($(this).attr('answer'));
      $('#cardnumberLoginButton').overlay().onClose = function () {
        $('#cardnumberForm').submit();
      };
      $('#cardnumberLoginButton').overlay().close();
    });

    //captcha refresh
    $('#refreshHumanImages').live('click', function (e) {
      e.preventDefault();
      $("#humanVerificationContainer").load('/welcome/are_you_human');
    });

    // form submit handler
    $('#cardnumberForm').live('submit', function (e) {
      $('#cardnumberLoginButton').showProgress();
      if (this.isValid()) {
        if ($('#areYouHumanAnswer').val() === "no") {
          e.preventDefault();
          showHumanVerificationQuestion();
        } else {
        }
      } else {
        e.preventDefault();
        $('#cardnumberLoginButton').hideProgress();
      }
    });
	
  });
</script>
