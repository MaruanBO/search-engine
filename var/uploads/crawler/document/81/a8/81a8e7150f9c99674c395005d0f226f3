			getConditions();
			
			function getConditions() {
				
				if (referrer.indexOf("zergnet.com") >= 0 || referrer.indexOf("nrelate.com") >= 0 || referrer.indexOf("adblade.com") >= 0 || referrer.indexOf("disqus.com") >= 0 || referrer.indexOf("gravity.com") >= 0 || referrer.indexOf("outbrain.com") >= 0 || referrer.indexOf("taboola.com") >= 0 || referrer.indexOf("revcontent.com") >= 0 || referrer.indexOf("hexagram.com") >= 0 || document.URL.indexOf("utm_source=ContentAD") >= 0 || referrer.indexOf("mediafed.com") >= 0 || referrer.indexOf("powerinbox.com") >= 0 || document.URL.indexOf("cpc=true") >= 0 || referrer.indexOf("rantgirls.com") >= 0 || document.URL.indexOf("utm_source=Gravity") >= 0 || document.URL.indexOf("utm_medium=referral") >= 0 || document.URL.indexOf("utm_medium=Referral") >= 0 || document.URL.indexOf("utm_medium=referralTop") >= 0 || document.URL.indexOf("utm_source=slideshow") >= 0) {
					window.cpc = true;
				}
				else {
					window.cpc = true;
				}
				
				if (width<768) {
					window.mobile=true; window.tablet=false; window.desktop=false;
				}
				else if (width>767 && width<1025) {
					window.mobile=false; window.tablet=true; window.desktop=false;
				}
				else if (width>1024) {
					window.mobile=false; window.tablet=false; window.desktop=true;
				}
				
			}
