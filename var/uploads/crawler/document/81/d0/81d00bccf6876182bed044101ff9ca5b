        /* can't do this check in headerVelocity.html because query params are cached by the Content Server */
        if (window.top.location.host.indexOf("sports") >= 0 && (window.top.location.href.indexOf("tmi=") >= 0 || window.top.location.href.indexOf("mi=") >= 0)) {
            document.getElementById("heading").className = document.getElementById("heading").className.replace( /(?:^|\s)fixedHeaderWidth(?!\S)/g , ' autoHeaderWidth' );
        }
        var pathName = document.location.pathname;
        pathName = (pathName.indexOf("/") === 0) ? pathName.substring(1, pathName.length) : pathName;
        pathName = pathName.replace(betfair.defaults.productName + "/", "");
        pathName = pathName.replace(betfair.defaults.brandId + "/", "");
        pathName = pathName.replace(betfair.defaults.regionId + "/", "");
        pathName = pathName.replace(betfair.defaults.localeId + "/", "");

