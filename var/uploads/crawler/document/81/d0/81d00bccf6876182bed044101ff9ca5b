        <!-- Header Analytics must be included after the Betfair Analytics Library has been included -->
<script type="text/javascript">
    //JSLint Parameters
/*global betfair: false, i13n: false, window: false, console: false, YUI: false */

var betex = betex || {};
betex.util = betex.util || {};
betex.util.Analytics = betex.util.Analytics || {};

betex.util.Analytics.LoginObserver = function () {
	var login_i13n, mediaplex_i13n, loginNode, defaultParams;
    defaultParams = {
        'prod': betfair.defaults.productName,
        'region': betfair.defaults.regionId,
        'loc': betfair.defaults.localeId,
        'brnd' : betfair.defaults.brandId,
        'status' : betfair.defaults.themeName,
        'page_name' : 'loggedin'
    };

	YUI({bootstrap: false}).use('*', function (Y) {
		loginNode = Y.one('#account');                          // An arbitrary but semantically-useful Node
		login_i13n = new Y.i13n({
			'class': 'login_i13n',
			'eventClass': 'login_i13n',
			'node': loginNode
		});

        Y.on("i13n_mediaplex_generic_login:postParam", function(e, node, params) {
            var value = {
                'int'                 : 'I',      // Interactive?
                'interactive'         : 'I',      // Interactive? (full length key)
                'log'                 : 'Log',    // Log param
                'ver'                 : 'Ver',    // Library Version
                'pkg'                 : 'G',      // Package/Market etc.
                'package'             : 'G',      // Package/Market etc. (full length key)
                'ltxt'                : 'X',      // Link indentifier
                'link'                : 'X',      // Link indentifier (full length key)
                'pg'                  : 'P',      // Page identifier
                'page'                : 'P',      // Page identifier (full length key)
                'pos'                 : 'N',      // Position (link order)
                'position'            : 'N',      // Position (link order) (full length key)
                'sec'                 : 'S',      // Section
                'section'             : 'S',      // Section (full length key)
                'tab'                 : 'T',      // Tab
                'li'                  : 'L',      // Is the User logged in?
                'user'                : 'U',      // User account ID
                'module'              : 'M',      // Module
                'platform'            : 'pl',     // Platform
                'test'                : 'A',      // Indentify User in 'A/B' Testing
                'gen'                 : 'Z',      // Other information
                'sport'               : 'Sp',     // Sport
                'ref'                 : 'dr',     // Referrer information
                'referrer'            : 'dr',     // Referrer information (full length key)
                'prod'                : 'product',   // Comodity key shortening for 'product'
                'loc'                 : 'locale',    // Comodity key shortening for 'locale'
                'brnd'                : 'brand',   // Comodity key shortening for 'brand'
                'thm'                 : 'status',    // Comodity key shortening for 'theme'
                'user_search_term'    : 'Ust',    // Comodity key shortening for 'user_search_term'
                'user_screen_size'    : 'Us',     // Comodity key shortening for 'user_screen_size'
                'user_viewport_size'  : 'Uvs'     // Comodity key shortening for 'user_viewport_size'
            }, key, item = null, temp;
            for (item in defaultParams) {
                // We need to make sure we throw away any
                // unwanted keys for optimisation, null, undefined, empty and 'function' are discarded
                if (defaultParams.hasOwnProperty(item) && defaultParams[item] !== undefined && defaultParams[item] !== null && defaultParams[item] !== "" && //< need to be precise with what gets filtered out
                        typeof(defaultParams[item]) !== 'function') {
                    key = (value[item] || item);
                    temp = defaultParams[item];
                    delete defaultParams[item];
                    defaultParams[key] = temp;
                }
            }
            params = defaultParams;
        });
        mediaplex_i13n = new Y.i13n({
            'class' : 'i13n_mediaplex_generic_login',
            'eventClass' : 'i13n_mediaplex_generic_login',
            'keysMapping': false,
            'bubbleToParent': false,
            'server': 'img-cdn.mediaplex.com',
            'file': '0/16689/universal.html',
            'createTransportNode': function () {
                var iFrame = document.createElement("iframe");
                iFrame.setAttribute('style', 'height: 0px; width: 0px; border: 0px; border-style: initial; border-color: initial; display: none;');
                Y.one("body").append(iFrame);
                return iFrame;
            },
            'defaultKeys': defaultParams
        });
	});

    this.mediaplex_i13n = mediaplex_i13n;
	this.i13n = login_i13n;
	this.node = loginNode;

	this.logoutSourceParams = null;
	this.loginSourceParams = null;

	betfair.Events.delegate.general.custom.addListener([
		{
			event: betfair.Events.custom.User.dataUpdated,          //have to wait for user data to be ready
			handler: this.loggedInStateHandler,
			context: this
		},
		{
			event: betfair.Events.custom.User.loginState.loggedOut,
			handler: this.loggedOutStateHandler,
			context: this
		},
		{
			event: betfair.Events.custom.LoginObserver.notifyLoginStarted,
			handler: this.loginStartedHandler,
			context: this
		},
		{
			event: betfair.Events.custom.LoginObserver.failedLogin,
			handler: this.concludedLoginHandler,
			context: this,
			param: {'W': 'LoginFailure'}
		},
		{
			event: betfair.Events.custom.LoginObserver.messagedLogin,
			handler: this.concludedLoginHandler,
			context: this,
			param: {'W': 'LoginLimbo'}
		},
		{
			event: betfair.Events.custom.LoginObserver.notifyLogoutStarted,
			handler: this.logoutStartedHandler,
			context: this
		},
		{
			event: betfair.Events.custom.LoginObserver.successfullyLoggedOut,
			handler: this.concludedLogoutHandler,
			context: this,
			param: {'W': 'LogoutScs'}
		},
		{
			event: betfair.Events.custom.LoginObserver.abandonedLogOut,
			handler: this.concludedLogoutHandler,
			context: this,
			param: {'W': 'LogoutAbandoned'}
		}
	]);
};

betex.util.Analytics.LoginObserver.prototype.loggedInStateHandler = function () {
	var accountId, callParams;

	if (!betfair.CookieManager.get("alog")) {
		accountId = betfair.User.getData("accountId") + "";
		betfair.CookieManager.set("alog", accountId, 0, null, null, true);      //set alog as a SESSION cookie with 0 day expiry

		callParams = {
			'prod': betfair.defaults.productName,
			'region': betfair.defaults.regionId,
			'thm' : betfair.defaults.themeName,
			'brnd': betfair.defaults.brandId,
			'loc': betfair.defaults.localeId,
			'ref': '@ref',
			'user': accountId,
			'bucket': '@cookie-bucket',
			'user_screen_size' : '@screensize',
			'W': 'LoginScs'
		};
		if (this.loginSourceParams) {
			callParams = this.mergeObjects(callParams, this.loginSourceParams);
		}
		this.loginSourceParams = null;
		this.i13n.makeCall(this.node, callParams);
        var that = this;
        if (thirdparty.isIE()) {
            that.mediaplex_i13n.makeCall(that.node, {});
        } else {
            setTimeout(function() {
                that.mediaplex_i13n.makeCall(that.node, {});
            }, 0);
        }
    }
};

betex.util.Analytics.LoginObserver.prototype.loggedOutStateHandler = function () {
	// ensure the user account cookie is always empty when we're in the logged out state
	betfair.CookieManager.clear("alog", true);
};

betex.util.Analytics.LoginObserver.prototype.loginStartedHandler = function (sourceParams) {
	this.loginSourceParams = this.getParamsToStore(sourceParams);
};

betex.util.Analytics.LoginObserver.prototype.concludedLoginHandler = function (loginFailureKey, loginStatusParams) {
	var callParams = {
		'prod': betfair.defaults.productName,
		'region': betfair.defaults.regionId,
		'thm' : betfair.defaults.themeName,
		'brnd': betfair.defaults.brandId,
		'loc': betfair.defaults.localeId,
		'ref': '@ref',
		'user': '@cookie-alog',
		'bucket': '@cookie-bucket',
		'user_screen_size' : '@screensize',
		'Err': loginFailureKey
	};

	callParams = this.mergeObjects(callParams, loginStatusParams);
	callParams = this.mergeObjects(callParams, this.loginSourceParams);
	this.i13n.makeCall(this.node, callParams);

	this.loginSourceParams = null;
};

betex.util.Analytics.LoginObserver.prototype.logoutStartedHandler = function (sourceParams) {
	this.logoutSourceParams = this.getParamsToStore(sourceParams);
};

betex.util.Analytics.LoginObserver.prototype.concludedLogoutHandler = function (logoutStatusParams) {
	var callParams = {
		'prod': betfair.defaults.productName,
		'region': betfair.defaults.regionId,
		'thm' : betfair.defaults.themeName,
		'brnd': betfair.defaults.brandId,
		'loc': betfair.defaults.localeId,
		'ref': '@ref',
		'user': '@cookie-alog',
		'bucket': '@cookie-bucket',
		'user_screen_size' : '@screensize'
	};
	if (this.logoutSourceParams) {
		callParams = this.mergeObjects(callParams, logoutStatusParams);
		callParams = this.mergeObjects(callParams, this.logoutSourceParams);
		this.i13n.makeCall(this.node, callParams);
	}
	this.logoutSourceParams = null;
};

// CONVENIENCE METHODS

betex.util.Analytics.LoginObserver.prototype.getParamsToStore = function (sourceParams) {
	// clone sourceParams so we leave it untouched
	var paramsToStore = this.mergeObjects(sourceParams, null);
	// we don't wish to store the 'ltxt' aka. 'X' parameter
	delete paramsToStore.ltxt;
	delete paramsToStore.X;
	delete paramsToStore.x;	//watch for typos
	return paramsToStore;
};

betex.util.Analytics.LoginObserver.prototype.mergeObjects = function (masterObj, childObj) {
	//child object's properties will supersede any shared properties of the master
	var property, mergeObj = {};

	for (property in masterObj) {
		if (masterObj.hasOwnProperty(property)) {
			mergeObj[property] = masterObj[property];
		}
	}

	for (property in childObj) {
		if (childObj.hasOwnProperty(property)) {
			mergeObj[property] = childObj[property];
		}
	}
	return mergeObj;
};
    // Don't attach to the dom if 'i13nLatestEventsInitialise' is already attached
    if (typeof i13nLatestEventsInitialise !== 'function') {
        var i13nLatestAccountEvents = {
            DOM: [
                { element: window, eventType: "DOMContentLoaded",   handler: i13nLatestAccountEventsInitialise},
                { element: window, eventType: "unload",             handler: i13nLatestAccountEventsUnload}
            ]
        },
        i13nLatestAccountEventHandler = new betfair.Events.delegate(i13nLatestAccountEvents, true);
    }
    function i13nLatestAccountEventsInitialise() {
        betex.util.Analytics.loginObserver = new betex.util.Analytics.LoginObserver();
        YUI({bootstrap: false}).use("*", function(Y) {
            var defaultKeys = {
                'prod': betfair.defaults.productName,
				'region': betfair.defaults.regionId,
				'thm' : betfair.defaults.themeName,
				'brnd': betfair.defaults.brandId,
				'loc': betfair.defaults.localeId,
				'ref': '@ref',
				'user': '@cookie-alog',
				'bucket': '@cookie-bucket',
				'user_screen_size' : '@screensize'
            };
            var account_i13n = new Y.i13n({
                'class' : 'account_i13n',
                'eventClass' : 'account_i13n',
                'node' : Y.one('#account'),
                'listenOn': 'a, input[type="submit"], input[type="button"], input[type="text"], input[type="password"], div#refreshBalance',
                'events': ['click', 'keydown'],
                'defaultKeys': defaultKeys
            });
            Y.on('account_i13n:isValidTarget', function (event) {
                var target,
                    targetID,
                    sourceParams = {};
                // cache these to prevent lots of property lookups
                target = event.target;
                targetID = target.get('id');
                // for the user hitting enter to login, the analytics should be behave just like a login click
                // put this at top because non-enter key events will occur frequently and we want to exit early
                if (target.get('type') === 'text' || target.get('type') === 'password') {
                    if (event.type === 'keydown' && event.keyCode === 13) {                         // look for an Enter keypress
                        //account_i13n.collectParams(Y.one('#login'), sourceParams);
                        //account_i13n.makeCall(Y.one('#login'), sourceParams);
                        betfair.Events.custom.LoginObserver.notifyLoginStarted.fire(sourceParams);  // and hand the params to the login observer
                    }
                    return false;
                }
                // don't fire on hidden or disabled elements or elements tagged with dontfire
                if (target.hasClass('hide') || target.hasClass('disabled') || target.hasClass('account_i13n-dontfire')) {
                    return false;
                }
                // for login and logout click event, LoginObserver is handed the signal params for sending with success/failure signals later
                if (targetID === 'logout') {
                    account_i13n.collectParams(target, sourceParams);
                    betfair.Events.custom.LoginObserver.notifyLogoutStarted.fire(sourceParams);
                } else if (targetID === 'login') {
                    account_i13n.collectParams(target, sourceParams);
                    betfair.Events.custom.LoginObserver.notifyLoginStarted.fire(sourceParams);
                }
                return true;
            });
        });
    }
    function i13nLatestAccountEventsUnload() {
        i13nLatestAccountEvents = null;
        i13nLatestAccountEventHandler = null;
    }
</script>
	
