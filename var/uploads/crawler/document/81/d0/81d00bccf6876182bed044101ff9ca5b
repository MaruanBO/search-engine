        pathName = pathName.replace("ALL_THEMES/", "");
        pathName = pathName.replace("retention/", "");
        pathName = pathName.replace("unfunded/", "");
        pathName = pathName.replace("prospect/", "");
        pathName = pathName.replace("generic/", "");
        pathName = pathName.replace("rtd/", "");

        if ((document.location.hostname.indexOf("www.") >= 0 && pathName === "") ||
            (document.location.hostname.indexOf("games.") >= 0) ||
            (document.location.hostname.indexOf("arcade.") >= 0) ||
            (document.location.hostname.indexOf("gamesportal.") >= 0 && document.location.pathname.indexOf("backgammon") >= 0) ||
