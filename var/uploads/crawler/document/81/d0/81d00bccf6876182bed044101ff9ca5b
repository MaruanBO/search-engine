<script type="text/javascript">
    /* US33408 - update the moo and mootwo cookies on each page load
    This is done to avoid some nice IE limitation that just does not allow more then 50(20 in older versions) cookies per domain.
    Because the logic of IE is to delete old cookies, this code is a attempt to keep this one fresh...
    Always update the expiry date to 10 years starting from current time.
     */
    (function() {
        var moo = betfair.CookieManager.get("moo"),
            mootwo = betfair.CookieManager.get("mootwo");

        if (moo) {
            betfair.CookieManager.set("moo", moo, 3650, null, null, true);
        }
        if (mootwo) {
            betfair.CookieManager.set("mootwo", mootwo, 3650, null, null, true);
        }
    }());

    // Footer Clock
    betfair.FooterClock = (function() {
    var DomContentLoadedEvent = {
            DOM: [
                { element: window, eventType: "DOMContentLoaded",   handler: footerClock}
            ]
        },
        DomContentLoadedEventHandler = new betfair.Events.delegate(DomContentLoadedEvent, true),
        footerClockTag;

    /*
    * function updateClock()
    * Update the time display
    * */
    function updateClock() {
        var currentTime, currentHours, currentMinutes, currentSeconds, newDate,
            timeoutMillis = 1000, timeoutMaxInterval = 30;

        newDate = new Date().getTime();
        currentTime = new thirdparty.Date();

       //timezone for Central Europe Time from wich "Europe/Copenhagen" is part of it
        currentTime.setTimezone(betfair2olsonMapping["CET"]);
        currentTime.setTime(newDate);

        currentHours = currentTime.getHours();
        currentMinutes = currentTime.getMinutes();
        currentSeconds = currentTime.getSeconds();

        currentMinutes = ( currentMinutes < 10 ? "0" : "" ) + currentMinutes;
        currentHours = ( currentHours < 10 ? "0" : "" ) + currentHours;
        currentSeconds = ( currentSeconds < 10 ? "0" : "" ) + currentSeconds;

        // set the timeout interval to the timeoutMaxInterval value or the remaining number of seconds from now to the timeoutMaxInterval value
        timeoutMillis = timeoutMillis * (currentSeconds < timeoutMaxInterval ? timeoutMaxInterval - currentSeconds : 60 - currentSeconds);

        footerClockTag.innerHTML = currentHours + ":" + currentMinutes;

        setTimeout(updateClock, timeoutMillis);
    }

    function footerClock() {
        if (betfair.defaults.brandId === "dk-reg" || betfair.defaults.brandId === "es-reg") {
            footerClockTag = document.getElementById("footer-clock");
            if (footerClockTag) {
                betfair.cssjs("add", footerClockTag, "enabled");

                updateClock();
            }
        }
    }
})();





    // Experience Handling
    betfair.Experience = (function() {
    var DomContentLoadedEvent = {
            DOM: [
                { element: window, eventType: "DOMContentLoaded",   handler: experienceHandling}
            ]
        },
        DomContentLoadedEventHandler = new betfair.Events.delegate(DomContentLoadedEvent, true);


    function experienceHandling() {
        var sportsTabLink;
        if (betfair.defaults.experience && betfair.defaults.experience === "mantis-sbw") {
            sportsTabLink = jQuery("#sportsTab a");
            if (sportsTabLink) {
                sportsTabLink.attr("href", "http://www.betfair.com/sport");
            }
        }
    }
})();





