<script type="text/javascript">
    (function showAppStoreBadge () {
        var appStoreContainer, appStoreLink;

        if (document.location.pathname === "/sport" || document.location.pathname.indexOf("/sport/") === 0) {
            appStoreLink = document.getElementById("appStoreSbw");
        }
        if (document.location.pathname === "/exchange" || document.location.pathname.indexOf("/exchange/") === 0) {
            appStoreLink = document.getElementById("appStoreSsw");
        }
        if (appStoreLink) {
            appStoreContainer = document.getElementById("appStoreContainer");
            if (appStoreContainer) {
                appStoreContainer.style.display = "block";
                document.getElementById("globalNavigation").className += "showAppStoreBadge";
            }
            appStoreLink.style.display = "block";
        }
    })();
</script>
