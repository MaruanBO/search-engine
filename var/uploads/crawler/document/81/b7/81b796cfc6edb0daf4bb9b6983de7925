<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"> </script>
<script type="text/javascript">

    $(document).ready(function () {
        
        $('.makeMeDefault1 > .stdBtnText').on("click", function (e) {
            codeAddressTop('gasInputUnique1');
            return false;
        });

        $('.makeMeDefault2 > .stdBtnText').on("click", function (e) {
            codeAddressTop('gasInputUnique2');
            return false;
        });

        $('.makeMeDefault3 > .stdBtnText').on("click", function (e) {
            codeAddressTop('gasInputUnique3');
            return false;
        });


        $('.mydefault1').keypress(function (e) {
            if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                codeAddressTop('gasInputUnique1');
                return false;
            } else {
                return true;
            }
        });

        $('.mydefault2').keypress(function (e) {
            if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                codeAddressTop('gasInputUnique2');
                return false;
            } else {
                return true;
            }
        });

        $('.mydefault3').keypress(function (e) {
            if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                codeAddressTop('gasInputUnique3');
                return false;
            } else {
                return true;
            }
        });
    });

    //MPROCOPIO- Removed Validation so an empty string can be passed
    function codeAddressTop(gasinputbox) {
        $('.gasPriceInput').css('background', '#FFFFFF');
        var address = $('.' + gasinputbox).val();
        //if (address.length > 0) {
            var geocoder = new google.maps.Geocoder();
            geocoder.geocode({ 'address': address + " USA" }, function (results, status) {
                if (status == google.maps.GeocoderStatus.OK) {
                    var loc = results[0].geometry.location;
                    var url = "/Pages/GasPriceSearch.aspx?lat=" + loc.lat() + "&lng=" + loc.lng() + "&st=" + address;
                    if (loc.lat() == 36.207399) //then this is junk
                        url = "/Pages/GasPriceSearch.aspx?lat=0&lng=0&st=";
                    window.location.assign(url);
                    return false;
                }
                return false;
            });
        //}
        //return false;
            return true;
    }
</script>

