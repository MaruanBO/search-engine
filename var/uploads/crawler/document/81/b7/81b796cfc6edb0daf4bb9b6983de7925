    $("ul.thumb li").hover(function() {
        $(this).css({ 'z-index': '10' });
        $(this).find('img').addClass("hover").stop().animate({ marginTop: '-80px', marginLeft: '-80px', top: '60%', left: '80%', width: '92px', height: '100px' }, 300);
    }, function() {
        $(this).css({ 'z-index': '0' });
        $(this).find('img').removeClass("hover").stop().animate({ marginTop: '0', marginLeft: '0', top: '0', left: '0', width: '75px', height: '80px' }, 200);
    });
