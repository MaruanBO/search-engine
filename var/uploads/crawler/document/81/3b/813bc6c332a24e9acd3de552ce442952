					var outbrain_callback = function (json) {
						// Iterate through json.doc array
						for (var i = 0; i < json.doc.length; i++) {
							if(i < 1) {
								var doc = json.doc[i];
								var article = document.getElementById("obr_ts_1");
								var obcontent = document.getElementById("ob-content");
								var obr_width = doc.thumbnail.width;
								var obr_height = doc.thumbnail.height;
								if(obr_height > 52) {
									obr_width = (obr_width * 52) / obr_height;
									obr_height = 52;
								}
								var txtMargin = obr_width + 14;
								var sponsorTxt = "";
								if(doc.source_name != "") {
									sponsorTxt = "<a href=\"" + doc.url + "\" target=\"_blank\">" + doc.source_name + "</a>&nbsp;&nbsp;";
								}
								var articleInner = "<div class=\"pull-left rightMargin\"><a href=\"" + doc.url + "\" target=\"_blank\"><img src=\"" + doc.thumbnail.url + "\" class=\"pixelpush img-polaroid text_white\" style=\"height:auto; max-width:99%;\" width=\"" + obr_width + "\" height=\"" + obr_height + "\" /></a></div><div style=\"margin-left:" + txtMargin + "px;\"><h5 class=\"pixelpush\" style=\"display:inline;\"><a href=\"" + doc.url + "\" target=\"_blank\">" + doc.content + "</a><br /><span style=\"font-weight:normal; font-size:8pt; color:#999999;\">" + sponsorTxt + "Sponsored</span></h5></div>";
								obcontent.innerHTML = articleInner;
								article.style.display = "block";
							}
						}
					};
					
					OBR.extern.callRecs(request_data, outbrain_callback);
				</script>
				<style type="text/css">
					.ob-text { font-size: 8pt; font-weight:normal; }
					.ob-logo { background-size: 55px 20px !important; height:10px !important; width:55px !important; }
				</style>
				<article id="obr_ts_1" style="display:none;">
