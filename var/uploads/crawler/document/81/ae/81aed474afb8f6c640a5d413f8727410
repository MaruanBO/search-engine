<script type='text/javascript'>    $(document).ready(function(){   
/*
start_layerslider( $('.feature_layerslider').find('.layerslider-selector'),
                    {
                        imgPreload : false,
                        navPrevNext : true,  
                        navStartStop : false, 
    	                autoStart : true,    	                                                                      
    	                animateFirstLayer : true,
						cbStop: function() {
							console.log('stopping layerslider');
						}
                    }
);
*/

currentId = '';
currentState = 'IDLE';
direction = 'next';
slideTimer = null;

start_layerslider( $('.feature_layerslider').find('.layerslider-selector'),
                    {
                        autoStart : false,
						firstLayer : 'random',
						imgPreload : false,
						navPrevNext : true,  
                        navStartStop : false,
						animateFirstLayer : true,
						cbAnimStart: function() {
							if(currentId != ''){
								jwplayer(currentId).stop();
							}
							currentState = 'STOPPED';
							clearTimeout(slideTimer);
							slideTimer = null;
						},
						cbAnimStop: function() {
							if(slideTimer == null){
								slideTimer = setTimeout(function(){
									changeSlide();
								}, 8000);
							}
						},
						cbNext: function() {
							if(currentId != ''){
								jwplayer(currentId).stop();
							}
							currentState = 'STOPPED';
							direction = 'next';
							clearTimeout(slideTimer);
							slideTimer = null;
						},
						cbPrev: function() {
							if(currentId != ''){
								jwplayer(currentId).stop();
							}
							currentState = 'STOPPED';
							direction = 'prev';
							clearTimeout(slideTimer);
							slideTimer = null;
						}
                    }
);


$('.layerslider-selector').hover(function(){
	clearTimeout(slideTimer);
	slideTimer = null;
},function() {
	if(slideTimer == null){
		slideTimer = setTimeout(function(){
			changeSlide();
		}, 8000);
	}
});

//player setup configuration
					jwplayer('videoPlayer19').setup({"playlist":[{"sources":[{"file":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/m3u8\/5137r593s18s49n3ss963n3snq632s42playlist.m3u8"},{"file":"rtmp:\/\/s1k2r504ethydi.cloudfront.net\/cfx\/st\/mp4:videos\/video_mp4\/1000k\/5137r593s18s49n3ss963n3snq632s42.mp4"},{"file":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/video_mp4\/1000k\/5137r593s18s49n3ss963n3snq632s42.mp4"}],"image":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/video_thumb\/XL\/1-29p420nnps44o43046o9p0opn5o6q5rp.jpg"}],"width":"100%","aspectratio":"16:9","primary":"flash","abouttext":"SendtoNews","aboutlink":"http:\/\/sendtonews.com","autostart":false,"mute":false,"analytics":{"enabled":false},"ga":{}});

					// player bind events
					jwplayer('videoPlayer19').onPlay(function(){
						
						currentState = 'PLAYING';						
						currentId = 'videoPlayer19';

						stopAllJwPlayers('videoPlayer19');
						clearTimeout(slideTimer);
						slideTimer = null;

					});
					
					jwplayer('videoPlayer19').onPause(function(){ 
						
						currentState = 'PAUSED';
						if(slideTimer != null){
							clearTimeout(slideTimer);
							slideTimer = null;
							slideTimer = setTimeout(function(){ 
								changeSlide(); 
							}, 8000);
						}
					});
					jwplayer('videoPlayer19').onComplete(function(){
						
						currentState = 'COMPLETE';
						if(slideTimer != null){
							clearTimeout(slideTimer);
							slideTimer = null;
							slideTimer = setTimeout(function(){
								changeSlide();
							}, 8000);
						}
					});//player setup configuration
					jwplayer('videoPlayer7').setup({"playlist":[{"sources":[{"file":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/m3u8\/92n55osn8s8o2n6rpr0r00q1s285s9n3playlist.m3u8"},{"file":"rtmp:\/\/s1k2r504ethydi.cloudfront.net\/cfx\/st\/mp4:videos\/video_mp4\/1000k\/92n55osn8s8o2n6rpr0r00q1s285s9n3.mp4"},{"file":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/video_mp4\/1000k\/92n55osn8s8o2n6rpr0r00q1s285s9n3.mp4"}],"image":"http:\/\/d29xw9s9x32j3w.cloudfront.net\/videos\/video_thumb\/XL\/3-q8r079546opn6sq16608153372psq2s8.jpg"}],"width":"100%","aspectratio":"16:9","primary":"flash","abouttext":"SendtoNews","aboutlink":"http:\/\/sendtonews.com","autostart":false,"mute":false,"analytics":{"enabled":false},"ga":{}});

					// player bind events
					jwplayer('videoPlayer7').onPlay(function(){
						
						currentState = 'PLAYING';						
						currentId = 'videoPlayer7';

						stopAllJwPlayers('videoPlayer7');
						clearTimeout(slideTimer);
						slideTimer = null;

					});
					
					jwplayer('videoPlayer7').onPause(function(){ 
						
						currentState = 'PAUSED';
						if(slideTimer != null){
							clearTimeout(slideTimer);
							slideTimer = null;
							slideTimer = setTimeout(function(){ 
								changeSlide(); 
							}, 8000);
						}
					});
					jwplayer('videoPlayer7').onComplete(function(){
						
						currentState = 'COMPLETE';
						if(slideTimer != null){
							clearTimeout(slideTimer);
							slideTimer = null;
							slideTimer = setTimeout(function(){
								changeSlide();
							}, 8000);
						}
					});

});

function changeSlide(){
	
	if(currentState != 'PLAYING' && currentState != 'BUFFERING'){
		$('.feature_layerslider .layerslider-selector').layerSlider(direction);
	}
	
}

</script>
