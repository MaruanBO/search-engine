<script type="text/javascript">

var highlight_page= 1; 
var highlight_posts_per_page = 8;
var hightlight_total_pages = 1;
var highlight_category = 'Sports';
var channel_page = 1;
var channel_posts_per_page = 12
var channel_category = 'sport';
var channel_total_pages = 10;
var ajax_loading = '<div class="" style="text-align: center; padding: 110px 0 20px;"><img src="/assets/images/template/ajax-sendtonews.gif" alt="AJAX Loading"/></div>'

$(function() {    
/*    $('.highlight-prev').addClass('disabled');  

    
    // This loads 
    $('#section-highlight').on('click','.highlight-next, .highlight-prev', function()
    {
        if ($(this).hasClass('disabled'))
            return;
        highlight_page = highlight_page + parseInt($(this).attr('data-direction'));
        $('.highlight-next, .highlight-prev').removeClass('disabled');
        $('#highlight-load > div').animate({opacity : 0}, 500,'easeOutQuad', function(){   })
        
        // valid checks 
        if (highlight_page <= 1)
        {
            highlight_page = 1;
            $('.highlight-prev').addClass('disabled')            
        }
        
        if (highlight_page >= hightlight_total_pages )
        {
            highlight_page =  hightlight_total_pages
            $('.highlight-next').addClass('disabled')
        }
        
        var ajax_data = {   category :highlight_category , 
                            p : highlight_page,
                            n : highlight_posts_per_page,     
                            cmd : 'HIGHLIGHT_INDEX'
                        }     
        $('#highlight-load').html(ajax_loading)             ;
        $.ajax(
        {
            url : 'http://sendtonews.com/story-preview-ajax.html',
            data : ajax_data ,
            success : function(data)    
                {        
                    $('#highlight-load').html(data.HTML) 
                    hightlight_total_pages = data.TOTAL
                    
                }, 
            dataType : 'json'
            
        })
    })

//This activates the links from main highlight page and the next / previous on channel page
 $('#section-highlight').on('click','.channel-next, .channel-prev', function()
    {       
        if ( $(this).hasClass('disabled'))
            return ;
        
        var direction = parseInt($(this).attr('data-direction'))
        direction = (direction == 0 ) ? 1 : direction;
        $('#channel-load > div').animate({opacity : 0}, 500,'easeOutQuad', function(){   })
        channel_page = $(this).hasClass('get-channel-link') ? 1 : channel_page + parseInt($(this).attr('data-direction'));
        

        // set the prev and next buttons with the correct group
        if ($(this).hasClass('get-channel-link'))
        {
            $('.channel-next, .channel-prev').attr('data-group',group)
            $('#channel-page-header').html(highlight_category+ '<span class="channel-breadcrumb previous">' + $(this).find('.highlight-overlay').text() + '</span>')
        }
        
        $('.channel-prev, .channel-next').removeClass('disabled');
        // valid checks 
        if (channel_page <= 1)
        {
            channel_page = 1;
            $('.channel-prev').addClass('disabled')            
        }
        
        if (channel_page >= channel_total_pages )
        {
            channel_page =  channel_total_pages
            $('.channel-next').addClass('disabled')
        }             
        // this is a call to jquery.sendtonws.v#.js            
		
        setup_highlight({selector:'#channel-load',uri_type :'highlight',post_type : $('#section-highlight').data('beat') ,permalink : $('#section-highlight').data('focus'),wid: channel_page}, false);
