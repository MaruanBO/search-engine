    $('#section-highlight').on('click', ' .show-story',function()
    {
        var storyId = $(this).attr('data-story');
       // $('#story-page-header').html(highlight_category+ '<span class="story-breadcrumb previous">' + $(this).find('.channel-overlay').text() + '</span>')
        // this is a call to jquery.sendtonws.v#.js            
        //setup_highlight({selector: '#highlight-story-content', uri_type : 'highlight',post_type : 'story', permalink : storyId, wid :0}, false);        
    })
    
    $('#section-highlight').on('click','.clip-load', function()
    {
        $('.clip-load[data-smid]').removeClass('active');
        $(this).addClass('active');
        var smid = $(this).attr('data-smid');
        var ajax_data = {   smid : smid,     
                            cmd : 'LOAD_VIDEO',
                            width : '460',
                            height : '250'
                        }
       // $('#videoPlayer').html(ajax_loading)   
        if (smid != 'IMAGE')
        {
           $.ajax(
            {
                url : 'http://sendtonews.com/story-preview-ajax.html',
                data :ajax_data,
                success : function(data)    
                {   
                    // this sets up the smid for loggin metrics, and then sets a flag so only sets once per load
                    if ($('.story-video-container').data(smid) == undefined)
                    {
                        
						log_highlight_metrics('LOG_VIDEO_PLAY', smid)
						$('.story-video-container').data(smid, 1);
							
					}   
                    jwplayer("storyPlayer").setup(data.jwplayerConfig);
                    bindPlayerHandlers();
                }, 
                dataType : 'json'
                
            }) 
        }
        else 
        {
            $('#videoPlayer').html('<div style="text-align: center; margin: 0 auto;"><img src="' + $(this).attr('data-preview') + '" /></div>')
        }
    })

});

