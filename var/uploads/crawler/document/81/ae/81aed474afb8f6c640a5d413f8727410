<script>
    $(document).ready(function(){
        setTimeout(function(){
            $('.codeDisplay img').get(0).src = $('.codeDisplay img').get(0).src+"?" + new Date().getTime();
        },1000);
    });
    $(function() {

     
     
     $('.contact-error-text').on('click', function() {
                      $(this).siblings('.contact-input').focus().click();
         
       
     })     
     $('.contact-input').on('click', function() {
         var error_text = $(this).siblings('.contact-error-text').text();
         if ((error_text.length > 3) && ($(this).val().length == 0))
         {
             $(this).siblings('.contact-error-text').animate({opacity: .5}, 200)
         }
       
     })
     
     $('.contact-input').on('blur', function() {
         var error_text = $(this).siblings('.contact-error-text').text();
         if ((error_text.length > 3 ) && ($(this).val().length == 0))
         {
             $(this).siblings('.contact-error-text').animate({opacity:1}, 200)
         }
       
     })    
     
     $('.contact-input').on('keyup', function() {         
         var error_text = $(this).siblings('.contact-error-text').text();
         if ((error_text.length > 3 ) )
         {
             if ($(this).val().length > 0)
            {
                $(this).siblings('.contact-error-text').animate({opacity:0}, 200)
            }
             else 
            {
                console.log('go back')
                $(this).siblings('.contact-error-text').animate({opacity: 0.5}, 200)
            }
             
         }
         
       
     })       
     
     $('#contact-reset-button').click(function(){
         $('.contact-response-text').css({display: 'none'});
         $('.formWrapper.contact-form-text').css({display: 'block'})
         $('.footer-contact-form').trigger('reset');
         d = new Date();
         $('.codeDisplay img').attr('src',"http://sendtonews.com/veriwordjpg?"+d.getTime());
     })
     
    });
    
    
</script>


                </div>
            </div>
            
            
            

        </div>                           
