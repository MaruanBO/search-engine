<script>

    
        
  $(function () {
    $('#widget-home-latest a').click(function (e)
    {
        e.preventDefault();
        $(this).tab('show');   

       if ($(this).attr('href') == '#latest-announcements')
       {           
           if (!$('#latest-announcements').find('.home-latest-content').hasClass('mCustomScrollbar'))
           {           
               // this function is called in jquery.sendtonews.js
             	//gets the data for announcements
            	get_widget_latest(_PRESS_AJAX_URL,
    				'#latest-announcements .home-latest-content',
    				{chunk : 'press_announcements_homewidget_template',   post_type : 'company-announcement',	posts_per_page : 10, json : 'true',	paged : 1},false);	              
               add_scrollbar('#latest-announcements .home-latest-content')
           }
           
       }
       if ($(this).attr('href') == '#latest-events')
       {           
          	//gets the data for latest events 
           
           if (!$('#latest-events').find('.home-latest-content').hasClass('mCustomScrollbar'))
           {      
        	get_widget_latest(     _PRESS_AJAX_URL, 
        					       '#latest-events .home-latest-content',
    					           {chunk : 'press_event_template',   post_type : 'event', order:'ASC', orderby: 'meta_value',	meta_key:'wpcf-start-date',  posts_per_page : 2000, json : 'true',	paged : 1},false);
               
               // this function is called in jquery.sendtonews.js
               add_scrollbar('#latest-events .home-latest-content')
          
           }
           
       }    
            
    })
    

    

  })

