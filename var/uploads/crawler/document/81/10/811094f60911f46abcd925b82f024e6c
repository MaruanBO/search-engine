<script type="text/javascript" language="javascript">

jQuery(document).ready(function($){

    var carousel = $('#dlContent');
    carousel.show();

    if(carousel.children().length > 1){
        $('#dlNavigation').show();
    }

    //var dl_cycle = '<?=continuous?>';
    var dl_cycle = "continuous";
    var carousel_cycle_auto = true;
    if(dl_cycle == "none")
    {
        carousel_cycle_auto = false;
    }

    //init the carousel script.
    carousel.carouFredSel({
        circular : false,
        infinite : true,
        auto: carousel_cycle_auto,
        align: 'center',
        items: 1,
        width: 670,
        scroll : {
            duration: 1000,
            pauseOnHover: 'true',
            onEnd       : function(){
                //we have gone through the DL items once, send new configuration options.
                $('#dlContent').trigger('configuration',{
                    circular: true,
                    auto:false,
                    items: 1,
                    width: 670,
                    scroll : {
                        duration: 1000,
                        onEnd: null
                    }
                },true);

                //wait for 5 seconds
                var wait = setInterval(function(){
                    clearInterval(wait);//remove the interval
                    $('#dlContent').trigger('next');
                },5000);
            }
        },
        prev : {
            button  : "#leftNav"
        },
        next : {
            button  : "#rightNav"
        }
    });

});
</script>

