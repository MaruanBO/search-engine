                            var dartPushSplit='0';
                            var dartPushNumberOverrideOverlay = Math.floor(Math.random()*100+1);
                            var	dartPushNameNewTop="";
                            var dartPushNameNew2Top="";

                            if(dartPushNumberOverrideOverlay<=dartPushSplit){
                                dartPushNameNewTop="&type=national";
                                dartPushNameNew2Top="&type2=national";
