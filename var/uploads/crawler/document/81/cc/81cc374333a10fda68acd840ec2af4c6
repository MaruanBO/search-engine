	function trackAddToCart_monetate(cartData) {
		var cartDetails = [];
		for ( var i = 0; i < cartData.products.length; i++) {
			cartDetails.push({
				"productId" : cartData.products[i].sku,
				"quantity" : cartData.products[i].qty,
				"unitPrice" : cartData.products[i].price
			});
