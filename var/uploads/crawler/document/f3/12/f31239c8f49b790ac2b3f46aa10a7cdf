<script type='text/javascript'>



  if (window.location.hostname == "es.medicare.gov"){
  google.load('search', '1', { language: 'es', style: google.loader.themes.BUBBLEGUM });
  }else{
  google.load('search', '1', { language: 'en', style: google.loader.themes.BUBBLEGUM });
  }
  google.load('feeds', '1');

  function init() {
  try {
  if (window.location.hostname == "es.medicare.gov"){
  google.search.CustomSearchControl.attachAutoCompletion('004711181796222465330:f-_vzp1vfzy',document.getElementById('Search_TextBox'),'SearchImageButton');
  }else{
  google.search.CustomSearchControl.attachAutoCompletion('004711181796222465330:4efeow-xvgm',document.getElementById('Search_TextBox'),'SearchImageButton');
  }
  } catch (err) { }

  try {
  if (window.location.hostname == "es.medicare.gov"){
  google.search.CustomSearchControl.attachAutoCompletion('004711181796222465330:nvsifwkzkaw',document.getElementById('searchcovered-textbox'),'searchcovered-button');
  }else{
  google.search.CustomSearchControl.attachAutoCompletion('004711181796222465330:jj3pfbcxpx8',document.getElementById('searchcovered-textbox'),'searchcovered-button');
  }
  } catch (err) { }
  }


  function siteSearchRedirect() {
  try {
  if (window.location.hostname == "www.mymedicare.gov") {
  var siteSearchResultsPath = 'http://www.medicare.gov/site-search/search-results.html';
  }
  else if (window.location.hostname == "plancompare.medicare.gov") {
  var siteSearchResultsPath = 'http://www.medicare.gov/site-search/search-results.html';
  }
  else
  {
  var siteSearchResultsPath = '/site-search/search-results.html';
  }

  if (document.getElementById('Search_TextBox').value !== null) {
  var searchValue = encodeURIComponent(document.getElementById('Search_TextBox').value);
  window.location.href = siteSearchResultsPath + '?q=' + searchValue;
  }
  } catch (err) { }
  }


  function YMCsiteSearchRedirect() {
  try {

  var siteSearchResultsPath = '/coverage/your-medicare-coverage.html';

  if (document.getElementById('searchcovered-textbox').value !== null) {
  var searchValue = encodeURIComponent(document.getElementById('searchcovered-textbox').value);
  window.location.href = siteSearchResultsPath + '?q=' + searchValue;
  }
  } catch (err) { }
  }


  google.setOnLoadCallback(init);

  function searchTextBoxKeyUp(event) {
  try {
  var keyPressed = String.fromCharCode(event.keyCode);

  if (keyPressed == '\n' || keyPressed == '\r') {
  siteSearchRedirect();
  }
  } catch (err) { }
  }
</script>
