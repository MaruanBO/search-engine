        function getAnchorValue() {
            var url = location.href;
            var urlArray = url.split('#');

            //Delete old cookie.
            document.cookie = "anchor=" + "" + "; expires=-1" + "; path=/";

            if (urlArray[1] != null && urlArray[1] != "") {
                //Create new cookie.
                document.cookie = "anchor=" + urlArray[1] + "; path=/";
            }
        }
