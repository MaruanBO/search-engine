	var tapElement = document.getElementById('art_list_icon_img');
	var menuElement = document.getElementById('art_cat');
	if(menuElement.style.display === 'block') {
		menuElement.style.display = 'none';
		//tapElement.style.backgroundImage = 'url(/assets/resource/mbart_list_icon.png)';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_list_icon2.png';
	} else {
		menuElement.style.display = 'block';
		//tapElement.style.backgroundImage = 'url(/assets/resource/mbart_x.png)';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_x2.png';
	}
}

