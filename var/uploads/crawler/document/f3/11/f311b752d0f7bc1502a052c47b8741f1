				<script language="JavaScript1.2">
                setInterval("UpdateNums()",500);
                
                //var vDivCal = document.getElementById? document.getElementById("div_calories") : document.all.div_calories;
                //var iDivCal = 31799469041.3192;
                
                var vDivCups = document.getElementById? document.getElementById("div_cups") : document.all.div_cups;
                var iDivCups = 553530983.895972;
                
                var vDivPounds = document.getElementById? document.getElementById("div_pounds") : document.all.div_pounds;
                var iDivPounds = 25716717.7585301;
                
                var vDivMin2008 = document.getElementById? document.getElementById("div_min2008") : document.all.div_min2008;
                var iDivMin2008 = 4044851731.53656;
                
                function UpdateNums()
                {
                    //iDivCal = iDivCal + 73.5904166666667;
                    //vDivCal.innerHTML = '<h3>' + addCommas(parseInt(iDivCal)) + '</h3>';
                    iDivCups = iDivCups + 0.677013888888889;
                    vDivCups.innerHTML = '<h3>' + addCommas(parseInt(iDivCups)) + '</h3>';
                    iDivPounds = iDivPounds + 2.69849537037037E-02;
                    vDivPounds.innerHTML = '<h3>' + addCommas(parseInt(iDivPounds)) + '</h3>';
                    iDivMin2008 = iDivMin2008 + 10.07546875;
                    vDivMin2008.innerHTML = '<h3>' + addCommas(parseInt(iDivMin2008)) + '</h3>';
                }
                
                function addCommas(nStr)
                {
                    nStr += '';
                    x = nStr.split('.');
                    x1 = x[0];
                    x2 = x.length > 1 ? '.' + x[1] : '';
                    var rgx = /(\d+)(\d{3})/;
                    while (rgx.test(x1)) {
                        x1 = x1.replace(rgx, '$1' + ',' + '$2');
                    }
                    return x1 + x2;
                }
                </script>

