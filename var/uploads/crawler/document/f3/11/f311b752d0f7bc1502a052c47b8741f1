<script type="text/javascript">
<!--//

function isEnterKey(theForm1,event)
{
	if(event.keyCode)
	{
		if (event.keyCode == 13)
		{
			if (theForm1.strUserName.value == "")
			{
				alert("Please enter your User Name.");
				theForm1.strUserName.focus();
				return (false);
			}
			if (theForm1.strPassword.value == "")
			{
				alert("Please enter your Password.");
				theForm1.strPassword.focus();
				return (false);
			}
			theForm1.action = '/myspark/mysparkstart.asp';
			theForm1.submit();
			return (false);
		}
	}
	else if(event.which)
	{
		if (event.which == 13)
		{
			if (theForm1.strUserName.value == "")
			{
				alert("Please enter your User Name.");
				theForm1.strUserName.focus();
				return (false);
			}
			else if (theForm1.strPassword.value == "")
			{
				alert("Please enter your Password.");
				theForm1.strPassword.focus();
				return (false);
			}

			theForm1.action = '/myspark/mysparkstart.asp';
			theForm1.submit();
			return (false);

		}
	}
}

function ValidateUserName(strUserName)
{
	var strName; 
	strName = new String(strUserName);
	strName = strName.toLowerCase();

	var aIllegalWord = new Array("shit","fuck");
	var i;
	
	for (i = 0; i < aIllegalWord.length; i++)
	{
		if(strName.indexOf(aIllegalWord[i]) != -1)
		{
			return (true);
		}
	}

	return (false);
}

function LTrim(orgString)
{
	return orgString.replace(/^\s+/,'');
}

function RTrim(orgString)
{
	return orgString.replace(/\s+$/,'');
}

function OldEnough(BirthDay)
{
	var dateToday = new Date();
	var monthEnd = BirthDay.indexOf("/");
	var dayStart = BirthDay.indexOf("/") + 1;
	var dayEnd = BirthDay.indexOf("/",dayStart);
	var yearStart = BirthDay.indexOf("/",dayEnd) + 1;
	
	var year = BirthDay.substring(yearStart);

	if ((dateToday.getFullYear() - year) <= 18)
	{
		if ((dateToday.getFullYear() - year) < 18)
		{
			return (false);
		}
		
		var month = BirthDay.substring(0,monthEnd);
		
		if (dateToday.getMonth() + 1 <= month)
		{
			if (dateToday.getMonth() + 1 < month)
			{
				return (false);
			}
			
			var day = BirthDay.substring(dayStart,dayEnd);

			

			if (dateToday.getDate() < day)
			{
				return(false);
			}
		}
	}

	return (true);
}

function TrimString(strString)
{
	strString=strString.replace(/^\s*/,"")		// Remove leading spaces
	strString=strString.replace(/\s*$/,"")	// Remove trailing spaces
	return strString
}

function Validate(theForm)
{

	if (theForm.strEmail.value == "")
	{
		alert("Please enter a value for the \"Email address\" field.");
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf("@") == -1)
	{
		alert("Your email address must contain \"@\". Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf(" ") != -1)
	{
		alert("Your email address can not contain spaces. Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf(";") != -1)
	{
		alert("Your email address can not contain \";\". Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf(",") != -1)
	{
		alert("Your email address can not contain \",\". Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf(":") != -1)
	{
		alert("Your email address can not contain \":\". Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strEmail.value.indexOf("/") != -1)
	{
		alert("Your email address can not contain \"/\". Please enter a new value for the \"Email Address\" field.");    
		theForm.strEmail.focus();
		return (false);
	}

	if (theForm.strUserName.value.indexOf("#") != -1)
	{
		alert("Your username can not contain the # character. Please enter a new value for the UserName field.");    
		theForm.strUserName.focus();
		return (false);
	}

	if (theForm.strUserName.value == "")
	{
		alert("Please enter a value for the \"Username\" field.");
		theForm.strUserName.focus();
		return (false);
	}
	
	if (theForm.strUserName.value.length > 15)
	{
		alert("Your Username is too long. Please enter a new value for the \"Username\" field.");
		theForm.strUserName.focus();
		return (false);
	}

	if (theForm.strUserName.value.length < 6)
	{
		alert("Your Username is too short. Please enter a new value for the \"Username\" field.");
		theForm.strUserName.focus();
		return (false);
	}

	if (ValidateUserName(theForm.strUserName.value))
	{
		alert("Your Username contains profanity. Please enter a new value for the \"Username\" field.");    theForm.strUserName.focus();
		return (false);
	}

	if (LTrim(RTrim(theForm.strUserName.value)).indexOf(" ") != -1)
	{
		alert("Please no spaces in the username. Please enter a new value for the \"Username\" field.");    
		theForm.strUserName.focus();
		return (false);
	}

	if (LTrim(RTrim(theForm.strUserName.value)).indexOf("'") != -1)
	{
		alert("Please no apostrophes in the username. Please enter a new value for the \"Username\" field.");    
		theForm.strUserName.focus();
		return (false);
	}
	if (LTrim(RTrim(theForm.strUserName.value)).indexOf("?") != -1)
	{
		alert("Please no question marks in the username. Please enter a new value for the \"Username\" field.");
		theForm.strUserName.focus();
		return (false);
	}
	if (LTrim(RTrim(theForm.strUserName.value)).indexOf("&") != -1)
	{
		alert("Please no ampersands in the username. Please enter a new value for the \"Username\" field.");
		theForm.strUserName.focus();
		return (false);
	}
	if (!bOnlyValidChars(theForm.strUserName.value))
	{
		alert("Your username can only contain letters, numbers, _, and -.");
		theForm.strUserName.focus();
		return (false);
		
	}
	if (theForm.strPassword.value.indexOf("#") != -1)
	{
		alert("Your password can not contain the # character. Please enter a new value for the password field.");    
		theForm.strPassword.focus();
		return (false);
	}
	if (theForm.strPassword.value == "")
	{
		alert("Please enter a value for the \"Password\" field.");
		theForm.strPassword.focus();
		return (false);
	}

	if (theForm.strPassword.value.length > 10)
	{
		alert("Your password is too long. Please enter a new value for the \"Password\" field.");
		theForm.strPassword.focus();
		return (false);
	}

	if (theForm.strPassword.value.length < 6)
	{
		alert("Your password is too short. Please enter a new value for the \"Password\" field.");
		theForm.strPassword.focus();
		return (false);
	}

	if (theForm.strPassword.value != theForm.strPassword2.value)
	{
		alert("Your passwords do not match. Please re-enter your password.");
		theForm.strPassword.focus();
		return (false);
	}
	
	if (LTrim(RTrim(theForm.strPassword.value)).indexOf("'") != -1)
	{
		alert("Please no apostrophes in the password. Please enter a new value for the \"Password\" field.");    
		theForm.strPassword.focus();
		return (false);
	}
	if (LTrim(RTrim(theForm.strPassword.value)).indexOf("?") != -1)
	{
		alert("Please no question marks in the password. Please enter a new value for the \"Password\" field.");
		theForm.strPassword.focus();
		return (false);
	}
	if (LTrim(RTrim(theForm.strPassword.value)).indexOf("&") != -1)
	{
		alert("Please no ampersands in the password. Please enter a new value for the \"Password\" field.");
		theForm.strPassword.focus();
		return (false);
	}

	if (!bOnlyValidChars(theForm.strPassword.value))
	{
		alert("Your password can only contain letters, numbers, _, and -.");
		theForm.strPassword.focus();
		return (false);
		
	}
	
	if (theForm.selCountry.options[theForm.selCountry.selectedIndex].value == "265")
	{
		if (theForm.strZip.value == "")
		{
			alert("Please enter a value for the \"Zip Code\" field.");
			theForm.strZip.focus();
			return (false);
		}
	}
	
	if (theForm.strMonth.options[theForm.strMonth.selectedIndex].value == "0")
	{
		alert("Please enter a value for the \"Month\" field.");
		theForm.strMonth.focus();
		return (false);
	}

	if (theForm.strDay.options[theForm.strDay.selectedIndex].value == "0")
	{
		alert("Please enter a value for the \"Day\" field.");
		theForm.strDay.focus();
		return (false);
	}
	if (theForm.strYear.options[theForm.strYear.selectedIndex].value == "0")
	{
		alert("Please enter a value for the \"Year\" field.");
		theForm.strYear.focus();
		return (false);
	}
	theForm.dateBirthdate.value = theForm.strMonth.options[theForm.strMonth.selectedIndex].value + '/' + theForm.strDay.options[theForm.strDay.selectedIndex].value + '/' +  theForm.strYear.options[theForm.strYear.selectedIndex].value;

	var rbGenderValue = -1;
	for(var i=0; i < theForm.bitGender.length; i++)
	{	
		if (theForm.bitGender[i].checked)
		{
			rbGenderValue = theForm.bitGender[i].value;
		}
	}

	if(rbGenderValue == -1)
	{
		alert("Please choose your gender.");
		theForm.bitGender[0].focus();
		return (false);
	}

	//APL 04/13/2011 added code to get the current client date/time - will use this to populate the time zone offset
	cDate =  new Date();
	theForm.dateCurrentClient.value = (cDate.getMonth()+1)+"/"+cDate.getDate()+"/"+cDate.getFullYear()+" "+cDate.getHours()+":"+cDate.getMinutes()+":"+cDate.getSeconds();

	bPopup=false;

	var rbPayOptionValue = 0;

	theForm.action='/myspark/profileclean.asp';
	theForm.submit();
	return (false);
}

function bOnlyValidChars(strParam)
{
	var strValidChars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789_-';
	for(i = 0; i < strParam.length; i++)
	{
		if (strValidChars.indexOf(strParam.charAt(i)) < 0)
		{
			return false;
		}
	}
	return true;
}

