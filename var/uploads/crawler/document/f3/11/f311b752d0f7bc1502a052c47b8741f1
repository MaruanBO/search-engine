<script type="text/javascript">

function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

jQuery(function($){
	
		
		//shows callout when the page loads	
		if (readCookie("ban%5Fusernumber") == null)
		{
			$(function(){
				$('#top_callout_w')
				//delays the callout animation on page load for 2 seconds
				  .delay(2000).animate({
							//left: ['toggle', 'swing'],
							//top: ['toggle', 'swing'],
							//width: ['toggle', 'swing'],
							height: ['toggle', 'swing'],
							opacity: 'toggle'
						  }, 400, 'swing'
						  ,function() {
							  //sets a timer on completion
							  //if (hideDelayTimer) clearTimeout(hideDelayTimer);
							  //hideDelayTimer = setTimeout(function () {$('.offer_bubble').filter(":visible").hide();},7000);
				});	
				//for callout at the top
				$('.x_btn').click(function () {
					//$(this).parent().hide();
					$(this).parent().parent()
					  .animate({
							//left: ['toggle', 'swing'],
							//top: ['toggle', 'swing'],
							//width: ['toggle', 'swing'],
							height: ['toggle', 'swing'],
							opacity: 'toggle'
						  }, 200, 'swing'
						  ,function() {
					});	
				});	  
			});
		}//cookie
	
	

		//for sucess story slideshow
		$(function(){
			$('#featured_s .slide:gt(0)').hide();
			setInterval(function(){
			 $('#featured_s .slide:first-child').fadeOut('fast')
				 .next('.slide').fadeIn('slow')
				 .end('.slide').appendTo('#featured_s');}, 
			  5000);
		});
	

			
		//for Inside SparkPeople Carousel and the Main Article Carousel
		//https://github.com/kswedberg/jquery-carousel-lite
     		var carousel1Options = {
				auto: true,
				timeout:10000,
				visible: 1,
				scroll: 1,
				speed: 400,
				pause: true,
				autoWidth: true,
  				responsive: true,
				
				btnPrev: function() {
				  return $(this).find('.prev2');
				},
				btnNext: function() {
				  return $(this).find('.next2');
				},
				btnGo: $('div.content_c_nav').find('a')
      		};

		//Iniside SparkPeople
		 var carousel2Options = {
				auto: true,
				timeout:10000,
				visible: 4,
				scroll:4,
				speed: 400,
				pause: true,
				autoWidth: true,
  				responsive: true,
				btnPrev: function() {
				  return $(this).find('.prev');
				},
				btnNext: function() {
				  return $(this).find('.next');
				}
      		};
		//for Inside SparkPeople Carousel and the Main Article Carousel
		var $slideshows = $('.content_c');
		$slideshows.eq(0).jCarouselLite(carousel1Options);
		$slideshows.eq(1).jCarouselLite(carousel2Options);
		

	

});  

function listIcon() {
	var tapElement = document.getElementById('nav_list_icon_img');
	var menuElement = document.getElementById('nav');
	if(menuElement.style.display === 'block') {
		menuElement.style.display = 'none';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_list_icon2.png';
	} else {
		menuElement.style.display = 'block';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_x2.png';
	}
}

function listIconRes() {
	var tapElement = document.getElementById('nav_sublist_icon_img1');
	var menuElement = document.getElementById('sub_res');
	if(menuElement.style.display === 'block') {
		menuElement.style.display = 'none';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_list_icon2.png';
	} else {
		menuElement.style.display = 'block';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_x2.png';
	}
}
function listIconTools() {
	var tapElement = document.getElementById('nav_sublist_icon_img2');
	var menuElement = document.getElementById('sub_tools');
	if(menuElement.style.display === 'block') {
		menuElement.style.display = 'none';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_list_icon2.png';
	} else {
		menuElement.style.display = 'block';
		tapElement.src = 'http://assets3.sparkpeople.com/assets/resource/mbart_x2.png';
	}
}
