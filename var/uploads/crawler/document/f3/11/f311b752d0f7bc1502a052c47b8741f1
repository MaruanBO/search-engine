var strUserAgent = navigator.userAgent;
strUserAgent = strUserAgent.toLowerCase();
var pagename = window.location.href;

if ((strUserAgent.indexOf("iphone") > 0 || strUserAgent.indexOf("mobile") > 0 || strUserAgent.indexOf("ipod") > 0 || strUserAgent.indexOf("blackberry") > 0 || strUserAgent.indexOf("symbian") > 0 || strUserAgent.indexOf("bada") > 0 || strUserAgent.indexOf("windows phone") > 0 || strUserAgent.indexOf("smartphone") > 0) && (strUserAgent.indexOf("ipad") == -1))
{
	if (pagename.indexOf("index.asp") == -1)
	{
		window.location = "http://www.sparkpeople.com/index.asp";
	}
}

