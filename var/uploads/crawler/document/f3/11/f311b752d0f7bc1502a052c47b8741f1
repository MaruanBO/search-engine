<script type="text/javascript" language="javascript">

//if (navigator.userAgent.indexOf('iPhone') >= 0)
//{
	//if (!readCookie("iphone_shown"))
	//{
		//createCookie("iphone_shown", "true", 1000);
		//window.location.href = '/safari_splash.asp'
