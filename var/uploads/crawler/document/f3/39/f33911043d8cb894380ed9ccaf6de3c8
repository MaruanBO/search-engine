<script type="text/javascript">
$(document).ready(function() {
	$('#html5_upload_page').show();
	var $dialog = $('#signin_diag').dialog({
		modal: true,
		autoOpen: false,
		dialogClass: 'ui_diag',
		width: 430,
		closeText: ''
	});
	$dialog.find('.close').click(function(evt) {
		evt.preventDefault();
		$dialog.dialog('close');
	});
	$('div.uploadform_new .upload_btn, div.uploadform_new .htmlupload_queue').click(function(evt) {
		evt.preventDefault();
		$dialog.dialog('open');
	});
});
</script>

