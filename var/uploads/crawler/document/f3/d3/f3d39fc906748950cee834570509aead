//widget
gptAdSlots['widget'] = googletag.defineSlot('/33464461/coolhunting/homepage/widget-1x1', [1, 1], 'div-gpt-widget').addService(googletag.pubads());

      googletag.pubads().enableSingleRequest();
      googletag.pubads().disableInitialLoad();
      googletag.pubads().collapseEmptyDivs();
      googletag.enableServices();
      
    });
