        <script language="javascript">

function mobilepopupDismiss() {
    var d = new Date();
    d.setTime(d.getTime() + (365*24*60*60*1000));
    var expires = "expires="+d.toGMTString();
    document.cookie = "mobilepopupDontshow=1; " + expires;
} 

function mobilepopupDelay() {
    var d = new Date();
    d.setTime(d.getTime() + (2*24*60*60*1000));
    var expires = "expires="+d.toGMTString();
    document.cookie = "mobilepopupDontshow=1; " + expires;
} 

function mobilepopupShow() {
    var cookies = document.cookie;
    if (cookies.indexOf('mobilepopupDontshow=1') == -1)
		return true;
    else return false;
} 

var iPhone = navigator.userAgent.match(/iPhone/i) != null;
var Android = navigator.userAgent.match(/Android/i) != null
var iPad = navigator.userAgent.match(/iPad/i) != null;
var Chrome = navigator.userAgent.match(/Chrome/i) != null
var Firefox = navigator.userAgent.match(/Firefox/i) != null
var Safari = navigator.userAgent.match(/Safari/i) != null
var IEMobile = navigator.userAgent.match(/IEMobile/i) != null

if (!(IEMobile)) {

  if ((iPhone) && (Safari) && (mobilepopupShow())){
