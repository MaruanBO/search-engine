<script type="text/javascript">
	// Remove all quotes from the search term
    function removeQuotes() {
        var queryElement = document.forms["search_form"]["search_query"];
        var queryString = queryElement.value;
        queryString = queryString.replace(/"/g,"");
		queryElement.value = queryString;
	}
</script>
	
      
    
    
