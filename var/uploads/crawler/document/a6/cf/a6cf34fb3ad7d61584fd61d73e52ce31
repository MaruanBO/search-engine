<script type="text/javascript">
  var offset = new Object();
  function getElementsByClassName(className) {
    if (document.getElementsByClassName) return document.getElementsByClassName(className);
    else return document.querySelectorAll('.' + className);
  }
  function show_main_menu_item(n) {
    document.getElementById('menu_content').style.top=offset.top-1+(document.body.scrollTop || document.documentElement.scrollTop)+'px';
    /*
    document.getElementById('menu_content').style.left='195px';
    var x = getElementsByClassName('nohide');
    for (var i = 0 ; i < x.length ; i++) {
      x[i].className = 'hide';
    }
    getElementsByClassName('hide')[n].className='nohide';
    */
    $('#menu_content > div').hide();
    if ($('#major_cat'+n).length > 0) { 
      offset.has_subcat = 1;
      $('#major_cat'+n).show();
    } else offset.has_subcat = 0;
  }
  function show_menu_content() {
    if (offset.has_subcat == 1) $('#menu_content').show();
  }
</script>
