<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
<script>
                function addToCartClose() {
                  $('#mask').remove();
                  $('#addToCart').remove();
                }
                function addToCart(pid, qty) {
                  if (typeof(pid) == 'string' && pid.indexOf('product options') == 0) {
                    alert('Please choose an in stock option before adding to cart.');
                    return false;
                  } else if (!parseInt(pid)) return false;
                  
                  addToCartClose();
                  $('#shoppingCartBoxContainer').load('/add_to_cart.php?products_id=' + pid + '&qty=' + qty + ' #addToCart, #shoppingCartBox', function () {
                    if ($('#addToCartContent').find('.multiple_products_added').length) window.location.assign("http://www.airsoftgi.com/shopping_cart.php");
                    
                    $('#addToCartContent').css('margin-top', $(window).scrollTop()+'px');
                    $('body').append('<div id="mask" style="position:fixed;top:0;bottom:0;left:0;right:0;background:rgba(0,0,0,0.5);z-index:1000;"/>');
                    $('#cartRelatedProducts').find('.image_box').css('height', '120px');
                    $('#cartRelatedProducts').find('.image_box img').each(function () {
                      $(this).attr('height', parseInt($(this).attr('height')*0.87));
                      $(this).attr('width', parseInt($(this).attr('width')*0.87));
                    });
                    $('#addToCart_img').attr('height', parseInt($('#addToCart_img').attr('height')*0.8)).attr('width', parseInt($('#addToCart_img').attr('width')*0.8));
                    $('#cartRelatedProducts').find('.related_product_div').each(function (idx) {
                      if (idx > 3) $(this).remove();
                    });
                    $('#cartRelatedProducts .STORE .grid_padding').css('width', '157px');
                    $('#shoppingCartBox').hover(
                      function () {
                        $('#shoppingCartBoxContent').stop(true,true).slideDown('slow');
                      }
                      , function () {
                        $('#shoppingCartBoxContent').stop(false,true).slideUp(0);
                      }
                    );
                  });
                }
                $(function() {
                  $('#shoppingCartBox').hover(
                    function () {
                      $('#shoppingCartBoxContent').stop(true,true).slideDown('slow');
                    }
                    , function () {
                      $('#shoppingCartBoxContent').stop(false,true).slideUp(0);
                    }
                  );
                });
              </script>
