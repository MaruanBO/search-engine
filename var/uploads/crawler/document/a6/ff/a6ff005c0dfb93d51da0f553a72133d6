			<div class="footer"><script type="text/javascript">

function submitGeoCodeForm(form) {
    try {
        var searchValue = form.cityStateZip.value;
        form.geoError.value = "";
        //
        // we dont know what the coords are so go to geocoding provider to grab
        // them.
        //
        MyMapGeocode(
            searchValue,
            function(geocode) {

                if (!geocode) {
                    if (form.cityStateZip.value == "") {
                        geoError(form, "EMPTY_GEOCODE");
                    } else {
                        //
                        // what we have here... is.. a failure to communicate
                        //
                        geoError(form, "INVALID_GEOCODE");
                    }
                    return false;

                } else {
                    //
                    // submit the populated form
                    //
                    submitGeoCodeResultsForm(form, geocode.lat, geocode.lng);
                }
            }
        );
        return false;
    } catch (e) {
        if (e.name.indexOf("TypeError") > 0) {
            form.submit();
        }

    }
}


function submitGeoCodeResultsForm(form, lat, lng) {
    //
    //
    // copy the passed latitude and longitude results to the form
    // and submit it
    //
    form.latitude.value = lat;
    form.longitude.value = lng;
    form.submit();
}

  // need to re-work to display the error message from here..for now, lets pass the error to the command
	function geoError(form, errorType) {
	    form.geoError.value = errorType;
	    form.submit();

	}
</script>
