<script type="text/javascript">

    var resx = new Object();
;
    resx.appid = "charlotterusse01";
    resx.links = "";
    resx.rrec = true;
    resx.rrelem = "home_rr";
    resx.rrnum = "10;";
    var url = certonaResx.getURL();

    $.ajax({
        url: url,
        dataType: 'jsonp',
        jsonp : 'jsonp',
        success: function(response) {
            for(var i in response){
                var lookupItems =  response[i].schemes[0].items;
                var styleStr="";
                for(var j in lookupItems){
                    styleStr += (j==0?"":",") + parseInt(lookupItems[j].item_id);
                }
            }
            
            getHtml(styleStr);  
        },
        error: function() {
            // handle error here
            }
    });

    function getHtml(styleStr){
        $.ajax({
            url: "/catalog/includes/single_item_display.jsp?styleIds="+styleStr,
            dataType: 'text/html',
            success: function(response) {
                $("#home_rr").html(response);
            },
            error: function() {
                // handle error here
            }
        });
    }

</script>

