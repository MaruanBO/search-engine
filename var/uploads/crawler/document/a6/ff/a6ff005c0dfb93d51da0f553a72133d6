    <script type="text/javascript">

    $(document).ready(function(){

        /* getCookie(c_name)*/
        var emailOptInCookieSet = getCookie('emailOptIn');

        
            /* if the url is a promo url, do not show the popup */
                
        /* if cookie was not previously set */
        if(!emailOptInCookieSet){
            /* create the cookie */
            createEmailOptInCookie();
        }
        

        function createEmailOptInCookie(){
            
                    setTimeout(function(){
                        binder.hopup.load("EmailOptInHopup",{width:'250px',url:"/bff/email_opt_in.jsp"});
                    },1500);
            /* createCookie(name, value, domain, secs, path) */
            createCookie('emailOptIn', 'true','www.charlotterusse.com','2592000');
            
        }

        function createEmailOptInCookieForPromo(){
        
                    createCookie('emailOptIn', 'true','www.charlotterusse.com','2592000');
        
        }
    });
</script>
