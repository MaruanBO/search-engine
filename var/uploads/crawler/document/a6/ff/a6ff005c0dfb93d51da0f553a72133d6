<script type="text/javascript">

   var searchInstructions = "Search a Product or Keyword";
   var searchErrorText = "Uh-oh. You forgot to type in what you're looking for. Try again, please.";

      function openBasketPage()
      {
         window.location = '/checkout/basket.jsp';
      }
      $(function(){
			// Active HeaderNav if needed
			if( $("#header-nav").size() > 0 ) {
				// Setup the buttons for image
				$("#header-nav a").each(function(i) {
					var imageOff = $("img",this).attr("src");
					var imageOn = imageOff;
					if(imageOn != null && imageOn.indexOf(".") != -1) {
						imageOn = imageOn.substr(0, imageOn.lastIndexOf(".")) + "_on" + imageOn.substr(imageOn.lastIndexOf("."), imageOn.length);
					}

					$(this).hover(function(){
							$("img",this).attr('src',imageOn);
						},

						function(){
							$("img",this).attr('src',imageOff);
						}
					);
				});
			}

			$("#mainNave_shoppingCart").click(function(){
				openBasketPage();
			});

			$(".cart-search-keyword-container .search-button").click(function(){
				submitSearchForm(document.searchForm);
			});

			if ($(".keyword-text").height() < 16) {
				$(".keyword-text").height("22px");
			}

            $("#topLogin").hover(
                function(e){
                    $(this).removeClass("mouseout").addClass("mouseover");
                    $("#topLogin_child-0").css("visibility","visible");

                },function(e){
                    $(this).removeClass("mouseover").addClass("mouseout");
                    $("#topLogin_child-0").css("visibility","hidden");
                }
            );

		});

		$(document).ready(function() {
						   
			$("#keyword").autocomplete("/catalog/keyword_item.cmd",{
				minChars:3,
				width:209
			});

            if($("#keywordTop").length){
                $("#keywordTop").autocomplete("/catalog/keyword_item.cmd",{minChars:3,width:209});
            }

            if($("#keywordBottom").length){
                $("#keywordBottom").autocomplete("/catalog/keyword_item.cmd",{minChars:3,width:209});
            }

			if(document.searchForm.keyword.value == "") {
				document.searchForm.keyword.value = searchInstructions;
			}

            document.searchForm.keyword.onblur = function() {
                if (this.value == '') {
                    this.value = searchInstructions;
                }
            };
            document.searchForm.keyword.onfocus = function() {
                if(this.value == searchInstructions || this.value != '') {
                    this.value = '';
                }
            };
            if(document.subscribeHeaderForm.userEmailHeader.value == "") {
				document.bffBubble.cellOrEmail.value = "ENTER EMAIL";
			}

            document.subscribeHeaderForm.userEmailHeader.onblur = function() {
                if (this.value == '') {
                    this.value = 'ENTER EMAIL';
                }
            };
            document.subscribeHeaderForm.userEmailHeader.onfocus = function() {
                if(this.value == 'ENTER EMAIL') {
                    this.value = '';
                }
            };
            $("#nav .happyHour").click(function(){
	            s.linkTrackVars='prop8';
	            s.prop8='happy hour popup'
	            s.tl(this,'o','Happy Hour Popup');
			}); 
            
        	/* var showHideSearchButton = document.getElementById("searchShowHide");
        	var showHideSearchForm = document.getElementById("searchForm");
        	
        	document.getElementById("searchShowHide").onclick = function(){
        	   if($(showHideSearchForm).hasClass('hide')){
        		   	$(showHideSearchButton).addClass('show');
        		   	$(showHideSearchButton).removeClass('hide');
        		   	$(showHideSearchForm).addClass('show');
        			$(showHideSearchForm).removeClass('hide');
        	   }else if($(showHideSearchForm).hasClass('show')){
        			$(showHideSearchButton).addClass('hide');
        		   	$(showHideSearchButton).removeClass('show');
        		   	$(showHideSearchForm).addClass('hide');
        			$(showHideSearchForm).removeClass('show');
        	   }
        	}; */
            
        	
/* new chrome browser comes with touch enabled on desktop, this works on ipad, so leaving out the touch device support for now */
/*         	if(is_touch_device()){
    			document.addEventListener("touchend", expandCollapseHeader, false);
	    	}else{*/
