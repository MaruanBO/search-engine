 <script>
    $(".featured-image .image-link img.fitfill,\n\
        .news-slide .image-link img.fitfil,\n\
        .Section_Five_Left .image-link img.fitfil,\n\
        .more-articles-item .image-link img.fitfil").imgFitFill({mode:'fill'});
    // a little fancy hover effect per Jeff :)
    $(".news-slide, .Sec4_Box5").hover(
        function () {
            if($('.image-link .myOverlay', this).length) return;
            $('.image-link', this).css('position', 'relative')
                .append("<div class='myOverlay' style='background: black; opacity: 0; width: 100%; height: 100%; position: absolute; top: 0; left: 0;'></div>");
            $('.image-link .myOverlay', this).fadeTo(500, 0.3);
        },
        function () {
            $('.image-link .myOverlay', this).fadeOut(500, function() {
                $(this).remove();
            });
        }
    );
</script>


