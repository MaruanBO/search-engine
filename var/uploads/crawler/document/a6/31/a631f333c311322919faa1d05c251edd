<script>
     $(".search-txt").focus(function () {
        this.value='';
        $('.search-txt-wrapper').animate({
            width: "200px"
        }, 500);
        $(".search-txt").css('width','85%');
    });

     $(".search-smt").click(function () {
         if ($(".search-txt").val() != "" && $(".search-txt").val() != "Search" ) {
             $(".search-txt-wrapper").css({'border': 'none'});
             $("#frmSearch").submit();
         }else {
             $(".search-txt-wrapper").css({'border': '1px solid red'});
             return false;
         }
     });
     
    $(".search-txt").blur(function () {
        if(this.value == ''){
            this.value='Search';
            $('.search-txt-wrapper').animate({
                width: "162px"
            }, 500);
            $(".search-txt").css('width','82%');
        }
    });

</script>        <div id="inner-container">
