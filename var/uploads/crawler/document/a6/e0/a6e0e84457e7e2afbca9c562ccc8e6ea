	<script type="text/javascript">
	function hideDiv(val,dest) {
	if(val.length < 1) { return; }
		document.getElementById("optList").style.display = "none";
		document.getElementById("product").value= val;
		document.getElementById("destination").value=dest;
	}
	function showDiv(id) {
	if (document.getElementById(id).style.display != "block") {
		document.getElementById(id).style.display = "block";
		}
		else {
			document.getElementById(id).style.display = "none";
			document.getElementById("product").value= "Select one...";
		}
	}
	
	 function reDir () {
		x=document.getElementById("destination").value;
		if (x.length > 1) {
			location.href=x;
		}
	}
	</script>
	
 
