	var movieClicked = false;
	$(document).ready(function() {
		$(".moviePlayer").click(function() {
			if(currentSlide == 2){
				$(".ooyalamovie").click();
				return false;
			}
			else{
				return false;
			}
		});
		$(".moviePlayer").mouseover(function() {
			if(currentSlide == 2){
				$(".moviePlayer").css("cursor","pointer");
			}
			else{
				$(".moviePlayer").css("cursor","default");
			}
		});
	});		
