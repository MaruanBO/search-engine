			$(document).ready(function(){
			$("ul.nav")
			.superfish({
				animation : { opacity:"show", height:"show" }
			})
			.find(">li>ul>li:has(ul)")
				.addClass("isparent")
			.find(">li:has(ul)")
				.mouseover(function(){
					$("ul", this).bgIframe({opacity:false});
				})
				.find("a")
					.focus(function(){
						$("ul", $(".nav>li:has(ul)")).bgIframe({opacity:false});
					});
			});
	</script>

