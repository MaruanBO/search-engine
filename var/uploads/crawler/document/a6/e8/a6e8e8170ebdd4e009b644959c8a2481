 <script>
 $(document).ready(function(){
$("img.wp-post-image").each(function(){
$(this).removeAttr("width");
$(this).removeAttr("height");
});
});

 $(document).ready(function(){
    
    jQuery('.subscriber .login-block #mw_login .login-username input#user_login').val('Username');
       jQuery('.subscriber .login-block #mw_login .login-username input#user_login').focus(function() {

        var focusval = jQuery(this).val();

        if(focusval == 'Username' || focusval == ''){       

            jQuery(this).val(''); 

        }

    })

    jQuery('.subscriber .login-block #mw_login .login-username input#user_login').blur(function() {        

        var l = jQuery(this).val();

        if (l == '') {

            jQuery(this).val('Username');
             

        }

    })
    $(".subscriber .login-block #mw_login .login-username input#user_login").prop('required',true);
    
    
    
    jQuery('.subscriber .login-block #mw_login .login-password input#user_pass').val('Password');
       jQuery('.subscriber .login-block #mw_login .login-password input#user_pass').focus(function() {

        var focusval = jQuery(this).val();

        if(focusval == 'Password' || focusval == ''){       

            jQuery(this).val(''); 

        }

    })

    jQuery('.subscriber .login-block #mw_login .login-password input#user_pass').blur(function() {        

        var l = jQuery(this).val();

        if (l == '') {

            jQuery(this).val('Password');
             

        }

    })


    
    $(".subscriber  .login-block #mw_login").find("div").css("display","none");

 $(".subscriber  .login-block #mw_login").find("p").last().addClass("foregetpass");
    $('header .main-nav nav ul li.current-menu-parent').find('.sub-nav').parent("li").parent("ul#menu-header-menu").parent("nav").parent(".main-nav").parent("header").css( "margin-bottom", "60px" );        
 
    $('header .main-nav nav ul li.current-menu-item').find('.sub-nav').parent("li").parent("ul#menu-header-menu").parent("nav").parent(".main-nav").parent("header").css( "margin-bottom", "60px" );
    
    $('li#nav-menu-item-6').parent('ul').parent('.sub-nav').parent('li.lessons').addClass('current-menu-ancestor current-menu-parent current_page_parent current_page_ancestor');
    
    $('li#nav-menu-item-6').parent('ul').parent('.sub-nav').parent('li.quizzes').addClass('current-menu-ancestor current-menu-parent current_page_parent current_page_ancestor');
    
    $('ul#menu-course-menu li.lessons .sub-nav ul li.nav-menu-item-6').addClass('current-menu-item');
    
    $('ul#menu-course-menu li.quizzes .sub-nav ul li.nav-menu-item-6').addClass('current-menu-item');
    
    var qizheight = $("ul#menu-course-menu li.current-menu-ancestor .sub-nav").height();
    //alert(qizheight);
    
    $('header .main-nav nav ul li.current-menu-parent').find('.sub-nav').parent("li").parent("ul#menu-course-menu").parent("nav").parent(".main-nav").parent("header").css( "margin-bottom", qizheight+"px" );
    
    $("ul.sub-menu").wrap("<div class='sub-nav'>");
    
    //$('a').prop('title', ' ');

    $('header .main-nav nav ul li').find('.sub-nav').parent("li").addClass('have-sub-menu');
    
    $('header .main-nav nav.mobile-nav ul li.current-menu-parent').find('.sub-nav').parent("li").addClass('active');
    
    $("header .main-nav nav.mobile-nav ul li").find('.sub-nav').parent("li").children("a").attr("href", "javascript:void(0);");
       
    
    $('a').data("title", $('a').attr("title")).removeAttr("title");
  });
 </script>
 
