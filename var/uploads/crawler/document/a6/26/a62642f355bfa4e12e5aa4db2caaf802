        <script>
            jQuery(function () {
                adapt_to_orientation();
                placeScrollButtons();
            });
            function scroll(d) {
                var rotator = $find("rotator1");
                if (d == 'p') {
                    rotator.showNext(Telerik.Web.UI.RotatorScrollDirection.Right);
                }
                else {
                    rotator.showNext(Telerik.Web.UI.RotatorScrollDirection.Left);
                }
                return true;
            }
            var lastShownButton;
            function showItemByIndex(index) {
                var oRotator = $find("rotator1");
                oRotator.set_currentItemIndex(index);
                if (lastShownButton) {
                    lastShownButton.removeClass('indicatorSelected');
                }
                var currentButton = getButtonByIndex(index);
                currentButton.addClass('indicatorSelected');
                lastShownButton = currentButton;
            }
            function ClientItemShown(oRotator, args) {
                var currentIndex = args.get_item().get_index();
                if (lastShownButton) {
                    lastShownButton.removeClass('indicatorSelected');
                }
                var currentButton = getButtonByIndex(currentIndex);
                if (currentButton) {
                    currentButton.addClass('indicatorSelected');
                    lastShownButton = currentButton;
                }
                var wrapper = getWrapperElement(args.get_item());
                var aspHidLeftTitle = findAspControl("hidLeftTitle", wrapper);
                if (jQuery("#leftTitle")) {
                    jQuery("#leftTitle").html(aspHidLeftTitle.value);
                }
                var aspHidRightTitle = findAspControl("hidRightTitle", wrapper);
                if (jQuery("#rightTitle")) {
                    jQuery("#rightTitle").html(aspHidRightTitle.value);
                }
            }
            function getButtonByIndex(index) {
                var buttonIdSelector = String.format('#Indicator{0}:first', index);
                var currentButton = $telerik.$(buttonIdSelector)
                return currentButton;
            }
            function getWrapperElement(rotatorItem) {
                var itemElem = rotatorItem.get_element();
                var wrapper = itemElem.firstChild;
                return wrapper;
            }
            function findAspControl(id, wrapperElement) {
                var control = $get(wrapperElement.id + "_" + id, wrapperElement);
                return control;
            }
            jQuery(window).resize(function () {
                placeScrollButtons();
            });
            jQuery(window).bind('orientationchange', function (event) {
                adapt_to_orientation();
                placeScrollButtons();
            });
            function adapt_to_orientation() {
                var content_width = 1024;
                var screen_dimension;
                if (window.orientation == 0 || window.orientation == 180) {
                    screen_dimension = screen.width * 0.98;
                } else if (window.orientation == 90 || window.orientation == -90) {
                    screen_dimension = screen.height;
                }
                var viewport_scale = screen_dimension / content_width;
                jQuery('meta[name=viewport]').attr('content',
                  'width=' + content_width + ',' +
                  'minimum-scale=' + viewport_scale + ', maximum-scale=' + viewport_scale);
            }
            function placeScrollButtons() {
                if (jQuery('body')) {
                    var width = jQuery('body').width();
                    if (width > 1300) {
                        var newLeft = (width - 1300) / 2;
                        jQuery('#btn-left-feature').animate({ left: newLeft }, { duration: 0 });
                        var newRight = ((width / 2) + 650) - 69;
                        jQuery('#btn-right-feature').animate({ left: newRight }, { duration: 0 });
                    }
                    else {
                        jQuery('#btn-left-feature').animate({ left: 0 }, { duration: 0 });
                        jQuery('#btn-right-feature').animate({ left: width - 69 }, { duration: 0 });
                    }
                }
            }
        </script>
    
