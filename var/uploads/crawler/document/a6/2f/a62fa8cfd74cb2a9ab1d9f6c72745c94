  <!-- SEARCHBOX CODE // Must have jquery, bgiframe and $isLocal. -->
  <script type='text/javascript' src='http://secure.rezserver.com/js/bgiframe.js'></script>
  <script type='text/javascript'>
    var rs_refID = 5734,
      rs_currentProduct = 'hotel',
      rs_generateRooms= false;
  </script>
  <script type="text/javascript">
    (function($){
      $(document).ready(function(){
        function generateGuests($rooms, $guests){
          var min_guests = Number($rooms.val()),
            max_guests = min_guests*4,
            options = '';

          for(var i=min_guests;i<max_guests+1;i++){
            if(i===1){
              var guestText = 'Guest';
            }else{
              var guestText = 'Guests';
            }
            console.log(i===1);
            options += '<option value='+i+'>'+i+' '+guestText+'</option>';
          }
          $guests.html(options);
        }
        $('.rs_multi_rooms').on('change',function(){
          generateGuests($(this),$('.rs_multi_guests'));
        });
      });
    })(jq172)
  </script>
  <style type="text/css">
