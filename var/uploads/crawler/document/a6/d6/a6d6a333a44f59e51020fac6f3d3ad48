							function open_helpwindow(page)
							{ 
								ekmwindow=window.open(page, 'ekmhelpwindow', 'toolbar=no,menubar=no,scrollbars=yes,resizable=yes,status=no,location=no,directories=no,copyhistory=no,height=450,width=426'); 
							} 
							
							function open_helpwindow_big(page)
							{ 
								ekmwindow=window.open(page, 'ekmhelpwindow_big', 'toolbar=no,menubar=no,scrollbars=yes,resizable=yes,status=no,location=no,directories=no,copyhistory=no,height=560,width=490'); 
							}
							
							function ExpandCollapse_Swap(id) {
								if (document.getElementById(id).style.display == 'none') {
									document.getElementById(id).style.display = '';
									
									if (document.getElementById(id + '_break') != null)
									{
										document.getElementById(id + '_break').style.display = '';
									}
									
									document.getElementById(id + '_button').src='/ekmpp/images/system/option_expanded.gIf';
									document.getElementById(id + '_state').value = 'EXPANDED';
								}else{
									document.getElementById(id).style.display = 'none';
									
									if (document.getElementById(id + '_break') != null)
									{
										document.getElementById(id + '_break').style.display = 'none';
									}
									
									document.getElementById(id + '_button').src='/ekmpp/images/system/option_collapsed.gIf';
									document.getElementById(id + '_state').value = 'COLLAPSED';
								}
							}						
							
