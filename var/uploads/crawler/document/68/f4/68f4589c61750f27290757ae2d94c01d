<script type="text/javascript">
    $(window).load(function() {

        var smallBoxTitle = $('.feature-small .article-text > a.article-title, .featured-video-item .article-summary > a, .inspiration-recent-small .inspiration-recent-small-content > a');
        var smallBoxContainerWidth = $(smallBoxTitle).eq(0).parent('div').width();
        var textSample = $('#span-text-size');

        $(smallBoxTitle).each(function() {

            if ($(this).hasClass('article-title'))
                $(textSample).css('font-size', '12px');

            var endOfLine = false;
            var lineMax = 2;
            var dots = false;
            var leftOverText = $(this).text();
            var actualText = '';

            for (var l = 0; l < lineMax; l++) {

                if (endOfLine)
                    break;

                var words = leftOverText.split(' ');
                var currentText = '';
                var currentTextWidth = 0;

                for (var i = 0; i < words.length; i++) {

                    currentText = currentText + words[i] + ' ';
                    currentTextWidth = $(textSample).text(currentText).width();

                   if (currentTextWidth <= smallBoxContainerWidth) {

                        actualText = actualText + words[i] + ' ';
                        leftOverText = leftOverText.replace(words[i] + ' ', '');
                        
                        if (i == words.length - 1)
                            endOfLine = true;

                    } else {
                        if (l == lineMax - 1) {
                            dots = true;
                        }
                       
                        break;
                    }

                }

                if (l == lineMax - 1 && dots == true) {

                    var charToTest = actualText.charAt(actualText.length - 5);

                    if (charToTest == ' ') {
                        actualText = actualText.substring(0, actualText.length - 4) + '...';
                    } else {
                        actualText = actualText.substring(0, actualText.length - 5) + ' ...';
                    }
                }

            }

            $(this).text(actualText);
        });

        textSample.remove();

        // Make sure all small boxes have the height of the tallest small box
        for (var i = 0; i < 2; i++) {
            var biggestHeight = 0;
            var smallBoxes;
            if ($('div.featured-videos-row div.featured-video-item').length > 0) {
                smallBoxes = $('div.featured-videos-row div.featured-video-item');
            } else {
                smallBoxes = i == 0 ? $('div.big-column div.feature-small') : $('div.recently-left div.feature-small');
            }

            $(smallBoxes).each(function() {
                var currentHeight = $(this).height();
                if (currentHeight > biggestHeight)
                    biggestHeight = currentHeight;
            });
            $(smallBoxes).each(function() {
                $(this).height(biggestHeight);
            });
        }
    });
</script>
