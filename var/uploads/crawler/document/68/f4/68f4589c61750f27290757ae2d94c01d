        <script type="text/javascript">
            $(document).ready(function() {
                var leftContainers = $('.blogs-left-column div.div-border-bottom');
                var rightContainers = $('.blogs-right-column div.div-border-bottom');

                var index = leftContainers.length > rightContainers.length ? rightContainers.length : leftContainers.length;

                for (var i = 0; i < index; i++) {

                    if ($(leftContainers).eq(i).height() < $(rightContainers).eq(i).height()) {

                        $(leftContainers).eq(i).height($(rightContainers).eq(i).height());

                    } else {

                        $(rightContainers).eq(i).height($(leftContainers).eq(i).height());

                    }

                }
            });
        </script>
