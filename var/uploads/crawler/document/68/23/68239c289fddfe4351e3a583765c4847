  <script src="/Scripts/modernizr.custom.91323.js"></script>

  <script>
    (function($){
      var nav = $("#TopNav");
      nav.find("li").each(function() {
        if ($(this).find("ul").length > 0) {
          $(this).mouseenter(function() {
            $(this).find("ul").stop(true, true).show();
          });
          $(this).mouseleave(function() {
            $(this).find("ul").stop(true, true).hide();
          });
        }
      });
    })(jQuery);
  </script>
  <script type="text/javascript" language="Javascript">
    $('.MobileSiteLink').click(function() {
      var COOKIE_NAME = 'MobilePreference';
      var options = { path: '/', expires: 300 };
      $.cookie(COOKIE_NAME, 'yes', options);
      var CurrentURL = $(this).attr('href');
      window.location=CurrentURL;
      return false;
    });
  </script>
  <script type="text/javascript">
    $(function() { $('.ChronBoxTabs li').not('.spacer').ChronBoxTabs(); });
    $('#calButton').click(function() {
      $('#popCalendar').toggle();
      return false;
    });
    $('#calClose').click(function() {
      $('#popCalendar').hide();
      return false;
    });
  </script>
  <link type="text/css" href="/scripts/jQuery/fancybox/v134/jquery.fancybox-1.3.4.css" media="screen" rel="stylesheet" />
  <script src="/scripts/jQuery/fancybox/v134/jquery.fancybox-1.3.4.js" language="Javascript" type="text/javascript"></script>

  <script language="JavaScript">
    function formatTitle(title, currentArray, currentIndex, currentOpts) {
      var printTitle = title.replace("--|", "</b><i>");
      var printTitle = printTitle.replace("|--", "</i><b>");
      return '<div id="pic-title"><span><a href="javascript:;" onclick="$.fancybox.close();"><img src="/scripts/jQuery/fancybox/v134/fancy_close.png" /></a></span>' + (printTitle && printTitle.length ? '<b>' + printTitle + '</b>' : '' ) + '<h4>Image ' + (currentIndex + 1) + ' of ' + currentArray.length + '</h4></div>';
    }
    $(document).ready(function() {
      $('.lightbox').fancybox();
      $("a.grouped_elements").fancybox({
        'showCloseButton'  : false,
        'titlePosition'   : 'inside',
        'titleFormat'    : formatTitle
      });
    });
  </script>
