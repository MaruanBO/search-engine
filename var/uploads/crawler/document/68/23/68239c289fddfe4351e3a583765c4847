<script type="text/javascript" src="/Scripts/Highlights/Carousel.min.js"></script>
<script type="text/javascript">
/* 
Carousel function
  n = number of the new slide to show in the DOM, relative to the parent UL. 
  If s=true, the slideshow will stop.
  If s != true, the slideshow counter continues.
*/
function Carousel(n,s) { 
  // if stopped, transition speed from one slide to another is 600ms. Otherwise, it's 2 seconds.
  // if(s==true) { sp=600; } else { var sp=2000; };
  if(s==true) { sp=0; } else { var sp=0; };
  // slideshow duration before moving to the next slide.
  var d = 8500;
  // get the number of total slides by <li>. We exclude the <li> containing the controls.
  var i = $('#Chronosel li').not('.Slide-Control-Box').length;
  // Slideshow has reached the end (is greater or equal to total slides). Reset.
  if(n >= i) { n=0; };
  // clear any center controls from displaying an active state
  $('#Chronosel .Slide-Control-Jumper').css('background-position','-15px 0px');
  // set the slide's relative control box to the active state
  $('#Chronosel .Slide-Control-Jumper').eq(n).css('background-position','-15px -23px');
  // fadeout all slides, fade in slide [n].
  $('#Chronosel li').not('.Slide-Control-Box').fadeOut(sp).eq(n).fadeIn(sp);
  // Increment slideshow. We don't need a conditional here, since it's trapped 
  // in the function scope unless we pass it with setTimeout.
  n++;
  // If s=true, stop the slideshow timer. 
  // Otherwise call this function again in d seconds, passing the new value of [n].
  // Note that the timing is roughly closer to d+sp seconds due to transition time.
  if(s==true) { 
    clearTimeout(window.t); 
  } else { 
    // t exists as a global variable so we don't worry about scope. 
    // We could do this as a closure, but since it's an isolated element, 
    // the overhead is negligible. Profiling suggests an extra 
    // 10kb of memory total, for the life of the slideshow.
    window.t = setTimeout("Carousel("+n+")",d); 
  };
};
/* 
Carousel Control Box.
Note: it may help to look at /Components/Highlights/getCarousel.html 
for source on the elements referenced below 
*/
$('#Slide-Controls .Slide-Control a').click(function() { // user has clicked on a slideshow control item.
  // If there's a currently running animation, jump to the end of it. Prevents the cue from getting backed up if a user is spamming on a link.
  $('#Chronosel li').stop(false,true);
  // n contains the number of the slide that the clicked control element refers to by getting it from the element's rel attribute.
  var n = $(this).attr('rel');
  // s contains the slide object that's currently displaying, for re-use below.
  var s = $('#Chronosel li:visible').not('.Slide-Control-Box');
  // previous button was clicked.
  if(n == 'prev') {
    // get the numerical index of the previous slide.
    var sSlide = $(s).prev().index();
    var sSlide = sSlide + 1;
    // Oh, we're at the beginning. Jump to the end instead by getting the last li. 
    if(sSlide==-1) { sSlide=$('#Chronosel li').not('.Slide-Control-Box').last().index();};
  // next button clicked.
  } else if(n=='next') {
    // get the total length of all the slides.
    var i = $('#Chronosel li').not('.Slide-Control-Box').length;
    // get the numerical index of the next slide.
    var sSlide = $(s).next().index();
    var sSlide = sSlide - 1;
    // We're at the last slide, because "next" is greater or equal to the total slides. 
    // Get the first slide insetad.
    if(sSlide>=i) { 
      sSlide=$('#Chronosel li').not('.Slide-Control-Box').first().index();
    };
  };
    console.log("n = " + n + " and i = " + i + " and s = " + sSlide);
  // n is either equal to the prev/next slide, or if that doesn't exist, then we default to the value
  // n was to begin with, since it's most likely the user has clicked on a specific slide in the control box.
  n=sSlide || n;
  // Catch something from going wrong with the above.
  if(n=='next'||n=='prev') { n=0;};  
  // Send the result to our carousel function.
  // Because the user clicked on a control item, we pass true to the second
  // argument to stop the slideshow. Setting this to false would mean the slideshow
  // would continue regardless ("iTunes style")
  Carousel(parseInt(n),true);
  // Catch the click event from populating further.
  return false;
});
$('#Slide-Control-All').click(function() { // user wants to view all slides in the 'expose-style' view.
 
 
});
// Start the carousel the first time.
window.t = setTimeout("Carousel(1,false)",8500);
</script>

      
    
  </section>
