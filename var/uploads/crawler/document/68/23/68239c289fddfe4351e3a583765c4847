<script>
function toggle(source) {
  var checkboxes = $('.selectedId');
  for(var i=0, n=checkboxes.length;i<n;i++) {
    checkboxes[i].checked = source.checked;
  }
}
$(document).ready(function(){
  $('#results').hide();
  var checkboxes = $("input[type='checkbox']"),
  submitButt = $("input[type='submit']");
  checkboxes.click(function() {
    submitButt.attr("disabled", !checkboxes.is(":checked"));
  });
  $("#myform").validate({
    errorLabelContainer: "#error-note",
    errorElement: "span",
    debug: false,
    rules: {
      email: {
        required: true,
        email: true
      }
    },
    messages: {
      email: "Please use a valid-looking email address"
    },
    submitHandler: function(form) {
      // do other stuff for a valid form
      $.ajax({
        type: 'POST',
        data: $('#myform').serialize(),
        url: 'http://newsletters.austinchronicle.com/lists/?p=subscribe&id=1',
        success: function(response) {
          var pattern = /Thank you for subscribing/,
          pattern2 = /A subscriber with that address already exists/,
          verdict;
          if (pattern.test(response)) {
            verdict = "Thank you for subscribing. You should receive an email asking you to confirm your subscription."; 
          } else if (pattern2.test(response)) {
            verdict = "A subscriber with that address already exists.<br><a href='/newsletters/preferences/' style='text-decoration:underline;'>Update your preferences.</a>"; 
          } else {
            verdict = "There was a problem signing you up, please try again later.";
          }
          $('#results').html(verdict);
          $('#results').slideDown().delay(9000).slideUp();
          $('#myform')[0].reset();
        }
      });
    }
  });
});
</script>
