    <script type="text/javascript">
        function showDiv() {
            $("#LoginLinkButton").focus();
            document.getElementById("UserNameTextBox").value = "";
            document.getElementById("PasswordTextBox").value = "";
            div = document.getElementById("logindiv");

            if (div.style.visibility == "" || div.style.visibility == "hidden") {
                $("#logindiv").attr("style", "visibility:visible");
                $("#logindiv").attr("style", "border:1px solid black");
                $("#logindiv").slideDown(250, function () {
                    $("#UserNameTextBox").focus();
                    $('html').click(function () {
                        $("#logindiv").slideUp(10, function () {
                            $('html').unbind('click');
                            $("#logindiv").unbind('click');
                            $("#logindiv").attr("style", "visibility:hidden");
                            $("#LoginLinkButton").toggleClass('clicked');
                        });
                    });
                    $('#logindiv').click(function (event) {
                        event.stopPropagation();
                    });
                });

            }
            $(this).toggleClass('clicked');
        };
        $(function () {
            $(" .text_container_small_nav").stickySidebar({
                speed: 150
          , easing: "easeInOutQuad"
          , constrain: true
            });
            $(".remove").click(function (ev) {
                ev.preventDefault();
                $("#" + this.rel).stickySidebar("remove");
            });
            $("#destroyAll").click(function (ev) {
                ev.preventDefault();
                $(".stickybox").stickySidebar("destroy");
            });

        });
  
    </script>
