    <script type="text/javascript">
        $(document).ready(function () {
            $(window).load(function () {
                mCustomScrollbars();
            });
            function mCustomScrollbars() {
                $("#jl-scroll").mCustomScrollbar("vertical", 500, "easeOutCirc", 1.05, "fixed", "yes", "no", 0);
            }
            $.fx.prototype.cur = function () {
                if (this.elem[this.prop] != null && (!this.elem.style || this.elem.style[this.prop] == null)) {
                    return this.elem[this.prop];
                }
                var r = parseFloat(jQuery.css(this.elem, this.prop));
                return typeof r == 'undefined' ? 0 : r;
            }
        });

		function submitFormLocation() 
		{     
			var locationtype = document.getElementById("ddLocationType");
			var locationtypeName = locationtype.options[locationtype.selectedIndex].text;
			if (locationtypeName == "Select a Location Type") {
				alert("Please select Location Type");
				return false;
			}

			var zipcode = document.getElementById("txtZipSearch");
			if (zipcode.value == "ZIP Code" || zipcode.value == "") {
				alert("Please enter ZIP Code");
				return false;
			}

			var distance = document.getElementById("ddMiles");
			var distanceMiles = distance.options[distance.selectedIndex].value;
			window.location.href = "/mh_templates/LocationSearchResults.aspx?mhls_ResetState=yes&mhls_ZipCode=" + zipcode.value + "&mhls_locationType=" + locationtype.value + "&mhls_locationTypeName=" + locationtypeName + "&mhls_Distance=" + distanceMiles;
		}
	</script>
    
    
    
    
