  sCollection = "ironman_collection";
if (window.location.hostname == "healthplan.memorialhermann.org" || window.location.hostname == "healthplan.mh.org" || window.location.hostname == "healthplantest.mh.org") {
    sCollection = "healthplan_collection";
    if (window.location.pathname.indexOf("/brokers") == 0)
        sCollection = "healthplanbrokers_collection";
    if (window.location.pathname.indexOf("/employers") == 0)
        sCollection = "healthplanemployers_collection";
