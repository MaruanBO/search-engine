		<script type='text/javascript'>
	function getScheduleNowServiceValue(providertype) {
		switch(providertype){
		case '1':
			service = document.getElementById('ddSchedulenowService1').value;
			break;
		case '3':
			service = document.getElementById('ddSchedulenowService3').value;
			break;
		case '4':
			service = document.getElementById('ddSchedulenowService4').value;
			break;
		case '5':
			service = document.getElementById('ddSchedulenowService5').value;
			break;
			case 'all':
				service = document.getElementById('ddSchedulenowServiceAll').value;
				break;
		}
		return service;
}	

	$(document).ready(function () {
		$('#uxFindADoctorScheduleNow_schedulenowProvidertype ').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
	$('#ddSchedulenowService1').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
	$('#ddSchedulenowService3').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
	$('#ddSchedulenowService4').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
	$('#ddSchedulenowService5').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
		$('#ddSchedulenowServiceAll').keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
		$('#uxFindADoctorScheduleNow_schedulenowProvidertype').change(function () {
			$('#schedulenow-service-disabled').hide();
		$('#ddSchedulenowService1').hide();
		$('#ddSchedulenowService3').hide();
		$('#ddSchedulenowService4').hide();
		$('#ddSchedulenowService5').hide();
			$('#ddSchedulenowServiceAll').hide();
			switch($(this).attr('value'))
			{
			case '1':
				$('#ddSchedulenowService1').show();
				break;
			case '3':
				$('#ddSchedulenowService3').show();
				break;
			case '4':
				$('#ddSchedulenowService4').show();
				break;
			case '5':
				$('#ddSchedulenowService5').show();
				break;
				case 'all':
					$('#ddSchedulenowServiceAll').show();
					break;
				default:
					$('#schedulenow-service-disabled').show();
					break;
			}
			return false;
		});
		$('#uxFindADoctorScheduleNow_schedulenowProvidertype').change();
	});
function scheduleNowSubmitForm(){
	var providertype = document.getElementById("uxFindADoctorScheduleNow_schedulenowProvidertype").value;
	var service = getScheduleNowServiceValue(providertype);
	var zipcode = document.getElementById("schedulenow-zipcode").value;
	if (providertype == "Provider type"){
		alert("Please select Provider Type");
		return false;
	}
	if (service == "Select a Service" || service == "Select a service"){
		alert("Please select Service");
		return false;
	}
	if (zipcode == "ZIP code" || zipcode == ""){
		alert("Please enter ZIP Code");
		return false;
	}
	if (! /^\d{5}(-\d{4})?$/.test(zipcode)){
		alert("Enter a valid 5 digit ZIP code");
			return false;
	}
	try { ga('send', 'event', 'ScheduleNow Module', 'Submit Button'); } catch(e) { }
	var availablethisweek = "";
	if (document.getElementById("schedulenow-availablethisweek").checked)
		availablethisweek = "&search[available_in]=7";
	if (window.location.href.indexOf(".mh.org") != -1){
		if (window.location.href.indexOf("heart.mh.org") != -1){
			window.location.href = "http://heart.mh.org/schedulenow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}else if (window.location.href.indexOf("mhmg") != -1){
			window.location.href = "http://mhmg.mh.org/medical-group-schedulenow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}else{
			window.location.href = "http://wwwtest.mh.org/ScheduleNow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}
	}
	else if (window.location.href.indexOf(".memorialhermann.org") != -1){
		if (window.location.href.indexOf("heart.memorialhermann.org") != -1){
			window.location.href = "http://heart.memorialhermann.org/schedulenow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}else if (window.location.href.indexOf("mhmg") != -1){
			window.location.href = "http://mhmg.memorialhermann.org/medical-group-schedulenow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}else{
			window.location.href = "http://www.memorialhermann.org/ScheduleNow/?mhps_ProviderType=" + providertype + "&mhps_Service=" + service + "&mhps_ZipCode=" + zipcode + availablethisweek;
		}
	}
}
</script>

    </div>
</div>

<script type='text/javascript'>
					$(document).ready(function () {
