function submitsearch() {
    if (document.getElementById('uxGoogleSearch_searchform').value.length > 0) {
        var sSearchURL;
        sSearchURL = "site=" + sCollection + "&";
        sSearchURL += "output=xml_no_dtd&";
        sSearchURL += "filter=0&";
        sSearchURL += "btnG=Search&";
        sSearchURL += "getfields=description&";
        sSearchURL += "client=global_mh_frontend&";
        sSearchURL += "q=" + escape(document.getElementById('uxGoogleSearch_searchform').value);

//alert(window.location = "/search.aspx?" + sSearchURL);
		
        window.location = "/search.aspx?" + sSearchURL;		
    }
}
function searchenter(e) {
    if (window.event) {
        e = window.event;
    }
    if (e.keyCode == 13) {
        submitsearch();
        return false;
    }
}
