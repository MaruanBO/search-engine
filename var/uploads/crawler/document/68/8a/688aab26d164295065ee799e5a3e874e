<script type="text/javascript" language="javascript">
    function submitForm(){
        var zipcode = document.getElementById("uxFindADoctorScheduleNow_txtZipCode");
        var specialty = document.getElementById("uxFindADoctorScheduleNow_ddSpecialty");

        if (zipcode.value == "ZIP code" || zipcode.value == ""){
            alert("Please enter ZIP code");
            return false;
        }
	
		if (! /^\d{5}(-\d{4})?$/.test(zipcode.value)){
			alert("Enter a valid 5 digit ZIP code");
				return false;
		}

        if (specialty.options[specialty.selectedIndex].text == "Select a specialty"){
            alert("Please select a specialty");
            return false;
        }
		
		try { ga('send', 'event', 'Find a Doctor Module', 'Submit Button'); } catch(e) { } 
		
		window.location.href = "http://" + window.location.hostname + "/find-a-doctor/?mhps_Refine=yes&mhps_SearchType=Location&mhps_ZipCode=" + zipcode.value + "&mhps_specialty=" + specialty.value + "&mhps_Distance=10";
    }
	
    function findadoctorAdvanced() {
		window.location.href = "http://" + window.location.hostname + "/find-a-doctor/?mhps_SearchType=Advanced";
    }

    function schedulenowAdvanced() {
        if (window.location.href.indexOf("mhmg") != -1) 
		{
            window.location.href = "/medical-group-schedulenow/";
        }
        else 
		{
			if (window.location.href.indexOf("heart.") != -1) {
				window.location.href = "/schedulenow/";
			}
			else if (window.location.href.indexOf(".mh.org") != -1) {
				window.location.href = "http://wwwtest.mh.org/schedulenow/";
			}
			else {
				window.location.href = "http://www.memorialhermann.org/schedulenow/";
			}
        }
    }

	function findadoctorName(){
		window.location.href = "http://" + window.location.hostname + "/find-a-doctor/?mhps_SearchType=Name";
	}
	
	// trap for enter 
	$(document).ready(function (){ 
		$("#uxFindADoctorScheduleNow_ddSpecialty").keydown(function(event){ if (event.keyCode == 13) { submitForm(); } });
		$("#uxFindADoctorScheduleNow_txtZipCode").keydown(function(event){ if (event.keyCode == 13) { submitForm(); } });
		$("#schedulenow-zipcode").keydown(function(event){ if (event.keyCode == 13) { scheduleNowSubmitForm(); } });
	});	
</script>

