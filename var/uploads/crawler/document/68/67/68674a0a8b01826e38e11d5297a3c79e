<script>
require([sandals_app.js_filepath+'/app.js'], function(){
	   	$('#living-unlimited').click(function(){
		   	if($('#living-unlimited-vid').css('display') === 'none'){
				$('#living-unlimited-vid').fadeIn(function(){
					$(this).append('<iframe width="853" height="480" src="//www.youtube.com/embed/esA7bBjr8b4?rel=0&amp;showinfo=0&autoplay=1&wmode=transparent" wmode="Opaque" frameborder="0" allowfullscreen></iframe>');
				});
				$('#living-unlimited img:hover').css('cursor','initial');
			}
		});
	$('#closeModal').click(function(){
		$('#living-unlimited-vid iframe').fadeOut('fast',function(){
			$('#living-unlimited-vid').fadeOut();
			$('#living-unlimited-vid iframe').remove();
		});
		$('#living-unlimited img').removeAttr('style');
		return false;
	});
});

</script>


	
	
