       <script>

$("input").keypress(function(event) {
    if (event.which == 13) {
        event.preventDefault();
		OpenPreview();
	
    }
});


function OpenPreview()
{
	var sellerId = document.getElementById("preview").value;
	sellerId = sellerId.trim();
	if(sellerId!="" && sellerId!="Enter your eBay seller id.." )
		window.open ("http://"+sellerId+".webstoreplace.com/");
	else
		alert("Please choose an eBay seller Id");

}

function deleteAndChnageColor(obj)
{
	obj.style.color="#333";
	obj.value="";

}

$('#preview').change(function(e) {
    $('input[type=submit]').focus();
});


</script>
