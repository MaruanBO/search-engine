homepage_filter( jQuery("#tab-filter-default"), '.filter-week');

function homepage_filter( element, classname) {
	
	jQuery('.tab-sub').hide();
	jQuery(classname).show();
	jQuery(".tab-filter dd").removeClass("active");
	jQuery(element).parent().addClass("active");
	
	return false;
}
