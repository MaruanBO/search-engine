var navigated = false;

$('#drip').galleria({
    autoplay: 7000,
    data_source: data,
    position: "left",
    autoplay: true,
    idleMode: false,
    carouselSpeed: 1200,
    imagePosition: "left",
    maxscale: 1,

    extend: function(options) {

        this.play(7000); // will advance every 4th second

        // listen to when an image is shown
        this.bind('image', function(e) {

        });
        
        this.bind('loadfinish', function(e) {

            var aspect = e.imageTarget.height / e.imageTarget.width;
            e.imageTarget.style.height = '325px';
            e.imageTarget.style.width = parseInt( 325 / aspect, 10 ) + 'px';

        });

        var gallery = this;
        
        this.$('stage, image-nav-right, image-nav-left').click(function(e) {

            if ( /image-nav/.test( this.className ) ) {
                navigated = true;
                gallery.play(7000);
                $('.galleria-images .galleria-image').css( 'background-image', 'url(/images/icons/pause.gif)' );
                return;
            }

            if ( navigated == true ) {
                navigated = false;
                return;
            }

            var img = $('.galleria-images .galleria-image').css( 'background-image' );

            if ( img.indexOf( 'pause') > 0 ) {
                gallery.pause();
                $('.galleria-images .galleria-image').css( 'background-image', 'url(/images/icons/play.gif)' );
            }
            else {
                gallery.play(7000);
                gallery.next();
                $('.galleria-images .galleria-image').css( 'background-image', 'url(/images/icons/pause.gif)' );
            }
        });
        
    }
});
</script><div class="box">
