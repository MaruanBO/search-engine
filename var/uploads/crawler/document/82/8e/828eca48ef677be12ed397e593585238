		<script>
		var lineSystem={} 
		$(document).ready(function() {
			function effectiveDeviceWidth() {
		  		var deviceWidth = window.screen.width;
		  		if (navigator.userAgent.indexOf('Android') >= 0 && window.devicePixelRatio) {
		    		deviceWidth = deviceWidth / window.devicePixelRatio;
		  		}
		  		return deviceWidth;
			}

			var the_width = effectiveDeviceWidth(); 

		    if (the_width <= 376) {
		        $("body").addClass("mobile");
		   	} else {
		   		$("body").addClass("desktop").css('width', '100%');
		   		$('meta[name=viewport]').attr('content','width=1000, maximum-scale=1.0');
		   	}
		   	var isMobile = $("body").hasClass("mobile");
		   	
		   	if(isMobile) {
		   		$.getScript('http://www.designsponge.com/wp-content/themes/ds-live-13/js/site_mobile.js', function() {
		   			
		   			$( "body.mobile #search_icon").bind("tap", function(e){
		   				window.scrollTo(0,0);
						$('#search_form').fadeIn();
						$('#search_hidden').focus();
					});

					$( "body.mobile #navigation").bind("tap", function(e){
						$.sidr('open');
					});

					$( "body.mobile #nav_close").bind("tap", function(e){
						$.sidr('close');
					});

				// end getscript functions
		   		});

				$('.ad_unit.mobile').show();
				$('.ad_unit.mobile').dfp({
					dfpID:"1014960",
					enableSingleRequest: "true",
		    							setTargeting: {"PageName": ["Homepage"]},
		    		refreshExisting: "true"
				});
				
				setTimeout(function(){
					$('.social_button').fadeIn(500);
				}, 6000);

			// end isMobile
			} else {

				// ad call
		   		$('.ad_unit.desktop').dfp({
		    		dfpID:"1014960",
		         	enableSingleRequest: "true",
		    							setTargeting: {"PageName": ["Homepage"]},
		    		refreshExisting: "true"
				});

	   		} // end desktop call

	   		$('.timeago').timeago();

	   		// homepage carousel
				$('body.desktop #carousel').cycle({
					fx: 'carousel',
					slides: '.carousel_block',
				    timeout: 0,
				    carouselVisible: 3,
				    next: '#carnext',
				    prev: '#carprev'
				});

			
			// all pages
			// img load
			$("img").unveil(600);

			$('.slide_left, .slide_right').click(function(){
				window.scrollTo(0, 0);
			});
        		
			

			$('#search_icon').click(function(){
				$('#search_wrapper').addClass('open');
				$('#search_hidden').focus();
			});
			$('.desktop #search_hidden').blur(function(){
				$('#search_wrapper').removeClass('open');
			});

			$('#mobile_search_icon').click(function(){
				$('#mobile_search_form').fadeIn(300);
				$('#search_hidden').focus();
			});

			$('#search_hidden').blur(function(){
				$('#mobile_search_form').fadeOut(300);
			});

			$('#textclick').click(function(){
				$('#comment_input').fadeIn(300)
				$('#textclick').remove();
			});


			$('div.mobile_non p').each(function() {
				if($(this).find('span.insert').length > 0) {
					var viewmore = $(this).html();
					var upward = $(this).prev('p');
					upward.append(viewmore);
					$(this).remove();
				}
			});

			$('.article_promo').each(function() {
				var viewmore = '';
				var upward = '';
				var lastp = '';
				if($(this).find('span.insert').length > 0) {
					var original = $(this).children('span.insert');
					viewmore = $(this).children('span.insert')[0].outerHTML;
					lastp = $(this).children("p").last();
					if (viewmore != '') {
						upward = lastp;
					}
					if(upward != '') { 
						upward.append(viewmore);
						original.remove();
					}

				}
			});

			

			

			// random on
			function reducer() {
				$(".loop_wrap").each(function(){
					if ($('body').hasClass('mobile')) {
						var the_limit = $(this).data('mobile');
					} else {
						var the_limit = $(this).data('desktop');
					}
					
					var randomElements = $(this).children(".loopset").get().sort(function(){ 
  						return Math.round(Math.random())-0.5
					}).slice(0, the_limit);
					$(randomElements).addClass("vis_inline");
				});
			}
			reducer();

			function recent() {
				$(".loop_recent").each(function(){
					if ($('body').hasClass('mobile')) {
						console.log('its mobile!');
						var the_limit = $(this).data('mobile');
					} else {
						console.log('its desktop!');
						var the_limit = $(this).data('desktop');
					}
					var randomElements = $(this).children(".loopset").get().slice(0, the_limit);
					$(randomElements).addClass("vis_inline");
				});
			}
			recent();

			// header
			$('body.mobile #navigation').sidr({
				speed: 300,
				displace: false,
				onOpen: function(){
					$('header').removeClass('sidr_close');
					$('header').addClass('sidr_open');
					// $('html, body').addClass('no_scroll');
					// $('html.no-touch .left_column_index, aside, html.no-touch div.post_side').hcSticky('off');
				},
				onClose: function(){
					$('header').removeClass('sidr_open');
					$('header').addClass('sidr_close');
					// $('html, body').removeClass('no_scroll');
					// $('html.no-touch .left_column_index, aside, html.no-touch div.post_side').hcSticky('on');
				}
			});

			$('body.desktop #nav_close, body.desktop header, body.desktop .full').click(function() {
				$.sidr('close');
			});




			



				
			$('#comment_hider').click(function(){
				$('#comment_wrapper').addClass('comment_fit');
				$(this).fadeOut();
			});

			$(".full_size").fancybox({});

			jQuery.fn.middle = function () {
			    this.css("position","absolute");
			    this.css("margin-top", (($(window).height() - this.outerHeight()) / 4) + "px");
			    this.css("margin-left", (($(window).width() - this.outerWidth()) / 2) + "px");
			    return this;
			}

			jQuery.fn.centered = function () {
			    this.css("position","absolute");
			    this.css("margin-top", (($(window).height() - this.outerHeight()) / 2) + "px");
			    this.css("margin-left", (($(window).width() - this.outerWidth()) / 2) + "px");
			    return this;
			}

			$('.pop_up').middle();
			$('.solo_page').centered();
			$('.modal_close').click(function(){
				$(this).parent('.modal_form').fadeOut();
			});
			$('#email_button').click(function(){
				$('#email_signup_modal').fadeIn();
			});

			$("#ss-form").validate({
			  	submitHandler: function(form) {
			  		$( "#ss-form" ).fadeTo( "fast" , 0.0, function() {
						$( "#ss-form" ).css('height', 0 + 'px');
						$("#ss-form").next(".modal_confirm").fadeIn("slow", function() {
							setTimeout(function() { 
						      	$('#email_signup_modal').fadeOut(800);
						      }, 2250);
						});
						localStorage.setItem("nl_signup", true);
						});
					// $('#ss-submit-1').remove();
					form.submit(); 
			  	}
			});

		});
		</script>

