        $("#divNewsImage").click(function () {
            debugger;
            $("#aNewsVideo1").attr("class", "article-video caption right");
            var varVideoText = $("#divNewsVideo").text();
            var varIframeVideo = "<iframe id='ytplayer' type='text/html' width='400' height='195' src='" + varVideoText + "?autoplay=1&controls=1&modestbranding=1&rel=0&showinfo=0&autohide=1&color=white&theme=light' frameborder='0' allowfullscreen></iframe>";
            $("#divNewsImage").replaceWith(varIframeVideo);
            $("#divNewsImage").focus();
        });
        $("#acloseglobe").click(function () {            
            $("#divGlobalLinkDetails").addClass("visuallyhidden");
        });
    });
