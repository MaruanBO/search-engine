          function MouseClickTaleExec(CTobj) {
            var CTrightdiv = document.getElementById(CTobj).childNodes[1].id;
            if (typeof ClickTaleExec == "function") {
                if (CTrightdiv) {
                    ClickTaleExec("CTchangestyle(document.getElementById('" + CTrightdiv + "'))");
                }
            }
          }

          function CTchangestyle(CTobje) {
              if (CTobje.style.display == "block") {
                  CTobje.style.display = "none";
              }
              else {
                  CTobje.style.display = "block";
              }
          }
