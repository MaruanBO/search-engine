            recordingratio = 0.0;
          }
          else if (urlPathIs("http://www.microsoft.com/net", "http://www.microsoft.com/net/download")) {
          /* For the dotNet site, set the ratio to 5% */
            recordingratio = 0.0
          }
          else {
          /* For all other pages, set the ratio to 18%. */
            recordingratio = 0.0;
          }
          /* Now place the determined ratio in recording function in tracking code. */
          if (typeof ClickTale == 'function' && recordingratio > 0) {
            ClickTale(11972, recordingratio, "www03");
          }
