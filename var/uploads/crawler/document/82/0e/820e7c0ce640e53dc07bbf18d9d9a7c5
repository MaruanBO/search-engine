            function ExpanClickTaleExec(CTobj) {
                if (typeof ClickTaleExec == "function") {
                    if (CTobj) {
                        ClickTaleExec("CTopendesc('" + CTobj + "')");
                    }
                }
            }

            function CTopendesc(CTobje) {
                var CTclass = document.getElementById(CTobje).getAttribute('depclickevents').split("|")[1].split("]")[0];
                var CTitems = $("." + CTclass + "_row");
                for (w = 0; w < CTitems.length; w++) {
                    if (CTitems[w].getAttribute("class").search(/hidden/i) == -1) {
                        $(CTitems[w]).addClass("hidden");
                        $("#" + CTobje + " img:eq(0)").attr("src", "http://www.microsoft.com/visualstudio/_base_v1/modules/table/images/expand_plus.png");
                    }
                    else {
                        $(CTitems[w]).removeClass("hidden");
                        $("#" + CTobje + " img:eq(0)").attr("src", "http://www.microsoft.com/visualstudio/_base_v1/modules/table/images/expand_minus.png");
                    }
                }
            }
