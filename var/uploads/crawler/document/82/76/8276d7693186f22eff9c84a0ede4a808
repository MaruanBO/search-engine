		function rotateimages(){
			slideimg1 = currimg1;
			document.getElementById("slide1").setAttribute("src", slideimage1[currimg1]);			
			currimg1 = (currimg1 < slideimage1.length-1)? currimg1+1 : 1
			setTimeout("rotateimages()", 6000);
		}
		rotateimages();
		function slidelink(){
				window.location = slidehref1[slideimg1];
		}
		
		
		
	</script>
     
