    var firePixels = function(e) {
      var pixelContainer = document.getElementById("pixels"),
          pixels = document.getElementById('img-pixels');
    
      if (pixelContainer.insertAdjacentHTML) {
        pixelContainer.insertAdjacentHTML('beforeend', decodeHTML(pixels.innerHTML));
