<script type="text/javascript">
var d = new Date();
var day = d.getDate();
var month = d.getMonth() + 1;
var year = d.getFullYear();

jQuery(document).ready(function($) {
    $("span.month_edited").html(month);
    if (month.length == 1) {
        date = year + "-" + 0 + "" + month + "-" + day;
        date_SID = "ITx" + year + "" + 0 + "" + month + "" + day + "TriggeredKENWelcomeHP";
    } else {
        date = year + "-" + month + "-" + day;
        date_SID = "ITx" + year + "" + month + "" + day + "TriggeredKENWelcomeHP";
    }
    $("#opt_status_date").attr("value", date);
    $("#launch_date").attr("value", date);
    $("#sid_code").attr("value", date_SID);

    $(document).on('submit', '#email_sign_up', function(event) {
        event.preventDefault();
        if ($(this) !== 'undefined') { // checking to make sure we don't run without form contents
            if ($("#email_sign_up span.emailError").hasClass('active')) {
                $("#email_sign_up span.emailError").removeClass('active');
            }
            if ($("#email_sign_up span.thank_you").hasClass('active')) {
                $("#email_sign_up span.thank_you").removeClass('active');
            }

            if (window.location.pathname.indexOf("Signup") > 0 || window.location.pathname.indexOf("signup") > 0) {
                console.log('fire Email');
                if (emailCheck($(this)) == true) {
                    subscriber = encodeURIComponent($(this).find('#email').val());
                    $.ajax({
                        url: 'https://spc.gc.epsilon.com/prefcenter/external/updSubscriptions.aspx?regcode=677&emailaddress=' + subscriber + '&optcode=KM&optstatus=Y',
                        type: 'GET',
                        crossDomain: true,
                        dataType: 'jsonp'
                    });
                    $('#thankYou').removeClass('hide');
                    // $('#benefits').addClass('hide');
                    return false;
                }
            } else {
                if (emailCheck($(this))) {
                    subscriber = encodeURIComponent($(this).find('#email').val());
                    $.ajax({
                        url: 'https://spc.gc.epsilon.com/prefcenter/external/updSubscriptions.aspx?regcode=677&emailaddress=' + subscriber + '&optcode=KM&optstatus=Y',
                        type: 'GET',
                        crossDomain: true,
                        dataType: 'jsonp',
                    });
                    $(this).find("span.thank_you").addClass('active');
                    window.setTimeout(function() {
                        $(this).find('span.thank_you').removeClass('active');
                    }, 5000);
                    return false;
                }
            };
        }
    });
});

function emailCheck($item) {
    var emailInput = $item.find("#email").attr("value");
    if (emailInput === "" || emailInput === "email address") {
        $item.find("span.emailError").addClass('active');
        return false;
    } else {
        if (!FED.Util.isValidEmail(emailInput) || !FED.Util.isValidEmailUser(emailInput)) {
            $item.find("span.emailError").addClass('active');
            return false;
        }
    }
    return true;
}
</script>

<script type="text/javascript">
(function() {
    var $focuses = [];
    $(document).on("ready", function() {
            $focuses[0] = $("#gnf_header").find("a, input").not("li>p>a, .gnf_nav_depth2_list a");
        })
        .on("keydown", "#gnf_header, #gnf_header li.kenh_hover", function(event) {
            var depth = +this.className.match(/\d/);
            if (event.which == 9) {
                $("#gnf_header").addClass("kenh_keyboard");
                var index = $focuses[depth].index(document.activeElement) - event.shiftKey * 2 + 1;
                console.log(depth);
                if (depth)
                    event.preventDefault();
                if ($focuses[depth][index]) {
                    event[depth ? "stopPropagation" : "preventDefault"]();
                    $focuses[depth].eq(index).focus();
                } else if (depth) {
                    $focuses[depth].first().focus();
                    $(this).parent().add(this).removeClass("kenh_hover");
                }
            } else if (event.which == 27 && depth) {
                event.preventDefault();
                event.stopPropagation();
                $focuses[depth].first().focus();
                $(this).parent().add(this).removeClass("kenh_hover");
            }
        })
        .on("keydown", "#gnf_header :not(.kenh_hover)>span>.gnf_tree_junction", function(event) {
            if (event.which == 13) {
                event.preventDefault();
                $(this).closest("li").addClass("kenh_hover").parent().addClass("kenh_hover");
                var depth = +$(this).closest("li").attr("class").match(/\d/);
                $focuses[depth] = $(this).closest("li").find(".gnf_nav_depth" + (depth + 1) + "_item>span>a").add(this);
            }
        })
        .on("mouseenter", "#gnf_header a, #gnf_header input", function() {
            $("#gnf_header").removeClass("kenh_keyboard");
            $("#gnf_header .kenh_hover").removeClass("kenh_hover");
        });
}(jQuery));
</script>



