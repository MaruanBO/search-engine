<script language="Javascript" type="text/javascript">


		$(document).ready(function (){
				
				
				
				trpPersonalizationUtil.getGeoLocationV2();
				trpPersonalizationUtil.personalize();
				
			
				
				
				
				
				//REMOVE FOR PROD
				if(document.domain == 'corporatequal.troweprice.com'){
					$('li[data-attr="iws"] a').attr('href','https://www4qual.troweprice.com/iws/wps/screens/html/welcome.jsp?pdurl=/iws/wps/myportal/');
					$('li[data-attr="icw"] a').attr('href','https://www4qual.troweprice.com/icw/consultant/site/Start?placementGUID=CorpHome');
				}else if (document.domain == 'corporatedev.troweprice.com'){
					$('li[data-attr="iws"] a').attr('href','https://www4dev.troweprice.com/iws/wps/screens/html/welcome.jsp?pdurl=/iws/wps/myportal/');
					$('li[data-attr="icw"] a').attr('href','https://www4dev.troweprice.com/icw/consultant/site/Start?placementGUID=CorpHome');
				}
				
				
				$('.countries ul li a').click(function(){
					if (document.location.href.indexOf('troweprice.com') === -1){ 
						var cookieDomain = '.troweprice.net';
						}
						else{
							
							var cookieDomain = '.troweprice.com';
						}
					$.cookie('trp-country', $(this).attr('data-attr'),{domain: cookieDomain, path: '/', expires: 365});
					$.removeCookie('trp-remember', { domain: cookieDomain, path: '/' });
   					$.removeCookie('trp-remember');
				});









				$( "#sm-fnt" ).click(function() {
					$('.trp-box-white').css("font-size", "0.9em");
					$(this).attr('src','/ccw/images/font_smallAplainhover.gif');
					$('#md-fnt').attr('src','/ccw/images/font_middiumAplain.gif');
					$('#lg-fnt').attr('src','/ccw/images/font_biggerAplain.gif');
				});
				$( "#md-fnt" ).click(function() {
				  $('.trp-box-white').css("font-size", "1.0em");
				  $(this).attr('src','/ccw/images/font_middiumAplainhover.gif');
				  $('#sm-fnt').attr('src','/ccw/images/font_smallAplain.gif');
				  $('#lg-fnt').attr('src','/ccw/images/font_biggerAplain.gif');
				});
				$( "#lg-fnt" ).click(function() {
				  $('.trp-box-white').css("font-size", "1.2em");
				  $(this).attr('src','/ccw/images/font_biggerAplainhover.gif');
				  $('#sm-fnt').attr('src','/ccw/images/font_smallAplain.gif');
				  $('#md-fnt').attr('src','/ccw/images/font_middiumAplain.gif');
				}); 
				
				
				//CLEAR-COOKIES
				$('.js-clear-cookies').click(function(event){
					if (document.location.href.indexOf('troweprice.com') === -1){ 
						var cookieDomain = '.troweprice.net';
						}
						else{
							
							var cookieDomain = '.troweprice.com';
						}
					event.preventDefault();
    		  		var cookies = $.cookie();
      				for(var cookie in cookies) {
   						$.removeCookie(cookie, { domain: cookieDomain, path: '/' });
   						$.removeCookie(cookie, { path: '/' });
   						$.removeCookie(cookie);
					}
					alert('Your cookies have been cleared.');
					location.reload();
			});
				
				
		});
		
		
			var imageTagList = new Array();
			var vsLinkList = new Array();
			var wtaPageTitle = " T. Rowe Price - Corporate Home  ";
			
			function vsLinkObject(element, linktext, linkplmt)
			{
				this.element = element;
				this.linktext = linktext;
				this.linkplmt = linkplmt;
			}
			
			function imageTagObject(id, path)
			{
				this.id = id;
				this.path = path;
			}
			
			function addVSLinkPostLoad(element, linktext, linkplmt)
			{
				vsLinkList[vsLinkList.length] = new vsLinkObject(element, linktext, linkplmt);
			}
			
			function addImageToPostLoad(id, path)
			{
				imageTagList[imageTagList.length] = new imageTagObject(id, path);
			}
			
			function handleOnLoadEvent()
			
			{
				var image1 = new Image();
				try
				{	
					for (counter in imageTagList)
					{
						image1.src = imageTagList[counter].path;
						document.getElementById(imageTagList[counter].id).src = image1.src;
					}
				
					if(grayLayer)
					{
						grayLayer.className = "midContainer midContainerOn" + rNumber;   
						//imgDisciplined.src = document.getElementById('companyImage1').src;
						//imgCareer.src = document.getElementById('careerImage1').src;
					}
				
				}
				catch (err) {}
				
				try
				{
					sfHover("institution");   
					sfHover("finacial");  
					sfHover("secnav");  
				
					sfHover("tabmenu");
					sizeFix();
				}
				catch (err) {}
    
				try
				{
					var m = document.uniqueID /*IE*/
						&& document.compatMode  /*>=IE6*/
						&& !window.XMLHttpRequest /*<=IE6*/
						&& document.execCommand ;		
					
					if (!!m)			
						m('BackgroundImageCache', false, true);
				}
				catch (err) {}
			
				if (typeof scrollToCoordinates != "undefined")
				{
					scrollToCoordinates();
				}
				
				try
				{
					AttachScript("http://metrics.troweprice.com/scripts/wtatags_i.js", "ISO-8859-1");
					AttachScript("/ccw/js/oo_engine.js", "windows-1252");
				}
				catch (err) {}

				window.setTimeout(initMetrics, 1000);
				window.setTimeout(sendVSLinks, 350);
				
			}

			function initMetrics()
			{
				if (typeof WTA === 'undefined')
				{
					window.setTimeout(initMetrics, 1000);
					return;
				}
				
				
				
					WTA.utils.tags.initializesite("CORP");
				

				WTA.utils.tags.addPageData("pageTitle=" + wtaPageTitle);
				WTA.utils.tags.flush2();
			}
					
			function sendVSLinks()
			{				
				try
				{
					for (counter in vsLinkList)
					{
						checkLinkVSNewWindow(vsLinkList[counter].element, vsLinkList[counter].linktext, vsLinkList[counter].linkplmt);
					}
				}
				catch (err) {}
			}
				
			function AttachScript(src, charset)
			{
				var headID = document.getElementsByTagName("head")[0];
				var newScript = document.createElement('script');
				newScript.type = 'text/javascript';
				newScript.src = src;
				newScript.charset = charset;
				headID.appendChild(newScript);
	        }
			
			function checkLinkVS(element, linktext, linkplmt)
			{
				
				
					
					if (element.href.indexOf('careers') != -1 || element.href.indexOf('ccw') == -1)
					{
						WTA.utils.tags.sendexiturltag2(element, 'Home', linktext, linkplmt, "CORP");
						return false;
					}
					
				
				return true;
			}
			
			function checkLinkVSNewWindow(element, linktext, linkplmt)
			{
				
				
				
					if (element.href.indexOf('careers') != -1 || element.href.indexOf('ccw') == -1)
					{
						WTA.utils.tags.sendcorpexiturl(element, 'Home', linktext, linkplmt);
					}
				
			}

			
			function trackExitNewWindow(element, linktext, linkplmt)
			{
				
				
					
					WTA.utils.tags.sendcorpexiturl(element, 'Home', linktext, linkplmt);
				

			}

			
			function trackJSClick(element, linktext, linkplmt)//  linkplmt new parameter added.
			{
				
				
					
					WTA.utils.tags.sendmclink(element, 'Home', linktext, "CORP");
				
			}
			
			function checkLinkVSjs(element, linktext, linkplmt)
			{
				
				
					
					if (element.href.indexOf('careers') != -1 || element.href.indexOf('ccw') == -1)
					{
						WTA.utils.tags.sendcorpexitjs(element, 'Home', linktext, linkplmt);
					}
				

			}
	
	<!--		 -->
			function scrollToCoordinates()
			{
				var x = "0";
				var y = "0";
			
				window.scrollTo(x, y);
			}
		<!-- 	  -->
		
			function log(aForm)
			{
				
				// Cleanup hidden added fields
				try{aForm.logAsCommand.value = "";} catch(err){}
				try{aForm.wptTab.value = "";} catch(err){}
				try{aForm.wtpDocFilename.value = "";} catch(err){}
				try{aForm.wptDocTitle.value = "";} catch(err){}
				try{aForm.logAsCommand.value = "";} catch(err){}
				try{aForm.wtpUrlTrackingCode.value = "";} catch(err){}
				try{aForm.wtpUrl.value = "";} catch(err){}
				try{aForm.wptData.value = "";} catch(err){}
				
			}
			
			function logCommandClick(form, logAsCommand, tab, data)
			{
				addField(form, "hidden", "logAsCommand", logAsCommand);
				if (typeof tab != 'undefined')
				{
					addField(form, "hidden", "wptTab", tab);
				}
				if (typeof data != 'undefined')
				{
					addField(form, "hidden", "wptData", data);
				}
				log(form);
				return true;
			}
			
			function logDocumentClick(form, title, filename, url)
			{
				addField(form, "hidden", "wtpDocFilename", filename);
				addField(form, "hidden", "wtpDocTitle", title);
				addField(form, "hidden", "logAsCommand", "documentClick");
				log(form);
				return true;
			}
			
			function logUrlClick(form, trackingCode, url)
			{
				addField(form, "hidden", "wtpUrlTrackingCode", trackingCode);
				if (typeof url != 'undefined')
				{
					addField(form, "hidden", "wtpUrl", url);
				}
				addField(form, "hidden", "logAsCommand", "urlClick");
				log(form);
				
				return true;
			}
			
			function getJunctionPath()
			{
				var hostname = window.location.hostname;
				var junction = "";
				
				if (hostname == "pddev"
						|| hostname == "pddev.troweprice.com" 
						|| hostname == "ssodev.troweprice.com" 
						|| hostname == "205.128.228.121" 
						|| hostname == "pdqual" 
						|| hostname == "pdqual.troweprice.com" 
						|| hostname == "ssoqual.troweprice.com" 
						|| hostname == "home2" 
						|| hostname == "home2.troweprice.com" 
						|| hostname == "www2.troweprice.com" 
						|| hostname == "www3.troweprice.com")
				{
					var path = window.location.pathname;
					
					// The path will be /<junction>/<dir or file> so skip the first
					// slash
					var indexOfSecondSlash = path.indexOf("/", 1);
					var junction = path.substring(0, indexOfSecondSlash);
				}	
			
				return junction;
			
			}


			
			function addField (form, fieldType, fieldName, fieldValue) 
			{
			  if (typeof form[fieldName] != 'undefined')
			  {
			  	form[fieldName].value = fieldValue;
			  	return;
			  }
			  
			  if (document.getElementById) 
			  {
			    var input = document.createElement('INPUT');
			      if (document.all) 
			      { // what follows should work 
			        // with NN6 but doesn't in M14
			        input.type = fieldType;
			        input.name = fieldName;
			        input.value = fieldValue;
			      }
			      else if (document.getElementById) 
			      { // so here is the
			        // NN6 workaround
			        input.setAttribute('type', fieldType);
			        input.setAttribute('name', fieldName);
			        input.setAttribute('value', fieldValue);
			      }
			    form.appendChild(input);
			    form[fieldName] = input;
			  }
			}
		</script>
		
		
		
