<script type="text/javascript">
/* Swap out SEARCH when focused */
var searchBox = $('sli_search_1');
searchBox.observe("focus", function(){
searchBox.addClassName("active");
searchBox.setAttribute("value", "");
});
searchBox.observe("blur", function(){
if(searchBox.value == ""){
searchBox.removeClassName("active");
searchBox.setAttribute("value", "search");
}
});
$('headerSearchForm').onsubmit = function(){
if(searchBox.value == "Search") { return false; }
}
</script>
