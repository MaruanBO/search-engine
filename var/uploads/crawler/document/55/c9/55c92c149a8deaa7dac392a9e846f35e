<script>					
	$(document).ready(function() { 
	    	// bind 'signupForm' and don't submit when "enter" is hit 
		$("#signupForm").bind("keypress", function (e) {
		    if (e.keyCode == 13) {
		        return false;
		    }
		}); 			

	   $("#email").on("focus", function(){
	      var $this = $(this);
	      if($this.val() == "Enter Email Address"){
	          $this.val("");
  				  }
	   }).on("blur", function(){
   				  var $this = $(this);
   				  if($this.val() == ""){
       				$this.val("Enter Email Address");
  				  }
			   });

    	// bind 'submitBtn' and wait until submit is complete 
		$("#submitBtn").click(function(e) {
			e.preventDefault();
				var err = false;
			var filter=/^([\w-+]+(?:\.[\w-+]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
			if (signupForm.email.value == "") {
				$(".error").text("Please enter a valid email address").fadeIn();
				err = true;
			}				
			if (!filter.test(signupForm.email.value)) {
				$(".error").text("Please enter a valid email address").fadeIn();
				err = true;
			}
			if (err)
			{
				var $this = $('#footer .c-wrap');
    			var parentheight = $this.innerHeight();
				$('.footerMH').css({'height':parentheight});
				return false; 						
			}
			$(".error").fadeOut();
    		$("#signupForm").fadeOut();
			$('#signupForm').ajaxSubmit();
    		$(".results").text("We will now process your request. Thank You!").fadeIn(2000);						
		});
	}); 
</script>
						
						
