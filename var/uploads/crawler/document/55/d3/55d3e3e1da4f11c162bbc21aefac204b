  <script type="text/javascript">
    $(document).ready(function(){
      var input = $(".marketplace-search input[id=m]");
      var default_text = "Find local businesses...";
      input.focus(function () {
        if ($(this).val() == default_text) {
          $(this).val('');
        }
      });
      input.blur(function () {
        if (!input.val()) {
          $(this).attr("value", default_text);
        }
      });
    });
  </script>
  

  
  
