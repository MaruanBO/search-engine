  <script type="text/javascript" src="http://worldonline.media.clients.ellingtoncms.com/static/ellington_defaults/2.3.0/javascript/lib/cookies.js"></script>
  <script type="text/javascript" src="http://worldonline.media.clients.ellingtoncms.com/static/ellington_defaults/2.3.0/javascript/lib/mobile_detect.js"></script>
  <script type="text/javascript">
    var cookie_value = Ellington.Cookies.get_cookie('template_preference');
    if (cookie_value === null) {
      if (Ellington.TabletDetect()) {
        Ellington.Cookies.set_cookie('template_preference', 'tablet');
        window.location.reload(true);
      }

      if (Ellington.MobileDetect()) {
        Ellington.Cookies.set_cookie('template_preference', 'mobile');
        window.location.reload(true);
      }
    } else if (cookie_value == 'mobile' || cookie_value == 'tablet') {
      if (window.location.host == "www2.ljworld.com") {
        window.location.reload(true);
      }
    }
  </script>
  

