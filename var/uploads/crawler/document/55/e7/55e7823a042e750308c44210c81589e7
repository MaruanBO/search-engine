<script type="text/javascript"> 

function openDir( form ) { 

	var newIndex = form.quicknav.selectedIndex; 

	if ( newIndex == 0 ) { 

		alert( "Please select a location!" ); 

	} else { 

		cururl = form.quicknav.options[ newIndex ].value; 

		window.location.assign( cururl ); 

	} 

} 

</script> 
