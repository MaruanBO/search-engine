<script type="text/javascript">

$(document).ready(function(){
    /* Author: Chris */
    {
        /* Specify these values to affect the slider's behavior */
        // The amount of time to pause between slides
        // Specify milliseconds, i.e. 1000 = 1 second
        var frequency = 5000;
        // The speed of the fade transition
        // Specify milliseconds, i.e. 1000 = 1 second
        // "slow" == 0.6s, "normal" = 0.4s, "fast" = 0.2s
        var speed = .4;
        // The container/wrapper for the slider
        // Specify a css-valid selector, i.e. "#slider" for <div id="slider"></div>
        var container = "#fluidslider";
    }

    var slides = $(container).find("img");
    var total = slides.length;
    var current_index = 0;
    var current_banner = $(slides[current_index]);
    var current_color = current_banner.data("color");
    var allow_change = true;

    $(container).css("background-color", current_color);
    $.each(slides, function(index){
        var number_element = '<div class="number">' + (index + 1) + '</div>'
        $(container + " .numbers").append(number_element);
        if (index == 0) {
            $(this).show();
            //$(this).css("display","block");
        }
    });


    var number = $(container).find(".number")
    if (total > 1) {
        var play;
        startSlideshow();
        $(container).hover(function(){
            stopSlideshow();
            $(container + " .button").show();
            $(container + " .numbers").show();
        }, function(){
            $(container + " .button").hide();
            $(container + " .numbers").hide();
            startSlideshow();
        });
    }

    $(container + " .button.prev").on("click", function(){
        if (current_index > 0)
            var to_index = current_index - 1;
        else
            var to_index = total-1;
        changeSlide(to_index);              
    });
    $(container + " .button.next").on("click", function(){
        if (current_index < (total - 1))
            var to_index = current_index + 1;
        else
            var to_index = 0;
        changeSlide(to_index);
    });
    $(container + " .numbers .number").on("click", function(){
      $(".number").removeClass("active");
        var to_index = Number($(this).text()) - 1;
        if (current_index == to_index)
            return;
        if(!$(this).hasClass("active")){
            $(this).addClass("active");
        }
        changeSlide(to_index);
    });

    function startSlideshow() {
        play = setInterval(slideshow, frequency);
    }
    function stopSlideshow() {
        clearInterval(play);
    }
    function slideshow() {
        if (current_index < total-1)
            var next_index = current_index + 1;
        else
            var next_index = 0;
        var next_banner = $(slides[next_index]);
        var next_color = next_banner.data("color");

        current_banner.fadeToggle(speed, function(){
            $(container).animate({
                backgroundColor : next_color
            }, 600, function(){
                next_banner.fadeToggle(speed);
            });
        });

        //Update current

        current_index = next_index;
        current_banner = next_banner;
        $(".number").removeClass("active");
        $(number[current_index]).addClass("active");
    }
    function changeSlide(to_index) {

        //Prevent rapid triggering
        if (!allow_change)
            return;
        allow_change = false;
        current_banner.fadeToggle(speed, function(){
            $(container).animate({
                backgroundColor : $(slides[to_index]).data("color")
            }, 600, function(){
                $(slides[to_index]).fadeToggle(speed);
                allow_change = true;
            });
        });

        current_index = to_index;
        current_banner = $(slides[current_index]);
        $(".number").removeClass("active");
        $(number[current_index]).addClass("active");
    }
});
