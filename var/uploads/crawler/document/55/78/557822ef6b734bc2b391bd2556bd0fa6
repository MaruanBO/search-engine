		function rotate(DBOXidx,IMAXidx,AVXidx) {
			if(DBOXidx>=DBOXpictureArchive.length) {
				DBOXidx=0;
				DBOXjdx=0;
				DBOXkdx=0;
			}
			if(IMAXidx>=IMAXpictureArchive.length) {
				IMAXidx=0;
				IMAXjdx=0;
				IMAXkdx=0;
			}
			if(AVXidx>=AVXpictureArchive.length) {
				AVXidx=0;
				AVXjdx=0;
				AVXkdx=0;
			}
			//alert(DBOXpictureArchive[DBOXidx++]);
			if(DBOXidx < DBOXpictureArchive.length) {
				//alert(DBOXidx);
				document.getElementById('dboxlist').src=DBOXpictureArchive[DBOXidx++];
				document.getElementById('dboxhref').setAttribute("href", DBOXhrefArchive[DBOXjdx++]); 
				document.getElementById('dboxinner').innerHTML = DBOXinnerArchive[DBOXkdx++];
			}
			if(IMAXidx < IMAXpictureArchive.length) {
				//alert(IMAXidx);
				document.getElementById('imaxlist').src=IMAXpictureArchive[IMAXidx++];
				document.getElementById('imaxhref').setAttribute("href", IMAXhrefArchive[IMAXjdx++]); 
				document.getElementById('imaxinner').innerHTML = IMAXinnerArchive[IMAXkdx++];
			}
			if(AVXidx < AVXpictureArchive.length) {
				//alert(IMAXidx);
				document.getElementById('avxlist').src=AVXpictureArchive[AVXidx++];
				document.getElementById('avxhref').setAttribute("href", AVXhrefArchive[AVXjdx++]); 
				document.getElementById('avxinner').innerHTML = AVXinnerArchive[AVXkdx++];
			}
			timerID=setTimeout('rotate('+DBOXidx+','+IMAXidx+','+AVXidx+')', 4000);
			
			
		}
