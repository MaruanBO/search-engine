<script>
$(document).ready(function(){
    // Get current date
    var currentDate = new Date(),
        dateYMD = currentDate.getFullYear() + '-' + (currentDate.getMonth() + 1) + '-' + currentDate.getDate(),
        currentHours = currentDate.getHours();

    // The display period is: Start => 5-20 21:00 | End => 5-21 24:00
    if ((dateYMD === '2013-5-20' && currentHours > '20') || dateYMD === '2013-5-21') {
        $('#js-features').addClass('js-yt-player').find('header h1.header').html('Comedy Week Videos');
    }
})
</script>
