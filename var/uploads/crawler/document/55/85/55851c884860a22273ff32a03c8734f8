	<script>

		$(function () {
			// dom ready stuff here

			

			// define vars for catchup
			var _expire = new Date(),
				_catchup = {
					page : 1,
					key : 'all'
				};

			// if last page was a not cdp then reset catchup values & cookie
			if (BUI.page.lastPageType.indexOf('cdp') === -1 || !$.cookie('brkTimestream')) {
				// expiration date to 1 hour for our catchup cookie
				_expire.setTime(_expire.getTime() + (60 * 60 * 1000));

				// refresh cookie
				$.cookie('brkTimestream', JSON.stringify(_catchup), {
					expires : _expire,
					domain : BUI.globals.ENV_URLS.DOTBREAK
				});

			} else if ($.cookie('brkTimestream')) {
				try {
					_catchup = JSON.parse($.cookie('brkTimestream'));
				} catch (e) {

				}
			}

			//BUI.utils.log('catchup', _catchup);

			//Timestream init
			// Timestream has to be in the dom ready otherwise BUI.page was not yet defined.
			// The catchUp param lets the timestrean handle the catchup sequence.
			window.BUI.timestream.Init({
				pageType			: 'Home',
				currentPageNum		: 1,
				maxRecords			: 35154,
				totalPages			: 1758,
				maxItems			: 35154,
				maxPageNum			: 1758,
				breakUri			: 'http://www.break.com/',
				categoryId			: 1,
				pageSize			: 20,
				pushState			: false,
				usePagination		: false,
				thumbSize			: '_550x309',
				sashSize			: 'ribbon-135',
				useCarousel			: false,
				catchUp				: _catchup,
				paginationPageTitle : 'Page {pageNum} | Funny Videos, Funny Clips & Funny Pictures | Break.com',
				paginationPageMetaDescription: 'Watch funny videos and video clips on {pageNum} at Break.com. Our editors find the best funny videos, clips and pictures for you to watch right now. - Page {pageNum}',
				defaultPageTitle: 'Funny Videos, Funny Clips &amp; Funny Pictures | Break.com',
				defaultPageMetaDescription: 'Watch funny videos and video clips at Break.com. Our editors find the best funny videos, clips and pictures for you to watch right now.'
			});

			// Custom scroller initialization
			$("#activity-scroll").scrollbars();

			// Ellipsis initialization for Community Activity
			$(".ellipsis").dotdotdot({
				wrapper : "span",
				watch : true
			});

		/* brkr-2908 Remove the archive grid from dom so it will never be visible for user */
		$(".js-archives-grid").remove();

			// brkr-2790 : make section sticky to timestream
			// except on mobile like the sticky nav
			// brkr-2955 make the btf ad sticky
			if (!BUI.globals.IS_MOBILE) {
				$("#orig-series, #js-btf-ad").brksticky({
					stickToElement : $("#homepage-worthy"),
					rdWrapper : $("#container"),
					offsetTop : $(".ui-masthead").height() + 10
				});
			}

		});
	</script>


