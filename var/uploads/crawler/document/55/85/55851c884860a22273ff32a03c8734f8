<script>
	(function () {
		// render sign in UI based on login cookie
		var cookie = $.cookie('brk_net_13'),
			isLoggedIn = false;

		if (typeof cookie !== 'undefined') {
			try {
				cookie = JSON.parse(cookie);
				isLoggedIn = true;
			} catch (ee) {
				BUI.utils.log('brk_net_13 cookie parse error', cookie, ee);
			}
		}

		if (!isLoggedIn) {
			BUI.controls.user.handleLogin();
		}
	})();
</script>

