<script>
	var HA = window.HA || {};
	HA.scripts = (function() {
    	var scripts = [];
    	var enabled = false;
    	var exec = function(f) {
      		f.call();
    	}
    	return {
    		enable: function() {
    			enabled = true;
    		},
    		disable: function() {
    			enabled = false;
    		},
      		defer: function(f) {
      			if (enabled) {
	        		scripts.push(f);
      			} else {
      				exec(f);
      			}
      		},
      		run: function() {
        		for (var i = 0; i < scripts.length; i++) {
        			exec(scripts[i]);
       		 	}
       		 	scripts = [];
       		 	enabled = false;
      		}
    	};
  	})();
	HA.scripts.enable();
</script>
