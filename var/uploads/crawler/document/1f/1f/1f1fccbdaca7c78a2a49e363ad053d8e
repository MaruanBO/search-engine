	<script>
	    function setCookie(c_name,value,exdays)
	    {
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString())+'; path=/';
		document.cookie=c_name + "=" + c_value;
	    }

	    function getCookie(c_name)
	    {
		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + c_name + "=");
		if (c_start == -1)
	        {
		  c_start = c_value.indexOf(c_name + "=");
		}
		if (c_start == -1)
		{
		  c_value = null;
		}
		else{
		  c_start = c_value.indexOf("=", c_start) + 1;
		  var c_end = c_value.indexOf(";", c_start);
		  if (c_end == -1){
		    c_end = c_value.length;
		  }
		  c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;
	    }
	    var full_site=getCookie("full_site");
	    if (full_site==null || full_site==""){
	        $('head').append('<link rel="stylesheet" href="http://i.dlisted.com/wp-content/themes/dlisted_44/mobile_styles.css?v=1392299985">');
	    }
	    var ba_is_mobile = false;

	    var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
	    ba_is_mobile = width<768;
    
	    if(ba_is_mobile)
	        ba_is_mobile = 1;
	    else 
	        ba_is_mobile = 0;
            
            setCookie('ba_is_mobile',ba_is_mobile,1);

	    $(document).ready(function(){
		if (full_site==null || full_site==""){
	    	    $('.header-box').append('<div class="full_site_version_container"><a href="#" id="full_site_version">Full site</a></div>');
		}
		else {
		    $('footer > div.container').append('<div class="mobile_site_version_container"><a href="#" id="mobile_site_version">Mobile version</a></div>');
		}
		$('.full_site_version_container a').click(function(){
		    setCookie('full_site','1',5*365);
		    document.location="/";
		    return false;
		})
		$('.mobile_site_version_container a').click(function(){
		    setCookie('full_site','0',-1);
		    document.location="/";
		    return false;
		})
	    });
	</script>

