<script type="text/javascript">
jQuery(document).ready(function($){

//Set Cookie Declare functions
function setCookie(name,value,days) {
    if (days) {
        var date = new Date();
        date.setTime(date.getTime()+(days*24*60*60*1000));
        var expires = "; expires="+date.toGMTString();
    }
    else var expires = "";
    document.cookie = name+"="+value+expires+"; path=/";
}

function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

function deleteCookie(name) {
    setCookie(name,"",-1);
}


var interstitialData =[]
interstitialData['uniqueID'] = 'CityofLaPorteInterDTI116979'; // Customer Ref Num
interstitialData['backgroundFile'] = 'http://2021bd507a521c251523-868d1f34e6e26ab3afab26f7d8dbc2a7.r31.cf2.rackcdn.com/2002442/CityofLaPorteInterDTI116979_700x500.jpg'; //creative background
interstitialData['closeButtonImage'] = 'http://c15059404.r4.cf2.rackcdn.com/close-button.png'; //creative background
interstitialData['URL'] = []
interstitialData['URL'][0] = []
interstitialData['URL'][0]['url'] = 'http://bit.ly/17iEbDQ'; // Advertiser Website
interstitialData['URL'][0]['top'] ='0px';
interstitialData['URL'][0]['width'] ='700px';
interstitialData['URL'][0]['height'] ='500px';


if(getCookie(interstitialData['uniqueID'])){ 
  return; //If Unique ID is loaded to cookie die
}

//Add elements to page
 $('body').append('<div id="interstatial-ad-container" style="width:700px; height:500px; background-color: #ffffff; box-shadow: 0px 5px 30px rgba(0,0,0,.5); position:absolute; left: 50%; margin-left: -350px; top:10%; background-image:url('+interstitialData['backgroundFile']+'); z-index: 5001"><a id="close-button-interstatial"><img src="'+interstitialData['closeButtonImage'] +'"/></a><a id="skip-ad-link" style="color:#ffffff; cursor:pointer; bottom:0px; display:block; line-height:30px; font-family:Arial; font-size:12px; padding:0px; position:absolute; right: 3px; text-decoration:none; z-index:10000003; ">[ skip this ad ]</a></div>')
  
 $('body').append('<div id="interstatial-click-container"></div>')
 
 $('#close-button-interstatial').css({'color':'#000c87', 'padding':'5px 10px', 'top':'4px', 'right': '7px', 'position':'absolute',  'line-height':'30px', 'text-decoration':'none', 'display':'block', 'cursor':'pointer', 'z-index':'5003' })
 
 $('#close-button-interstatial img').css({'min-width':'30px', 'min-height':'30px'})
 
 $('#interstatial-click-container').css({'width': '700px', 'height':'500px',  'left': '50%', 'margin-left': '-350px', 'top':'10%', 'position':'absolute', 'z-index':'999' })

//Reposition when scrolled
 $(document).scroll(function(){
  $('#interstatial-ad-container, #interstatial-click-container').css({'top': $(window).scrollTop()+($(window).height()*.05)})
 })

//close button
 $('#close-button-interstatial, #skip-ad-link').click(function(){
  $('#interstatial-ad-container, #interstatial-click-container').remove();
 })
// Click through
 function createClickThrough(){
  if($('#interstatial-click-container').html()==''){
   for(i=0; i<interstitialData['URL'].length; i++){
    $('#interstatial-ad-container').append('<a href="' + interstitialData['URL'][i]['url'] + '" style="position:absolute; top:'+ interstitialData['URL'][i]['top'] +'; left:'+ interstitialData['URL'][i]['left'] +'; height:'+interstitialData['URL'][i]['height']+'; width:' +interstitialData['URL'][i]['width'] + '; cursor: pointer; z-index:5002; display:block;"></a>'); // Additional Direction
   }
  }
 }

createClickThrough();

$('#interstatial-ad-container, #interstatial-click-container').delay(10000).fadeOut()

//Set Advertisment Unique ID to browser cache and store it for a day
setCookie(interstitialData['uniqueID'], true, 1);

}(jQuery))
</script>

