<script type="text/javascript">

 var browserInfo=navigator.userAgent;
 var ieList=$('#ie').val();
 var ieb2b = $('#ieb2b').val();
 var chromeList=$('#chrome').val();
 var firefoxList=$('#firefox').val();
 var safariList=$('#safari').val();
 var issmbsite=$('#issmbsite').val();
 
 var safariList=$('#safari').val();
 /* Finding version for Internet Explorer */
   if (((navigator.appName == "Microsoft Internet Explorer")|| (navigator.appName == "Netscape"))  && (ieList!="Please Configure the version from the dialog box"))
     {
       
            if (document.documentMode != "undefined")
             {
                 var version = document.documentMode;
                 
                           if (version<ieList)
                           {
                            $('#UpgradeMainDiv').removeClass("hide"); 
                            }
             }                      
     }
     
   /*Finding version for Internet Explorer, IE version for b2b Website*/
   if((navigator.appName == "Microsoft Internet Explorer") 
   && (ieb2b !="Please Configure the version from the dialog box") 
   && (issmbsite == "true" || issmbsite != null))
   {
       //alert(issmbsite );
       if (document.documentMode != "undefined")
             {
                 var version = document.documentMode;
                 
                           if (version < ieb2b)
                           {
                                $("#UpgradeMainDiv").removeClass("hide"); 
                            }
             }  
   }
   
   
  /* Finding version for Google Chrome */ 
    
       if ((browserInfo.indexOf("Chrome")!= -1) && (chromeList !="Please Configure the version from the dialog box"))
         {
           
              var header=browserInfo.slice(browserInfo.indexOf("Chrome/")); 
              var pos=header.slice(header.indexOf("/"));
              var version=pos.substr(1,(pos.indexOf("."))-1);
                    if (version<chromeList)
                           {
                            $('#UpgradeMainDiv').removeClass("hide"); 
                            }
         } 
         
      /* Finding version for Mozilaa Firefox */
      
         if ((browserInfo.indexOf("Firefox")!=-1) && (firefoxList !="Please Configure the version from the dialog box"))
          {
            var header=browserInfo.slice(browserInfo.indexOf("Firefox/")); 
            var pos=header.slice(header.indexOf("/"));
            var version=pos.substr(1,(pos.indexOf("."))-1);
                if (version<firefoxList)
                    {
                            $('#UpgradeMainDiv').removeClass("hide"); 
                     }
         } 
         
      /* Finding version for Safari */
      
      if ((browserInfo.indexOf("Version")!= -1) && (safariList !="Please Configure the version from the dialog box"))
          {
             var header=browserInfo.slice(browserInfo.indexOf("Version/")); 
             var pos=header.slice(header.indexOf("/"));
             var version=pos.substr(1,(pos.indexOf("."))-1);
             var safari= parseInt(safariList);
              if (version<safari)
                  {
                            $('#UpgradeMainDiv').removeClass("hide"); 
                   }
          } 
</script>
