<script type="text/javascript">
Date.prototype.addHours= function(h){
    var MILLISECS_PER_HOUR = 60 /* min/hour */ * 60 /* sec/min */ * 1000 /* ms/s */;
    return new Date(+this + h*MILLISECS_PER_HOUR);
}
Date.prototype.subtractMinutes= function(m){
    var MILLISECS_PER_MINUTES = 60 /* sec/min */ * 1000 /* ms/s */;
    return new Date(+this - m*MILLISECS_PER_MINUTES);
}

var currentServerTime = new Date();

$(document).ready(function(){
    $.ajax({
        type: "GET",
        url: '/etc/designs/tmo-wem/clientlibs_base/img/tmo_loader.gif',
        cache: false,
        success: function(data, textStatus, request){
            currentServerTime =new Date(request.getResponseHeader('Date'));
            startTimeTicker();

            $(".next-slide").click(function(){
                showHideTickerByslide();
    
            });
            $(".prev-slide").click(function(){
                showHideTickerByslide();
    
            });
        },
        error: function (request, textStatus, errorThrown) {
            currentServerTime =new Date(request.getResponseHeader('Date'));
            startTimeTicker();

            $(".next-slide prev-slide").click(function(){
                showHideTickerByslide();
    
            });
            $(".prev-slide").click(function(){
                showHideTickerByslide();
    
            });
        }
      });
  
});

    function startTimeTicker()
    {

        $( ".tickerclock" ).each(function( index ) {

            var OfferStartTime= new Date($(this).attr("deal-start-time"));
            var OfferDuration= $(this).attr("deal-duration");

            var OfferEndTime=OfferStartTime.addHours(OfferDuration);


                $(this).countdown(OfferEndTime, function(event) {
                    var format = '<p class="text-black"><span class="text-magenta">%D</span>Days</p><div class="space">:</div><p class="text-black"> <span class="text-magenta">%H</span> Hours </p> <div class="space">:</div> <p class="text-black"> <span class="text-magenta">%M</span> Minutes </p> <div class="space">:</div> <p class="text-black"> <span class="text-magenta">%S</span> Seconds </p>';
                    var $this = $(this).html(event.strftime(format));
                    }).on('update.countdown', showTickerAlertMessage);

            var tickerDiv=$(this).closest(".timeticker");
            if(OfferStartTime>currentServerTime)
            {
                if('false'=='true')
                {
                    if($(tickerDiv).next(".marquee-slide").is(":visible"))
                    {
                        tickerDiv.show();
                    }
                    else
                    {
                        //console.log('hide ticker');
                        tickerDiv.hide();
                    }
                }
                else
                {
                    tickerDiv.hide();
                }
            }
            else
            {
                if($(tickerDiv).next(".marquee-slide").is(":visible"))
                {
                    tickerDiv.show();
                }
                else
                {
                    //console.log('hide ticker');
                    tickerDiv.hide();
                }
            }

            if(OfferEndTime>=currentServerTime)
            {
                $(this).closest(".row").find("a.deal-active").show();
            }
            else
            {
                var row=$(this).closest(".row");
                row.find("span.deal-expired").show();
                row.find("a.deal-expired").show();
            }

        });
    }

    function showHideTickerByslide()
    {
        $( ".marquee-slide" ).each(function( index ) {

            if($(this).is(":visible"))
            {
                var tickerDiv=$(this).prev(".timeticker");
                var tickerclock=tickerDiv.find(".tickerclock");
                //console.log('show ticker');
                var OfferStartTime= new Date(tickerclock.attr("deal-start-time"));
                if(OfferStartTime>(new Date()))
                {
                    if('false'=='true')
                    {
                        tickerDiv.show();
                    }
                    else
                    {
                        tickerDiv.hide();
                    }
                }
                else
                {
                    tickerDiv.show();
                }
            }
            else
            {
                //console.log('hide ticker');
                $(this).prev(".timeticker").hide();
            }

        });

    }

    function showTickerAlertMessage()
    {

        $( ".tickerclock" ).each(function( index ) {

            var OfferStartTime= new Date($(this).attr("deal-start-time"));
            var OfferDuration= $(this).attr("deal-duration");
            var alertDuration= $(this).attr("alert-duration");
            var OfferEndTime=OfferStartTime.addHours(OfferDuration);
            var alertDisplayTime=OfferEndTime.subtractMinutes(alertDuration);
            var tickerDiv=$(this).closest(".timeticker");

            if(OfferStartTime>=(new Date()))
            {
                if('false'=='true')
                {
                    if($(tickerDiv).next(".marquee-slide").is(":visible"))
                    {
                        tickerDiv.show();
                    }
                    else
                    {
                        //console.log('hide ticker');
                        tickerDiv.hide();
                    }
                }
                else
                {
                    tickerDiv.hide();
                }
            }
            else
            {
                if($(tickerDiv).next(".marquee-slide").is(":visible"))
                {
                    tickerDiv.show();
                }
                else
                {
                    //console.log('hide ticker');
                    tickerDiv.hide();
                }
            }
            if(alertDisplayTime<=(new Date()))
            {
                var row=$(this).closest(".row");
                row.find("span.deal-alert").show();
                row.find("a.deal-active").show();
            }
            if(OfferEndTime<=(new Date()))
            {
                var row=$(this).closest(".row");
                row.find("span.deal-alert").hide();
                row.find("a.deal-active").hide();
                row.find("span.deal-expired").show();
                row.find("a.deal-expired").show();
            }
        });
    }

</script> </div>

</div>

        </div>
