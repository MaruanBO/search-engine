window.REVIEW_STAR_RATING_IMAGE_URL = "//t-mobile.ugc.bazaarvoice.com/9060redes2-en_us/{0}/rating.gif";
window.NO_REVIEW_IMAGE_URL = "//t-mobile.ugc.bazaarvoice.com/static/9060redes2-en_us/translucent.gif";
window.BV_REVIEW_STAR_API_URL = "//api.bazaarvoice.com/data/statistics.json?apiversion=5.3&passkey=iedghsauz9fm2oe3h7pjqhxxg&filter=productid:{0}&stats=Reviews";   
//CQ-1036
$(document).ready(function() {

            var pricingText = "down";
            var fullRetailPriceText = "full retail price";
            //Reading the Credit Level Cookie Value
            var creditLevelCookie = $.cookie('TMobileCreditLevel')||"A";
            var deviceServiceUrl =  "/Services/Activation/ActivationService.svc/deviceprice";
            var accessoryServiceUrl=  "/Services/Activation/ActivationService.svc/AccessoryPrice";
            
            var fullRetailPrice;
            var suggestedPrice;
            var instantDiscount;
            var webDiscount;
            var downPayment;
            var refurbishedPrice;
            var discount;
                      
            //Reading the Location Cookie value
            var locationCookieValArr = $.cookie('TMobileUSLocation');
            
            if((locationCookieValArr != undefined)||(locationCookieValArr != null)){
                var stateCode = locationCookieValArr.split("STATE=");    
                var zipCode = locationCookieValArr.split("ZIP=");
                var stateVal  = stateCode[1];
                if( stateVal != undefined ){
                  stateVal = stateVal.substring(0,2);              
                }
                var zipVal = zipCode[1];
                if( zipVal != undefined ){
                 zipVal = zipVal.substring(0,5);
               }
                if(stateVal == ""){
                 stateVal = "undefined";
                }
                if(zipVal == ""){
                 zipVal = "undefined";
                }
            }else{
                var stateVal = "undefined";
                var zipVal = "undefined";
            }

            
           Clover.princingModule.getDevicePrices = function(){
            var deviceIdValues = 'deviceIdValue';
            var accessoryIdValues = 'accessoryIdValue';
                
            $('.flexslider .product-card').each(function(){
                var deviceIdValue = $(this).attr('data-deviceid');
                var deviceType = $(this).attr('data-devicetype');
                if ((typeof deviceIdValue != "undefined")||(deviceIdValue != null)) {
                    if(deviceType == "accessory"){
                       accessoryIdValues =  deviceIdValue +','+accessoryIdValues;
                    }else if(deviceType == "phone" || deviceType == "internet"){
                       deviceIdValues =  deviceIdValue +','+deviceIdValues;
                    }
                }
            });                           

            $('.OfferText').click(function(){
                var deviceId = $(this).parent().attr("data-deviceid");
                var deviceType = $(this).parent().attr('data-devicetype');
                if(deviceType == 'accessory'){
                         popUp('https://www.t-mobile.com/Popup/Generic_Popup.aspx?action=accessorypromotion&accessory=' + deviceId, '400', '568', '45', '30', '0', '1', '1' );
                    }
                    else {
                         popUp('https://www.t-mobile.com/Popup/Generic_Popup.aspx?action=additionaloffer&device=' + deviceId, '400', '568', '45', '30', '0', '1', '1' );
                    }
                });
                
            
            function popUp( url, height, width, top, left, toolbar, resize, scrollbars )
                {   
                    var pop = window.open( url, 'pop', 'height=' + height + ',width=' + width + ',top=' + top + ',left=' + left + ',toolbar=' + toolbar + ',resizable=' + resize + ',scrollbars=' + scrollbars );   
                    if( typeof window.focus != 'undefined' ) javascript: pop.focus();
                }
            
            if ((deviceIdValues !== '')&&(deviceIdValues !== 'deviceIdValue' )) {
                deviceIdValues  = deviceIdValues.split(',deviceIdValue');
                Clover.princingModule.productJsonJSquery = '{"DeviceID":"'+deviceIdValues[0]+'","State":"'+stateVal+'","CreditClass":"'+creditLevelCookie+'","ZIPCode":"'+zipVal+'"}';
                                  
                $.ajax({
                type: "POST",
                contentType: "application/json",
                dataType: 'json',
                url: deviceServiceUrl,
                data: Clover.princingModule.productJsonJSquery ,
                async: true,
                success: function(data){

                    for (var i = 0; i < data.Device.length; i++){
                    
                       suggestedPrice = Number(data.Device[i].SuggestedPrice).toFixed(2); 
                       webDiscount = Number(data.Device[i].WebOnlyDiscount).toFixed(2);
                       instantDiscount = Number(data.Device[i].InstantDiscount).toFixed(2);
                       refurbishedPrice = Number(data.Device[i].RefurbishedDiscount).toFixed(2);                       
                       fullRetailPrice = Number(suggestedPrice) - (Number(webDiscount)+Number(instantDiscount)+Number(refurbishedPrice));
                    
                        if((data.Device[i].EIP != null)&&(data.IsEIPEligible)&&(data.Device[i].EIP.IsDeviceEIPEligible)){
                           downPayment = Number(data.Device[i].EIP.DownPayment).toFixed(2);

                           discount = Number(webDiscount) + Number(refurbishedPrice);
                           
                           if(Number(downPayment) >= Number(discount )){
                               downPayment = Number(downPayment)-Number(discount);
                            }else{
                                downPayment = 0;
                            }                           
                           
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".generic-upfront-price").html('$'+Clover.princingModule.getSuperDecimalValue(Number(downPayment).toFixed(2)));
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".upfront-text").text(pricingText);
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".fc-full-retail-price-visible").addClass("hide");                        
                        }else{
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".generic-upfront-price").html('$'+Clover.princingModule.getSuperDecimalValue(Number(fullRetailPrice).toFixed(2)));
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".upfront-text").text(fullRetailPriceText);
                           $('[data-deviceid=' + data.Device[i].DeviceID+ ']').find(".fc-upfront-price-visible").addClass("hide");                                                                         
                        }
                       }
                 }      
                });                  
            }
                
            if ((accessoryIdValues !== '')&&(accessoryIdValues !== 'accessoryIdValue')) {
                    accessoryIdValues = accessoryIdValues.split(',accessoryIdValue');
                    Clover.princingModule.accessoryJsonJSquery = '{"AccessoryID":"'+accessoryIdValues[0]+'"}';
                    
                    $.ajax({
                    type: "POST",
                    contentType: "application/json",
                    dataType: 'json',
                    url: accessoryServiceUrl,
                    data: Clover.princingModule.accessoryJsonJSquery,
                    async: true,
                    success: function(data){
                      for (var i = 0, max = data.accessory.length; i < max; i++){
                        var acc = data.accessory[i];
                        $('[data-deviceid=' + acc.AccessoryID+ ']').find(".generic-upfront-price").html('$'+Clover.princingModule.getSuperDecimalValue(acc.Price));
                        $('[data-deviceid=' + acc.AccessoryID+ ']').find(".upfront-text").text(pricingText);
                        if (acc.IsOutOfStock === true) {
                          $('[data-deviceid=' + acc.AccessoryID+ ']').find(".add-to-cart-btn-container").html(window.OUT_OF_STOCK_TEXT);
                        }
                      }
                    }      
                  });                   
            }             
          }
                
                
        $(".dropdown-menu li").click(function(ev){ 
            ev.preventDefault();
            setTimeout(function(){Clover.princingModule.getDevicePrices()}, 100);
         });
         Clover.princingModule.getDevicePrices();



 });

</script>

