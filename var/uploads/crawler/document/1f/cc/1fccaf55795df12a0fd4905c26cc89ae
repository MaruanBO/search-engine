<script language="JavaScript" type="text/javascript">

	var max_age_for_time_kicker = 1000 * 60 * 60 * 3;
	var now    = new Date();
	var now_ts = now.getTime();

	$( '.ezb-lead_story time, .ezb-12_items time, .ezb-big_event time' ).each( function( key )
	{
		if( $( this ).attr( 'data_show_datestamp' ) == '1' )
		{
			var article_ts = $( this ).attr( 'data_timestamp' ) * 1000;

			if( ( now_ts - article_ts ) <= max_age_for_time_kicker )
			{
				$( this ).html( $(this).html().trim().substr( 11 ) );
				$( this ).show();
			}	
		}
	});

</script>

