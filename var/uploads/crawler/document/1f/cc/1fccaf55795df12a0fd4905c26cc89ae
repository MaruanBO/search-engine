<script>

$(function()
{
	// global spotlight carousel
	var carouselElement = $( '#cover-story-carousel-container #cover-story-carousel' ).jcarousel(
	{
		transitions: Modernizr.csstransitions ? {
			transforms:   Modernizr.csstransforms,
			transforms3d: Modernizr.csstransforms3d,
			easing:       'ease'
		} : false
	})
    .on('jcarousel:scrollend', function( event, carousel ) {
		if( ! carousel._target.find( 'img.lazy-load' ).attr( 'src' ) ) {
			carousel._target.find( 'img.lazy-load' ).attr( 'src',carousel._target.find( 'img.lazy-load' ).attr( 'data-src' ) );
		}
	});

	// sliding events
	$( '#cover-story-carousel-container .jcarousel-next' ).click( function(e) {
		carouselElement.jcarousel('scroll', '-=1');
		afterSlide( carouselElement.jcarousel( 'target' ).index() );
	});
	$( '#cover-story-carousel-container .jcarousel-prev' ).click( function(e) {
		carouselElement.jcarousel('scroll', '+=1');
		afterSlide( carouselElement.jcarousel( 'target' ).index() );
	});
	
	// Read Now buttons
	$( '#cover-story-carousel-container .btn_1 a' ).each( function()
	{
		var target = $(this).closest( 'li' ).find( '.story_headline > a' ).attr( 'href' );
		if( target )
		{
			$(this).attr( 'href', target );
		}
	});
});

function afterSlide( currentIndex )
{
	var lastIndex = $( '#cover-story-carousel li' ).length - 1;
	switch ( currentIndex ) 
	{
		case 0: 
			$( '#cover-story-carousel-container .jcarousel-next' ).addClass( 'disabled' ).removeClass( 'clickable' );
			break;
		case lastIndex:
			$( '#cover-story-carousel-container .jcarousel-prev' ).addClass( 'disabled' ).removeClass( 'clickable' );
			break;
		default:
			$( '#cover-story-carousel-container .jcarousel-next' ).addClass( 'clickable' ).removeClass( 'disabled fade_separator' );
			$( '#cover-story-carousel-container .jcarousel-prev' ).addClass( 'clickable' ).removeClass( 'disabled' );
	}
}

</script>		</div>
		
