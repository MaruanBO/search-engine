    <script>
        
        function ShowCarasolDetail(Id) {
            var opaueElements = document.getElementsByClassName('promo-main opaque');
            if (opaueElements != null && opaueElements.length > 0) {
                for (var i = 0; i < opaueElements.length; i++) {
                    document.getElementById(opaueElements[i].id).setAttribute("class", "promo-main");
                }
            }
            document.getElementById(Id).setAttribute("class", "promo-main opaque");
        }
        

        $(function () {

            $('div.show-more-container-text').expander({

                slicePoint: 200,
                expandText: '.....More',
                userCollapseText: 'Less',

            });
        });

        function cascadingdropdown() {
            //debugger;
            //$("#SpecialityId").empty();
            //$("#stateID").append("<option value='0'>--Select State--</option>");
            //var roleId = 'Role';//$('#RoleId option:selected').text();
            var roleId = $('#RoleId option:selected').text();
            //var Url="/api/sitecore/Home/GetSpecialities";
            $.ajax({
                url: "/api/sitecore/Home/GetSpecialities",
                //dataType: 'json',
                type: 'POST',
                cache: false,
                data: { roleId: roleId },
                error: function () {
                },
                success: function (result) {
                    $('#SpecialityId').empty();
                    $.each(result, function () {
                        $('#SpecialityId').append(
                            $('<option/>', {
                                value: this.Value,
                                html: this.Text
                            })
                        );
                    });
                }
            });
        }

        $("#btnFilterQuratedList").click(function () {
            //if ((($("#RoleId option:selected").val() == "Role" || ($("#RoleId option:selected").val() == "All"))
            //    && ($("#SpecialityId option:selected").val() == "Specialties" || $("#SpecialityId option:selected").val() == "All")
            //    //&& $("#ProductTypeId option:selected").val() == "Product Type")
            //    )) {
            //    return false;
            //}
            //else {
            window.location = './?Role=' + $("#RoleId option:selected").text() + '&Speciality=' + $("#SpecialityId option:selected").text()
                //+ '&ProductType=' + $("#ProductTypeId option:selected").val()
                ;
            //}
        });

        function ShowCuratedListPage(pageCount) {
            if (pageCount != undefined && pageCount > 0) {
                window.location = './?Role=' + $("#RoleId option:selected").text()
                    + '&Speciality=' + $("#SpecialityId option:selected").text()
                    + '&Page=' + pageCount;
            }
        }
    </script>

    <script>
        
        function changePromo(selected) {
            $('.inner-container.promo .promo-main').animate({
                opacity: 0
            }, 400, 'linear', function() {
                $(this).removeClass('opaque');
            });

            $('div[data-item=' + $(selected).data('item') + ']').animate({
                opacity: 1
            }, 400, 'linear', function() {
                $(this).addClass('opaque');
            });
        }

        $(document).ready(function() {
            var $slider = $('.slider'),
                slider;

            $slider.tinycarousel({
                axis: 'y',
                interval: true,
                intervalTime: 5000,
                animationTime: 500,
                infinite: true,
                buttons: true
            });
            slider = $slider.data('plugin_tinycarousel');


            $slider.bind('move', function(e, selected) {
                //visually, the middle item is 'selected', so need to target that one as 'selected'
                selected = $(selected).next('li');

                changePromo(selected);
            });
            
        });
    </script>

