<script type="text/javascript">
// config settings
var mlbSettings = {
deviceType : DeviceType.device,
override : 'desktop',
startup : 'desktop',
hostname: "http://ui.bamstatic.com/",
sourceDirectory : "deploy/2.0.4"
};
if (mlbSettings.deviceType === 'phone' || mlbSettings.override === 'phone') {
mlbSettings.startup = 'phone'
} else if (mlbSettings.deviceType === 'tablet' || mlbSettings.override === 'tablet') {
mlbSettings.startup = 'tablet'
}
</script>
<script src="http://mlb.mlb.com/bundle?js=http://ui.bamstatic.com//bridge/core/shims/string.js&js=http://ui.bamstatic.com//bridge/core/shims/date.js&js=http://ui.bamstatic.com//bridge/core/require/require.js&js=http://ui.bamstatic.com//bridge/core/jquery/jquery.min.js&js=http://ui.bamstatic.com//clubs/mlb/startup/requireConfigs.js&v=2.0.4"></script>
<script type="text/javascript">
// need to add this shim for other browsers that dont support local storage - ugly but should only imapct a small set of browsers and that pos kisok downstairs
if (typeof(localStorage) == 'undefined' ) {
document.write('<script src="http://ui.bamstatic.com//bridge/core/shims/ie7/localStorage.js" type="text/javascript"><\/script>');
document.write('<script src="http://ui.bamstatic.com//bridge/core/shims/ie8/es5shim.js" type="text/javascript"><\/script>');
}
</script>
