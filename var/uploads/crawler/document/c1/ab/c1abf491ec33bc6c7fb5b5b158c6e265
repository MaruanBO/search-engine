<script type="text/javascript">
    $(function () {
        var $storeSubNav = $("#storeSubNav");
        var $recipesSubNav = $("#recipesSubNav");
        var $currentNavItem;
        var currentUrl = window.location.pathname;
        var navItems = $('#main-nav ul li');
        switch(window.location.pathname)
        {
            case '/store.aspx':
                $(navItems[0]).addClass("currentNavItem");
                break;
            case '/WhySodaStream.aspx':
                $(navItems[1]).addClass("currentNavItem");
                break;
            case '/HowItWorks.aspx':
                $(navItems[2]).addClass("currentNavItem");
                break;
            case '/TasteNutritionInfo.aspx':
                $(navItems[3]).addClass("currentNavItem");
                break;
            case '/where_to_buy.aspx':
                $(navItems[4]).addClass("currentNavItem");
                break;
        }
        //<li>
		//		<a href="/store.aspx">ONLINE STORE</a>
				
		//	</li>
		//	<li><a href="/WhySodaStream.aspx">WHY SODASTREAM</a></a></li>
		//	<li><a href="/HowItWorks.aspx">HOW IT WORKS</a></li>
		//	<li><a href="/flavors.aspx">RECIPES &amp; NUTRITION</a></li>
		//	<li><a href="/where_to_buy.aspx">WHERE TO BUY</a></li>

        $("#main-nav ul li").on("mouseenter mouseleave", function (e) {
            var $current = $(this).find("a")[0];
            var currentLink = $current.text == undefined ? $current.innerText : $current.text;
            $currentNavItem = $(this);
            var event = e;
            if (currentLink == 'NUTRITION & RECIPES' || currentLink == 'ONLINE STORE') {
                switch (e.type) {
                    case "mouseenter":
                        if (currentLink == 'ONLINE STORE') {
                            $("#storeSubNav").show();
                            $.data($current, 'hover', true);
                            $(this).addClass("navHover");
                            var temp = $.data($current, 'hover');
                        }
                        else {
                            $("#recipesSubNav").show();
                            $.data($current, 'hover', true);
                            $(this).addClass("navHover");
                        }
                        break;

                    case "mouseleave":
                        if (currentLink == 'ONLINE STORE') {
                            $.data($current, 'hover', false);
                            if (!$.data($current, 'hover') && !$.data($storeSubNav, 'hover')) {
                                $storeSubNav.hide();
                                $(this).removeClass("navHover");
                            }
                        }
                        else {
                            $.data($current, 'hover', false);
                            if (!$.data($current, 'hover') && !$.data($recipesSubNav, 'hover')) {
                                $recipesSubNav.hide();
                                $(this).removeClass("navHover");
                            }
                        }
                        break;
                }
            }
        });

        $("#storeSubNav, #recipesSubNav").on("mouseenter mouseleave", function (e) {
            var event = e;
            var current = $(this)[0];
            switch (e.type) {
                case "mouseenter":
                    if (current.id == 'storeSubNav') {
                        $.data($storeSubNav, 'hover', true);
                        $currentNavItem.addClass("navHover");
                        $storeSubNav.show();
                    }
                    else {
                        $.data($recipesSubNav, 'hover', true);
                        $currentNavItem.addClass("navHover");
                        $recipesSubNav.show();
                    }
                    break;
                case "mouseleave":
                    if (current.id == 'storeSubNav') {
                        $.data($storeSubNav, 'hover', false);
                        $currentNavItem.removeClass("navHover");
                        $storeSubNav.hide();
                    }
                    else {
                        $.data($recipesSubNav, 'hover', false);
                        $currentNavItem.removeClass("navHover");
                        $recipesSubNav.hide();
                    }
                    break;
            }
        });
    });
</script>


