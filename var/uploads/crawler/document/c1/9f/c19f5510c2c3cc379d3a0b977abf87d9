  window.burtApi.startTracking(function(api) {
    api.setTrackingKey('BUR6B5MTXHL1', 'burtcorp-agency.com');
    api.setCategory('home', 'home-sub');
    // api.setDomain('burtcorp.com');
    api.startUnitTracking();
    api.setDemographicsProvider('cint');
    api.annotate('pa-scope', 'pa-name', 'pa-value');
    api.withUnits('ad_sales', function (unit) {
      unit.connect('burt.campaign', 'campaign-id', '12345');
    });
    api.withUserSegments(function(segments) { window.segments = segments; });
  });

  window.burtApi.use('cloud_store', function() {
    var store = window.burtApi.cloudStore('BUR54ON7NUTL');
    store.all(function(all) {
      all = all || {};

      if (all.foo) {
        window.burtApi.annotate('cloud_store', 'foo', all.foo);
      } else {
        var date = new Date();
        date.setDate(date.getDate() + 1);

        store.set('foo', Math.random(), { expires: date });
      }
    });
  });
</script>
<script type="text/javascript">
  setTimeout(function() {
    window.burtApi.withUnits('ad_adops', function (unit) {
      unit.restart({name: 'ad_adops_restart'});
      unit.annotate('ur-scope', 'ur-name', 'ur-value');
    });
  }, 5000);

  window.burtApi.use('watcher', function() {
    window.burtApi.addWatch(function(watcher) {
      watcher.setRequireContinuous(false);
      watcher.setRequireActive(true);
      watcher.setDuration(10);
      watcher.register(function() {
        window.burtApi.annotate('watcher.page', 'inc', '1');
      });
      watcher.start();
    });

    window.burtApi.withUnits('ad_decision', function(unit) {
      unit.addWatch(function(watcher) {
        watcher.setRequireContinuous(true);
        watcher.setRequireActive(true);
        watcher.setDuration(3);
        watcher.setVisibility(100);
        watcher.register(function() {
          unit.annotate('watcher.decision', 'inc', '1');
        });
        watcher.start();
      });
    });

    window.burtApi.withUnits('ad_sales', function(unit) {
      unit.addWatch(function(watcher) {
        watcher.setRequireContinuous(true);
        watcher.setRequireInactive(true);
        watcher.setDuration(3);
        watcher.setVisibility(0, 50);
        watcher.register(function() {
          unit.annotate('watcher.sales', 'inc', '1');
        });
        watcher.start();
      });
    });
  });
</script>
