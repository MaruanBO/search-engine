        <script type="text/javascript">
            jQuery(document).ready(function($){
                
                    $maxHeight = 0;
                    $('#centerpiece-685412 li.grid_8').each(function() {
                        $maxHeight = $maxHeight > $(this).height() ? $maxHeight : $(this).height();
                    });
                    $('#centerpiece-685412 li.grid_8').each(function() {
                        $mHeight = $maxHeight - $(this).height();
                        $(this).css('height', $(this).height());
                        if( $mHeight > 0 ) $(this).css('margin-bottom',$mHeight);
                    });
                    $('#centerpiece-685412 .viewport').css('height', $maxHeight);
                
                
                $('#centerpiece-685412').tinycarousel({
                    pager: true,
                    
                    interval: true,
                    intervaltime: 5000,
                    
                    axis: 'y',
                    duration: 500,
                    
                    callback: function(element, index){
                        
                        $('#centerpiece-685412 .pager li').removeClass('active');
                        $('#centerpiece-685412 .pager li').eq(index).addClass('active');
                        
                    }
                });
                
                  
                
                //$('#centerpiece-685412 .image img').css('width', $('#centerpiece-685412').width());
                
                
                    $('#centerpiece-685412 .slider-display').each(function(){
                        $sImg = $(this).parent().find('.image');
                        if($sImg.length > 0 ) {
                            $(this).css('bottom',0).css('background-color','rgba(0, 0, 0, 0.5)').css('width', ($(this).parent().width()-30));
                            $(this).parent().parent().show().css('height',$sImg.height());
                            $aHeight = $(this).parent().height();
                            $hHeight = ($(this).find('h1').height()+8);
                            $(this).attr('target',$(this).height());
                            $(this).attr('rel',$hHeight);
                            $(this).height($hHeight).css('overflow','hidden');
                        }
                    });
                                    
                    $('#centerpiece-685412 .slider-display').hover(
                        function (){
                            $(this).animate({height: $(this).attr('target')}, {queue:false,duration:300}, 'linear');
                        }, 
                        function () {
                            $(this).animate({height: $(this).attr('rel')}, {queue:false,duration:300}, 'linear');
                        }
                    );
                
            });
        </script>
