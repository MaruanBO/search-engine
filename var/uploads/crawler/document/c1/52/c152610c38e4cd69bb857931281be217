<script type="text/javascript">
function showValueLegend(newValue)
{
        if(newValue == "1"){
	  document.getElementById("rangeLegend").innerHTML="Good";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(0, 228, 0)";
        }else if(newValue == "2"){
	  document.getElementById("rangeLegend").innerHTML="Moderate";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(255, 255, 0)";
        }else if(newValue == "3"){
	  document.getElementById("rangeLegend").innerHTML="Unhealthy for Sensitive Groups";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(255, 126, 0)";
        }else if(newValue == "4"){
	  document.getElementById("rangeLegend").innerHTML="Unhealthy";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(255, 0, 0)";
        }else if(newValue == "5"){
	  document.getElementById("rangeLegend").innerHTML="Very Unhealthy";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(153, 0, 76)";
        }else{
	  document.getElementById("rangeLegend").innerHTML="Hazardous";
//          document.getElementById("sliderLegend").style.backgroundColor = "rgb(126, 0, 35)";
        }

}
</script>
