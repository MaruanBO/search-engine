function auto_rotate_items(dom_selector) {
var auto_rotate_list_dom = jQuery(dom_selector);
var auto_rotate_list_items = auto_rotate_list_dom.size() - 1;
var auto_rotate_list_current = 0;
 
auto_rotate_list_dom.eq(auto_rotate_list_current).show();
 
var i = setInterval(function() {
auto_rotate_list_current++;
if(auto_rotate_list_current === 1){
}else{
  auto_rotate_list_dom.eq(auto_rotate_list_current - 1).hide();
}
if(auto_rotate_list_current === 0){
auto_rotate_list_current++;
}
auto_rotate_list_dom.eq(auto_rotate_list_current).show();
if(auto_rotate_list_current === auto_rotate_list_items) {
auto_rotate_list_current = -1;
}
}, 4000);
