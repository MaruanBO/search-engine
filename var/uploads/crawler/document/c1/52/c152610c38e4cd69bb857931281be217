<script>
function ga_track( id, url, type, path, title, byline )
{
    var url = url.replace(/^.*\/\/[^\/]+/, '');
    dataLayer.push({
        'event': 'VirtualPageview',
        'virtualPageURL': url,
        'virtualPageTitle': title
    });
}
function omniture_track( id, url, type, path, title, byline ) {
    ga_track( id, url, type, path, title, byline );
}
</script>
