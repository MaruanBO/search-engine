function onLinearAdStart(ad) {
  comscoreTracking("adstart");
}

function comscoreTracking(trackingType) {
  var comscoreTrackingUrl = "http://b.scorecardresearch.com/p?c1=1&c2=7922264&c5=02";
  if (trackingType == "adstart") {
     comscoreTrackingUrl = "http://b.scorecardresearch.com/p?c1=1&c2=7922264&c5=01";
  }
  jQuery('.comscore-tracking').append('<img src="' + comscoreTrackingUrl +'" />');
}
