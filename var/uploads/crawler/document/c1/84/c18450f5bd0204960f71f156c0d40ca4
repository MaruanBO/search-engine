		$(document).on("mouseenter", ".catnav", function() {
			var ids = jQuery(this).attr("id").split('_');
			var url = "http://www.rodales.com/on/demandware.store/Sites-rodales-Site/en_US/Home-RenderSlot";
			url = app.util.appendParamToURL(url,'category',ids[2]);
			jQuery.ajax({
				url: url,
				cache: true,
				success: function(data){
					jQuery('#navslot'+ids[0]).html(data);
					// jQuery('#navslot'+ids[0]+' .menu-section-title').text('New in '+ids[3].charAt(0).toUpperCase() + ids[3].slice(1))
				}
			});
		});
		
		// product-set color refinements: width calculation 
		jQuery('.Color').each(function() {
			if( jQuery(this).children().size() > 3 ) {
				jQuery(this).parents('.attribute').width('100%');
			}
		});
		
		// hide level3 wrapping div if there is no items inside 
		var cats = jQuery('.sub-categories');
		if( jQuery(cats).find('li').length === 0 ) {
			jQuery(cats).hide();
		}
		
	});	
//-->
</script>

			
		</div><!-- END: categorymenu -->
	
		
		
	
	


			</nav>
