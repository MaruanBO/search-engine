<script>
function returnNVP(nvp) {
   var val = "";
   queryString = document.location.search;
   if (queryString.indexOf(nvp+'=') != -1) {
       startIndex = queryString.indexOf(nvp+'=') + (nvp.length + 1);
       endIndex = queryString.indexOf('&',startIndex);
       if (endIndex == -1) endIndex = queryString.length;
       val = queryString.substring(startIndex,endIndex);
   }
   return val;
}

