function checkBandwidth() {
   var tEndMs = new Date().getTime();
   var tDownloadMs = tEndMs - tStartMs;
   var fBytesPerSec = (iFileSize * 1000) / tDownloadMs;
   if (fBytesPerSec < fHighBandwidth) valSetI = 0;
   else valSetI = 1;
   updateCookiePC('I',valSetI);
}
if ((typeof doBndwdthDtct) != "undefined" && (doBndwdthDtct == 1)) {
	var iFileSize = 9500;
	var fHighBandwidth = 12800;   // 100K bits/sec
	var tStartMs;
	var randomNum=Math.round(Math.random() * 100000000000);
	tStartMs = new Date().getTime();
	document.write('<img width=0 height=0 src="http://imgfarm.com/images/ads/excite_shop_300x170/rocher_300-X-170.gif?' + randomNum + '" onload="checkBandwidth();">');
}
