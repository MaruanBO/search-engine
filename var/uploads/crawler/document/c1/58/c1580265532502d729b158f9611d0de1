<script>
function toggleSlide(direction) {
    var elements = document.getElementsByTagName("li");
    var visibleID = getVisible(elements);
    removeClass(elements[visibleID], 'show');
    if (!direction) {
        var makeVisible = prev(visibleID, elements.length);
    } else {
        var makeVisible = next(visibleID, elements.length);
    }
    addClass(elements[makeVisible], 'show');
}
function getVisible(elements) {
    var visibleID = -1;
    for (var i = 0; i < elements.length; i++) {
        if(hasClass(elements[i], 'show') ) {
            visibleID = i;
        }
    }
    return visibleID;
}
function prev(num, arrayLength) {
    if (num == 0) return arrayLength - 1;
    else return num - 1;
}
function next(num, arrayLength) {
    if (num == arrayLength - 1) return 0;
    else return num + 1;
}
function hasClass(ele,cls) {
    return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)'));
}
function addClass(ele,cls) {
    if (!this.hasClass(ele,cls)) ele.className += " "+cls;
}
function removeClass(ele,cls) {
    if (hasClass(ele,cls)) {
        var reg = new RegExp('(\\s|^)'+cls+'(\\s|$)');
        ele.className=ele.className.replace(reg,' ');
    }
}
</script>
