		<script type="text/javascript">
		window.onscroll = scroll;
		 
		function scroll () {
			if( typeof( window.pageYOffset ) == 'number' ) {
				//Netscape compliant
				PageScrollTop = window.pageYOffset;
			} else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
				//DOM compliant
				PageScrollTop = document.body.scrollTop;
			} else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
				//IE6 standards compliant mode
				PageScrollTop = document.documentElement.scrollTop;
			}
			if (PageScrollTop >= 196) {
				document.getElementById('SearchBox').style.position = 'fixed';
				document.getElementById('SearchBox').style.marginTop = '0';
				document.getElementById('SearchBox').style.marginBottom = '0';
				document.getElementById('SearchBox').style.backgroundColor = '#FFCC66';
				document.getElementById('SearchBox').style.top = '0';
			} else {
				document.getElementById('SearchBox').style.position = 'relative';
				document.getElementById('SearchBox').style.marginTop = '0px';
				document.getElementById('SearchBox').style.marginBottom = '0px';
				document.getElementById('SearchBox').style.backgroundColor = '#FFCC66';
			}
		}
		</script>
