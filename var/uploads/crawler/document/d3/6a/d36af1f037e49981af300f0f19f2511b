<script type="text/javascript">    
	jQuery(document).ready(function($){		
	
		//FancyBox That Jawn<
		$(".fancybox").fancybox({
			'padding' : 0,
			'autoDimensions': true,
			//'autoHeight': true,
			//'fitToView' : false,
			'wrapCSS' : 'margin-box',
			'width' :  '100%',
			'scrolling' : 'no',
			helpers: {
				overlay: {
				  locked: false
				}
			  }
		});
		
		$('.fancybox-media').fancybox({
			'padding' : 0,
			'autoDimensions': true,
			//'autoHeight': true,
			//'fitToView' : false,
			'wrapCSS' : 'margin-box',
			helpers : {
				media : {}
			}
		});
		
		$('#menu-item-71 a').attr('href','javascript:void(0);');	
		
		$('#menu-item-71 a, .chat_with_us').click(function(){
			olark('api.box.expand');
		});	
		
		
		// Fancy SelectBoxs
		$('select').selectBox();
		
		// Ajax Read More
		$(document).on('click', '.ajax-holder .ajax-more a', function(event)
		{
			var $holder = $(event.currentTarget).closest('.ajax-holder');
			var targetId = $holder.data('ajax-target');
			var items = $holder.data('ajax-item') || '.ajax-item';
			var $target = $holder.find(targetId);
			var $more = $holder.find('.ajax-more');
	
			$holder.block();
	
			$.get($(this).attr('href'), function(data)
			{
				var $doc = $('<body></body>').html(data);
				window.data = data;
				window.doc = $doc;
	
				var $newTarget = $doc.find(targetId);
				var $newHolder = $newTarget.closest('.ajax-holder');
				var $newMore = $newHolder.find('.ajax-more');
	
				$more.replaceWith($newMore);
	
				$newTarget.find(items).appendTo($target);
	
				$holder.unblock();
			});
	
	
			return false;
		});
	
	
		// override these in your code to change the default behavior and style
		$.blockUI.defaults.message = '<div id="infscr-loading"><img src="http://www.snipsnap.it/wp-content/themes/SnipSnap/images/loading.gif"/><em>loading...';
		$.blockUI.defaults.css.margin = '[-10, 0, 0, -10]';
		$.blockUI.defaults.css.width = '200';
		$.blockUI.defaults.css.top = '50%';
		$.blockUI.defaults.css.left = '50%';
		$.blockUI.defaults.css.border = '0';
		$.blockUI.defaults.css.backgroundColor = 'transparent';
		$.blockUI.defaults.overlayCSS.backgroundColor = '#fff';
		$.blockUI.defaults.overlayCSS.opacity = '0';
		
	});
</script>

	<script type="text/javascript" src="http://www.snipsnap.it/wp-content/themes/SnipSnap/js/vendors.js"></script>
	<script type="text/javascript" src="http://www.snipsnap.it/wp-content/themes/SnipSnap/js/jquery-scrollspy.js"></script>
    <!--[if lt IE 9]>
    <script src="http://www.snipsnap.it/wp-content/themes/SnipSnap/js/ie-support.js"></script>
    <![endif]-->
    <script type="text/javascript" src="http://www.snipsnap.it/wp-content/themes/SnipSnap/js/parallaxScroller.js"></script>
    <script type="text/javascript" src="http://www.snipsnap.it/wp-content/themes/SnipSnap/js/snipsnap-that-yo.js"></script> 
    
    
    
    <script type="text/javascript">    
        jQuery(document).ready(function($){		
			
			$('#menu-item-31 a').attr('href','javascript:void(0);');	
			
			
			$('#menu-item-31').click(function(){
				$("html, body").animate({ scrollTop: $('#main-block').offset().top }, 1000);
				$(this).addClass('current-menu-item');
			});
			
			$('#Learn_more_SS').click(function(){
				var offset = $('#main-block').offset().top;
				$("html, body").animate({ scrollTop: (offset) }, 1000);
			});
			
			
			function doScroll($ele) {
				var top = parseInt($ele.css("top"));
				if(top < -92) {
					top = 770;
					$ele.css("top", top);
				}
				$ele.animate({ top: (parseInt(top)-110) },1200,'linear', function() {doScroll($(this))});
			}
			
			function doScrollMobile($ele) {
				var top = parseInt($ele.css("top"));
				if(top < -39) {
					top = 357;
					$ele.css("top", top);
				}
				$ele.animate({ top: (parseInt(top)-51) },1200,'linear', function() {doScrollMobile($(this))});
			}

            function getWindowHeight()
			{
				// Get Window Size
				var window_height = "innerHeight" in window
						? window.innerHeight
						: document.documentElement.offsetHeight;
			
				return window_height;
			}	
                        
            
            $( window ).resize(function() {  	
                if (matchMedia('only screen and (min-width: 768px)').matches) {	
				
						var window_height = getWindowHeight();
                        var snipsnap_nav_position = ( window_height - 63 );
                        // Fix navbar after height
                        $('#snipsnap-nav').scrollspy({
                            min: snipsnap_nav_position,
                            max: 0,
                            container: document,
                            onEnter: function(element, position) {
                                $("#snipsnap-nav").addClass('navbar-fixed-top');
                                $("#snipsnap-nav").removeClass('navbar-absolute');
                            },
                            onLeave: function(element, position) {
                                $("#snipsnap-nav").removeClass('navbar-fixed-top');
                                $("#snipsnap-nav").addClass('navbar-absolute');	
                            }
                        });
                        
                        /*
                        // Marquee's
                        var i = 0;
                        $("ul#mholder-1 li").each(function() {
                              var $this = $(this);
                              $this.css("top", i);
                              i += 110;
                              doScroll($this);
                        });
                        
                        var i = 0;
                        $("ul#mholder-2 li").each(function() {
                              var $this = $(this);
                              $this.css("top", i);
                              i += 110;
                              doScroll($this);
                        });
                        
                        
                        var i = 0;
                        $("ul#mholder-3 li").each(function() {
                              var $this = $(this);
                              $this.css("top", i);
                              i += 110;
                              doScroll($this);
                        });
                        
                        
                        var i = 0;
                        $("ul#mholder-4 li").each(function() {
                              var $this = $(this);
                              $this.css("top", i);
                              i += 110;
                              doScroll($this);
                        });
                        */
                        
                        // Testimonials Scrollable
                        $(".scrollable").scrollable({size:2, circular: true, touch: false}).navigator().autoscroll({ 
                            autoplay: true,
                            interval: 8000
                        });
                    
                }
                else {
                    $("#snipsnap-nav").removeClass('navbar-absolute');	
                    $("#snipsnap-nav").addClass('navbar-fixed-top');	
                    
					/*
                    // Marquee's
                    var i = 0;
                    $("ul#mholder-1 li").each(function() {
                          var $this = $(this);
                          $this.css("top", i);
                          i += 51;
                          doScrollMobile($this);
                    });
                    
                    var i = 0;
                    $("ul#mholder-2 li").each(function() {
                          var $this = $(this);
                          $this.css("top", i);
                          i += 51;
                          doScrollMobile($this);
                    });
                    
                    
                    var i = 0;
                    $("ul#mholder-3 li").each(function() {
                          var $this = $(this);
                          $this.css("top", i);
                          i += 51;
                          doScrollMobile($this);
                    });
                    
                    
                    var i = 0;
                    $("ul#mholder-4 li").each(function() {
                          var $this = $(this);
                          $this.css("top", i);
                          i += 51;
                          doScrollMobile($this);
                    });
					*/
                    
                    
                    // Testimonials Scrollable
                    $(".scrollable").scrollable({size:1, circular: true, touch: false}).navigator().autoscroll({ 
                        autoplay: true,
                        interval: 8000
                    });
                                
                }
            }).resize();
            
        });
    </script>



