        <script type="text/javascript">
            // Sample code only, can load the application at any point!!!!!
            function listen(el, evt, handler) {
                if ( typeof( el.addEventListener ) == 'function' ) {
                    el.addEventListener( evt, handler );
                }
                else {
                        el.attachEvent( 'on' + evt, handler );
                    }
            }

            listen( window, 'load', function() {
                loadEmbeddedQuoteBox( function ( api ) {
                                          api.embedQuoteBox();
                                      },
                                      {flowType: 'home',
                                          mc2:'e'} );
                try {
                    hackNavBar();
                }
                catch(e) {}
            } );
            function hackNavBar() {

                var moreDropDown = document.getElementById( 'menu-trigger' );
                if ( moreDropDown ) {
                    listen( moreDropDown, 'click', toggleMore );
                }
                var navBar = document.getElementById('navbar-toggle');
                if ( navBar ) {
                    listen( navBar, 'click', toggleNavbar );
                }
            }

            function toggleNavbar(evt) {
                var target = evt.target||evt.srcElement||evt.source;
                var navbar = document.getElementById( target.getAttribute('data-target' ).substring(1) );
                if ( target.className.indexOf( 'collapsed' ) > -1 || navbar.offsetWidth == 0 ) {
                    target.className = 'navbar-toggle';
                    navbar.className = 'navbar-collapse collapse in';
                    navbar.style.display = '';
                }
                else {
                    target.className = 'navbar-toggle collapsed';
                    navbar.className = 'navbar-collapse collapse';
                }
            }
            function toggleMore(evt) {
                var target = evt.target||evt.source;
                if ( target.className != 'dropdown' ) {
                    target = target.parentNode;
                }
                if ( target ) {
                    var className = target.className;
                    if ( className.indexOf(' open') > -1 ) {
                        className = className.replace(' open','')
                    }
                    else {
                        className += ' open';
                    }
                    target.className = className;
                }

            }
        </script>
