			var featureTO;
		
			function switchFeature(selName, clicked){
				var nameAry = new Array();
				var i = 0;
				var selId = 0;
		
				for (var name in features){
					if (name == selName)
						selId = i;
					nameAry[i] = name;
					i++;
		
					document.getElementById(name + 'Feature').style.display = 'none';
					document.getElementById(name + 'Feature').style.visibility = 'hidden';
				}
		
				selId = (selId == nameAry.length - 1) ? 0 : selId + 1;
		
				if (!clicked)
					featureTO = setTimeout("switchFeature('" + nameAry[selId] + "', false)", 9000);
				else
					clearTimeout(featureTO);
		
				document.getElementById(selName + 'Feature').style.display = 'block';
				document.getElementById(selName + 'Feature').style.visibility = 'visible';
			}	Barchart.Page.Init = function() {
		if (Barchart.Data.DataMaster) Barchart.Data.DataMaster.addSymbols(com.barchart.QuoteTableListener('1'), ['ES*1','YM*1','NQ*1','RJ*0','DXY00'], ['ES*1','YM*1','NQ*1','RJ*0','DXY00']);
		if (Barchart.Data.DataMaster) Barchart.Data.DataMaster.addSymbols(com.barchart.QuoteTableListener('2'), ['GCJ5','SIH5','CLJ5'], ['GCJ5','SIH5','CLJ5']);
		if (Barchart.Data.DataMaster) Barchart.Data.DataMaster.addSymbols(com.barchart.QuoteTableListener('3'), ['OVAS','ESPR','RDUS','TTPH'], ['OVAS','ESPR','RDUS','TTPH']);
	}
</script>

