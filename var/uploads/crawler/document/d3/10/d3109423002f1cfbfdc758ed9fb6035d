<script type="text/javascript">
function parseSearchQueryString(A) {
  A=A.replace(/,/g,"");
  A=A.replace(/\s/g,"-");
  A=A.replace(/[^a-zA-Z0-9\-]/g,"");
  A=A.toLowerCase();
  return A;
}

function buildSNSearchUrl() {
  var A=jQuery("#snSearchText");
  var C=parseSearchQueryString(jQuery.trim(A.val()));
  if(C.length==0) { return ""; }
  var B="http://www.divinecaroline.com/shop/"+C+"-s.html";
  return B;
}

jQuery("#snSearchText").focus(function() {
  if(jQuery.trim(jQuery(this).val()) == jQuery(this).attr("placeholder") || jQuery.trim(jQuery(this).val()) == "") {
    jQuery(this).val(" ");
  }
});

jQuery("#snSearchSubmit").click(function() {
  window.location=buildSNSearchUrl();
  return false;
});
jQuery("#snSearchText").blur(function() {
  if(jQuery.trim(jQuery(this).val())=="") {
    jQuery(this).val(jQuery(this).attr("placeholder"));
  }
});
</script>  </div>
