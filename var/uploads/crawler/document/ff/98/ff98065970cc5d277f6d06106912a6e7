    <script>
      jQuery(document).ready(function($){

        //AJAX
        var ajaxRequest = {
          wrapper : $('.agora-properties .inner-wrapper .row'),
          maxContentsLength : 300,
          init : function () {
            //before initial call
          },
          complete : function () {
            //when everything is build and finished
          },
          get : function (ajaxurl, ajaxtype, callback) {  
            if (ajaxtype === 'json') {
              $.getJSON(ajaxurl)
              .done(function (data) {
                var newdata = ajaxRequest.assembleDataJson(data);
                if (callback) callback(newdata);
              })
              .fail(function () {
                //console.log(ajaxurl+' isnt loading');
              });
            }
            else if (ajaxtype === 'xml') {
              $.get(ajaxurl, function(data){  
                var newdata = ajaxRequest.assembleDataXML(data);
                //console.log(newdata);
                if (callback) callback(newdata);
              });
            }

          },
          assembleDataJson : function (data) {
            //taking the ajax data and splicing it
            var author = new Object(), post = new Object(), finalData = new Object();
            
            author = data[0].author;
            post.contents = data[0].content;

            //shorten the contents
            post.contents = ajaxRequest.shortenPostContents(post.contents);

            post.date = data[0].date;
            post.links = data[0].link;
            post.title = data[0].title;

            finalData.author = author;
            finalData.post = post;

            return finalData;

          },
          assembleDataXML : function (data) {
            var author = new Object(), post = new Object(), finalData = new Object();
            var currentItem = $(data).find('item')[0];

            post.contents = $(currentItem).filterNode('content:encoded');
            post.contents = post.contents.text();

            post.contents = ajaxRequest.shortenPostContents(post.contents);

            post.title = $(currentItem).find('title').text();
            post.links = $(currentItem).find('link').text();
            post.date = $(currentItem).find('pubDate').text();
            
            author.name = $(currentItem).filterNode('dc:creator').text();

            finalData.author = author;
            finalData.post = post;

            return finalData;
            
          },
          shortenPostContents : function (postContents) {
            postContents = postContents.replace(/<img[^>]*>/g,"");
            postContents = postContents.replace(/<p[^>]*>/g, '');
            postContents = postContents.replace('</p>', '');
            postContents = postContents.substr(0, ajaxRequest.maxContentsLength);
            postContents = postContents.substr(0, Math.min(postContents.length, postContents.lastIndexOf(" ")));

            postContents += '...';
            postContents = '<p>'+postContents+'</p>';

            return postContents;  
          }, 
          buildHolder : function (targetdiv, rawData, callback) {
            var finalData = rawData;
            var html = '<div class="large-4 columns">';
                html += '<div class="article-excerpt '+targetdiv+'">';
                  html += '<h2><a target="_blank" href="'+finalData.post.links+'">'+finalData.post.title+'</a></h2>';
                  html += '<div class="meta">'
                    html += '<ul class="meta-list">';
                      html += '<li class="meta-author">'+finalData.author.name+'</li>';
                    html += '</ul>';  
                  html += '</div>';   
                  html += '<div class="description">';
                  html += finalData.post.contents;
                  html += '<a class="button small radius" href="'+finalData.post.links+'">Read More</a>';
                  html += '</div>';
                html += '</div>';
              html += '</div>';

              this.wrapper.append(html);
            
            if (callback) callback();
          },
        };

        //build json/xml requests
        var requests = {
          one :  {
            targetdiv : 'laissez-faire-today',
            ajaxurl : 'http://lfb.org/feed',
            type : 'xml'
          }, 
          two : {
            targetdiv : 'the-daily-reckoning',
            ajaxurl : 'http://dailyreckoning.com/wp-json/posts',
            type : 'json'
          },
          three : {
            targetdiv : 'daily-resource-hunter',
            ajaxurl : 'http://dailyresourcehunter.com/wp-json/posts',
            type : 'json'
          }
        }

        //loopthrough and make requests
        $.each (requests, function (k, v) {
          ajaxRequest.init();
          ajaxRequest.get(v.ajaxurl, v.type, function (data) {
            var rawData = data;
            ajaxRequest.buildHolder(v.targetdiv, rawData, function () { 
                ajaxRequest.complete();
            });
          });
        });

        //need those crazy psuedos
        $.fn.filterNode = function(name) {
            return this.find('*').filter(function() {
              return this.nodeName === name;
            });
          };
      });

    </script>

