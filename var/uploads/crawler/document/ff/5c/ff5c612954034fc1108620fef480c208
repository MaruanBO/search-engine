function popSearch(s) {
   $("#what").val(s);
 }
function submit_form() {
var s = "";
var what = $.trim($("#what").val());
if (what=="") {
$("#what").addClass("error");
s += "Please enter a job title, keyword or company name.\n";
} else {
$("#what").removeClass("error");
}

/*
var loco = $.trim($("#loco").val());
 if (!loco.match(/^\d{5}$/)) {
$("#loco").addClass("error");
 s += "Please enter a 5 digit zip code.\n";
} else {
$("#loco").removeClass("error");
}
*/

 if (s != "") {
   alert(s);
   return false;
 }

 loadcoupon_notrack();
 $("#job_form").submit();

}

function loadcoupon_notrack() {
//popup window 

  //mindspark
  var url="http://www.bettercareersearch.com/index.jhtml?partner=Z4xdm192&sub_id=0";
  //var url="http://www.bettercareersearch.com/index.jhtml";

  var windowname = "newwin";
  var height = 630;
  var width = 930;

  leftVal = (screen.width - width)/2;
  topVal = (screen.height - height)/2;
  options = "left=" + leftVal + ",top=" + topVal + ",height=" + height + ",width=" + width;

  var mywin = window.open(url,"newwin",options,false);
  mywin.focus();

  return true;
}


