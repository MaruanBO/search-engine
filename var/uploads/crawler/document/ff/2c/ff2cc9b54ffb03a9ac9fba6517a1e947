<pre class='p-large'><span class="n">Index</span> <span class="n">myIndex</span> <span class="o">=</span> <span class="n">apiClient</span><span class="o">.</span><span class="na">initIndex</span><span class="o">(</span><span class="s">&quot;contacts&quot;</span><span class="o">);</span>&#x000A;<span class="n">myIndex</span><span class="o">.</span><span class="na">addObject</span><span class="o">(</span><span class="k">new</span> <span class="n">JSONObject</span><span class="o">()</span>&#x000A;    <span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">&quot;firstname&quot;</span><span class="o">,</span> <span class="s">&quot;Jimmie&quot;</span><span class="o">)</span>&#x000A;    <span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">&quot;lastname&quot;</span><span class="o">,</span> <span class="s">&quot;Barninger&quot;</span><span class="o">)</span>&#x000A;    <span class="o">.</span><span class="na">put</span><span class="o">(</span><span class="s">&quot;company&quot;</span><span class="o">,</span> <span class="s">&quot;California Paint&quot;</span><span class="o">));</span></pre>
