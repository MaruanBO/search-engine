<script>
  $(function() {
  
    var currentPopover = null;
    function homePopover(element, text, placement) {
      hideHomePopover();
      $(element).popover({
        content: text,
        html: true,
        placement: placement
      });
      $(element).popover('show');
      currentPopover = element;
    }
  
    function hideHomePopover() {
      if (currentPopover) {
        $(currentPopover).popover('destroy');
        currentPopover = null;
      }  
    }
  
    function homeAnnotationSearch(q) {
      $('#home-animation-searchinput').val(q)
      $('#home-animation .browser-page-hit').hide();
      $('#home-animation .browser-page-hit[data-query="' + q + '"]').show();
    }
  
    var homeAnnotationState = 0;
    var homeAnimate = true;
    function homeAnnotationLoop() {
      if (!homeAnimate) {
        return;
      }
      var delay = 200;
      ++homeAnnotationState;
      switch (homeAnnotationState) {
        case 1:
          delay = 2000;
          homePopover('#home-animation-searchinput', '<strong>Blazing fast</strong><br/>Search after each keystroke.', 'top');
          homeAnnotationSearch("")
          break;
        case 2:
          delay = 200;
          homeAnnotationSearch("b");
          break;
        case 3:
          homeAnnotationSearch("br");
          break;
        case 4:
          homeAnnotationSearch("bre");
          break;
        case 5:
          homeAnnotationSearch("brea");
          break;
        case 6:
          homeAnnotationSearch("break");
          break;
        case 7:
          delay = 3000;
          homePopover('#home-animation-hits', '<strong>Outstanding ranking</strong><br/>Combine popularity &amp; relevance.', 'right');
          break;
        case 8:
          delay = 200;
          hideHomePopover();
          homeAnnotationSearch("breaki");
          break;
        case 9:
          homeAnnotationSearch("breakin");
          break;
        case 10:
          homeAnnotationSearch("breaking");
          break;
        case 11:
          homeAnnotationSearch("breaking c");
          break;
        case 12:
          homeAnnotationSearch("breaking cr");
          break;
        case 13:
          homeAnnotationSearch("breaking cra");
          break;
        case 14:
          homeAnnotationSearch("breaking craz");
          break;
        case 15:
          homeAnnotationSearch("breaking crazy");
          break;
        case 16:
          homePopover('#home-animation-hits .browser-page-hit:nth-child(40) .subtitle', '<strong>Results from any attributes</strong><br/>"breaking" in TV Show,<br/>"crazy" in episode name.', 'right');
          delay = 3000;
          break;        
        case 17:
          homePopover('#home-animation-hits .browser-page-hit:nth-child(41) .subtitle', '<strong>Typo-tolerance</strong><br/>and <strong>smart</strong> highlight</strong>.', 'right');
          delay = 4000;
          break;        
        default:
          homeAnnotationState = 0;
          delay = 0;
          break;
      }
      setTimeout(homeAnnotationLoop, delay);
    }
  
    function pauseAnimation() {
      homeAnimate = false;
      $("#refresh-on").hide();
      $("#refresh-off").show();
    }
  
    function resumeAnimation() {
      homeAnimate = true;
      $("#refresh-on").show();
      $("#refresh-off").hide();
      homeAnnotationLoop();
    }
  
    var idx = new AlgoliaSearch('latency', '6be0576ff61c053d5f9a3225e2a90f76', { dsn: true, tld: 'net' }).initIndex('tvshows_tae_followers');
  
    function searchAnimation(e) {
      if (e.which == 27) {
        $('#home-animation-searchinput').blur();
        return;
      }
      var q = $('#home-animation-searchinput').val();
      if (q == '') {
        $('#home-animation-hits-live').empty();
        return;
      }
      idx.search(q, function(success, content) {
        if (success && $('#home-animation-searchinput').val() == content.query) {
          var html = '';
          for (var i = 0; i < content.hits.length; ++i) {
            var hit = content.hits[i];
            console.log(hit);
  
            html += '<div class="browser-page-hit">' +
              '<div class="image">' +
                '<img src="' + hit.image + '" alt="TV-show cover" width="34" height="50" />' +
              '</div>' +
              '<div class="nb_followers">' + hit.nb_followers + ' follower' + (hit.nb_followers > 1 ? 's' : '') +'</div>';
  
            if (hit.type === 'actor') {
              html += '<div class="title">' + hit._highlightResult.actor_in.value + '</div>' +
                '<div class="subtitle pull-left">Actor: ' + hit._highlightResult.actor_name.value + '</div>';
            } else if (hit.type === 'show') {
              html += '<div class="title">' + hit._highlightResult.show_name.value + '</div>';
            } else {
              html += '<div class="title">' + hit._highlightResult.episode_of.value + '</div>' +
                '<div class="subtitle pull-left">Episode: ' + hit._highlightResult.episode_name.value + '</div>';
            }
            html += '<div class="clearfix"></div>' +
            '</div>';
          }
          $('#home-animation-hits-live').html(html);  
        }
      }, { hitsPerPage: 3 });
    }
  
    function raiseAnimation() {
      if (!$('#home-animation').hasClass('live')) {
        pauseAnimation();
        hideHomePopover();
        if (typeof(_kmq) !== 'undefined') {
          _kmq.push(['record', 'Clicked Homepage Animation']);
        }
        $("html, body").animate({ scrollTop: 0 }, 600);
        $('#home-animation').addClass('live');
        $('#home-animation-searchinput').val('');
        $('#home-animation-hits').hide();
        $('#home-animation-refresh-buttons').hide();
        $('#home-animation-hits-live').empty().show();
        $('#home-animation-searchinput').
          on('keyup', searchAnimation).
          on('blur', raiseAnimation).
          focus();
      } else {
        $('#home-animation').removeClass('live');
        $('#home-animation-hits').show();
        $('#home-animation-hits-live').hide();
        $('#home-animation-refresh-buttons').show();
        $('#home-animation-searchinput').
          off('keyup', searchAnimation).
          off('blur', raiseAnimation);
        resumeAnimation();
      }
    }
  
    $("#home-animation-searchinput").on('click', raiseAnimation);
  
    $("#refresh-on").on('click', pauseAnimation);
  
    $("#refresh-off").on('click', resumeAnimation);
  
    homeAnnotationLoop();
  });
</script>
<script>
  $(function() {
    var $api_calls_count = $('#api_calls_count');
    var refreshCounter = function() {
      counter.pause();
      $.get('/live/api_calls?' + new Date().getTime(), function(data) {
        var calls = '' + data.api_calls;
        var html = '';
        for (var i = 0; i < calls.length; ++i) {
          if (i > 0 && (calls.length - i) % 3 === 0) {
            html += ' , '
          }
          html += '<span class="number">' + calls[i] + '</span> ';
        }
        $api_calls_count.html(html);
        counter.play();
      }, 'json');
    };
    var counter = $.timer(refreshCounter, 1000, true);
  });
</script>
<script>
  $(function() {
    var $quotes = $('.quote');
  
    $('.trust-us li.with-quote img').on('mouseover', function(e) {
      var company = $(this).parent().data('company');
      $quotes.removeClass('active');
      $('.quote-' + company).addClass('active');
    });
  })
</script>
