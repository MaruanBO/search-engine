<pre class='p-large'><span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">myIndex</span><span class="o">.</span><span class="na">search</span><span class="o">(</span><span class="k">new</span> <span class="n">Query</span><span class="o">(</span><span class="s">&quot;jimmie b&quot;</span><span class="o">)));</span></pre>
