<script type="text/javascript">
/*<![CDATA[*/

var id = "http://aa.gv.facdn.com/ep/flash/special.swf";

	var version = deconcept.SWFObjectUtil.getPlayerVersion();
	if (version["major"]==0){
		document.getElementById('flash_special').innerHTML = "<div id='flash_not_instaled'><br /><br /><p align='center'>You do not have the <a href='http://www.adobe.com/go/getflashplayer' target='_blank' >Flash plugin installed</a>, or your browser does not support Javascript (you should enable it, perhaps?). </p><br /><br /><p align='center'><a href='http://www.adobe.com/go/getflashplayer' target='_blank'><img src='"+"http://aa.gv.facdn.com/ep//images/no_flash.gif"+"' alt='Get Flash Player'/></a></p><br /><br /></div>";	
	}else{
		if (version["major"]>0 && version["major"]<9) { 
