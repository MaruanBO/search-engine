<script type="text/javascript">
require(['jquery', 'jqueryui', 'jq.ajaxForm'], function($){
    var post_url = '/sensiads/%(advert_id)s/email_friend/';
    var $dialog = $("#js-dialog-email-to-friend-modal");

    var clear_form = function(){
        var form = $('#friend_mail_form');
        $('input:visible, textarea:visible', form).val('');
        $('tr, ul', form).removeClass('showError').find('span.inlineError').empty();
        $('#mail_friend_messages').empty();
    }

    var email_dialog = $dialog.dialog({
        height: 630,
        modal: true,
        autoOpen: false,
        zIndex: 2000,
        open: function(event, ui) {
            clear_form();
            var advert_id = '';
            if(!advert_id) {
                advert_id = $dialog.dialog('option', 'advert_id');
            }
            post_url = '/sensiads/%(advert_id)s/email_friend/'.replace('%(advert_id)s', advert_id);
            $('form', $dialog).attr('action', post_url);

            $('form', $dialog).trigger('reload_captcha');
        }
    });

    email_dialog.dialog('close');

    $(document).on("userside_call_refreshed", function(){
        if (window.SENSI_SETTINGS.userside_call &&
            window.SENSI_SETTINGS.userside_call.is_authenticated) {
            $(".js_user_email", $dialog).hide()
                .find("input").val(window.SENSI_SETTINGS.userside_call.user_data.email);
        } else {
            $(".js_user_email", $dialog).show();
        }
    });

    $('#friend_mail_form').ajaxForm({
        success: function(responseText, statusText, xhr, form) {
            if (xhr.responseText == '<success/>'){
                
                    if ($.userside_call !== undefined)
                        $.userside_call.refreshData();
                    email_dialog.dialog('close');
                
            } else {
                data = $('<div>' + xhr.responseText + '</div>');
                var j_data = {};
                var msg = data.not('dt');
                var errors = false;
                data.find('dt').each(function() {
                    var item = $(this);
                    errors=true;
                    j_data[item.text()] = item.next().find('li');
                });
                $('#mail_friend_messages').html(msg)
                form = $('#friend_mail_form');
                form.find('tr, ul').each(function() {
                    var tr = $(this);
                    var tr_find = tr.find(':input:first');
                    var errors = j_data[tr_find.attr('name')];
                    
                        // HACK: field name for recaptcha is
                        // 'recaptcha_field_ident', while its value is what we
                        // actually want
                        if(tr.has('div[name=recaptcha_field]').length > 0) {
                            errors = j_data[tr.find('div[name=recaptcha_field]').data('form-name')];
                        }
                        if (!!tr.has('[name=adcopy_response]').length) {
                                errors = j_data['captcha']
                        }
                    

                    if (errors) {
                        tr.addClass('showError').find('span.inlineError').empty().append(errors);
                    } else {
                        tr.removeClass('showError').find('span.inlineError').empty();
                    }
                });
                form.trigger('reload_captcha');
                $('#friend_mail_form.submit_button').removeAttr('disabled');
            }
        }
    });
    $('#friend_mail_form button').bind('click', function(e) {
        
    });
});
</script>

