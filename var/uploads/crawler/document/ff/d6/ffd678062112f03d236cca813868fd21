<script type="text/javascript">
require([
    'jquery',
    'sensi_render',
    'lib/base64_json_load',
    'text!@template/silverpop_newsletter/dialog.tmpl.html',
    'text!@template/silverpop_newsletter/checkbox.tmpl.html',
    'text!@template/silverpop_newsletter/retype_email.tmpl.html'
    ], function(
        $,
        sensi_render,
        loader,
        dialog_tmpl,
        checkbox_tmpl,
        retype_tmpl) {

        // 

        var email_input = $("#newsletter_subscribe input[name=email]");

        var get_email = function() {
            if (email_input.hasClass('inactive')) {
                return '';
            } else {
                return email_input.val();
            }
        };

        var set_email = function(email) {
            email_input.val(email);
            if (email!='') {
                email_input.removeClass('inactive');
            } else {
                email_input.addClass('inactive');
            }
            email_input.trigger('blur');
        };

        var set_no_email = function() {
            set_email('');
        };

        var set_default_email = function() {
            // if user is logged, set her/his email; otherwise set empty string
            var logged_user_email = window.SENSI_SETTINGS.userside_call.user_data.email;
            if (logged_user_email) {
                set_email(logged_user_email);
            } else {
                set_no_email();
            }
        };

        // 

        var dialog_window = null;
        var ct_newsletter_layer_header = null;
        var ct_newsletter_form_error_message = null;
        var ct_newsletter_form_email_input_container = null;
        var ct_newsletter_form_message = null;
        var ct_newsletter_form_input_container = null;
        var ct_newsletter_form_submit = null;
        var ct_newsletter_form = null;

        var  preapre_dialog_and_then = function(callback){
            if (dialog_window != null){
                callback();
                return
            }
            $.ajax({
                url: '/sp_newsletter/ajax_form/',
                type: 'POST',
                //data: $.extend({step: step, email: email}, custom_data),
                success: function(response) {
                    dialog_window = sensi_render(dialog_tmpl, {
                        user_ip: "5.153.20.131",
                        form: loader(response)
                    });
                    dialog_window.dialog({
                        resizable: false,
                        modal: true,
                        autoOpen: false,
                        zIndex: 2000,
                        beforeClose: function(event, ui) {
                            email_input.trigger('blur');
                        }
                    });
                    ct_newsletter_layer_header = $("#ct_newsletter_layer_header");
                    ct_newsletter_form_error_message = $("#ct_newsletter_form_error_message");
                    ct_newsletter_form_email_input_container = $("#ct_newsletter_form_email_input_container");
                    ct_newsletter_form_message = $("#ct_newsletter_form_message");
                    ct_newsletter_form_input_container = $("#ct_newsletter_form_input_container");
                    ct_newsletter_form_submit = $("#ct_newsletter_form_submit");
                    ct_newsletter_form = $("#ct_newsletter_form");

                    callback();
                },
                error: function() {
                    handle_error('GENERAL_ERROR', null);
                }
            });
        };

        var open_dialog_window = function () {
            dialog_window.dialog('open');
        };
        var close_dialog_window = function () {
            dialog_window.dialog('close');
        };



        $(document).bind('userside_call_refreshed', set_default_email);

        $("#newsletter_subscribe").bind('submit', function(e) {
            sensi_gaq.push(['_trackEvent', 'email', 'subscribe']);
            preapre_dialog_and_then(function(){
                do_ajax_request('GET_LISTS', {category: '1009421'}, choosing_lists_to_subscribe)
            });

            e.preventDefault();
        });

        var do_ajax_request = function(step, custom_data, ok_callback) {
            var email = $.trim(get_email());
            $.ajax({
                url: '/sp_newsletter/subscribe_newsletter/',
                type: 'POST',
                data: $.extend({step: step, email: email}, custom_data),
                success: function(response) {
                    if (response.status == 'OK') {
                        ok_callback(response.data);
                    } else {
                        handle_error(response.status, response.data);
                    }
                },
                error: function() {
                    handle_error('GENERAL_ERROR', null);
                }
            });
        };

        var handle_error = function(status, response_data) {
            if (status == 'UNAVAILABLE') {
                getting_list_info_again({
                    header_text: 'Whoa! Something went wrong!',
                    message_text: 'We are sorry, our email subscription service is temporarily unavailable. Please try again later, or call our customer service department at (866) 640-3900.',
                    submit_text: 'Retry'
                }, false);
                $('.captcha_field', ct_newsletter_form).hide();
            } else if (status == 'INVALID_EMAIL') {
                choosing_lists_to_subscribe(response_data, {
                    header_text: 'Confirm your subscription',
                    error_message: 'Oops! Your email address appears to be invalid. Please correct your email address and then select your lists below.',
                    retype_email: true
                });
                $('.captcha_field', ct_newsletter_form).show();
            } else if (status == 'CAPTCHA_ERROR') {
                choosing_lists_to_subscribe(response_data, {
                    header_text: 'Confirm your subscription',
                    error_message: 'There was an error with captcha validation. Please try again.',
                    retype_captcha: true
                });
                $('.captcha_field', ct_newsletter_form).show();
            } else if (status == 'NO_PUBIDS_GIVEN') {
                choosing_lists_to_subscribe(response_data, {
                    header_text: 'Oops! No email lists were selected.',
                    error_message: 'Please select at least one interest in order to subscribe to our email list, or you can <a id="ct_newsletter_cancel_on_error_link" href="#">click here to cancel.</span>'
                });
                $('.captcha_field', ct_newsletter_form).show();
            } else if (status == 'CANNOT_SUBSCRIBE' || status == 'ILLEGAL_PUBID') {
                choosing_lists_to_subscribe(response_data, {
                    header_text: 'Whoa! Something went wrong!',
                    message_text: 'We are sorry, something went wrong. Please try your subscription request again. If you continue to experience this error, please call our customer service department at (866) 640-3900.',
                    submit_text: 'Retry'
                });
                $('.captcha_field', ct_newsletter_form).hide();
            } else {
                getting_list_info_again({
                    header_text: 'Whoa! Something went wrong!',
                    message_text: 'We are sorry, something went wrong. Please try your subscription request again. If you continue to experience this error, please call our customer service department at (866) 640-3900.',
                    submit_text: 'Retry'
                });
                $('.captcha_field', ct_newsletter_form).hide();
            }
        };

        var getting_list_info_again = function(callback_options) {
            callback_skeleton(callback_options, null, function() {
                do_ajax_request('GET_LISTS', {}, choosing_lists_to_subscribe);
            });
        };

        var choosing_lists_to_subscribe = function(response_data, callback_options) {
            var prepare_list_checkboxes = function() {
                if (response_data && response_data.checkbox_dicts) {
                    // if checkbox dicts are given (containing CTCT list pubids, titles and is-checked flags)
                    // -- we provide a new set of checkboxes (with CTCT list pubids as HTML input names)
                    ct_newsletter_form_input_container.html('');
                    for (var i in response_data.checkbox_dicts) {
                        ct_newsletter_form_input_container.append(sensi_render(checkbox_tmpl, response_data.checkbox_dicts[i]));
                    };
                }
                ct_newsletter_form_input_container.show();
            };
            var use_list_checkboxes = function() {
                // we create an array of pubids of CTCT lists chosen by user
                // (being an array of HTML input names of checkboxes checked by user)
                var pubids_to_subscribe = $('#ct_newsletter_form_input_container input:checkbox:checked').map(function (i, obj) {
                    return obj.name;
                }).toArray();
                if (pubids_to_subscribe.length) {
                    // the array is not empty => user checked some checkboxes (i.e. chose some CTCT lists)
                    var adcopy_response = $('input[name=adcopy_response]', ct_newsletter_form).val();
                    var adcopy_challenge = $('input[name=adcopy_challenge]', ct_newsletter_form).val();
                    var user_ip = $('input[name=user_ip]', ct_newsletter_form).val();
                    do_ajax_request('SUBSCRIBE_LISTS', {pubids_to_subscribe: pubids_to_subscribe, adcopy_response: adcopy_response, adcopy_challenge: adcopy_challenge, user_ip: user_ip, category: '1009421' }, finished_successfully);
                } else {
                    // the array is empty => no checkbox is checked (i.e. no CTCT list was chosen)
                    handle_error('NO_PUBIDS_GIVEN', null);
                }
            };
            callback_skeleton(callback_options, prepare_list_checkboxes, use_list_checkboxes);
            $('.captcha_field', ct_newsletter_form).show();
            sensi_gaq.push(['_trackPageview', '/email/signup/lists/']);
        };

        var finished_successfully = function() {
            callback_options = {
                'header_text': 'Congratulations! You are subscribed!',
                'message_text': 'You are now subscribed to our email list. You will receive your first email soon, confirming your subscription.',
                'submit_text': 'Done'
            };
            callback_skeleton(callback_options, set_no_email, close_dialog_window);
            sensi_gaq.push(['_trackPageview', '/email/signup/success/']);
        };

        var callback_skeleton = function(callback_options, immediate_action, on_submit_action) {
            close_dialog_window();
            prepare_window_content(callback_options);
            if (immediate_action) {
                immediate_action();
            }
            ct_newsletter_form.unbind('submit');
            ct_newsletter_form.bind('submit', function() {
                if (callback_options && callback_options.retype_email) {
                    set_email($('#ct_newsletter_form_email_input').val());
                }
                if (callback_options && callback_options.retype_captcha) {
                }
                if (on_submit_action) {
                    on_submit_action();
                }
                return false;
            });
            ct_newsletter_form.trigger('reload_captcha');
            $('.captcha_field', ct_newsletter_form).hide();
            open_dialog_window();
        };

        var prepare_window_content = function(callback_options) {
            if (callback_options && callback_options.header_text) {
                ct_newsletter_layer_header.html(callback_options.header_text);
            } else {
                // default header text
                ct_newsletter_layer_header.html('Sign up for our email newsletter');
            }

            if (callback_options && callback_options.message_text) {
                ct_newsletter_form_message.html(callback_options.message_text);
            } else {
                // default message text
                ct_newsletter_form_message.html('Which emails would you like us to send? Choose your interests below, and we will email you to confirm your subscription. (We will ONLY send you the emails that you request.)');
            }

            if (callback_options && callback_options.submit_text) {
                ct_newsletter_form_submit.html(callback_options.submit_text);
            } else {
                // default submit text
                ct_newsletter_form_submit.html('Subscribe!');
            }

            // (#ct_newsletter_cancel_on_error_link is contained in the error_message for 'NO_PUBIDS_GIVEN' status -- see: handle_error())
            $("#ct_newsletter_cancel_on_error_link").unbind('click');

            if (callback_options && callback_options.error_message) {
                ct_newsletter_form_error_message.html(callback_options.error_message);
                ct_newsletter_form_error_message.show();
                // (#ct_newsletter_cancel_on_error_link is contained in the error_message for 'NO_PUBIDS_GIVEN' status -- see: handle_error())
                $("#ct_newsletter_cancel_on_error_link").bind('click', function() {
                    dialog_window.dialog('close');
                    return false;
                });
            } else {
                // there is no error text by default
                ct_newsletter_form_error_message.hide();
            }

            if (callback_options && callback_options.retype_email) {
                ct_newsletter_form_email_input_container.html(sensi_render(retype_tmpl, {email: get_email()}));
                ct_newsletter_form_email_input_container.show();
            } else {
                // email input is hidden by default
                ct_newsletter_form_email_input_container.hide();
            }

            // list checkboxes are hidden initially (then they may by shown explicitly
            // -- see: prepare_list_checkboxes() defined in choosing_lists_to_subscribe() above)
            ct_newsletter_form_input_container.hide();
        };
    });
</script>

