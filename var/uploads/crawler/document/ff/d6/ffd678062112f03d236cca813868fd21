<script type="text/javascript">

require(['jquery', 'jqueryui', 'jq.ajaxForm'], function($) {
    $( "#js-dialog-change-location-modal-main_page").dialog({
        height: 440,
        modal: true,
        zIndex: 2000,
        autoOpen: false
    });

    $('#js-dialog-change-location-form-main_page').ajaxForm({
        success: function(responseText, statusText, xhr, form) {
            obj = JSON.parse(responseText);
            if (obj.success) {
                $("#js-dialog-recover-modal-main_page").dialog('close');
                $(document).trigger('password_sent');
            } else {
                $('#remind_form_layer_messages-main_page').html(obj.data)
            }
        }
    });

    $("a[name=change_location_submit]").bind('click', function(){
        $('#search_form').submit();
    });

    $("a[name=change_location]").bind('click', function(){
        $("#near_to").val('USA');
        $("#category").val('1009421');
        $("input#location_path").attr('value', 'w-us');
        $('#search_form').submit();
    });

    var near_to_opts = {
        source: function(request, response) {
            $.getJSON('/xhr/completeregion/',{
                text: request.term
            }, function(data) {
                if(typeof(data)!="undefined")
                {
                    if(!data.err_happend) {
                        suggestions = [];
                        $.each(data.completions, function(i, val) {
                            suggestions.push({label:val[1], value:val[0]});
                        });
                        if(suggestions.length == 1){
                            $('#near_to').val(suggestions[0].label);
                            $('input#location_path').attr('value', suggestions[0].value);
                        }
                        response(suggestions);
                    } else {
                        //_show_error_message(data.err_msg);
                    }
                } else {
                    //_show_error_message('Connection error. Please retry later.');
                }

            });
        },
        select: function(e, ui) {
              input_val = ui.item.label;
              $('#near_to').val(input_val);
              $('input#location_path').attr('value', ui.item.value);
              input_val = ui.item.label;
            if (input_val.length < 1) {
                input_val = 'California';
            }
            if(input_val.length > 20) {
                input_val = input_val.substr(0,20) + '...';
            }
            $("a[name=change_location_submit]").html('Go to <stron>' + input_val + '</strong>');
            $("a[name=change_location_submit]").show();
            e.preventDefault();
        },
        focus: function(e, ui) {
            $("#near_to").val( ui.item.label );
            e.preventDefault()
        },
        appendTo: 'span.js_result'
    };

    if(!!$.fn.autocomplete_backup){
        $('#near_to').autocomplete_backup(near_to_opts);
    } else {
        $('#near_to').autocomplete(near_to_opts);
    }
});
</script>
