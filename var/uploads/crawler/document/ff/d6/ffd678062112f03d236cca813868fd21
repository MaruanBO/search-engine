<script type="text/javascript">
    require(['jquery', 'jqueryui', 'jq.ajaxForm'], function ($) {
        $("#js-dialog-moderation-request").dialog({
            height: 480,
            modal: true,
            autoOpen: false,
            zIndex: 2000,
            open: function(event, ui){
                advert_id = $("#js-dialog-moderation-request").dialog('option', 'advert_id');
                if (isNaN(parseInt(advert_id))){
                    advert_id = "";
                }
                $('#moderation_form').attr('action', '/moderation/request-ajax/sensiad/' + advert_id + '/');
                var user_email = window.SENSI_SETTINGS.userside_call.user_data.email;
                if(user_email) {
                    $('#moderation_form input#id_email').val(user_email);
                }
            },
            close: function(event, ui){
                $('#moderation_form').removeAttr('action');
                $('#moderation_form #id_reason option:first').attr('selected', 'selected');
                $('#moderation_form #id_description').val('');
            }
        });

        $('#moderation_form').bind('submit', function(){
            $('#moderation_form .submit_button').attr('disabled', 'disabled');
        });

        $('#moderation_form').ajaxForm({
            success: function(responseText, statusText, xhr, form) {
                data = $.parseJSON(xhr.responseText);
                if($.isEmptyObject(data.errors)) {
                    $("#js-dialog-moderation-request").find('ul').removeClass("showError");
                    $("#js-dialog-moderation-request").find('span.inlineError').html('');
                    $("#js-dialog-moderation-request").dialog("close");
                    $("#moderation_form .submit_button").removeAttr('disabled');
                    $("#sessionMessages").append('<div class="notice">' + data.message + '<a class="close js_close" href="#"></a></div>');
                    $(document).scrollTop(0);
                } else {
                    $("#js-dialog-moderation-request").find('ul').removeClass("showError");
                    $("#js-dialog-moderation-request").find('span.inlineError').html('');
                    // this first if is ported from devel
                    if (data.errors && data.message) {
                        for (el in data.errors) {
                            $('ul.js_' + el).addClass('showError');
                            $('ul.js_' + el).find('span.inlineError').html(data.errors[el].join(' '));
                        }
                    } else {
                        // this is original code in branch new_django
                        $("#moderation_messages").html('<div class="messageCloud"><div class="error">' + data.message + '</div></div>');
                        form.find('tr, ul').each(function() {
                            var tr = $(this);
                            var errors = data.errors[tr.find(':input:first').attr('name')];
                            if (errors) {
                                for (var i in errors) {
                                    tr.addClass('showError').find('span.inlineError').empty().append(errors[i]);
                                }
                            } else {
                                tr.removeClass('showError').find('span.inlineError').empty();
                            }
                        });
                    }
                    $('#moderation_form .submit_button').removeAttr('disabled') 
                }
            }
        });
    });
</script>

