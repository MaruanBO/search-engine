                        <a target="_blank" title="${title}" href="${site_full_url}">
                          <img src="${site_logo_url}" alt="${title}" />
                        </a>
                        {{if $index + 1 % 3 === 0}} <div class="transformLogosSeparator"></div>{{/if}}
						{{/each}}
                    
                </div>
            </li>
        </ul>
        <div class="layerNotice">
            <p>This email address <b>${user_email}</b> has already been registered. You can use this address and <u>update your profile</u> on this website.</p>
            <p>Alternatively, please register with a different email address.</p>
        </div>
