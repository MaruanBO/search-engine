    <input type="hidden" name="ajax" value="1" />
    <input type="hidden" name="email" value="${user_email}" />
    <input type="hidden" name="password" value="${password}" />
    <div class="layerFormContainer basicForm">
        <ul class="clearfix">
            <li class="label"><label>You are already registered in:</label></li>
            <li class="value">
                <div class="transformLogosContainer">
                    
