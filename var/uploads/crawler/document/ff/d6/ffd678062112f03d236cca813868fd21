<script type="text/javascript">
require(['jquery', 'jqueryui', 'jq.ajaxForm'], function($){
    var dialog_window = $("#js-dialog-set-modal-main_page");
    var dialog_form = dialog_window.find('form');

    var print_errors = function(txt) {
        var data = $('<div>' + txt + '</div>');
        var j_data = {};
        var msg = data.not('dt');
        var errors = false;
        data.find('dt').each(function() {
            var item = $(this);
            errors=true;
            j_data[item.text()] = item.next().find('li');
        });
        dialog_window.find('.js-layerMessages').html(msg);
        dialog_form.find('tr, ul').each(function() {
            var tr=$(this);
            var errors = j_data[ tr.find(':input:first').attr('name')];
            if (errors) {
                tr.addClass('showError').find('span.inlineError').empty().append(errors);
            } else {
                tr.removeClass('showError').find('span.inlineError').empty();
            }
        });
    };

    var remove_errors = function() {
        print_errors('');
    };

    dialog_form.bind('submit', function(e){
        dialog_form.find('.js-layerMessages').attr('disabled', 'disabled');
    });

    dialog_window.find('form').ajaxForm({
        xhrFields: {
            withCredentials: true
        },
        beforeSend: function(jqXHR, settings) {
            jqXHR.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
        },
        success: function(responseText, statusText, xhr, form) {
            var obj = responseText;
            if (obj.success == true) {
                dialog_window.dialog('close');
                $(document).trigger('user_state_changed');
            } else {
                print_errors(obj.errors[0]);
            }
        },
        error: function(jqXHR, textStatus, errorThrown) {
        },
        complete: function(jqXHR, textStatus) {
            dialog_form.find('.submit_button').removeAttr('disabled');
        }
    });

    dialog_window.dialog({
        height: 280,
        modal: true,
        autoOpen: false,
        open: remove_errors,
        zIndex: 2000
    });
});
</script>
