    /** require error handling */
    require._error_handlers = [];
    require.registerErrorHandler = function(callback){
        require._error_handlers.push(callback);
    };
    require.unregisterErrorHandler = function(callback){
        require._error_handlers.splice(require._error_handlers.indexOf(callback), 1);
    };
    require.onError = function(err){
        var throw_error = true;
        for (var i=0; i < require._error_handlers.length; ++i){
            if (!require._error_handlers[i](err))
                throw_error = false;
        }
        if (throw_error)
            throw err;
    };

