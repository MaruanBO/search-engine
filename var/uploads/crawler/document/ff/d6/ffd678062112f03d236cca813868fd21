<script type="text/javascript">
require(['jquery', 'jqueryui', 'jq.layer_manager'], function($){
    var dialog_window = $("#js-dialog-register-modal-main_page");

    $('input[name=business_profile]', dialog_window).live('click', function(e) {
        $('ul.js_p_private,ul.js_p_business').hide();
        var show_class = $(this).val() == '1' ? 'ul.js_p_business' : 'ul.js_p_private';
        $(show_class).show();
        });

    $('input[name=business_profile]:checked', dialog_window).click();

    dialog_window.layer({
        height: 450,
        title: 'Join PennySaverUSA.com',
        success: function() {
            window.location.reload();
        },
        fail: function(e, obj) {
            if (obj.code == 200) {
                dialog_window.layer('close');
                var message = '<div class="notice">%(message)s</div>'.replace('%(message)s', obj.data.message);
                $(document).trigger('open_layer_login', message)
            } else {
                var all_errors = [];
                for (var el in obj.data) {
                    for (var el2 in obj.data[el]) {
                        for (var i in obj.data[el][el2]) {
                            all_errors.push(obj.data[el][el2][i]);
                        }
                    }
                }
                // it's a bit workaround, but we dont report so precisely errors (error code's usually means
                // that form is invalid without going into details)
                if (obj.code == 202 && _.indexOf(all_errors, 'This field is required.') != -1) {
                    sensi_gaq.push(['_trackEvent', 'Error', 'Join', 'This field is required.']);
                }
                if (obj.code == 202 && _.indexOf(all_errors, 'Enter a valid e-mail address.') != -1) {
                    sensi_gaq.push(['_trackEvent', 'Error', 'Join', 'Enter a valid e-mail address.']);
                }
                if (obj.code == 202 && _.indexOf(all_errors, 'The two email fields didn\'t match.') != -1) {
                    sensi_gaq.push(['_trackEvent', 'Error', 'Join', 'The two email fields didn\'t match.']);
                }
                if (obj.code == 202 && _.indexOf(all_errors, 'The two password fields didn\'t match.') != -1) {
                    sensi_gaq.push(['_trackEvent', 'Error', 'Join', 'The two password fields didn\'t match.']);
                }
            }
        }
    });

});
</script>

        

<script type="text/javascript">
require(['jquery', 'jqueryui', 'jq.ajaxForm', 'jq.layer_manager', 'jq.tmpl'], function($){
    var dialog_window = $("#js-dialog-transfer-modal-main_page");
    
    var setup_form = function(dialog_window) {
        var dialog_form = dialog_window.find('form');

        dialog_form.ajaxForm({
            xhrFields: {
                withCredentials: true
            },
            beforeSend: function(jqXHR, settings) {
                jqXHR.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
            },
            beforeSubmit: function(data, form, options) {
                choice_checked = false;
                terms_checked = false;
                for (i in data) {
                    if (data[i].name == 'choice_transfer') {
                        choice_checked = true;
                        dialog_window.find('li.choice_transfer').hide();
                    } else {
                    	dialog_window.find('li.choice_transfer').show();
                    }
                    if (data[i].name == 'terms') {
                    	dialog_window.find('li.terms').hide();
                        terms_checked = true;
                    } else {
                        dialog_window.find('li.terms').show();
                    }
                }
                return choice_checked && terms_checked;
            },
            success: function(response, statusText, xhr, form) {
                var obj = JSON.parse(response);
                if (obj.success === true) {
                        dialog_window.dialog('close');
                        $(document).trigger('user_logged_in', obj.user);
                } else {
                    for (var i in obj.errors) {
                        print_error(i, obj.errors[i]);
                    }
                }
            }
        });
    };

    var dialog_options = {
            height: 350,
            modal: true,
            zIndex: 2000
        };

    open_layer_transfer = function(data) {
        dialog_window.empty();
        $('#js-template-transfer-modal-main_page').tmpl(data).appendTo(dialog_window);
        setup_form(dialog_window);
        $('#js-dialog-transfer-modal-main_page').dialog(dialog_options);
    };
    
    $(document).bind('open_layer_transfer', function(e, data) {
        open_layer_transfer(data);
    });
});
</script>

<span id="js-dialog-transfer-modal-main_page"></span>

<script id="js-template-transfer-modal-main_page" type="text/x-jquery-tmpl">
<h2 class="layerHeader">Already registered, Update your profile</h2>
