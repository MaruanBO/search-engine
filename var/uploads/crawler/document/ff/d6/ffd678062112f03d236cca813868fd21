    <script type="text/javascript">
    require(["jquery", "jqueryui"], function($){
        $("a.js_show_moderation_get_advert_id").live('click', function(e){
            $("#js-dialog-moderation-request").dialog('option', 'advert_id', $(this).data('obj'));
            $("#js-dialog-moderation-request").dialog('open');
            e.preventDefault();
            });
    });
