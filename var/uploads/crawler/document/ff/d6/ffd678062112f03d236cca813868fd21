<script type="text/javascript">
(function(){
var events = {
	//settings_choices.EVENT_*
	1: ['_trackEvent', 'Error', 'Login', 'Email is not registered. Please create an account.'],
	2: ['_trackEvent', 'Error', 'Login', 'Login or password incorrect.'],
	3: ['_trackEvent', 'Error', 'Forgot Password', 'Email & confirmation email does not match.'],
	4: ['_trackEvent', 'Error', 'Forgot Password', 'User does not exist.'],
	5: ['_trackEvent', 'Error', 'Forgot Password', 'Email could not be sent. Sorry for the inconvenience.'],
	6: ['_trackEvent', 'Error', 'Join', 'This field is required.'],
	7: ['_trackEvent', 'Error', 'Join', 'Enter a valid e-mail address.'],
	8: ['_trackEvent', 'Error', 'Join', 'The two email fields didn\'t match.'],
	9: ['_trackEvent', 'Error', 'Join', 'The two password fields didn\'t match.'],
	38: ['_trackEvent', 'Error', 'Expired', 'Sorry, this ad has expired. Please check out a similar ad!'],
	39: ['_trackEvent', 'Error', '404', ''],
	40: ['_trackEvent', 'Error', 'No Results', ''],
	41: ['_trackEvent', 'Error', 'Login', 'Login or password cannot be empty.'],
	42: ['_trackEvent', 'Error', 'Login', 'You are already logged in.'],
	43: ['_trackEvent', 'Error', 'Forgot Password', 'Email or confirmation email is empty.']
};
$(document).bind('system_event', function(e, code, message){
	if (events[code] != undefined){
		var event = events[code];
		if (message) {
			event[3] = message;
		};
		sensi_gaq.push(event);
	}
});
})();
</script>

