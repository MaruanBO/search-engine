var SENSI_PLUGINS = [];

/* Allow pushing commands to many GA accounts. */
var _gaq = _gaq || [];
var sensi_gaq = {
    ga_accounts: {},

    /* Register account */
    set_account: function(name, code) {
        this.ga_accounts[name] = code;
        var cmd = name == 'default' ? '_setAccount' : name + '._setAccount';
        _gaq.push([cmd, code]);
    },

    /* Push command for all accounts */
    push: function(args) {
        if(!args instanceof Array) return;

        var cmd = args[0];
        for(var name in this.ga_accounts) {
            args[0] = name == 'default' ? cmd : name + '.' + cmd;
            _gaq.push(args);
        }
    }
}
sensi_gaq.set_account('default', 'UA-1868970-1');
if(typeof sensi_extra_ga_accounts !== "undefined") {
    for(var name in sensi_extra_ga_accounts) {
        sensi_gaq.set_account(name, sensi_extra_ga_accounts[name]);
    }
}
delete sensi_gaq.set_account; // registering accounts is allowed only above
sensi_gaq.push(['_setDomainName', '.pennysaverusa.com']);

require(['jquery', 'underscore', 'underscore.string'], function($, _, module) {
    if (!_ || !_.str) {
        window._ = _;

        _.str = module;

        // Mix in non-conflict functions to Underscore namespace if you want
        _.mixin(_.str.exports());

        // All functions, include conflict, will be available through _.str object
        _.str.include('Underscore.string', 'string'); // => true
    }

    window.underscore_lib = _; // for namespaces where _ is overridden
});

    var DefferedObjectsFactory = function() {
        var self = this;

        self.objects = {};

        self.getOrCreate = function(name) {
            if (!(name in self.objects)) {
                self.objects[name] = $.Deferred();
            }

            return self.objects[name];
        };

        self.getObjects = function() {
            return self.objects;
        };
    };

    window.SENSI_SETTINGS['deferred_objects'] = new DefferedObjectsFactory();
</script>


