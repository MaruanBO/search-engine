<script type="text/javascript">
require(['jquery', 'jq.jcarousel'], function($) {
    function mpfeaturedgallery_init(carousel)
    {
        // create controls
        var elems_num = $('div#js_featured_gallery ul li').length;
        if(elems_num > 1) {
            for(var i=1; i<=elems_num; i++) {
                $('div#js_featured_gallery .js_control').append('<a href="#">'+ i +'</a>');
            }
        }
        // attach controls
        $('div#js_featured_gallery .js_control a').bind('click', function() {
            carousel.scroll($.jcarousel.intval($(this).text()));
            return false;
        });
        // stop rotation on user input
        carousel.clip.hover(
                function() { carousel.stopAuto(); },
                function() { carousel.startAuto(); }
                );
        $('div#js_featured_gallery .js_control a').hover(
                function() { carousel.stopAuto(); },
                function() { carousel.startAuto(); }
                );
    }

    function mpfeaturedgallery_beforeswitch(carousel, elem, index, action){
        $('div#js_featured_gallery .js_control a').removeClass('current');
        $('div#js_featured_gallery .js_control a:eq('+(index-1)+')').addClass('current');
    }

    $("div#js_featured_gallery").jcarousel({
        scroll: 1,
        visible: 1,
        wrap: 'last',
        auto: 5,
        initCallback: mpfeaturedgallery_init,
        buttonNextHTML: null,
        buttonPrevHTML: null,
        itemFallbackDimension: 450,
        itemVisibleInCallback: {
            onBeforeAnimation: mpfeaturedgallery_beforeswitch
        }
    });
});
</script>


             
                
