                        <input type="hidden" name="region_seo_url" ng-model="region_seo_url" value="[[ region_seo_url ]]" />
                        <input type="hidden" name="category" ng-model="options.search_area.category" value="[[ options.search_area.category ]]" />
                        <input type="hidden" name="category_seo_url" id="category_seo_url"
                               ng-model="category_seo_url" value="[[ category_seo_url ]]" />
                        <input type="hidden" name="advert_type" ng-model="ad_type" value="[[ ad_type ]]" />
                        <input type="hidden" name="has_radius" id="has_radius" ng-model="has_radius" value="[[ has_radius ]]" />
                        <input type="hidden" name="radius" ng-model="current_radius" value="[[ current_radius ]]" />
                        <input type="hidden" name="region_or_area_completer_touched" id="region_or_area_completer_touched" value="unalltered" />
                        <fieldset>
                            <input id="search_query" type="text" name="query" value="[[ options.query ]]" class="searchQuery">
                                <div class="categories_controls" ng-click="show_category_choices = ! show_category_choices">[[current_type.name]]</div>
                                <ul class="categories_choices" ng-style="get_categories_style()">
                                    <li class="header" ng-click="show_category_choices = !show_category_choices">[[current_type.name]]</li>
                                    <li ng-repeat="choice in options.choices"
                                        class="js_advert_type"
                                        ng-class="{'current': choice.current}"
                                        data-type="[[ choice.type ]]"
                                        ng-click="choose(choice)">
                                        <a onclick="return false;" class="js_no_follow" ng-href="[[ choice.href ]]"
                                           title="[[ choice.name ]]">[[ choice.name ]]</a>
                                    </li>
                                </ul>
                            <span class="near_to_search">
                                <input id="search_near_to"
                                       class="searchQuery2 [[ options.search_near_to_class ]]"
                                       name="near_to"
                                       type="text"
                                       value="[[ options.search_near_to ]]"
                                       ng-keydown="search_near_to_keydown($event)"
                                       autocomplete="off" />
                                <div class="radius_controls" ng-click="show_radius_choices = radius_control_enabled && ! show_radius_choices" ng-class="{'isValue': current_radius_name, 'disabled': !radius_control_enabled}">[[ current_radius_name ]]</div>
                                <ul class="radius_choices" ng-style="get_radius_style()">
                                    <li class="header" ng-click="show_radius_choices = ! show_radius_choices">within</li>
                                    <li ng-repeat="(value, name) in options.radius_choices" ng-click="set_radius(value, name)" ng-show="has_radius">[[ name ]]</li>
                                    
                                </ul>
                            </span>
                            <button type="submit" class="searchButton">Search</button>
                        </fieldset>
                    </form>
                </div>
            </div>

