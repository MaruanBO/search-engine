function display_more(id)
{
	document.getElementById('ex' + id).style.display='none';
	document.getElementById('de' + id).style.display='inline';
}
function display_less(id)
{
	document.getElementById('ex' + id).style.display='inline';
	document.getElementById('de' + id).style.display='none';
}
