     <script>

	function validate_SignIn(){
		if( validateEmail('strEmail', 'strEmail-note', 'Please enter valid email.', '')
			&validateControl('strPassword', 'strPassword-note', 'Please enter password.', '')) {
			return true;
		}
		else {
			return false;
		}
	}

</script>
<script type="text/javascript">
$(document).ready(function() {

$("#Save").click(function(ev1) {

var chk=validate_SignIn();

	ev1.preventDefault();
		if(chk==true){
		//var action = $("#form1").attr('action');
		var form_data = {
			strEmail: $("#strEmail").val(),
			
			

			strPassword: $("#strPassword").val(),
			strIP: $("#strIP").val(),
			is_ajax: 1
		};
		
		$.ajax({
			type: "POST",
			url: "http://www.searchquotes.com/sof/chk_login.php",
			data: form_data,
			success: function(response)
			{
			
		
				if(response == 'success'){
					$("#strEmail-note").html("Successfully logged");
					window.location.href = "http://www.searchquotes.com/quotes/";
					}
					
				else{
	
	
	$("#strEmail-note").html("Invalid authentication");
			}
			
			}
		});		
		//return false;
		
		}
		
		
		});
		
		});
		
</script>

