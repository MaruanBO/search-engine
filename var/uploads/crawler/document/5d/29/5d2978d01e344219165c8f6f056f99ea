<script>
document.write(unescape("%3Clink href=\"http://corp.marketmetrix.com/wp-content/themes/mm/statics/css/slide.css\" rel=\"stylesheet\" type=\"text/css\" /%3E"));
jQuery('.slide_show').css('display', 'block');
jQuery('#Stage').css('display', 'none');
jQuery(window).load(function(){
	(function($){
		$('.slides_container, .slide_item').css('width', $(window).width());
		$(window).resize(function(){
			$('.slide_item').css('width', $(window).width());
		});
		$('.slide_wrap').slides({
			play: 7000,
			pause: 7000
			/*start: Math.round(Math.random()*2+1)*/
		});	
		
		$('.ss_world_data li a').hover(function(){
			$(this).find('.slide_down_data').stop(true, true).fadeIn(300)
						.parent().find('.magnifier_icon').fadeOut(300)
						.parents('li').siblings().find('.slide_down_data').fadeOut(300)
						.parent().find('.magnifier_icon').fadeIn(300);
		}, function(){
			$(this).find('.magnifier_icon').stop(true, true).fadeIn(300)
						.parent().find('.slide_down_data').fadeOut(300);
		});
		//$('.ss_1').fadeIn(300);
		if ($('.ss_1').css('display') == 'block'){
			var dfd = $.Deferred();
			dfd.done(function(){
				//if ($('.ss_1').)
				$('.ss_1').fadeIn(300);
			}, function(){
				$('#ss_sub_1 div').fadeIn(600);
			}, function(){
				$('#ss_sub_2 div').fadeIn(1600);
			}, function(){
				$('#ss_sub_3 div').fadeIn(2300);
			})
			dfd.resolve("or");
		}
		else {
			$('.ss_1').hide();
			$('.ss_1 *').show();
			$('.slide_down_data').hide();
		}
	})(jQuery)
})

jQuery('#logo_list_inner a').css('cursor', 'default').attr('href', 'javascript:;');
