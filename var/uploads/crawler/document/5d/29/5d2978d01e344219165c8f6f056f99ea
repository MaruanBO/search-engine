<script>
var lang = "en";
var img_tag = document.getElementsByTagName('img');
var imgSrc = [];
for(var i = 0; i < img_tag.length; i++) {
	if (img_tag[i].getAttribute('src').indexOf('statics/') == 0) {
		imgSrc[i] = img_tag[i].getAttribute('src');
		if (lang != 'en') {
			lang_imgSrc = imgSrc[i].replace('images/', 'images/' + lang +'/');
		}
		else {
			lang_imgSrc = imgSrc[i];
		}
		
		img_tag[i].src = "http://corp.marketmetrix.com/wp-content/themes/mm/" + lang_imgSrc;
//		jQuery(img_tag[i]).error(function(){
//			img_tag[i].src = "http://corp.marketmetrix.com/wp-content/themes/mm/" + imgSrc[i];
//		})
		img_tag[i].onerror = function() {
			if (lang != 'en') {
				this.src = this.src.replace('images/' + lang +'/', 'images/');
			}
			
		}
	}
}
