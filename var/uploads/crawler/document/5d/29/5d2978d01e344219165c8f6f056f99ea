jQuery('.fake_checkbox').bind('click', function(){ 
	if (jQuery(this).hasClass('checked')) {
		jQuery(this).removeClass('checked')
		jQuery('input[name=newsletter]').val('0');
	}
	else {
		jQuery(this).addClass('checked')
		jQuery('input[name=newsletter]').val('1');
	}
});

