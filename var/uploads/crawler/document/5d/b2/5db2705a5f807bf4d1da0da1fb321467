            <script type="text/javascript">
            $(document).ready(function () {
                if ((!isNaN(parseInt(sessionStorage.getItem('userid')))) & (sessionStorage.getItem('userid') != '0')) {
                    sessionStorage.removeItem('userid');
                    openPopup("http://" + DomainName + "/HtmlFiles/SessionExpired.html?r=" + Math.floor(Math.random() * 10001), 500);
                    setTimeout(function () {
                        $(".btnClose").click(function () {
                            SetSessionExpired();
                        });
                    }, 2000);
                }
            });
            function SetSessionExpired() {
                var DomainName = 'http://newspaperarchive.com';
                var URL = DomainName + '/Login/SetSessionExpired';
                $.ajax({
                    cache: false,
                    type: "GET",
                    url: URL,
                    success: function (data) {
                        window.location.href = document.location.href = DomainName + "/login";
                    },
                    error: function (ex) {

                    }
                });
            }
        </script>



