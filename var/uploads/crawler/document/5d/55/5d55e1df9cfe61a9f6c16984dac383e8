        <script type="text/javascript">
            (function(){
                $('.j-featured-magazines-section').imagesLoaded()
                    .progress( function( instance, el ) {
                        var magItem = $(el.img).closest('.j-magazine'),
                            detailsBox = magItem.find('.j-details-box'),
                            coverHeight = magItem.find('.j-cover img').height();

                        magItem.find('.j-hover-overlay').css('line-height', coverHeight + 'px');
                        magItem.find('.j-cover').css('height', coverHeight + 'px');

                        detailsBox
                            .attr('data-collapse-position', (-1 * (detailsBox.outerHeight() + 10)) + 'px') // 10px for arrow
                            .css({
                                bottom: detailsBox.attr('data-collapse-position')
                            });

                        magItem.hover(function(){
                            $(this).find('.j-details-box').css({
                                bottom: '0px'
                            });
                        }, function(){
                            var detailsBox = $(this).find('.j-details-box');

                            detailsBox.css({
                                bottom: detailsBox.attr('data-collapse-position')
                            });
                        });
                    });
            })();
            joomag.removeScriptTag();
        </script>
