        <div class="j-holder" onclick="javascript: window.open('https://support.joomag.com/visitor/index.php?/newDesign/LiveChat/Chat/Request/_sessionID=/_promptType=chat/_proactive=0/_filterDepartmentID=/_randomNumber=zbke7f5pr9lryf7ztbrvdfa5q8pldfcb/_fullName=/_email=/', 'livechatwin', 'toolbar=0,location=0,directories=0,status=1,menubar=0,scrollbars=0,resizable=1,width=600,height=680');">
            <span class="j-icon"></span>
            <span class="j-text" data-online="Live Support" data-offline="Leave message" ></span>
            <span class="j-text-small">
                <span>Live Chat</span><br/>
                <span class="j-status" data-online="(Online)" data-offline="(Offline)"></span>
            </span>
        </div>
    </div>
    <script type="text/javascript">
        (function(e){
            // Setup Main navigation
            {
                var mainNavBar = $('.j-header-nav'),
                    subNavContainer = $('.j-header-sub-nav'),
                    subNavPanes = $('.j-header-sub-nav-pane', subNavContainer),
                    navElms = $(),
                    navArrow = $('.j-nav-arrow', subNavContainer),
                    openedPageName = mainNavBar.attr('data-opened-page'),
                    openedNavItem = mainNavBar.find('a[data-page="'+ openedPageName +'"]'),
                    timeOutId = null,
                    navIsOpened = false,
                    navIsActive = false,
                    navIsHovered = false,
                    calculateArrowPos = function( navItem ){
                        if( !navItem.attr('data-position') ) {
                            var parent = navItem.parent(),
                                leftPos = (parent.position().left + parent.outerWidth()/2 - 6);

                            navItem.attr('data-position', leftPos);
                        }

                        return navItem.attr('data-position') + 'px';
                    };

                subNavPanes.each(function(){
                    var pane = $(this),
                        pageName = pane.attr('data-page'),
                        openedSubPage = pane.find('[data-page="'+ openedPageName + '"]'),
                        navItem = mainNavBar.find('[data-page="'+ pageName +'"]');

                    if( navItem.length == 1 ) {
                        navElms = navElms.add(navItem);
                    }

                    openedSubPage.addClass('active');

                    if(
                        pageName == openedPageName ||
                        openedSubPage.length == 1
                    ) {
                        if( openedNavItem.length == 0 ) {
                            openedNavItem = mainNavBar.find('a[data-page="' +
                                openedSubPage.closest('.j-header-sub-nav-pane').attr('data-page') +
                            '"]');
                        }
                        pane.addClass('opened');
                        subNavContainer.addClass('opened').css('position', 'relative');
                        navArrow.css('left', calculateArrowPos(openedNavItem));
                        navIsOpened = navIsActive = true;
                    }
                });

                openedNavItem.addClass('active');

                navElms.add(subNavContainer).hover(function(e){
                    navIsHovered = true;
                    var target = $(e.target);

                    if( timeOutId ) { clearTimeout(timeOutId) }
                    if( target.is('.j-header-nav a[data-page]') ) {
                        var subNavPane = subNavPanes.filter('[data-page="' + target.attr('data-page') + '"]'),
                            currentlyActivePane = subNavPanes.filter('.active');
                        subNavPanes.css('display', 'none').removeClass('active');
                        if( navIsActive === false && navIsOpened !== true ) {
                            timeOutId = setTimeout(function(){
                                navArrow.css('left', calculateArrowPos(target));
                                subNavPane.slideDown(200, function(){
                                    subNavContainer.addClass('opened');
                                    $(this).addClass('active').trigger('shown');
                                    navIsActive = true;
                                }).trigger('show');
                            }, 100);
                        } else {
                            navArrow.animate({
                                left: calculateArrowPos(target)
                            });
                            currentlyActivePane.trigger('hidden');
                            subNavPane.css('display', 'block').addClass('active').trigger('shown');
                        }
                    }
                }, function(){
                    navIsHovered = false;
                    if( timeOutId ) { clearTimeout(timeOutId) }
                    timeOutId = setTimeout(function(){
                        if( navIsHovered === false ) {
                            if( navIsOpened === false ) {
                                subNavContainer.removeClass('opened');
                                subNavPanes.filter(':not(.opened)').slideUp(200, function(){
                                    $(this).removeClass('active').trigger('hidden');
                                    navIsActive = false;
                                }).trigger('hide');
                            } else {
                                navArrow.animate({
                                    left: calculateArrowPos(openedNavItem)
                                });
                                subNavPanes
                                    .filter(':not(.opened)').css('display', 'none').removeClass('active')
                                    .end()
                                    .filter('.opened').css('display', 'block');
                            }
                        }
                    }, 500);
                });
            }

            // Setup notifications
            
            // Setup newsstand search box
            {
                var $headerSubNav = $('.j-header .j-header-sub-nav'),
                    $newsstandSubNavPane = $headerSubNav.find('.j-header-sub-nav-pane[data-page="newsstand"]'),
                    $navArrow = $headerSubNav.find('.j-nav-arrow'),
                    $searchPanel = $headerSubNav.find('.j-main-search-panel'),
                    $searchBtn = $searchPanel.find('.j-search-btn'),
                    $searchInput = $searchPanel.find('.j-search-input');

                if( $searchPanel.is('.active') ) {
                    $searchPanel.addClass('active');
                    $navArrow.addClass('j-blue');
                }

                $newsstandSubNavPane
                    .on('shown', function(){
                        if( $searchPanel.is('.active') ) {
                            $newsstandSubNavPane.css('overflow', 'visible');
                            $navArrow.addClass('j-blue');
                        }
                    })
                    .on('hidden', function() {
                        $navArrow.removeClass('j-blue');
                    });
                $searchBtn.click(function(e){
                    e.preventDefault();

                    if( $searchPanel.is('.active') ) {
                        $searchPanel.removeClass('active');
                        $navArrow.removeClass('j-blue');
                        $searchInput.val('');
                    } else {
                        $searchPanel.addClass('active');
                        $navArrow.addClass('j-blue');
                        $searchInput.focus();
                        $newsstandSubNavPane.css('overflow', 'visible');
                    }
                });
            }

            // Setup livechat
            {
                var liveChatContainer = $('.j-livechat'),
                    kayakoScriptContainer = liveChatContainer.find('.j-kayako-script-container'),
                    liveChatIndicator,
                    setupLiveChat = function() {
                        var status = liveChatIndicator.data('status'),
                            hoverText = liveChatContainer.find('.j-holder .j-text'),
                            smallTextStatus = liveChatContainer.find('.j-holder .j-text-small .j-status'),
                            $window = $( window ),
                            topPos = 103,
                            currentPos = topPos,
                            updatePosTimer;

                        hoverText.text( hoverText.data(status) );
                        smallTextStatus.text( smallTextStatus.data(status) );
                        liveChatContainer.addClass( status ).animate({
                            top: topPos
                        }, 1000);

                        $window.scroll(function() {
                            if( !updatePosTimer ) {
                                updatePosTimer = setTimeout(function(){
                                    var newPost = $window.scrollTop() + topPos,
                                        m = Math;

                                    updatePosTimer = false;
                                    liveChatContainer.css({
                                        position: 'absolute',
                                        top: currentPos
                                    });
                                    liveChatContainer.stop(true).animate({
                                        top: newPost
                                    }, {
                                        duration: 500,
                                        easing: 'easeOutQuint'
                                    });

                                    currentPos = newPost;
                                }, 300);
                            }
                        });
                    };

                setTimeout(function(){
                    liveChatIndicator = $('#j-livechat-status-indicator');
                    if( liveChatIndicator.length == 1 ) {
                        setupLiveChat();
                    } else {
                        kayakoScriptContainer.find('script[src]').on('load', function(){
                            liveChatIndicator = $('#j-livechat-status-indicator');
                            if( liveChatIndicator.length == 1 ) {
                                setupLiveChat();
                            }
                        });
                    }
                }, 3);
            }
        })();
        joomag.removeScriptTag();
    </script>
</header>
