	$(document).ready(function() {
		
		// Global variables
		vMagColor = '#33ccff';
		vManColor = '#cccc66';
		baseUrl = 'http://vmagazine.com/';
		vmanUrl = 'http://www.vman.com/';
		vmagUrl = 'http://www.vmagazine.com/';
		staticUrl = 'http://0cda887037eb80a4f75b-00d0e5b3b9d1e59019327f4bb7af5e9c.r8.cf1.rackcdn.com/';
		gridUrl = 'http://vmagazine.com/site/grid';
		magType = 'vmag';
		gridInit = false;
		templateColor = '#cccc66';
		homeBg = 'http://1513f43de1df8b08e16c-d8d6213b156ba3a73e333057f5885fa8.r32.cf1.rackcdn.com/1280-x6pkd2kn7xj1sdany31f87nw9p67jbptcy36k4nm.jpg';
		infiniteScrollBufferStart = 160;

		fashionSubNavCount = 4;
		newsSubNavCount = 4;
		articlesSubNavCount = 2;
		videoSubNavCount = 0;
		archiveSubNavCount = 0;
		
		$('#searchGlobal').autocomplete({
			url: baseUrl+'site/api/search', 
			filterResults: false,
			useCache: false,
			sortResults: false
		});

		// Mouse events
		// Set-up
		
		
		setColor(templateColor);
		loadBg(homeBg);		enableHeaderHover(templateColor);
		$.address.change(function(event) { 
			address(event.value);
		});
		
		// Mobile set-up
		enableMobileNav();

		if (magType == 'vman') {
			$('#header-logos-switch span').css({left: '160px'});
		} else if (magType == 'vmag') {
			$('#header-logos-switch span').css({left: '0px'});
		} else {
			$('#header-logos-switch span').css({left: '80px'});
		}
	});

</script>
