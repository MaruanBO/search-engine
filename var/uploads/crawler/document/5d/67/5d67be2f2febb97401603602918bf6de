<script type="text/javascript">
  // add some extra classes to validate some stuff
  jQuery.validator.addClassRules({
    name: {
      required: true,
      minlength: 2
    },
    zip: {
      required: true,
      digits: true,
      minlength: 5,
      maxlength: 5
    }
  });

  // do some stuff on load
  (function ($, Drupal, window, document, undefined) {
    // validate the crap out of our form
    $('#signup-form-form').validate();

    // input "labels"
    $('.fields .field input').each(function() {
      var originalValue = $(this).val(); // store the original value
      $(this).val($(this).attr('data-label')); // set the value to the data-label attribute

      $(this).focus(function() { // when we focus...
        var value = $(this).val();
        if (value === $(this).attr('data-label')) { // if the value is the same as the data-label attribute...
          $(this).val(''); // remove the value
        }
      });

      $(this).focusout(function() { // when we lose focus...
        var value = $(this).val(); // store the current value
        if (value === '') { // if the value is empty...
          $(this).val($(this).attr('data-label')); // set the value to the data-label attribute
        }
      });
    });
    // submit hover
    $('#signup_submit').hover(function() { // when we hover...
      var hoverImage = '/sites/all/themes/food_democracy_now/images/submit-btn-hover.png';
      $(this).attr('src', hoverImage); // update the image src
    }, function() { // when we move away...
      var regularImage = '/sites/all/themes/food_democracy_now/images/submit-btn.png';
      $(this).attr('src', regularImage); // update the image src
    });
  })(jQuery, Drupal, this, this.document);
</script>  
