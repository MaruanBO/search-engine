<div id="node-125866" class="node node-home-page-view view-mode-homepage clearfix" about="/poem-day-0" typeof="sioc:Item foaf:Document">

  
        <h2 class="node__title node-title"><a href="/poem-day-0">poem-a-day</a></h2>
    <span property="dc:title" content="poem-a-day" class="rdf-meta element-hidden"></span><span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span>
    
      <div class="content clearfix ">

      
      <div class="node-content clearfix">
        <div class="field field-name-field-views field-type-viewfield field-label-hidden"><div class="field-items"><div class="field-item even"><div class="view view-poems view-id-poems view-display-id-poems_block_day node-type-poems view-dom-id-6e2b9b2400cb321a1b5893642ab3b697">
        
  
