    if (navigator.userAgent.indexOf('Mozilla/3') != -1 || navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1)
    {
        _version=10;
    }

    function OAS_AD(position)
    {
        var oaspage = OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + position + '?' + OAS_query;
        if (_version < 11)
        {
            document.write ('<a href="' + OAS_url + 'click_nx.ads\/' + oaspage + '" target="' + OAS_target + '"><img src="' + OAS_url + 'adstream_nx.ads/' + oaspage + '" border="0" alt="Ad" \/></a>');
        }
        else
        {
            document.write ('<scr'+'ipt type="text/javascript" src="' + OAS_url + 'adstream_jx.ads\/' + oaspage + '"><\/script>');
        }
    }

    var sitepage = OAS_sitepage;
//-->
</script>
<!-- End OAS header code -->


<script>
function equalHeight(group) {
   tallest = 0;
   group.each(function() {
      thisHeight = $(this).height();
      if(thisHeight > tallest) {
         tallest = thisHeight;
      }
   });
   group.height(tallest);
}
$(document).ready(function() {
   equalHeight($(".eq-size.highlight-white"));

});
</script>

