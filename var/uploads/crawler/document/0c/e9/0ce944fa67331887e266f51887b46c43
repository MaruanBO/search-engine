        function setBackgroundImage(url) {
          var string=url;
          document.getElementById("header_container").style.backgroundImage='url(' + url + ')';
          if (url == "NONE") {
            document.getElementById("header_container").style.display='none';
          }
          else{
            document.getElementById("header_container").style.display='block';
          }
          setCookie('BG', url, 100);    // Or however many days you want.
        }

function setPrinttype(type) {
          
console.log(type);

if (type == "print") {
	$("#newsidebar-wrapper").css("display", "none");
	$("#nav_container").css("display", "none");
	$("#main-wrapper").css("width", "730px");
	$("#content-wrapper").css("width", "740px");
	$("#main-wrapper").css("border-top", "0px");
	$("#main-wrapper").css("border-left", "0px");
	$("#content-wrapper").css("min-height", "0px");
	$("#content-wrapper").css("background-color", "white");
	$(".navtabs").css("display", "none");
	$(".breaking-news").css("display", "none");
	$(".adsensewide").css("display", "none");
	$(".newauthorboxwrapper").css("display", "none");
	$(".OUTBRAIN").css("display", "none");
	$(".adsense").css("display", "none");
	$(".announce").css("display", "none");
	$("#menufooter").css("display", "none");
	$("#disqus_thread").css("display", "none");
	$("#header_container").css("display", "none");
	$("#blog-pager").css("display", "none");
	$(".addthis-smartlayers").css("display", "none");
	$(".at4-share-outer.addthis-smartlayers.addthis-smartlayers-desktop").css("display", "none !important");
	$(".announce").css("width", "97%");
	$("span.normalbutton").css("visibility", "visible");
	$("input.normalbutton").css("visibility", "visible");
	$("span.printbutton").css("display", "none");
	$("input.printbutton").css("display", "none");
ga('send', 'event', 'printfriendly', document.title);

}

else if (type == "normal") {
	window.location.reload();
}

}

    	function setThumbtype(type) {
          setCookie('TT', type, 100);    // Or however many days you want.
        }

    	function setThemetype(type) {
          setCookie('TH', type, 100);    // Or however many days you want.
        }

