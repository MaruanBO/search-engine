<script type="text/javascript">
var thumbtype = getThumbCookie("TT");
var themetype = getThemeCookie("TH");
var unreadtype = getUnreadCookie("UP");
var backgroundcookie = getBackgroundCookie('BC');
var bannercookie = getBannerCookie('BG');

if(unreadtype == "on") {
$('input:radio[name="unreadtype"][value="On"]').prop('checked', true);
}
  else {
$('input:radio[name="unreadtype"][value="Off"]').prop('checked', true);
  }


if(thumbtype == "none") {
$('input:radio[name="thumbtype"][value="Rounded"]').prop('checked', true);
}
else if(thumbtype == "sharp") {
$('input:radio[name="thumbtype"][value="Sharp"]').prop('checked', true);
}
else if(thumbtype == "circle") {
$('input:radio[name="thumbtype"][value="Circle"]').prop('checked', true);
}
else if(thumbtype == "square") {
$('input:radio[name="thumbtype"][value="Square"]').prop('checked', true);
}
else if(thumbtype == "grid") {
$('input:radio[name="thumbtype"][value="Grid"]').prop('checked', true);
}
else if(thumbtype == "grid3") {
$('input:radio[name="thumbtype"][value="Grid3"]').prop('checked', true);
}

if(themetype == "dark") {
$('input:radio[name="themetype"][value="Dark"]').prop('checked', true);
}
else {
$('input:radio[name="themetype"][value="Light"]').prop('checked', true);
}

$('input[name="inputboxcolour"]').val(backgroundcookie);
$('input[name="inputbox"]').val(bannercookie);
    
</script>
