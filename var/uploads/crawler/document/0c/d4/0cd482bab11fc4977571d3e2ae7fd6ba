currentUrl : encodeURIComponent(window.location.href)
};
</script>
<script>
//var adslot;
//var adslot1;
function addDataValuesToDfpTag( dataForDFP ) {
// set key:values to the DFP tag and, if this is the final step, call DFPs
//These values need to be passed in custom criteria as below
//.setTargeting("proximic", dataForDFP)
//console.log(dataForDFP);
if(typeof(dataForDFP) !== "undefined"){
if(adConfig.debug)console.log('addDataValuesToDfpTag()',dataForDFP);
googletag.pubads().setTargeting("proximic", dataForDFP);
}else{
if(adConfig.debug)console.log('addDataValuesToDfpTag() no data from proxf');
}
//googletag.display('leaderboard0');
//googletag.display('leaderboard1');
googletag.pubads().refresh();
};
var dfp_called = false;
var timer;
var time_out = 200;//(timeout in ms) call dfp if timeout is exceeded
function gotSegmentData(dataViaJsonp) {
if(adConfig.debug)console.log('gotSegmentData() dfp_called='+dfp_called,dataViaJsonp);
if (dfp_called == false) {
dfp_called = true;
/*
Data has now been received and is now in dataViaJsonp
Sample dataViaJsonp (note: response format can be easily changed):
{"data":["300003","210000","240000","240001","240005","240006","3fy87w"]}
*/
/* kill the timeout timer */
window.clearTimeout(timer);
// use the information within the 'data' key, if key isn't there - don't use the data
if ("data" in dataViaJsonp) {
/*call DFP with the data*/
addDataValuesToDfpTag(dataViaJsonp.data);
} else {
/*
Do not send error codes or unknown values to DFP
call DFP without using the data
console.log("error");
*/
addDataValuesToDfpTag();
}
}
};
/* timer = window.setTimeout(
function(){
if(adConfig.debug)console.log('timeout - dfp_called='+dfp_called);
if (dfp_called == false) {
dfp_called = true;
//Timeout occurred, call DFP without data
if(adConfig.debug)console.log('proximic timeout occured');
addDataValuesToDfpTag();
}
},time_out);
*/
</script>
<script>
var e = (adConfig.currentUrl ? ("url=" + adConfig.currentUrl) : "");
if(adConfig.debug)console.log(adConfig);
function refreshAds(adId) {
var adArr = [];
if(typeof adId !== 'undefined'){
adArr.push(adConfig.adsToRefresh[adId]);
}else{
adArr = $.map(adConfig.adsToRefresh, function (value, key) { return value; });
}
if(adConfig.debug)console.log('refreshAds() called for', adArr);
googletag.pubads().refresh(adArr);
}
googletag = window.googletag || {cmd:[]};
googletag.cmd.push(function() {
if(adConfig.debug)console.log('Starting ad configuration');
configureAdUnits();
googletag.pubads().collapseEmptyDivs();
googletag.pubads().enableAsyncRendering();
googletag.pubads().enableVideoAds();
var pubAds = [],
slotPrefix = '/8456/IDG.UK_B2C_PCAdvisor.co.uk/';
var adUnits = Object.keys(adConfig.units).sort(function(a, b){
return 1 + Math.max(a.indexOf('avid'), a.indexOf('inskin')) + ((a.indexOf('leaderboard') == 0 ? -5 : 0) + (b.indexOf('leaderboard') == 0 ? 5 : 0));
});
if(adConfig.debug)console.log('Defining ad slots');
for(var adName in adUnits){
var adName = adUnits[adName]
if(adName.indexOf('welcomead') >= 0){
if(adConfig.debug)console.log('enabling welcomead');
var ad = googletag.defineOutOfPageSlot(slotPrefix + adConfig.zone, 'welcomead-pos1')
.defineSizeMapping(AD_CONFIG_UNITS["welcomead"].mappings)
.addService(googletag.pubads());
pubAds.push(ad);
continue;
}
var adIdPrefix = adName + '-pos',
adUnitName = slotPrefix + adConfig.zone,
adCount = adConfig.units[adName].count,
adMappings = adConfig.units[adName].mappings,
j=0,
adSlotSize = adConfig.units[adName].slotSize;
for(var i=1; i <=adCount; i++){
if(adConfig.debug)console.log('attempting to enable '+adName + ' @ pos='+i+' with ', adConfig.units[adName]);
++j;
var targetPrefix = '';
if(adName=='leaderboard' || adName=='mpu' || adName=='sky' || adName=='mpuSmall'){
if(i==1 && adName!='mpuSmall'){
targetPrefix = 'atf1';
j=0;
} else {
targetPrefix = 'btf'+j;
}
}
var adId = adIdPrefix+i,
ad = googletag.defineSlot(adUnitName, adSlotSize, adId)
.defineSizeMapping(adMappings)
.addService(googletag.pubads());
if(adName=='leaderboard' || adName=='mpu' || adName=='sky' || adName=='mpuSmall'){
ad.setTargeting("pos", [targetPrefix]);
}
pubAds.push(ad);
adConfig.adsToRefresh[adId] = ad
if(adConfig.debug)console.log('enabled '+adName+' with ', adUnitName, adSlotSize, adMappings, adId, 'pos='+i);
}
}
if(adConfig.debug)console.log('Finished defining ad slots');
var resizeTimeOut = false;
window.onresize = function(){
if(resizeTimeOut !== false){
clearTimeout(resizeTimeOut);
}
resizeTimeOut = setTimeout(refreshAds, 500);
};
//meta tags
var rs_tax = [];
var metatags = jQuery('meta[name=keywords]').attr("content");
if (metatags != undefined && metatags != '') {
strmetatags = metatags.toLowerCase().replace(/&amp;/g, "").replace(/&/g, "").split(",");
for(var i=0,len=strmetatags.length; value=strmetatags[i], i<len; i++) {
rs_tax.push(gptEscape(jQuery.trim(value)));
}
}
if (rs_tax.length > 0) {
if(adConfig.debug)console.log('kw=' + rs_tax.join(','));
googletag.pubads().setTargeting("kw", rs_tax);
}
googletag.pubads().setTargeting("URL", adConfig.currentUrl);
if(typeof amznads !== 'undefined') {
try {
amznads.setTargetingForGPTAsync('amznslots');
}catch(ex){
console.error(ex);
}
}else if(adConfig.debug){
if(adConfig.debug)console.log('amznads is undefined');
}
//googletag.pubads().disableInitialLoad();//enable with proximic call back
googletag.pubads().enableSingleRequest();
googletag.enableServices();
if(adConfig.debug)console.log('Finished ad configuration');
});
