<script type="text/javascript">
var AD_CONFIG_UNITS = {},
configureAdUnits = function(){throw('not defined yet');};
(function(){
configureAdUnits = function() {
if(adConfig.debug)console.log('configureAdUnits called');
var iPad = navigator.userAgent.match(/iPad/i) != null,
Android = navigator.userAgent.toLowerCase().indexOf("android") > -1,
AndroidMobile = navigator.userAgent.toLowerCase().indexOf("android") > -1 && navigator.userAgent.toLowerCase().indexOf("mobile") >-1,
Win8Touch = navigator.userAgent.toLowerCase().indexOf("windows nt 6.2") > -1 && navigator.userAgent.toLowerCase().indexOf("touch") > -1;
var mappings = {
leaderboard : googletag.sizeMapping().
addSize([980, 0], [[970, 250], [997, 123], [970, 66], [728, 90]]).
addSize([570, 0], [[728, 90]]).
addSize([478, 0], [[468, 60]]).
addSize([280, 0], [[320, 50], [300, 50], [300, 250]]). //mpu in leaderboard slots for mobile only
addSize([0, 0], [[12, 1]]).
build(),
mpu : googletag.sizeMapping().
addSize([830, 0], [[300, 250], [300, 600]]).
addSize([0, 0], [12, 1]).
build(),
mpuSmall : googletag.sizeMapping().
addSize([830, 0], [[300, 250]]).
addSize([320, 0], [[300, 250],[320, 50]]).
addSize([0, 0], [12, 1]).
build(),
welcomead : googletag.sizeMapping().
addSize([650, 0], [1, 1]).
build(),
sky : googletag.sizeMapping().
addSize([0,0], [[120,600],[160,600],[160,800]]).
build(),
inskin : googletag.sizeMapping().
addSize([980,0], [[2,2]]).
build(),
avid : googletag.sizeMapping().
addSize([0,0], [[2,1]]).
build()
};
var slotSize = {
leaderboard: [728, 90],
mpu: [300, 250],
mpuSmall: [300, 250],
sky: [120, 600],
inskin: [2, 2],
avid: [2, 1]
};
if(adConfig.debug)console.log('adding welcomead to unit config');
AD_CONFIG_UNITS["welcomead"] = {count: 1, mappings: mappings['welcomead'], slotSize: slotSize['welcomead']};
if(adConfig.debug)console.log('adding mpu to unit config');
AD_CONFIG_UNITS["mpu"] = {count: 1, mappings: mappings['mpu'], slotSize: slotSize['mpu']};
if(adConfig.debug)console.log('adding mpuSmall to unit config');
AD_CONFIG_UNITS["mpuSmall"] = {count: 1, mappings: mappings['mpuSmall'], slotSize: slotSize['mpuSmall']};
if(iPad || (Android && !AndroidMobile) || Win8Touch || screen.width >= 1260) {
AD_CONFIG_UNITS["inskin"] = {count: 1, mappings: mappings['inskin'], slotSize: slotSize['inskin']};
}
if(adConfig.debug)console.log('adding avid to unit config');
AD_CONFIG_UNITS["avid"] = {count: 0, mappings: mappings['avid'], slotSize: slotSize['avid']};
if(adConfig.debug)console.log('adding leaderboard to unit config');
AD_CONFIG_UNITS["leaderboard"] = {count: 2, mappings: mappings['leaderboard'], slotSize: slotSize['leaderboard']};
if(adConfig.debug)console.log('adding sky to unit config');
AD_CONFIG_UNITS["sky"] = {count: 0, mappings: mappings['sky'], slotSize: slotSize['sky']};
if(adConfig.debug)console.log('configureAdUnits returning');
return AD_CONFIG_UNITS;
};
})();
</script>
