</div>
</div>
<script>
var cookieAppearanceDelay = 5000;
var cookieMessageLifeSpan = 20000;
function hideCookieBox(){
$('#cookieBar').fadeOut();
}
function showCookieBox(){
$('#cookieBar').show();
$('#cookieBar').fadeIn();
setTimeout(function(){hideCookieBox()},cookieMessageLifeSpan);
}
setTimeout(function(){showCookieBox()},cookieAppearanceDelay);
$(document).ready(function(){
$('.cookieClose').click(function(){
hideCookieBox();
});
});
</script>
