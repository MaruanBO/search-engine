<script type="text/javascript">
$(document).ready(function(){
//function to close other panels
/*function closeAllPanels(){
$('.forumLinks').slideUp(200);
$('.toggleBarOpen').removeClass('toggleBarOpen').addClass('toggleBar');
$('.toggleBar').attr('id','');
};*/
//set number of forums to initially expand
var numberToShow = 9;
//onload function to open the first panel
function openInitialPanels(){
$('ul.articleListingList li ul li.forumLinks').slice(0,numberToShow*3).slideDown(200);
$('ul.articleListingList li ul li.toggleBar').slice(0,numberToShow).addClass('toggleBarOpen');
$('ul.articleListingList li ul li.toggleBarOpen').slice(0,numberToShow).attr('id','panelOpen');
}
openInitialPanels();
//click event for a panel
$('.toggleBar').click(function(){
//checking if id 'openPanel' exists
if($('#panelOpen').length){
//and if this is the one
if(this.id=='panelOpen'){
$(this).parent().children('.forumLinks').slideUp(200);
$(this).removeClass('toggleBarOpen');
$(this).attr('id','');
}else{
$(this.parentNode.children).slideDown(200);
$(this).addClass('toggleBarOpen');
$(this).attr('id','panelOpen');
}
}else{
//
$(this.parentNode.children).slideDown(200);
$(this).addClass('toggleBarOpen');
$(this).attr('id','panelOpen');
}
});
});
</script>
</div>
