$.ajax({
type: "GET",
url: "http://ad.doubleclick.net/8456/IDG.UK_B2C_PCAdvisor.co.uk/video-clicktoplay_section;sz=1x1;ord=" + ord + '?',
cache: false
});
tracked=true;
}
setTimeout(function(){tracked=false;},500);
}
});
</script>
