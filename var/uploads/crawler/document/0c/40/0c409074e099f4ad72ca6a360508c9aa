<script type="text/javascript">
function displaySubMenu(){
		var userFirstNameCookie = dojo.cookie("userFirstName");		
		if(userFirstNameCookie != null && dojo.trim(userFirstNameCookie) != ""){
			dojo.byId("submenu_reg").style.display="block";
			dojo.byId("submenu_guest").style.display="none";
		} else {
			dojo.byId("submenu_guest").style.display="block";
			dojo.byId("submenu_reg").style.display="none";
		}
}
dojo.addOnLoad(displaySubMenu);
</script>
