<script type="text/javascript">
   function removeFromCart(orderItemId) {
      //For handling multiple clicks
      if(!submitRequest()){
         return;
      }

      var params = [];
      params.storeId = '10154';
      params.catalogId = '10051';
      params.langId = '-1';
      params.orderId = ".";
      params.orderItemId = orderItemId;
      
      dropDownDlg.hide();
      
      wc.service.invoke("AjaxDeleteOrderItem", params);
   }

