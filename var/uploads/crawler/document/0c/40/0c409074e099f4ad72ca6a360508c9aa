<script type="text/javascript">
function displayHeaderName(){
		var userFirstNameCookie = dojo.cookie("userFirstName");		
		if(userFirstNameCookie != null && dojo.trim(userFirstNameCookie) != "" && dojo.byId("welcomemsg2_headername") != null){
			dojo.byId("welcomemsg2_headername").innerHTML=userFirstNameCookie;
			
		}
}
</script>
                  
