function displayTopMenu(){
		var userFirstNameCookie = dojo.cookie("userFirstName");		
		if(userFirstNameCookie != null && dojo.trim(userFirstNameCookie) != ""){
			dojo.byId("toprow_reg").style.display="inline";
			dojo.byId("welcomemsg2").style.display="inline";
			dojo.byId("toprow_guest").style.display="none";
		} else {
			dojo.byId("toprow_guest").style.display="inline";
			dojo.byId("toprow_reg").style.display="none";
			dojo.byId("welcomemsg2").style.display="none";
		}
}
dojo.addOnLoad(displayTopMenu);
