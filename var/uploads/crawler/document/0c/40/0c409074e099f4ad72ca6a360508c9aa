<script type="text/javascript">
dojo.addOnLoad(function() {
	enterZipModal = findDialog("enterZipModal");
});
//dijit enterZipModal
var enterZipModal = null;

zipCodeModalDisplayJS = {
	closeModal: function() {
		enterZipModal.hide();
	},
	
	setZipCode: function(zipCodeFieldName) {
		var zipCode = dojo.byId(zipCodeFieldName).value;
		var validZIP = this.validateZipCode(zipCode);
		if (validZIP) {
			if (storeUtilJS) {
			storeUtilJS.loginmodalinit = false;
				storeUtilJS.validateCountryAndSetZipCode(zipCode,zipCodeFieldName,'true','true','true');
				//TODO: setTimeout(dojo.hitch(storeUtilJS.validateCountryAndSetZipCode(zipCode,zipCodeFieldName,'true'),"callFBLogin"),700);
			} else {
				console.debug('storeUtil.js missing!!');
			}
		} else {
			//show Error
			addInlineError(dojo.byId(zipCodeFieldName), "Please enter a 5 digit ZIP Code");
		}
	},
	
	callFBLogin: function() {
		this.closeModal();
		login(); //Facebook login
	},
	
	validateZipCode: function(zipCode) {
		var re = new RegExp(/^(\d*[1-9]\d*|[0-9]\d*)$/);
		return re.test(zipCode) && zipCode.length == 5;
	}
}
</script>

