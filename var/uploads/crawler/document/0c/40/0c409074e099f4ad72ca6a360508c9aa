              <script type="text/javascript">
                dojo.addOnLoad(function(){
                  if(dojo.byId('MiniShoppingCart') != null){
                    originalMiniCartWidth = dojo.coords(dojo.byId('MiniShoppingCart'),true).w;
                  }
  
                  
                    dojo.connect(dojo.byId("SimpleSearchForm_SearchTerm"), "onkeydown", function(evt) {
                      key = evt.keyCode;
                      if(key == dojo.keys.ENTER) {
                        if(document.CatalogSearchForm.searchTerm.value.length > 0 && document.CatalogSearchForm.searchTerm.value != 'search for a product or item #') {
                          document.CatalogSearchForm.searchTerm.value = trim(document.CatalogSearchForm.searchTerm.value);
                          document.CatalogSearchForm.action = 'http://www.hhgregg.com/webapp/wcs/stores/servlet/SearchDisplay?';
                          submitSpecifiedForm(document.CatalogSearchForm);
                        }
                      }
                    });
                  
                });
                
                                

function submitSearch() {
         var sValue = trim(document.CatalogSearchForm.searchTerm.value);
     if(sValue == '' || sValue == 'search for a product or item #'){
      return false;
     }  
         fillSearchField();
         if (noSearch) {
            document.CatalogSearchForm.searchTerm.value='';
         }
         else {
        sValue = sValue.replace(/\\/g, "");
        sValue = sValue.replace(/\//g, " ");
            document.CatalogSearchForm.searchTerm.value= sValue;
         }
         document.CatalogSearchForm.action='http://www.hhgregg.com/webapp/wcs/stores/servlet/SearchDisplay?';
         submitSpecifiedForm(document.CatalogSearchForm);
      }
      
      
function hideDropDownMenu(statusCount){
		 var dropDownId = 'WC_CachedHeaderDisplay_div_3_'+statusCount;
		 var dropDownObj = document.getElementById(dropDownId);
			if(dropDownObj != null && dropDownObj != 'undefined' ){		
				dropDownObj.className = "dropdown nodisplay";
			}	
		}
		
function showDropDownMenu(statusCount){
		var dropDownId = 'WC_CachedHeaderDisplay_div_3_'+statusCount;
		var dropDownObj = document.getElementById(dropDownId);
		 if(dropDownObj != null && dropDownObj != 'undefined' ){
			dropDownObj.className = "dropdown";
		 }	
       }


      
function showDropDownMenuTouch(statusCount){
    
	  var dropDownId = 'WC_CachedHeaderDisplay_div_3_'+statusCount;
	  var dropDownObj = document.getElementById(dropDownId);
	  var location = dropDownObj.getAttribute("showallurl");
	  var allActiveMenus = document.querySelectorAll(".active.dropdown");
	 var toplink = 'HeaderMenu_DropDown_'+statusCount;
	 
	 var topLinkObj =  document.getElementById(toplink);
	 
	   if(dropDownObj != null && dropDownObj != 'undefined'&& hasClass(dropDownObj,'nodisplay') ){
	      event.preventDefault(); 
	      event.stopPropagation();
	      hideOtherActiveMenus();
	      dropDownObj.className = "active dropdown";  
	   }
	
	   else if(dropDownObj != null && dropDownObj != 'undefined' && dropDownObj.className == "active dropdown"){
		  
		 setTimeout(function(){dropDownObj.className = "dropdown nodisplay";},1500);
		  
	   }	
     }

   
function showDropDownMenuClearanceTouch(){
 
	  var dropDownId = 'WC_CachedHeaderDisplay_div_3_clearance';
	  var dropDownObj = document.getElementById(dropDownId);
	  var location = dropDownObj.getAttribute("showallurl");
	  var allActiveMenus = document.querySelectorAll(".active.dropdown");
	 var toplink = 'HeaderMenu_DropDown_5';
	 
	 var topLinkObj =  document.getElementById(toplink);
	 
	   if(dropDownObj != null && dropDownObj != 'undefined'&& hasClass(dropDownObj,'nodisplay') ){
	      event.preventDefault(); 
	      event.stopPropagation();
	      hideOtherActiveMenus();
	      dropDownObj.className = "active dropdown";  
	   }
	
	   else if(dropDownObj != null && dropDownObj != 'undefined' && dropDownObj.className == "active dropdown"){
		 
		 setTimeout(function(){dropDownObj.className = "dropdown nodisplay";},1000);
		
	   }	
     }

function hideOtherActiveMenus(){
		 var allActiveMenus = document.querySelectorAll(".active.dropdown"); 
		
		  if(allActiveMenus.length > 0){
		     for(var i = 0; i < allActiveMenus.length; i++) {
		          
		      var thiselement = allActiveMenus.item(i); 
		    
		    	    thiselement.className = "dropdown nodisplay";
		     }		  
		  }
    }


var menuOpen = document.getElementById('header_nav');
document.ontouchstart = function (e) {
 
    e = e || window.event;      
    var target = e.target || e.srcElement;
   
    if(target.className == 'menuLinks'){
   
    }
   
    else if ((!target.contains(menuOpen) || target !==  menuOpen) ||  target.className !== 'menuLinks') {
   
        hideOtherActiveMenus();
       
       
    }  
}
            </script>
