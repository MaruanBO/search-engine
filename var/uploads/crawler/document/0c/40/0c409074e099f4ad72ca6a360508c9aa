	dojo.addOnLoad(function() {
      	//this is for images that get loaded on scroll or using JS
      	$("img.lazy").lazyload({skip_invisible: true, failure_limit: 20});
     	
     	//load these images ('lazyload' class) right on 'onload' of the page. these are more immediate than 'lazy' class
     	var $images = $('img.lazyload');
 
	    $images.each(function(){
	        var $img = $(this),
	            src = $img.attr('data-original');
	 
	        $img.attr('src',src);
	    });
  	});
