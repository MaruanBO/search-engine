<script>


function validate_zip()
{
		
if(document.zip_form.zip.value=='Enter Your Zip Code')
		{
		alert("Please Enter Your Zip Code.");
		document.zip_form.zip.value='';
		document.zip_form.zip.focus();
		return false;
		}
if(isNaN(document.zip_form.zip.value))
		{
		alert("Zip Code must be numeric.");
		document.zip_form.zip.focus();
		return false;
		}
if(document.zip_form.zip.value.length<5)
		{
		alert("Please Enter 5 digits Zip Code.");
		document.zip_form.zip.focus();
		return false;
		}
						
	
return true;
}



</script>
