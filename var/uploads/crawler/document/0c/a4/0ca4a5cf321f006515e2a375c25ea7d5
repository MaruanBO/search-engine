  // three temporary functions to make the divs line up
  // royal slider has no event that is triggered
  // when it loads so I have to do this
  (function(){
    var sliderHeight = 0,

    adjustHeight = setInterval(function(){
      var first, second, firstOffset, secondOffset;

      sliderHeight = $('#ContentPane13 .module.spotlight').height() || 0;

      // constantly check the height to see if loaded
      if(sliderHeight > 200){

        // whichever is higher gets lengthened
        first = $('#ContentPane12 > div:last');
        second = $('#ContentPane13 > div:last');
        firstOffset = first.offset().top + first.height();
        secondOffset = second.offset().top + second.height();

        if(firstOffset < secondOffset){
          first.height(first.height() + (secondOffset - firstOffset));
        } else if(firstOffset > secondOffset){
          second.height(second.height() + (firstOffset - secondOffset));
        } else {
          clearInterval(adjustHeight);
        }
      }
    }, 200);
  })();

  (function(){
    var first, second, firstOffset, secondOffset;

    // whichever is higher gets lengthened
      first = $('.specialReport.module');
      second = $('.oneQuestionQuiz.module');
      firstOffset = first.offset().top + first.outerHeight();
      secondOffset = second.offset().top + second.outerHeight();

    if(firstOffset < secondOffset){
      first.height(first.outerHeight() + (secondOffset - firstOffset));
    } else if(firstOffset > secondOffset){
      second.height(second.outerHeight() + (firstOffset - secondOffset));
    }
  })();

  // temporary because of request to clear input fields
  $(function(){
    setTimeout(function(){
      $('.bmiCalculator input[type=text]').val('');
    }, 1000);
    setTimeout(function(){
      $('.bmiCalculator input[type=text]').val('');
    }, 2000);
  });

