function GetMarketAreaID()
{
    return document.forms[0].ddlSearchBoxCitySelector.options[document.forms[0].ddlSearchBoxCitySelector.selectedIndex].value;
}

function SearchSubmitFromQS() 
{
    document.forms[0].txtExecuteSearchFlag.value = escape(document.forms[0].txtExecuteSearchFlag.value).replace(/%20/gi, " ").replace(/%27/gi, "/'");
