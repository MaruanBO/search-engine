        document.forms[0].submit();
    }
}

function QuickSearchKeywordSetFocus1(SearchKeyword, DefaultSearchBoxText) 
{
    document.forms[0].txtExecuteSearchFlag.value = escape(document.forms[0].txtExecuteSearchFlag.value).replace(/%20/gi, " ").replace(/%27/gi, "/'");
    SearchKeyword = escape(document.getElementById('ctl50_txtQuickSearch').value).replace(/%20/gi, " ").replace(/%27/gi, "/'");
    document.getElementById('ctl50_txtQuickSearch').value = SearchKeyword;
    
    if (SearchKeyword == escape(DefaultSearchBoxText).replace(/%20/gi, " ").replace(/%27/gi, "/'"))
    {
        document.getElementById('ctl50_txtQuickSearch').focus();
        document.getElementById('ctl50_txtQuickSearch').value = "";
        document.getElementById('ctl50_txtQuickSearch').select();
    }
}
