<script type="text/javascript">
    require(['easybib.global'], function () {
        require(['jquery', 'config', 'jquery.json'], function ($, Config) {
            $(document).on(Config.listener.click, 'a.ac_link', function() {
                var pieces    = $(this).attr('id').split('_');
                var link_name = pieces[2];
                var service   = pieces[1];
                window._gaq.push(['citations._trackEvent', 'Autocite_', service, link_name]);
            });
            $(document).on(Config.listener.click, '.sponsor', function(e) {
                if (e.target.className.indexOf('sponsor') >= 0) {
                    var sponsor_item = $(this).attr('id');
                    window._gaq.push(['easybib._trackEvent', 'Sponsor', 'Click', sponsor_item]);
                }
            });
                        if ($('#citation_new').size() == 1) {
                var trak = $.evalJSON("");
                window._gaq.push(['citations._trackEvent', trak[0], trak[1], trak[2]]);
            }
            $(document).on(Config.listener.click, 'a', function() {
                var l = location.href; //where we are
                var u = $(this).attr('href'); //where we're going
                var t = $(this).text(); //what we said
                window._gaq.push(['easybib._trackEvent', l, u, t]);
            });
        });
    });
</script>
