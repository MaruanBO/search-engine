<script>
    $(document).ready(function() {
        $(".cycle-slide a").click(function(event) {
            if (($(this).attr("data-youtube").length !== 0) || ($(this).attr("data-iva").length !== 0)) {
                event.preventDefault();
                var youtubeID = $(this).attr("data-youtube");
                var IVAid = $(this).attr("data-iva");
                var trailerTitle = $(this).attr("data-trailertitle");

                if (youtubeID.length !== 0) {
                    Regal.showYoutubeTrailerModal({
                        YoutubeEmbedCode: youtubeID,
                        Title: trailerTitle
                    });
                }
                if ((youtubeID.length === 0) && IVAid.legnth !== 0) {
                    Regal.showTrailerModal({
                        ivaId: IVAid,
                        Title: trailerTitle
                    });
                }
                return false;
            }
        });
    });
</script>
