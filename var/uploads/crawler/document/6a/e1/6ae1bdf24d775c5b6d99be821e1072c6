	<div id="leap_mandrill-2" class="widget widget_leap_mandrill">	<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/themes/smoothness/jquery-ui.css">
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.js"></script>
	<script type="text/javascript">
	(function($){
    window.setupMandrill = function(selector){
      $(selector).each(function(){
        var $mandrill=$(this),
          $form=$('.mandrill-form',$mandrill),
          $date=$('.mandrill-date',$mandrill),
          $email=$('.mandrill-email',$mandrill),
          $result=$('.mandrill-result',$mandrill);
        $date.datepicker({
          changeMonth: true,
          changeYear: true,
          dateFormat: "m/d/y"  
        });
        $email.focus(function(){
          if ($email.val()=='Your Email') $email.val('');
        }).blur(function(){
          if ($email.val()=='') $email.val('Your Email');      
        });
        
        // When submitted
        $form.submit(function(e){
          e.preventDefault();
          var d=$date.val();
          if (d=='Due Date' || d.length==0) {
            alert('Please enter a date');
            return;
          }
          var e=$email.val(),
            atpos=e.indexOf("@");
            dotpos=e.lastIndexOf(".");
          if (atpos<1 || dotpos<atpos+2 || dotpos+2>=e.length) {
            alert("Please enter a valid e-mail address");
            return;
          }
          $result.html('<div class="result"><img src="http://cdn1.alphamom.netdna-cdn.com/wp-includes/images/spinner.gif" /> One second please</div>');
          $.ajax({
            dataType: "json",
            url: "/wp-content/plugins/leap-mandrill/mandrill_submit.php",
            data: { 'expected_date': d, 'email': e },
            error: function() {
              $result.html('<div class="result exception">Sorry -- we encountered an error submitting the request</div>');
            },
            success: function(data) {
              var msg;
              switch(data.msg){
                case 'Invalid':
                  msg = '<div class="result invalid">Invalid expected date</div>';
                  break;
                case 'Exception':
                  msg = '<div class="result exception">An exception occurred, please try again</div>';
                  break;
                case 'success':
                  msg = '<div class="result success">Successfully subscribed!</div>';
                  break;
              } // switch
              $result.fadeOut(500,function(){ $result.html(msg).fadeIn(500); })
            } // success
          }); // $.ajax
        }); // $form.submit
      }); // $('.mandrill')
    }
    $(function(){setupMandrill('.mandrill')});
  })(jQuery);   
	</script>
  <div class="mandrill">
    <form class="mandrill-form" name ="mandrillform" action="." method="post">
      <input type="text" name="expected_date" class="mandrill-date" value="Due Date">   
      <input type="text" name="email" class="mandrill-email" value="Your Email" />
      <input type="submit" class="button submit-button">
    </form>
