    <script type="text/javascript">
        function fbLoginStatus(response) {
            if (response.authResponse) {
                //user is logged in, display profile div
                $('#facepileDiv').show();
            } else {
                //user is not logged in, display guest div
                $('#facepileDiv').hide();
            }
        }
