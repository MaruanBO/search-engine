                                closeButtontoDeal();
                            };

                            var onCurrentMember = function (event, code, settings) {
                                isFacebook = 0;
                                isGoogle = 1;

                                var dealurl = getDealUrl(requestModeSignedIn, isFacebook, isGoogle);

                                if (ovrlynewwindow) {
                                    $('#' + ovrlydivid).dialog('close');
                                    window.location.reload();
                                    window.open(dealurl, 'dl' + ovrlyadid);
                                }
