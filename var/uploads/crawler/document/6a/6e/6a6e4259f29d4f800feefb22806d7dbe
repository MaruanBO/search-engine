		function doPostBack(eventArgument) {
			var theform;
			if (window.navigator.appName.toLowerCase().indexOf("microsoft") > -1) {
				theform = document.Form1;
			}
			else {
				theform = document.forms["Form1"];
			}
			if(eventArgument == '')
				eventArgument = 'us';
			theform.link_button.value = eventArgument;
			theform.submit();
    }
