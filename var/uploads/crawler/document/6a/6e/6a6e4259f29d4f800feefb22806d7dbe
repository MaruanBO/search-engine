            FB.init({ appId: '140333999364794', status: true, cookie: true, xfbml: true, channelUrl: 'http://www.tzoo-img.com/static/fbConnectChannel.htm?v=4964' });
                FB.getLoginStatus(fbLoginStatus);
                FB.Event.subscribe('auth.statusChange', fbLoginStatus);
            };
