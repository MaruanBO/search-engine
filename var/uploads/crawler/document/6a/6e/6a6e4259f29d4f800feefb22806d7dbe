<script type="text/javascript">
$('iframe#divFacepile').attr('src', 'http://www.facebook.com/plugins/facepile.php?action&size=small&max_rows=1&width=250&colorscheme=light&app_id=140333999364794&href=http%3A%2F%2Fwww.facebook.com%2Ftravelzoo');
  function block(obj) {
      if($(obj).parents('.nolock').size() > 0 ) { return true; }
      if($.cookie && $.cookie('subscriber-us') ) { return true; }
      var url = $(obj).attr('href');
      if( url ) {
          var urlmatch = url.match(/.*www.travelzoo.com\/.*\/.*?(\d{6,}).*/);
          if( urlmatch ) {
              var adid = urlmatch[1];
              $.get('/_ajax/GetDealHeadline.ashx', { id: adid, imagewidth: 380}, 
               function(data){
                  adid = data.split('|')[1];
                  $('.ovrlhdl').text(data.split('|')[0]);
                  $('.ovrlcompany').text(data.split('|')[2]);
              var imageUrl = data.split('|')[3];
              if(imageUrl != '') {
                  $('.ovrldealimage').attr('src', imageUrl);
                  $('.ovrldealimagecnt').show();
                  $('.ovrldealnoimagecnt').hide();
              }
                  $('#signupOverlay').dialog({ dialogClass: 'signupOverlay', closeOnEscape: true, autoOpen: true, modal: true, width: 380, height: 800, closeText: '', resizable: false, draggable: false, open: trackOpen_with_dialog_move});
                  ovrlyadid = adid;
                  ovrlyadurl = url;
                  ovrlynewwindow = (($(obj).attr('target') == '_blank') ? true : false);
                  postOpen();
              });
              return false;
          }
      } 
      return true;
}
function trackOpen_with_dialog_move() {
     trackOpen;
     $('.signupOverlay').css({'position':'fixed','top':'24px'});
}
