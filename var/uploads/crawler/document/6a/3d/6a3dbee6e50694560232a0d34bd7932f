<script type="text/javascript">
    function setInitialHeight(that) {
        var height = $(".cmg_editorial_teaser_title", $(that)).height();
        $(".cmg_editorial_teaser", $(that)).animate({ "top": (height + 15) * -1 }, 200);
    }

    $(function() {
        $(".cmg_editorial_teaser_container").each(function () {
            setInitialHeight(this);
        });
        
        $(".cmg_editorial_teaser_container").hover(
            // hover in
            function () {
                var height = $(".cmg_editorial_teaser_text", $(this)).height() + $(".cmg_editorial_teaser_title", $(this)).height();
                $(".cmg_editorial_teaser", $(this)).animate({ "top": (height + 30) * -1 }, 200);
            },
            // hover out
            function () {
                console.log("hovering out");
                setInitialHeight(this);
            }
        );
    });
</script>
