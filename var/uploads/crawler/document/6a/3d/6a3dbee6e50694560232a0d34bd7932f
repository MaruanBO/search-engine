<script type="text/javascript">
    $(document).ready(function () {

        $("a.articleToolTip").each(function () {

            function openArticleNewWindow(link, event) {
                var href = $(link).attr('href');
                if (typeof href === "string" && href.length > 0) {
                    window.open(href, "_blank");
                    event.stopImmediatePropagation();
                    return false;
                }
                return true;
            }

            var tipUrl = $(this).attr('rel');
			var pageViewUrl = $(this).attr('rel') ? $(this).attr('rel') : $(this).attr('href') ;
			pageViewUrl = '/' + pageViewUrl.replace(/^(?:\/\/|[^\/]+)*\//, "");

            if (tipUrl) {
                $(this).qtip({
                    content: {
                        text: 'Loading...',
                        ajax: {
                            url: tipUrl,
                            dataType: 'jsonp',
                            jsonpCallback: 'SetArticleQtip',
                            success: function(data) {
                                this.set('content.text', data);
                            }
                        } 
                    },
                    show: {
                        event: 'click',
                        solo: true
                    },
                    hide: {
                        event: 'click'
                    },
                    style: {
                        classes: 'tooltip cmg_tooltip cmg_headlines_tooltip',
                        def: false
                    },
                    position: {
                        my: 'bottom left',  // Position my bottom left...
                        at: 'bottom right', // at the bottom right of...
                        viewport: $(window)
                    },
                    events: {
                        render: function(event, api) {
                            $(api.elements.tooltip).prepend($('<a class="cmg_close_button">Close</a>'));

                            //Google tracking the popups in the latest news section for the home pages
                            
                            ga('send', 'pageview', pageViewUrl);
                            comscore();

                            $("a.cmg_close_button").click(function () {
                                api.hide();
                            });
                        },
                        show: function(event, api) {
                            var timer = $("div#latestNews").data("timer");
                            console.log(timer);
                            timer.pause();
                        },
                        hide: function (event, api) {
                            var timer = $("div#latestNews").data("timer");
                            console.log(timer);
                            timer.resume();
                        }
                    }
                }).click(function(event) {
                    event.preventDefault();
                    if (event.metaKey || event.ctrlKey) {
                        return openArticleNewWindow(this, event);
                    }
                    return false;
                }).bind('mousedown', function (event) {
                    if (event.which === 2) { // middle mouse button
                        event.preventDefault();
                        return openArticleNewWindow(this, event);
                    }
                    return false;
                });
                var api = $(this).qtip('api');
                $(document).keyup(function (e) {
                    if (e.keyCode == 27) { // escape key
                        api.hide();
                    }
                });
            }
        });
    });
    
    function comscore() {
        var _comscore = _comscore || [];
        _comscore.push({ c1: "2", c2: "16964994" });
        (function () {
            var s = document.createElement("script");
            s.async = true;
            s.src = (document.location.protocol == "https:" ? "https://sb" : "http://b") + ".scorecardresearch.com/beacon.js";
            var el = document.getElementsByTagName("script")[0];
            el.parentNode.insertBefore(s, el);
        }
        )();
    }

</script>
