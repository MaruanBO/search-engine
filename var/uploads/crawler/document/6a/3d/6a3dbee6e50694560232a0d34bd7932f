<script>
        //elementId: Id for user header brick holder. If does not exist, will be added to body.
        //data: optional; data for the dialog template { Host: <sports host> }
        CMG_EXPERTS.createSteamwireBrick = function (elementId, data) {

            this.templates.externalSteamwireBrick.render(function (source) {

                var selector = "#" + elementId;
                if ($(selector).length === 0) {
                    console.log("Error. Missing container: " + elementId);
                }
                
                var id = $("li:first", selector).attr("id");
                var newId = $("li:first", source).attr("id");

                if (newId !== id) {
                    $(selector).fadeOut("fast");
                }

                $(selector).html(source);

                if (newId !== id) {
                    $(selector).fadeIn("fast", HighlightNewItem(newId));
                }
                
            }, data, { "id": "", "size": "2" });
                
        };

        $(function () {
            var updateSteamwire = function() {

                CMG_EXPERTS.createSteamwireBrick('steamwires', null);  

                //window.setTimeout(updateSteamwire, 10000);
            };
            
            window.setTimeout(updateSteamwire, 10000);
        });
                                  
        function HighlightNewItem(itemId) {
            $("#timestamp_" + itemId).effect("highlight", { color: "#FFF" }, 250);
            $("#timestamp_" + itemId).effect("highlight", { color: "#C3AD7A" }, 9000);
        }
    </script>

