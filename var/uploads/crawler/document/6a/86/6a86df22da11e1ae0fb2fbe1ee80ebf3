<script>
// jQuery document ready
jQuery(document).ready(function() {

	// Adjust subfeature box height
	var height = 0;
	jQuery('#section-body .zone.zone-subfeatures .region.region-subfeatures .block.block-subfeatures .block-content ul li a').each(function() {
		if (jQuery(this).height() > height) {
			height = jQuery(this).height();
		}
	});
	jQuery('#section-body .zone.zone-subfeatures .region.region-subfeatures .block.block-subfeatures .block-content ul li a').height(height);

	// Load the hot topics
	loadHotTopics();

	// Floating sidebar
	floatingSidebar();

	// Resize the sidebar
	resizeSidebar();

	// Social media counters
	socialMediaCounters();

});

// jQuery window load
jQuery(window).load(function() {

	// Floating sidebar
	floatingSidebar();

	// Resize the sidebar
	resizeSidebar();

});

// jQuery window scroll
jQuery(window).scroll(function() {

	// Floating sidebar
	floatingSidebar();

});

// jQuery window resize
jQuery(window).resize(function() {

	// Floating sidebar
	floatingSidebar();

	// Resize the sidebar
	resizeSidebar();

});

// Floating sidebar
function floatingSidebar() {

	// Initialize variables
	var scrollTop = jQuery(window).scrollTop();
 	var zoneContent = jQuery('#zone-content');
	var zoneContentHeight = parseInt(zoneContent.outerHeight());
	var zoneContentOffsetTop = parseInt(zoneContent.offset().top);
	var zoneContentOffsetBottom = parseInt(zoneContentOffsetTop + zoneContentHeight);
 	var regionSide = jQuery('#region-side');
	var regionSideHeight = parseInt(jQuery('#region-side').outerHeight());
	var regionSideOffsetTop = parseInt(regionSide.offset().top);
	var regionSideOffsetBottom = parseInt(regionSideOffsetTop + regionSideHeight);
	var headerHeight = parseInt(jQuery('#section-header').outerHeight());

	// If the window is scrolled past the content zone...
	if ( ((scrollTop + headerHeight) > zoneContentOffsetTop) ) {

		// If the bottom of the content zone has not scrolled past the height of the sidebar...
		if ( ((zoneContentOffsetBottom - scrollTop) - headerHeight) > regionSideHeight ) {

			// Set the sidebar's height to the top of the page (floating sidebar)
			regionSide.removeClass('bottom').addClass('fixed');

		// If the bottom of the content zone is scrolled past the height of the sidebar...
		} else {

			// Set the sidebar to the bottom position to flow with the content
			regionSide.removeClass('fixed').addClass('bottom');

		}

	// If the window is not scrolled past the content zone...
	} else {

		// Reset the sidebar position
		regionSide.removeClass('fixed bottom');

	}

}

// Resize sidebar
function resizeSidebar() {

	// Initialize variables
	var windowHeight = parseInt(jQuery(window).height());
	var headerHeight = parseInt(jQuery('#section-header').outerHeight(true));
	var viewportHeight = (windowHeight - headerHeight);

	// Region side
 	var regionSide = jQuery('#region-side');
	var regionSideHeight = parseInt(regionSide.height());
	var regionSideOuterHeight = parseInt(regionSide.outerHeight(true));
	var regionSideHeightDifference = parseInt(regionSideOuterHeight - regionSideHeight);

	// Block topics
	var blockTopics = regionSide.find('.block.block-topics:first');
	var blockTopicsHeight = parseInt(blockTopics.height());
	var blockTopicsOuterHeight = parseInt(blockTopics.outerHeight(true));
	var blockTopicsHeightDifference = parseInt(blockTopicsOuterHeight - blockTopicsHeight);
	var blockTopicsBlockHeaderHeight = parseInt(blockTopics.find('.block-header').height());
	var blockTopicsBlockHeaderOuterHeight = parseInt(blockTopics.find('.block-header').outerHeight(true));
	var blockTopicsBlockHeaderHeightDifference = parseInt(blockTopicsBlockHeaderOuterHeight - blockTopicsBlockHeaderHeight);
	var blockTopicsBlockContentHeight = parseInt(blockTopics.find('.block-content').height());
	var blockTopicsBlockContentOuterHeight = parseInt(blockTopics.find('.block-content').outerHeight(true));
	var blockTopicsBlockContentHeightDifference = parseInt(blockTopicsBlockContentOuterHeight - blockTopicsBlockContentHeight);

	// Block social
	var blockSocial = regionSide.find('.block.block-social:first');
	var blockSocialHeight = parseInt(blockSocial.height());
	var blockSocialOuterHeight = parseInt(blockSocial.outerHeight(true));
	var blockSocialHeightDifference = parseInt(blockSocialOuterHeight - blockSocialHeight);

	/*
	// Block advertisement
	var blockAdvertisement = regionSide.find('.block.block-advertisement:first');
	var blockAdvertisementHeight = parseInt(blockAdvertisement.height());
	var blockAdvertisementOuterHeight = parseInt(blockAdvertisement.outerHeight(true));
	var blockAdvertisementHeightDifference = parseInt(blockAdvertisementOuterHeight - blockAdvertisementHeight);
	*/

	// Block topics first list item
	var blockTopicsFirstListItem = regionSide.find('.block.block-topics:first .block-content ul li:first');
	var blockTopicsFirstListItemHeight = parseInt(blockTopicsFirstListItem.height());
	blockTopicsFirstListItemHeight = isNaN(blockTopicsFirstListItemHeight) ? 0 : blockTopicsFirstListItemHeight;
	var blockTopicsFirstListItemOuterHeight = parseInt(blockTopicsFirstListItem.outerHeight(true));
	blockTopicsFirstListItemOuterHeight = isNaN(blockTopicsFirstListItemOuterHeight) ? 0 : blockTopicsFirstListItemOuterHeight;

	// Max block topics height
	//var outerHeight = parseInt(regionSideHeightDifference + blockTopicsHeightDifference + blockSocialHeightDifference + blockAdvertisementHeightDifference);
	//var maxBlockTopicsBlockContentHeight = parseInt(viewportHeight - outerHeight - (blockSocialHeight + blockAdvertisementHeight) - blockTopicsBlockHeaderOuterHeight - blockTopicsBlockContentHeightDifference);
	var outerHeight = parseInt(regionSideHeightDifference + blockTopicsHeightDifference + blockSocialHeightDifference);
	var maxBlockTopicsBlockContentHeight = parseInt(viewportHeight - outerHeight - blockSocialHeight - blockTopicsBlockHeaderOuterHeight - blockTopicsBlockContentHeightDifference);
	if (maxBlockTopicsBlockContentHeight < blockTopicsFirstListItemOuterHeight) {
		maxBlockTopicsBlockContentHeight = blockTopicsFirstListItemOuterHeight;
	}

	// Set the topics block height
	jQuery('#region-side .block.block-topics:first .block-content').css({
		'max-height' : maxBlockTopicsBlockContentHeight+'px'
	});

	// Initialize variables
	var listItemLength = regionSide.find('.block.block-topics:first .block-content ul li').length;
	var blockContentHeight = blockTopicsFirstListItemOuterHeight;

	// Cycle through the remaining list items
	for (var x = 1; x < listItemLength; x++) {

		// Calculate the list item height
		var listItem = regionSide.find('.block.block-topics:first .block-content ul li').eq(x);
		var listItemHeight = parseInt(listItem.height());
		var listItemOuterHeight = parseInt(listItem.outerHeight(true));
		var listItemHeightDifference = parseInt(listItemOuterHeight - listItemHeight);

		// Check if the current list item fits inside the max block height
		if ( (blockContentHeight + listItemOuterHeight) <= maxBlockTopicsBlockContentHeight) {
			blockContentHeight += listItemOuterHeight;
		}

	}

	// Set the topics block height
	jQuery('#region-side .block.block-topics:first .block-content').css({
		'max-height' : blockContentHeight+'px'
	});

}

// Load hot topics
function loadHotTopics() {
	// Ajax
	jQuery.ajax({
		url: 'http://www.cinemablend.com/listHot.json',
		type: 'GET',
		beforeSend: function(){}
	}).done(function(data, textStatus, jqXHR) {
		if (data.code == 0) {
			jQuery('#region-side .block.block-topics').fadeIn(200);
			jQuery('#region-side .block.block-topics .block-content').append('<ul></ul>');
			var length = data.response.length;
			for (var x = 0; x < 10; x++) {
				var url = data.response[x].link;
				var title = data.response[x].title;
				var clean_title = data.response[x].clean_title;
				var html = '';
				var type = (x == 0 ? 'main' : 'sub');
				html += '<a href="'+url+'" title="'+clean_title+'">';
					html += '<div class="image">';
					html += '</div>';
					html += '<div class="content">';
						html += '<div class="title"><h3>'+title+'</h3></div>';
					html += '</div>';
				html += '</a>';
				jQuery('#region-side .block.block-topics .block-content ul').append('<li class="'+type+' clearfix">'+html+'</li>');
				if (x == 0) {
					getHotTopicImage({
						url: url,
						element: jQuery('#region-side .block.block-topics .block-content ul li').eq(x),
						type: 'main',
						width: '300',
						height: '200'
					});
				} else {
					getHotTopicImage({
						url: url,
						element: jQuery('#region-side .block.block-topics .block-content ul li').eq(x),
						type: 'sub',
						width: '100',
						height: '100'
					});
				}
			}

			// Resize the sidebar
			resizeSidebar();
		}
	}).fail(function(jqXHR, textStatus, errorThrown) {
		alert(textStatus+' - '+errorThrown);
	});
}

// Load hot topics
function getHotTopicImage(args) {
	// Ajax
	jQuery.ajax({
		url: 'http://www.cinemablend.com/front-ajax.php',
		type: 'POST',
		dataType: 'json',
		data: {
			url: args.url,
			type: args.type,
			width: args.width,
			height: args.height
		},
		beforeSend: function() {
		}
	}).done(function(data, textStatus, jqXHR) {
		if (data.response == 'success') {
			args.element.find('.image').append(data.html);
		}
	}).fail(function(jqXHR, textStatus, errorThrown) {
		//console.log(textStatus+' - '+errorThrown);
		//console.log(jqXHR.textStatus);
	});
}

// Social media counts
function socialMediaCounters() {
	// Ajax (Facebook)
	jQuery.ajax({
		url: 'https://graph.facebook.com/?ids=https://www.facebook.com/cinemablendcom',
		type: 'GET',
		dataType: 'jsonp',
		beforeSend: function() {
		}
	}).done(function(data, textStatus, jqXHR) {
		var count = data['https://www.facebook.com/cinemablendcom'].likes;
		count = formatSocialCounter(count);
		jQuery('#region-side .block.block-social .block-content ul li.facebook .count').html(count);
	});

	// Twitter
	var apikey='Y5oTKd5iaWxt9qV6UF8lkh3aM';
	var twitterid='15770846';  
    jQuery.ajax({
		url: "http://www.cinemablend.com/twitter_followers.php",
		type: 'GET',
		beforeSend: function() {
		}
	}).done(function(data, textStatus, jqXHR) {
		console.log(data);
		
		var count = data;
		count = formatSocialCounter(count);
		jQuery('#region-side .block.block-social .block-content ul li.twitter .count').html(count);
	});
	// google
	var profileid = '117300777613266559308';
	var apikey = 'AIzaSyDslTXfCvMs4RY4tZeenA93tQg-EMdrJCU';
	var url = 'https://www.googleapis.com/plus/v1/people/' + profileid + '?key=' + apikey;
    jQuery.ajax({
		url: url,
		type: 'GET',
		dataType: 'json',
		beforeSend: function() {
		}
	}).done(function(data, textStatus, jqXHR) {
		var count = data.circledByCount;
		count = formatSocialCounter(count);
		jQuery('#region-side .block.block-social .block-content ul li.googleplus .count').html(count);
	});
}

// Format social counters
function formatSocialCounter(number) {
	// Counter is greater than 1M
	if (number >= 1000000) {
		number = (Math.floor(number/100000) / 10) + 'M';
	// Counter is greater than 1K
	} else if (number >= 1000) {
		number = (Math.floor(number/1000)) + 'K';
	// Counter is less than than 1K
	} else {
		number = number;
	}

	// Return the formatted counter
	return number;
}
