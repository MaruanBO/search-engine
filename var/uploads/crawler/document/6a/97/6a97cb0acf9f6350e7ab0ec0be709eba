<script type="text/javascript" src="http://s3.amazonaws.com/s3.boldprogressives.org/wp-content/themes/premise/ak/jquery.embedly.min.js.gzip"></script>
<script type="text/javascript">
  validators = {};
  validators.email = function(value) {
    if ( !/^\s*\S+@\S+\.\S+\s*$/.test(value) )
        return false
    return true;
  };

  validators.zip = function(value) {
    if ( !/\d{5}/.test(value) )
        return false;
    return true;
  };	  
  var validate = function() {
    var valid = true;
    var val = $("#actionkit-signup-form input[name=email]").val();
    if( !val || !validators.email(val) ) {
      $("#actionkit-signup-form input[name=email]").css("background-color", "pink");
      valid = false;
    } else {
      $("#actionkit-signup-form input[name=email]").css("background-color", "white");
    }
    var val = $("#actionkit-signup-form input[name=zip]").val();
    if( !val || !validators.zip(val) ) {
      $("#actionkit-signup-form input[name=zip]").css("background-color", "pink");
      valid = false;
    } else {
      $("#actionkit-signup-form input[name=zip]").css("background-color", "white");
    }
    if( !valid ) return false;
  };
  $("#actionkit-signup-form").on("submit", validate);
  $("#actionkit-signup-form input[type=submit]").on("click", validate);

  window.formatDate = function(datestring) {

  var zeroPadded = function(n) {
    return n < 10 ? "0" + n : "" + n;
  };
	     
	     var last_updated = new Date(datestring) // e.g. "Feb. 13, 2013, 03:39:14 AM", assumed to be UTC
	     last_updated.setMinutes(last_updated.getMinutes() - last_updated.getTimezoneOffset());
	     var months = ["Jan.", "Feb.", "Mar.", "Apr.", "May", "Jun.",
	     "Jul.", "Aug.", "Sep.", "Oct.", "Nov.", "Dec."];
	     last_updated = months[last_updated.getMonth()] + " " + last_updated.getDate()
	     + ", " + (last_updated.getYear() + 1900) + ", "
	     + (last_updated.getHours() > 12 ? last_updated.getHours() - 12 : last_updated.getHours()) 
    + ":" + zeroPadded(last_updated.getMinutes())
    + (last_updated.getHours() > 11 ? " PM" : " AM");
    return last_updated;
    }
    $("span.time a").each(function() { 
      var gmt = $(this).text();
      var localeTime = formatDate(gmt);
      $(this).attr("title", localeTime).text(humaneDate(localeTime) || localeTime);
    });
</script>
<script type="text/javascript">
    window.tweetList = [];
    window.newestVisibleTweetId = null;
    function reloadTweetList(callback) {
      $.getJSON("/tweets/", function(data) { 
        var tweetListIds = $.map(tweetList, function(i) { return i.id_str });
        for( var i=data.length; i>0; --i ) {
          var tweet = data[i-1];
          if( $.inArray(tweet.id_str, tweetListIds) != -1 ) {
            continue;
          }
          tweetList.unshift(tweet);
        }
        callback && callback();
      });
    };
    function cycleTweets() {
      var tweetListIds = $.map(tweetList, function(i) { return i.id_str });
      var nextTweetIndex;
      for( var i=tweetListIds.length; i>0; --i ) {
        if( newestVisibleTweetId == tweetListIds[i-1] ) {
          nextTweetIndex = i-2;
          if( nextTweetIndex < 0 ) { nextTweetIndex = tweetListIds.length-1; }
          break;
        }
      }
      if( nextTweetIndex === undefined ) {
        nextTweetIndex = tweetListIds.length-6;
        if( nextTweetIndex  < 0 ) nextTweetIndex = tweetListIds.length-1;
      }
      newestVisibleTweetId = tweetList[nextTweetIndex].id_str;
      return tweetList[nextTweetIndex];
    };
    function buildTweetDiv(tweet) {
      var div = '<div class="tweet"><div class="tcont"><p>';
      if( tweet.user && tweet.user.screen_name ) {
        div +=  'From <a href="http://twitter.com/' + tweet.user.screen_name + '">@' + tweet.user.screen_name + '</a>: ';
      }
      if( tweet.text ) {
        var tweet_text = tweet.text;
        if( tweet.entities && tweet.entities.user_mentions ) {
          var mentions = tweet.entities.user_mentions;
          for( var i=0; i<mentions.length; ++i ) {
            tweet_text = tweet_text.replace(RegExp("@" + mentions[i].screen_name, "g"), "<a href='https://twitter.com/" + mentions[i].screen_name + "' target='_blank'>@" + mentions[i].screen_name + "</a>");
          }
        }
        if( tweet.entities && tweet.entities.hashtags ) {
          var hashtags = tweet.entities.hashtags;
          for( var i=0; i<hashtags.length; ++i ) {
            tweet_text = tweet_text.replace(RegExp("#" + hashtags[i].text, "g"), "<a href='https://twitter.com/search?q=%23" + hashtags[i].text + "' target='_blank'>#" + hashtags[i].text + "</a>");
          }
        } 
        if( tweet.entities && tweet.entities.urls ) {
          var urls = tweet.entities.urls;
          for( var i=0; i<urls.length; ++i ) {
            tweet_text = tweet_text.replace(RegExp(urls[i].url, "g"), "<a href='" + urls[i].expanded_url + "' target='_blank' rel='nofollow'>" + urls[i].url + "</a>");
          }
        } 
        div += tweet_text;
      }
      div += '</p></div>';
      var localeTime = formatDate(tweet.created_at);
      div += '<div class="tact"><span class="time"><a target="_blank" href="http://twitter.com/' + tweet.user.screen_name + '/status/' + tweet.id_str + '" title="' + localeTime + '">' + (humaneDate(localeTime) || localeTime) + '</a></span><span class="reply"><a target="_blank" href="https://twitter.com/intent/tweet?in_reply_to=' + tweet.id_str + '">Reply</a></span><span class="retweet"><a target="_blank" href="https://twitter.com/intent/retweet?tweet_id=' + tweet.id_str + '">Retweet</a></span><span class="favorite"><a target="_blank" href="https://twitter.com/intent/favorite?tweet_id=' + tweet.id_str + '">Favorite</a></span></div></div>';

      $(div).hide().prependTo("#tweet_stream").slideDown();
      var tweetstream = $("#tweet_stream div.tweet");
      for( var i=5; i<tweetstream.length; ++i ) {
        $(tweetstream[i]).fadeOut().remove();
      }
    };
    reloadTweetList(function() { 
      for( var i=0; i<5; ++i ) {
        buildTweetDiv(cycleTweets());
      }
      enableScrollbar();
    });
    window.setInterval(function() { buildTweetDiv(cycleTweets()); }, 8000);
</script>

