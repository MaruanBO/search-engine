<script>
  function mobileRedirect() {
    var ua = navigator.userAgent.toLowerCase();
    if (!ua.match(/ip(?:od|hone)/) && !ua.match(/(?:webos|android)/)) {
      return false;
    }
    if (window.location.search.match(/fullsite=1/)) {
      Cookie.write('prevent_mobile_redirect', 1, {
        domain: 'moma.org',
        path: '/'
      });
      return false;
    }
    if (Cookie.read('prevent_mobile_redirect') == "1") {
      return false;
    }
    window.location = '/m';
    return true;
  }
  mobileRedirect();
</script>

