<script type="text/javascript">
    function setUpdateAction(btnID) {        
		var buttonID = btnID.split('_');
		switch (buttonID[1]) {
		    case "ucTopSellers":
		        $("#update-action").text("TopSellersCarousel");		        
		        break;
			case "ucNewArrivals":
				$("#update-action").text("NewReleasesCarousel");
				break;
			case "ucEditorsPicks":
				$("#update-action").text("EditorsPicksCarousel");
				break;
			case "ucArtistsYouMayKnow":
				$("#update-action").text("ArtistsYouMayKnowCarousel");
				break;
			case "ucEditorsPicksSub":
				$("#update-action").text("EditorsPicksSubCarousel");
				break;
			case "ucNewArrivalsSub":
				$("#update-action").text("NewReleasesSubCarousel");
				break;
			case "ucTopSellersSub":
				$("#update-action").text("TopSellersSubCarousel");
				break;
			case "ucStaffPicksSub":
				$("#update-action").text("StaffPicksSubCarousel");
				break;
            case "ucArtistsYouMayKnowSub":
                $("#update-action").text("ArtistsYouMayKnowSubCarousel");
            break;
            case "album-pager":
                $("#update-action").text("AlbumPager");
                break;
		}
	}

    // ELEMENTS:
    //  rptFormats contains the real buy buttons (they are hidden)
    //  rptFormatsClones contains fake buy buttons for tooltip display
    // ACTION: 
    //  The tooltip plugin copies the contents of the .tooltip div and diplays accordingly
    //  Clicking the fake tooltip button will trigger the click of the real button using the chained call below
    //  The real button is found using the copied ID and the location of the button contained in the "real-buy-buttons" span
    function triggerBuyButtonClick(buttonClone) {
    	setUpdateAction(buttonClone.attr("id"));
    	$(".carousel-album-container")            
            .has("#" + buttonClone.attr("id"))
            .find(".real-buy-buttons input[type='submit'][value='" + buttonClone.val() + "']")
            .click();
        return false;

    }   // triggerBuyButtonClick()
   
</script>
				                </div>
			                
			                	
		            </div><!-- #new-arrivals-carousel-container -->
	            
</div>
			</div>
            <div id="new-arrivals-right" class="carousel-right"></div>
        </div><!-- #new-arrivals-carousel -->

