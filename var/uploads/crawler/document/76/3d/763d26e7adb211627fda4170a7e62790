    	function UpdateSearchField(terms) {
			// Encode search terms
    		$('#ucNav_hfSearch').val(encodeURI(terms));

    		if ($('.search-input').val() == 'type your search here...' || $('.search-input').val() == '')
    		{
    			return false;
    		};
    	}	// UpdateSearchField(terms)
		
