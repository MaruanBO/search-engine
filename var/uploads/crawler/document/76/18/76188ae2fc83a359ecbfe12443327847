				<script type="text/javascript">

                    var timerIDHP = null;
                    var timerRunningHP = false;

                    function startclockHP() {
                        stopclockHP();
                        showtimeHP();
                    }

                    function stopclockHP() {
                        if(timerRunningHP)
                            window.clearTimeout(timerIDHP);
                            timerRunningHP = false;
                    }

                // first date is start time with 00:00:00 being midnight
                // second date is end time

                    function gettimeHP(timerstart,timerstop) {
                        var dateHP = new Date();
                       // 24-hour clock goes to 23:59:59

                       var startdateHP=new Date(timerstart + " 00:00:00");
                       var enddateHP = new Date(timerstop + " 23:59:59");

		                if ((dateHP>enddateHP)||(dateHP<startdateHP)) {
		                    document.clockHP.countdownHP.value ="";
		                    timerIDHP = window.setTimeout("gettimeHP(timerstart,timerstop)", 100);
							timerRunningHP = true;
			                return;
		                }
		                var diffHP = Date.parse(enddateHP) - dateHP.getTime();
		                diffHP = Math.floor(diffHP / 1000);
		                var secondsHP = diffHP % 60;
		                var secdescHP = (secondsHP == 1) ? ' sec' : ' sec';
		                var minutesHP = Math.floor((diffHP % 3600)/60);
		                var mindescHP = (minutesHP == 1) ? ' min, ' : ' min, ';
		                var hoursHP   = Math.floor((diffHP % 86400)/3600);
		                var hourdescHP = (hoursHP == 1) ? ' hr, ' : ' hrs, ';
		                var daysHP    = Math.floor(diffHP / 86400);
		                var daydescHP = (daysHP == 1) ? ' day, ' : ' days, ';


                		if (daysHP>0){
						  document.clockHP.countdownHP.value = daysHP  + daydescHP
														+ hoursHP + hourdescHP
														+ minutesHP+ mindescHP
														+ secondsHP+ secdescHP;
						}
						if (daysHP<=0){
						  document.clockHP.countdownHP.value = hoursHP + hourdescHP
														+ minutesHP+ mindescHP
														+ secondsHP+ secdescHP;
						}


                		if ((daysHP<=0)&&(hoursHP<=0)){
						  document.clockHP.countdownHP.value = minutesHP+ mindescHP
														+ secondsHP+ secdescHP;
						}
						if ((daysHP<=0)&&(hoursHP<=0)&&(minutesHP<=0)){
						  document.clockHP.countdownHP.value = secondsHP+ secdescHP;
						}
				        if ((daysHP<=0)&&(hoursHP<=0)&&(minutesHP<=0)&&(secondsHP<=0)){
						  document.clockHP.countdownHP.value = " ";
						}

	                  timerIDHP = window.setTimeout("gettimeHP(timerstart,timerstop)", 100);
	                  timerRunningHP = true;

	                 }

                </SCRIPT>
				<!--  image buttons 1-5 -->
				<div class="hp_rotation_buttons">


					
