<script LANGUAGE="javascript">

var g_cGif = 24;
var g_ixGif = 1;
var g_rgGif = new Array(g_cGif);
var g_cLoops = 1; //old
var g_cLoopsMax = 300; //old

function UpdateGif()
{
		imgAnim = document.imageswitch.AImg;
		imgAnim.src = g_rgGif[g_ixGif%g_cGif].src;
		imgAnim.alt = g_rgGif[g_ixGif%g_cGif].alt;
		g_ixGif++;
		setTimeout("UpdateGif()", 500);
}

function SubmitFormSP(doc)
{
	for (i = 1 ; i <= 24 ; i++)
	{
		g_rgGif[i - 1] = document.createElement("IMG");
		g_rgGif[i - 1].src = "/images_unique/cruisesonly/wait/CO_wait-" + i + ".gif";
		g_rgGif[i - 1].alt = "Please wait...";
	}
		UpdateGif();
}


</script>


