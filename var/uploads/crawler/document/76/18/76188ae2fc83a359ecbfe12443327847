<script type="text/javascript">
function changeDepartureMonth(monthDropdown) {
    var index  = monthDropdown.selectedIndex;
    var value = monthDropdown.options[index].value;

  if (document.getElementById("DepartureDay")!=null)  {
    var dayDropdown = document.getElementById("DepartureDay");
    populateDepartureDays(value, dayDropdown);
    showOrHide(dayDropdown);
  }
}
function prepopuplateDepartureDaysDropdown()  {
    var mDropdown= document.getElementById("DepartureMonth");
    changeDepartureMonth(mDropdown);
}

function removeAllOptions(selectbox)
{
    var i;
    for(i=selectbox.options.length-1;i>=0;i--)
    {
      selectbox.remove(i);
    }
}

function addOption(selectbox,text,value, selected )
{
    var optn = document.createElement("OPTION");
    optn.text = text;
    optn.value = value;
    selectbox.options.add(optn);
    if ( selected == 'true' ) {
         optn.selected = true;
    }
}


function daysInMonth(Month, Year)
 {
     return 32 - new Date(Year, Month, 32).getDate();
 }


function populateDepartureDays(date, dropdown)
{
   var today = new Date();
   var currentYear = today.getFullYear();
   var currentMonth = today.getMonth();
   var currentDay = today.getDate();
   removeAllOptions(dropdown);
   addOption(dropdown, 'Any', 'ALL', 'true');
   if (date != 'ALL')  {
       var mdy = date.split("/");
       var m = mdy[0] - 1;
       var y = mdy[2];
       var days = daysInMonth(m, y);
       var cday =1;
      if (dd == null || dd == '' || dd == 'Any') {
		   var dd = 0; }
	   else {
          var dd = 0;
       }
       if (y == currentYear && m == currentMonth) {
           cday = currentDay;
       }
       for (var i=cday; i <= days;i++)  {
              if (i == dd) {
                  addOption(dropdown, i, i, 'true');
              } else {
                  addOption(dropdown, i, i, 'false');
              }
       }
   }
}


var searchFormName='SearchForm';

/**
 * handles GA tracking for all search elements
 * usually fired from onChange of these elements
 */
function handleSearchEvent(formName, elementName, gaEventName)
{

	var dropValue;
	var formDOM;
	var refineName;
	formDOM = document.getElementById(formName);
	dropValue = $('#'+elementName).val();
	refineName = "DynamicRefine";

	/**
	If the changed element is month, we need to get the window- difference between current month and month selected
	rather than the actual month
	**/
	if(elementName=='Month'){
		dropValue= getDepartureMonthWindow(dropValue);
	}
	var category = "HomePage/" + refineName
}

function getDepartureMonthWindow(dropValue){

	var day1, day2;
	var month1, month2;
	var year1, year2;
	var value1, value2;
	var date1, date2;
	var firstDate, secondDate;
	var msPerDay, dbd;

	value1 = dropValue;
	value2 = new Date();

	day1 = value1.substring (0, value1.indexOf ("/"));
	month1 = value1.substring (value1.indexOf ("/")+1, value1.lastIndexOf ("/"));
	year1 = value1.substring (value1.lastIndexOf ("/")+1, value1.length);

	day2 = value2.getUTCDate();
	month2 = value2.getUTCMonth() + 1;
	year2 = value2.getUTCFullYear();

	date1 = year1+"/"+day1+"/"+month1;
	date2 = year2+"/"+month2+"/"+day2;

	firstDate = Date.parse(date1);
	secondDate= Date.parse(date2);

	msPerDay = 24 * 60 * 60 * 1000;
	dbd = Math.round((firstDate.valueOf()-secondDate.valueOf())/ (msPerDay*30));
	return  dbd.toString();
}
function showOrHide(dropdown) {
if(dropdown){

   var txt = dropdown.options[dropdown.selectedIndex].value;
   var flexLabel = document.getElementById('FlexLabel');

   if ( txt.match('Any') ) {
       flexLabel.style.display = 'block';
   }else {
       flexLabel.style.display = 'none';
   }
   calculateDepartureDates();
   }
}

function calculateDepartureDates()
{
    var monthDropdown =  document.getElementById("DepartureMonth");
    var dayDropdown = document.getElementById("DepartureDay");
    var flexDropdown = document.getElementById("FlexibleDays");
    var departureStartDate = document.getElementById('DepartureStartDate');
    var departureEndDate = document.getElementById('DepartureEndDate');
    var dDay = dayDropdown.options[dayDropdown.selectedIndex].value;

    if ( dDay.match('Any') ) {
         departureStartDate.value = '';
         departureEndDate.value = '';
    } else {
         var dMonth= monthDropdown.options[monthDropdown.selectedIndex].value;
         var flex = flexDropdown.options[flexDropdown.selectedIndex].value;
         var mdy = dMonth.split("/");
         var m = mdy[0] - 1;
         var y = mdy[2];
		 var dDate = new Date(y, m, dDay);
         var flexDays = 0;
         if (flex.match('1')) { flexDays = 1;}
         if (flex.match('2')) { flexDays = 3;}
         if (flex.match('3')) { flexDays = 7;}
         if (flex.match('4')) { flexDays = 14;}
         var startDate = new Date(dDate);
         var endDate = new Date(dDate);
         startDate.setDate(startDate.getDate() - flexDays);
         endDate.setDate(endDate.getDate() + flexDays);
         departureStartDate.value = (startDate.getMonth()+1) + '/' + startDate.getDate() + '/' + startDate.getFullYear();
         departureEndDate.value = (endDate.getMonth()+1) + '/' + endDate.getDate() + '/' + endDate.getFullYear();
    }

}

</script>


