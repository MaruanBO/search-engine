// this function gets the destination name the user changes to in the widget on the results page and sends it to GA
function getValueDropdown(formName)
{
	var dropValue;
	var formDOM;
	var refineName;
	formDOM = document.getElementById(formName);
	dropValue = formDOM.places.options[formDOM.places.selectedIndex].text;
	refineName = "DynamicRefine";
	var category = "HomePage/" + refineName
	CallTracker(category, 'Destination', dropValue );
}

// this function gets the amount of days out the user chooses in the main widget on the results page and sends it to GA
function getValueDropdown2(formName)
{
	var dropValue;
	var formDOM;
	var refineName;
	formDOM = document.getElementById(formName);
	dropValue = formDOM.Month.options[formDOM.Month.selectedIndex].value;
	refineName = "DynamicRefine";

var day1, day2;
var month1, month2;
var year1, year2;
var value1, value2;
var date1, date2;
var firstDate, secondDate;
var msPerDay, dbd;

value1 = dropValue;
value2 = new Date();

day1 = value1.substring (0, value1.indexOf ("/"));
month1 = value1.substring (value1.indexOf ("/")+1, value1.lastIndexOf ("/"));
year1 = value1.substring (value1.lastIndexOf ("/")+1, value1.length);

day2 = value2.getUTCDate();
month2 = value2.getUTCMonth() + 1;
year2 = value2.getUTCFullYear();

date1 = year1+"/"+day1+"/"+month1;
date2 = year2+"/"+month2+"/"+day2;

firstDate = Date.parse(date1);
secondDate= Date.parse(date2);

msPerDay = 24 * 60 * 60 * 1000;
dbd = Math.round((firstDate.valueOf()-secondDate.valueOf())/ (msPerDay*30));

var category = "HomePage/" + refineName
CallTracker(category, 'DepartureWindowMonths', dbd.toString() );
}

