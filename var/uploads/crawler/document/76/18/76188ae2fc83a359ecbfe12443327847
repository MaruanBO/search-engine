	<script language="javascript">
	(function startList() {

		if (document.getElementById){
			var navRoot=document.getElementById("nav");

			if(navRoot!=null){
				for(i=0;i<navRoot.childNodes.length;i++){
					var node=navRoot.childNodes[i];
					//if(node.nodeName=="LI"){
						node.onmouseover=function(){this.className+=" over";}
						node.onmouseout=function(){this.className=this.className.replace(" over","");}

					//}
				}
			}
		}
	})();
	</script>

	


<script language="JavaScript">
   
    
	
    
    function CallUrchin(event_name)
    {
		
			pageTracker._trackPageview(event_name);
			//alert(event_name);
		
		return true;
    }
    
    function CallTracker(cat, cat2, value)
    {
    		pageTracker._trackEvent(cat, cat2, value);
			//alert(cat2);
		return true;
    }
     function setGACustomVar(slot,varName,varValue,scope) {

      pageTracker._setCustomVar(slot,varName,varValue,scope); 

    }  

</script>


