	<!-- floating masthead script -->
	

		 <script type="text/javascript">
			document.getElementById("floatinghdr").style.display="none";
			 window.onscroll = scroll
			 function scroll(){
				 // called when the window is scrolled.
				var ScrollTop = document.body.scrollTop || document.documentElement.scrollTop;
				var ScreenWidth = document.body.offsetWidth;
				if ((ScrollTop > 114) && (ScreenWidth > 991)){
					document.getElementById("floatinghdr").style.display="block";
					document.getElementById("floatinghdr").style.visibility="visible";
				}
				else{
					document.getElementById("floatinghdr").style.display="none";
					document.getElementById("floatinghdr").style.visibility="hidden";
				 }
			  }
		</script>
	
<!-- Bootstrap core JavaScript:Placed at the end of the document so the pages load faster -->
