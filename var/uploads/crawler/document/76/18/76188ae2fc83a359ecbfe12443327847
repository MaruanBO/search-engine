						(function(){
							var PlayPauseIMG = document.getElementById('imageplaypause'),
							IMGLink = document.getElementById('imagelink'),
							images = [
								['/images_unique/cruisesonly/header-resp/player/player_pause-off.png','javascript:SLIDES.next();SLIDES.play()'],
								['/images_unique/cruisesonly/header-resp/player/player_play-off.png','javascript:SLIDES.pause()']
							];
							PlayPauseIMG.onclick = function(){
								var nextImgIndex = (this.src.indexOf(images[0][0]) > -1) ? 1 : 0;
								this.src = images[nextImgIndex][0];
								IMGLink.href = images[nextImgIndex][1];
							}
						})();

