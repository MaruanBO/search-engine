function submitForm(){
	// if called from the cruise search or vacation search
	var pi_zipVal = document.getElementById("shoppingZipCode").value
	if(isValidZip(pi_zipVal)) {
		hide_inlinemsg('errShowZipMsg');
		return true;
	}else{
		display_inlinemsg('errShowZipMsg');
		return false;
	}
}

