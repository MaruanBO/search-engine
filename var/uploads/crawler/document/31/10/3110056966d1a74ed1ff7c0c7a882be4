<script type="text/javascript">

if (!dojo.cookie("VistorIdentification")){
      var d = new Date();
      var dateAsNumber = d.getDate() + ((d.getMonth() + 1) * 100) + (d.getFullYear() * 10000);
      var timeAsNumber = (d.getMilliseconds() + 1) + (d.getMinutes() * 10000) + ((d.getHours()) * 1000000);       if (timeAsNumber < 100000){
          timeAsNumber = "000" + timeAsNumber;
        }
      else if (timeAsNumber < 1000000){
          timeAsNumber = "00" + timeAsNumber;                                                              		}
      else if (timeAsNumber < 10000000){
          timeAsNumber = "0" + timeAsNumber;                                                                	}
      var rand = Math.floor(Math.random()*1000);
      if (rand < 10){
          rand = "00" + rand;         
        }
      else if (rand < 100){
          rand = "0" + rand;          
        }
      var vid = dateAsNumber + "-" + timeAsNumber + "-" + rand;
      dojo.cookie("VistorIdentification", vid, {expires: 9999, path: '/'});
}
