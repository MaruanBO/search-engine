<!-- Scripts -->



<script type="text/javascript">
var prevent_open = false;

function validateEmail(email) { 
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
} 
function open_ad_mod() {
	if(! prevent_open)
		$('#ad-test').modal('show');
}
    $(window).load(function(){
        setTimeout("open_ad_mod();",10000);
		$('.button').click( function() { prevent_open = true; });
		
		$('.modal-ad-btn').click( function() {
			var test = $('#modal-ad-email').val();
			if(! validateEmail(test))
			{
				alert('Please enter a valid email address');
				return;
			}
			$.ajax({ url: '/ajax/hma.php', type: "POST", data: { ad_email: $('#modal-ad-email').val(), ad_id: $('#modal-ad-id').val() } });
			$('#pop-form').hide();
			$('#modal-main').html('Thanks, check your email');
			$('#modal-sub').hide();
			 setTimeout("$('#ad-test').modal('hide');",2500);
		});
    });
</script>

