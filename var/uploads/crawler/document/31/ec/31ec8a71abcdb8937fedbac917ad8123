      	$(".preview-widget").toggleClass("visible").toggleClass("invisible");
      	$(".preview-popup").toggleClass("visible").toggleClass("invisible");
  	});
  	$(".img-widget").hover(function() { 
      	$(".preview-full").toggleClass("visible").toggleClass("invisible");
      	$(".preview-popup").toggleClass("visible").toggleClass("invisible");
  	});
  	$(".img-popup").hover(function() { 
      	$(".preview-full").toggleClass("visible").toggleClass("invisible");
      	$(".preview-widget").toggleClass("visible").toggleClass("invisible");
  	});
  	$(".preview-full").hover(function() { 
      	$(".preview-full .preview-text").toggleClass("hidden").toggleClass("shown");
  	});
  	$(".preview-widget").hover(function() { 
      	$(".preview-widget .preview-text").toggleClass("hidden").toggleClass("shown");
  	});
  	$(".preview-popup").hover(function() { 
      	$(".preview-popup .preview-text").toggleClass("hidden").toggleClass("shown");
  	});
  	$(".preview-full .preview-text").hover(function() { 
      	$(".preview-widget").toggleClass("visible").toggleClass("invisible");
      	$(".preview-popup").toggleClass("visible").toggleClass("invisible");
  	});
  	$(".preview-widget .preview-text").hover(function() { 
      	$(".preview-full").toggleClass("visible").toggleClass("invisible");
      	$(".preview-popup").toggleClass("visible").toggleClass("invisible");
  	});
  	$(".preview-popup .preview-text").hover(function() { 
      	$(".preview-full").toggleClass("visible").toggleClass("invisible");
      	$(".preview-widget").toggleClass("visible").toggleClass("invisible");
  	});
	$('.forgot-password').click( function() {
		$('.btn-login').hide();
		$('.title-login').hide();
		$('.title-login').html('Forgot Password?');
		$('.title-login').fadeIn();
		$('.forgot-password').fadeOut();
		$('#password').fadeOut();
		$('#postTypeLogin').val('forgot');
		$('.btn-forgot').fadeIn();
	});
	$('.btn-reset-login').click( function() {
		$('.btn-login').show();
		$('.title-login').html('Log In');
		$('.forgot-password').show();
		$('#password').show();
		$('#userEmail').focus();
		$('#postTypeLogin').val('login');
		$('.btn-forgot').hide();
	});
	$('a#inpage').click(function(){
    $('html, body').animate({
	        scrollTop: $( $(this).attr('href') ).offset().top
	    }, 500);
	    return false;
	});
	
	$(document).ready( function() {
	    $('.pricing-monthly').click( function() {
		  $(this).addClass('selected');	
		  $('.pricing-annual').removeClass('selected').addClass('no-sel');
		  $('#pricing-toggle').removeClass('do-annual');	
		});
		
		$('.pricing-annual').click( function() {
		  $(this).addClass('selected');	
		  $('.pricing-monthly').removeClass('selected').addClass('no-sel');
		  $('#pricing-toggle').addClass('do-annual');	
		});

		$('#demo-two .quotes').quovolver({
			children : 'li',
			transitionSpeed : 250,
			autoPlay : true,
			autoPlaySpeed : 7500,
			equalHeight : false,
			navPosition : 'above',
			navPrev     : false,
			navNext     : false,
			navNum      : true,
			navText     : false,
		});
		
	});
    </script>
    <script type="text/javascript">
	    $('.pricing-tooltip').tooltip();
	</script>
