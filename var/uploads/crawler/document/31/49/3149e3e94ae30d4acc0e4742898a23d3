        (function (ng, tc, omniture, TCTrack) {'use strict';

            ng.module('tcdc.search.services')
                .constant('IMAGE_SERVICE_URL', '//a.tcimg.net/v/')
                .constant('USED_CAR_HOST', 'https://usedcars.truecar.com/')
                .constant('SEARCH_INITIAL_ACTIVE_SEARCH', 'new')
                .constant('PAGE_INITIAL_MAKE_ID', tc.s.makeId || false);

            var zipData = {
                zipcode: '22368',
                city: '',
                state: '',
                display_city_state: '22368',
                using_default_zipcode: true,
                aaa_fulfillment: false,
                supports_carvana: false
            };

            ng.module('truecar.zipcode.services').run(function (tcZip) {
                tcZip.data(zipData);
            });

            ng.module('tcdc.sellMyCar.services')
                .config(function(sellMyCarServiceProvider){
                    sellMyCarServiceProvider
                        .configure({
                            url: '/user/smc.ajax'
                        })
                        .setInitialPhoneNumber('');
                });

            //
            ng.module('truecar.tracking')
                .config(function(trackingServiceProvider, omnitureTrackingServiceProvider, tctrackTrackingServiceProvider){
                    //
                    var trackers = ['omniture'];
                    tctrackTrackingServiceProvider.setInstance(TCTrack);
                        trackers.push('tctrack');
                    omnitureTrackingServiceProvider.setInstance(omniture);
                    //
                    omnitureTrackingServiceProvider.setOmnitureVars({
                        'prop4': zipData.zipcode
                    });
                    //
                    trackingServiceProvider.setPageChangeEvent(false);
                    //
                    trackingServiceProvider.registerTrackers(trackers);
                });

        })(angular, TrueCar, s, TCTrack);
    </script>
    
