<script type="text/javascript">
$(document).ready(function() {
 
            var nextDrawTimeLDL = calcNextDrawTimeLDL();
            $("#nextDrawTimeLDL").html(nextDrawTimeLDL);
       
        

});
 function calcNextDrawTimeLDL() {
            //alert("here in calcNextDrawTime");
            var d = new Date();
            var yhour = d.getHours();
            var yminute = d.getMinutes();
            var nextDrawTimeLDL = "";

            if (yminute <= 9) {
              yminute = 0 + '' + yminute;
            }
            
            if (yhour == 0) {
              yhour = '00';
            }
            
            var newtime = yhour + '' + yminute;
           
            /* count to today's 12:40 draw */
            if (newtime <= 1240) {
                nextDrawTimeLDL = "12:40 PM CST";
            }   
            /* count to today's 9:22 draw */
            else if (newtime > 1240 && newtime < 2122 ) {
                nextDrawTimeLDL = "9:22 PM CST";
            }
            /* count to tomorrow's 12:40 draw */
            else if (newtime >= 2122 ) {
                nextDrawTimeLDL = "12:40 PM CST";            
            }
            
            return nextDrawTimeLDL;
        }
        
 
</script></div>
</div>
