    <script>

      function getURLParameter(name) {
        return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null
      }

    
    var queryString = window.location.search;
    var stopMobileRedirect = false;

    //if the cookie exists set too true
    if (getCookie('deskTop')) {
    	stopMobileRedirect = true
    }

     
     
        

        //Do not redirect if we have the cookie to not redirect or if the query parameter was passed in
        if (queryString.indexOf("app=noRedirect") == -1 && stopMobileRedirect == false) {

        var urlhash = window.location.hash;                                                  // Store for multiple accesses
        var redirect_url = mobile_domain;                                                    // Mobile url
        var t_redirect_url = tablet_domain;                                                  // Tablet url
        var queryParameter = "";
        if (urlhash.lastIndexOf('?') > 0) {
        	queryParameter = urlhash.substring(urlhash.lastIndexOf('?'));                    // Get first parameter after a hash
        }
        var currentPathName = window.location.pathname;                                      // Store for multiple accersses
        var currentHref = window.location.href;                                              // Current url with parameters
        var sponsoredStoryParam = "";
        if (currentHref.indexOf('?') > 0) {
        	sponsoredStoryParam = currentHref.substring(currentHref.lastIndexOf('?'));  // Store all url params
        }

        if (urlhash.indexOf("#mystars/") >= 0) {
            if (urlhash.indexOf("password")>=0) {
                hash_path = "password";
            }
            else if (urlhash.indexOf("confirm")>=0) {
                hash_path = "confirm";
            }
            redirect_url = mobile_domain + '/#/' + hash_path + queryParameter;
            t_redirect_url = tablet_domain + '/#/' + hash_path + queryParameter;
        }
        else if (getURLParameter('confirmation_token') || getURLParameter('reset_password_token')) {
            redirect_url =  mobile_domain + "/#/settings";
            t_redirect_url =  tablet_domain + "/#/settings";
        }
        else if (currentPathName === "/" || currentPathName === "/content/thestar" || currentPathName === "/index.html") {
            redirect_url =  mobile_domain;
            t_redirect_url =  tablet_domain;
        }
        //if on an authors page go here
        else if (currentPathName.indexOf('authors.') !== -1 ) {

             var newpath = '';
             //remove content thestar if exists
             newPath = currentPathName.replace( /content\/thestar/,'');
             //change from /authors.cohn_martin_regg.html to http://mobile.smgdigitaldev.com/#/author/cohn_martin_regg.html
             newPath = newPath.replace( /\/authors./,'#/author/');

             redirect_url= mobile_domain + newPath + sponsoredStoryParam;
             t_redirect_url= tablet_domain + newPath + sponsoredStoryParam ;
        }
        else  {
            var path = window.location.pathname;
			
            
		    redirect_url= mobile_domain + '/#/index' + path + sponsoredStoryParam;
            t_redirect_url= tablet_domain + '/#/index' + path + sponsoredStoryParam;
      
        }
        SA.redirection_mobile ({
            mobile_url: redirect_url,
            tablet_url: t_redirect_url,
            mobile_disable: false,
            tablet_disable: false
        });
         }
        
    
    
    </script>


    
