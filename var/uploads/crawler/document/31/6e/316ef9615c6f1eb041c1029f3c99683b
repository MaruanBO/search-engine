function loadMainRoundedCorners() {}



var newExit = true;
var newExitSetup = false;

function attachExitConsole()
{
	var links = document.links;
	var forms = document.forms;
	for (i=0; i < links.length; i++)
		attachExitTrigger(links[i], 'click');

	for (i=0; i < forms.length; i++)
		attachExitTrigger(forms[i], 'submit');

	newExitSetup = false;
}

function attachExitTrigger(obj, trigger)
{
	if (obj.attachEvent)
		obj.attachEvent('on' + trigger, new Function('newExit=false;'));
	else if(obj.addEventListener)
		obj.addEventListener(trigger, new Function('newExit=false;'), false);
}

function runExitConsole()
{
	if (newExit && newExitSetup)
	{
		//exitWin = window.open('/exit/', 'newExitConsole');
		//exitWin.blur();
		//window.focus();
	}
}

if (window.attachEvent)
{
	window.attachEvent('onload', attachExitConsole);
	window.attachEvent('onbeforeunload', runExitConsole);
	window.attachEvent('onload', loadMainRoundedCorners);
}
else if(window.addEventListener)
{
	window.addEventListener("load", attachExitConsole, false);
	window.addEventListener("beforeunload", runExitConsole, false);
	window.addEventListener("load", loadMainRoundedCorners, false);
}

