<!--FED-8754-->
<script>
os.isCluboGrace = function() {
  var cluboExpDate = new Date(''),
    daysToExp = Math.round((cluboExpDate - new Date())/1000/60/60/24);

  if (daysToExp > -90 && daysToExp < 0) {
    return true;
  } else {
    return false;
  }
};
function runCluboGrace() {
  var cluboGraceCheck = os.isCluboGrace();

  if (cluboGraceCheck === true) {
      var cluboRenewalPrice = '$9.95',
          cluboRenewalText = 'Renew Club O Now for ' + cluboRenewalPrice
          cluboRenewalTextHdr = 'Renew Now';

      //Light Box
      jQuery('#joinCluboBtn').text(cluboRenewalText);

      //Static Page
      jQuery('.joinCluboBtn').text(cluboRenewalText).attr('href','http://www.overstock.com/cart?addqty654756=1&addid654756=1168750&TID=CLBOSGNUP:ADDCART:NEWCO');

      //Header
      jQuery('.hd-clubo .hd-top-title').text(cluboRenewalTextHdr);
      
  }
};
jQuery(document).ready(function() {
     runCluboGrace() 
});
</script>
