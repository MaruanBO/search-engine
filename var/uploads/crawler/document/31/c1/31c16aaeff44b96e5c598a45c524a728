	aaMenu.init = function() {
		if(os.UserSettings.isLogged && this.userDD) {
			var nameDisplay = '<span><a href="' + os.Otags.login_page_header_link + '">Hi, ' + os.UserSettings.firstName + '</a></span><i class="down-arrow"></i>';

