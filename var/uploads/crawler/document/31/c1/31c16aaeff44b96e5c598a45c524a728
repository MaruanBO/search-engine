            // BEGIN: HP_04_MOBILE_DETECT
// BEGIN: MWS-575 - detects the mobile url hash-mark

           var processUrlHash =  (function(){
                var thisUrl = document.URL;
                if(thisUrl.indexOf('#') !== -1){
                    var re1 = /#/;
                    thisUrl = thisUrl.replace(re1, "");
                    if(thisUrl.indexOf('//')){
                        var re2 = /\/\//;
                        thisUrl = thisUrl.replace(re2, "/");
                    }
                    window.location = thisUrl;
                }
            })();

            // BEGIN: MWS-575 - detects the mobile url hash-mark
// BEGIN: HP_04_MOBILE_DETECT

