<script>
    jQuery(document).ready(function() {
        os.hpVertOasis.init();

        var $siteSales = jQuery('.hp-site-sales');

        $siteSales.on('mouseover', '.deal-link.unhovered', function(evt) {
            var $currTarget = jQuery(evt.currentTarget);

            $currTarget.removeClass('unhovered');
        });

        $siteSales.on('touchstart', '.deal-link.unhovered', function(evt) {
            evt.preventDefault();

            var $link = jQuery(evt.currentTarget);

            if($link.hasClass('unhovered')) {
                $link.removeClass('unhovered');
            } else {
                location.href = $link.attr('href');
            }
        });

        $siteSales.on('touchend', '.deal-link.unhovered', function(evt) {
            var $body = jQuery('body');

            $body.css('pointer-events', '');
        });
    });
</script>
