/* 
 * 
 * Description: Function checks the promo bar and the blue bar and adjusts the widths so they both work together.
 * Page/Region/Element: SITE_01_JS (Inside the Header Only)
 * Dependencies:
 * Type (function, object literal, function return): function return
 * Tags: ##sitewide, ##resize, ##debounce
 * Usage: 
 * 		var runThis = new os.checkPromoBar();
 *		runThis.init(); 
 */

// SITE_01_CAMBAR_JS

jQuery(document).ready(function() {
    var $promoBanner = jQuery('.grid-container-promo .banner'),
        $cambarPrompt = jQuery('.cambar-active-prompt'),
        $promoCID = "",
        camUrl = 'https://api.overstock.com/cambar/' + $promoCID;

    if ($promoCID) {

        var ajaxComplete = function(data) {
            var camCluboCheck = -1,
                $isProductPage = jQuery('#product-page');

            if (typeof data == 'string') {
                data = $.parseJSON(data);
            }
            if (data.camBar) {
                camCluboCheck = data.camBar.indexOf('Club O');
                $promoBanner.show();
                if (data.camExcludeValues) {
                    data.camExcludeValues = data.camExcludeValues.replace('Extra 5% Off', 'Doorbusters');
                }
                var camExclude = '';
                if (data.camSavings) {camExclude += data.camSavings + ' '}
                if (data.camCheckout) {camExclude += data.camCheckout + ' '}
                if (data.camExcludeMessage) {camExclude += data.camExcludeMessage + ' '}
                if (data.camExcludeValues) {camExclude += data.camExcludeValues + ' '}
                if (data.camStackable) {camExclude += data.camStackable}

                var camMessage = '',
                    pageCIDUrl = '';

                if (data.camCustomer) {camMessage += data.camCustomer + ' '}
                if (data.camBar) {camMessage += data.camBar + ' '}
                if (camMessage) {
                    $promoBanner.find('.heading-text').text(camMessage);
                    $cambarPrompt.find('.cambar-active-message').text(camMessage);
                    
                    // SITE_01_CAMBAR_VAR 
                    pageCIDUrl = document.URL;
                    pageCIDUrl = pageCIDUrl.toLowerCase();


                    if (pageCIDUrl.indexOf("?cid=") != -1 || pageCIDUrl.indexOf("&cid=") != -1) {
                           $cambarPrompt.show();
                        setTimeout(function(){
                           $cambarPrompt.hide();
                        },3000)
                    }
                    
 

                }
                if (camExclude) {
                    camExclude += '<i class="os-icon-x-long os-icon"></i>'
                    $promoBanner.find('.disclaimer').html(camExclude);
                }

                if (typeof responsivePage != 'undefined' && camMessage) {
                    var newMSSCambar = '<div class="mss-cambar-area">' + '<div class="heading-text">' + camMessage + '</div>' + '<div class="disclaimer">' + camExclude + '</div></div>';
                    jQuery('#newo-hd').append(newMSSCambar);

                    jQuery('.mss-cambar-area .heading-text').on('click', function() {
                        jQuery('.mss-cambar-area .disclaimer').slideToggle();
                    });
                }

            }
            if (camCluboCheck > -1 && $isProductPage) {
                var cluboCouponPrct = data.camBar.split('%')[0] + '%';
                jQuery('#cluboCoups').addClass('clubo-show');
                jQuery('.pp-clubo-coup-pcnt').text(cluboCouponPrct);
                jQuery('#cluboDflt').addClass('clubo-hide');
            }

        };
        jQuery.support.cors = true;

        jQuery.ajax({
            url: camUrl,
            dataType: 'json',
            success: function(data) {
                ajaxComplete(data);
            }
        });
    }
});

os.checkPromoBar = function() {
	
	var $hdHeader = jQuery('#newo-hd.hd-ss-b'),
		$hdPromoContainer = $hdHeader.find('.grid-container-promo'),
		$hdPromo = $hdHeader.find('.hd-shippingbanner a'),
		$hdBanner = $hdHeader.find('.banner'),
		$hdDisclaimer = $hdHeader.find('.disclaimer')

	return {
		init: function() {
            var _this = this;
			var checkPromoBar = _.debounce(_this.calcPromoBar, 300);
						
			if($hdBanner.length){
				jQuery(window).resize(checkPromoBar);
				$hdBanner.on('click.showdisclaimer',function(){
					$hdPromoContainer.addClass('promoCollapse');
					$hdHeader.on('mouseleave',function(){
						_this.calcPromoBar();
						$hdHeader.unbind('mouseleave');
					});
				})
				$hdDisclaimer.on('click.closebtn','i', function(e){
					e.stopPropagation();
					$hdPromoContainer.removeClass('promoCollapse');
				})
				_this.calcPromoBar();
			}
		},
		calcPromoBar: function(){
				$hdPromoContainer.removeClass('promoCollapse');
			if ($hdPromo.outerHeight() > $hdPromoContainer.outerHeight()){
				$hdPromoContainer.addClass('promoCollapse');
			}else{
				$hdPromoContainer.removeClass('promoCollapse');	
			}
		}
	}
};

var runThis = new os.checkPromoBar();
runThis.init();


    aaMenu.init();
    jQuery('.dropdown-key').one('mouseenter', function() {
        if (aaMenu.accAreaMenu == true){
            aaMenu.loadMenu();
        }
    });

    // BIND OR PRELOAD HORIZONTAL MENUS.
    
    jQuery('#content-top-rail').unbind('mouseenter').one('mouseenter', function() {
        if (hrztlNav.dropDownLoaded === false) {
            hrztlNav.loadHorizonalMenuFile(hrztlConfig);
        }
    });

    if (os.isTouchDevice) {
        hrztlNav.loadHorizonalMenuFile(hrztlConfig);
    }

    // This has been added to to fix the images in IE (All Versions).  This JS allows you to now click on these images.  This will need to be removed as soon as the actual HTML fix goes into place.
    jQuery('.oasis .mod-image, .oasis .product-image').each(function(){
        jQuery(this).click(function() {
            window.location = jQuery(this).parent('.mod-container').attr('href');
        });
    });
        
    //initialize cartLayer
    (function(config) {
        config = config || {};
        if (config.enabled) {
            var cartLayer = (new os.CartLayer()).initialize(config);
        }
    })({"apiUrl":"http://www.overstock.com//api/cart/cartLayer.json","editUrl":"http://www.overstock.com/cart?TID=CartLayer","enabled":true,"layerUsedForAdditions":false,"requestTimeoutInMillis":10000});
        
        //handle colorbox on secure pages
        if (location.protocol === 'https:') {
        var secureColorbox = function(obj,sid) {    
            if (jQuery(obj).is("*")) {          
                var targetURL = jQuery(obj).attr("href");
                targetURL = "https://" + targetURL.split("/")[2] + '/' + sid + "/spage.html";
                jQuery(obj).attr("href", targetURL);
            }
        };
        secureColorbox('.colorbox.holiday-icon',"22645");
            secureColorbox('.colorbox.bad-weather',"68249");
        }


        // JS used for inline auto-sized content
        jQuery('.colorbox-iauto').bindColorbox('click', {inline: true, width: 'auto', height: 'auto'});

        //bind .colorbox 
        //jQuery('.colorbox').bindColorbox();

       if(os.UserSettings.itemsInCart > 0) jQuery('#hdrchkbtn').css('display','block');

     //Omail sign up in header.
     var headOmail = new os.HeaderOmail();
     headOmail.init();

    jQuery('.omailheader-btn-signup').click(function(e){
        e.preventDefault();
        headOmail.checkEmail(this);
    });

    if(typeof headOmail.getUrlVar('isOmailSubscribed') !== "undefined") headOmail.alreadySub();

     //Old Quickview
     


     


    // init 404 page recs FED-8240

    if ( typeof errPageRecs == 'function' ){
        errPageRecs();
    }

}); // END DOCUMENT.READY FUNCTION.

function omailSuccess() {
    jQuery('#omailSignup').slideUp();
}
