    $wn.merge(storyData, $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract());
    if (storyData.length < 1) {
      return;
    }

      // Replace csSiteOptions.tsrRightCount with actual number desired "mg_number_right_items"
      var mg_number_right_items = 10;

    // STATIC TOP STORY AREA
    if (csSiteOptions.tsrType === 'static') {
      // Built left side
      var story = storyData.shift();
      
      if (!story.graphicSrc) { story.graphicSrc = csSiteOptions.tsrDefaultImage; }
      
      if (navigator.userAgent.indexOf('Mac OS X') == -1 && $.browser.mozilla && parseInt($.browser.version,10) >= 21) {    
        var cDate = new Date(story.dateText);
        cDate = (cDate.setHours(cDate.getHours()-1));
        story.dateText = cDate;   
      }
      
      story.custDate = (new Date(story.dateText)).customFormat('%DDDD%, %MMMM% %D%, %YYYY%');      
     
      story.hasVideo = (story.isClip || story.isVideo);
      $wn('#csTsr .csTsrLeft').csFormatterHtml(leftSide, story);

      // Build right side

      $wn.each(storyData, function(i) {
        if (i >= mg_number_right_items) { return false;  /* break */ }
        
        if (!this.graphicSrc) { this.graphicSrc = csSiteOptions.tsrDefaultImage; }
        
        if (navigator.userAgent.indexOf('Mac OS X') == -1 && $.browser.mozilla && parseInt($.browser.version,10) >= 21) {    
          var dDate = new Date(this.dateText);
          dDate = (mDate.setHours(dDate.getHours()-1));
          this.dateText = dDate;   
        }
        
        if (CSHELPERS.secondsSince(this.dateText) < csSiteOptions.tsrDateCutoff) {
          this.custDate = CSHELPERS.timeAgo(this.dateText, '100 years');
        } else {
          this.custDate = false;
        }
        this.hasVideo = (this.isClip || this.isVideo);
        $wn('#csTsr .csTsrRight ul').csFormatterAppend(rightSide, this);
      });
    
    // DYNAMIC TOP STORY AREA
    } else if (csSiteOptions.tsrType === 'dynamic') {
      $wn('#csTsr').addClass('csDynamicTsr');
    
      // Add each story
      $wn.each(storyData, function(i) {
        if (i >= mg_number_right_items) { return false;  /* break */ }

        if (!this.graphicSrc) { this.graphicSrc = csSiteOptions.tsrDefaultImage; }
        
        if (navigator.userAgent.indexOf('Mac OS X') == -1 && $.browser.mozilla && parseInt($.browser.version,10) >= 21) {    
          var mDate = new Date(this.dateText);
          mDate = (mDate.setHours(mDate.getHours()-1));
          this.dateText = mDate;   
        }
        
        this.hasVideo = (this.isClip || this.isVideo);
      	this.custDate = (new Date(this.dateText)).customFormat('%DDDD%, %MMMM% %D%, %YYYY%');

        
        // Built left side        
        var $leftSlide = $wn.$csFormatter(leftSide, this);

        // Build right side
        if (CSHELPERS.secondsSince(this.dateText) < csSiteOptions.tsrDateCutoff) {                 
            
	          this.custDate = CSHELPERS.timeAgo(this.dateText, '100 years');

        } else {
          this.custDate = false;
        }
        var $rightLinks = $wn.$csFormatter(rightSide, this);

        // Make the list links do something when interacted with
        $rightLinks.mouseenter(function() {
          $wn('#csTsr .csTsrSlide').not($leftSlide).stop().fadeTo('fast', 0, function() {
            $wn(this).css('z-index', 1);
          });
          $leftSlide.stop().fadeTo('fast', 1, function() {
            $wn(this).css('z-index', 10);
          });
          $wn('#csTsr .csTsrRight li').removeClass('csActive');
          $wn(this).addClass('csActive');
        });

        $wn('#csTsr .csTsrLeft').append($leftSlide);
        $wn('#csTsr .csTsrRight ul').append($rightLinks);
      });
      
      // Trigger slide 1 (so the stage isn't empty)
      $wn('#csTsr .csTsrRight li:first').mouseover().mouseout();
    }

    // Add classes for right side borders
    $wn('#csTsr .csTsrRight li').first().addClass('csFirst');
    $wn('#csTsr .csTsrRight li').last().addClass('csLast');
    
    // Add more link and dotdotdot
    var MG_tsrMoreLinkToggle = (typeof MG_tsrMoreLinkOverride !== 'undefined') ? MG_tsrMoreLinkOverride : csSiteOptions.tsrMoreLink;
    $wn('#csTsr .csTsrRight #csMore').html(MG_tsrMoreLinkToggle);
    $wn('#csTsr .csTsrLeft .csHeadlineLeft a, #csTsr .csTsrRight li span, #csTsr .csTsrLeft .csSummary').dotdotdot();
  
    // Hide original sources
    $wn('#WNTopStoryWrap, #WNCol2 .wnDS-20').hide();
}, 'WNCol23done');
</script>

<div id="csTsr" class="MG_display10">
	<div class="csTsrLeft"></div>
	<div class="csTsrRight">
		<ul></ul>
		<span id="csMore"></span>
	</div>
