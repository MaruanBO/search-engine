<!-- Start Session timeout counter tag -->
<script type="text/javascript">
        var sessionTimeout = "30";
		var url=window.location.pathname;
		var checkoutPresent=url.indexOf("/checkout");
        function DisplaySessionTimeout()
        {
            
            sessionTimeout = sessionTimeout - 1;
            
            //if session is not less than 0
            if (sessionTimeout >= 0)
                //call the function again after 1 minute delay
                window.setTimeout("DisplaySessionTimeout()", 60000);
         else
            {
        	 	if(checkoutPresent==-1){
					document.getElementById("sessionTimeOutPopup").style.display='block';
					document.getElementById("sessionTimeOutOverLay").style.display='block';
	                if(checkoutPresent !==-1){
							document.getElementById("gotoCart").style.display='block';
							document.getElementById("returnToShopping").style.display='none';
						
					}
					else {
						document.getElementById("returnToShopping").style.display='block';
						document.getElementById("gotoCart").style.display='none';
					}
        	 	}
        	 	else {
        	 		window.location.href="/cart/shoppingCart.jsp";
        	 	}
			
            }
        }
       
		
		function reloadSessionReturnToCart()
		{
			document.getElementById("returnToShopping").style.display='none';
			document.getElementById("gotoCart").style.display='none';
			document.getElementById("sessionTimeOutPopup").style.display='none';
			document.getElementById("sessionTimeOutOverLay").style.display='none';
			window.location.href="/cart/shoppingCart.jsp";
		}
		
		function reloadSessionRefresh()
		{
			document.getElementById("returnToShopping").style.display='none';
			document.getElementById("gotoCart").style.display='none';
			document.getElementById("sessionTimeOutPopup").style.display='none';
			document.getElementById("sessionTimeOutOverLay").style.display='none';
			window.location.reload(true);
		}
		
		 $( window ).load(DisplaySessionTimeout());
		
</script>

<!-- End Session timeout counter tag --><!-- Session Time out overlay include end -->
