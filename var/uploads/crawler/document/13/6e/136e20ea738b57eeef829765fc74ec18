<script>
//this takes care of centering the carousel when the sides are cropped (i.e. window width less than 1200)
function PositionCarousel(){
	var centerOffset = $("#content-main").offset().left;
	var initialLeftOffset = ((1200 - 980) / 2) - centerOffset;
	if(initialLeftOffset > 0){
		$("#home-carousel .carousel-clip").css('left',-(initialLeftOffset));
	}
	else{
		$("#home-carousel .carousel-clip").css('left','0');
	}
}

function HigCarousel_initCallback(carousel){
	//console.log('called initCallback');
}

function HigCarousel_ScrollFinishedCallback(carousel){
	currentActivePanelIndex = carousel.find('.panel-active').data('index');
	//console.log('currentActivePanelIndex='+currentActivePanelIndex);
	carousel.find('.hig-carousel-controls > a').removeClass('active');
	carousel.find('.hig-carousel-controls > a > span:contains("'+currentActivePanelIndex+'")').parent().addClass('active');

}


(function( $ ){

	var $carousel;

  // set up default options
  var defaults = {
    duration:      8000,
    loop:     'continuous',
    itemWidth: 600,
    itemHeight: 260,
    numVisible : 1,
    speed : 1500,
    scrollLocked : false,
    controls : false,
    numItems : 0,
    maxCycles : -1

  };

  var options;

  var methods = {
    init : function(settings) {
   	 options = $.extend({}, defaults, settings);
       return this.each(function(){
         var $carouselItems = $(this).find('.carousel-ribbon > li');
         options.numItems = $carouselItems.length;
$(this).wrapInner('<div class="carousel-clip" />');

         $(this).append('<a class="carousel-previous" href="#" title="previous"><span>Previous</span></a><a class="carousel-next" href="#" title="next"><span>Next</span></a>');
         $(this).find('.carousel-next').click(function(event){
         	methods.ScrollNext('manual',1);
         	event.preventDefault();
         });
         $(this).find('.carousel-previous').click(function(event){
         	methods.ScrollPrev(1);
         	event.preventDefault();
         });
         $(this).hover(function(){
         	methods.HoverPauseOn();},
         	function(){
         	methods.HoverPauseOff();
         });
					$carouselItems.addClass('panel');
         $firstPanel = $carouselItems.first();
         $lastPanel = $carouselItems.last();
         $lastPanel.clone().insertBefore($firstPanel);
         $firstPanel.clone().insertAfter($lastPanel);

         $carouselItems.each(function(index){
         	$(this).data('index',index+1);
         	$(this).attr('data-displayed-x-times',0);
         });
         $firstPanel.addClass('panel-first panel-active').attr('data-displayed-x-times',1);
         $lastPanel.addClass('panel-last');
         $(this).height(options.itemHeight);
         $(this).find('.carousel-ribbon').width((options.numItems +2) * options.itemWidth).css('left' , '-'+options.itemWidth+'px');
         methods.StartCycle($(this));
         if (options.controls) methods.InitControls();
					if(typeof window.HigCarousel_initCallback == 'function') {
						HigCarousel_initCallback($(this));
					}

       });

    },


    InitControls : function() {
			$carousel.append('<div class="hig-carousel-controls">');
			//console.log('numItems=['+options.numItems+']');
			for (i=1; i<=options.numItems; i++){
				$carousel.find('.hig-carousel-controls').append('<a href="#"><span>'+i+'</span></a>');
			}
		//	<a href="#"><span>1</span></a><a href="#"><span>2</span></a><a href="#"><span>3</span></a><a href="#"><span>4</span></a>
			$carousel.append('</div>');
			$carousel.find('.hig-carousel-controls > a').first().addClass('active');
			$carousel.find('.hig-carousel-controls > a').click(function(e){
				e.preventDefault();
				methods.ScrollTo($(this).children().text());
			});

    },


    HoverPauseOn : function() {
		//console.log("HoverPauseOn");
    		$carousel.addClass('paused-hover');
				return $carousel;
    },


    HoverPauseOff : function() {
    //console.log("HoverPausOff");
    		$carousel.removeClass('paused-hover').removeClass('paused');
				return $carousel;
    },


    StartCycle : function() {
    		//console.log('StartCycle : ' + $carousel.attr('class'));
    		$carousel.removeClass('paused');
				$carousel.interval = setInterval(function(){methods.ScrollNext('auto',1)},options.duration);
				return $carousel;
    },

    StopCycle : function(){
    	$carousel.addClass('paused');
    	clearInterval($carousel.interval);
    	//console.log('StopCycle');
			return $carousel;
    },

    ScrollNext : function(mode,numPanels) {
    	//console.log('ScrollNext: mode='+mode+', scrollLocked='+options.scrollLocked+', hasClass='+$carousel.attr('class'));
    	if((options.scrollLocked==false && mode=='manual') || (mode=='auto' && !$carousel.hasClass('paused-hover'))){
    	//console.log('ScrollNext proceeding');
    		var loop = false;
    		//check if last panel, if so do special procedure
    		if($carousel.find('.panel-active').hasClass('panel-last')){
					loop = true;
    		}

    		//console.log('itemWidth * numPanels='+options.itemWidth * numPanels);
   			methods.StopCycle();
				$carousel.find('.carousel-ribbon').animate({
					left: '-='+(options.itemWidth * numPanels * options.numVisible)
				}, options.speed, function() {
				//	console.log('animation complete');
					options.scrollLocked = false;
					if (loop){
						//change from this placeholder panel to first panel
						$carousel.find('.carousel-ribbon').css('left' , '-'+options.itemWidth*options.numVisible+'px');
						$carousel.find('.panel-active').removeClass('panel-active');
						$carousel.find('.panel-first').addClass('panel-active');
					}
					else{
						//go next again based on how many times the carousel has advanced
						for (i=1; i<=numPanels; i++){
							$carousel.find('.panel-active').removeClass('panel-active').next('.panel').addClass('panel-active');
						}
					}
					if(typeof window.HigCarousel_ScrollFinishedCallback == 'function') {
						HigCarousel_ScrollFinishedCallback($carousel);
					}
					var timesDisplayed = $carousel.find('.panel-active').attr('data-displayed-x-times')*1 + 1;
					$carousel.find('.panel-active').attr('data-displayed-x-times',timesDisplayed);
					if (timesDisplayed <= options.maxCycles) methods.StartCycle();
				//	console.log('just finished: ' + options.scrollLocked);
					return $carousel;
				});
				options.scrollLocked = true;
				//console.log('just locked: ' + options.scrollLocked);

			}
    },


    ScrollPrev : function(numPanels) {
    	//console.log("ScrollPrev Start");
    	if(options.scrollLocked==false){
    		var loop = false;
    		//check if first panel, if so do special procedure
    		if($carousel.find('.panel-active').hasClass('panel-first')){
					loop = true;
    		}
    		methods.StopCycle();
				$carousel.find('.carousel-ribbon').animate({
					left: '+='+(options.itemWidth * numPanels * options.numVisible)
				}, options.speed, function() {
					//alert('animation complete');
					options.scrollLocked = false;
					if (loop){
						//change from this placeholder panel to last panel
						 //take me to the real last panel "panel-last" using this left offset
						 var lastOffset = options.itemWidth*options.numVisible*(options.numItems);
						 //console.log(lastOffset);
						$carousel.find('.carousel-ribbon').css('left' , '-'+lastOffset+'px');
						$carousel.find('.panel-active').removeClass('panel-active');
						$carousel.find('.panel-last').addClass('panel-active');
					}
					else{
						//go next again based on how many times the carousel has advanced
						for (i=1; i<=numPanels; i++){
							$carousel.find('.panel-active').removeClass('panel-active').prev('.panel').addClass('panel-active');
						}
					}
					if(typeof window.HigCarousel_ScrollFinishedCallback == 'function') {
						HigCarousel_ScrollFinishedCallback($carousel);
					}
					var timesDisplayed = $carousel.find('.panel-active').attr('data-displayed-x-times')*1 + 1;
					$carousel.find('.panel-active').attr('data-displayed-x-times',timesDisplayed);
					if (timesDisplayed <= options.maxCycles) methods.StartCycle();
					//console.log('ScrollPrev just finished: ' + options.scrollLocked);
					return $carousel;
				});
				options.scrollLocked = true;
				//console.log('ScrollPrev just locked: ' + options.scrollLocked);
			}
    },


    ScrollTo : function(panel) {
			var currentPanel = $carousel.find('.panel-active');
			var currentPanelIndex = currentPanel.data('index');
			var num2Scroll;
			//console.log('panel='+panel+' currentPanel='+currentPanelIndex);
			if (panel > currentPanelIndex){
				num2Scroll = panel - currentPanelIndex;

				methods.ScrollNext('manual',num2Scroll);
			}
			else if (panel < currentPanelIndex){
				num2Scroll = currentPanelIndex - panel;
				methods.ScrollPrev(num2Scroll);
			}
    }



  };

  $.fn.HigCarousel = function( method ) {
    // Method calling logic
    $carousel = this;
    if ( methods[method] ) {
      return methods[ method ].apply( this, Array.prototype.slice.call( arguments, 1 ));
    } else if ( typeof method === 'object' || ! method ) {
      return methods.init.apply( this, arguments );
    } else {
      $.error( 'Method ' +  method + ' does not exist on jQuery.HigCarousel' );
    }

  };

})( jQuery );

// calls the init method

$(function(){
	$('.hig-carousel').HigCarousel({
		duration : 6000,
		itemWidth : 1200,
		itemHeight : 350,
		controls : true,
		maxCycles : 3

	});

	//this takes care of centering the carousel when window is resized
	$(window).resize(function() {
		PositionCarousel();
	});

	PositionCarousel();//for initial positioning

	//quote lower left swap
	var $bottomQuote = $('#bottom-quote');
	if($bottomQuote.length > 0){
		$('#hp-row-3 > .link-section > div:first-child').empty().append($bottomQuote);
		$bottomQuote.show();
	}

});



</script>

