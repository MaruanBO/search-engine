<script type="text/javascript">
	var sectionid="unknown";
	var subsectionid="unknown";
	

	$(document).ready(function(){
		$('a[higUT]').click(function(){
			var usertype=$(this).attr('higUT')
			$.cookie("higusertype", usertype, { expires: 365, path:"/" });

		});
