			if ($('#NoShowPromo').val() != 'True') {
				$('#NoShowPromo').val('True');
			   $('#ex3b').jqm({ 
					overlay: 0,
					onHide: function(h) { 
						h.w.fadeOut(0);
					}
				 });
			  
			   // Close Button Highlighting. IE doesn't support :hover. Surprise?
			   if($.browser.msie) {
			   $('div.jqmAlert .jqmClose')
			   .hover(
				 function(){ $(this).addClass('jqmCloseHover'); }, 
				 function(){ $(this).removeClass('jqmCloseHover'); });
			   }
			   
			   //Ridiculous hacking here to accomodat IE6
			   $('#ex3b').css("top", "-300px");  
			   
			   //Load it automatically 
			   $('#ex3b').jqmShow(); 
			   
			   //Drop it in from the top of the screen
			   var badBrowser = (/MSIE ((5\.5)|6)/.test(navigator.userAgent) && navigator.platform == "Win32");
			   if (badBrowser) {
					$('#ex3b').animate({ top: "20px" }, 800);
			   }
			   else {
				   $('#ex3b').animate({ top: "200px" }, 800);  
			   } 
			   
			   //Close it after 5 seconds               
			   setTimeout("$('#ex3b').fadeOut(1100)",5000);     
			   }
		              
		});
