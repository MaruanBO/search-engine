<script type="text/javascript">
(function () {
	try {
		var templates = document.querySelectorAll("[data-merchandise-block]");
		for (var i = 0; i < templates.length; i++) {
			var template = templates[i];
			var render = new Function("with(this) { return \"" + template.innerHTML.replace(/[\r\n]+/g, "\\n").replace(/"/g, "\\\"").replace(/\#=([a-z0-9]+)\#/gi, "\" + (data.$1 || \"\") + \"") + "\" }");
			var merchandiseBlockID = template.getAttribute("data-merchandise-block");
			var sku = template.getAttribute("data-sku");
			var json = { "activities" : sku };
			_ltk.MerchandiseBlock.GetRecommendations(merchandiseBlockID, json, function (recommendations) {
				var skus = $.map(recommendations, function (r) { return r.Sku; }).join(",");
				$.ajax({
					url: "/musite/recs_detail.asp?skus=" + skus,
					dataType: "json",
					success: function (productInfo) {
						$(recommendations).each(function (index) {
							if (productInfo[this.Sku]) {
								this.Year = productInfo[this.Sku].Year;
								this.Media = productInfo[this.Sku].Media;
								this.Button = productInfo[this.Sku].Button;
								this.Boxcover = productInfo[this.Sku].Boxcover;
								this.Actors = productInfo[this.Sku].Actors;
								this.Title = this.Title.replace(/\s*\[(.*)\]/g, '');
							} else {
								recommendations.remove(index);
							}
						});
						var container = document.createElement("div");
						for (var j = 0; j < recommendations.length; j++) container.innerHTML = render.call({ data: recommendations[j] }) + container.innerHTML;
						for (var j = 0; j < container.childNodes.length;) template.parentNode.insertBefore(container.childNodes[j], template.nextSibling);
						_ltk.MerchandiseBlock.SetTracking(recommendations,function(){});
						$(".RecsWrap").slideDown();
					}
				});
			});
		}
	}
	catch (err) {
		if (typeof window.console != 'undefined') {
				console.log(err.message);
			}
	}
})();
// Array Remove - By John Resig (MIT Licensed)
Array.prototype.remove = function(from, to) {
  var rest = this.slice((to || from) + 1 || this.length);
  this.length = from < 0 ? this.length + from : from;
  return this.push.apply(this, rest);
};
</script>

