					<script>
						var newsletterTicked3;
					    jQuery(document).ready(function() {
					    	articleId = window.location.pathname.split( '/' )[2];
						jQuery("#newsletter-form").validate({
							rules: {
								email: {
									required: true
								   ,email: true
								}
							},
							errorPlacement: function(error, element) {
								error.appendTo( "#newsletter-error-mobile" );
							},
							submitHandler: function(form) {
								jQuery("#desktop-signup").attr('disabled', 'disabled').css('opacity','.4');
								jQuery.ajax({
									url: '/signup?',
									data: {email: jQuery("#email").val(), briefing: jQuery("#ten_things").is(':checked'), top_opinions: jQuery("#best_articles").is(':checked'), captured: jQuery("#best_photos").is(':checked') },
									dataType: 'json',
									jsonp: false,
									success: function(data) {
										if(data.success) {
											parent.formSubmitted = true;
											alert("Your free email newsletter subscription is confirmed. Thank you for subscribing!");
										}
										else {
											jQuery("#desktop-signup").attr('disabled', false).css('opacity', '1');
											alert(data.message);
										}
									}
								});

							}
						});

					});

						//This part handles the adjustable brightness for the captured image
						//@todo generalize this function across all the captured stuff
						var minBrightness = 0; //constant value for the minimum brightness
						var zoneHeight = (jQuery(window).height() / 1.3); //the height of the "zone" in which brightness will change, over this zone brightness should go from minBrightness to 1.0
						var bottomZoneBuffer = 120; //the amount of space before any adjustments occur

						jQuery( document ).ready(function() {
							adjustBrightness(false); //handle the initial page load brightness adjustment before any scrolling
						});
						jQuery( window ).scroll(function() {
							adjustBrightness(true); //handle the scrolling brightness adjustments
						});
						document.addEventListener("touchmove", ScrollStart, false);
						function ScrollStart() {
							adjustBrightness(true);
						}

						//if onlyVisible is true then only adjust visible page elements
						function adjustBrightness(onlyVisible) {
							var brightnessRatio = ((1 - minBrightness) / zoneHeight); //The amount the brightness should change for each pixel scrolled.
							jQuery(".captured-module .image img").each(function( index ) {
								//xAboveBottom is the height is pixels from the top of the element to the bottom of the viewport (can be negative!)
								var xAboveBottom = ((jQuery("body").scrollTop() + jQuery(window).height()) - jQuery(this).offset().top);

								if ( (onlyVisible) || ((xAboveBottom > 100) && (xAboveBottom < jQuery(window).height())) ) { //don't bother with anything unless the element is visible
									var newBrightness = Math.max(0, Math.min (((xAboveBottom * brightnessRatio) + minBrightness), 1)); //cap the brightness between 0.0 & 1.0

									jQuery(this).css({"-webkit-filter" : "brightness(" + newBrightness + ")"});
								}
							});
						}
					</script>


