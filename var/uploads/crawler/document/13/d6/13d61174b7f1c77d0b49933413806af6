<script>
	//Start code for the lightbox
	var imgIndex = 1;
	// display the lightbox

	function lightbox(){

		// add lightbox element and background
		if(jQuery('#lightbox').size() == 0){

			var lightbox = jQuery('<div id="lightbox"><img src="/bundles/twsite/images/tour/tour-1.jpg" style="visibility:hidden;width: 100%;position: relative;"/></div>');
			var litebg = jQuery('<div id="lightbox-bg" class="tour-btn"/>');
			var rightBtn = jQuery('<div id="right-btn" class="tour-btn"/>');
			var closeBtn = jQuery('<div id="close-btn" class="tour-btn"/>');

		// add lightbox  first image
			jQuery('body').append(litebg).append(lightbox);
			
 			jQuery('#lightbox').css({"background": "url(/bundles/twsite/images/tour/tour-1.jpg) no-repeat", 'width': '100%',
 	 			'background-size':' cover'});
 			jQuery('#lightbox').append(rightBtn).append(closeBtn);
			jQuery('#right-btn').wrap("<a href='javascript:rightBtn();'></a>");
			jQuery('#close-btn').wrap("<a href='javascript:closeBtn();'></a>");
		}
		jQuery('#lightbox-bg').click(function(e){
			jQuery('#lightbox').hide();
			jQuery('#lightbox-bg').hide();
			jQuery('.tour-btn').hide();
			console.log('iwork');
		});
		jQuery('#lightbox').show();
		jQuery('#lightbox-bg').show();
		jQuery('.tour-btn').show();
	}



    function buttonSet(){}
	// light box left button
	function leftBtn(){
		if (imgIndex === 2) {
			//jQuery('#lightbox img').remove();
			jQuery('#lightbox').css({"background": "url(/bundles/twsite/images/tour/tour-1.jpg) no-repeat", 'width': '100%',
 	 			'background-size':' cover'});
			//jQuery('#lightbox').prepend('<img id="tour-1" src="/bundles/twsite/images/tour/tour-1.jpg"/>');
			jQuery('#left-btn').remove();
			imgIndex = 1;
			console.log(imgIndex);

		} else if (imgIndex === 3) {
			//jQuery('#lightbox img').remove();
			jQuery('.3-btn').remove();
			jQuery('#lightbox').css({"background": "url(/bundles/twsite/images/tour/tour-2.jpg) no-repeat", 'width': '100%',
 	 			'background-size':' cover'});
			//jQuery('#lightbox').prepend('<img id="tour-1" src="/bundles/twsite/images/tour/tour-2.jpg"/>');
			jQuery('#lightbox').append('<div id="right-btn" class="tour-btn"></div>');
			jQuery('#right-btn').wrap("<a href='javascript:rightBtn();'></a>");
			imgIndex = 2;
			console.log(imgIndex);


		}
	}

	// light box right button
	function rightBtn(){
		if (imgIndex === 1) {
			//jQuery('#lightbox img').remove();
			jQuery('#lightbox').css({"background": "url(/bundles/twsite/images/tour/tour-2.jpg) no-repeat", 'width': '100%',
 	 			'background-size':' cover'});
			//jQuery('#lightbox').prepend('<img id="tour-2" src="/bundles/twsite/images/tour/tour-2.jpg"/>');
			jQuery('#lightbox').append('<div id="left-btn" class="tour-btn"></div>');
			jQuery('#left-btn').wrap("<a href='javascript:leftBtn();'></a>");
			imgIndex = 2;
			console.log(imgIndex);
			//document.getElementById("lightbox").style.backgroundSize="100% auto";

		} else if (imgIndex === 2) {
			//jQuery('#lightbox img').remove();
			jQuery('#lightbox').css({"background": "url(/bundles/twsite/images/tour/tour-3.jpg) no-repeat", 'width': '100%',
 	 			'background-size':' cover'});
			//jQuery('#lightbox').prepend('<img id="tour-2" src="/bundles/twsite/images/tour/tour-3.jpg"/>');
			jQuery('#lightbox').append('<div id="select-box" class="3-btn"/>');
 			jQuery('#select-box').append('<div id="close-btn2" class="3-btn"/>');
 			jQuery('#close-btn2').wrap("<a href='/'></a>");
 			jQuery('#select-box').append('<div id="editor-btn" class="3-btn"/>');
 			jQuery('#editor-btn').wrap("<a href='/articles/531362/welcome'></a>");
 		    jQuery('#select-box').append('<div id="msg-btn" class="3-btn"/>');
 		    jQuery('#msg-btn').wrap("<a href='/contact\'></a>");
  		    jQuery('#right-btn').remove();
			imgIndex = 3;
			console.log(imgIndex);
			document.getElementById("lightbox").style.backgroundSize="100% auto";

		}
	}
	// close lightbox
	function closeBtn(){
		jQuery('#lightbox').hide();
		jQuery('#lightbox-bg').hide();
		jQuery('.tour-btn').hide();
	}

    jQuery('#lightbox-bg').click(function(){
        jQuery(this).closeBtn();
    })
    //End code for the lightbox
    
    
    //Show Geico
    function showGeico(){
    	if( TheWeek.getCurrentDevice() == 'desktop'){
           
    		//get the currentDate
    		var fullDate = new Date();
    		var twoDigitMonth = fullDate.getMonth()+1+"";if(twoDigitMonth.length==1)  twoDigitMonth="0" +twoDigitMonth;
    		var twoDigitDate = fullDate.getDate()+"";if(twoDigitDate.length==1) twoDigitDate="0" +twoDigitDate;
    		var currentDate = fullDate.getFullYear()+"-"+ twoDigitMonth +"-"+  twoDigitDate;

    		//Dates when Geico shouldn't show up
    		var geicoCalendar= [
        		        		'2015-01-15',
        		        		'2015-01-16',
        		        		'2015-01-20',
        		        		'2015-01-22',
        		        		'2015-01-26',
        		        		'2015-02-03',
        		        		'2015-02-11',
        		        		'2015-02-12',]

   		 //if it's homepage show it only if it's not one of those days
    		if(jQuery('.homepage-articles').length){
	    		if( (jQuery.inArray( currentDate, geicoCalendar)) == -1 ){
	
		        	var Geicopixel = new Image();
		    		Geicopixel.src="https://ad.doubleclick.net/ad/N6595.126161THEWEEKMAGAZINE/B8452320.114491333;sz=1x1;ord=[timestamp]?";
		    		Geicopixel.width="1px"; Geicopixel.height="1px";
		    		Geicopixel.alt="Advertisement";
		
		  		    jQuery('#tour-container').append('<a href="javascript:lightbox();"><img src="/bundles/twsite/images/tour/welcome.jpg" /></a>');
		
		    	    jQuery('#tour-container').show();
	    	    }
 		   }
   		   //the rest of the pages
   		   else{
			    var Geicopixel = new Image();
	    		Geicopixel.src="https://ad.doubleclick.net/ad/N6595.126161THEWEEKMAGAZINE/B8452320.114491333;sz=1x1;ord=[timestamp]?";
	    		Geicopixel.width="1px"; Geicopixel.height="1px";
	    		Geicopixel.alt="Advertisement";
	
	  		    jQuery('#tour-container').append('<a href="javascript:lightbox();"><img src="/bundles/twsite/images/tour/welcome.jpg" /></a>');
	
	    	    jQuery('#tour-container').show();
 	 		   }
         }

     }

    //set a cookie for the Geico tour
    jQuery(function(){
    	if ( (jQuery.cookie("t_cookie") ) == null) {
    		jQuery.cookie("t_cookie", "first_time", { path: '/', expires: 365 });
    	    showGeico();
    				    
    	}else{
    		if( jQuery.cookie("t_cookie") == "first_time"){
    			jQuery.cookie("t_cookie", "second_time", {expires: 365, path: '/' });
    			showGeico();
    		}else if( jQuery.cookie("t_cookie") == "second_time"){
    			jQuery.cookie("t_cookie", "third_time", { expires: 365, path: '/' });
    			showGeico();
    		}else if( jQuery.cookie("t_cookie") == "third_time"){
    			jQuery('#tour-container').hide();
  		    }
    	}
    });
</script>
            
<script>
    //adjust the content marging on different pages regarding header positioning 
    function adjustHeader(){
    	if( TheWeek.getCurrentDevice() == 'desktop'){
    		if(jQuery('#sp-section').length ==0){
    			if( jQuery('#header').css('position')=='fixed' ){
    				jQuery( "#content").css('margin-top', '77px');
    				if(jQuery('.most-popular').length){
    					jQuery( ".most-popular").css('margin-top', '77px');
    					jQuery( ".article-push-down-ad").css('margin-top', '100px');
    					
    				}
    				if(jQuery('.hp-drawer').length){
    					jQuery( ".hp-drawer").css('margin-top', '77px');
    				}
    				if(jQuery('.cartoon-page').length){
    					jQuery('#sub-header').css('margin-top', '84px');
    				}
    				if(jQuery('#featured-audio').length){
    					jQuery('#sub-header').css('margin-top', '45px');
    				}
    				if(jQuery('.featured-video').length){
    					jQuery('#sub-header').css('margin-top', '45px');
    				}
//     				if(jQuery('#captured-wrapper').length){
//     					jQuery('#sub-header').css('margin-top', '75px');
//     				}
    				if(jQuery('.magazine').length){
    					jQuery('#sub-header').css('margin-top', '64px');
    				}
    				if(jQuery('#five-things-wrapper').length){
    					jQuery('#sub-header').css('margin-top', '129px');
    				}
    				if(jQuery('#mp-container').length){
    					jQuery('#content').css('margin-top', '0px');
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
    				if(jQuery('#author_header').length){
    					jQuery('#sub-header').css('margin-top', '129px');
    				}
    				if(jQuery('#author-page').length){
    					jQuery('#author-page').css('margin-top', '106px');
    				}
    				if(jQuery('.homepage-articles').length){
    					jQuery('.pushdown-ad').css('margin-top', '110px');
    					jQuery( "#content").css('margin-top', '0px');
    				}
    				
    			}else if( jQuery('#header').css('position')=='relative' ){
    				jQuery( "#content").css('margin-top', '0px');
    				if(jQuery('.most-popular').length){
    					jQuery( ".most-popular").css('margin-top', '0px');
    					jQuery( ".article-push-down-ad").css('margin-top', '30px');
    				}
    				if(jQuery('.hp-drawer').length){
    					jQuery( ".hp-drawer").css('margin-top', '0px');
    				}
    				if(jQuery('.cartoon-page').length){
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
    				if(jQuery('#featured-audio').length){
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
    				if(jQuery('.featured-video').length){
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
//     				if(jQuery('#captured-wrapper').length){
//     					jQuery('#sub-header').css('margin-top', '0px');
//     				}
    				if(jQuery('.magazine').length){
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
    				if(jQuery('#five-things-wrapper').length){
    					jQuery('#sub-header').css('margin-top', '35px');
    				}
    				if(jQuery('#mp-container').length){
    					jQuery('#content').css('margin-top', '0px');
    					jQuery('#sub-header').css('margin-top', '0px');
    				}
    				if(jQuery('#author_header').length){
    					jQuery('#sub-header').css('margin-top', '35px');
    				}
    				if(jQuery('#author-page').length){
    					jQuery('#author-page').css('margin-top', '0px');
    				}
    				if(jQuery('.homepage-articles').length){
    					if( jQuery('#tour-container').css('display')=='block' ){
    						jQuery('.pushdown-ad').css('margin-top', '200px');
    					}else{
    						jQuery('.pushdown-ad').css('margin-top', '35px');
    					    }
    					
    					jQuery( "#content").css('margin-top', '0px');
    				}
    			}
    	    }
    	}
    }
    jQuery(function(){
    	//as long as we have geico banner
        if( TheWeek.getCurrentDevice() == 'desktop'){
    		 if( jQuery('#tour-container').css('display')=='block' ){
    			jQuery( "#header" ).css('position', 'relative');
    			jQuery( "#header" ).css('top', 'auto');
    			jQuery( ".header-wrapper" ).css('top', 'auto');
    			jQuery( ".header-wrapper" ).css('float', 'left');


    			if(jQuery('#sp-section').length){
    				jQuery('.sp-subheader').css('height', '363px');
    				jQuery('.sp-subheader .speed-logo').css('margin-top', '65px');
    			}

    	     }
    		 adjustHeader(); 
    	}
    });

    //change header from relative to fixed once you start scrlling on desktop
    function fixedHeaderOnScrollwithGeicoBanner() {
    	adjustHeader();
    	if( (TheWeek.getCurrentDevice() == 'desktop') ){
    		if(jQuery('#sp-section').length){
    			//on speedreads remove banner when you start scrolling
    			jQuery( "#header" ).css('position', 'fixed');
    				jQuery( "#header" ).css('top', 'fixed');
    				jQuery( "#header" ).css('top', '0px');
    				jQuery( ".header-wrapper" ).css('top', '0px');
    				jQuery( ".header-wrapper" ).css('float', 'none');
    			    menu_back_color =jQuery('body').css('background-color');
    			    if(jQuery('#sp-section').length){
    			    	jQuery('.sp-subheader').css('height', '260px');
    			    	jQuery('.sp-subheader .speed-logo').css('margin-top', '112px');
    			    }
    			    else{
    			    	if(jQuery( "#sliding-menu").css("display")=="none" ) {
    			    			jQuery( "#header" ).css('background-color', menu_back_color );
    					}
    				}
    				jQuery('#tour-container').hide();
    		}
    		//on rest of the pages remove banner when you pass it
    		else{
    		    if( ( jQuery(window).scrollTop() + 1 ) > 80 ){
    		    	jQuery( "#header" ).css('position', 'fixed');
    				jQuery( "#header" ).css('top', 'fixed');
    				
    				jQuery( "#header" ).css('top', '0px');
    				jQuery( ".header-wrapper" ).css('top', '0px');
    				jQuery( ".header-wrapper" ).css('float', 'none');
    			    menu_back_color =jQuery('body').css('background-color');
    			    if(jQuery('#sp-section').length){
    			    	jQuery('.sp-subheader').css('height', '260px');
    			    	jQuery('.sp-subheader .speed-logo').css('margin-top', '112px');
    			    }
    			    else{
    			    	if(jQuery( "#sliding-menu").css("display")=="none" ) {
    			    			jQuery( "#header" ).css('background-color', menu_back_color );
    					 }
    				}
    				jQuery('#tour-container').hide();
    			 }
    		}

    	}
    }
    jQuery( window ).scroll(function() {
    	fixedHeaderOnScrollwithGeicoBanner();
    });

</script>

		    
    
