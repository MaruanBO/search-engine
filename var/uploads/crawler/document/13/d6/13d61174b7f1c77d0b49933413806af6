	    	var mainbx = jQuery('.mobile-wrapper-art-sr');
// 	    	var $.browser.chrome = /chrom(e|ium)/.test(navigator.userAgent.toLowerCase());
	    	var mainbxBottom = mainbx.position().top + mainbx.outerHeight(true);
// 	    	if($.browser.chrome){
// 	    		var mainbxBottom = mainbx.position().top + mainbx.outerHeight(true) + 330;
//     		}

	        jQuery(document).ready(function() {
		    	for (var i=0; i<jQuery('.mobile-wrapper-art-sr .module-330 ul li').length; i++) {
		    		if (jQuery('.mobile-wrapper-art-sr .module-330 ul li:eq('+i+')').offset().top + jQuery(' .mobile-wrapper-art-sr .module-330 ul li:eq('+i+')').outerHeight(true) > mainbxBottom){
		  			        jQuery('.mobile-wrapper-art-sr .module-330 ul li:eq('+i+')').css("display","none");
		     				jQuery('.mobile-wrapper-art-sr .module-330 ul li:eq('+ (i-1)+')').css("border-bottom","none")
		    		}
		    	}
	        });
        }
