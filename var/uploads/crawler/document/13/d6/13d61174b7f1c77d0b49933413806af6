<script>
function createSocialbtnsOnSinglePages(){
	var referrer ="default";
	if(document.referrer){
		var reffArray= document.referrer.split('/')[2].split('.');
		if (reffArray.indexOf('t') > -1){
			referrer = 'twitter';
		}
		else{
			 referrer = 'default';
		}
	}
	if(jQuery('.article-social').length){
		if (referrer == 'twitter'){
			jQuery('.article-social').find('.second-button span').removeClass().addClass('twitter')
			jQuery('.article-social').find('.second-button span').css("width","120px");
			jQuery('.article-social').find('.second-button span').css("height", "120px");
			imageUrl = ("/bundles/twsite/images/social_buttons/twitter_big.png");
			jQuery('.article-social').find('.second-button span').css("background-image", "url(" + imageUrl + ")");
			jQuery('.article-social').find('.first-button span').removeClass().addClass('fb');
		}else{
			jQuery('.article-social').find('.second-button span').css("width","120px");
			jQuery('.article-social').find('.second-button span').css("height", "120px");
			imageUrl = ("/bundles/twsite/images/social_buttons/fb_big.png");
			jQuery('.article-social').find('.second-button span').css("background-image", "url(" + imageUrl + ")");
		}
	}else if(jQuery('.five-things-social').length){
		if (referrer == 'twitter'){
			jQuery('.five-things-social').find('.second-button span').removeClass().addClass('twitter')
			jQuery('.five-things-social').find('.second-button span').css("width","120px");
			jQuery('.five-things-social').find('.second-button span').css("height", "120px");
			imageUrl = ("/bundles/twsite/images/social_buttons/twitter_big.png");
			jQuery('.five-things-social').find('.second-button span').css("background-image", "url(" + imageUrl + ")");
			jQuery('.five-things-social').find('.first-button span').removeClass().addClass('fb');
		}else{
			jQuery('.five-things-social').find('.second-button span').css("width","120px");
			jQuery('.five-things-social').find('.second-button span').css("height", "120px");
			imageUrl = ("/bundles/twsite/images/social_buttons/fb_big.png");
			jQuery('.five-things-social').find('.second-button span').css("background-image", "url(" + imageUrl + ")");
		}
	 }

    jQuery('.twitter').sharrre({
		  share: {
		    twitter: true
		  },
		  enableHover: false,
		  enableTracking: true,
		  click: function(api, options){
		    api.simulateClick();
		    api.openPopup('twitter');
		  }
		});

	jQuery('.fb').sharrre({
		  share: {
		    facebook: true
		  },
		  enableHover: false,
		  enableTracking: true,
		  click: function(api, options){
		    api.simulateClick();
		    api.openPopup('facebook');
		  }
		});
}

</script>


