                sidVal = window.location.href.substring(sidLoc);
            } else {
                sidVal = window.location.href.substring(sidLoc, postSidLoc);
            }
        }
        var ronepix = document.getElementById('radiumOnePix');
        ronepix.src = 'http://panel.gwallet.com/network-node/pixel/healthguruclearpixel/1x1.gif?sid='+sidVal;
    };
    radiumCountDown.delay(radiumTime);   
}
window.which_test = '';
window.report = '';
</script>
