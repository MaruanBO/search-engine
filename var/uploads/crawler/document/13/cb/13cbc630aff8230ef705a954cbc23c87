function iFrameBust() {
    var checkInIframe = function() {
        return (window.location != window.parent.location) ? true : false;
    };
    
    var isInIframe = checkInIframe();
    if (!isInIframe) {
        return;
    }
    top.location.href = document.location.href;
}

