<script type="text/javascript">
    jQ(document).ready(function () {
        jQ('.jq-back-to-top-hg').attr('href', '#' + jQ('.sticky-nav').attr('id'));

        jQ(".footer-menu .jq-has-expansion").click(function () {
            //reset all
            jQ(".footer-menu-child").slideUp();
            jQ(".footer-menu .child").text("+");

            //open the clicked item

            if (jQ(this).next().is(":hidden")) {
                var nChildUlHeight = ( jQ(this).next().height() + 30 );
                jQ(this).parents().find(".footer-menu").css("height", nChildUlHeight);
                jQ(this).siblings().slideDown();
                jQ(this).find('.child').text("-");
            } else {
                jQ(this).next().slideUp('slow', function () {
                    jQ(this).parents().find(".footer-menu").css("height", 30);
                });
                jQ(this).find('.child').text("+");
            }
        });
        jQ(window).on("resize", function () {
            if ( jQ(".footer-menu .footer-menu-child").is(":visible") ) {
                var nChildUlHeight = ( jQ(".footer-menu .footer-menu-child").height() + 30 );
                jQ(".footer-menu").css("height", nChildUlHeight);
            } else {
                jQ(".footer-menu").css("height", 20);
            }
        });

    });
    
    // Begin Rocket Fuel Health Guru Universal Pixel
    (function () {
        var cachebust = (Math.random() + "").substr(2);
        var protocol = "https:" == document.location.protocol ? 'https:' : 'http:';
        new Image().src = protocol+"//20568063p.rfihub.com/ca.gif?rb=10245&ca=20568063&ra="+cachebust;
    })();
   // End Rocket Fuel Health Guru Universal Pixel
