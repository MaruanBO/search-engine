<!-- Exit Email Popup -->
<script type="text/javascript"> 
	var display_popup = true;
	function unload() {

		// Remove 
		return true;

		var source_code = get_cookie("smithsonianSourceCode");
		var smith_cart = get_cookie("smith_cart");
		var email_exit = get_cookie("email_exit");

		if((email_exit == "") && display_popup && (source_code == "") && (smith_cart != "") && (smith_cart != "Cart (0 items)")) {
			if(confirm("Would you like to be notified about the items you've left in your SmithsonianStore.com shopping cart?")) {
				//Check for IE
				if($.browser.msie) {
					set_cookie("email_exit", true, 1);
					window.location = "/checkout/basket.jsp";
				}else {
					var email = prompt("Please provide your email address:");
					$.ajax({ 
		                url: '/inetz/email_capture.jsp?email=' + email, 
		                cache: false
		            });
					set_cookie("email_exit", true, 1);
				}
			}
		}

	};

	function isInternalLink() {
		display_popup = false;
	}	
	$(document).ready(function() {
		$("a").each(function() {
			$(this).bind("click", function(){isInternalLink();});
		});
		$("input").each(function() {
			$(this).bind("click", function(){isInternalLink();});
		});
		$("select").each(function() {
			$(this).bind("change", function(){isInternalLink();});
		});		
	});
</script>

