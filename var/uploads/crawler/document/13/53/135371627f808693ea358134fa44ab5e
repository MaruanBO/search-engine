    $(document).ready(function(){
      ScrollingBanner();


      $("#safety-banner").mouseenter(function() {
        $("#safety-banner-people").hide();
        $("#safety-banner-links-view").css('top',-20);
        $("#safety-banner-links").show();
        $("#safety-banner-links-view").animate(
            {
                top:"32px",
            },500
        );
      }).mouseleave(function() {
        $("#safety-banner-links").hide();
        $("#safety-banner-people").show();
      });


    });
  </script>


