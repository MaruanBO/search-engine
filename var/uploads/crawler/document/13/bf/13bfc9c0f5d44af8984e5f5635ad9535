<script type="text/javascript">
    /* Temporary fix to change text on Minibag */
    $(document).ready(function() {
        $(".mbBottomTotalsContainer").livequery(function () {            
            var match = '3-7 Business Days';
            jQuery(".mbShipMessage:contains(" + match + ")").each(function(){
               var $this = $(this);
                $this.html(function(){
                    return $this.html().replace(match, "Estimated Shipping:");
                });
            });
        });
    });
</script>

<script type="text/javascript">
  /* Temporary fix to change text on Cart */
    jQuery(document).ready(function() {
        var match = '3-7 Business Days Shipping:';
        jQuery("table#cartTotalsTable div.ViewSr_shippingSummaryDiv:contains(" + match + ")").each(function(){
           var $this = $(this);
            $this.html(function(){
                return $this.html().replace(match, "Estimated Shipping:");
            });
        });
    });
</script>


