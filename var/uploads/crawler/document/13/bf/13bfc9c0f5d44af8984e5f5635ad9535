jQuery("body").on("click", "div.qlAddToCartContainer input#addToCart", function (e) {
    try {
                var productID = jQuery(e.target).data("pid");
                productID = productID.toString();
                var pContainer = jQuery(e.target).closest('div.qlRightBorderContainer');
                var productName = pContainer.find('div.qlTitleContainer a').attr('title');
                var retailPrice = pContainer.find('div.RegularContainer .price').html().replace('$', '');
                var salePrice = pContainer.find('div.SaleContainer .price').html().replace('$', '');
                BrTrk.getTracker().logEvent('Cart', 'add', { 'prod_id': productID, 'sku': productID, 'prod_name': productName }, { 'price': retailPrice, 'sale_price': salePrice, 'group': 'cart', 'etype': 'click-add' });
         
    } 
    catch(ex){
        
    }
});


