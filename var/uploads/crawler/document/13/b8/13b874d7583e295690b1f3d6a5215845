</div><script>Worldnow.Helpers.debug('UB Public Information File Home Page Col 4(242955)', 'UB_242955');</script><script>Worldnow.Helpers.debug('UB Archive Calendar - Shove(182941)', 'UB_182941');</script><div class="wnDVUtilityBlock"><script type="text/javascript">
GNM.Calendar = function(loc) {

	var
	
	// Templates
	templates = {
		controls:'<h3 class="gnmSubHeader">Find Stories by Date <span>&raquo;</span></h3><div class="controls"><a href="javascript:void(0);" rel="-1" class="prev">Previous</a><span class="month">{month} {year}</span><a href="javascript:void(0);" rel="1" class="next">Next</a></div>',
		calendar:'<table class="calendar"><thead><tr><th>Su</th><th>Mo</th><th>Tu</th><th>We</th><th>Th</th><th>Fr</th><th>Sa</th></tr></thead><tbody>{days}</tbody></table>',
		week:'<tr>{week}</tr>',
		dayClickable:'<td class="day past"><a href="#" rel="{date}">{date}</a></td>',
		dayCurrent:'<td class="day today"><a href="#" rel="{date}">{date}</a></td>',
		dayNone:'<td></td>',
		day:'<td class="day future">{date}</td>',
		search:'<div class="calSearch"><form action="/Global/searchresults.asp" method="get"><input type="hidden" name="vendor" value="ez" /><input type="text" name="qu" placeholder="Search Our Story Archive" /><button type="submit">Search</button></form></div>'
	},
	
	// Stored objects
	$container = $(loc),
	now = new Date(),
	current = now,
	minDate = new Date('12/5/2007'),
	station = wng_pageInfo.affiliateName||'kotv',
	months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],
	monthLengths = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31],
	
	// Helper methods
	renderTemplate = function(tpl, vars) {
		var retVal = templates[tpl];
		if (typeof(retVal) === 'string') {
			for (var i in vars) {
				var regEx = new RegExp('\{' + i + '\}', 'g');
				retVal = retVal.replace(regEx, vars[i]);
			}
		} else {
			retVal = null;
		}
		return retVal;
	},
	
	// Renderers
	makeCalendar = function(date) {
		
		// Copy the date to a new object (so as not to overwrite the original) and set us to the beginning of the month
		date = new Date(date);
		date.setDate(1);
		current = date;
		
		var
		month = date.getMonth(),
		year = date.getFullYear(),
		firstDay = date.getDay(),
		out = '',
		days = '',
		colCount = 0,
		monthLength = monthLengths[month] + (month == 1 && year % 4 == 0 ? 2 : 1); // Figure up the month length taking into consideration leap years. Not accurate to 100+ years
		
		// Render the days before the start of the month if necessary
		for (var i = 0; i < firstDay; i++) {
			days += templates.dayNone;
			colCount++;
		}
		
		// Render the main part of the calendar
		for (var i = 1; i < monthLength; i++) {
			var tpl = 'day';
			date.setDate(i);

			if (date.toDateString() == now.toDateString()) {
				tpl = 'dayCurrent';
			} else if (date < now && date >= minDate) {
				tpl = 'dayClickable';
			}
			days += renderTemplate(tpl, {date:i});
			colCount++;
			if (colCount % 7 == 0) {
				out += renderTemplate('week', {week:days});
				days = '';
			}
		}
		
		// Tack on the last week
		if (days != '') {
			out += renderTemplate('week', {week:days});
		}
		
		// Render to the DOM
		out = renderTemplate('calendar', {days:out});
		out = renderTemplate('controls', {month:months[month], year:year}) + out + templates.search;
		$container.html(out);
		
		// Determine whether the previous/next buttons should be shown
		date.setDate(1);
		if (date < minDate) {
			$container.find('.prev').hide();
		}
		if (month == now.getMonth() && year == now.getFullYear()) {
			$container.find('.next').hide();
		}
		
		// Set up events
		$container.find('.day').click(dateClick);
		$container.find('.controls a').click(controlClick);
		
		// Resize the height of the dates so that they are square
		var width = $container.find('.day').width();
		//$container.find('.day').height(width);
		
	},
	
	// Event callbacks
	ajaxLoaded = function(data) {
		window.location.href = 'http://' + wng_pageInfo.baseUrl + '/global/category.asp?c=' + data.catId;
	},
	
	dateClick = function(e) {
if (!$(e.currentTarget).hasClass('future')) {
		// Get the date that was clicked
		var rel = $(e.target).attr('rel');
		if (!rel) {
			rel = $(e.target).find('a').attr('rel');
		}
		
		// Make a call to get the category ID for this date
		var date = (current.getMonth() + 1) + '/' + rel + '/' + current.getFullYear();
		$.ajax({
			url:'http://www2.newson6.com/archives/query/json.aspx?dt=' + date + '&s=' + station,
			dataType:'jsonp',
			success:ajaxLoaded
		});
}
	},
	
	controlClick = function(e) {
		var dir = parseInt($(e.target).attr('rel'));
		
		if (dir) {
			var
			month = current.getMonth() + dir + 1,
			year = current.getFullYear();
			
			// Adjust the year and month accordingly if we go out of bounds
			if (month <= 0) {
				month = 12;
				year--;
			} else if (month > 12) {
				month = 1;
				year++;
			}
			makeCalendar(new Date(month + '/1/' + year));
		}
	},
	
	// Init
	init = function() {
		$container.addClass('gnmCalendar');
		makeCalendar(now);
	};
	
	init();

};
</script>
<style type="text/css">
    .gnmCalendar { width:325px !important; background:#f5f5f5; margin-bottom:15px; }
#WNCol4 .gnmCalendar { width:300px !important; }
    .gnmCalendar .calendar { width:100%; }
    .gnmCalendar .calendar th { background:#666; color:#fff; font:bold 14px sans-serif !important; }
    .gnmCalendar h3 { padding:10px 0; background:#fff; border-top:1px solid #ccc; border-bottom:1px solid #ccc; }
    .gnmCalendar .controls { position:relative; font:bold 14px sans-serif !important; padding:3px 0; }
    .gnmCalendar .controls a { display:block; width:16px; height:0; padding:16px 0 0; overflow:hidden; position:absolute; top:3px; background:url(http://ftpcontent.worldnow.com/kotv/custom/calendar/cal_arrows.png) no-repeat; }
    .gnmCalendar .controls span { text-align:center; display:block; }
    .gnmCalendar .controls .prev { left:3px; }
    .gnmCalendar .controls .next { right:3px; background-position:-16px 0; }
    .gnmCalendar .calendar .day { width:14%; height:27px; text-align:center; background:#ccc; font:12px sans-serif !important; overflow:hidden; }
    .gnmCalendar .calendar a { color:#000; text-decoration:none; font-weight:bold; display:block; width:100%; height:16px; padding:2px 0 0; }
    .gnmCalendar .calendar .future { color:#eee; height:26px; }
    .gnmCalendar .calendar .today { background:#f9cf6c; font-weight:bold; }
    .gnmCalendar .calendar .past:hover, .gnmCalendar .calendar .today:hover { background:#be0000 !important; cursor:pointer; }
    .gnmCalendar .calendar .past:hover a, .gnmCalendar .calendar .today:hover a { color:#fff !important; }
    .gnmCalendar .calSearch { width:300px; padding:5px; }
    .gnmCalendar .calSearch input { width:200px; margin-right:10px; }
    .gnmCalendar .calSearch button { width:80px; }
</style>
