	(function($) {
		
		var
		
		$gTopStories = $('#gTopStories'),
		$gDontMiss = $('#gDontMiss'),
		
		rowEqualizer = function() {
			$('.gContainerWrap').each(function() {				
				var height = $(this).height();
				$(this).find('.gContainerHalf').height(height);
			});
		},
		
		storyHover = function(e) {
			var $target = $(e.currentTarget);
			if (!$target.hasClass('gMore')) {
				$gTopStories.find('li.selected').removeClass('selected');
				$(e.currentTarget).addClass('selected');
			}
		},
		
		init = function() {
			if (typeof $gTopStories.attr('data-generated') == 'string') {
				GNM.log('TSB last updated at: ' + $gTopStories.attr('data-generated'));
			}
			rowEqualizer();
			$gTopStories.find('> li').mouseover(storyHover);
		};
		
		$(init);
		
	}(jQuery));
