$(document).ready(function() {

	// inject ads
	console.log("ad inj");
	var spontext = "<li class='gptadli spontext'>ADVERTISEMENT:</li>";
	<!-- VisitPhilly.com/www.visitphilly.com/Homepage/formatted-top -->
	var ad1 ="<LI class='gptadli'><!--AD1--><div class='adsection gpt345'><div id='div-gpt-ad-1362746860167-0' class='gptad gptad1'></div><!--AD1--></div></LI>";
	$(".split-alt .node.first ul.summary").append(spontext);
	$(".split-alt .node.first ul.summary").append(ad1);
	googletag.cmd.push(function() {googletag.display('div-gpt-ad-1362746860167-0');});
	var ad2 ="<LI class='gptadli'><!--AD2--><div class='adsection gpt345'><div id='div-gpt-ad-1362746844743-0' class='gptad gptad2'></div><!--AD2--></div></LI>";
                $(".split-alt .node.first ul.summary").append(spontext);
	$(".split-alt .node.first ul.summary").append(ad2);

	googletag.cmd.push(function() {googletag.display('div-gpt-ad-1362746844743-0');});

	$(".gptadli").show();

});
