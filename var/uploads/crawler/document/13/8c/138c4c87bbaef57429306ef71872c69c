				if (!analyticsfired || typeof(gatrackingscript) != "object" ) {
						 var msg = "ANALYTICS NOT FIRED AFTER: " + checksec;
						 console.log(analyticsfired);
						 console.log(typeof(gatrackingscript));
						 //console.log(ga);
						 sendrollbar(msg);
				}
				else {
					console.log("Analytics Fired");
				}
		}
},checksec);

