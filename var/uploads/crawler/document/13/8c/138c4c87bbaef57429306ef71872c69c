var isanadmin = false;
var gourl;
var cacherefresh = false;

var cacheone = false;
var cachetwo = false;

function bustcache() {
	gourl = window.location.href.split('?')[0];
	if (cacherefresh) {return;}
	cacherefresh = true;

	var rando = Math.floor((Math.random() * 10000) + 1);
	//var purl = "http://" + document.domain + "/utils/purge.php?url="+ escape(gourl) + "&" + rando;

	var purl1 = "http://192.237.179.26//utils/purge.php?url="+ escape(gourl) + "&" + rando;
	var purl2 = "";
	cachetwo = true;
	//var purl2 = "http://162.209.10.251/utils/purge.php?url="+ escape(gourl) + "&" + rando;

	console.log("UPDATE : PURGING URL....");
	console.log(purl1);
	console.log(purl2);
	$.ajax({
			  type: 'GET',
			  url: purl1,
			  dataType: 'text',
			  success: function(data) {
			  		cacheone = true;
			  		console.log("PURGE1  OK" + data);
			  		if (cacheone && cachetwo) {
			  			console.log("BOTH CACHES RESET");
				  		setTimeout(function() {
				  				location.href = gourl; // GO THERE
				  		},800);
				  	}

			    },
	     error: function (request, status, error) {
             console.log("PURGE ERROR 1");
             console.log(request);
             console.log(status);
             console.log(error);
             //location.href = gourl; // GO THERE


         }

	});

	if (purl2 !== "") {
		$.ajax({
				  type: 'GET',
				  url: purl2,
				  dataType: 'text',
				  success: function(data) {
				  	 	cachetwo = true;
				  		console.log("PURGE 2 OK" + data);
				  		if (cacheone && cachetwo) {
				  			console.log("BOTH CACHES RESET");
					  		setTimeout(function() {
					  				location.href = gourl; // GO THERE
					  		},800);
					  	}

				    },
		     error: function (request, status, error) {
	             console.log("PURGE ERROR 2");
	             console.log(request);
	             console.log(status);
	             console.log(error);
	             //location.href = gourl; // GO THERE


	         }

		});
	}



}
$.fn.isOnScreen = function(){

    var win = $(window);

    var viewport = {
        top : win.scrollTop(),
        left : win.scrollLeft()
    };
    viewport.right = viewport.left + win.width();
    viewport.bottom = viewport.top + win.height();

    var bounds = this.offset();
    bounds.right = bounds.left + this.outerWidth();
    bounds.bottom = bounds.top + this.outerHeight();

    return (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));

};

$(window).scroll(function() {imageslidercheck();});
function imageslidercheck() {
	$(".imageslider").each(function() {
		if ($(this).isOnScreen() && !$(this).hasClass("issetup")) {setupslider($(this));}

	});
}

function setupslider(obj) {
	obj.addClass("issetup");
	var iwidth = 0;
	obj.find("img").each(function() {
		if ($(this).width() > iwidth) {
			iwidth = $(this).width();
		}
	});
	if (iwidth > 0) {
		console.log("SET WIDTH: " + iwidth);
		obj.width(iwidth);
	}
	console.log("SETUP");
	console.log(obj);
	obj.twentytwenty();

}
$(document).ready(function() {		// READY

	imageslidercheck();

	$(".foottimeholder").append('<a class="cachebust" href="javascript:;" onclick="bustcache();"><i class="fa fa-rocket"></i>FLUSH VARNISH</a>');// jshint ignore:line

	if ((typeof(entry_id) == "string") && (typeof(weblog_id) == "string")) {
		if (!isNaN(entry_id) && !isNaN(weblog_id)) {
			$(".foottimeholder").append('<a class="adminlink" target="_admin" href="/admin.php?S=0&C=edit&M=edit_entry&weblog_id='+weblog_id+'&entry_id='+entry_id+'"><i class="fa fa-cog"></i>EDIT IN ADMIN</a>');// jshint ignore:line
		}
		else {
			$log("no admin link, bad ids");
		}
	}

	// set value based on cookie
	isanadmin = (!empty($.cookie("exp_sessionid")) && !empty($.cookie("exp_uniqueid")) && !empty($.cookie("exp_userhash")));
	//console.log("COOKIE CHECK");
	//console.log($.cookie("exp_sessionid"));
	//console.log($.cookie("exp_uniqueid"));
	//console.log($.cookie("exp_userhash"));
	//console.log(isanadmin);

	if (isanadmin) {
		setTimeout(function() {
			if (!mobile) {	// only show on non mobile
				$(".foottime").show();
			}
			else {
				console.log("MOBILE, Don't show admin");
			}
		},300);
	}

	if ((typeof(entry_id) == "string") && isanadmin) {	/// set current url and id to file for admin linking

		if (!isNaN(parseInt(entry_id,10))) {
			var docurl = document.URL;
			if (docurl.contains("?")) {
				docurl = docurl.slice(0, docurl.indexOf("?"));
			}
			geturl = "/urls/u.php?e="+entry_id+"&url="+encodeURIComponent(docurl);

			console.log("capturing url: http://" + document.domain + geturl);
			setTimeout(function() { $.get( geturl ); },10000);	// AFTER 10s
		}
	}
});

