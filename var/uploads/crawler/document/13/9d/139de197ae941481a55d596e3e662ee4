            jsname = jsname + '.js';
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? ('https://'+sslpref) : ('http://'+pref)) + domain + jsname;
