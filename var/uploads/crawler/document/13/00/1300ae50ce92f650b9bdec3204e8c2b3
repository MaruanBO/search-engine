    <script>
            //alert(screen.width);

            var cssFile, browserWidth, browserHeight;
            
            function browserSize(){
                // Get the dimensions of the viewport
                browserWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
                browserHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;

                //document.getElementById('jsWidth').innerHTML = browserWidth;  // Display the width
                //document.getElementById('jsHeight').innerHTML = browserHeight;// Display the height

                if(768 < browserWidth){
                    //full size desktop version
                    cssFile = "/css/computer.css?v=15";
                    
                                        
                    $('#in_the_news').insertBefore('#homepage_did_you_know');
                    $('.subscribe_webopedia_weekly').insertBefore('#homepage_did_you_know');
                    
                                        
                    $('nav').css("display", "block");

                }else if(640 < browserWidth && browserWidth < 769){
                    //tablet version
                    cssFile = "/css/tablet.css";
                    
                                        
                    $('#in_the_news').insertBefore('.social_follow_us');                   
                    $('.subscribe_webopedia_weekly').insertBefore('.social_follow_us');
                    
                                        
                    $('nav').css("display", "block");

                }else if(360 < browserWidth && browserWidth < 641){
                    //smartphone landscape mode
                    cssFile = "/css/phone-landscape.css";
                    
                                        
                    $('#in_the_news').insertBefore('#homepage_quick_reference ');

                                        $('nav').css("display", "none");
                    //$('#main-search').insertAfter('header');


                }else if(browserWidth < 361){
                    //smartphone portrait mode
                    cssFile = "/css/phone.css";
                    
                                        
                    $('#in_the_news').insertBefore('#homepage_quick_reference ');

                                        
                    $('nav').css("display", "none");

                }
                //alert(cssFile);
                document.getElementById('deviceStylesheet').href = cssFile;

            };

            window.onload = browserSize;       // When the page first loads
            window.onresize = browserSize;     // When the browser changes size
            
            /* CSS media queries seem to be applied sometime after the document.ready and before the window.load events.  If you are using the "onRedraw" callback, you should call it again after the page is finished loading. :-) */
            $(window).on('load', function () {

                $('#carousel_multi_slide').responsiveCarousel('redraw');
            });
        
</script>
