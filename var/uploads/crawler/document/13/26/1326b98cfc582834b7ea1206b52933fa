<div id="contenedorPublicidad"><div id="falsopadding"><div id="contenedorImgs"></div></div></div>

<script>
	var jqxhr = $.getJSON( urlMTEuro+"json/carrusel_seps/seps.json", function(json) {
		console.log(json);
	})
	.done(function(json) {
		console.log("DONE: Leí el especial de publicidad",json.length);
		$("#contenedorImgs").html("");
		$.each(json, function(index,dataJSON) {
			if(json.length-1 > index){
				$("#contenedorImgs").append('<a class="aPublicidad" href="'+dataJSON.url+'"><div class="newPublicidad"><img class="imgPublicidad" src="'+dataJSON.imagen+'" /><span class="titPublicidad">'+dataJSON.titulo+'</span></div><div class="divPublicidad"></div></a>');
			}
			else{
				$("#contenedorImgs").append('<a class="aPublicidad" href="'+dataJSON.url+'"><div class="newPublicidad"><img class="imgPublicidad" src="'+dataJSON.imagen+'" /><span class="titPublicidad">'+dataJSON.titulo+'</span></div></a>');
			}
		});
		if(json.length >= 5){
			$("#prevPublicidad").css("display","block");
			$("#nextPublicidad").css("display","block");
		}
		$( "#prevPublicidad" ).on( "click", function() {
			$('#falsopadding').animate({scrollLeft : '-=183'});
		});
		$( "#nextPublicidad" ).on( "click", function() {
			$('#falsopadding').animate({scrollLeft : '+=183'});
		});
	})
	.fail(function() {
		console.error("ERROR: No se pudo leer especial de publicidad");
	});
	jqxhr.complete(function() {
		console.log("COMPLETE: Terminé de cargar especial de publicidad");
	});
	$(document).ready(function() {
		$(".aPublicidad").mouseenter(function() {
			$(this).find('span').css("text-decoration","underline");
			$(this).find('span').css("color","white");
			$(this).find('img').addClass('transparent');
		}).mouseleave(function() {
			$(this).find('span').css("text-decoration","");
			$(this).find('.efectoImagen').css("display","none");
			$(this).find('img').removeClass('transparent');
		});
	});
</script>

