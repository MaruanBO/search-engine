<script type="text/javascript">
$(".botonequipo").mouseover(function(){
	$("div.lineabotonequipo").css("border-color", "");
	$(this).prev().css("background-color", "#FFCC00");
	$(this).next().css("background-color", "#FFCC00");
	$(this).css({"border-color":"#FFCC00", "background-color":"#E5E5E5"});
}).mouseout(function(){
	$(this).prev().css("background-color", "");
	$(this).next().css("background-color", "");
	$(this).css({"border-color":"", "background-color":""});
}).click(function(){
	contadorGoogleEvent('portada', 'Click', 'equipo-'+$(this).attr("equipo"));
	location.href = $(this).attr("link");
});
</script>
