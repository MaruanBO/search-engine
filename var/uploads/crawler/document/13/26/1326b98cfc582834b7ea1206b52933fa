			<script type="text/javascript">

				var showSpecialLeaderboard = function(tipoLeaderboard) {
					
					var previous = document.getElementById('leaderboard-especial-footer').src;
					previous = previous.substring(previous.lastIndexOf('_')+1, previous.lastIndexOf('.'));
					previous = 'leaderboard-'+previous;
					document.getElementById(previous).className="";
					document.getElementById('leaderboard-'+tipoLeaderboard).className="active";
					document.getElementById('leaderboard-especial-footer').src='http://euro.mediotiempo.com/publicidad/leaderboard_especial_'+tipoLeaderboard+'.html';
					return false;
				};				
			</script>

			<ul id="tabspub">
				<li id="leaderboard-netshoes" class="active"><a class="deportesad" href="#pub1" onclick="return showSpecialLeaderboard('netshoes')"><span>DEPORTES</span></a></li>
				<li id="leaderboard-dafiti" class=""><a class="modaad" href="#pub3" onclick="return showSpecialLeaderboard('dafiti')"><span>MODA</span></a></li>
			</ul>
		</div>

