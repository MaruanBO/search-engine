	<script src="//s3.amazonaws.com/brandcaptcha-st1/js/bcaptcha.js" type="text/javascript"></script>
	
	<script type="text/javascript">
		
		var pollsFeed = "http://euro.mediotiempo.com/json/encuesta_home.json";
		var poll_count = 1;
		var poll_current = 1;
		var poll_data = [];
		var refreshPollCountsInterval;
		var refreshPollCountsTimeMS = 30 * 1000;
		
		var pollFallbackHTML = "";
		
		//Handler that should take the clickEvent and a subsequentHandlerreturn 
		
		/**
		 * Optionally set this to a function that takes the original clickEvent.
		 * 
		 * The function should return a jQuery.Deferred object that will be used to append the 
		 * appropriate promise handlers. (The operation will continue only if the promise is resolved)
		 * 
		 * @param {DomEvent} clickEvent the original onClick event
		 * @param {callback} voteHandler a function that shuold be called passing it the original clickEvent
		 * @returns {Deferred|onBeforeVoteHandler.def}
		 */
		var onBeforeVoteHandler = null;
		
		// === BrandCaptcha Code === //  Just remove or comment this section to disable
		var captchaLoaded = false;
		onBeforeVoteHandler = function(clickEvent, voteHandler) {
			
			//BrandCaptcha loading and verification
			var def = $.Deferred(),
				msgDiv = $('#encuesta_captcha_msg');
			
			if(captchaLoaded) {
				BrandCaptcha.reload();
			} else {
				BrandCaptcha.initAsync('15c4ce55a57fe1a92bfd3ef5958dcd7076785147', 'encuesta_captcha_widget');
			}
			
			$('#encuesta').hide();
			$('#encuesta_captcha').show();
			msgDiv.html('Para validar tu voto por favor completa el siguiente CAPTCHA');
			
			var showError = function(msg) {
				msgDiv.html('<span style="font-weight: bold; color: darkred;">' + msg + '</span>');
			};
			
			var cForm = $('#encuesta_captcha_form');
				
			cForm.off('submit');
			cForm.on('submit', function(evt) {
				//Intercept form and send it through AJAX
				
				$.post(cForm[0].action, cForm.serialize())
					.done(function(data) {
						if(data.hasOwnProperty('status')) {
							if(data.status == 'ok') {
								console.info("CAPTCHA verification success.  Response: ", data);
								$('#encuesta').show();
								$('#encuesta_captcha').hide();
								def.resolve();
							} else {
								console.warn("Captcha verification failed.  Response: ", data);
								showError('Incorrecto, por favor intenta de nuevo');
								BrandCaptcha.reload();
								//def.reject();
							}
						} else {
							console.error("CAPTCHA verification failed.  Server response is: ", data);
							showError('Error.  Por favor intenta m&aacute;s tarde');
							//def.reject();
						}
					})
					.fail(function(jqXHR, textStatus, errorThrown) {
						console.error("CAPTCHA verification failed.  AJAX request could not be completed: ", textStatus, errorThrown, jqXHR);
						showError('Error.  Por favor intenta m&aacute;s tarde');
						//def.reject();
					});
				
				return false;
			});
			
			return def;
		};
		// ===  END BrandCaptcha  === //
		
		String.prototype.replaceAll = function(find, repl) {
			var ret = this;
			while(ret.indexOf(find) != -1) {ret = ret.replace(find, repl);}
			return ret;
		};
		
		//Set the percentage bar width and baloon value (newVal) for the answer 'answerID' of the 'pollID'
		function setAnswer(pollID, answerID, newVal) {
			$('#poll_' + pollID + '_answer_' + answerID + '_bar_inside').css('width', newVal + '%');
			$('#poll_' + pollID + '_answer_' + answerID + '_thumb').css('left', newVal + '%');
			$('#poll_' + pollID + '_answer_' + answerID + '_perc').text(newVal + ' %');
		}

		function onLoadClosureFactory(num) {
			var r = function() {
				$('#poll_' + num + '_answers>div').attr("class", "");
				$('#poll_' + num + '_answers .gig-button-container').attr("class", "");
				
				var answerDivs = $('#poll_' + num + '_answers td>div>div');
				answerDivs.each(function(j) {
					var divHTML = $(this).html();
					divHTML = divHTML.replaceAll('ANSID', j+1);
					$(this).html(divHTML);
					
					var checkboxJQElem = $(this).find('.answer_checkbox');
					if($.isFunction(onBeforeVoteHandler) && checkboxJQElem.hasClass('unchecked')) {
						var gigyaClickHandler = checkboxJQElem[0].onclick;
						checkboxJQElem.removeAttr('onclick');
						
						$(this).click(function(evt) {
							if(checkboxJQElem.hasClass('unchecked')) {
								onBeforeVoteHandler(evt)
									.done(function() {
										gigyaClickHandler(evt);
									});
							} else {
								//If checkbox is not unchecked, handle normally
								gigyaClickHandler(evt);
							}
						});						
					}
				});

				var pollDataStore = gigya.socialize.plugins.reactions.instances[poll_data[num-1].reactionParams.containerID];
				for(var buttonInstance in pollDataStore.buttonInstances) if(pollDataStore.buttonInstances.hasOwnProperty(buttonInstance)) {
					var buttonID = buttonInstance;
					buttonInstance = pollDataStore.buttonInstances[buttonInstance];
					if(buttonInstance.hasOwnProperty('state')) {
						$('#poll_' + num + '_answers .answer_bar').removeClass('invisible');
						if(buttonInstance.state == "Disabled") {
							//Checkbox should be disabled (because another one is checked)
							$('#' + buttonID + ' .answer_checkbox').removeClass('unchecked').removeClass('checked').addClass('disabled');
						} else if(buttonInstance.state == "Down") {
							//Checkbox should be checked
							$('#' + buttonID + ' .answer_checkbox').removeClass('unchecked').removeClass('disabled').addClass('checked');
						} else {
							console.log('POLLS WARNING: Unknown button state.');
						}
					} else {
						//Checkbox should be unchecked
						$('#' + buttonID + ' .answer_checkbox').removeClass('checked').removeClass('disabled').addClass('unchecked');
					}
				}
				window.setTimeout(function() {
					gigya.socialize.getReactionsCount(poll_data[num-1].countParams);
				}, 400);
			};
			return r;
		}

		function getCountClosureFactory(num) {
			var r = function(countData) {
				if(countData.errorCode == 0) {
					var totalCount = 0;
					for(var i in countData.counts) if(countData.counts.hasOwnProperty(i)) {
						totalCount += countData.counts[i].count;
					}
					for(var k in countData.counts) if(countData.counts.hasOwnProperty(k)) {
						var c = (totalCount == 0 ? 0 : Math.round(countData.counts[k].count * 100 / totalCount));
						setAnswer(num, parseInt(k,10)+1, c);	
					}
					$('#poll_' + num + '_answerCount_number').text(totalCount);
				} else {
					console.log('ERROR: Gigya getCount failed');
				}
			};
			return r;
		}

		function onReactionClickClosureFactory(num) {
			var r = function(ev) {
				var ansID = null;
				for(var q = 0; q < poll_data[num-1].reactionParams.reactions.length; q++) {
					if(ev.reaction.ID == poll_data[num-1].reactionParams.reactions[q].ID) {
						ansID = q+1;
						break;
					}
				}
				$('#poll_' + num + '_answers .answer_checkbox').removeClass('unchecked').addClass('disabled');
				$('#poll_' + num + '_answer_' + ansID + '_cb').removeClass('unchecked').removeClass('disabled').addClass('checked');
				$('#poll_' + num + '_answers .answer_bar').removeClass('invisible');
				window.setTimeout(function() {
					gigya.socialize.getReactionsCount(poll_data[num-1].countParams);
				}, 600);
			};
			return r;
		}

		function onReactionUnclickClosureFactory(num) {
			var r = function(ev) {
				$('#poll_' + num + '_answers .answer_checkbox').removeClass('disabled').removeClass('checked').addClass('unchecked');
				window.setTimeout(function() {
					gigya.socialize.getReactionsCount(poll_data[num-1].countParams);
				}, 600);
			};
			return r;
		}

		var refreshPollCounts = function() {
			if(poll_data) for(var i in poll_data) if(poll_data.hasOwnProperty(i)) {
				gigya.socialize.getReactionsCount(poll_data[i].countParams);
			}
		};
		
		/**
		 * Transforms the given string to lowercase and any non-alfanumeric (az09)
		 * character into an underscore (including spaces) so as to form a suitable
		 * HTML ID.
		 * @param {String} string
		 * @returns {String}
		 */
		function idfy(string) {
			string = string.toLowerCase();
			string = string.replace('[^a-z0-9]', '_');
			while(string.indexOf('__')) string = string.replace('__', '_');
			return string;
		}
		
		var loadPolls = function(feed) {
			if(typeof(feed)=="undefined") feed = pollsFeed;

			poll_data = [];
			corsTempGet(feed, function(data) {
				if(typeof JSON != "undefined")
					data = JSON.parse(data);
				else 
					data = eval("(function(){return " + data + ";})()");
				var i = 0;
				var pollsDiv = $('#encuesta');
				pollsDiv.html('');
				
				//We constrain the function to only load the first poll (i==0)
				for(var poll in data.questions) if(i==0 && data.questions.hasOwnProperty(poll)) {
					i++;
					
					var pollHtml = $('<div id="poll_' + i + '_question" class="question">' + data.questions[poll].title + '</div>\n' + 
						'<div id="poll_' + i + '_answers" class="answers"></div>\n' +
						'<div id="poll_' + i + '_answerCount" class="answer_count">Votos totales: <span id="poll_' + i + '_answerCount_number" class="number">-</span></div>');
					
					pollsDiv.append(pollHtml);

					var uaObj = new gigya.socialize.UserAction();
					uaObj.setLinkBack('http://mediotiempo.com/'); //TODO: Change this
					uaObj.setTitle(data.questions[poll].title);
					uaObj.setActionName("ha votado en"); 
					uaObj.setDescription('Tu opini&oacute;n es importante para nosotros. Vota en este sondeo');
					
					var buttonIDs = [];
					for(var btn in data.questions[poll].answers) if(data.questions[poll].answers.hasOwnProperty(btn)) {
						buttonIDs.push(data.questions[poll].answers[btn].id);
					}

					var onLoadClosure = onLoadClosureFactory(i);
					var onReactionClickClosure = onReactionClickClosureFactory(i);
					var onReactionUnclickClosure = onReactionUnclickClosureFactory(i);
					var getCountClosure = getCountClosureFactory(i);
					
					var tempReactions = [];
					for(var react in data.questions[poll].answers) if(data.questions[poll].answers.hasOwnProperty(react)) {
						var r = {
							ID: data.questions[poll].answers[react].id,
							text: data.questions[poll].answers[react].display,
							feedMessage: data.questions[poll].answerPrefixes.share + 
								(data.questions[poll].answers[react].share ? 
									data.questions[poll].answers[react].share : 
									data.questions[poll].answers[react].display),
							tooltip: data.questions[poll].answerPrefixes.tooltip + 
								(data.questions[poll].answers[react].tooltip ? 
									data.questions[poll].answers[react].tooltip : 
									data.questions[poll].answers[react].display)
						};
						tempReactions.push(r);
					}
					
					var single_poll_data = {
						pollID: data.questions[poll].id,
						countParams: {
							barID: data.questions[poll].id,
							buttonIDs: buttonIDs.toString(),
							callback: getCountClosure
						},
						reactionParams: {
							barID: data.questions[poll].id,
							reactions: tempReactions,
							userAction: uaObj,
							containerID: "poll_" + i + "_answers",
							layout: "vertical",
							multipleReactions: false,
							countType: "percentage",
							onLoad: onLoadClosure,
							onReactionClicked: onReactionClickClosure,
							onReactionUnclicked: onReactionUnclickClosure,
							buttonTemplate: "$count<div class=\"answer\" id=\"poll_" + i + "_answer_ANSID\">\n" + 
											"	<div class=\"answer_checkbox unchecked\" id=\"poll_" + i + "_answer_ANSID_cb\" onclick=\"$onClick\"></div>\n" + 
											"	<div class=\"answer_text\" id=\"poll_" + i + "_answer_ANSID_text\">$text</div>\n" + 
											"	<div class=\"answer_bar invisible\" id=\"poll_" + i + "_answer_ANSID_bar\">\n" + 
											"		<div id=\"poll_" + i + "_answer_ANSID_perc\" class=\"answer_perc\">--</div>\n" + 
											"		<div id=\"poll_" + i + "_answer_ANSID_thumb\" class=\"answer_thumb\"></div>\n" + 
											"		<div id=\"poll_" + i + "_answer_ANSID_bar_inside\" class=\"answer_bar_fill\" style=\"width: 0%\"></div>\n" + 
											"	</div>\n" + 
											"</div>",
							lang: "es"
						}
					};
					poll_data.push(single_poll_data);
					gigya.socialize.showReactionsBarUI(single_poll_data.reactionParams);
				}
				poll_count = i;

				window.setTimeout(refreshPollCounts, 100);
				if(refreshPollCountsInterval == null)
					refreshPollCountsInterval = window.setInterval(refreshPollCounts, refreshPollCountsTimeMS); //Conteos desde Gigya
			}, function() {
				//On error
				$('#encuesta').replaceWith(pollFallbackHTML);
			});
		};
		
		//*
		$(document).ready(function() {
			pollFallbackHTML = $('<div>').append($('#encuesta').clone()).html();
			loadPolls();
		});//*/
		
	</script>
	
    </div>
