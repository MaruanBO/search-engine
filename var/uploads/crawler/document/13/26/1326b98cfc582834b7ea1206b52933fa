<script>
/*Medio tiempo TV*/
var melementos = new Array();
var celemento = 0;
var melemento = function(img, vid) {
	this.thumb = img;
	this.file = vid;
};
var newPlayerHorizontal = function(scopeQ, isnew, whichxml) {
	var esnew = isnew || true;
	var scope = $(scopeQ);
	var urlxml = whichxml || urlMTEuro + "html/multimedia/videos/feeds/futbol.xml";
	if (scope.length == 0) {
		return false;
	}
	var elems = $(".mttv-playlist > div", scope);
	var partidoUrl = elems.attr("rel");
	var dia = new Date();
	var timestamp = dia.getTime();
	var flashvarsTop = {repeat: "list", id: "mttv-player", skin: urlMTEuro + "includes/swf/skinmttv.swf", stretching: "fill", playlist: "right", playlistsize: "480", file: "/xml_multimedia.php", 'gapro.accountid': 'UA-238420-1', 'gapro.trackstarts': true, 'gapro.trackpercentage': true, 'gapro.tracktime': true};
	var flashvarsTopBlip = {frontcolor: "0xDDDDDD", backcolor: "0xFFFFFF", lightcolor: "0xFFFFFF", brandname: " ", brandlink: urlMT, showguidebutton: false, showsharebutton: false, showmorebutton: false, enablejs: true, showplayerpath: "http://a.blip.tv/scripts/flash/stratos.swf?timestamp=" + timestamp, playerUrl: "http://a.blip.tv/scripts/flash/stratos.swf?timestamp=" + timestamp};
	$.extend(flashvarsTop, flashvarsTopBlip);
	if (esnew == true) {
		$("#" + flashvarsTop.id).html('<div style="width: 380px; height: 245px; background-color: rgb(255, 255, 255);position:absolute;" id="mttv-player-video"><div id="mttv-player-video-obj"></div></div>');
	} else {
		console.log('version del player no soportada');
	}
	corsTempGet(urlxml, function(data){
		var xml;
		if ($('body').hasClass('IE')) {
			xml = new ActiveXObject("Microsoft.XMLDOM");
			xml.async = false;
			xml.loadXML(data);
		} else {
			xml = data;
		}
		fillNewPlaylist("mttv-player", xml);
	});
};

var fillNewPlaylist = function(id, data) {
	$("#" + id + "-playlist").html("");
	destroyPerfectScroll();
	melementos = [];
	//
	var w = 212;
	var h = 52;
	var tagtitle = "title";
	if ($(tagtitle, $("item:first", data)).length == 0) {
		tagtitle = "titulo";
		data = data.replace(/<!\[CDATA\[/ig, "").replace(/\]\]>/ig, "").replace(/<link>http:\/\/www.mediotiempo.com\/<\/link>/ig, "").replace(/title/ig, "titulo");
	}
	var videos_num = 12;
	var videos_van = 0;
	$("item", data).each(function() {
		if (videos_van < videos_num) {
			videos_van++;
			var elemento = "<div class='melemento player-btn-off' style='height:" + h + "px;width:" + w + "px;margin:6px 0 3px 8px;'>";
			var thumb = $(this).get(0).getElementsByTagName("media:thumbnail");
			var file = $(this).get(0).getElementsByTagName("media:content");
			if (thumb && thumb[0] != undefined) {
				elemento = elemento + "<img src='" + thumb[0].getAttribute("url") + "' style='width:75px; height:45px;position: absolute;border: 1px solid #d6d6d6;margin: 3px 0 0 2px;'>";
				var thumb_final = thumb[0].getAttribute("url");
			}
			else {
				elemento = elemento + "<img src='" + urlMTEuro + "movil/mt/images/thumbVideo.png' style='width:75px; height:45px;position: absolute;border: 1px solid #d6d6d6;margin: 3px 0 0 2px;'>";
				var thumb_final = urlMTEuro + "movil/mt/images/thumbVideo.png";
			}
			var titulo = ($(tagtitle, this).text() != "") ? $(tagtitle, this).text() : $(tagtitle, this).html();
			var descripcion = ($("description", this).text() != "") ? $("description", this).text() : $("description", this).html();
			titulo = titulo.replace("<![CDATA[", "").replace("]]>", "").replace("<!--[CDATA[", "").replace("]]-->", "");
			descripcion = descripcion.replace("<![CDATA[", "").replace("]]>", "").replace("<!--[CDATA[", "").replace("]]-->", "");
			if(descripcion.length > 62){
				 descripcion = descripcion.substring(62, 0);
				 var ultima = descripcion.lastIndexOf(' ');
				 descripcion = descripcion.substring(ultima, 0);
				 descripcion = descripcion + '...';
			}
			elemento = elemento + "<div class='new-description' >" + descripcion + "</div>";
			elemento = elemento + "</div>";
			$("#" + id + "-playlist").append(elemento);
			var melem = new melemento(thumb_final, file[0].getAttribute("url"));
			melementos.push(melem);
		}
	});
	$('.melemento').click(function() {
		$('.melemento').removeClass('msel');
		$('.melemento').removeClass('player-btn-on');
		$('.melemento').addClass('player-btn-off');
		$(this).addClass('msel');
		$(this).removeClass('player-btn-off');
		$(this).addClass('player-btn-on');
		var pos = $('.melemento').index(this);
		var flashvarsTop = {skin: urlMTEuro + "includes/swf/blip-skin.swf", stretching: "fill", plugins: urlMTEuro + "includes/swf/justPreroll_v8.swf", duration: 33};
		flashvarsTop.file = melementos[pos].thumb;
		flashvarsTop.image = melementos[pos].thumb;
		celemento = pos;
		flashvarsTop.autostart = true;
		cargarNewVideo(true);
	}).css('cursor', 'pointer');
	var flashvarsTop = {skin: urlMTEuro + "includes/swf/blip-skin.swf", stretching: "fill", plugins: urlMTEuro + "includes/swf/justPreroll_v8.swf", duration: 33};
	flashvarsTop.file = melementos[0].thumb;
	flashvarsTop.image = melementos[0].thumb;
	celemento = 0;
	flashvarsTop.autostart = true;
	iniciaPerfectScroll();
	cargarNewVideo(false);
};

var cargarNewVideo = function(autostart, link, thumb) {
	var dia = new Date();
	var timestamp = dia.getTime();
	link = link || melementos[celemento].file;
	thumb = thumb || melementos[celemento].thumb;
	if (link.indexOf('blip.tv') != -1) {
		var flashvarsTop = {frontcolor: "0xDDDDDD", backcolor: "0xFFFFFF", lightcolor: "0xFFFFFF", brandname: " ", brandlink: urlMT, showguidebutton: false, showsharebutton: false, showmorebutton: false, enablejs: true};
		flashvarsTop.file = link;
		flashvarsTop.showplayerpath = "http://a.blip.tv/scripts/flash/stratos.swf?timestamp=" + timestamp;
	} else {
		var flashvarsTop = {frontcolor: "0xBBBBBB", backcolor: "0xFFFFFF", overcolor: "0x4B4B4B"};
		flashvarsTop.embedCode = link;
		flashvarsTop.showplayerpath = urlMTEuro + "includes/swf/mtooyala.swf";
	}
	flashvarsTop.image = thumb;
	flashvarsTop.thumb = thumb;
	flashvarsTop.playerUrl = flashvarsTop.showplayerpath;
	//flashvarsTop.enableHtml5Player = true;
	flashvarsTop.autostart = autostart;
	//if (flashvarsTop.autostart && flashvarsTop.autostart != false) {flashvarsTop.autoplay=true;}
	if (flashvarsTop.showplayerpath == urlMTEuro + "includes/swf/mtooyala.swf") {
		videoPlayers++;
		if (flashvarsTop.autostart) {
			Ooyala.loadVideo('mttv-player-video-obj', flashvarsTop.embedCode, {autoplay: true});
		}
		else {
			Ooyala.loadVideo('mttv-player-video-obj', flashvarsTop.embedCode, {autoplay: false});
		}
	} else {
		swfobject.embedSWF(flashvarsTop.showplayerpath, "mttv-player-video-obj", "480", "290", "9", '', flashvarsTop, {allowscriptaccess: "always", wmode: "transparent", allowfullscreen: "true"}, {style: "width:480px; height:263px;"});
	}
};

var iniciaPerfectScroll = function() {
	$('#mttv-player-playlist').perfectScrollbar({
		wheelSpeed: 40,
		suppressScrollX: true
	});
}
var destroyPerfectScroll = function() {
	$('#mttv-player-playlist').perfectScrollbar('destroy');
}

var Ooyala = {
	scriptLoaded: false
	, _scriptLoading: false
	, defaultPlatform: '?platform=flash'
	, playerSkinID: 'ZGQwMjQzNWU3OTAxNTJhYzQ3ZjgxZWMx'
	, defaultOptions: {wmode: 'opaque', autoplay: false, height: '100%', width: '100%'}
	, thruParam: '&thruParam_liverail-ads-manager[LR_AUTOPLAY]=0&thruParam_vast[tagUrl]=' + encodeURIComponent("http://ad.amgdgt.com/ads/?t=dr&f=vast2&p=8851&pl=b3e84fca&rnd=" + Math.floor(Math.random()*1000000))
	, playerInstances: []

			/**
			 * Asynchronously load the Ooyala main script if not loaded already.
			 * After loading, all previously "loaded" videos (through the {@link loadVideo()} 
			 * funciton will be initialized.
			 * 
			 * @returns {null|jQuery.Promise}
			 */
	, loadScript: function() {
		if (!this._scriptLoading) {
			if (typeof OO !== 'undefined') {
				/* Don't load the script again if it is already loaded.
				 * This is because some browsers may load the script even after the
				 * AJAX call timed out and the .fail handler is executed.
				 */
				this.scriptLoaded = true;
				this._loadAllPendingVideos();
				return null;
			} else {
				this._scriptLoading = true;
				var me = this;
				return $.ajax({
					dataType: "script",
					cache: true,
					url: "http://player.ooyala.com/v3/" + this.playerSkinID + this.defaultPlatform +this.thruParam
				})
						.done(function(script, textStatus) {
							//console.log('Ooyala script loaded');
							me.scriptLoaded = true;
							me._scriptLoading = false;
							OO.ready(function() {
								me._loadAllPendingVideos();
							});
						})
						.fail(function(jqxhr, settings, exception) {
							me._scriptLoading = false;
							console.warning('ERROR: Could not load Ooyala player script (retrying in 8 seconds).  Exception: ', exception);
							window.setTimeout(me.loadScript, 8000); //Try again after some time
						});
			}
		}
	}

	/**
	 * Load the specified video, instantiating a new Ooyala Player into the 
	 * given <code>divID</code>.<br>
	 * 
	 * This function automatically executes the {@link loadScript()} function
	 * if necessary.
	 * 
	 * This function will defer the player instantiation if the ooyala script
	 * has not been loaded yet.
	 * 
	 * @param {string} divID the ID of the div where the player should be instantiated
	 * @param {string} embedCode the embed code for the video to be loaded
	 * @param {object} ooyalaOptions [optional] this parameter overrides the default player options
	 * @returns {player|null} 
	 */
	, loadVideo: function(divID, embedCode, ooyalaOptions) {
		console.log('Loading ooyala player embed: ', embedCode, ' on div: ', divID, '.  Options: ', ooyalaOptions);
		var player;
		ooyalaOptions = typeof ooyalaOptions !== 'undefined' ? ooyalaOptions : this.defaultOptions;
		if (typeof divID == "string") { //Create new player
			var loaded = false;
			if (this.scriptLoaded) {
				for (var pi in this.playerInstances)
					if (this.playerInstances.hasOwnProperty(pi)) {
						if (this.playerInstances[pi].hasOwnProperty('divID') && this.playerInstances[pi].divID == divID)
							this.playerInstances[pi].player.destroy();
					}
				player = OO.Player.create(divID, embedCode, ooyalaOptions);
				loaded = true;
			} else
				this.loadScript();
			this.playerInstances.push({loaded: loaded, player: player, divID: divID, embedCode: embedCode, options: ooyalaOptions});
			return player;
		} else {
			console.error('Error: Cannot load video.  Null ID parameter');
			return null;
		}
	}

	, _loadAllPendingVideos: function() {
		for (var v in this.playerInstances)
			if (this.playerInstances.hasOwnProperty(v)) {
				var vid = this.playerInstances[v];
				if (!vid.loaded || (vid.player && (!vid.player.state || vid.state == "destroyed"))) {
					vid.player = OO.Player.create(vid.divID, vid.embedCode, vid.options);
					vid.loaded = true;
				}
			}
	}
};
$(document).ready(function(){
	newPlayerHorizontal("#mttv");
	$('#menu_player_button').click(function() {
		$('#channels_menu').slideToggle(250);
	});
	$('.canal').click(function() {
		var id_canal = $(this).data('idChannel');
		$('.canal').removeClass('selected');
		$(this).addClass('selected');
		$('#channels_menu').slideToggle(250);
		if (id_canal == 1){
			newPlayerHorizontal("#mttv",true,urlMTEuro+"html/multimedia/videos/feeds/futbol.xml");
			$('#menu_player_button_name').text('Futbol');
		}
		if (id_canal == 2){
			newPlayerHorizontal("#mttv",true,urlMTEuro+"html/multimedia/videos/feeds/mas-deportes.xml");
			$('#menu_player_button_name').html('M&aacute;s Deportes');
		}
	});
});
</script>
