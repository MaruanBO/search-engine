var d = document.getElementById("pl-bottom");
var html = d.innerHTML;

jwplayer("player-custom-front-block").onReady(function(){
var playlist = jwplayer("player-custom-front-block").getPlaylist();
for (var index=0;index<playlist.length;index++){
var playindex = index +1;
    var activeClass = '';
    var vli = 'vli' +index;
    if(index == 0) activeClass = 'class="active"';
html += '<li id="' +vli+ '" ' + activeClass + '><a href="javascript:playThis('+index+')" '
html += 'onclick="showActive('+'\''+vli+'\''+')">'
html += playlist[index].thumb + "</a><br>"
html += '<a onclick="showActive('+'\''+vli+'\''+')" href="javascript:playThis('+index+')">'+playlist[index].title+'</a>'
html += "</li>"
d.innerHTML = html;
}

});
function playThis(index) {
jwplayer("player-custom-front-block").playlistItem(index);
}

function showActive(vli){
    $('.frontVideo').find('.active').removeClass('active');
    $('#' +vli).addClass('active');
}

