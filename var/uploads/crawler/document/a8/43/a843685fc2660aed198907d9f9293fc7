<script type="text/javascript">
//<![CDATA[
	var siteChoice	= '';
	var stayFull	= 'false';
	var currentSite = 'full';
	var sbSignup	= 'false';
	var pixels		= window.screen.availHeight * window.screen.availWidth;
	
	//set the expire date to 7 days from now (604,800,000 milliseconds)
	var date = new Date();
	date.setTime(date.getTime()+(604800000));
	
	// If this is set to true, the user is never sent to mobile and the mobile nav bar is gone.
	function setStayFull() {
		document.getElementById('mobileContainer').style.display = 'none';
		document.cookie = 'stayFull=true; max-age=2700; domain=.bookit.com; path=/';
	}
	
	//sets a cookie for the users preference of either the mobile or full site
	function setSiteChoice(choice) {
		document.cookie = 'siteChoice='+choice+'; max-age=2700; domain=.bookit.com; path=/';
		if (choice == 'mobile') {
			window.location = 'http://m.bookit.com?resetpos=1';
		}
	}
	
	if (sbSignup == 'true') {
		setSiteChoice('full');
	}

	if (stayFull !== 'true' || stayFull.length == 0) { 
		if (document.getElementById('mobileContainer')) {
			document.getElementById('mobileContainer').style.display = 'block';
		}
	}
	
	if (currentSite == 'voucher' && siteChoice !== 'full') {
			setTimeout(function() {
				var url = (window.location.href).split("?");
				var newResolution = window.screen.availWidth * window.screen.availHeight;
				if (newResolution < 480000) {
					window.location = 'http://m.bookit.com/voucher.php?'+url[1];
					exit();
				}
			}, 300);
			
	} else if (pixels <= 480000 && siteChoice !== 'full' && currentSite !== "mobile") {
		window.location = 'http://m.bookit.com?resetpos=1';
	}
//]]>
