<script type='text/javascript'>

jQuery(document).ready(function() {

	jQuery('#signOutMember').click(function() {
		//BookitJs.memberSignin module should be used whenever present, ignore this event if it is, keep for backwards compatability when its not present
		if (!window.BookitJs || !window.BookitJs.memberSignIn) {
		var params = {};

		jQuery.ajax({
			type : 'POST',
			url : 'ajaxRedirect.php',
			data : {'action' : 'server_members.php', 'params' : JSON.stringify(params), 'handle' : 'signOutMember'},
			dataType : 'json',
			success : function(memberInfo) {

				// check if any errors were returned and handle them accordingly
				if (memberInfo.status == false) {

					if(memberInfo.code == 100) {

						// show your custom errors, if any, here

					} else {
						alert('An error occurred!');
					}
				} else {
					if (jQuery(location).attr('href').search('/member')>0) {
						window.location.replace("http://" + "bookit.com" );
					}
					jQuery('#signOutMember').hide();
					jQuery('#accountLink').hide();
					jQuery('#memberAccount').html('You have been signed out.');
					jQuery('#memberAccount').animate({top: 0}, 3000).fadeOut();
					jQuery('#signInMember').show();

				}
			}
		});
		}
	});

	if (window.BookitJs && BookitJs.events) {
		BookitJs.subscribe(BookitJs.events.MEMBER_SIGN_IN,function(member){

			jQuery('#UserOptions')
				.find('.loggedInButtons')
					.show()
					.html((["<a class=\"grayBtn btnContainer\" id=\"signOutMember\" href=\"javascript:void(0);\" onclick=\"jQuery(this).parent().fadeOut('slow');if(window.BookitJs && window.BookitJs.memberSignIn){BookitJs.memberSignIn.signout(event);}\">Sign Out<\/a>\n<a class=\"grayBtn btnContainer \" id=\"accountLink\" href=\"http:\/\/from.bookit.com\/member\/profile.php\">Your Account<\/a>\n"])[0])
					.end()
				.find('.logged-in-text')
					.removeClass('hidden')
					.end()
				.find('.userFirstName')
				.html(member.memberFirstName)
					.end()
				.find('#signInMember')
					.hide()
					.end();
		});

		BookitJs.subscribe(BookitJs.events.MEMBER_SIGN_OUT,function(member){
			jQuery('#UserOptions')
				.find('.loggedInButtons')
					.html('')
					.end()
				.find('.logged-in-text')
					.addClass('hidden')
					.end()
				.find('#signInMember')
					.show();

		});
	}
	});
</script>

