<script type="text/javascript">
	var currentPage = {};
	var originalSpecialRequestText = "Search BookIt.com";
	originalSpecialRequestText=  jQuery('<span/>').html(originalSpecialRequestText).text();
	function initClearSearchBox(){
	  jQuery('input[name="q"]').val(originalSpecialRequestText);
	  //Add focus and blur events to set or clear the value
	  jQuery('input[name="q"]').bind('focus', function() {
		jQuery(this).val('');
	  });
	  jQuery('input[name="q"]').bind('blur', function() {
	    if(jQuery(this).val() == '') {
		jQuery(this).val(originalSpecialRequestText);
		jQuery(this).css('color','#888888');
	    }
	  });
	}

	jQuery(document).ready(function(){
		initClearSearchBox();
		var heightMoreOn = '61px';
		//var classOn = jQuery('#mainNavMore[class*=On]').attr('class').split(/\s+/)[0];
		jQuery('#mainNavMore').click(function() {
			var height = jQuery('.more').css('height');
			if (height == heightMoreOn) { // turn the 'More' navigation off
				jQuery(this).removeClass('moreOn on').css('height','30px');
			} else { // turn the 'More' navigation on
				jQuery(this).removeClass('moreOff');
				jQuery(this).addClass('moreOn on').css('height','61px');
			}
		}
    );
		// Drop downs
		jQuery(".home, .package, .hotel, .flight, .activities, .sub_hoverMenu").hover(
	        function(){
	            jQuery(this).show().closest('ul').next('.sub_hoverMenu').show();
	            jQuery(this).closest('ul').next('.sub_hoverMenu').show().data('last_tab', jQuery(this));
	            if( jQuery(this).hasClass('sub_hoverMenu')) {
	                jQuery( jQuery(this).data('last_tab') ).addClass('hover');
	            } else {
	                jQuery(this).addClass('hover');
	            }

	            if( jQuery(this).hasClass('sub_hoverMenu') === false) {
	                jQuery('.sub_hoverMenu').find('.inner').children().hide();
	                var offset = jQuery(this).position();
	                jQuery('.sub_hoverMenu').css({'left': offset.left+'px'});
	            } else {

	            }
	            if(jQuery(this).hasClass('home')) {
	                jQuery('.sub_hoverMenu').children('.inner').find('.hoverHome').show();
				} else if(jQuery(this).hasClass('package')) {
	                jQuery('.sub_hoverMenu').children('.inner').find('.hoverPackage').show();
				} else if(jQuery(this).hasClass('hotel')) {
	                jQuery('.sub_hoverMenu').children('.inner').find('.hoverHotel').show();
				} else if(jQuery(this).hasClass('flight')) {
	                jQuery('.sub_hoverMenu').children('.inner').find('.hoverFlights').show();
				} else if(jQuery(this).hasClass('activities')) {
	                jQuery('.sub_hoverMenu').children('.inner').find('.hoverActivities').show();
				}
	        },
	        function(){
	            if( jQuery(this).hasClass('sub_hoverMenu')) {
	                jQuery(this).hide();
	                jQuery( jQuery(this).data('last_tab') ).removeClass('hover');
	            } else {
	                var sub_hoverMenu = jQuery(this).closest('ul').next('.sub_hoverMenu');
	                jQuery(this).removeClass('hover');
	                sub_hoverMenu.hide();
	            }
	        }
	    );
	});
</script>
