<script type="text/javascript">
	$(document).ready(function(){
		$.ajax({
			url:'/apps/json/aflcio_unions.json',
			dataType : 'json',
			success:function(data){
				$.each(data, function(key, val) {
					$("#reg_union_org").append('<option value="'+key+'">'+key+'</option>');
					$("#form_union_org").append('<option value="'+key+'">'+key+'</option>');
					});
			}
		});
	});
			</script>
		


			
				
		
	


