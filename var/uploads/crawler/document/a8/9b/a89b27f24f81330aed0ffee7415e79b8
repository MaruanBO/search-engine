<script type="text/javascript" language="javascript">

$(document).ready(function(){

 $("#getLocalForm").validate({
	rules: {
	 postalCode: {
     required: true,
	 zipRegex: true
	 }
   },
   highlight: function(label) {
    $(label).closest('.group').addClass('error');
	},
   messages: {
			postalCode: {
				required: "Enter a zip",
				zipRegex: "Enter a 5 or 9 digit zip"
			}
	}
		
	});



$.validator.addMethod("zipRegex", function(value, element) {
        return this.optional(element) || /^(\d{5}|\d{5}\-\d{4})$/i.test(value);
    });

});	


</script>
	
