<script type="text/javascript">
	$(document).ready(function(){
		/**
		 * Connect With Us Form Handling
		**/
		$('#getEmailForm').live('submit',function(){
			var email = $.trim($('#cwu_emailAddress').val());
			var zip = $.trim($('#cwu_zipCode').val());
            var msg = '';
			var phone = $.trim($('#cwu_phone').val());
            var phoneNumber = /[0-9-()+]{3,20}/;
			if ($('#mc_text_opt_in').is(':checked')){
                if(!phoneNumber.test(phone)) {
                 msg ='Uncheck "GET TEXT FROM AFL-CIO" or enter a valid phone number.\n\n';
                 }
             }

			if(is_email(email)){
				$(this).find('input[name="redirect"]').each(function(){
					var $this = $(this);
					var appendString = email;
					(typeof zip === 'string' && zip !== "")?appendString += "&zip="+zip:false;
					$this.val($this.val() + appendString);
				});
			}
			else{
				alert('In order to receive email from AFL-CIO, please enter a valid email address.\n\n' + msg);
				return false;
			}
		});		
	});
</script>

            
