//s_account = ((typeof omni_account )!="undefined" && omni_account != "") ? omni_account  : "hrblockglobal,hrblocktcutcom";  // for taxcut.com

try{
	if (g_sys.checkEnv()=="LnP" || g_sys.checkEnv()=="DEV" || g_sys.checkEnv()=="QA"){
		s_account = "hrblockcomdev";
	}
}
catch(e)
{
	try {
		// support for pages that implement omniture_page_view_recording.js which might not include g_sys
		if (g_sys.checkEnv()=="LnP" || parent.g_sys.checkEnv()=="DEV" || parent.g_sys.checkEnv()=="QA"){
			s_account = "hrblockcomdev";
		}
	}
	catch(e){}
}
</script>

