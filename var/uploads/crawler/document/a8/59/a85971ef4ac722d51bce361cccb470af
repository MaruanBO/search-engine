<script>
    var i18nValue = "USA / English";

    $(document).ready(function() {
        seti18n();
    });

    function seti18n() {
        var loc = window.location.href;
        var selectedObj = null;
        if (loc.indexOf("tax-offices/international") == -1) {
            i18nValue = "USA / English";
            $(".dropdown-menu li").find("a[href='/']").parent().css("display", "none");
        } else if (loc.indexOf("tax-offices/international") > -1) {
            var match = false;
            $(".dropdown-menu li").each(function(index) {
                var linkText = $(this).find("a").attr("href");
                $(this).css("display", "block");
                loc = loc.substr(loc.indexOf("/tax-offices"));
                if (loc == linkText && match == false) {
                    i18nValue = $(this).find("a").text();
                    selectedObj = this;
                    match = true;
                }
            });
            if (selectedObj != null)
                $(selectedObj).css("display", "none");
        }


        $("#i18n").text(i18nValue);
    }
</script>






