<script type="text/javascript">
$(document).ready(function(){
		// WIDGET CLICK TRACKING					
		function writeWidgetInfo(widgetName){
			// set eVar36 for click tracking in this page
			HRB.dataLayer.eVar36 = "";
			HRB.dataLayer.eVar36 = omni_pagename + ":widget:offices:nearest:" + widgetName + ":en";
			
			// set session storage for retrieval in appointment flow destination page
			try{
				sessionStorage.setItem('widget', "widget:offices:nearest:" + widgetName);
			} catch (e) {
				console.log('Error: Saving widget name to local storage.');
			}
		}
		
		// Directions links
		$('.map-office-pod .office-address .address a, .map-office-pod .directions').on('click', function(){
			writeWidgetInfo("directions");
		})
		// Office Info link
		$('.tps-in-office').on('click', function(){
			writeWidgetInfo("view office");
		})
		// Make an Appointment button
		$('.btn.make-appt').on('click', function(){
			writeWidgetInfo("choose appt");
		})
		// See Appointment Options link
		$('.other-appt').on('click', function(){
			writeWidgetInfo("other appt");
		})
                // Office Info link
		$('.zip-search').on('click', function(){
			writeWidgetInfo("find an office");
		})
	});
</script>
