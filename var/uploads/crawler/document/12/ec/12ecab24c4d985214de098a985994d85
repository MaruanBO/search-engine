var ucxAccess = "A2C68263";
var ucxKeycode = "";
var ucxCampaign = "utm_source:" + UCX.GetParameter("utm_source") + "|utm_medium:" + UCX.GetParameter("utm_medium") + "|utm_campaign:" + UCX.GetParameter("utm_campaign") + "|utm_keyword:" + UCX.GetParameter("utm_keyword");
