<script>
jQuery(function() {
  $.getJSON('/category/Customer_Favorites_Puzzle?jsonviewprods=true', function(data) {
    var items = [];
    var i = 0;
    $.each(data.data, function(key, val) {
      if(i < 5) {
        var frm = '<form method="post" action="index.php" id="customer-favorites-puzzle-buynow-'+val.sku+'_form" style="margin:0;padding:0;">'
          + '<input type="hidden" name="action" value="AddCart" />'
          + '<input type="hidden" name="prod_name" value="'+val.prod_name+'" />'
          + '<input type="hidden" name="prod_id" value="'+val.prod_id+'" />'
          + '<input type="hidden" name="cat_id" value="'+val.categories[0]+'" />'
          + '<input type="hidden" name="sku" value="'+val.sku+'" />'
          + '<input type="hidden" name="price" value="'+val.price+'" />'
          + '<input type="hidden" name="qty" id="qty_'+val.prod_id+'" value="1"  class="formtext" maxlength="3" />'
          + '<input type="submit" value="Add To Cart" class="red_btn" alt="Add To Cart" name="submit">'
          + '</form>';
         
      
          
        if((val.is_parent=='y')||(val.stock_status=='Out of Stock') || (val.has_attributes == 'y'))
          frm = '<a href="/product/'+val.url_name+'"class="more-info">More Info »</a>';
        var prc = '<b>$'+parseFloat(val.price).toFixed(2)+'</b>';
        if((parseFloat(val.special_price) > 0) && (parseFloat(val.special_price) < parseFloat(val.standard_price)))
          prc = '<b style="text-decoration:line-through;">$'+parseFloat(val.standard_price).toFixed(2)+'</b><br>'
              + '<b class="red_txt">$'+parseFloat(val.special_price).toFixed(2)+'</b>';
              
        items.push('<div class="product_tile">'
        + '<a class="quicklook-button" href="/product/'+val.url_name+'?quickview=true" rel="#overlay"></a>'
          + '<div>'
          + '<a href="/product/'+val.url_name+'">'
          + '<img class="lazy" src="http://cdn.bitsandpieces.com'+val.web_image+'" data-original="http://cdn.bitsandpieces.com'+val.web_image+'" border="0" alt="'+val.prod_name+'" style="width: 99%; display: inline;">'
          +'</a>'
          
          +'<div class="pro_detail">'
          +'<div style="margin: 0 0 5px;">'
          +'<a href="/product/'+val.url_name+'" class="catblue">'
          + val.prod_name
          +'</a>'
          +'</div>'
          
          +'<span class="left">'
          +prc
          +'</span>'
          + '<div style="float:right">'
          +frm
          + '</div>'
          + '</div>'
          + '</div>'
          +'</div>');
          
        i++;
      }
    });
    $('<div/>', {
      'class': 'products',
      html: items.join('')
    }).appendTo('#customer_favorites_puzzle');
    $(".quicklook-button").colorbox();
    
    i = 0;
    $.each(data.data, function(key, val) {
      if(i < 5) {
        
        if((val.is_parent!='y')&&(val.stock_status!='Out of Stock') || (val.has_attributes == 'y'))
          windowshade_add_to_cart('#customer-favorites-puzzle-buynow-'+val.sku+'_form',val.sku);
        i++;
      }
    });    
  });
});
</script>


