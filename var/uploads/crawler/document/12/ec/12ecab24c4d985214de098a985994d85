          <script type="text/javascript">
    function lookup(inputString) {
        $("#searchinput").blur(function () { setTimeout("clearResults()", 500) });
      
        var searchstring = trim(inputString);
        if (searchstring.length > 3) {
            if (searchstring.length % 2 == 0) {
              getResults(searchstring);
            }           
        }
        else {
            clearResults();
        }
    }
    $('.gotopage').live('click', function () {

        var hreflid = $(this).attr('id');
        window.location = hreflid;     
    });    
    function hidelookup() {
        $("#autoSuggestionsList").html('');
        $("#suggestions").hide();

    }
    function getResults(inputString) {
      $("#LoadingSearch").show();
            $.getJSON("/services/michiganbulb/get_search.php", { store: "bitsus" , keyword: "" + inputString + "" },
                function (data) { 
                    if (data.length > 0) {
                        $("#LoadingSearch").hide();
                        $("#suggestions").show();
                     
                       var result = data[0].itemList;
                      
                        var itemCount = data[0].count;
		     
                      
                        $('#autoSuggestionsList').html('<div style=\"margin-left:45%;padding:5px;\"></div>');
                        var searchstr = '<ul style=\"margin:0px; padding:0px;\">';
                        
                        if(data[0].no_matches == 'y')
                        {
                           searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:10px 5px;background-color: #ff9b28; font-size:11px; color:#98232b;\">';
                           searchstr += 'No Matches found for keyword: <strong>' + inputString + '</strong>.<br><strong style=\"color:#3d3d3d;\">Showing the top 5 best sellers</strong>';
                           searchstr += '<div></li>';
                           
                          
                        }
                        else
                        {
                           searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:10px 5px;background-color: #ff9b28; font-size:11px;\">';
                           if (itemCount < 5)
                           {
	                           searchstr += 'Showing the top '+ itemCount  + ' results out of '+ itemCount  + ' for keyword: \"' + inputString + '\".';
                           }
                           else
                           {
                                   searchstr += 'Showing the top 5 results out of '+ itemCount  + ' for keyword: <strong>' + inputString + '</strong>.';
                             
                           }
                           searchstr += '<div></li>';
                          
                        }
                       
                        $.each(result, function (i, item) {
                           if(i < 5)
        		   {
                              searchstr += '<li>';
                              searchstr += '<a href=\"javascript:void()\" class="gotopage" id=\'/product/' + item.url_name + '\'>';
                              searchstr += '<table border=\"0\" cellpadding=\"0\" cellspacing=\"3\"><tr><td>';
                              searchstr += '<img src=' + item.web_image + ' width=\"50\" height=\"50\" />';
                              searchstr += ' </td><td valign=\"top\">';
                              searchstr += '' + item.name + ' <br>';
                              searchstr += '<strong style=\"font-size:11px; color:#3d3d3d;\">Item # '+ item.sku + '</strong>';
                              searchstr += '</td></tr></table>';
                              searchstr += '</a>';
                              if(i != 4 && (i+ 1) != itemCount)
                              {  
	                              searchstr += '<hr>';
                              }
                             
                              searchstr += '</li>';
                            }
                          });
                        searchstr += '<li style=\"margin:0px; padding:0px;\"><div style=\"padding:15px 5px;background-color: #ff9b28;\">';
                        searchstr += '<a href=\"/category/s?keyword='+ inputString +'\" style=\"font-size:11px;\">More >></a>';
                        searchstr += '<div></li>';
                        searchstr += '</ul>';
                        $("#autoSuggestionsList").html(searchstr);
			
                    }
                     else
                     {
                         $("#LoadingSearch").hide(); 
                     }  
                }); 
    }
    // clear auto complete box
    function clearResults() {
        $("#autoSuggestionsList").html('');
        $("#suggestions").hide();
    }
    function trim(stringToTrim) {
        return stringToTrim.replace(/^\s+|\s+$/g, "");
    }  
</script>
          
