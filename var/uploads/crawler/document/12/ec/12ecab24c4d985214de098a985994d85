function UCX_Queue(accessKey, cookieId) {
var pg = "";

  pg = "home";
      UCX.LogCategoryVisit(accessKey, cookieId, 0, "Homepage", "", "");
  

if(UCX.GetCookie("UCX_CreateReview")!="") {
  UCX.LogContact(accessKey, cookieId, UCX.GetCookie("UCX_CreateReview"), "", "create_review", "", "");
  UCX.SetCookie("UCX_CreateReview","",null);
    }
if(UCX.GetCookie("UCX_Tracking")!="") {
  UCX.LogContact(accessKey, cookieId, UCX.GetCookie("UCX_Tracking"), UCX.GetCookie("UCX_TrackingZ"), "tracking", "", "");  
  UCX.SetCookie("UCX_Tracking","",null);
  UCX.SetCookie("UCX_TrackingZ","",null);
  }
if(UCX.GetCookie("UCX_MemberLogin")!="") {
  UCX.LogContact(accessKey, cookieId, UCX.GetCookie("UCX_MemberLogin"), "", "member_login", "", "");
  UCX.SetCookie("UCX_MemberLogin","",null);
  }

}
