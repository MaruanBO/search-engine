<script type="text/javascript">
		$(function($){
				//cache nav
				var nav = $("#multiDropMenu2");
				//add indicator and hovers to submenu parents
				nav.find("li.firstm").each(function() {
					if ($(this).find("ul").length > 0) {
						//$("<span>").text("^").appendTo($(this).children(":first"));

						//show subnav on hover
						$(this).mouseenter(function() {
							$(this).find("ul").stop(true, true).delay(500).slideDown('fast');
						});
						
						//hide submenus on exit
						$(this).mouseleave(function() {
							$(this).find("ul").stop(true, true).delay(250).slideUp('fast');
						});
					}
				});
	$('.menu_div ul li a').hover(function() {
		$(this, 'a').stop(true).animate({left:'5px'}, 'fast');
	},function(){
		$(this, 'a').stop(true).animate({left:'0px'}, 'fast');
	});
			});
</script> 

