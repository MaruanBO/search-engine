<script type="text/javascript">

$(document).ready(function(){
  //Windowshade Cart Events
  if (($('#wsh-cart').length > 0)&&($('#windowshade-cart').length > 0)) {
	$('#wsh-cart').hoverIntent({
		//interval: 500,
		over: function() { $( "#windowshade-cart" ).stop( true, true ).filter( ":hidden" ).show( "drop", { direction: "up" }, 500 ); },
		out: function() {}
	});
  }
  
  
  
  
  if ($('#windowshade-cart').length > 0) {
	$('#windowshade-cart').hoverIntent({
		//interval: 250,
		over: function() {},
		out: function() { $( this ).stop( true, true ).filter( ":visible" ).hide( "drop", { direction: "down" } ); }
	});
  
  $( document ).click( function() {
	  $( "#windowshade-cart" ).stop( true, true ).filter( ":visible" ).hide( "drop", { direction: "down" } ); 
  } );
  
	$('#windowshade-cart').click(function(e){
		e.stopPropagation();
	});
  }
});

</script>        </div>
