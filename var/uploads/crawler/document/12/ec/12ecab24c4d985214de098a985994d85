<script type="text/javascript" src="//malsup.github.com/jquery.blockUI.js"></script>
<script type="text/javascript">
function windowshade_update_cart(key,sku,qty, giftwrapremove) {
    var update_cart = windowshade_get_data("UpdateCart",key,sku,qty, giftwrapremove);
    var script_name = '/';
    if (update_cart != "") {
        if (qty==0)
            windowshade_busyscreen("Removing Item #:" + sku);
        else
            windowshade_busyscreen("Updating Quantity for Item #:" + sku);
        $.ajax({
            type: "POST",
            url: "index.php",
            data: update_cart,
            success: function(response) {
                $('#windowshade-cart-box').load('/windowshade_cart?pid='+sku+'&show_addtocart');
                $('#windowshade-cart').stop( true, true ).filter( ":hidden" ).show( "drop", { direction: "up" }, 500 );
                if ($('#left-nav-minicart').length > 0)
                    $('#left-nav-minicart').load('/minicart_new_home?show_output');
    //$('#top-cart').html('');              
                $('#top-cart').load('/top_cart_home');
                $.unblockUI();
                if(script_name=='/viewcart')
                    location.reload();
            }
        });
        //if(script_name=='/viewcart')
        //    window.location = '/viewcart';
    }
}
function windowshade_get_data(action,key,sku,qty, giftwrapremove) {
    var data = "action=" + action;
    var items = $('#windowshade_items').val().split("|");
    if (items.length > 1) {
        for (i = 1; i < items.length; i++) {
            details = items[i].split(";");
            if (details.length == 3) {
                if (details[1] == sku)
                    data += "&sku" + details[0] + "=" + details[1] + "&qty" + details[0] + "=" + qty;
                else
                    data += "&sku" + details[0] + "=" + details[1] + "&qty" + details[0] + "=" + details[2];
            }
        }
        if (giftwrapremove != "")
        	 data += "&" + giftwrapremove + "=0";
    }
    if (data == "action=" + action)
        data = "";
    
    return data;
}
function windowshade_busyscreen(msg)
{
    $.blockUI({ 
        message: "<p style='font-family:Georgia;font-size:18px;color:#ffffff;font-weight:bold;'>" + msg+ "</p>",
        css: {
            border: 'none', 
            padding: '15px', 
            backgroundColor: '#000000', 
            '-webkit-border-radius': '10px', 
            '-moz-border-radius': '10px', 
            opacity: .6, 
            color: '#eeeeee' 
        },
        overlayCSS: { backgroundColor: '#f9f8f8' }
    }); 
}
</script>


