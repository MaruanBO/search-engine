      <script type="text/javascript">
        function is_email_text_valid(email_text){
            var validated = false;
            var validRegExp = /^.+@.+\..+$/;
            var strEmail = email_text;
            if (strEmail.search(validRegExp) == -1){
            validated = false;
            }else{
            validated = true;
            }
            return validated;
        }

        function val_newsletter_form(){
          var valid = true;

          var month = $("#newsletter_due_month").val();
          var day = $("#newsletter_due_day").val();
          var year = $("#newsletter_due_year").val();

          if(month!="Month" || day!="dd" || year!="yyyy"){
            //Validate only if one of the fields is selected.

            if(month=="Month"){
              valid = false;
              alert("Please enter a valid month.")
              return;
            }

            
            if(day=="dd"){
              valid = false;
              alert("Please enter a valid day.")
              return;
            }

            
            if(year=="yyyy"){
              valid = false;
              alert("Please enter a valid year.")
              return;
            }            
          }


          var email = $("#newsletter_email").val();
          if(!is_email_text_valid(email)){
            valid = false;
            alert("Please enter a valid email.")
            return;                 

          }

          var d = new Date();
          var n = d.getTime();
          var form = $('#newsletter_new_form');  


/*          $.getJSON('/newsletters_new/add?'+form.serialize()+"&r="+n, function(jd) {
              alert("Thank you for subscribing to our newsletter.");
          });
*/

          $.post('/newsletters_new/add', form.serialize()+"&r="+n, function(data) {
            alert("Thank you for subscribing to our newsletter.");
          }, "json");

        }

        $(document).ready(function() {
          $("#newsletter_email").click(function(){
            if($("#newsletter_email").val()=="Enter Email Address"){
              $("#newsletter_email").val("") ;
            }
          });

          $("#newsletter_email").blur(function(){
            if($("#newsletter_email").val()==""){
              $("#newsletter_email").val("Enter Email Address") ;
            }
          });

        });             
      </script>



          <a href="javascript:val_newsletter_form();">Sign up</a>
        </div>
