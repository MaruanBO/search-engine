<script type="text/javascript">
    var scrollDiv = function(dir, px) {
        var belt_scroller = document.getElementById('belt_scroller');
        if (dir == 'l') {
            belt_scroller.scrollLeft -= px;
        }
        else if (dir == 'r') {
            belt_scroller.scrollLeft += px;
        }
    }
</script>

