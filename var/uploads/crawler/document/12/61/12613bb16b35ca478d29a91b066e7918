    <script>
        function KeyCheck(e) {
            var KeyID = (window.event) ? event.keyCode : e.keyCode;
            if (KeyID == 13) {
                Sigin();
            }
        }


        function Sigin() {

            var isformvalidate = true;
            var uname = $.trim($('#txtUserName').val());
            var pwd = $.trim($('#txtPassword').val());
            var iserror = false;
            if (uname == "" || uname == null) {
                iserror = true;
                $("#spnLoginErrorUserName").html("Required");
                $("#spnLoginErrorUserName").show();
                $('#dvLoginUserName').addClass('field hasError');
            }
            if (pwd == "" || pwd == null) {
                iserror = true;
                $("#spnLoginErrorPassword").html("Required");
                $("#spnLoginErrorPassword").show();
                $('#dvLoginPassword').addClass('field hasError');
            }

            if (iserror == false) {
                setTimeout(function () {
                    var currentPageURL = (window.location.pathname.toLowerCase().indexOf("/altrenovate_qa") > -1) || (window.location.pathname.toLowerCase().indexOf("/renovate_marketing") > -1) ? "/" : window.location.pathname;
                    var model = {
                        UserName: $.trim($('#txtUserName').val()),
                        Password: $.trim($('#txtPassword').val()),
                        
                        pageUrl: (window.location.pathname.toLowerCase().indexOf("users/create") > -1 || window.location.pathname.toLowerCase().indexOf("404") > -1) ? "/" : currentPageURL + window.location.search
                    }
                    $.ajax({
                        type: 'post',
                        dataType: 'json',
                        contenttype: 'application/json',
                        url: baseUrl + 'Users/Login',
                        data: model,
                        beforeSend: function () {
                          
                            //$('#loader-wrapper').show();
                            $('#iloader').show();

                            $('#btnLogin').attr('disabled', 'disabled');
                          
                        },
                        success: function (result) {
                          
                           
                            if (result.success) {
                                
                                if (redirecturl == "" || redirecturl == null) {
                                    // 3-deleted; 4- blocked
                                    if (result.IdStatus == 3) {
                                        $('#iloader').hide();
                                        $('#btnLogin').removeAttr('disabled');
                                        closePopDiv('SignInPopup', 'bounceOutDown');
                                        ShowErrorMessage("Your Profile has been deleted. Please contact Admin for further queries.");
                                        DelayedroomAlert();
                                    }
                                    else if (result.IdStatus == 4) {
                                        $('#iloader').hide();
                                        $('#btnLogin').removeAttr('disabled');
                                        closePopDiv('SignInPopup', 'bounceOutDown');
                                        ShowErrorMessage("Your Profile has been blocked. Please contact Admin for further queries.");
                                        DelayedroomAlert();
                                    }
                                    else {
                                        // user can login
                                        //code for goodBlogs login - goodBlogsId                                                                               

                                        //var temp = getCookie('LoginUserName');
                                        setCookie('GoodBlogCookie', "GoodBlog");
                                        setCookie('LoginUserName', result.username);
                                        var temp = getCookie('LoginUserName');

                                        
                                        window.location.href = baseUrl+result.goodBlogsResult;  // "http://community.renovate.com/api/login?user_id=211&next_page=http%3A%2F%2Flocalhost%3A4555%2F&auth=41a5036c47f78ccf9883d3848c8b0bf8";
                                    }
                                    //var l = window.location;
                                    //if (l.pathname.split('/')[1] != null && l.pathname.split('/')[1] != "") {
                                    //    setCookie('LoginUserName', result.username);
                                    //    //var temp = getCookie('LoginUserName');
                                    //    window.location.reload();
                                    //}
                                }
                                else {

                                    if (redirecturl == "Follow") {
                                        location = baseUrl + "ideabook/follow";
                                    }
                                    else if (redirecturl == "Ideabook") {
                                        location = baseUrl + "ideabook/My";
                                    }
                                    else if (redirecturl == "Contribute") {
                                        location = baseUrl + "ideabook/contribute";
                                    }
                                    else if (redirecturl == "MyActivity") {
                                        location = baseUrl + "Item/myactivity";
                                    }
                                    else if (redirecturl == "RoomsPreferences") {
                                        location = baseUrl + "users/set-category";
                                    }
                                    else if (redirecturl == "MyPictures") {
                                        location = baseUrl + "my-pictures";
                                    }


                                }
                            } else if (result.errors) {
                                $('#iloader').hide();
                                $('#btnLogin').removeAttr('disabled');

                                // alert(result.errors);
                                $("#spnLoginErrorUserName").html(result.errors);
                                $("#spnLoginErrorUserName").show();
                                $('#dvLoginUserName').addClass('field hasError');

                            }

                            
                        },
                        error: function (result) {
                            // alert(result.errors);
                            // $('#loader-wrapper').hide();
                            $('#iloader').hide();
                            $('#btnLogin').removeAttr('disabled');
                        }

                    });
                }, 200);
            }
            else {
                return false;
            }
        }
        function openForgotdiv() {

            closePopDiv('SignInPopup', 'bounceOutDown');
            openPopDiv('ForgotPassword', 'bounceInDown')
            $('.validation-summary-errors').remove();
            $('#Email').val('').trigger('blur');
        }


