    <script>
        function setNotificationid(sender) {
            var divNotificationId = $(sender).attr('rel');
            var actionNewURL = '/Renovate.Web_deploy/TestLead/setNotificationId';
            $.ajax({
                type: "POST",
                url: actionNewURL,
                data: '{"NotificationId": "' + divNotificationId + '"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data, xhr, settings) {
                    window.location = baseUrl + "activity/feeds";
                }
            });
        }
        var GlobalNotificationCount = 0;
        function getDashBoardNotifications() {
            $.ajax({
                type: "POST",
                url: baseUrl + 'Notification/GetUserWebsiteNotification',
                data: '{ "pageNum":"' + NotificationStartIndex + '", "pageSize": "' + NotificationEndIndex + '" }',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.dashboardDetailsData != undefined && data.dashboardDetailsData != null && data.dashboardDetailsData != "") {
                        var template = Handlebars.getTemplate("NotificationList");
                        var html = template(data.dashboardDetailsData);
                        if (NotificationStartIndex == 0)
                            $('#notificationlist').empty().append(data.dashboardDetailsData.notificationResult == "" ? "<div class=\"alert info-alert\">You don't have any notification yet</div>" : html);
                        else
                            $('#notificationlist').append(html);
                        NotificationStartIndex = NotificationStartIndex + 1;

                        GlobalNotificationCount = data.dashboardDetailsData.Notifcount;
                        if (data.dashboardDetailsData.Notifcount > (NotificationStartIndex * NotificationEndIndex))
                            $('#ancLoadMore').show();
                        else
                            $('#ancLoadMore').hide();
                    }
                    $('#notificationLoader').hide();
                }, error: function (data) {
                    $('#notificationLoader').hide();
                }
            });
        }

        var redirecturl = "";
        function openSignInPopup() {
            $('#txtUserName').val('').trigger('blur');
            $('#txtPassword').val('').trigger('blur');
            $('#dvLoginUserName').removeClass('hasError');
            $('#dvLoginPassword').removeClass('hasError');
            openPopDiv('SignInPopup');
        }
        function openSignInPopupIdeabook(sender) {
            var arel = $(sender).attr('rel');
            $('#UserName').val('');
            $('#Password').val('');
            $('#dvUserName').removeClass('hasError');
            $('#dvPassword').removeClass('hasError');
            openPopDiv('SignInPopup');
            redirecturl = arel;
        }
       

