    <script type="text/javascript">
        var GoodBlogRedirect = getCookie('GoodBlogCookie');       
        if (GoodBlogRedirect != null && GoodBlogRedirect == "GoodBlog") {
            $('<iframe id="myFrame" name="myFrame" width="0" height="0">').appendTo('body');
            $.ajax({
                type: 'get',
                dataType: 'json',
                contenttype: 'application/json',
                url: baseUrl + 'Users/LoginToGoodBlog',
                data: { username: getCookie('LoginUserName') },
                success: function (result) {                    
                    $('#myFrame').attr('src', result.url);
                    eraseCookie("GoodBlogCookie");
                    //return xmlHttp.responseText;
                }
            });
        }
        var fineuploaderCount = 0;
        function FBCallBack(user_data) {
            // user_data will be in JSON format
            $('#gid').val(user_data.id);
            $('#gfirst_name').val(user_data.first_name);
            $('#glast_name').val(user_data.last_name);
            $('#gemail').val(user_data.email);
            $('#gAccessToken').val(acessTokenFb);
            var img_size = 100;
            user_data.image = user_data.picture.normal.replace(/(sz=)[^\&]+/, '$1' + img_size);
            $('#google_img').val(user_data.image);
            var currentPageUrl = "";
            currentPageUrl = document.location.toString().toLowerCase();
            if (currentPageUrl.match("settings") || currentPageUrl.match("accountsettings")) {
                $.ajax({
                    type: 'post',
                    url: baseUrl + 'Users/connectchk',                    
                    data: '{"Userid":"' + $('#useridhidn').val().toString() + '","Socialid":"' + user_data.id.toString() + '","type":"2"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response == "true") {
                            $.ajax({
                                type: 'post',
                                dataType: 'html',
                                url: baseUrl + 'Users/FBConnectA',
                                data: $('#MyForm').serialize(),
                                success: function () {
                                    window.location = baseUrl + "Users/AccountSettings";
                                }
                            });
                        }
                        else {

                            ShowErrorMessage("Social Network identity is already associated with another user");
                            roomAlert();
                            return false;
                        }
                    },
                    error: function () {
                        ShowErrorMessage("Social Network identity is already associated with another user");
                        roomAlert();
                    }
                });
            }
            else {
                $.ajax({
                    type: 'post',
                    dataType: 'html',
                    url: baseUrl + 'Users/FBConnect',
                    data: $('#MyForm').serialize(),
                    success: function () {
                        var redirectURL = '';
                        if ($('#hdnPageURL').val() != 'undefined')
                            redirectURL = "?redirectUrl=" + $('#hdnPageURL').val()

                        window.location = baseUrl + "Users/Create" + redirectURL;
                    }
                });
            }

            // Area for Facebook Ends Here
        }
        function Callback(user_data) {
          
            $('#gid').val(user_data.id);            
            $('#gfirst_name').val(user_data.first_name);
            $('#glast_name').val(user_data.last_name);
            $('#gemail').val(user_data.email);
            var img_size = 100;
            user_data.image = user_data.image.replace(/(sz=)[^\&]+/, '$1' + img_size);            
            $('#google_img').val(user_data.image);
            var currentPageUrl = "";
            currentPageUrl = document.location.toString().toLowerCase();
            if (currentPageUrl.toLowerCase().match("settings") || currentPageUrl.toLowerCase().match("accountsettings")) {

                $.ajax({
                    type: 'post',
                    url: baseUrl + 'Users/connectchk',                   
                    data: '{"Userid":"' + $('#useridhidn').val().toString() + '","Socialid":"' + user_data.id.toString() + '","type":"1"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {

                        if (response == "true") {
                            $.ajax({
                                type: 'post',
                                dataType: 'html',
                                url: baseUrl + 'Users/GoogleConnectA',
                                data: $('#MyForm').serialize(),
                                success: function () {
                                    window.location = baseUrl + "Users/AccountSettings";
                                }

                            });
                        }
                        else {
                            ShowErrorMessage("Social Network identity is already associated with another user");
                            roomAlert();
                        }
                    },
                    error: function () {
                        ShowErrorMessage("Social Network identity is already associated with another user");
                        roomAlert();
                    }
                });
            }
            else {
                $.ajax({
                    type: 'post',
                    dataType: 'html',
                    url: baseUrl + 'Users/GoogleConnect',
                    data: $('#MyForm').serialize(),
                    success: function () {
                        var redirectURL = '';
                        if ($('#hdnPageURL').val() != 'undefined')
                            redirectURL = "?redirectUrl=" + $('#hdnPageURL').val()
                        
                        window.location = baseUrl + "Users/Create" + redirectURL;                        
                    }
                });
            }
        }


