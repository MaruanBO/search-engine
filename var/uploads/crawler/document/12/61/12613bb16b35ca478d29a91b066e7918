<script>
    function ForgotPassword() {
        var email = $('#txtEmail').val();
        if (email == "" || email == null) {          
            $("#spnErrorEmail").html("Required");
            $("#spnErrorEmail").show();
            $('#dvEmails').addClass('field hasError');
        }
        else {
            $.ajax({
                type: 'post',
                dataType: 'json',
                url: baseUrl + 'Users/ForgotPassword',
                data: $('#MyForgotPasswordForm').serialize(),
                success: function (result) {
                    if (result.success) {

                        closePopDiv('ForgotPassword', 'bounceOutDown');
                        openPopDiv('ForgotPasswordMessage', 'bounceInDown')
                    } else if (result.errors) {

                        $("#spnErrorEmail").html(result.errors);
                        $("#spnErrorEmail").show();
                        $('#dvEmails').addClass('field hasError');
                       // displayErrors($('#MyForgotPasswordForm'), result.errors);
                    }
                }
            });
        }

    }

