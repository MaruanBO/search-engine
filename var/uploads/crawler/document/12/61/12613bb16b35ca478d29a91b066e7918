        function setProfileUrl() {
            baseUrl = 'http://www.renovate.com/';
            var temp = $('#LoginName').val();
            var tempLoginUserUserType = $('#LoginUserUserType').val();
            if (tempLoginUserUserType == 1) {

                location = baseUrl + "user/" + temp;
            }
            else {

                location = baseUrl + "Pro/" + temp;
            }
        }
