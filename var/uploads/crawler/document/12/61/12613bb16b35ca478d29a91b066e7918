        $(function () {
            $('.navbar > a:eq(0)').addClass('active');
            $('.srch-ctrl .search-btn').on('click', function () {
                if (!$(this).next('.field').hasClass('active')) {
                    $(this).next('.field').addClass('active');
                    $(this).addClass('active')
                } else {
                    $(this).next('.field').removeClass('active');
                    $(this).removeClass('active')
                }
            });
            $(window).on('resize', function () {
                $('.srch-ctrl .search-btn').removeClass('active')
                $('.srch-ctrl .field').removeClass('active')
            });
        });
        function KeyCheckOfSearch1(e) {
            var KeyID = (window.event) ? event.keyCode : e.keyCode;
            if (KeyID == 13) {
                var SearchText = $.trim($('.globalSrch').val());
                if (SearchText != '')
                    window.location = baseUrl + "items/SearchResult?SearchText=" + encodeURIComponent(SearchText);
            }
        }
        function SendForGlobalSearch() {
            if ($.trim($('.globalSrch').val()) != '')
                window.location = baseUrl + "items/SearchResult?SearchText=" + encodeURIComponent($('.globalSrch').val());
        }

        function NotifFlySec() {
         
            $('#notify').on('click', function (e) {
                $('[data-toggle="toggleDrop"]').removeClass('active');
                if (!$(this).parent('.loggedin-menu').find('.submenu-region').is(':visible')) {
                    $(this).parent('.loggedin-menu').addClass('active');
                } else {
                    $(this).parent('.loggedin-menu').removeClass('active');
                }
                e.stopPropagation();
            });
            $(document.body).on('click', function () {
                $('.loggedin-menu').removeClass('active');
            });
            

            $('#notify').on('click', function (e) {
                $.ajax({
                    url: baseUrl + "Notification/UpdateWebsiteNotificationStatus",
                    dataType: "json",
                    success: function (data) {
                        $('#NotificationCount').hide();
                        NotificationStartIndex = 0;
                        NotificationEndIndex = 5;                        
                        getDashBoardNotifications();
                    }, error: function (data) {
                    }
                });

                return false

            })

        }
        function getCountOfNotifications() {
            $.ajax({
                url: baseUrl + "Notification/SelectWebsiteNotificationActiveCount",
                dataType: "json",
                success: function (data) {
                    if (data.result != 0 && data.result != '0' && data.result != '') {
                        $('#NotificationCount').html(data.result);
                        $('#NotificationCount').show();
                    }
                }, error: function (data) {
                }
            });
        }
        function RedirectAfterSessionClear(sender) {
            $.ajax({
                url: baseUrl + "items/clearSessionValue",
                dataType: "json",
                success: function (data) {
                    if (sender == "space") {
                        location = baseUrl + "Spaces";
                    }
                    else if (sender == "collection") {
                        location = baseUrl + "collections";

                    }
                },
                error: function (data) { }
            });
        }

        if (/Android|webOS|iPhone|iPod|Windows Phone|BlackBerry/i.test(navigator.userAgent)) {
            $(function () {
                $('#mobileNavToggle').on('click', function () {
                    if ($(this).parents('.mobile-global-header').hasClass('active')) {
                        $(this).parents('.mobile-global-header').removeClass('active');
                        $('#mobileNavContent').hide();
                    } else {
                        $(this).parents('.mobile-global-header').addClass('active')
                        $('#mobileNavContent').show();
                    }
                });


                $(document).click(function (e) {
                    var target = e.target;
                    if (!$(target).is('#mobileNavToggle') && !$(target).parents().is('#mobileNavContent')) {
                        $('#mobileNavContent').hide();
                        $('.mobile-global-header').removeClass('active');
                    }

                });


            });
        }

        
