        var DataExists = false;
        DataExists = 'True'; 
        function getRooms() {
            var dvHtml = $('#dvroomFilter').html();
            var currentSelectedPageUrl = "";
            var currentPageUrl = "";
            currentPageUrl = document.location.toString().toLowerCase();
            if (currentPageUrl.match("item/myactivity")) {
                currentSelectedPageUrl = "myactivity";
            }
            else if (currentPageUrl.match("my-pictures/pending")) {
                currentSelectedPageUrl = "pendingpictures";
            }
            else if (currentPageUrl.match("my-pictures/upload")) {
                currentSelectedPageUrl = "";
            }
            else if (currentPageUrl.match("my-pictures")) {
                currentSelectedPageUrl = "mypictures";
            }
            else if (currentPageUrl.match("users/set-category")) {
                currentSelectedPageUrl = "managerooms";
            }

            if ($.trim(dvHtml) == "" || dvHtml == null) {
                
                if (!(DataExists == "False")) {                  
                    $('#dvroomFilter').html($('#hdnRoomList').val());                    
                    switch (currentSelectedPageUrl) {
                        case 'managerooms':
                            $('.icon-preferences').parent().attr('class', 'button-blue pull-right');
                            break;
                        case "mypictures":
                            $('.icon-picture').parent().attr('class', 'button-blue pull-right mright10');
                            break;
                        case "pendingpictures":
                            $('.icon-pending').parent().attr('class', 'button-blue pull-right mright10');
                            break;
                        case "myactivity":
                            $('.icon-activity').parent().attr('class', 'button-blue pull-left');
                            break;
                    }

                }
                else {
                var actionNewURL = '/Renovate.Web_deploy/Items/GetRoomList';
                $.ajax({
                    type: "POST",
                    url: actionNewURL,
                    data: '{"currentSelectedPageUrl": "' + currentSelectedPageUrl + '"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data, xhr, settings) {
                            $('#dvroomFilter').html(data.MoreResults);
                            //if ($.trim(data.MoreResults) == "") {
                            //    getRooms();
                            //}
                            //else {
                            //    $('#dvroomFilter').html(data.MoreResults);
                            //}
                    }
                });
            }
        }
        }
        var roomid = "0";
        var isnewRequestForRoom = false;
        function getItemsByoomid(sender) {
            var arel = $(sender).attr('rel');
            var partsOfStr = arel.split(',');
            var currentPageUrl = document.location.toString().toLowerCase();

            if (currentPageUrl.match("items/create")) {
                roomid = partsOfStr[0];
                location = baseUrl + "spaces/" + partsOfStr[1].replace('&', "and");
            }
            if (currentPageUrl.match("items/globalsearchresult")) {
                roomid = partsOfStr[0];
                location = baseUrl + "spaces/" + partsOfStr[1].replace('&', "and");
            }
            else {
                if ((currentPageUrl.match("items") || currentPageUrl.match("item")) & !(currentPageUrl.match("items/myactivity"))) {
                    roomid = partsOfStr[0];
                    $('#hdnRoomId').val(roomid);
                    $('#QuickSearch').val('');
                    $('#hSearch').html(partsOfStr[1])
                    $('#hdnSearchValue').val(partsOfStr[1]);
                    var SearchText = $('#QuickSearch').val();
                    $('.sbSelector').html('Sort by');
                    isnewRequest = true;
                    isnewRequestForRoom = true;
                    $('#hdnCurrentPageIndex').val("0");
                    GetMoreRecords();
                }
                else {                    
                    roomid = partsOfStr[0];
                    location = baseUrl + "spaces/" + partsOfStr[1].replace('&', "and");
                }
            }
        }

