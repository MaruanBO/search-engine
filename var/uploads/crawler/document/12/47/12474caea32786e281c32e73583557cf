            $().ready(function(){
                $(".form").each(function(){
                    $(this).attr('rel',$(this).val());
                    $(this).on('focus',function(){
                        if ( $(this).val() == $(this).attr('rel') ){
                            $(this).val('');
                        }
                    }).on('blur',function(){
                        if ( $(this).val() == '' ){
                            $(this).val($(this).attr('rel'));
                        }
                    })
                })

                $("#search-form").submit(function(){


                    var searchUpdated = false;
                    $(".form").each(function(){
                        if ($(this).val() == $(this).attr('rel') ){
                            $(this).val('');
                            searchUpdated = true;
                        }
                    })

                    if ( searchUpdated ){
                        $("#search-form").submit();
                        return false;
                    }

                    return true;
                })

                            })
