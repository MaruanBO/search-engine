    <div class="collectionsWidgetWrapper">
            <div class="teaser imageOnTop-true imageWidth-120" itemscope itemtype="http://schema.org/CreativeWork">
        <meta itemprop="url" content="http://www.kxly.com/kxly/contact"/>
<figure  class="ib-figure-image" itemscope itemtype="http://www.schema.org/ImageObject" itemprop="associatedMedia">
        <meta itemprop="url" content="http://www.kxly.com/22121480"/>
<meta itemprop="dateCreated" content="2013-09-25T03:19:40"/>
    <meta itemprop="dateModified" content="2013-09-25T03:18:26"/>
<a href="/kxly/contact"><img 
                src="/image/view/-/22121480/medRes/1/-/h/67/maxh/67/maxw/120/w/120/-/jcajapz/-/Valley-View-Fire.jpg"
                alt=""
                title=""
                
                />
