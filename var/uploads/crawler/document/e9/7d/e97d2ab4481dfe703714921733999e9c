        function callMediaItems(activityID) {

            var ebRand = Math.random() + '';
            ebRand = ebRand * 1000000;
            var ebIframe = document.createElement('iframe');
            ebIframe.width = 0;
            ebIframe.height = 0;
            ebIframe.marginwidth = 0;
            ebIframe.marginheight = 0;
            ebIframe.hspace = 0;
            ebIframe.vspace = 0;
            ebIframe.frameborder = 0;
            ebIframe.scrolling = 'no';
            ebIframe.bordercolor = '#000000';
            ebIframe.src = "http://bs.serving-sys.com/BurstingPipe/ActivityServer.bs?cn=as&ifrm=1&ActivityID=" +activityID + "&rnd=" + ebRand;
            document.getElementsByTagName('body')[0].appendChild(ebIframe);
