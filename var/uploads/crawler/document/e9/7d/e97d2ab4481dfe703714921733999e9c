<script type="text/javascript">

    $(document).ready(function () {
        //Read user name from cookie and set in header span
        setHeaderUserName();
        function fbEnsureInit() {
            if (!window.fbApiInit) {
                setTimeout(function () { fbEnsureInit(); }, 50);
            } else {
                FB.Event.subscribe('edge.create',
                        function (response) {
                            if (response.toLowerCase().indexOf('facebook.com/campbellskitchen') > 0) {
                                callOmnitureMainPageLiked();
                            }
                            else {
                                callOmnitureSocial('facebook');
                                
                                if(typeof mmActivityID != "undefined")
                                { 
                                    callMediaItems(mmActivityID);
                                }
                            }
                        });
            }
        }

        fbEnsureInit();
    });

    function setHeaderUserName() {
        // read cookie

        var getCookie = function (name) {
            var val = null;
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) {
                    val = c.substring(nameEQ.length, c.length);
                    break;
                }
            }
            return val;
        };

        var aspxAuth = getCookie('.ASPXAUTH');
        var userName = getCookie('username');
        if (aspxAuth != null && aspxAuth.length > 0) {
            $(".headerLoggedIn").show();
            $(".headerRegisterAnon").hide();
            if (userName != null && userName.length > 0) {
                $("#spanUserName").html(userName);
            }
        } else {
            $(".headerLoggedIn").hide();
            $(".headerRegisterAnon").show();
            $('.hlMyKitchenAnounymous').attr('href', "javascript:Popin.getInstance().show('#logInForm');setActionAttemptCookie('MemberActionAttempt', 'my_kitchen', 1);");
            $('.hlMyKitchenAnounymous').addClass("myKitchen");
        }
    }

    function callOmnitureSocial(socialNetwork,isRecipePage) {
    
      
      omnitureClearVars();
       var s_click = s_gi(s_account)
        s_click.prop20 = socialNetwork;
        s_click.prop24 = document.title;

        s_click.eVar33 = document.title;
        
        if (document.location.href.toLowerCase().indexOf('recipedetails') > 0) {
            s_click.prop20 = socialNetwork;
            s_click.events = 'event76';
            s_click.prop24 = s.prop10;
            s_click.eVar33 = s.prop10;            
            s.linkTrackEvents = "event76";
        }
        else if (document.location.href.toLowerCase().indexOf('plateofmind') > 0 && socialNetwork !='email') {
            s_click.events = 'event17';   
            s.linkTrackEvents = "event17";         
        }
        else if (document.location.href.toLowerCase().indexOf('plateofmind') > 0 && socialNetwork =='email') {
            s_click.events = 'event77';   
            s.linkTrackEvents = "event77";
            }
        else 
        {
            s_click.events = 'event77';
            s.linkTrackEvents = "event77";
        }

        if (document.location.href.toLowerCase().indexOf('savingscenterlandingpage') > 0) {
            s_click.prop24 = 'COUPONS';
            s_click.eVar33 = 'COUPONS';
        }

        s_click.eVar15 = socialNetwork;
        
       // s.events = document.location.href.toLowerCase().indexOf('recipedetails') > 0 ? 'event76' : 'event17';
        //writeOmniture(s);
        s.linkTrackVars = "events,prop20, prop24,eVar33,eVar15";
          s_click.tl(this, 'o', 'click ' + socialNetwork);
        
    }

    function callOmnitureMainPageLiked() {
      
        s.prop20 = 'facebook';
        s.prop24 = 'Campbell\'s Kitchen Fan';
        s.eVar15 = 'facebook';
        s.eVar33 = 'Campbell\'s Kitchen Fan';

        //set events_original to undefined
        //otherwise it will be replaced with the previous exit link event (e.g. 'pin' it link)
        s.events_original = undefined;
        s.events = 'event18';
        writeOmniture(s);
         
    }
    
</script>

