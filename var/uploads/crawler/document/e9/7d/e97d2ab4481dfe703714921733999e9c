            document.getElementById("mediaItemsDiv").appendChild(fileref);

        }
        function clearMediaItems() {
            var node = document.getElementById('mediaItemsDiv');
            while (node.hasChildNodes()) {
                node.removeChild(node.firstChild);
            }
        } 
