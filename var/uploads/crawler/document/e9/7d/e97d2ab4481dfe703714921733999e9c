	  <script type="text/javascript">
	      $(function () {

	          var name = 'st';
	          var _value = "";
	          var regexS = "[\\?&]" + name + "=([^&#]*)";
	          var regex = new RegExp(regexS);
	          var results = regex.exec(window.location.search);
	          if (results == null) { return ""; }
	          else { _value = decodeURIComponent(results[1].replace(/\+/g, " ")); }
	          if (_value != "") {
	              $(window).scrollTop($('a[name=' + _value + ']').offset().top);

	          }
	      });
	   </script>
	
