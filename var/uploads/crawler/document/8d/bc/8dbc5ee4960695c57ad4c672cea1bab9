    <script>

                function showBadgeError() {

                    var showBadgeErrorTimer = setTimeout( function() {
                            $("div.login_error").show();
                            $("#login_link, #badge_link").trigger("click");
                        },
                        500
                    );

                };

                var login_base = (urls.login_override == "") ? document.location.toString() : urls.login_override;
                var error_url = (document.location.search) ? login_base + "&loginError=true" : login_base + "?loginError=true";
                var crossdomain = (urls.badge_url.substr(0,4) == "http") ? true : false;

                if (urls.badge_url != "no-badge" && urls.badge_url != "") {

                    $(document).ready(function(){
                        $.ajax({
                            url: urls.badge_url,
                            data: { 
                                "redirect_url": login_base,  
                                "error_url": error_url
                            },
                            cache: false,
                            xhrFields: { withCredentials: true },
                            crossDomain: crossdomain,
                            success: function(html){

                                $("#badge-holder").replaceWith(html);

                            

                            }
                        });
                    });

                };

