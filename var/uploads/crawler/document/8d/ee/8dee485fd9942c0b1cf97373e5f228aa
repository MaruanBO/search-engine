                    <script type="text/javascript" src="/js/yahoo-min.js"></script>
<script type="text/javascript" src="/js/connection-min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
        var YahooRSSConnection = {
            handleSuccess:function(o) {
                this.processResult(o);
                $('#left-column, #content-area').equalHeightColumns();
            },
            handleFailure:function(o) {
                msg = document.getElementById('rss-loading');
                msg.firstChild.data = "Error getting news feed";
            },
            processResult:function(o) {
                response = o.responseXML.documentElement;
                var items = response.getElementsByTagName("item");
                display = new YahooRSSDisplay(items,3);
                display.render();        
            },
            /* MODIFY THE URL to the proxy ASPX file to the correct location on the server */
            startRequest:function() {
                loading = document.getElementById("rss-loading");
                loading.appendChild(document.createTextNode("Loading..."));
                YAHOO.util.Connect.asyncRequest('GET','/js/RSSProxy.aspx',rssCallback,null);
            }
        };
        var rssCallback = 
        {
            success:YahooRSSConnection.handleSuccess,
            failure:YahooRSSConnection.handleFailure,
            scope:YahooRSSConnection
        };
        function YahooRSSDisplay(items,maximum) {
            this.items = items;
            this.maximum = (this.items.length < maximum) ? this.items.length : maximum;
            this.container = document.getElementById("news-ticker");
            this.loading = document.getElementById("rss-loading");
            this.createAnchorTag = function(url,text) {
                var anch = document.createElement("a");
                anch.href=url;
                anch.target = "_blank";
                anch.innerHTML = text;
                return anch;
            }
            this.createTitleTag = function(url,title,className) {
                var p = document.createElement("p");
                p.appendChild(this.createAnchorTag(url,title));
                p.className = className;
                return p;
            }
            this.createDateTag = function(url,date,className) {
                var anch = document.createElement("a");
                anch.href=url;
                anch.target = "_blank";
                anch.innerHTML = date;
                anch.className = className;
                return anch;
            }
            this.createNewsItem = function(dateTag,titleTag,counter) {
                var li = document.createElement("li");
                li.appendChild(dateTag);
                li.appendChild(titleTag);
                li.className = "side-post";
                li.setAttribute("id",counter);
                return li;
            }
            this.render = function() {
                var month=new Array(12);
                month[0]="January";
                month[1]="February";
                month[2]="March";
                month[3]="April";
                month[4]="May";
                month[5]="June";
                month[6]="July";
                month[7]="August";
                month[8]="September";
                month[9]="October";
                month[10]="November";
                month[11]="December";
                for (var i=0; i<this.maximum; i++) {
                    url = this.items.item(i).getElementsByTagName("link").item(0).firstChild.data;
                    title = this.items.item(i).getElementsByTagName("description").item(0).firstChild.data;
                    if (title.length > 200) title = title.substring(0,200) + "...";
                    date = this.items.item(i).getElementsByTagName("pubDate").item(0).firstChild.data;
                    var ds = Date.parse(date);
                    var d = new Date(ds);
                    var mo = month[d.getMonth()];
                    var dy = d.getDate();
                    var yr = d.getFullYear();
                    var forDate = mo + ' ' + dy;
                    this.container.appendChild(this.createNewsItem(this.createDateTag(url,forDate,"date"),this.createAnchorTag(url,title),i+1));
                }
                this.loading.style.display = 'none';
                //this.container.style.display = 'block';
            }
        }
        YahooRSSConnection.startRequest();
    });
</script>
