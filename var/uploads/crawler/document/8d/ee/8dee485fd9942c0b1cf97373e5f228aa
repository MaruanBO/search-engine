<script type="text/javascript">
    $(document).ready(function() {
        //Favorites
        $("#addFavorite").click(function () {
            $('body').addClass('is-favorite');
            //Create a favorite using a call to the api.
            $.ajax({
                url: 'https://otms.fiserv.com/communitiesapi/api/favorite/newbookmark/',
                type: "GET",
                data: 'Parameters=' + encodeURIComponent('H4sIAAAAAAAEAB3IvQ5AMBAA4FcxdTtaNM5wMzYDD9DqkSb+oiR9fGH8vs6RFD1f4dg7R6BEbxYe/L0ytcfGycf/xmulzO+OY2rCGf9rHu+oqqtJIzJIoxFKmc9gsc7BSmV1gTgpxS9Xxqo3ZgAAAA=='),
                dataType: 'jsonp',
                mimeType: "text/html"
            });
            //Hide add link and display remove link.
            $("#addFavorite").hide();
            $("#removeFavorite").show();
            return false;
        });
        $("#removeFavorite").click(function () {
            $('body').removeClass('is-favorite');
            if(confirm('Are you sure you want to permanently remove this favorite?')){
                //Remove the favorite using a call to the api.
                $.ajax({
                    url: 'https://otms.fiserv.com/communitiesapi/api/favorite/removebookmark/',
                    type: "GET",
                    data: 'Parameters=' + encodeURIComponent('H4sIAAAAAAAEAB3IvQ5AMBAA4FcxdTtaNM5wMzYDD9DqkSb+oiR9fGH8vs6RFD1f4dg7R6BEbxYe/L0ytcfGycf/xmulzO+OY2rCGf9rHu+oqqtJIzJIoxFKmc9gsc7BSmV1gTgpxS9Xxqo3ZgAAAA=='),
                    dataType: 'jsonp',
                    mimeType: "text/html"
                });
                //Hide remove link and display add link.
                $("#removeFavorite").hide();
                $("#addFavorite").show();
            }
            return false;
        });
        if("" == "no")
        {
            $("#removeFavorite").hide();
            $("#addFavorite").hide();            
        }else{
            $('<div class="corner-icon" title="This page is favorited."></div>').prependTo($('#wrapper'));
        }
        //Pre-select location and set remember checkbox.
        $('#locationSelect').val('US');
        $(function(){
            var $selects = $('#locationSelect'); 
            $selects.easyDropDown({
                cutOff: 10,
                wrapperClass: 'easyDropdown',
                onChange: function(selected){
                    // Send to correct location:
                    if (selected.value == "INTL")
                        
                                url = "/international/index.aspx";
                            
                    else
                        url = "/index.aspx"; 
                    $(location).attr('href', url);
                }
            });          
        });
        $('#theLang, #theLangLbl').click(function( event ) {
            event.stopPropagation();
            // Stop the checkbox and its label from causing the menu to close when clicked.
        });
        //Function for redirect notice close
        $("#close-alert-desktop, #close-alert-mobile").click(function(){
            $("#version-alert-desktop, #version-alert-mobile").hide();
            var d = new Date();
            d.setTime(d.getTime() + (30*24*60*60*1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = "LocationCookie=True; " + expires;
        });
        //Modal login code
        $(".signin-btn").click(function() {
            $("#signin-modal").show();
            $("body").addClass("signin-open");
        });
        
         $("#signin-modal .close-btn").click(function() {
            $("#signin-modal").hide();
            $("body").removeClass("signin-open");
        });
    });
    //Modal login code
    $(document).keypress(function (e) {
        //If the enter key is pressed when the signin-modal is visible then submit the login form.
        if (e.which == 13 && $('#signin-modal').is(':visible')) {
            $("#aspnetform").submit();
        }
    });
</script>
