            <script>

            $(function(){

                //https://github.com/odhyan/trie/blob/master/trie.js
                Trie=function(){this.words=0;this.prefixes=0;this.children=[]};Trie.prototype={insert:function(e,t){if(e.length==0){return}var n=this,r,i;if(t===undefined){t=0}if(t===e.length){n.words++;return}n.prefixes++;r=e[t];if(n.children[r]===undefined){n.children[r]=new Trie}i=n.children[r];i.insert(e,t+1)},remove:function(e,t){if(e.length==0){return}var n=this,r,i;if(t===undefined){t=0}if(n===undefined){return}if(t===e.length){n.words--;return}n.prefixes--;r=e[t];i=n.children[r];i.remove(e,t+1)},update:function(e,t){if(e.length==0||t.length==0){return}this.remove(e);this.insert(t)},countWord:function(e,t){if(e.length==0){return 0}var n=this,r,i,s=0;if(t===undefined){t=0}if(t===e.length){return n.words}r=e[t];i=n.children[r];if(i!==undefined){s=i.countWord(e,t+1)}return s},countPrefix:function(e,t){if(e.length==0){return 0}var n=this,r,i,s=0;if(t===undefined){t=0}if(t===e.length){return n.prefixes}var r=e[t];i=n.children[r];if(i!==undefined){s=i.countPrefix(e,t+1)}return s},find:function(e){if(e.length==0){return false}if(this.countWord(e)>0){return true}else{return false}},getAllWords:function(e){var t=this,n,r,i=[];if(e===undefined){e=""}if(t===undefined){return[]}if(t.words>0){i.push(e)}for(n in t.children){r=t.children[n];i=i.concat(r.getAllWords(e+n))}return i},autoComplete:function(e,t){if(e.length==0){return[]}var n=this,r,i;if(t===undefined){t=0}r=e[t];i=n.children[r];if(i===undefined){return[]}if(t===e.length-1){return i.getAllWords(e)}return i.autoComplete(e,t+1)}}

                //http://stackoverflow.com/questions/460070/detecting-mistyped-email-addresses-in-javascript#answer-460102
                levenshteinenator=function(e,t){var n;var r=e.length;var i=t.length;if(r<i){var s=e;e=t;t=s;var o=r;r=i;i=o}var u=new Array;u[0]=new Array;for(var s=0;s<i+1;s++){u[0][s]=s}for(var a=1;a<r+1;a++){u[a]=new Array;u[a][0]=a;for(var f=1;f<i+1;f++){n=e.charAt(a-1)==t.charAt(f-1)?0:1;u[a][f]=minimator(u[a-1][f]+1,u[a][f-1]+1,u[a-1][f-1]+n)}}return u[r][i]};minimator=function(e,t,n){if(e<t&&e<n)return e;if(t<e&&t<n)return t;return n}

                var T = new Trie();

                $.each(LOLKING.champions, function(i, o) {
                    T.insert(o.internal_name.toLowerCase());
                });

                var regions = LOLKING.regions;
                var cookie_name = 'default-region';
                var initial_region = Cookies.get(cookie_name) || 'none';

                var $region = $('.tselect-container ul li[data-tselect-value="' + initial_region.toUpperCase() + '"]');

                var hasPickedRegion = $region.length != 0;

                if( !hasPickedRegion ) {

                    initial_region = 'na';
                    $('#select-your-region-callout').fadeIn();
                    $region = $('.tselect-container ul li[data-tselect-value="' + initial_region.toUpperCase() + '"]');

                }

                var selectedText = $region.attr('data-tselect-selected-label') || $(this).text();
                var selectedValue = $region.attr('data-tselect-value') || $(this).text();
                $region.parent().children().first().text(selectedText);
                $region.parent().parent().find('[type="hidden"]').val(selectedValue);

                $('.tselect-container ul li:gt(0)').click(function(){

                    Cookies.set(cookie_name, $(this).attr('data-tselect-value').toLowerCase(), {expires: 31536000});
                    $('.suggestive-search-container [name="name"]').trigger('keyup').focus();

                });

                $('.suggestive-search-container [name="name"]').bind('focus', function(e){
                    if( $('#search-suggestions-container').children().length > 0 ) {
                        $('#search-suggestions-container').fadeIn();
                        $('.suggestive-search-container').addClass('suggestive-search-container-open');
                        if( !hasPickedRegion ) $('#select-your-region-callout').hide();
                    }
                });

                $('.suggestive-search-container [name="name"]').bind('keyup', function(e){

                    if( e.keyCode && e.keyCode == 27 ) {
                        $(this).blur();
                        return;
                    }

                    var str = $("<div/>").text($(this).val()).html();
                    var slug = str.toLowerCase().replace(/[^a-z]/g, "");
                    var champion_slug = null;

                    var rows = [];

                    if( slug.length ) {

                        var trie_matches = T.autoComplete(slug);

                        if( trie_matches.length ) {

                            champion_slug = trie_matches.sort(function (a, b) { return a.length - b.length; })[0];

                        } else {

                            var closest_champion;
                            var closest_champion_score = 10;

                            $.each(LOLKING.champions, function(i, o) {
                                var score = levenshteinenator(slug, o.internal_name);
                                if( score < closest_champion_score ) {
                                    closest_champion_score = score;
                                    closest_champion = o.internal_name;
                                }

                            });

                            if( closest_champion_score <= 3 ) {
                                champion_slug = closest_champion;
                            }

                        }

                        if( str.length >= 2) {

                            var html = [
                            '<li>',
                                '<a href="/search?name=' + str + '">',
                                    '<div style="text-align: left; text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.6);">Search All Regions</div>',
                                    '<p>See profiles from all supported regions.</p>',
                                '</a>',
                            '</li>'
                            ].join('');

                            rows.push(html);

                        }


                        if( champion_slug && LOLKING.champions[champion_slug] ) {

                            var html = [
                            '<li>',
                                '<div>',
                                    '<div style="width: 48px; height: 48px; background: url(//lkimg.zamimg.com/shared/riot/images/champions/' + LOLKING.champions[champion_slug].champion_id + '_64.png) center; border-radius: 6px; box-shadow: 0 0 2px #000; float: left; margin-right: 10px;"></div>',
                                    '<div style="float: left;">',
                                        '<div style="text-align: left; text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.6);">' + LOLKING.champions[champion_slug].name + '</div>',
                                        '<p>Check out <a href="/guides/list.php?champion=' + LOLKING.champions[champion_slug].file + '">guides</a>, <a href="/models/?champion=' + LOLKING.champions[champion_slug].champion_id + '&skin=0">3D models</a>, and <a href="/champions/' + LOLKING.champions[champion_slug].file + '#statistics">statistics</a>.</p>',
                                    '</div>',
                                '</div>',
                            '</li>'
                            ].join('');

                            rows.push(html);

                        }
                    }

                    $('.search-suggestions-container').empty().html(rows.join(''));

                    if( $('.search-suggestions-container').children().length > 0 ) {
                        $('.search-suggestions-container').fadeIn();
                        $('.suggestive-search-container').addClass('suggestive-search-container-open');
                        if( !hasPickedRegion ) $('#select-your-region-callout').hide();
                    } else {
                        $('.search-suggestions-container').hide();
                        $('.suggestive-search-container').removeClass('suggestive-search-container-open');
                        if( !hasPickedRegion ) $('#select-your-region-callout').fadeIn();
                    }

                });

            });
            </script>


