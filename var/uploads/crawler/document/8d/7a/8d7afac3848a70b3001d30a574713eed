    <script>
        $(function(){
            $('.right_panel_tabs').each(function(){
                $(this).find('li a').click(function(e){
                    var hash = $(this).attr('href');

                    if( hash.charAt(0) == '#' ) {
                        e.preventDefault();

                        $tab = $(hash);
                        $tab.parent().children().hide();
                        $tab.show();

                        $(this).addClass('selected').parent().siblings().find('a').removeClass('selected');
                    }
                }).first().trigger('click');
            });
        });
    </script>

