<script type="text/javascript">
jQuery(document).ready(function($) {

	$('#home-industries li a').click(function(event) { event.preventDefault() });

	$('#home-industries li').first().addClass("hover");

	$('#home-industries li').mouseenter(function() {
		var page_id = $("a", this).data("id");

		$(this).addClass("hover");
		$(this).siblings().removeClass("hover");

    		$.ajax({
        		url: 'http://www.ramp.com/wp-admin/admin-ajax.php',
        		data: {
            			'action':'ramp_home_industry',
            			'page_id': page_id
        		},
        		success:function(data) {
				if($($('#home-industries li.hover').children('a')[0]).data('id') == this.url.substring(this.url.indexOf('page_id')+8)){
            			 $("#industry-data").html(data);
				}
        		}
    		});
	});

	// Default
	var firstChild = $('#home-industries li:first-child');
	var page_id = $(firstChild.children('a')[0]).data("id");
	firstChild.addClass("hover");
	firstChild.siblings().removeClass("hover");
	$.ajax({
       		url: 'http://www.ramp.com/wp-admin/admin-ajax.php',
       		data: {
          		'action':'ramp_home_industry',
           		'page_id': page_id
       		},
        	success:function(data) {
			if($($('#home-industries li.hover').children('a')[0]).data('id') == this.url.substring(this.url.indexOf('page_id')+8)){
            			 jQuery("#industry-data").html(data);
			}
        	}
    	});

	$( "#mobile-industries" ).change(function() {
 		var page_id = $(this).find(':selected').data('id');

    		$.ajax({
        		url: 'http://www.ramp.com/wp-admin/admin-ajax.php',
        		data: {
            			'action':'ramp_home_industry',
            			'page_id': page_id
        		},
        		success:function(data) {
					$("#industry-data").html(data);
        		}
    		});
	});

});
</script>

