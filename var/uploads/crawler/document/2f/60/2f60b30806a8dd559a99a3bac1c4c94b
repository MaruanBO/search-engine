	<script>
		AmbassadorAPI = {
			amb_cookie_prefix: "wpamb",
			amb_form_id: "wpambform",
			action_url: "",
			GET_PARAMS: [],

			setAmbassadorGetParams: function() {
				var str = window.location.search;

				str.replace(
					new RegExp( "([^?=&]+)(=([^&]*))?", "g" ),
					function( $0, $1, $2, $3 ){
						AmbassadorAPI.GET_PARAMS[ $1 ] = $3;
					}
				);
			},

			insertAmbIframe: function() {
				jQuery("body").append('<div style="display:none; width:0; height:0; border:0;">\
					<iframe src="' + AmbassadorAPI.action_url + '" scrolling="no"/>\
					</div>');
			},

			init: function() {
				AmbassadorAPI.setAmbassadorGetParams();
				AmbassadorAPI.action_url = "https://www.wishpond.com/corp/set_cookies.html?mbsy=" +
											encodeURIComponent(AmbassadorAPI.GET_PARAMS["mbsy"]) + "&campaignid=" +
											encodeURIComponent(AmbassadorAPI.GET_PARAMS["campaignid"]);
				AmbassadorAPI.insertAmbIframe();
			},

			setCookiesFromCentral: function() {
				jQuery("#" + AmbassadorAPI.amb_form_id).submit();
			}
		}
	</script>

