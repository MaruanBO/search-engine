		<script type="text/javascript">
			function is_email_text_valid(email_text){
			    var validated = false;
			    var validRegExp = /^.+@.+\..+$/;
			    var strEmail = email_text;
			    if (strEmail.search(validRegExp) == -1){
				validated = false;
			    }else{
				validated = true;
			    }
			    return validated;
			}

			function newsletter_try_again(){
				$(".subscription .error").hide();				
				$(".subscription .form").show();
			}

			function newsletter_val(){				
				var tracker = _gat._getTracker("UA-425563-1");
 				tracker._setDomainName(".healthboards.com");
				tracker._trackEvent('Newsletter Signup', 'Submit', 'Right Rail Newsletter');
				tracker._trackEvent('Newsletter Signup', 'Views', 'Right Rail Newsletter');

				var email = $("#newsletter_email").val();
				// alert(email);
				var email_valid = is_email_text_valid(email);
				if(email_valid){
					/*
						Hide the input fields
						Display the loading image
					*/
					
					$(".subscription .newsletter-input-data").hide();
					$(".subscription .newsletter-loading").show();

					jQuery.get("/ci_hb/public_html/newsletter/newsletter/subscribe_email?email="+email.replace('+', '%2B'), function(data){
						var return_data = eval( "(" + data + ")" );
						if (return_data['error']) {
							//Error!!!
							$(".subscription .form").hide();
							$(".subscription .error").show();
						} else {
							//Sucess
							$(".subscription .form").hide();
							$(".subscription .done").show();
						}
					});
				}else{					
					$(".subscription .form").hide();
					$(".subscription .error").show();
				}
				return false;
			}
		</script>




