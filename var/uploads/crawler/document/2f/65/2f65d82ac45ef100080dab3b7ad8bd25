					<script type="text/javascript">
						function show_login_box(){
							$("form.login").show();
							$("form.login #navbar_username").focus();
							
						}
						//Hide the login box when clicked outside of it
						$(document).mouseup(function (e)
						{
						    var container = $("form.login");
						    if (container.has(e.target).length === 0)
						    {
						        container.hide();
						    }
						});
						
						function login_box_validation(){							
							var valid = true;
							var err_msg = "";
							if( !$("#navbar_username").val().trim() ){
								valid = false;
								err_msg += "Please enter your username.</br>"

							}
							if( !$("#navbar_password").val().trim() ){
								valid = false;
								err_msg += "Please enter your password."
							}
							if( !valid ){
								$("#navbar_error_box").html(err_msg);
							}
							return valid;
						}
					</script>
