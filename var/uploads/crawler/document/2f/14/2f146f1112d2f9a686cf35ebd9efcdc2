	<script>
	jQuery( function( $ ) { 
		// force the same height for all slides
		var $slides = $('.flexslider').find('li'),
			max_height = 0;
		$slides.each( function() {
			var slide_height = $( this ).height();
			if ( slide_height > max_height )
				max_height = slide_height;
		} );
		$slides.height( max_height );

		var $sliders = $('.flexslider').flexslider({
			slideshowSpeed: 3500,
			animationDuration: 600
		});

		setTimeout( function() {
			$sliders.flexslider( 'pause' );
		}, 30000 ); // 30s
	} );
	</script>
	
