$( function() {

    var visitorLocation = null;

    if (!visitorLocation || visitorLocation.source == 'ip_address' || visitorLocation.source == 'default') {

        if (geoPosition.init()) {

            geoPosition.getCurrentPosition(
                function(success) {                    
                    locationData = {
                        'Location': {
                            'lat':success.coords.latitude,
                            'lon':success.coords.longitude
                        }
                    };
                    console.log(locationData);
                    $.post('/front/locations/set_locale_from_html5.json', locationData, function(data) {
                        if (data.status == 'success') {
                            window.location.reload(true);
                        }
                    }, 'json');
                },
                function(error) {
        		    return;
                }
            );
        }

    }

});
