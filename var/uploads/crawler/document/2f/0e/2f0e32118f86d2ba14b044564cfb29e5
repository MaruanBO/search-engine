<script type="text/javascript">
    try {        
        // get the array of qs's
        var data = document.location.search.substr(1).split("&");
        // loop thru searching for 'evar'
        for (var i = 0; i < data.length; i++) {
            data[i] = data[i].split("=");
            
            if (data[i][0].indexOf("evar") > -1) {
                var eVarNumber = data[i][0].substring(data[i][0].indexOf("evar") + 4, data[i][0].length);
                var eVarValue = data[i][1];
                SetOmnitureEvar(eVarNumber, eVarValue);    
            }
        }
    }
    catch (err)
    {
    }
</script>
