<script>

    $(document).ready(function() {

        var reloadUrl = "/S-e6GRvPJbs62/Car/outfitmycar/car.aspx"; 

        if (reloadUrl.indexOf("#") > 0) {
            // strip off the #tab
            reloadUrl = reloadUrl.substring(0, reloadUrl.indexOf("#"));
        }

        // selected a previously selected vehicle
        $('body').on('change', '#PreviousVehiclesDdl', function(event) {
            event.preventDefault();

            if (typeof (CreateOnClickEventAjax) != "undefined") {
                CreateOnClickEventAjax('VehicleSelector:savedvehicleend', 58, 'Car Selector:SavedVehicleEnd:Homepage');   // prop7
            }

            var vehicleId = $("#PreviousVehiclesDdl option:selected").attr("vehicleId");
            var CarSelectorHandlerURL = '/S-e6GRvPJbs62/car/CarSelectorHandler.ashx';

            if (vehicleId != "0")
            {
                CarSelectorHandlerURL += '?VehicleId=' + $("#PreviousVehiclesDdl option:selected").attr("vehicleId");
            }
            else
            {
                CarSelectorHandlerURL += '?BaseId=' + $("#PreviousVehiclesDdl option:selected").attr("value");
            }

            var ajaxObject = {
                cache: false,
                url: CarSelectorHandlerURL,
                type: 'get',
                dataType: 'text'
            };

            AJAXWrapper.getData(ajaxObject).then(function (data) {
                window.location = reloadUrl;
            });
        });
    });
</script>
