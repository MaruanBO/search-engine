<script type="text/javascript">
  jQuery(document).ready(function() {
    ideeli.PromotionEngine.replaceInviteFriendImage(); 
  });
  function goto(elem) {
    var ahref = elem.data('href');
    if (ahref) {
      ideeli.joinFlow.setReturnToForForms(ahref);
    }
  }
  function recordPostLoginAction(elem) {
    var event_unit_elem = jQuery(elem).closest('.event_unit')[0];
    if (event_unit_elem) {
      ideeli.joinFlow.setActionToFollowLogin(event_unit_elem);
    } else {
      ideeli.joinFlow.setActionToFollowLogin(elem);
    }
  }
</script>

