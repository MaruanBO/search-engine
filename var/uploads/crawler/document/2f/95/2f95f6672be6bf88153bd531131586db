  <script type="text/javascript" charset="utf-8">
    window.LP_S3_HOSTNAME = "http://lp-img-production.ideel.com/";
    window.USE_LP_IMAGES = true;
    window.FINGERPRINT = "5e7fec2372b0132ee28b248474dc922e6eb5b57d";
    window.EDITORIAL_UNIT_CAROUSEL_TRANSITION_TIME_MS = 5000;

    var isHepLatest = true;
    on_server_rendering = true;

    /*ideeli.masthead.loadChannelCallbacks.push(function mastheadLoadChannelCallback(channel) {
      ideeli.latest_events.initLatest(channel);
    });*/

    window.LP_SRC_HOSTNAMES =
      ["http://dam.ideel.com/"];

    ideeli.available_channels = ["women","shoes","home","men","plus"];

    document.observe('dom:loaded',function() {
      //var latest_data = document.getElementById("latest_data").innerHTML;
      //document_latest_data = jQuery.parseJSON(latest_data.substring(2, latest_data.length - 2));

      if ($('footer_social_media') && $('editorial_unit')) {
        $('footer_social_media').hide();
      }
      // show gift card redeem related modals
      ideeli.redeem_gift_cards.init();

      ideeli.masthead.addCurrentUserLoadedEventHandler(function () {
        if (window.location.href.toString().match(/open_cart=1/)) {
          ideeli.PromotionEngine.fire('cart_open','open');
          ideeli.cart.open();
        }
      });

      ideeli.masthead.addNonLoggedUserEventHandler(function () {
        if (window.location.href.toString().match(/open_cart=1/)) {
          ideeli.joinFlow.showJoinFlowModal();
          ideeli.joinFlow.showLogin();
        }
      });

      ideeli.latest_events.markAsViewed();
    });
  </script>
  <!-- in yield js -->

