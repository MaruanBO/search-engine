<script type="text/javascript">

  function addScriptToElement(src, parent_tag_name) {
    //console.log("Appending script " + src + " to " + parent_tag_name);
    var elem = document.createElement('script');
    elem.type = 'text/javascript';
    elem.async = true;
    elem.src = src;
    document.getElementsByTagName(parent_tag_name)[0].appendChild(elem);
  }
  jQuery(window).load(function() {
    addScriptToElement('http://0-static.ideel.com/foresee_v6/web/foresee-alive.js', 'head');
    addScriptToElement('http://0-static.ideel.com/foresee_v6/web/foresee-trigger.js', 'body');
  });

</script>
