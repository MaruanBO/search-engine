    <script type="text/javascript">
        function formSubmitLBox(elt) {
            var date = new Date();
            date.setTime(date.getTime()+(365*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();

            document.cookie="localmktofirst=" + jQuery('.lboxcontactform input[name=FirstName]').val() +expires+";path=/";
            document.cookie="localmktolast=" + jQuery('.lboxcontactform input[name=LastName]').val() +expires+";path=/";
            document.cookie="localmktoemail=" + jQuery('.lboxcontactform input[name=Email]').val() +expires+";path=/";
            document.cookie="localmktocompany=" + jQuery('.lboxcontactform input[name=Company]').val() +expires+";path=/";
            document.cookie="localmktophone=" + jQuery('.lboxcontactform input[name=Phone]').val() +expires+";path=/";
            document.cookie="localmktoctype=" + jQuery('.lboxcontactform select[name=EmailContactGroup__c_lead]').val() +expires+";path=/";
            document.cookie="localmktostate=" + jQuery('.lboxcontactform select[name=State]').val()  +expires+";path=/";
            
            return Mkto.formSubmit(elt);
        }
        function formReset(elt) {
            return Mkto.formReset(elt);
        }
    </script>

