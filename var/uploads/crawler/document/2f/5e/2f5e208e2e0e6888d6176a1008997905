    jQuery(".product-sales-lb").click(function() {
		var btnText = 'Submit Now';
        var product = jQuery('.sub-menu .product-sales-lb a[rel]').attr('rel');
        var proarray = product.split('-');
        jQuery('#mktForm_21 input[name=Web_Form__c]').val(proarray[1]);
		jQuery(".frm21submitbutton").prop('value', btnText);
        setTimeout( function(){loadText(proarray[0],'RMITextHolder');}, 100);
        jQuery("#rmi-form").foundation('reveal', 'open');
        return false;
    });

    function loadText(textsection,outputholder){
        jQuery.ajax({
            type: "POST",
            url: 'http://www.realpage.com/wp-content/themes/realpage/marketo/lightboxformtext.php',
            data: 'section=' + textsection,
            success: function (data) {
                var formText = data;
                jQuery('#'+outputholder).html(formText);
            }
        });
    }
</script>
