    <script>

        jQuery(".lboxcontact").click(function() {
            var btnText = 'Submit Now';
            var product = this.rel;
            var proarray = product.split('|');
            if(proarray[0]== 'WindsorWebinar'){
                btnText = 'Register Now';
                jQuery('.lboxcontactform input[name=returnURL]').val(jQuery('.lboxcontactform input[name=returnURL]').val()+'&cstm=webinar');
                jQuery('.lboxcontactform input[name=retURL]').val(jQuery('.lboxcontactform input[name=retURL]').val()+'&cstm=webinar');
            }
            jQuery('#mktForm_21 input[name=Web_Form__c]').val(proarray[1]);
            jQuery('#mktForm_21 input[name=Products_of_Interest__c]').val(getpoi(proarray[1]));
            jQuery(".frm21submitbutton").prop('value', btnText);
            setTimeout( function(){loadText(proarray[0],'RMITextHolder');}, 100);
        });

    </script>

