    function createChart(selector, seriesOptions) {
        
        chart = new Highcharts.StockChart({
            chart: {
                renderTo: selector
            },
            
            rangeSelector: {
                selected: 0
            },
            
            yAxis: {
                plotLines: [{
                    value: 0,
                    width: 2,
                    color: 'silver'
                }],
                min: 0
            },
            
            tooltip: {
                pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b><br/>',
                yDecimals: 0
            },
            credits: {
                enabled: false
            },
            
            series: seriesOptions
        });
        
        return chart;
    }

    $(document).ready(function() {
	
        $(".wrap_menu > li").each(function () {
            $(this).children("ul").addClass("dropdown").parent().addClass("has_dropdown")
        });
		
        $(".sub").parent("li").addClass("has_drawer");
        $(".has_drawer > a").bind("click", function () {
            var e = $(this).parent().parent().hasClass("open_multiple");
            e != 1 && $(this).parent().siblings().removeClass("open").children("ul.sub").slideUp();
            $(this).parent().toggleClass("open").children("ul").slideToggle();
            return !1
        });

        $(".has_dropdown").mouseenter(function () {
            $(".has_dropdown").removeClass("open");
            $(this).addClass("open");
        });
        $(".has_dropdown").mouseleave(function () {
            $(this).removeClass("open");
        });

        $('#content_ext input[type="text"]').focus(function() {
            $(this).addClass("focused");
            if(this.value != this.defaultValue){
                this.select();
            }
        });
        $('#content_ext input[type="text"]').blur(function() {
            $(this).removeClass("focused");
        });
		
        $('#content_ext textarea').focus(function() {
            $(this).addClass("focused");
            if(this.value != this.defaultValue){
                this.select();
            }
        });
        $('#content_ext textarea').blur(function() {
            $(this).removeClass("focused");
        });

        $("select.select2").select2({
            containerCssClass : "wrap_select2"
        }); 
       
        $(".autocomplete_select2").each(function() {
            var url = $(this).data("url");
            var value = $(this).data("value");
            if (value) {
                var val = Array();
                if( value.length !== undefined ) {
                    for (var i = 0; i < value.length; i++) {
                        val[i] = value[i].id;
                    }
                    $(this).val(val.join());
                } else {
                    $(this).val(value.id);
                }
            }
            var param = $(this).data("param");
            var multiple = $(this).data("multiple");
            if(multiple) {
                multiple = true;
            } else {
                multiple = false;
            }

            var allowClear = $(this).data("allowclear");
            if(allowClear) {
                allowClear = true;
            } else {
                allowClear = false;
            }
            
            var minimumInputLength = $(this).data("minimum");
            if(typeof minimumInputLength == 'undefined') {
                minimumInputLength = "1";
            }
            
            $(this).select2({
                multiple: multiple,
                minimumInputLength: minimumInputLength,
                allowClear: allowClear,
                ajax: {
                    url: url,
                    dataType: 'json',
                    data: function (term, page) { // page is the one-based page number tracked by Select2
                        return {
                            q: term, //search term
                            param: $('#' + param).val(), 
                            perpage: 10, // page size
                            page: page // page number
                        };
                    },
                    results: function (data, page) { // parse the results into the format expected by Select2.
                        if (param && $('#' + param).val() == '') return {results: []};
                        return {results: data};
                    }
                },
                initSelection: function(element, callback) { // initialize element
                    callback(value);
                }
            });
        });
                        
        $.datepicker.setDefaults({
            dateFormat: "dd-mm-yy"
        });
					
        $.datepicker.setDefaults($.datepicker.regional['fr']);
					
        $(".vax_datepicker").datepicker({
            changeMonth: true,
            changeYear: true
        });
	
        $('.vax_datetimepicker').datetimepicker({
