            closeText: "Over",
            timeText: "",
            hourText: "Hours",
            minuteText: "Minutes"
        });
		
        $(".highlight").effect("highlight", {}, 2000);
		
        $("a.open_fancybox").each(function(i,e) {
            var href = $(e).attr("href") + '/is_fancybox/1';
            $(e).fancybox({
                "type": "ajax",
                "href": href,
                "autoScale": true,
                "height": 'auto',
                "autoDimensions": true,
                "centerOnScroll": true,
                onComplete: function(){
                    $("#fancybox-content").height("auto");
                },
                onClosed: function(){
                    location.reload();
                },
                helpers : {
                    title : null            
                }
            });
        });
        
        $("a.open_fancybox_top").each(function(i,e) {
            var href = $(e).attr("href") + '/is_fancybox/1';
            console.log(href);
            $(e).fancybox({
                "type": "ajax",
                "href": href,
                "width": '864px',
                "height": 'auto',
                'topRatio':0,
                fitToView : false,
                autoSize : false,
                onComplete: function(){
                    $("#fancybox-content").height("auto");
                },
                onClosed: function(){
                    location.reload();
                },
                helpers : {
                    title : null            
                }
            });
        });
        
        
        $("a.videolist").each(function(i,e) {
