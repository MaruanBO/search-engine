        <script type="text/javascript">
        
        function showhide_menu(){

            if($("#sub_contenu").hasClass("active")){
                $("#sub_contenu").hide();
                $("#sub_contenu").removeClass("active");
                $("#plus_contenu a").attr("title", "More");
                $("#plus_contenu a").removeClass("more_content");
                $("#plus_contenu a").html("<img src=\"/img/central/icon/more_content2.png\" alt=\"More content\" />");    
            }else{
                $("#sub_contenu").show();
                $("#sub_contenu").addClass("active");
                $("#plus_contenu a").attr("title", "Less");
                $("#plus_contenu a").addClass("more_content");
                $("#plus_contenu a").html("<img src=\"/img/central/icon/less_content2.png\" alt=\"Less content\" />");    
            }
        }
		
				
		$( "#search_ultimedia" ).submit(function( event ) {
			$("#authError_videopage").html("To browse our catalog and integrate videos on your site, please log in first");
			$("#authError_videopage").show();
			event.preventDefault();
			$(".link_login").click();
		});
        
        $( "#wrap_menu ul li a" ).on("click", function(event) {
			$("#authError_videopage").html("To access this service, you have to be signed in");
			$("#authError_videopage").show();
			event.preventDefault();
			$(".link_login").click();
		});
        
