<script type="text/javascript">
    function submitLoginFormVideoPage() {
        $.post("/auth/centrallogin", $("#loginform_videopage").serialize(),
        function(data) {
            if(!data.bool) { 
                $("#authError_videopage").html(data.message);
                $("#authError_videopage").show();
            } else {
                window.location.href = '/default/index/dispatchuser';
            }
        }, "json");
    }
	
    $(document).ready(function () {
        $("#email_login").focus(function(){
            if ($("#email_login").val() == $("#email_login_hidden").val() ) {
                $("#email_login").removeClass('placeholder');
                $("#email_login").val("");
            }
        });
		
        $("#password_login").focus(function(){
            if ($("#password_login").val() == $("#password_login_hidden").val()) {
                $("#password_login").removeClass('placeholder');
                $("#password_login").val("");
            }
        });
    });
	
