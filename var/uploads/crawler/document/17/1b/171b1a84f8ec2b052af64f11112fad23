			$("#authError_videopage").show();
			event.preventDefault();
			$(".link_login").click();
		});
        
       
        
        if($.cookie("cnil") != "ok"){
            $("#cnil").show();
        }
        
        $( "#cnil button" ).on("click", function() {
            $.cookie('cnil', "ok");
            $("#cnil").hide();
        });
        
        
        
        
		
        $(document).ready(function () {

            $('#content_ultimedia input[type="text"]').focus(function() {
                $(this).addClass("focused");
                if(this.value != this.defaultValue){
                    this.select();
                }
            });
  
