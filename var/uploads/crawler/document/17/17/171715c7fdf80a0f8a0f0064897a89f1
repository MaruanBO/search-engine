        </script><a href="/stream/" class="cmListenLiveButton"><span>Listen Live</span></a></div><script type="text/javascript">
/* <![CDATA[ */

var timeoutVar = 0;
var musicString = 'Music'; 
var historyString = 'History'; 
var retryInterval = 20; 
var defaultTitle = 'News/Talk WSB';
var defaultSubtitle = 'Atlanta\u0027s breaking news, ...';

function now_playing_jsonp_cb(data) {
    if (typeof data.objects[0] != 'undefined') {
        cmg.query('#cmNowPlayingDefaultTitle').text(typeof data.objects[0].title != 'undefined' ? data.objects[0].title : defaultTitle);
        cmg.query('#cmNowPlayingDefaultSubtitle').text(typeof data.objects[0].subtitle != 'undefined' ? data.objects[0].subtitle : defaultSubtitle);
        if (data.objects[0].data_source == historyString && typeof data.objects[0].recently_played[0] != 'undefined' && typeof data.objects[0].recently_played[0].type != 'undefined' && data.objects[0].recently_played[0].type == musicString) {
            var thisObject = new Object();
            var rpNode = data.objects[0].recently_played[0];
            thisObject.image = typeof rpNode.track.artist_detail.image != 'undefined' ? rpNode.track.artist_detail.image : null;
            thisObject.title = typeof rpNode.title != 'undefined' ? rpNode.title : '';
            thisObject.subtitle = typeof rpNode.artist != 'undefined' ? rpNode.artist : '';
            thisObject.title_url = typeof rpNode.track.absolute_url != 'undefined' ? rpNode.track.absolute_url : 'javascript:void(0);';
            thisObject.subtitle_url = typeof rpNode.track.artist_detail.html_link != 'undefined' ? rpNode.track.artist_detail.html_link : 'javascript:void(0);';
            thisObject.subtitle_chunk = typeof rpNode.track.artist_detail.html_link != 'undefined' ? rpNode.track.artist_detail.html_link : '';
            thisObject.ttl = data.objects[0].ttl != null ? data.objects[0].ttl : 10;
            populateNowPlaying(thisObject);
        } else if(data.objects[0].current_program != null){
            var thisObject = new Object();
            var cpNode = data.objects[0].current_program;
            thisObject = new Object();
            thisObject.image = typeof cpNode.photo_url != 'undefined' ? cpNode.photo_url : null;
            thisObject.title = typeof cpNode.title != 'undefined' ? cpNode.title : '';
            thisObject.subtitle = typeof cpNode.subtitle != 'undefined' ? cpNode.subtitle : '';
            thisObject.title_url = typeof cpNode.absolute_url != 'undefined' ? cpNode.absolute_url : 'javascript:void(0);';
            thisObject.subtitle_url = typeof cpNode.absolute_url != 'undefined' ? cpNode.absolute_url : 'javascript:void(0);';
            thisObject.subtitle_chunk = cmg.query('#cmNowPlayingSubtitle a').text(thisObject.subtitle).attr('href',thisObject.subtitle_url);
            thisObject.ttl = data.objects[0].ttl != null ? data.objects[0].ttl : 10;
            populateNowPlaying(thisObject);
        } else if (data.objects[0].ttl != null) {
            showDefault();
            runTimer(data.objects[0].ttl != null ? data.objects[0].ttl : 10);
        }
    } else {
        showDefault();
        runTimer(retryInterval);
    }
}

cmg.query(document).ready(function() { getData(); });


function getData() {
    try {
        
        var params = {format:'jsonp', callback: 'now_playing_jsonp_cb'};
        // IE caches ajax aggressively. Add a url param for this minute to get around that.
        if(cmg.query.browser.msie){
            params['timestamp'] = new Date(new Date().setSeconds(0)).setMilliseconds(0);
        }
        cmg.query.ajax({
            
            url: 'http://'+site_domain+'/lsp/_api/v1/stream/',
            data: params,
            accepts: 'text/javascript',
            method: 'GET',
            dataType:'script',
            error: function() { throw("error loading now playing"); },
            success: function() {
                
            }
        });
    }
    catch(err) {
        showDefault();
        runTimer(retryInterval);
    }
}

function populateNowPlaying(currentObject){
    if(currentObject.image != null){
        cmg.query('#cmNowPlayingImage').attr({src: currentObject.image, alt: currentObject.subtitle}).show();
    } else {
        cmg.query('#cmNowPlayingImage').hide();
    }
    cmg.query('#cmNowPlayingTitle a').text(currentObject.title).attr('href',currentObject.title_url);
    cmg.query('#cmNowPlayingSubtitle').html(currentObject.subtitle_chunk);
    showDetails();
    runTimer(currentObject.ttl);
}

function runTimer(timeInterval){
    clearTimeout(timeoutVar);
    timeInterval *= 1000;
    timeoutVar = setTimeout("getData()",timeInterval);
}

function showDefault(){
    cmg.query('#cmNowPlayingStreaming').hide();
    cmg.query('#cmNowPlayingDefault').show();
}

function showDetails(){
    cmg.query('#cmNowPlayingDefault').hide();
    cmg.query('#cmNowPlayingStreaming').show();
}

/* ]]> */
</script><div id="cmWeatherWidget"></div><script type="text/javascript">
                    
                    function weather_jsonp_cb(data) {
                        var html = data['html'];
                        if (html) {
                            cmg.query('#cmWeatherWidget').html(html);
                        }
                    }
                    var widget_path = '/weather/widget/';
                    
                    var request_url = 'http://' + site_domain + widget_path;
                    cmg.query.ajax({
                        url: request_url,
                        method:'GET',
                        accepts:'text/javascript',
                        data: {format:'jsonp', callback:'weather_jsonp_cb'},
                        dataType:'script',
                        error: function() { },
                        success: function() {
                            
                        }
                    });
