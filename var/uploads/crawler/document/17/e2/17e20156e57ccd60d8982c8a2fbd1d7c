	    var mytimer =$("#healthcounter").is(":visible");	
	    if (mytimer == true){
  		$("#medibutton").trigger( "click" );
	    } else {
  		$("#healthbutton").trigger( "click" );
	    }
	});
        $("#rightarrow").click(function(e){
	    var mytimer =$("#healthcounter").is(":visible");	
	    if (mytimer == true){
  		$("#medibutton").trigger( "click" );
	    } else {
  		$("#healthbutton").trigger( "click" );
	    }
	});

        $("#left-arrow").click(function(e){
	    var box1 =$(".inner-box-1").is(":visible");
	    var box2 =$(".inner-box-2").is(":visible");
	    var box3 =$(".inner-box-3").is(":visible");
	    var box4 =$(".inner-box-4").is(":visible");
	    var box5 =$(".inner-box-5").is(":visible");
	    var v = 1;
	    var w = 2;
	    var x = 3;
	    var y = 4;
	    var z = 5;
	    if(box1==true) {
		v = 5;
		w = 1;
		x = 2;
		y = 3;
		z = 4;
	    } else if (box2==true) {
		v = 1;
		w = 5;
		x = 4;
		y = 3;
		z = 2;
	    } else if (box3==true) {
		v = 2;
		w = 1;
		x = 5;
		y = 4;
		z = 3;
	    } else if (box4==true) {
		v = 3;
		w = 2;
		x = 1;
		y = 5;
		z = 4;
	    } else {
		v = 4;
		w = 3;
		x = 2;
		y = 1;
		z = 5;
	    }
            $(".inner-box-"+w).fadeOut(1,function(){
              $(".inner-box-"+x).fadeOut(1,function(){
                $(".inner-box-"+y).fadeOut(1,function(){
	    	  $(".inner-box-"+z).fadeOut(1,function(){
		    $(".inner-box-"+v).fadeIn("slow","swing");
		    });
	          });
	       });
	    });
        });
        $("#right-arrow").click(function(e){
	    //TODO: get inner-box number show next and hide others
	    var box1 =$(".inner-box-1").is(":visible");
	    var box2 =$(".inner-box-2").is(":visible");
	    var box3 =$(".inner-box-3").is(":visible");
	    var box4 =$(".inner-box-4").is(":visible");
	    var box5 =$(".inner-box-5").is(":visible");
	    var v = 5;
	    var w = 4;
	    var x = 3;
	    var y = 2;
	    var z = 1;
	    if(box1==true) {
		v = 2;
		w = 3;
		x = 4;
		y = 5;
		z = 1;
	    } else if (box2==true) {
		v = 3;
		w = 4;
		x = 5;
		y = 1;
		z = 2;
	    } else if (box3==true) {
		v = 4;
		w = 5;
		x = 1;
		y = 2;
		z = 3;
	    } else if (box4==true) {
		v = 5;
		w = 1;
		x = 2;
		y = 3;
		z = 4;
	    } else {
		v = 1;
		w = 2;
		x = 3;
		y = 4;
		z = 5;
	    }
            $(".inner-box-"+z).fadeOut(1,function(){
              $(".inner-box-"+y).fadeOut(1,function(){
                $(".inner-box-"+x).fadeOut(1,function(){
		  $(".inner-box-"+w).fadeOut(1,function(){
		    $(".inner-box-"+v).fadeIn("slow","swing");
		    });
	          });
	        });
	    });
        });
        $("#smallbiz h5").click(function(e){
	    for(var x = 1; x<6; x++) {
		$(".inner-box-"+x).fadeOut(1);
	    } 
	    var xclick = $(this).attr('id');
	    $(".inner-box-"+xclick).fadeIn("slow","swing");
       });
 //$("#medibutton").trigger("click");    
 };
</script>
