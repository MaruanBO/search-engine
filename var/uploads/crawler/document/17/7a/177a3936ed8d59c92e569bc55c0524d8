<script>
	$(document).ready(function() {

		$("a").click(function() {
			//make sure the link is in the 'footer links'
			if($(this).parents(".Copyright").length)
			{
				var linkHref = $(this).attr("href").replace(/(<([^>]+)>)/ig,"");
				var linkNewWindow = $(this).attr("target");			
				
				// if the link href is not relative
				if(!linkHref.match("^/USA"))
				{
					//create the base url
					var baseURI = location.protocol + '://' + location.hostname + '/USA';
										
					// determine whether or not the clicked on link is external or not
					if(!linkHref.match("^" + baseURI))
					{	
						var analyticsAccountNo = 'UA-223533-8';
						
  						try
  						{
    						var pageTracker=_gat._getTracker(analyticsAccountNo);
    						pageTracker._trackEvent('Outbound Links', linkHref);
    						goToUrl(linkHref, linkNewWindow);
    						return false;
  						}catch(err){}
						
					}
				
				}
			}
		});
	});
	
	function goToUrl(url, target)
	{
		if(target == '_blank')
		{
			window.open(url);
			return false;
		}
		
		setTimeout('document.location = "' + linkHref + '"', 100);
		return false;
	}
</script>
