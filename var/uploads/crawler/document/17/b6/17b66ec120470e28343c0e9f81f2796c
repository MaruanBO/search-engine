<script type="text/javascript">

                function submitPageFromFooter()
                {
                if(null!=document.getElementById("countryOptions")){
                var selectedCountry = document.getElementById("countryOptions").options[document.getElementById("countryOptions").selectedIndex].value;
                if(selectedCountry != ""){
                var url = "/car-rental/avisHome/redirectURL.ac";
                url += '?COUNTRY='+selectedCountry;
                window.open(url);
                }
                }
                }
				
				function getCarSalesUrl(state, isCarSalesEligible, section){
					var url = "https://www.carsalesavis.com/";
					if(isCarSalesEligible){
						url += 'state/' + state;
					}
					if(section == 'services'){
						url += '?ICID=ft_services_carsales';
					} else {
						url += '?ICID=ft_popsearch_carsales';
					}
					window.open(url);	
				}

</script>
