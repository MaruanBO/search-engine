<script language="JavaScript"> 
	
	
	
	
	
	function returnUserState()
	{
		var authenticated = 'false';
		var rememberMe	  = 'false';
		var userState = 'AVIS_ANONYMOUS';
		if(authenticated == 'true')
		{		  return 6;		}
		else if(rememberMe == 'true')
		{		  return 5;		}
		
		else if(userState == "AVIS_RECOGNIZED_EXPLICIT"){return 4;}
		else if(userState == "AVIS_RECOGNIZED_IMPLICIT"){return 3;}
		else if(userState == "AVIS_ANONYMOUS_IDENTIFIED"){return 2;}
		else {return 1;}
	}
	
	//For Used Car Sales campaign
	if (false) {
		s.events = "event43";
	}
	s.eVar15 =	returnUserState();
	//s.eVar21 = "";
	s.eVar30 = 'US&en';
</script>


