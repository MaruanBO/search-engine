	<script src="http://images.cwtv.com/inc/js/cwingo/libs/jstz.min.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript">
		var tko_start = 946713600;
		var tko_end = 1893484800;
		var tko_swf_url = "";
		var tko_watched = '20150218145755';
		var tko_image_close = false;
		var tko_has_been_closed = false;
		var tko_date = new Date();
		var timezone = jstz.determine_timezone();
		var tzoffset = timezone.offset();
		var tko_time = Math.floor(tko_date.getTime() / 1000);
		
		if (tzoffset == '-05:00') tko_time += (3 * 3600);
		if (tzoffset == '-06:00') tko_time += (3 * 3600);
		if (tzoffset == '-07:00') tko_time += (1 * 3600);
		
		CWTV.Site.tko_active = (tko_time >= tko_start && tko_time < tko_end);		
		CWTV.Site.overlayAdHasLoaded = (GetCookie('cwtko_69fc8a') == tko_watched);
				
		function tkoClicked(url) {
			if ( tko_image_close ) {
				s.tl(true,'o',"Takeover Clicked");
				clearTimeout( tko_image_close );
				tko_image_close = false;
			}
			document.location.href = url;
			return false;
		}
		function closeAdOverlay(track_it) {

			if (!tko_has_been_closed) {
				CWTV.Site.overlayIsOpen = false;
				tko_has_been_closed = true;

				if ( typeof track_it != 'undefined' && track_it )
					s.tl(true,'o',"Takeover Closed");
				
				$('#cw-fullscreen').hide();
				$('#cw-fullscreen').html('');
				$('#cw-normal').show();
				
				adjustLayout(true);
				
				if (typeof(LoadVideoPlayer) != 'undefined')
					LoadVideoPlayer();
			}

			return false;
		}
		function revealAdOverlay() {
			CWTV.Site.overlayIsOpen = true;
			$("#home-billboard div.items").trigger("pause", true);
			$('#cw-normal').hide();
			$('#cw-fullscreen').show();
		}
		
		if ( CWTV.Site.tko_active )
			SetCWCookie2('cwtko_69fc8a', tko_watched);
	</script>


