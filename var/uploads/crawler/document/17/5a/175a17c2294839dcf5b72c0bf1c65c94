<script>
var video_start = 0;
var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-2405615-1']);
_gaq.push(['_trackPageview']);

(function() {
	var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();

function loadImage( $items ) {
	$.each($items, function(index, $item) {
		if ( !$($item).is('img') )
			$img = $($item).find('img');
		else
			$img = $($item);
		if ( $img.is('img') ) {
			if ( $img.attr('src').indexOf('thumb-placeholder') != -1) {
				$img.attr('src', $img.data('original'));
			}
		}
	});
}

function toggleHeroSocial() {
}

var allowClick = true;
function carouselDragRelease() {
	allowClick = false;
	setTimeout(function() { allowClick = true; }, 700);
}

function socialClick(show_slug) {
	if (allowClick && typeof(CWTV.Site.socialBarClick) == "function") {
		return CWTV.Site.socialBarClick(show_slug);
	}
	allowClick = true;
	return false;
}

function heroOnBeforeChange( slider, index ) {
	if ( slider || CWTV.Site.heroSliderInstance ) {
		var id = $('#herolist').slickCurrentSlide() + 1;
		$('#herolist .info').hide().removeClass('shadowed');
	}
}

function heroOnAfterChange( slider, index ) {
	if ( slider || CWTV.Site.heroSliderInstance ) {
		var index = $('#herolist').slickCurrentSlide() + 1;
		$('#hero_'+index+' .info').fadeIn('fast', function() {
        	$('#hero_'+index+' .info').addClass('shadowed');
      	});
	}
}

function heroOnInit( slider ) {
	$('#herolist .info').addClass('shadowed');
}

function LoadSocialData( id, force ) {
	if (force || CWTV.Site.lastActiveHero != id) {
		CWTV.Site.lastActiveHero = id;
	}
}

function initHero( width ) {
	
	if ( !CWTV.Site.heroSliderInstance ) {
		
		var use_fade = (CWTV.Site.ua.is_ios || CWTV.Site.ua.is_android) ? false : true;
		var touch_move = (CWTV.Site.ua.is_ios || CWTV.Site.ua.is_android);

		$("#herocontainer").css('visibility','visible');

		CWTV.Site.heroSliderInstance = $('#herolist').slick({

			slidesToShow: 1,
			slidesToScroll: 1,
			autoplay: true,
			autoplaySpeed: 5000,
			infinite: true,

			arrows: false,
			dots: true,
			pauseOnDotsHover: true,

			touchMove: touch_move,

			cssEase: 'linear',
			slide: 'li',

			onInit: heroOnInit,
			onBeforeChange: heroOnBeforeChange,
			onAfterChange: heroOnAfterChange,

			fade: use_fade
		});
	}

	initHomeVideoCarousels();
}


function initVideoCarousel( folder_index ) {

	var touch_move = (CWTV.Site.ua.is_ios || CWTV.Site.ua.is_android);
	$("#touchcarousel_"+folder_index).css('visibility','visible');

    CWTV.Site.videoPageSliderInstances[folder_index] = $("#list_"+folder_index).slick({
            
        autoplay: false,
        infinite: false,
		focusOnSelect: true,

        touchMove: touch_move,

        dots: false,
        arrows: true,
        appendArrows: '#touchcarousel_'+folder_index,
        
        speed: 500,
        cssEase: 'cubic-bezier(0.695, 0.005, 0.145, 1.000)',
        slide: 'li',

        fade: false,

        variableWidth: true,
        slidesToShow: 4,
        slidesToScroll: 4,
        responsive: [
            {
              breakpoint: 1280,
              settings: {
                slidesToShow: 3,
                slidesToScroll: 3
              }
            },
            {
              breakpoint: 980,
              settings: {
                slidesToShow: 2,
                slidesToScroll: 2
              }
            },
            {
              breakpoint: 768,
              settings: {
                slidesToShow: 1,
                slidesToScroll: 1
              }
            }
          ]
    });
}

function initHomeVideoCarousels( ) {
	var touch_move = (CWTV.Site.ua.is_ios || CWTV.Site.ua.is_android);

	if ( !CWTV.Site.newVideosSliderInstance ) {

		$("#homevideolatest").css('visibility','visible');
		CWTV.Site.newVideosSliderInstance = $('#latestvid_list').slick({
			
			autoplay: false,
			infinite: false,
			touchMove: touch_move,

			dots: false,
			arrows: true,
			appendArrows: '#homevideolatest',
			
			speed: 500,
            cssEase: 'cubic-bezier(0.695, 0.005, 0.145, 1.000)',
			slide: 'li',

			fade: false,

			variableWidth: true,
			slidesToShow: 3,
			slidesToScroll: 3,
			responsive: [
			    {
			      breakpoint: 1280,
			      settings: {
			        slidesToShow: 2,
			        slidesToScroll: 2
			      }
			    },
			    {
			      breakpoint: 980,
			      settings: {
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    }
			  ]
		});
	}

	if ( !CWTV.Site.featuredVideosSliderInstance ) {

		$("#homevideofeatured").css('visibility','visible');
		CWTV.Site.featuredVideosSliderInstance = $('#featuredvid_list').slick({
			
			autoplay: false,
			infinite: false,
			touchMove: touch_move,

			dots: false,
			arrows: true,
			appendArrows: '#homevideofeatured',
			
			speed: 500,
            cssEase: 'cubic-bezier(0.695, 0.005, 0.145, 1.000)',
			slide: 'li',

			fade: false,

			variableWidth: true,
			slidesToShow: 4,
			slidesToScroll: 4,
			responsive: [
			    {
			      breakpoint: 1280,
			      settings: {
			        slidesToShow: 3,
			        slidesToScroll: 3
			      }
			    },
			    {
			      breakpoint: 980,
			      settings: {
			        slidesToShow: 2,
			        slidesToScroll: 2
			      }
			    },
			    {
			      breakpoint: 768,
			      settings: {
			        slidesToShow: 1,
			        slidesToScroll: 1
			      }
			    }
			  ]

		});
	}
}

function getSiteWidth( w ) {	
	var width = w >= 1280 ? 1280 :
	                w >= 980 ? 980 : 
	                w >= 768 ? 768 : 
	                w >= 480 ? 480 : 320;
	return width;
}

function adjustLayout(force) {	
	var width = $(window).width();
	var win_h = $(window).height();
	var hero_width = 1280;
	var hero_height = 494;
	
	width = getSiteWidth( width );
	if ( force || (width != CWTV.Site.width)) {
		var bannerResize = (CWTV.Site.width < 980 && width >= 980)
					|| (CWTV.Site.width >= 980 && width < 980);
		if ( bannerResize ) {
			reloadBannerSlots();
		}
		CWTV.Site.width = width;
		
		//_gaq.push(['_trackEvent', 'Layout Width', 'Range', width]);
				
		var onMobileScreen = (width < 480) || CWTV.Site.ua.is_ios;
		hero_width = CWTV.Site.width;

		var new_class = (CWTV.Site.width >= 1280) ? 'wLarge' :
		                (CWTV.Site.width <= 320) ? 'wSmall' : 'w'+CWTV.Site.width;
	    $('body').removeClass('wLarge w980 w768 w480 wSmall').addClass(new_class);
		
		$("#full-wrapper").trigger("windowresize");
		
		if ( !CWTV.Site.overlayIsOpen ) {

			$("#herolist .heroitem, #herolist .heroimg").css('width', hero_width+'px').css('height', hero_height+'px');
			if (!onMobileScreen) {
				$("#herocontainer").css('width', hero_width+'px');
				$("#herocontainer .touchcarousel-wrapper").css('height', hero_height+'px');
			}
			else
				$("#herocontainer").css('height', 'auto');

			if ( CWTV.Site.heroSliderInstance ) {
				//$("#herocontainer").trigger(CWTV.Site.heroSliderInstance._resizeEvent);
			}
			else {
				initHero( hero_width );
			}

			var thumb_width = 320;
			var itemsVisible = Math.floor(CWTV.Site.width / thumb_width);

			if ( CWTV.Site.newVideosSliderInstance ) {
				var latestItemsVisible = itemsVisible - 1;
				var latest_videos_width = latestItemsVisible * thumb_width;
				$("#homevideolatest-wrap").css('width', latest_videos_width+'px');
			}

			var videos_thumbs_width = itemsVisible * thumb_width;
			if ( CWTV.Site.featuredVideosSliderInstance ) {
				$("#homevideofeatured-wrap").css('width', videos_thumbs_width+'px');
			}

			if ( $('.videothumbslist').length ) {
				$(".videothumbslist").each(function(i) {
					var folder_index = $(this).data('index');
					var tc = CWTV.Site.videoPageSliderInstances[folder_index];
					if (tc) {
						var wrap_el = $("#touchcarousel_"+folder_index);
						wrap_el.css('width', videos_thumbs_width+'px');
					}
					
				});
			}

			for(var i=0; i<9; i++) {
				$('ul#day_'+i+' li div.wrap').attr('style', '');
				equalHeights('ul#day_'+i+' li div.wrap');
			}
		}
	}
}

function adjustVideoSize( container_width ) {

	if (typeof initSeedVideo != 'undefined') {
		$('#video-container').show();
		var width, height;

		if ( container_width > 0 ) {
			width = container_width;
			height = Math.floor(width *  (405 / 720));
		} else {
			var win_h = $(window).height();
			height = win_h - 150;
			if ( height < 214 ) height = 214;
			width = 720 / 405 * height;
			if ( width < 178 ) width = 178;
		}

		overlay_height = Math.floor(height - 210);
		if ( overlay_height < 0 ) overlay_height = 0;
		width+='px';
		height+='px';
		$('#videoplayer').css({'width':width,'height':height});	
		$('#videoplayer_flash').css({'width':width,'height':height});
		$('#vsplayer').css({'width':width,'height':height});
		$('#videoplayer_overlay').css({'height':overlay_height+'px'});
	}
}

function findLocation() {
	if ( navigator.geolocation )
    	navigator.geolocation.getCurrentPosition(foundCallback, errorCallback, {enableHighAccuracy:true,maximumAge:60000});
}
function foundCallback(position) {
	//alert( position );
}
function errorCallback(position) {
}


if ( CWTV.Site.overlayAdHasLoaded || CWTV.Site.tko_active == false )
	closeAdOverlay(false);
else
	revealAdOverlay();

adjustLayout(false);

CWTV.Site.appLinkTimer = false;
function tryAppLink(url) {
	if(CWTV.Site.appLinkTimer) {
		clearTimeout(CWTV.Site.appLinkTimer);
		CWTV.Site.appLinkTimer = false;		
	}
	CWTV.Site.currentURL = window.location;
	CWTV.Site.appLinkTimer = setTimeout(function() { window.location=CWTV.Site.appStorePath; }, 2200);
	window.location = url;
	return false;
}

var resizeTimer = false;
$(window).resize(function() {
	if(resizeTimer) {
		clearTimeout(resizeTimer);
		resizeTimer = false;		
	}
	adjustLayout(false);
});

if ( window.addEventListener ) {
	window.addEventListener("orientationchange", function() {
		adjustLayout(false);
	}, false);
}

$(window).load(function(){
	adjustLayout(false);
});

$(document).ready( function() {
	
	if (typeof(initCWVideo) != 'undefined') {
		initCWVideo();
	}
	
	CWTV.Site.startUI();
	
	if (CWTV.Site.ua.is_ios || CWTV.Site.ua.is_android) {
		$('a.watch, a.heroplay').click(function(e){
			var slug = $(this).data('showslug');
			var mediakey = $(this).data('mediakey');
			tryAppLink( CWTV.Site.getAppDeepLink(slug, mediakey));
			return false;
		});
	}
	else {
		$('footer .links').unselectable();
	}
});

$('#cw-channel-info .handle').click(function(){
	var el = $('#cw-channel-info');
	var if_el = $('#cw-channel-info iframe');
	var if_handle = $('#cw-channel-info a.handle');

	el.toggleClass('expanded');
	if ( el.hasClass('expanded') ) {
		if_el.attr('src', "/page/stations/v2/");
		if_handle.html('<span>CLOSE</span><img src="http://images.cwtv.com/images/r14/optin-close.png" width="15" height="15">');
	} else {
		if_el.attr('src', 'http://images.cwtv.com/images/spacer.gif');
		if_handle.html('<span>FIND YOUR CHANNEL</span>');
	}	
	return false;
});

</script>





