		var googletag = googletag || {};		
		googletag.cmd = googletag.cmd || [];
			

		
		(function() {
			var gads = document.createElement('script');
			gads.async = true;
			gads.type = 'text/javascript';
			gads.src = '//www.googletagservices.com/tag/js/gpt.js';
			var node = document.getElementsByTagName('script')[0];
			node.parentNode.insertBefore(gads, node);			
		})();
		
		googletag.cmd.push(function() {
			CWTV.Site.is_mobile = CWTV.Site.is_mobile || 0;
			if ( CWTV.Site.is_mobile) {
				adSlot2 = googletag.defineSlot(ad_network_site_zone, [300, 250], "ad_300x250");
				adSlot2.set("ad_type", "flash");
				if (is_video_page)
					adSlot2.addService(googletag.companionAds());
				adSlot2.addService(googletag.pubads());
			}
			else {
				adSlot0 = googletag.defineSlot(ad_network_site_zone, [160, 600], "ad_160x600");
				adSlot0.set("ad_type", "flash");
				if (is_video_page)
					adSlot0.addService(googletag.companionAds());
				adSlot0.addService(googletag.pubads());
				
				adSlot1 = googletag.defineSlot(ad_network_site_zone, [[970, 90], [728, 90]], "ad_728x90");
				adSlot1.set("ad_type", "flash");
				// [0, 0] fits browsers of any size smaller than 980 x 100
				var mapping = googletag.sizeMapping().addSize([980, 100], [[970, 90], [728, 90]]).addSize([0, 0], [728, 90]).build();
				adSlot1.defineSizeMapping(mapping);
				if (is_video_page)
					adSlot1.addService(googletag.companionAds());
				adSlot1.addService(googletag.pubads());
				
				adSlot2 = googletag.defineSlot(ad_network_site_zone, [300, 250], "ad_300x250");
				adSlot2.set("ad_type", "flash");
				if (is_video_page)
					adSlot2.addService(googletag.companionAds());
				adSlot2.addService(googletag.pubads());
				
				adSlot3 = googletag.defineSlot(ad_network_site_zone, [300, 60], "ad_300x60");
				adSlot3.set("ad_type", "flash");
				if (is_video_page)
					adSlot3.addService(googletag.companionAds());
				adSlot3.addService(googletag.pubads());				
			}
			
			googletag.companionAds().setRefreshUnfilledSlots(true);
			if (is_video_page)
				googletag.pubads().enableVideoAds();
			// Enable the companion ads service.
			googletag.enableServices();	
		});
		
		function getAdHTML(w, h) {
			if ($('#ad_'+w+'x'+h).length != 0)
				return '<iframe src="http://ad.doubleclick.net/adi/cwtv.home;sz='+w+'x'+h+';ord='+ord+'?" width="'+w+'" height="'+h+'" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling="no" bordercolor="#000000">';
			return false;		
		}
		function reloadCompanionSlots() {
			googletag.pubads().refresh([adSlot1, adSlot3]);
		}
		function reloadBannerSlots() {
			googletag.cmd.push(function() {
				googletag.pubads().refresh([adSlot1]);
			});
		}

