<script type="text/javascript">
    var currentFlyout = null;
    var flyoutHideTimer = null;
    function flyout(event, m) {
        if (m) {
            $("#flyoutcontent").css("visibility", "hidden");
            $("#flyouttab").css("visibility", "hidden");
            currentFlyout = m;
            var flyout = $("#flyout");
            var offset = flyout.offset();
            if (flyoutHideTimer != null) clearTimeout(flyoutHideTimer);
            //$("#menu_" + m).css({ top: offset.top + "px", left: offset.left + "px" }).css("visibility", "visible");
            var target = event.target;
            var position = $(target).offset();
            var positionLi = $(target).parent().offset();
            var widthElement = $(target).width();
            
            var totalHeight = $("#menu_" + m).outerHeight();
            var columns = 1;
            if (totalHeight > 300) columns = 2;
            if (totalHeight > 600) columns = 3;
            if (totalHeight > 900) columns = 4;
            if (totalHeight > 1200) columns = 5;
            $("#flyoutcontent").empty();
            $("#flyouttab").empty();
            $("#flyoutcontent").width(columns * 180);
            //$("#menu_" + m).columnize({ columns: columns, buildOnce: false, target: "#flyoutcontent" });
            for (var c = 0; c < columns; c++) {
                $("#flyoutcontent").append("<div class='column' id='flyout_" + m + "_c" + c + "'></div>");
                if (c < columns - 1) {
                    $("#flyoutcontent").append("<div class='column divider'> </div>");
                }
            }
            var blockHeight = new Array();
            for (var k = 0; k < $("#menu_" + m).children().length; k++) {
                blockHeight[k] = $($("#menu_" + m).children()[k]).outerHeight();
            }

            var averageHeight = totalHeight / columns;
            
            var columnAllocation = new Array();
            var columnHeight = new Array();
            for (var c = 0; c < columns; c++) {
                columnHeight[c] = 0;
                columnAllocation[c] = new Array();
            }
            //first item always goes to first column
            columnAllocation[0].push(0);
            columnHeight[0] += blockHeight[0];
            //last item always goes to last column, skip the last one which is a divider
            columnAllocation[columns - 1].push($("#menu_" + m).children().length - 2);
            columnHeight[columns - 1] += blockHeight[$("#menu_" + m).children().length - 2];
            var currentColumn = 0;
            
            //assign the remaining columns
            for (var b = 1; b < $("#menu_" + m).children().length - 2; b++) {
                if (columnHeight[currentColumn] > averageHeight) {
                    currentColumn++;
                }
                if (currentColumn == columns) {
                    currentColumn = columns - 1;
                }
                columnAllocation[currentColumn].push(b);
                columnHeight[currentColumn] += blockHeight[b];
            }
            for (var c = 0; c < columns; c++) {
                $("#flyout_" + m + "_c" + c).html();
                for (var b = 0; b < $("#menu_" + m).children().length; b++) {
                    if (columnAllocation[c].indexOf(b) > -1) {

                        $("#flyout_" + m + "_c" + c).append($($("#menu_" + m).children()[b]).clone());
                    }
                }
                if ($($("#flyout_" + m + "_c" + c).children()[0]).hasClass("separator")) {
                    $($("#flyout_" + m + "_c" + c).children()[0]).hide();
                }
                if ($($("#flyout_" + m + "_c" + c).children()[$("#flyout_" + m + "_c" + c).children().length - 1]).hasClass("separator")) {
                    $($("#flyout_" + m + "_c" + c).children()[$("#flyout_" + m + "_c" + c).children().length - 1]).hide();
                }
            }
            //remove empty columns
            if ($("#flyoutcontent").children().last().children().length == 0) {
                $("#flyoutcontent").children().last().remove();
                $("#flyoutcontent").children().last().remove();
                $("#flyoutcontent").width($("#flyoutcontent").width() - 180);
            }
            var widthMenu = $("#flyoutcontent").width();
            var left = position.left - (widthMenu - widthElement) / 2;
            if (left < offset.left) {
                left = offset.left;
            }
            if (left + widthMenu > offset.left + 960) {
                left = offset.left + 960 - widthMenu;
            }
            $("#flyouttab").width($(target).parent().outerWidth() + 1);
            $("#flyouttab").html("<a href='" + $(target).attr("href") + "'>" + $(target).html() + "</a>");
            $("#flyouttab").attr("rel", m);
            $("#flyouttab").css({ top: (offset.top - 34) + "px", left: (positionLi.left - 1) + "px" }).css("visibility", "visible");
            //$("#flyoutcontent .first.column").css({"padding-left": "20px", "border-right": "solid 1px #666666"});
            $("#flyoutcontent").css({ top: (offset.top - 9) + "px", left: left + "px" }).css("visibility", "visible");
            $("#flyoutcontent").attr("rel", m);
            //$(window).resize();
        }
        else {
            currentFlyout = null;
            flyoutHideTimer = setTimeout(flyoutMarkForHiding, 500);
        }
    }
    function flyoutMarkForHiding(m) {
        if (currentFlyout == null) {
            $("#flyoutcontent").css("visibility", "hidden");
            $("#flyouttab").css("visibility", "hidden");
        }
    }
    function flyoutover(event, a) {
        currentFlyout = $(a).attr("rel");
    }
    function flyoutout(event) {
        currentFlyout = null;
        flyoutHideTimer = setTimeout(flyoutMarkForHiding, 500);
    }
</script>
<div style="margin:auto; width: 960px; z-index: 2000; position:relative" id="flyout"></div>
<div style="margin:auto; z-index: 2000; position:absolute" id="flyoutcontent" onmouseover="flyoutover(event, this)" onmouseout="flyoutout(event)"></div>
<div style="margin:auto; z-index: 2000; position:absolute; height: 24px" id="flyouttab" onmouseover="flyoutover(event, this)" onmouseout="flyoutout(event)"></div>
