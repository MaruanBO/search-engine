var CurrentSlide = 1;
var SlideTimer;
function changeSlide(a) {
 var SlideCount = $(".hero_slide").length;
 CurrentSlide = a;
 if (CurrentSlide > SlideCount) CurrentSlide = 1;
 if (CurrentSlide < 1) CurrentSlide = SlideCount;
 $(".hero_slide").hide();
 $("#hero_slide" + CurrentSlide).show();
}
function prevSlide() {
    if (SlideTimer) clearTimeout(SlideTimer);
    changeSlide(CurrentSlide - 1);
    SlideTimer = setTimeout(slideShow,6000);
}
function nextSlide() {
    if (SlideTimer) clearTimeout(SlideTimer);
    changeSlide(CurrentSlide + 1);
    SlideTimer = setTimeout(slideShow,6000);
}
function slideShow() {
    changeSlide(++CurrentSlide);
    SlideTimer = setTimeout(slideShow,6000);
}

$(document).ready(function () {
 SlideTimer = setTimeout(slideShow, 6000);
