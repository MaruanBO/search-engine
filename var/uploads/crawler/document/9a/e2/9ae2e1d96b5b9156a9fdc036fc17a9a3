//Compares user's local time against a specified time and incorporates time zone adjustment.
function checkDate(launchDate){
	var dthen	= new Date(launchDate);
  	var dnow	= new Date();
	var offHours = (dnow.getTimezoneOffset()/60)-5;
	ddiff		= new Date(dthen-dnow);
	gsecs		= Math.floor(ddiff.valueOf()/1000);
	
	return gsecs;
}
	

function scheduledOverlay(){
if ( (difSeconds<5) && (difSeconds>-60) && (scheduledOverlayDisplay==false)){
flashTracking("Memorial Day Takeover|Display Honor");
$('body').prepend('<div id="overlay-outside"><div class="overlay-inside"><div class="overlay-content"><img src="/sxm/img/homepage/takeunder/memorial-day-2012/MemorialDaySilence-v1.png"></div></div></div>');
var position=$('#overlay-outside').position();
$('#overlay-outside').css('top',scheduledOverlayAlign);
$('#overlay-outside').css('z-index',scheduledOverlayIndex);
$('#overlay-outside').height($(document).height()-position.top);
$('#overlay-outside').fadeIn();
scheduledOverlayDisplay=true;
}
difSeconds=difSeconds-1;
if ((difSeconds>-60) | (scheduledOverlayDisplay==true)){
setTimeout(scheduledOverlay, 990);
}
if (difSeconds<-60 && scheduledOverlayDisplay==true){
$('#overlay-outside').fadeOut();
scheduledOverlayDisplay=false;
flashTracking("Memorial Day Takeover|Remove Honor");
}
}

