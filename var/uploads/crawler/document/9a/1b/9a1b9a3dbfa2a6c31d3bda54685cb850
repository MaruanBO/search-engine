  		function encodeToAsteriks (toEncode) {
  			var encoded = "";
		  		 if(toEncode != null ){
							for (i=0;i<=toEncode.length-1;i++){
							 	if (i<4){
									encoded= encoded + toEncode.charAt(i);
								}else {
									encoded= encoded + "*";
								}
							}
				 }
		 	return encoded;
  		}
        	      	
</script>


