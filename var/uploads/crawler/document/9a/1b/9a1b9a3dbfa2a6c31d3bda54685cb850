$(document).ready(function () {
  TypeAhead($('#siteSearchInput'), $('#SearchTypeAhead'), false, 2, function(suggestion) {
    var brk, suggestionURL;  
    if (suggestion.url.indexOf("?") > -1) {
      brk = "&";
    } else {
      brk = "?";
    }
    updateSearchTermCookie(suggestion.display,true); 
    suggestionURL = suggestion.url+brk+"searchAhead=true&searchAheadTerm="+encodeURIComponent($.trim($("#siteSearchInput").val()))+"&typahddsp="+encodeURIComponent(suggestion.display)+"&hdrsrh=true";
    
    forward(suggestionURL);
  });
  MSCJS.Navigation();
  MSCJS.loginModal();
  $('.tabs').each(function () {
    $(this).atabs({mode:"tabs",defaultTab:1});
  });
  $('.quickOrder > a, .quickOrder .close').click(function (e) {
    e.preventDefault();
    var anchor = $(this);
    var parent = $(this).parents('.quickOrder');
    var form = parent.find('.quickOrderForm');
    if (form.hasClass('visuallyhidden')) { 
    	parent.addClass("active"); 
    	anchor.attr("style","display: inline-block; width: 180px;");
    	anchor.css("cursor","default");
    	form.removeClass('visuallyhidden'); 
    	$("#qop").removeClass('visuallyhidden');
    }
	});
  setTimeoutCookie('sessionTimeLimit', 11+59/60);
});


$('input.mockPassword').each(function() {
  $(this).focus( function() {
    realPassword = $(this).parent().find('input.realPassword');
    $(this).hide();
    realPassword.show();
    realPassword.focus();
  });
});

$('input.realPassword').each(function() {
  $(this).blur( function() {
    if ($(this).val() == '') {
      mockPassword = $(this).parent().find('input.mockPassword');
      $(this).hide();
      mockPassword.show();
    }
  });
});

function validateSearchForm(element) {
  
  var $SearchButton = $('button[type="submit"]#goButton',element);
  if(validateSearch()){
      var st = ($.trim($('#siteSearchInput', element).val()));
	  $SearchButton.attr('disabled','disabled');
	  if (st.match(/page\s*\d+\s*$/i)) {
	    document.searchForm.action = '/FlyerView';
	    $('#pageLabelInputHidden', element).val($.trim(st.substring(4)));
	    $('#contentPathInputHidden', element).val('/sales-catalogs/big-book');
	  } else {
	    $('#siteSearchInputHidden', element).val(st);
	    $('#pageLabelInputHidden', element).attr("disabled", "disabled");
	    $('#contentPathInputHidden', element).attr("disabled", "disabled");
	  } 
	  updateSearchTermCookie(st,false);  
  }else{
	$SearchButton.removeAttr('disabled');
  	return false;		
  }
}

$('#virtualBigBookLink').click(function() {
  s.eVar45=s.prop52='big-book'; 
  analyticsOnClickEvent(this, 'event20', 'Virtual Big Book', 'events,eVar45,prop52');
});

function setTimeoutCookie(name, hours) {
    var date = new Date();
    date.setTime(date.getTime()+(hours*60*60*1000));
    var expires = "; expires="+date.toGMTString();
    document.cookie = name+"="+date.toGMTString()+expires+"; path=/";
}

function validateSearch(){
	
	var textBox = $('#siteSearchInput'),
   		textBoxVal = $.trim(textBox.val());
   	if (textBoxVal == '' || textBoxVal == 'By Keyword, Part #, Mfr #, Competitor part #, Customer part #, MSDS'){
   		alert('Please enter a search value');
   		textBox.focus();
   		return false;
   	 }
   	return true;
}
   
$("#addLineButton").click(function() {
	var count = $("#addLineCount").val();
	count = parseInt(count) + 1;
	$("#addLineCount").val(count);
	$("#addLineButton").attr("disabled","disabled");
	var requestData = "addLineCount="+count;
	//QB#2434 - build the line item - STARTS
	var formDIv = addMoreLineToQOP(count);
    $("#addLine").append(formDIv);
    
    //get all the new lines added. For the last new line added attach typeAhead event.
    var $this = $('#HeaderQuickOrder div#addLine .quickOrderItem');
	var len;
	if(null != $this){
		len = $this.length;
	}
	if(len >= 1){
		attachQuickOrderTypeAhead($this[len-1]);
	}
	//QB#2434 ENDS
	
    $("#addLineButton").removeAttr("disabled");
    $('#userEnteredPartnumber'+count).watermark("watermark", $('#userEnteredPartnumber'+count).attr('title'));
    $('#quantity'+count).watermark("watermark", $('#quantity'+count).attr('title'));
    if(count >= 5){
		$("#addLineButton").attr("disabled","disabled");
		$("#addLineButton").removeClass("buttonRed");
		$("#addLineButton").addClass("buttonGray");
	}
}); 

/* QB#2434 - This function creats a new line item for QOP */
function addMoreLineToQOP(lineNum){
	var formDiv = "";
	formDiv = '<input type="hidden" name="pattrName_'+lineNum+'" value="orderSource" />'+
				'<input type="hidden" name="pattrValue_'+lineNum+'" value="MO" />'+
				'<div class="spot quickOrderItem">'+
					'<label for="userEnteredPartnumber'+lineNum+'" class="visuallyhidden">Part number</label> '+
					'<input type="text" id="userEnteredPartnumber'+lineNum+'" name="userEnteredPartnumber_'+lineNum+'" class="watermarked itemNumber part" title="Part #" autocomplete="off" /> '+
					'<label for="quantity'+lineNum+'" class="visuallyhidden">Quantity</label> '+
					'<input type="text" id="quantity'+lineNum+'" name="quantity_'+lineNum+'" class="watermarked qty" maxlength="5" title="Qty" autocomplete="off" onkeypress="javascript:return isNumberKey(event);" />'+
					'<div class="typeAheadDisplay quickOrderTypeAheadDisplay"></div>' +
				'</div>';
	return formDiv;
}

$("#qop").click(function(){
	var parent = $(this).parents('.quickOrder');
    var form = parent.find('.quickOrderForm');
	$("#HeaderQuickOrder").addClass("shadow");
	$("#HeaderQuickOrder").removeClass("active");
   	form.addClass('visuallyhidden'); 
   	$("#qop").addClass('visuallyhidden');
   	$("#quickOrderLink").css("display","block");
   	$("#quickOrderLink").css("width","auto");
   	$("#quickOrderLink").css("cursor","pointer");
});

function showMiniShoppingCart(pItemId,quantity){
	var miniCartUrl = '/' + 'MiniCartContentsAPIView?storeId=10054&catalogId=10001&langId=-1' + '&MSCPartNumber='+pItemId.toString() + '&quantity='+quantity;
	 $.ajax({
       url: miniCartUrl,
       type: 'POST',
       success: function(data){
       	 MSCJS.miniCartShowApi(data);
         	 $("#numItemsInCart").html('<img src="//cdn.mscdirect.com/global/application-content/images/global/cartIcon.png" class="left"> &nbsp; ' + $("#miniCartNumItems").html());
            if($("#miniCartNumItems").html()== 1){
				$("#cartItemMessage").html("item");
		 }else{
				$("#cartItemMessage").html("items");
		 }
         window.location.hash="doc";
   	   }  
    });
}

var webParam_BasePartNum = '';
var webParam_RefNum = '';
var webParam_reqPartNum = '';
var webParam_PartSource = '';
var webParam_Quantity = '';
var webParam_CTLlength = '';
var webParam_CTLpieces = '';
var webParam_OrderAs = '';

var listHdr_Title =	'';
var listHdr_Notes =	'';
var listHdr_ShareFlag =	'';
var listHdr_Type =	'';
var listHdr_Id = '';
var listTypeVal = '';

var listformat = '';
var formatJSON = '';
var listInputPartListData = '';
var listInputFormat = '';

var validRow = [];
var qtyErr=false;
var parametersLOPCartAPI ;

$("#cartAPIIdRPH").click(function(event){
 	event.preventDefault();
	var userEntered = false, maxValLength = 0 ;
	for(var i=0; i< $QOPCartAPI.length; i++){
		var $thisVal = $.trim($QOPCartAPI[i].$inputElement.val()); 
		if ($thisVal && $thisVal!='Part #'){
			userEntered = true;
			if($thisVal.length>maxValLength){
				maxValLength=$thisVal.length;
			}
		}
	}
	if(userEntered){
		$("#QuickOrderPadAddItemsErrorDiv").addClass("hidden");
		if(maxValLength<2){
			loadLargeOrderPad();
			return;
		}
		$(this).attr('disabled','disabled');
		if(addToCartQOPCartAPI()){
			proceedAddToCartQOPCartAPI();
		}else{
			$('img#loadQOPCartAPI',$cartAPIIdQOPForm).show();
			$('div.quickOrderForm',$cartAPIIdQOPForm).css('padding-bottom','1px');
			var timeoutQOPCartAPI = setTimeout('loadLargeOrderPad()',60000);
			setInterval("handleHoverAfterClick()", 5000);
			$cartAPIIdQOPForm.bind('custom',function(){
				clearTimeout(timeoutQOPCartAPI);
				$cartAPIIdQOPForm.unbind('custom');
				$('img#loadQOPCartAPI',$cartAPIIdQOPForm).hide();
				$('div.quickOrderForm',$cartAPIIdQOPForm).css('padding-bottom','15px');
				proceedAddToCartQOPCartAPI();
			});
		}
	}else{
		$("#QuickOrderPadAddItemsErrorDiv").removeClass("hidden");
	}
});

 function proceedAddToCartQOPCartAPI(){
 	$("#QuickOrderPadAddItemsErrorDiv, #QuickOrderPadAddItemsQtyErrorDiv").addClass("hidden");
	var count = $("#addLineCount").val();
	var $userEnteredPartnumber;
	var $quan;
	var itemUrl = "";

	var jsonObject = "{\"InputPartListJson\":{"+
		"\"InputPartsHeader\":\{"+
				"\""+ listHdr_Id +"\":\"\","+
				"\""+ listHdr_Type +"\":\""+ listTypeVal +"\""+
		"},"+
		"\"InputParts\":{"+
		
			"\"InputPart\":[";     
		
	for(var i = 1; i <= count; i++){
		$userEnteredPartnumberVal = $.trim($("#userEnteredPartnumber"+i).val());
		$quan = $("#quantity"+i);
		hiddenPVal = $("#partNumber_"+i).val();
		hiddenOVal = $("#ordAs_"+i).val();
		if($userEnteredPartnumberVal && $userEnteredPartnumberVal.toUpperCase()!=hiddenPVal.toUpperCase() && $userEnteredPartnumberVal.toUpperCase()!=hiddenOVal.toUpperCase()){
			continue;
		}
		if ($userEnteredPartnumberVal != $("#userEnteredPartnumber"+i).attr("title") && $userEnteredPartnumberVal != ''){
			itemUrl = itemUrl + "basePartNumber=" +hiddenPVal+"&";

			jsonObject +="{"+
						"\""+ webParam_BasePartNum +"\":\""+hiddenPVal+"\","+
						"\"refnum\":\""+ $("#refNo_"+i).val() +"\",";
		
			var isUPCItem = $("#userEnteredPartnumber"+i).parent('div.spot.quickOrderItem').data('isUPC');
			if(hiddenOVal != null && hiddenOVal!='' &&  !(isUPCItem && isUPCItem==hiddenOVal)){
				jsonObject += "\""+ webParam_OrderAs +"\":\""+hiddenOVal+"\",";
			}
			if ($("#quantity"+i).val() =='' || $("#quantity"+i).val()== $quan.attr("title")){
				$("#quantity"+i).attr("value","1");	
			}
			if($quan.val() != $quan.attr("title")){
				itemUrl = itemUrl + "qty=" +$quan.val()+"&partSource=SR&";
				jsonObject += "\""+ webParam_Quantity +"\":\""+$quan.val()+"\",";
			}else{
				itemUrl = itemUrl + "qty=" +1+"&partSource=SR&";
				jsonObject += "\""+ webParam_Quantity +"\":\"1\",";
			}
			
			var ctlFlag = $("#ctlFlag_"+i).val();
			
			if (ctlFlag != null && ctlFlag != '' && ctlFlag == "true"){
				jsonObject += "\""+ webParam_CTLlength +"\":\"1\",";
			}
			
			jsonObject += "\""+ webParam_PartSource +"\":\"MO\""+
					"},";
		}
	}

	jsonObject = jsonObject.replace(/,$/,'');
	jsonObject +="]"+
			"}"+
		"}}";
	
//	alert(jsonObject);

	itemUrl = itemUrl.replace(/&$/,'');
	if(itemUrl){
		var url = listServiceURL + "/add?" + listInputFormat + "=" + formatJSON + "&" + listformat + "=" + formatJSON + "&" + listInputPartListData + "="+jsonObject;
		
		$.ajax({
		  url: url,
		  type: 'POST',
		  data: jsonObject,
		  dataType: 'json',
		  beforeSend: setHeader, 
		  success: function(obj){
		 	 if(parametersLOPCartAPI){
			 	 window.location.href = parametersLOPCartAPI;
			 }else{
			  	var quantity = obj.PartListResults.partLists[0].numberOfItems;
		  		window.location.href='http://www.mscdirect.com/webapp/wcs/stores/servlet/' + 'ShoppingCartAPIView?storeId=10054&catalogId=10001&langId=-1';
			 }
		 }
		});
	}else{
		if(parametersLOPCartAPI){
			  window.location.href = parametersLOPCartAPI;
		}
	}
}
	
function loadLargeOrderPad(){
	var count = $("#addLineCount").val();
	$("#QuickOrderPadAddItemsErrorDiv, #QuickOrderPadAddItemsQtyErrorDiv").addClass("hidden");
	var urlParameters= "";
	var totalCount=0;
	for(var i = 1; i <= count; i++){
		var $quantity = $('input[name="quantity_'+i+'"]',$(this));
		var enteredPartnumber = $('input[name="userEnteredPartnumber_'+i+'"]').val();
		
		var enteredQuantity = $('input[name="quantity_'+i+'"]').val();
		if(enteredPartnumber != 'Part #'){
			enteredPartnumber=enteredPartnumber.replace(" ",""); 
			totalCount++;
			validateQty($quantity,i);
			if(enteredQuantity == 'Qty'){
				enteredQuantity = '1';
			}
			if(urlParameters == ""){
				urlParameters="pNum_"+i+"="+encodeURIComponent(enteredPartnumber)+"+"+encodeURIComponent(enteredQuantity);
			}else{
				urlParameters=urlParameters+"&pNum_"+i+"="+encodeURIComponent(enteredPartnumber)+"+"+encodeURIComponent(enteredQuantity);
			}
		}
	}
	if(urlParameters == ""){
		urlParameters="totalCount="+count;
	}else{
		urlParameters=urlParameters+"&totalCount="+totalCount;
	}
	if((document.getElementById("isPunchOutView").value )== 'true'){
			window.location.href='/MSCPunchOutLargeOrderPadView?'+urlParameters;
	}else{
			window.location.href='/LargeOrderPadView?'+urlParameters;
	}
}

function handleEnter(inField, e) {
    var charCode;
    
    if(e && e.which){
        charCode = e.which;
    }else if(window.event){
        e = window.event;
        charCode = e.keyCode;
    }

    if(charCode == 13) {
    	
        validateQOPField();
    }
}

function validateQOPField(){
		var count = $("#addLineCount").val();
		for(var i = 1; i <= count; i++){
			validRow[i] = false;
		}
		$("#QuickOrderPadAddItemsErrorDiv, #QuickOrderPadAddItemsQtyErrorDiv").addClass("hidden");
		for(var i = 1; i <= count; i++){
			var $userEnteredPartnumber = $('input[name="userEnteredPartnumber_'+i+'"]',$(this));
			var $quantity = $('input[name="quantity_'+i+'"]',$(this));
			validateRowQOP($userEnteredPartnumber, $quantity,i);
		}
		rowValid = validRow[1];
		if (rowValid ){
			 $("#cartAPIIdRPH").click();
			 return true;
		}else{
			if (qtyErr){
				$("#QuickOrderPadAddItemsQtyErrorDiv").removeClass("hidden");
			}else{
				$("#QuickOrderPadAddItemsErrorDiv").removeClass("hidden");
			}
			 return false;
		}	 
}

function validateRowQOP($userEnteredPartnumber, $quantity, rowNo){
	var partNumber = $("#userEnteredPartnumber"+rowNo).val();
	if($("#userEnteredPartnumber"+rowNo).val() == $userEnteredPartnumber.attr("title") && $("#quantity"+rowNo).val() == $quantity.attr("title")){
		validRow[rowNo] = true;
	} else if($.trim($("#userEnteredPartnumber"+rowNo).val())!= "" && $("#userEnteredPartnumber"+rowNo).val() != $userEnteredPartnumber.attr("title")){
		validateQtyQOP($quantity, rowNo);
	} else {	
		qtyErr = true;
		validRow[rowNo] = false;
	}
}

function validateQtyQOP($quantity, rowNo){
	var qtyPtrn = /^[1-9][0-9]*$/;
	var qtyVal = $("#quantity"+rowNo).val();

	if(qtyPtrn.test(qtyVal)) {
		validRow[rowNo] = true;
	}else if ($("#quantity"+rowNo).val() == "Qty"){
		if($.trim($("#userEnteredPartnumber"+rowNo).val())!= "" && $("#userEnteredPartnumber"+rowNo).val() != "Part #"){
			if ($("#quantity"+rowNo).val() ==''){
				$("#quantity"+rowNo).attr("value","1");	
			}
		}
		validRow[rowNo] = true;
	}else{	
		qtyErr = true;
		validRow[rowNo] = false;
	}
}
function isNumberKeyQOP(element,evt){
	var charCode = (evt.which) ? evt.which : evt.keyCode;
	var submitQOP = false;
	if (evt.ctrlKey) {		
		return true;
	}
	if ((charCode > 31 && charCode <= 45) || charCode > 57 || charCode== 13){
		
		var qty = $("#"+element.id).val();
		if (qty == ''){
			$("#QuickOrderPadAddItemsQtyErrorDiv").removeClass("hidden");
		}else{
			 $("#cartAPIIdRPH").click();
			 return true;
		}
		return false;
	}
	return true;
}

function callApiToAddListjsonToCart(jsonObject){
	var url = listServiceURL + "/add?" + listInputFormat + "=" + formatJSON + "&" + listformat + "=" + formatJSON + "&" + listInputPartListData + "="+jsonObject;
	
	$.ajax({
	  cache: false,
	  url: url,
	  type: 'POST',
	  beforeSend: setHeader, 
	  success: function(obj){
	  	window.location.href='ShoppingCartAPIView?storeId=10054&catalogId=10001&langId=-1';
	  },
	  error: function(){
	  }
	});

}

function addItemCartAPI(pItemId,quantity,pxno,orderAs,pdLength,ctlpieces,reqPartNum,source){
	var ctlLength = "";
	if ($(pdLength).val() == undefined){
		ctlLength = "";
	}else{
		ctlLength = $(pdLength).val();
	}
	  var url = listServiceURL + "/add?" + listHdr_Type + "=ACTIVE_CART&" + webParam_BasePartNum + "=" + pItemId + "&" + webParam_RefNum + "=" + pxno + "&" + webParam_reqPartNum + "=" + reqPartNum + "&" + webParam_Quantity + "=" + $(quantity).val() + "&" + webParam_PartSource + "=" + source + "&" + webParam_OrderAs + "=" + orderAs + "&" + webParam_CTLlength + "=" + ctlLength + "&" + webParam_CTLpieces + "=" +  ctlpieces + "&" + listformat + "=" + formatJSON;
		
	  $.ajax({
        url: url,
        type: 'POST',
        dataType: 'json',
        beforeSend: setHeader, 
        success: function(data){
        	var length = 0;
        	var quantity = 0;
        	/*if (data.PartListResults.partLists){
        		 length = data.PartListResults.partLists.length;
        	}*/
        	
        	//for (var i=0; i<length; i++){
	        	quantity = data.PartListResults.partLists[0].numberOfItems;
	       // }	
	       
       		showMiniShoppingCart(pItemId,quantity);
      }  
      });
}


	function updateSearchTermCookie(searchTerm,typeAhead){
		if('undefined'!=searchTerm && 'null'!=searchTerm){
			if(typeAhead && typeAhead=='true'){
				var regNumber = new RegExp("^[-]?[0-9]+[\.]?[0-9]+$");	
				var regEndsWithNumber = /[0-9]$/;		
				if(regNumber.test(searchTerm) || regEndsWithNumber.test(searchTerm)){
					document.cookie = "SearchTerm=;expires="+new Date(0).toUTCString()+";path=/";
					//QB#4552- new cookie "SearchTermEncoded" will hold the encoded value of the search term
					document.cookie = "SearchTermEncoded=;expires="+new Date(0).toUTCString()+";path=/";
					return;
				}
			}
			var searchTermCookieVal = getCookieValue('SearchTerm');
				if(searchTerm==searchTermCookieVal){
					return;
				}
			document.cookie = "SearchTerm="+searchTerm+";path=/";
			//QB#4552- new cookie "SearchTermEncoded" will hold the encoded value of the search term
			document.cookie = "SearchTermEncoded="+encodeURIComponent(searchTerm)+";path=/";
		}
	}
</script>
