function analyticsOnClickEvent(element, eventName, linkName, linkTrackVars) {
  s.linkTrackVars=linkTrackVars; //'prop1,prop2,events';
  s.linkTrackEvents=eventName; //'event1';
  s.events=eventName; //'event1';
  s.tl(element,'o',linkName);
  analyticsClickWait();
}

function analyticsClickWait(){
  var date = new Date();
  var curDate = null;
  do { curDate = new Date(); } 
  while(curDate-date < 250); //waits 250 milliseconds
}

