function promoWindow(imageLocation,promoString){
	PopUp = window.open('/global/scripts/marketing/promo.html?id=' + imageLocation,'PopUp','width=440,height=600,top=40,left=40');
	PopUp.focus();

	// response
	if(autoApplyPromo(promoString) == "Y") {

		$("#header-slider-container").append('<div class="homepage-promo-applied" style="position: absolute; top: 155px; right: 0; font-weight: bold; color: green">Promo applied!</div>');
		$("#homepage-promo-bottom").append('<div class="homepage-promo-applied" style="padding-top: 5px; text-align: right; font-weight: bold; color: green">Promo Applied!</div>')

	}
}

</script>

