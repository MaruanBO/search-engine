<script type="text/javascript">
      function SetDonationLevels(widget) {	
          var alsoRemoveOther = false;
          widget.removeDonationLevels(alsoRemoveOther);
		  
		  globalVar_myHTML = '<div class="donationSet">';
		  
		  var askArray = [25, 50, 100, 250, 500, 750, 1000];
		  
          for (var i = 0; i < askArray.length; i++){
              var ask = askArray[i];
              if(ask === undefined){
                  continue;
              }
			 
			globalVar_myHTML =globalVar_myHTML + '<a class="donateTab">$' + ask + '</a>';
          }
		  
		globalVar_myHTML =globalVar_myHTML + '<input id="donationOther" class="k_text" value="" placeholder="Other Amount">';
    	globalVar_myHTML =globalVar_myHTML + '</div>';
      }
</script>
 


