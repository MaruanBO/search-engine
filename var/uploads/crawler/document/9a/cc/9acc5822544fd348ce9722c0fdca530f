		function fbEnsureInit(callback) {
			if(!window.fbApiInit) {
				setTimeout(function() {
					fbEnsureInit(callback);
				}, 50);
			} else {
				if(callback) {
					FB.getLoginStatus(function(response){
						callback();
					});
				}
			}
		}
