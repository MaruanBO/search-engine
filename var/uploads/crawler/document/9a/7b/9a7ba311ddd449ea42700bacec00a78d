    <script>/*globals window, jQuery, YT, revapi1 */
if (jQuery) {
    var $ = jQuery;
}
var NGX = {
    $navigation : $('.x-navbar'),
    headerScroll : function () {
        // React to scroll offset and add transparent class
        var offset = $(document).scrollTop(),
            obstacle = $('#x-content-band-1').outerHeight() - 80;
        if (offset === 0) {
            NGX.$navigation.addClass('transparent');
        }
        if (offset > obstacle) {
            NGX.$navigation.removeClass('transparent');
        } else {
            NGX.$navigation.addClass('transparent');
        }
    },
    showForm : function (e) {
        if (window.innerWidth > 979) {
            e.preventDefault();
            var $trigger = $(e.currentTarget),
                $form = e.data,
                offset = $trigger.position().top + $trigger.height() + 6;
            NGX.viewportResize($form);
            $form.css({top: offset});
            if (!$trigger.next().hasClass('widget')) {
                var $load = $form.find('.load-frame');
                $load.addClass('loading');
                $form.find('iframe').on('load', function () {
                    $load.removeClass('loading');
                });
                $('body').append($form);
            }
            $form.fadeToggle();
            $form.find('.close').on('click', function () {
                $form.fadeOut();
            });
        }
    },
    showFormAuto : function (e) {
        if (window.innerWidth > 979) {
            e.preventDefault();
            var $trigger = $(e.currentTarget),
                $form = $($trigger.data('form')),
                offset = $trigger.offset().top + $trigger.height() + 6;
            NGX.viewportResize($form);
            $form.css({top: offset});
            if (!$trigger.next().hasClass('widget')) {
                var $load = $form.find('.load-frame');
                $load.addClass('loading');
                $form.find('iframe').on('load', function () {
                    $load.removeClass('loading');
                });
                $('body').append($form);
            }
            $form.show();
            $form.find('.close').on('click', function () {
                $form.fadeOut();
            });
        }
    },
    showVideo : function (e) {
        e.preventDefault();
        if ($('body .overlay').length === 1) {
            $('body .overlay').fadeToggle();
        } else {
            var $overlay = $('<div class="overlay visible"><div class="overlay-inner"></div></div>'),
                videoSelector = $(e.currentTarget).data('video');
            $overlay.find('.overlay-inner').append($(videoSelector));
            $('body').append($overlay);
            var id = $('#overlay-player').data('id');
            $overlay.find('.close').on('click', function () {
                NGX.video.stop(NGX.video['player_' + id]);
                $overlay.fadeOut();
            });
            NGX.video.createPlayer('overlay-player', id);
            $overlay.fadeIn();
        }
    },
    viewportResize : function ($container) {
        var maxWidth = window.innerWidth,
            maxHeight = window.innerHeight,
            containerWidth = $container.width(),
            containerHeight = $container.height(),
            containerOffsetTop = $container.position().top,
            containerOffsetLeft = $container.position().left;
        if (containerWidth + containerOffsetLeft > maxWidth) {
            $container.width(maxWidth - 200);
        }
        if (containerHeight + containerOffsetTop > maxHeight) {
            $container.height(maxHeight - 200);
        }
    },
    hideMeta : function () {
        $('post').each(function () {
            var $time = $(this).find('time');
            $(this).find('.p-meta').hide();
            $(this).append($time);
        });
    },
    hoverParent : function () {
        $(this).parent('.menu-item-has-children').find('> a').toggleClass('hover');
    },
    video : {
        ready : false,
        player : {},
        createTag : function () {
            var tag = document.createElement('script'),
                firstScript = document.getElementsByTagName('script')[0];
            tag.src = '//www.youtube.com/iframe_api';
            firstScript.parentNode.insertBefore(tag, firstScript);
        },
        createPlayer : function (selector, id) {
            if (NGX.video.ready === true) {
                NGX.video['player_' + id] = new YT.Player(selector, {
                    videoId : id,
                    events : {
                        'onReady' : NGX.video.onReady
                    }
                });
            }
        },
        onReady : function (event) {
            if ((navigator.userAgent.match(/iPhone/)) || (navigator.userAgent.match(/iPod/)) || (navigator.userAgent.match(/iPad/))) {
                return false;
            }
            event.target.playVideo();
        },
        stop : function (player) {
            player.stopVideo();
        }
    },
    init : function () {
        // Prevent CSS Transitions  
        $('body').addClass('load');
        $(window).load(function () {
            $('body').removeClass('load');
            NGX.video.createTag();
        });
        // Do things if we are on the home page
        if ($('body').hasClass('home')) {
            $(document).on('scroll', this.headerScroll);
            $(document).ready(this.headerScroll);
        }
        this.hideMeta();
        // General handlers
        $('.get-demo-nav').on('click', $('.get-demo'), this.showForm);
        $('.get-download').on('click', $('.get-whitepaper'), this.showForm);
        $('.frame-form').on('click', this.showFormAuto);
        $('.watch').on('click', this.showVideo);
        $('.menu-item-has-children ul').on('mouseenter mouseleave', this.hoverParent);
    }
};
NGX.init();

// Player API
function onYouTubeIframeAPIReady() {
    NGX.video.ready = true;
}

// ‘responsive’ carousel stuff
$(document).ready(function () {
    if (typeof revapi1 === 'undefined') {} else {
        revapi1.bind('revolution.slide.onchange', function () {
            if (window.innerWidth < 700) {
                var $elevator = $('#rev_slider_1_1 .tp-caption').eq(2),
                    $talk = $('#rev_slider_1_1 .tp-caption').eq(1),
                    $whitePaper = $('#rev_slider_1_1 .tp-caption').eq(4),
                    left = $elevator.css('left'),
                    top = parseInt($elevator.css('top'), 10) + $talk.outerHeight() + 20;
                $elevator.css({
                    'top' : parseInt($elevator.css('top'), 10) + 20
                });
                $talk.css({
                    'top' : top,
                    'left' : left
                });
                $whitePaper.css({
                    'top' : parseInt($whitePaper.css('top'), 10) + 50
                });
            }
        });
    }
});</script>

  

  
    
