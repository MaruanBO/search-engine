    <!--	FIXAR HEADER	-->
    <script type="text/javascript">
        var onscroll = function () {
            var value = $(this).scrollTop();
            if (value > 40)
                $(".activeArrow").addClass('displayNone'),
                $("header").addClass('fixed'),
                $("header .middleSection").addClass('fixed_middleSection'),
                $("header .bottomSection").addClass('fixed_bottomSection'),
                $(".stampCorner").css('opacity', 0),
                $(".stampCorner_2").css('opacity', 1);
            else
                $(".activeArrow").removeClass('displayNone'),
                $("header").removeClass('fixed'),
                $("header .middleSection").removeClass('fixed_middleSection'),
                $("header .bottomSection").removeClass('fixed_bottomSection'),
                $(".stampCorner").css('opacity', 1),
                $(".stampCorner_2").css('opacity', 0);
        };
        $(document).scroll(onscroll);
        setTimeout(onscroll, 400);
    </script>
