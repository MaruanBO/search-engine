    <!--MOSTRAR COM PARALLAX A NAVEGAÇÃO DO SLIDER EM CASO DE ECRÃS MAIORES-->
    <script type="text/javascript">
        $(function () {
            $('#my-carousel-1 .pagination-links').attr('data-200', 'top: -160px;').attr('data-260', 'top: 0px');
            $('#my-carousel-1 .mask').attr('data-200', 'top: -160px;').attr('data-260', 'top: 0px');
            $('.prev, .next').attr('data-200', 'top: -110px;').attr('data-260', 'top: 50px');
            $("#revealPagination").click(function () {
                $("#my-carousel-1 .pagination-links").css("top", "0");
                $("#my-carousel-1 .prev,#my-carousel-1  .next").css("top", "50px");
            });
        })
    </script>

