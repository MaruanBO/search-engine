<script type="text/javascript">
$('img.lazy').lazyload({ failure_limit : 40, threshold : 100 });

$(document).ready(function() {

	/* -- Menu Hover Effect */
	var mainmenu=$('#main-menu li');
	mainmenu.hover(
		function(){
			if(allowdrop == 1) {
				var idx=$(this).index();
				switch(idx){
					case 0: $(this, ':eq(0)').addClass('hover');$('#style-menu').show(); break;
					case 1: $(this, ':eq(1)').addClass('hover');$('#eye-menu').show(); break;
					case 2: $(this, ':eq(2)').addClass('hover');$('#beauty-menu').show(); break; 
					case 3: $(this, ':eq(3)').addClass('hover');$('#shop-menu').show(); break;
				}
			}
		},
		function(){
			if(allowdrop == 1) {
				$(this).removeClass('hover');
				$('#style-menu, #eye-menu, #shop-menu, #beauty-menu').hover(function(){$(this).show();},function(){$(this).hide();});
				$('#style-menu, #eye-menu, #shop-menu, #beauty-menu').hide();
			}
		}
	);
	$('#style-menu').hover(function(){ mainmenu.eq(0).addClass('hover'); },function(){ mainmenu.eq(0).removeClass('hover'); });
	$('#eye-menu').hover(function(){ mainmenu.eq(1).addClass('hover'); },function(){ mainmenu.eq(1).removeClass('hover'); });
	$('#beauty-menu').hover(function(){ mainmenu.eq(2).addClass('hover'); },function(){ mainmenu.eq(2).removeClass('hover'); });
	$('#shop-menu').hover(function(){ mainmenu.eq(3).addClass('hover'); },function(){ mainmenu.eq(3).removeClass('hover'); });
	/* Menu Hover Effect -- */

	
	var aday=Math.floor(Date.now()/1000);
	$('.timeago').each(function(index){if($(this).attr('data-timestamp')>aday-86400){$(this).timeago();}});

})

</script>




