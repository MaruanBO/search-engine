<script>
var allowdrop = 1;
$('#m-menus-toggle').click(function(e){
			e.preventDefault();
			allowdrop = 0;
			$( "nav" ).toggle();
});
				
$(window).resize(function() {
	
	if($(window).width() > 640) {
		allowdrop = 1;
		$( "nav" ).show();
	} else {
		allowdrop = 0;
		$( "nav" ).hide();
	}
	console.debug('allowdrop  : ' + allowdrop );
});
</script>
