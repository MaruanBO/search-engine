		<script>
		function submitenter(myfield,e) {
			var keycode;
			var valueFromPropertyFile = "search by keyword, page code or item #";
			if (window.event)
			  { 	  	
				keycode = window.event.keyCode;
			  }
			else if (e)
				{
					keycode = e.which;
			  	}
			else {
				return true;
			}	
		 
			
			if (keycode == 13 && myfield.value.length > 1 && (jQuery.trim(myfield.value) !=(jQuery.trim(valueFromPropertyFile)))) 
			{
					if ($.trim(myfield.value).length === 0){
		   					alert('Please enter a valid search term');
							return false;
					}else
						{
								document.getElementById("SimpleSearchForm_SearchTerm").value = jQuery.trim(myfield.value.toLowerCase());
								if (jQuery.trim(myfield.value) != '')
								{
								myfield.form.submit();
								return false;
								}
								else{
									 myfield.value = document.getElementById("searchTextHolder").innerHTML;	
									 return true;
								}
						}
				
			}else if(keycode == 13 && myfield.value.length == 0){
				alert('Please enter a valid search term');
				return false;
			}
			else{
			   return true;
			   }
		}
		
		/* ***********************************
		* Catalog Search Form Submit page
		*/
		function CatalogSearchFormSubmit( myfield,e) {
			var searchTermValue = document.getElementById("SimpleSearchForm_SearchTerm").value;
			document.getElementById("SimpleSearchForm_SearchTerm").value = jQuery.trim(searchTermValue.toLowerCase());
			var valueFromPropertyFile = "search by keyword, page code or item #";
			if (jQuery.trim(searchTermValue) != '' && (jQuery.trim(searchTermValue).length != 0) && (jQuery.trim(searchTermValue) !=(jQuery.trim(valueFromPropertyFile))))
			{
				 document.CatalogSearchForm.submit();
				return true;
			}else{
				document.getElementById("SimpleSearchForm_SearchTerm").value = document.getElementById("searchTextHolder").innerHTML;
				alert('Please enter a valid search term');
				e.preventDefault ? e.preventDefault() : e.returnValue = false;
				return false;		
			}
		} 
		</script>
		
