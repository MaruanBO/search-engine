var validateaccountvar = false;
var validateaccount = function(theForm) {
	var theForm = document.getElementById(theForm);
	if(validateaccountvar) {
		alert('Form is being proceesed.  Please wait.');
		return false;
	}
	validateaccountvar = true;
	var isError = false;

	var e = new convioObject();
	e.setCallbackHandler(returnvalidateaccount);
	e.setErrorHandler(validateaccountErrorHandler);
	e.setForm(theForm);
	e.setHTTPMethod('POST');
	e.validateaccountCF();

}
var validateaccountErrorHandler = function(statusCode, statusMsg)
{
	validateaccountvar = false;
	alert('Status: ' + statusCode + ', ' + statusMsg);
}
var returnvalidateaccount = function(results) {

	var theForm = document.getElementById(results.THEFORM);
	validateaccountvar = false;

	if (results.ERROR == 1)
	   {	alert(results.MESSAGE);

	  }else
        {
            theForm.submit();
        }

}


var changePasswordvar = false;
var changePassword = function(theForm) {
	var theForm = document.getElementById(theForm);
	
	changePasswordvar = true;
	var isError = false;

	var e = new convioObject();
	e.setCallbackHandler(returnchangePassword);
	e.setErrorHandler(changePasswordErrorHandler);
	e.setForm(theForm);
	e.setHTTPMethod('POST');
	e.changePasswordCF();

}
var changePasswordErrorHandler = function(statusCode, statusMsg)
{
	changePasswordvar = false;
	alert('Status: ' + statusCode + ', ' + statusMsg);
}

var returnchangePassword = function(results) {

	var theForm = document.getElementById(results.THEFORM);
	changePasswordvar = false;
	if (results.ERROR == 1) alert(results.MESSAGE);

}

var subscribevar = false;
var subscribe = function(theForm) {
	var theForm = document.getElementById(theForm);
	
	subscribevar = true;
	var isError = false;

	var e = new convioObject();
	e.setCallbackHandler(returnsubscribe);
	e.setErrorHandler(subscribeErrorHandler);
	e.setForm(theForm);
	e.setHTTPMethod('POST');
	e.subscribeCF();

}
var subscribeErrorHandler = function(statusCode, statusMsg)
{
	subscribevar = false;
	alert('Status: ' + statusCode + ', ' + statusMsg);
}

var returnsubscribe = function(results) {

	subscribevar = false;
    if (results.ERROR == 1)

	alert(results.MESSAGE);




}



var subscribewcaivar = false;
var subscribewcai = function(theForm) {
	var theForm = document.getElementById(theForm);
	
	subscribewcaivar = true;
	var isError = false;

	var e = new convioObject();
	e.setCallbackHandler(returnsubscribewcai);
	e.setErrorHandler(subscribewcaiErrorHandler);
	e.setForm(theForm);
	e.setHTTPMethod('POST');
	e.subscribewcaiCF();

}
var subscribewcaiErrorHandler = function(statusCode, statusMsg)
{
	subscribewcaivar = false;
	alert('Status: ' + statusCode + ', ' + statusMsg);
}

var returnsubscribewcai = function(results) {

	subscribewcaivar = false;
    if (results.ERROR == 1)

	alert(results.MESSAGE);




}

</script>
<script type='text/javascript'>
  // this function determines whether the event is the equivalent of the microsoft
  // mouseleave or mouseenter events.
  function isMouseLeaveOrEnter(e, handler)
  {
  var reltg = e.relatedTarget ? e.relatedTarget : e.type == 'mouseout' ? e.toElement : e.fromElement;
  while (reltg && reltg != handler) reltg = reltg.parentNode;
  return (reltg != handler);
			}
</script>

<script type='text/javascript'>
function changeBox(divObj1,divObj2,pwObj)
{
  document.getElementById(divObj1).style.display='none';
  document.getElementById(divObj2).style.display='';
  document.getElementById(pwObj).focus();
}
function restoreBox(divObj1,divObj2,pwObj)
{
	if(document.getElementById(pwObj).value=='')
	{
	  document.getElementById(divObj1).style.display='';
	  document.getElementById(divObj2).style.display='none';
	}
}
</script>





