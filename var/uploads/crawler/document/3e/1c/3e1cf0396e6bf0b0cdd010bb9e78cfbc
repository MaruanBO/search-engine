function p2l(pathname,query,hash){
  pathname = pathname.replace(/\//gi,"__")
  if(pathname === "__") {
    pathname = "homepage";
  }
  pathname = pathname.replace(/\./gi,"_");
  query = query.replace(/\?/gi,'-');
  params = query.split('&');
  args = []
  for(arg = 0; arg < params.length; arg++){
    args.push(params[arg].split('=')[0]);
  }
  pathname += args.join('-');
  pathname += hash.replace(/#/gi,"-");
  return pathname;
};

