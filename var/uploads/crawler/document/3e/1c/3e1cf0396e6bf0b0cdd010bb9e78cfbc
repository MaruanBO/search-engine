		<!-- quick fix -->

		<script type="text/javascript">

		// update Dec 18
        function maindds(blockID) {
        if (document.getElementById(blockID).style.display=='block') {
		        hideClass('dropdown');
				jQuery('#headerNew .topNav a').removeClass('active');
		        document.getElementById(blockID).style.backgroundColor='#fff;';

        } else {
		        hideClass('dropdown');
				jQuery('#headerNew .topNav a').removeClass('active');
				document.getElementById(blockID + 'A').className='active';
        		document.getElementById(blockID).style.display='block';
        }
        }
		jQuery(document).ready(function() {
			var isTouch =  'ontouchstart' in window || navigator.msMaxTouchPoints;

			if( !isTouch ){
				jQuery('#tempStyle').html("<style>#headerNew .topNav a:hover {background: url(/imagesv2/nav-arrow-on.gif) 100% 15px no-repeat #5174b4;color: #fff;}</style>");
			}
		});
		function hideDropdown() {
			hideClass('dropdown');
			jQuery('#headerNew .topNav a').removeClass('active');
		}
		// update Dec 18
        </script>
