	<div id="featured-authors" class="widget">
		    <script type='text/javascript'>
      currentAuthorsPage = 1;
      function moreAuthors() {
        currentAuthorsPage ++;
        if(currentAuthorsPage > 3)
         currentAuthorsPage = 1;
         showAuthorsPage();
      }
      
      function showAuthorsPage() {
        $('#authors_list_1').css('display','none');
        $('#authors_list_2').css('display','none');
        $('#authors_list_3').css('display','none');
        
        $('#authors_list_'+currentAuthorsPage).css('display','');
      }
    </script>

