			<!--GA Event Tracking-->
	<script type="text/javascript">
			//Pinterest
			$('.pinterest-ga-track').click(function(){
				_gaq.push(['_trackEvent', 'Pin', 'HP_Click', 'Click on Pinterest',null, true]);
			}); 	
			//Comments
			$('.comment-ga-track a').click(function(){
				_gaq.push(['_trackEvent', 'Comment', 'HP_Click', 'Click on Comment', null, true]);
			});
			//Facebook Share
			$('.facebookshare-ga-track a').click(function(){
				_gaq.push(['_trackEvent', 'FB_Share', 'HP_Click', 'Click on Facebook Share', null, true]);
			});
			//Email 
			$('.email-ga-track a').click(function(){
				_gaq.push(['_trackEvent', 'Email', 'HP_Click', 'Click on Email', null, true]);
			});
			//Twitter
			$('.twitter-ga-track a').click(function(){
				_gaq.push(['_trackEvent', 'Twitter', 'HP_Click', 'Click on Twitter', null, true]);
			});

	</script>
	
	
		<!--End GA Tracking-->

