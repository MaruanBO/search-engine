<script type="text/javascript">// <![CDATA[

function getMetaValue(meta_name) {
  var my_arr=document.getElementsByTagName("META");
  for (var counter=0; counter<my_arr.length; counter++) {
    if (my_arr[counter].name.toLowerCase() == meta_name.toLowerCase()) {
      return my_arr[counter];
      }
  }
  return "N/A";
}

function hashCode(s){
  return s.split("").reduce(function(a,b){a=((a<<5)-a)+b.charCodeAt(0);return a&a},0);              
}

function getParameterByName(name) {
    var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
    return match ?
        decodeURIComponent(match[1].replace(/\+/g, ' '))
        : null;
}

var GA_category = [];
var GA_section = '';
var GA_tags = [];
var GA_debug_key = getParameterByName('GA_debug_key');

try{
  tag = getMetaValue('keywords');
  urlCode = "u"+Math.abs(hashCode(window.location.pathname.toLowerCase()))


  if(tag){
    var tag_list = tag['content'];
  } else {
    var tag_list = '';
  }
  
  if(document.location.href.match(/posts/)){ 
    GA_section = 'blogposts';
    array = tag_list.split(',');

    for (var i = 0; i < array.length; i++) {
      t = array[i];
      GA_tags.push( t.toLowerCase().trim() );
     }
  } else if ( document.location.href.match(/^http(?:s)?\:\/\/www.curbly.com\/(\?.*)?$/) ) {
    GA_section = 'homepage';
  }
} catch(err){
  var tag_list = 'other';
}


