googletag.defineSlot('/1015074/Billboard', [[970, 60], [970, 250], [1,1]], 'div-gpt-ad-1392309948904-0').addService(googletag.pubads());


googletag.pubads().setTargeting("tags",GA_tags);
googletag.pubads().setTargeting("section",GA_section);
googletag.pubads().setTargeting("category",GA_category);
googletag.pubads().setTargeting("debug_key",GA_debug_key);
googletag.pubads().setTargeting("url",urlCode);

googletag.pubads().enableSingleRequest();
googletag.pubads().collapseEmptyDivs();
googletag.enableServices();
});

});



</script> 
