     function drawPosts() {

         var strRet = "";

         for (i = 0; i < 50; i++) {

             if (AllArray[i].attachmenturl != "") {
                 var strImage = AllArray[i].attachmenturl;

                 var newstrImage = strImage.replace("reviews", "rcdn");
             }

             strRet += "<div class=\"line\"><div class=\"review-index-margin\"><div class=\"index-box floatleft\"><a href='" + AllArray[i].posturl + "'><img width=\"160\" border=\"0\" align=\"right\" src=" + newstrImage + "></a></div><div class=\"article-blurb\"><h4><a href='" + AllArray[i].posturl + "'>" + AllArray[i].title + '</a></h4><h5>' + AllArray[i].poster + ' <span class=\"timestamp\">' + AllArray[i].postdate + '</span></h5><p class=\"news-margin\">' + AllArray[i].msg + '&nbsp;&nbsp;&nbsp;<a href="' + AllArray[i].posturl + '">' + '<span class=\'span-read-more\'>Read More &raquo;</span></a></p></div></div></div>';

         }

         return strRet;
     }

     $(document).ready(function () {
         var h = $('#article-index div.review-index-margin p.news-margin').height();
         $('.review-index-single div.line, .widget-container').css("height", h);
     });

</script>
