var tVMU = VM.Utils.getCookie("_VMU"); //User Id
var tVMI = VM.Utils.getCookie("_VMI"); //Interaction Id

jQuery(document).ready(function(){
    
    VM.UT.callUTjson = function(utData)
    {
        VM.UT.set(utData);
        tVMU = utData.UserId;
        tVMI = utData.InteractionId;
        return false;
    };
    
    if(tVMU == '' || typeof tVMU === 'undefined' || tVMU == null
        || tVMI == '' || typeof tVMI === 'undefined' || tVMI == null
        || (actualScreen == '0')) {
        
        VM.UT.fetchUT(null,'&rand='+ Math.random(),'VM.UT.callUTjson');
        
    }
});
