var clock;

function isIE () {
  var myNav = navigator.userAgent.toLowerCase();
  return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
}

$(document).ready(function() {

    // Grab the current date
    var currentDate = new Date();

    // Set some date in the future. In this case, it's always Jan 1
    var futureDate  = new Date(2015, 1, 16);

    // Calculate the difference in seconds between the future and current date
    var diff = futureDate.getTime() / 1000 - currentDate.getTime() / 1000;
	
	var nav=true;
	
	if ((currentDate.getFullYear()>= 2015) && (currentDate.getMonth() >= 1) && (currentDate.getDate() >= 16))
    {

       document.getElementById("OEBegins").innerHTML = "Did You Miss the Open Enrollment Deadline?";
       document.getElementById("OEBeginss").innerHTML = "Don't Worry - You Still Have Options to Get Insured."; 

    }else{
    // Instantiate a coutdown FlipClock
        clock = $('.your-clock').FlipClock(diff, {
            clockFace: 'DailyCounter',
            countdown: true
        });

        
	}
	//IE8
	
	   if (isIE () == 8) {
		  nav=false;
	   }
	
	
	$('.flexslider_home').flexslider({
			animation: "slide",
			animationLoop: true,
			smoothHeight: false,
			touch: false,
			controlNav:false,
			directionNav: nav,
			slideshowSpeed: 8000,
			animationSpeed: 600
	});
	
    
	 //flip-clock-wrapper
		
			//$('.your-clock-wrapper').hide();

	
});
    
</script>
