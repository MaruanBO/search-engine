<script>
    //Local storage
    $(".mp-login-linkorbutton").click(function(){
        var loginInfo = amplify.store("mp_loginInfo");
        if(!loginInfo) {
            loginInfo = {
                firstTime : true,
                logincompleted: false
            };
            amplify.store("mp_loginInfo", loginInfo);
        } else if(loginInfo.loginCompleted){
            loginInfo.firstTime = false;
            amplify.store("mp_loginInfo", loginInfo);
        }
    });
</script>

