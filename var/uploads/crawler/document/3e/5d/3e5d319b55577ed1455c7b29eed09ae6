function keys(object) {
    if (Type(object) != OBJECT_TYPE) { throw new TypeError(); }
    var results = [];
    for (var property in object) {
      if (object.hasOwnProperty(property)) {
        results.push(property);
      }
    }
    return results;
  }
function getWeather_7289385_86() {
  var city = jQuery("#city_7289385_86").val();
  findCityId_7289385_86(weatherCities_7289385_86, city);
  var cityId = jQuery("#city_id_7289385_86").val();
  loadCityWeather_7289385_86(city, cityId);
}

function loadCityWeather_7289385_86(city, cityId) {
  jQuery.ajax({
  url: "http://www.ctvnews.ca/weather-national-homepage-7.289385/weather-national-homepage-7.289385?ot=AjaxLayout&city="+ escape(city) + "&city_id=" + cityId,
  success: function(data) {
    setWeatherElementCookie_7289385_86(escape(city), cityId);
    jQuery("#weather_7289385_86").fadeTo("slow",0, function() {
      jQuery("#weather_7289385_86").replaceWith(data);
    });
  }});

}


function displayCity_7289385_86(){
  jQuery("#NotYourCity_7289385_86").fadeToggle("fast", 
  function() {
    jQuery("#city_7289385_86").slideToggle("fast", 
      function() {
        jQuery("#city_7289385_86").focus();
          }
        )
      }
  );  
}

function hideCity_7289385_86() {
  jQuery("#city_7289385_86").slideToggle("fast", 
  function() {
    if (jQuery("#NotYourCity_7289385_86:visible").size() <= 0) {
      jQuery("#NotYourCity_7289385_86").fadeToggle("fast");
        }
      }
  );  
}

function findCityId_7289385_86(cities, cityname) {
  cityname = cityname.toLowerCase();
  jQuery.each(cities, function(city, city_id){
    if (city.toLowerCase() == cityname) {
      jQuery("#city_id_7289385_86").val(city_id);
      return false;
    }
  });
}
function setWeatherElementCookie_7289385_86(city, cityId) {
 var today = new Date();
 var expire = new Date();
 var nDays = 365 * 10;
 expire.setTime(today.getTime() + (60*60*1000*24*nDays));
  var c_name = "ctv_weatherelement_city";
  if (cityId != null && cityId != "") {
    document.cookie=c_name + "=" + city + "_" + cityId
    + ";expires="+expire.toGMTString();
  }
}

function getWeatherElementCookie_7289385_86()
{
  var c_name = "ctv_weatherelement_city";
  var i,x,y,ARRcookies=document.cookie.split(";");
  for (i=0;i<ARRcookies.length;i++)
  {
    x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
    y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
    x=x.replace(/^\s+|\s+$/g,"");
    if (x==c_name)
      {
      return unescape(y);
      }
   }
}

function getSavedWeatherCity_7289385_86(){
  var storedCity = getWeatherElementCookie_7289385_86();
  var city = '';
  var cityId = '';
  if(storedCity){
    var cookieValueArr = storedCity.split("_");
    if(cookieValueArr.length = 2){
      city = cookieValueArr[0];
      cityId = cookieValueArr[1];
      loadCityWeather_7289385_86(city, cityId);
    }
  }else{     
    jQuery("#weather_7289385_86").show();
  }
}

  getSavedWeatherCity_7289385_86();

</script>
  
  
  
