// Fix for IE9 background ads. Allows clickthrough on top-fade element.
	jQuery(function() {
		jQuery('.top-fade').click(function(event) {
		    var e = event, target = jQuery(e.target), top = jQuery(this),
		    x = e.clientX, y = e.clientY,
		    d = document;
		    if(top.is(target)) {
		        top.hide();
		        d.elementFromPoint(x,y).click();
