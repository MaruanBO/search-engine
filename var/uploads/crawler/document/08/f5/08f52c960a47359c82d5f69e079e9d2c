<!-- BOF Added by Bibhu for fb_small and fb_large shortcodes to integrate url querystring into fb share button -->
<script type="text/javascript">
function generate_my_custom_querystring(){
function getParameterByName(name) {
name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
results = regex.exec(location.search);
return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

var partqstr = [];
var ik = 0;

if(getParameterByName('utm_content')){
partqstr[ik] = 'utm_content='+getParameterByName('utm_content');
ik = ik + 1;
}

if(getParameterByName('utm_medium')){
partqstr[ik] = 'utm_medium='+getParameterByName('utm_medium');
ik = ik + 1;
}

if(getParameterByName('utm_source')){
partqstr[ik] = 'utm_source='+getParameterByName('utm_source');
ik = ik + 1;
}

if(getParameterByName('utm_campaign')){
partqstr[ik] = 'utm_campaign='+getParameterByName('utm_campaign');
}

var querystring = partqstr.join('&');
return querystring;
}

function tilds_fb_share_link(){	
window.location = 'https://www.facebook.com/sharer/sharer.php?u='+encodeURIComponent(document.URL.split('?')[0]+'?'+generate_my_custom_querystring());
}
</script>
