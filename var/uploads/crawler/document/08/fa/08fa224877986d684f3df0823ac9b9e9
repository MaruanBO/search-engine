    function miniwin(el,w,h){w = w || 600;h = h || 400;window.open(el.href,'_blank','width='+w+',height='+h+',resizable=1,status=0,menubar=0,scrollbars=1');return false};

    $(function(){
      $('.howitworks-link a').toggle(
        function(){
          $(this).html("Close How It Works Section");
          $('.howitworks-detail').slideDown();
          $('html,body').animate({scrollTop:$(window).scrollTop()+$('.howitworks .howitworks-detailin').height()});
        },
        function(){
          $(this).html("How It Works");
          $('.howitworks-detail').slideUp()
        }
      )
    });

    </script>

