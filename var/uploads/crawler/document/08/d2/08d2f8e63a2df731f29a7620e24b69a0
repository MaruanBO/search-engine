<script>
$('link[href*="?14/css/v1/main.css&14/css/v1/form.css&14/css/v1/buttons.css&14/css/v1/alerts.css"]').remove();
$('#HeaderRegion').find('#facebook-sso-webpart-region').remove();
$('#HeaderRegion').find('#fb-root').remove();
$(window).on('resize.lohpheroresize', function() {
	if($('#lohpFullHero').css('background-image') !== undefined) {
		var height = $(window).height();
		if($('#lohpFullHero').css('background-image').indexOf('1600') !== -1) {
			$("#lohpFullHero").css('min-height', height);
		} else {
			$("#lohpFullHero").css('min-height', 0);
		}
	}
}).trigger('resize.lohpheroresize');
$(function() {
	$('.ancLoginBtn')
		.callout({
			'classes': 'calloutAlignRight smpLoginBox',
			'content': '#smpLoginCallout',
			'onOpen':function(){
				if ($('#facebook-sso-webpart-login').length) {
					return;
				}
				AjaxManager.send({
					url: '//fbsso.ajax.ancestry.ca/SignOn/Login?partnerid=5543&lcid=4105',
					success: function () {
						$('#loginWithFacebook').removeClass('noDisplay');
					}
				});
			},
			'onAfterOpen':function(){
				$('#smpUserName').focus();
			}
		});
	$('#smpLoginCallout')
		.on('keydown.enter', '.smpLoginInput', function(event){
			if (event.keyCode === 13 && $('#smpPassword').val().length > 0) {
				document.forms['smpLoginForm'].submit();
			}
		})
		.on('click.submit', '#smpSigninBtn', function(){
			document.forms['smpLoginForm'].submit();
		})
		.on('click.fb', '#loginWithFacebook', function(){
			$('#login-ui .facebook-sso-webpart-fbC .acom-fb-signin').trigger('click');
		});
});
</script>
