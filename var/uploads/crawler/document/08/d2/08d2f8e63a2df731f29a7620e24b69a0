<!--[if IE 8]>
<script type="text/javascript">
$(function() {
	$('.ancLoginBtn').on('click.ie8', function(e) {
		e.preventDefault();
		window.location = "https://secure.ancestry.ca/login?ti.si=0&amp;ti=5543";
	});
});
</script>
