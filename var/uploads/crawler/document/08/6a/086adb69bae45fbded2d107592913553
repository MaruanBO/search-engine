<script type="text/javascript">
	function validateMastheadSearch(form) {
		if (trimAll(form.keyword.value) == '') {
			form.keyword.value = '';
			alert('Please enter one or more search words before clicking SEARCH.');
			return false;
		} 
		mastheadLinkClick('http://www.qvc.com/CatalogSearch','SEARCH', form.keyword.value , form.keyword.value );
		return true;
	}
	// add LinkClickTag
	function mastheadLinkClick(path,mid,cmre,cmsp)
	{
	  //add cmre
	  if(path !=''&& cmre != ''){
	    if(path.indexOf('?') > 0 ){
	       path = path + '&cm_re=MH-_-' + mid + '-_-' + cmre ;
	    }else{
	       path = path + '?cm_re=MH-_-' + mid + '-_-' + cmre ;
	    }
	  } 
	  //add cmsp
	  if(path !=''&& cmsp != ''){
	    if(path.indexOf('?') > 0 ){
	       path = path + '&cm_sp=MH-_-' + mid + '-_-' + cmsp ;
	    }else{
	       path = path + '?cm_sp=MH-_-' + mid + '-_-' + cmsp ;
	    }
	  }
	  	cmCreateManualLinkClickTag(path,null,'','');
	}
	// If not present, insure that these cookies have default values
	try {	
	  var uattrVal = qCookie.get('uattr');
      if (uattrVal != null) {
		qCookie.remove('uattr',{domain:document.domain});
        qCookie.set('uattr', uattrVal, {path: '/', domain: '.qvc.com', expires: 3650});
      } else {
        qCookie.set('uattr', 'NV|0|CSNULL|0|0|NA', {path: '/', domain: '.qvc.com', expires: 3650});
      }
	  if(qCookie.get('uattrmb') == null ) {
		qCookie.set('uattrmb', 'NV0CSNULL', {path: '/', expires: 365});
	  }
    }
	catch (e) {}
</script>



