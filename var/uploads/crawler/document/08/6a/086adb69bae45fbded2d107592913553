				addevents : function(){
					var p = this;
					$('#'+p.id).hover(
						function () {
					   		if(!$(this).hasClass('selected')){
					      		$(this).children('#'+p.menuid).finish().fadeIn(p.speed);
					      	}
					   	},
					   	function () {
						   	if(!$(this).hasClass('selected')){
						        $(this).children('#'+p.menuid).finish().fadeOut();
						    }
					   	}
					);
					document.getElementById(this.linkid).onclick = function(e){
						$('#'+p.id).children('#'+p.menuid).finish().fadeIn(p.speed);
						$('#'+p.id).addClass('selected');
						menuMask = document.createElement('div');
						menuMask.id = "menuClickMask";
						menuMask.onclick = function(){
							$('#'+p.menuid).finish().fadeOut();
							$('#'+p.id).removeClass('selected');
							this.parentNode.removeChild(this);
						}
						document.body.appendChild(menuMask);
						return false;
					}
				},
				build: function(){
					var isLoggedIn = null !== qCookie.get("qvcauthenticed");
					this.fn = qCookie.get("qvcfn");
					if(isLoggedIn || null !== this.fn){this.state ="plAuthenticated";}
					if(null !== this.fn){
	    				
	    				this.shfn = this.fn;
	    				if(this.fn.length > this.mobilelimit){this.shfn = this.fn.substring(0,this.mobilelimit)+"...";}
						this.text = "Hello, <span class=\"userfn\">" + this.fn+"</span><span class=\"userfnShort\">" + this.shfn+"</span>";
						this.subtext = "Not You?";
						this.href="#";
					}else if(isLoggedIn){
						this.text = "Hello";
						this.subtext = "Sign Out";
						this.subtextclass ="";
						this.href="https\x3A\x2F\x2Fwww.qvc.com\x2FLogoff\x3FlangId\x3D-1\x26storeId\x3D10251\x26catalogId\x3D10151\x26URL\x3DLogonForm";
					}
					document.write(this.content());
					if(null !== this.fn){this.addevents();}
				},
				builddefault: function(){
					try {
						
						var isLoggedIn = document.cookie.indexOf("qvcauthenticed") > 0;
						var url = window.location.href;
						if (isLoggedIn && url.indexOf("/Logon") < 0 && url.indexOf("/ReLogon") < 0){
							document.write('<a id="'+this.linkid+'" href="https://www.qvc.com/Logoff?langId=-1&storeId=10251&catalogId=10151" manual_cm_re="MH-_-CS-_-SIGNIN" manual_cm_sp="MH-_-CS-_-SIGNIN">Sign Out</a>');
						}
						else {
		 				 	document.write('<a id="'+this.linkid+'" href="https://www.qvc.com/LogonForm?langId=-1&storeId=10251&catalogId=10151" manual_cm_re="MH-_-CS-_-SIGNIN" manual_cm_sp="MH-_-CS-_-SIGNIN">Register (Sign In)</a>');
						}
					}catch (e) {}
				},
				init: function(){
					
					var active = qCookie.get("pLoginActive");
					this.active = typeof active == 'string' ? active == "true" : false;	
					if(this.active){
						this.build();
					}else{
						this.builddefault();
					}
				}
			};
			pSignInLink.init();
		</script>
		
