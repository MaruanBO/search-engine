<script>
var keys = '';

function golf_search() {
  var uri = keys;
  if (uri == '') uri = jQuery('#edit-keys').val();
  window.location = '/search/site/' + escape(uri);
  return false;
}
function golf_track(field) {
  keys = field.value;
}

jQuery('#edit-keys').bind('keypress keyup keydown', function(e) {
    if(this.selectionStart == 0 && e.charCode == 46) {
        return false;
    }
});

</script>
