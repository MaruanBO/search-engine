        '<span style="color: red; font-size: 12px;" id="cont_send_error"></span>' +
        '<br>' +
        '<input type="submit" onclick="cont_send(); return false;" value="Send">' +
        '<input type="button" value="Close" onclick="close_cont();" id="cont_close_button"> ' +
        '<p style="text-align: right; line-height: 13px;"><br><img src="/images/GBVcontactus.png"/><br>Zabotinski 7 Ramat Gan Israel<br>972-3-7546320<br>' +
        "<a href='mailto:contact@dmgrplus.com'>contact@dmgrplus.com</a></p>" +
