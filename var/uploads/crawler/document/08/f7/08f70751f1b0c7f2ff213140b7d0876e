<script type="text/javascript">
var str_h = '&raquo;';
var str_v = '<span style="padding-right:5px;"><span class="rotate-down">&raquo;</span></span>';
function redirme(url){
	document.location.href = url;
}
function toggleit(pid){
	span = $('span[pcatid="'+pid+'"]')[0];
	var $trs = $('tr[pcatid="'+pid+'"]');
	var v = ($($trs[0]).css('display')=='none')?false:true;
	if(v){
		$trs.hide();
		$(span).html(str_h);
	}else{
		$trs.show();
		$(span).html(str_v);
	}
}
</script>
