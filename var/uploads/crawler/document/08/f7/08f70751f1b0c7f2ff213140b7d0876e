<script type="text/javascript">
var Countries = {"Australia":["New South Wales","South Australia","Victoria","Western Australia"],"Austria":["Burgenland","Salzburg","Styria","Tyrol","Upper Austria","Vienna"],"Belgium":["Antwerp","Brussels-Capital Region"],"Brazil":["Federal District","Minas Gerais","Parana","Rio de Janeiro","Rio Grande do Sul","Sao Paulo"],"Canada":["Alberta","British Columbia","Manitoba","New Brunswick","Newfoundland and Labrador","Nova Scotia","Ontario","Prince Edward Island","Quebec","Saskatchewan"],"Chile":["II Region","IV Region","IX Region","Santiago RM","V Region","VI Region","VIII Region"],"Croatia":["Croatia"],"Czech Republic":["Praha"],"Denmark":["Central Jutland"],"Finland":["Finland"],"France":["Alpes-Maritimes","Calvados","Corse-du-Sud","Cotes-d'Armor","Gironde","Haute-Garonne","Haute-Savoie","Haute-Vienne","Herault","Ile-de-Vilaine","Loire","Loire-Atlantique","Marne","Morbihan","Moselle","Nord","Pas-de-Calais","Puy-de-Dome","Rhone","Seine-Maritime","Seine-Saint-Denis","Ville de Paris"],"Germany":["Baden-W\u00fcrttemberg","Bavaria","Berlin","Brandenburg","Bremen","Hamburg","Hesse","Lower Saxony","Mecklenburg-Vorpommern","North Rhine-Westphalia","Rhineland-Palatinate","Saarland","Saxony","Saxony-Anhalt","Schleswig-Holstein","Thuringia"],"Hong Kong":["Central and Western"],"Ireland":["Dublin"],"Italy":["Ancona","Ascoli Piceno","Bari","Benevento","Bergamo","Bologna","Cagliari","Caserta","Catania","Florence","Forli-Cesena","Genoa","Lecce","Livorno","Lucca","Mantua","Milan","Naples","Padua","Palermo","Parma","Pavia","Pescara","Pisa","Reggio nell'Emilia","Rimini","Rome","Treviso","Trieste","Turin","Udine","Venice","Verona"],"Japan":["Osaka","Saitama","Tokyo"],"Latvia":["Riga"],"Luxembourg":["Luxembourg"],"Mexico":["Distrito Federal","Guerrero"],"Monaco":["Monaco"],"Netherlands":["Gelderland","Groningen","Limburg","North Brabant","North Holland","South Holland"],"New Zealand":["Auckland"],"Poland":["Poland"],"Portugal":["Lisbon","Oporto"],"Puerto Rico":["Puerto Rico"],"Russia":["Moscow City","Saint Petersburg City"],"Singapore":["Singapore"],"Slovakia":["Slovakia"],"South Africa":["Eastern Cape","KwaZulu-Natal","Western Cape"],"Spain":["Andalusia","Aragon","Balearic Islands","Basque Country","Catalonia","Galicia","Madrid","Valencia"],"Sweden":["Stockholm","Vastra Gotaland"],"Switzerland":["Aargau","Basel-Stadt","Bern","Geneva","Sankt Gallen","Thurgau","Zurich"],"United Kingdom":["Belfast","Birmingham","Brighton and Hove","Bristol","Buckinghamshire","Cardiff","Devon","Dorset","East Riding of Yorkshire","Edinburgh","Fife","Glasgow","Gloucestershire","Greater London","Kingston upon Hull","Lancashire","Leeds","Leicester","Liverpool","Manchester","Newcastle upon Tyne","Nottingham","Sheffield","Sunderland"],"United States of America":["Alabama","Alaska","Arizona","Arkansas","California","Colorado","Connecticut","Delaware","District Of Columbia","Florida","Georgia","Hawaii","Idaho","Illinois","Indiana","Iowa","Kansas","Kentucky","Louisiana","Maine","Maryland","Massachusetts","Michigan","Minnesota","Mississippi","Missouri","Montana","Nebraska","Nevada","New Hampshire","New Jersey","New Mexico","New York","North Carolina","North Dakota","Ohio","Oklahoma","Oregon","Pennsylvania","Rhode Island","South Carolina","South Dakota","Tennessee","Texas","Utah","Vermont","Virginia","Washington","West Virginia","Wisconsin","Wyoming"]};
var Default_Country = "Singapore";
var Default_State = "None";
var Real_Default_Country = "Singapore";
var Real_Default_State = "None";
var DEFAULT_LOCALE = true;
var $Countries = {};
$(document).ready(function(e) {
	$Countries = {
		displayDelay: false,
		submitEm: function(){
			$Countries.submitForm();
		},
		setset: {
			state: function($to){	$('#state').val($to).trigger('chosen:updated');		$Countries.displayButton(); },
			country: function($to){	$('#country').val($to).trigger('chosen:updated');	$Countries.displayButton(); }
		},
		submitForm: function(){
			$('[loading-new-locale]').show();
			$('[update-button],[reset-button],[reset-button2]').hide();
			$('#Locale').submit();
		},
		resetRealDefault: function(){
			$Countries.setset.country(Real_Default_Country);
			$Countries.updateStates(Real_Default_Country);
			$Countries.setset.state(Real_Default_State);
			$Countries.submitForm();
		},
		resetDefault: function(){
			if(Default_Country.length > 0){
				$Countries.setset.country(Default_Country);
				$Countries.updateStates(Default_Country);
				if(Default_State.length > 0){
					if(Default_State == '' && Countries[Default_Country].length > 1)
						$Countries.setset.state('-1');
					else
						$Countries.setset.state(Default_State);
				}
			}
		},
		displayButton: function(){
			var $state = $('#state').val();
			var $country = $('#country').val();
			if($state == Default_State && $country == Default_Country)
				$('[update-button],[reset-button]').hide();
			else
				$('[update-button],[reset-button]').show();
			if(DEFAULT_LOCALE === false)
				$('[reset-button2]').show();
		},
		updateStates: function($Country){
			if(typeof(Countries[$Country]) != 'object') return;
			$('#state').removeAttr('disabled');
			var $opts = ['<option value="">Choose State...</option>'];
			if(Countries[$Country].length > 1)
				$opts.push('<option value="-1">All</option>');
			$.each(Countries[$Country], function(i, state){
				$opts.push('<option value="'+state+'">'+state+'</option>');
			});
			$('#state').html($opts.join(''));
			$('#state [state-default]').text('Choose State...');
			$('#state').trigger("chosen:updated");
			$Countries.displayButton();
		}
	};
	$Countries.resetDefault();
});
</script>
