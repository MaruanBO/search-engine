<script type="text/javascript">
var _qevents = _qevents || [];

(function() {
var elem = document.createElement('script');
elem.src = (document.location.protocol == "https:" ? "https://secure" : "http://edge") + ".quantserve.com/quant.js";
elem.async = true;
elem.type = "text/javascript";
var scpt = document.getElementsByTagName('script')[0];
scpt.parentNode.insertBefore(elem, scpt);
})();

_qevents.push({
qacct:"p-74YN39dvCaOWA"
});

$(document).ready(function(e) {
	// inject a
	 
	 var slideHeight = 70;
	 $(".container").each(function(index, element) {
		var $this = $(this);
		var $wrap = $this.children(".wrap");
		var defHeight = $wrap.height();
		if (defHeight >= slideHeight) {
			$wrap.css("height", slideHeight + "px");
			$this.find(".read-more").append('<a href="#">View More <span style="color:#0667c8;">&raquo;</span></a>');
			$this.find(".read-more").bind("click", function(event) {
				var curHeight = $wrap.height();
				if (curHeight == slideHeight) {
					$wrap.animate({
						height: defHeight
					}, "normal");
					$this.find(".read-more a").html('&laquo; Hide');
				} else {
					$wrap.animate({ height: slideHeight }, "normal");
					$this.find(".read-more a").html('Show more <span style="color:#0667c8;">&raquo;</span>');
				}
				return false;
			});
		}
		
		if(defHeight == 0){
			$this.find(".read-more").css('border', '0');
			$this.find(".read-more").css('margin', '0');
			$this.find(".read-more").css('padding', '0');
		}
	});
	$('img[cluetipme="true"]').each(function(index, element) {
		$(element).attr('title', '<div>'+$(element).attr('title')+'</div>');
		/*$(element).cluetip({
			activation: 'hover',
			splitTitle: '|',
			sticky: false,
			width: 161,
			showTitle: false
		});*/
		$(element).removeAttr('title');
	});
	setTimeout(function(){
		$('.ssc_lrDlvOptIdIcon').each(function(index, element) {
			$(element).attr('title', '<div>eTICKETS Available</div>');
			/*$(element).cluetip({
				activation: 'hover',
				splitTitle: '|',
				sticky: false,
				width: 161,
				showTitle: false
			});*/
			$(element).removeAttr('title');
		});
		$('.ssc_lrDlvOptEmIcon').each(function(index, element) {
			$(element).attr('title', '<div>eTICKETS Available</div>');
			/*$(element).cluetip({
				activation: 'hover',
				splitTitle: '|',
				sticky: false,
				width: 161,
				showTitle: false
			});*/
			$(element).removeAttr('title');
		});
	}, 1000);
	/*if(true || navigator.appVersion.indexOf('MSIE') >= 0){
		var pad = 50;
		$('[id="events_Pager"]').width($('[id="events_Pager"]').width() + pad);
		$('[id="events_Pager"] div').each(function(index, element) {
			if(index == 1){
				$(element).width($(element).width() + pad);
			}else if(index == 2){
				console.log($(element).css('left'));
				$(element).css('left', (parseInt($(element).css('left').replace(/px/, '')) + pad));
			}
		});
	}*/
	$('td[width="761"]').addClass('column-page-force');
	
	$('[results-table]').each(function(i, e) {
			$(e).find('.results_body').each(function(index, element) {
			$odd = (index%2!=0);
			$(element).removeClass('odd').removeClass('even');
			if($odd) $(element).addClass('odd');
			else $(element).addClass('even');
		});
	});
});
var $chosen_cfg = {
	'#country': {},
	'#state': {}
};
$.each($chosen_cfg, function(e, p){ $(e).chosen(p); });
	$('#country').on('change', function(e){ $Countries.updateStates($(this).val()); });
	$('#state').on('change', function(e){ $Countries.displayButton(); });
</script>
