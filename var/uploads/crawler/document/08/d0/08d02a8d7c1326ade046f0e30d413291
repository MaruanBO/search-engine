  			}

	});
$('a.fancybox-anchor-tag')
    .each(
        function ( e )
        {            
            $(this).fancybox({
                'type' : 'inline',
                'hideOnContentClick': true,
		'maxWidth'	: '500',
		'autoScale'     : 'true',
		'fitToView'	: 'true'
            });
