 <div style=" padding-right: 6px; background: none; padding-left: 6px;" id="social2"><script src="/stjude/common/javascript/default.js" language="JavaScript" type="text/javascript"></script><script type="text/javascript" language="JavaScript">

/**
 * Validates an email address against the RFC2822 standard.
 */
function isRFC2822Email(email) {
  return /^([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x22([^\x0d\x22\x5c\x80-\xff]|\x5c[\x00-\x7f])*\x22)(\x2e([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x22([^\x0d\x22\x5c\x80-\xff]|\x5c[\x00-\x7f])*\x22))*\x40([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x5b([^\x0d\x5b-\x5d\x80-\xff]|\x5c[\x00-\x7f])*\x5d)(\x2e([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x5b([^\x0d\x5b-\x5d\x80-\xff]|\x5c[\x00-\x7f])*\x5d))*$/.test(email);	
}

function signcheckFields() {
  var f = document.signUpForm;
  if (f.email.value == "") {
   showDiv();
   f.email.focus();
   return false;
  }
 if (f.email.value != "") {
   if( isRFC2822Email(f.email.value) === false ) {
	showDiv();
	f.email.focus();
	return false;
  }
 }
 OpenWindow('https://apps.stjude.org/EmailForms/jsp/stay_connect.jsp?reqemail='+f.email.value,'Newsletter','width=650,height=500,scrollbars=no,location=no,directories=no,status=no,menubar=no,toolbar=no,resizable=yes');
 hideDiv();
return false;
}
function showDiv() { 
  document.getElementById('sj-error').style.display = 'block';
  document.getElementById('social').style.backgroundPosition = '50% -7px'; 
}

function hideDiv() { 
 document.getElementById('sj-error').style.display = 'none';
 document.getElementById('social').style.backgroundPosition = '50% -38px';
}
</script>
<form id="signUpForm" onsubmit="return signcheckFields()" name="signUpForm">
