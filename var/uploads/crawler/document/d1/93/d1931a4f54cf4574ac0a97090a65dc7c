	function clicPrint(){
		s.linkTrackVars="eVar33,events";
		s.linkTrackEvents="event23";
		s.eVar33 = "St_Jude_Homepage";
		s.events = "event23";
		s.tl(this, 'o', 'Print Article');
		window.print();
	}
	function clicSocialMedia(sUrlLink){
		s.linkTrackVars="eVar12,eVar13,events";
		s.linkTrackEvents="event5";
		s.events = "event5";
		s.eVar12=sUrlLink;
		s.eVar13="st_jude_homepage:home";
		s.tl(this, 'o', 'Share Utility');
		window.location.assign(sUrlLink);
	}
	function clicFooterSocialMedia(sUrlLink){
		s.linkTrackVars="eVar12,eVar13,events";
		s.linkTrackEvents="event5";
		s.events = "event5";
		s.eVar12=sUrlLink;
		s.eVar13="st_jude_homepage:home";
		s.tl(this, 'o', 'Share Utility');
		window.open(sUrlLink);
	}
	function clicDreamHomeTicketReserve(sEventCode){
		s.linkTrackVars="prop21,events,products,eVar22";
		s.linkTrackEvents="prodView,event3";
		s.prop21="dream home";
		s.eVar22=sEventCode;
		s.events="prodView,event3";
		s.products=";"+sEventCode;
		s.tl(this, 'o', 'Reserve Dream Home Ticket');
		document.dreamhome.submit();		
	}
	function clicSlideShow(sCurrentPosition,sContentTitle,sUniqueLinkIdentifier){
		s.linkTrackVars='eVar48,eVar49,events';
		s.linkTrackEvents='event41';
		s.eVar49=sCurrentPosition + ':' + sContentTitle.toLowerCase() + ':' + sUniqueLinkIdentifier;
		s.events='event41';
		s.tl(this, 'o', 'Slide Show Interaction');		
	}

	function capturePlayPauseEvent() {
		var position = getSlidePosition(); 
		clicSlideShow(position,getContentTitle(position).replace(/[^a-zA-Z ]/g, ""),toggleControls());		
	}

	function captureLinkClickEvent(titleArg, contentType) {	
		var newTitleArg = titleArg.replace(/[^a-zA-Z ]/g, "");
		clicSlideShow(getSlidePosition(),newTitleArg.toLowerCase(),contentType);		
	}	

	function toggleControls() {
		if (toggleSwitch=="") {
			toggleSwitch = "stop";
		} else if (toggleSwitch=="stop") {
			toggleSwitch = "play";
		} else {
			toggleSwitch = "stop";
		}
		return toggleSwitch;
	}

	function getSlidePosition() {	
		var i = 1;	
		
		for (var i=1;i<10;i++) {
			var tmp = "slide_id:" + i;
			var slideIndex = document.getElementById(tmp);

			if (slideIndex!=null) {
				if(slideIndex.style.opacity == 1) {	
					return i;
				} 
			}
		}
	}

	function getContentTitle(position) {
		var tmp2 = "slide_id:" + position; 
		var myTitle = document.getElementById(tmp2).title;
			myTitle.replace(/[^a-zA-Z ]/g, "");
		return myTitle.toLowerCase();
	}

	function countPatientImageClick(title) {	
		var originalPageName = s.pageName;			
			originalPageName = originalPageName.replace(originalPageName.substr(originalPageName.lastIndexOf(":"), originalPageName.length), ":" + title.toLowerCase());	
 		s.pageName = originalPageName;
		s.t();
	}

