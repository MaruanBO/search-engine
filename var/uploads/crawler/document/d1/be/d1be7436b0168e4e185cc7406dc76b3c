<script type="text/javascript">
    function LHN_button_onLoad() {
        if (bLHNOnline == 0) {
            //All operators are offline
            $('#chat').hide();
            $('#call').hide();
            $('#chat_ico').hide();
            $('#chat_header').hide();
        }
        else {
            //There are operators online
            $('#chat').show();
            $('#call').show();
            $('#chat_ico').show();
            $('#chat_header').show();
        }
    }


</script>

