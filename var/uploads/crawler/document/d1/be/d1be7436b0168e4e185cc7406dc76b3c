        function LHNPop(url, width, height, showClose, hideOnContentClick, type) {
            //Type Can be set to 'image', 'ajax', 'iframe', 'swf' or 'inline'
            $.fancybox({
                'zoomSpeedIn': 300,
                'zoomSpeedOut': 300,
                'padding': 0,
                'href': url + 'pooky=' + Math.random(),
                'modal': false, //If set to true, will disable navigation and closing
                'width': width,
                'height': height,
                'fitToView': true,
                'autoSize': false,
                'closeBtn': showClose,
                'closeClick': hideOnContentClick,
                'type': type //Supported types are 'image', 'inline', 'ajax', 'iframe', 'swf' and 'html'
            });
        }

    </script>

    
