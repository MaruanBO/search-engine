<script type="text/JavaScript" src="/redesign/jquery.cycle.all.min.js"></script>
<script type="text/JavaScript">
 $(document).ready(function() {
   $('#cycle_image').cycle({ 
    fx:    'fade', 
    speed:  2000, 
    timeout: 12000,
    next:   '#cycle_image', 
    pager: '#cycle_links',
    before:   cycle_onbefore, 
    height: '300px',
    pause:  1 
    });
    

    function cycle_onbefore() { 
        var img = this;
        $img = $(this);
        if ( $img.hasClass('first') ) {
            $('#cycle_text').html(img.alt)
            $img.removeClass('first');
        }
        else {
            $('#cycle_text').fadeOut(1000, function() { 
                $('#cycle_text').html(img.alt)
                .fadeIn(1000); 
             });
         }
    }

   $('.event_details_link').click( function() {
        $(this).parent().children('.event_def').fadeIn('slow');        
    });
    $('.event_def_close').click( function() {
            $(this).parent().hide();
    });

 });

</script>
