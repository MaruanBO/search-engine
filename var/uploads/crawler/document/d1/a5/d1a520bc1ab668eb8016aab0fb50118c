<script  type="text/javascript">
$(document).ready(function() {
	var feeds = [ 'http://nsf.gov/rss/rss_www_discoveries.xml','http://www.nasa.gov/rss/breaking_news.rss' ];
	for( var i=0; i<feeds.length; i++ ) {
  	  $('#news_left').append('<div id="ticker'+i+'"> </div>');
  		$('#ticker'+i).rssfeed(feeds[i], { content: false }, function(e) {
                    $(e).find(' div.rssBody').vTicker({ showItems: 1, pause: 20000, height: 0, speed: 1000});
   	 	});
	}
	
	/*if ( $('#events_scroll').find('li').length > 2 ) {
	$('#events_scroll').vTicker({
		speed: 1000,
		pause: 20000,
		showItems: 2
	});
	}*/

	$('#bookmark_button').click( function() {
            $.get( '/php/bookmark.php', $(this).attr('name'), function(data) {
            $('#bookmark_result').append(data).show();
          });
	});
	
	 $('#bookmark_result_close').live( 'click',function() {
            $('#bookmark_result').hide().html('<a href="#" id="bookmark_result_close">x</a>') ;
        }); 
        
        $('#bookmark_remove').live( 'click',function() {
                $.get( '/php/bookmark.php', $(this).attr('name') + '&action=remove', function(data) {
                $('#bookmark_result').html('<a href="#" id="bookmark_result_close">x</a>').append(data).show();
            }); 
        });

	$('.current_events_item').live( 'click', function() {
		 $(this).parent('li').children('.current_events_detail').fadeIn('slow'); 
	});

	$('.current_events_close').click( function() {
            $(this).parent().hide();
       });

	$('#closewindow').live( 'click', function() {
		$('#donation_ad').hide();
		 $.get( '/php/ad_close.php');
	});
	$('a[href*="quiz.php"]').colorbox1({width:"700", height:"800",  opacity:0.5, iframe:true, overlayClose: false});
	$('a[href*="reg_donate.php"]').colorbox1({width:"600", height:"700",  opacity:0.5, iframe:true, overlayClose: false});

	/*$(document).bind('cbox_closed', function () {
		$('#cboxOverlay').remove();
		$('#colorbox').remove();
	});*/
 });
</script>
