<script>
var spotlight2;
$(document).ready(function(){
var spotlight = $('#spotlight').spotlight({transition:'slide', intervalLength: '8', rotate:true, mouseAction:'hover'});
$('#product_spotlight > .slide_content').products({
        imageDomain:'//images.ladyfootlocker.com',
        jpgImageDomain:'//www.ladyfootlocker.com',
		Ntt:'lfl2013slidingmodule',
		Rpp:'16',
		grpSize:'4',
		reviewNum:'8002',
		linkPDP:true
	}, 
	function(){
		spotlight2 = $('#product_spotlight').spotlight({
                   transition:'slide', 
                   rotate:false, 
                   endStop: true});
		setupCM("#product_spotlight");
	}
);
});
function getcmstring(term) {
	var searchTerm = $.trim(term.toString().toLowerCase());
	searchTerm = searchTerm.replace(/[/]/ig,'');
	searchTerm = searchTerm.replace(/[^a-z0-9-+ ]/ig,'');
	searchTerm = searchTerm.replace(/ +/ig,'_');
	searchTerm = searchTerm.replace(/-+/ig,'');
	searchTerm = searchTerm.replace(/_+/ig,'_');
	return searchTerm;
}
function setupCM(id) {
	$(id + ' .product_cell').on('click', function() {
		var cm = "product_spotlight-_-" + (parseInt($(this).parent('li').children('a').index(this))+1) + "-_-" + getcmstring($(this).attr('title'));
		if(typeof cmCreateManualLinkClickTag == "function")
			cmCreateManualLinkClickTag('www.ladyfootlocker.com/?cm_re=slidingmodule' + cm, getcmstring($(this).attr('title')));
	});
}

</script>

