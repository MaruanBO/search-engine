<script type="text/javascript">
var gpt_request = {};
gpt_request.inside = false;
gpt_request.staging = false;
gpt_request.admin = false;
gpt_request.doRubicon = true;
gpt_request.debugRubicon = false;
gpt_request.debugCasale = false;
gpt_request.yieldbot = true;
gpt_request.yieldbot_slots = ['dart_tower', 'dart_pushdown', 'dart_rect'];
gpt_request.adPositions = [];
gpt_request.adPositions.push('dart_oop');
gpt_request.adPositions.push('dart_pushdown');
gpt_request.adPositions.push('dart_rect');
var ybotq = ybotq || [];
function loadYieldBot() {
// Loading yieldbout script tag
var js = document.createElement('script');
js.src = '//cdn.yldbt.com/js/yieldbot.intent.js';
var node = document.getElementsByTagName('script')[0];
node.parentNode.insertBefore(js, node);
}
function setupYieldBotDesktop() {
// Setting up a function to be executed one yieldbot script tag is loaded
// Can never be accidentally fied since only the yieldbot script looks for ybotq
ybotq.push(function () {
yieldbot.pub('c7c4');
yieldbot.domainName('topix.com');
for (var ii=0; ii < gpt_request.adPositions.length; ii++) {
if ($jq.inArray(gpt_request.adPositions[ii], gpt_request.yieldbot_slots) >= 0) {
if (gpt_request.adPositions[ii] == 'dart_rect') {
yieldbot.defineSlot(gpt_request.adPositions[ii], { sizes: [[300, 250], [300, 600]]});
}
else {
yieldbot.defineSlot(gpt_request.adPositions[ii]);
}
if (gpt_request.staging) {
console.log('yieldbot setup: ' + gpt_request.adPositions[ii]);
}
}
}
yieldbot.enableAsync();
yieldbot.go();
});
}
function yieldBotNotReady(label) {
var tpx_log_type = label;
var tpx_log_cb = parseInt(Math.random() * 1000000000);
var tpx_log_img = new Image(1,1);
tpx_log_img.src = "http://stats.topix.com/adbackfill.gif?type=" + tpx_log_type + "&cb=" + tpx_log_cb;
if (gpt_request.staging) {
console.log("YIELDBOT: " + tpx_log_img.src);
}
tpx_log_img.onload = function() { return; };
}
if (gpt_request.yieldbot) {
loadYieldBot();
setupYieldBotDesktop();
}
