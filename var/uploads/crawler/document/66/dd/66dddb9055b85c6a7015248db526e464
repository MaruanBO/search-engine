<script type="text/javascript">
quantSegs="";
function qc_results(result)
{
for (var i = 0; i < result.segments.length; i++) {
quantSegs += "qcseg=" + result.segments[i].id + ";";
}
if(quantSegs != "")
{
ajax_request("/ajax/set-qcseg", {
parameters: {
qcseg:quantSegs
}
});
}
}
</script>
