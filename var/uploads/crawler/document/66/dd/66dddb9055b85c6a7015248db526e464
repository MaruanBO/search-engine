function set_targets() {
googletag.cmd.push( function () {
if ( typeof index_slots !== 'undefined') {
var slots = new Array();
for ( var i=0; i < index_slots.length; i++ ) {
var index_params = index_slots[i].split( '_' );
var op = index_params.shift( );
var k = op;
var v = index_params.join( '_' );
slots.push(v);
googletag.pubads().setTargeting( k, v );
if(gpt_request.staging || gpt_request.debugCasale)
console.log( 'key:'+k+' val:'+v );
}
googletag.pubads().setTargeting(k,slots);
}
else {
setTimeout( function(){set_targets();}, 50 );
}
});
}
