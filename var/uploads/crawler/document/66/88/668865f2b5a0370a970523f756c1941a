	<script src="http://tcache.sbgnet.com/template/countdown/countdown.js" type="application/javascript"></script>
	<script type="application/javascript">
	
	var checkedDays = false;
	var lo = '<div class="cntdn_block_1">{dnn}<div class="cntdn-lbl">Days</div></div>';
	var losub = '<div class="cntdn_block_1">{hnn}<div class="cntdn-lbl">Hours</div></div>'
		+ '<div class="cntdn_block_1">{mnn}<div class="cntdn-lbl">Mins</div></div>'
		+ '<div class="cntdn_block_1">{snn}<div class="cntdn-lbl">Secs</div></div>';
	
	function adjustDays(periods) {
		if (!checkedDays) {
			checkedDays = true;
			var days = Math.floor($.countdown.periodsToSeconds(periods)/86400);
			if (days > 99) {
				$('.hasCountdown').countdown('destroy');
				lo = '<div class="cntdn_block_1 cntdn_3day">{dnnn}<div class="cntdn-lbl">Days</div></div>';
				window.setTimeout(function() {
					$('.thecount_1').countdown({
							until: d,
							layout: lo + losub,
							timezone:-7,
						});
				}, 1)
			}
		}
	}
	
	
	
	<!-- Subtracting month to make 0 base as javaScript date requires -->
	var d = new Date(2014, 10, 22, 17, 30, 0, 0);
	var sDate = new Date(1416286800);
	var now = new Date();
	
	if (d.getTime() > now.getTime() && now.getTime() > sDate.getTime()) {
		$('#countdown-inner-60').countdown({
			until: d,
			layout: lo + losub,
			timezone: -7,
			onTick: adjustDays
			});
	} else {
		$('#countdown-60').remove();
	}
	</script>
 
