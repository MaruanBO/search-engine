<!-- Google Analytics -->
<script type="text/javascript">
/* <![CDATA[ */

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-2478988-1']);

  /* if hostname doesn't match starpulse.com - this rule should be first  */
  if (!window.location.hostname.match(/starpulse.com$/)) {
    _gaq.push(['_trackPageview','/off-site-frame/' + window.location.hostname]);

  /* truncate to ring level for topic channels */
  } else if (window.location.pathname.match(/^\/(Actors|Actresses|Athletes|Celebrity|Movies|Music|Notables|Supermodels|Television)\/(.*?)\/(.*?)\//)) {
    _gaq.push(['_trackPageview','/' + RegExp.$1 + '/' + RegExp.$2 + '/' + RegExp.$3 + '/']);

  /* truncate to title for slideshows - dynamic pageviews are also tracked in slideshows javascript */
  } else if (window.location.pathname.match(/^\/slideshows\/(.*?)\//)) {
    _gaq.push(['_trackPageview','/slideshows/' + RegExp.$2 + '/']);

  /* normalize to main gallery page 1 for event photos */
  } else if (window.location.pathname.match(/^\/events\/([0-9]*?)\/0\/[0-9]*?\/(.*?)\//)) {
    _gaq.push(['_trackPageview','/events/' + RegExp.$1 + '/0/1/' + RegExp.$2 + '/0/']);

  /* normalize to main page for Photos */
  } else if (window.location.pathname.match(/^\/Photos\//)) {
    _gaq.push(['_trackPageview','/Photos/']);

  /* remove query string on news - leave cat */
  } else if (window.location.pathname.match(/^\/news\//)) {
    var trackingURL =window.location.pathname;
    if (window.location.search.match(/cat=([0-9]*)/)) {
      trackingURL +='?cat=';
      trackingURL +=RegExp.$1;
    }
    _gaq.push(['_trackPageview',trackingURL]);

  /* lump all /search together */
  } else if (window.location.pathname.match(/^\/search/)) {
    _gaq.push(['_trackPageview','/search/']);

  /* default to track the page normally */
  } else {
    _gaq.push(['_trackPageview']);
  }

  _gaq.push(['_trackPageLoadTime']);
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

  (function (tos) {
    window.setInterval(function () {
      tos = (function (t) {
        return t[0] == 50 ? (parseInt(t[1]) + 1) + ':00' : (t[1] || '0') + ':' + (parseInt(t[0]) + 10);
      })(tos.split(':').reverse());
      _gaq.push(['_setAccount', 'UA-2478988-1']);
      window.pageTracker ? pageTracker._trackEvent('Time', 'Log', tos) : _gaq.push(['_trackEvent', 'Time', 'Log', tos]);
    }, 10000);
  })('00');

  function trackPageView(path) {
    try {
      _gaq.push(['_setAccount', 'UA-2478988-1']);
      _gaq.push(['_trackPageview',path]);
      if (typeof(_qevents.push) == 'function') { _qevents.push({ qacct:"p-4cjg46u42920Y"}); }
      if (typeof(_comscore.push) == 'function') { _comscore.push({ c1:"2",c2:"8555387"}); }
    }catch(err){}
    return true;
  }

  function trackOutboundLink(link,category) {
    try {
      if (typeof(category) == 'undefined') { category ='Outbound Link'; }
      var urlParts =link.href.split('/');
      var action =urlParts[2];
      _gaq.push(['_setAccount', 'UA-2478988-1']);
      _gat._getTrackerByName()._trackEvent(category, action, link.href);
    }catch(err){}
    return true;
  }

  function trackGenericEvent(action,category,label) {
    try {
      if (typeof(action) == 'undefined') { action ='Unknown'; }
      if (typeof(category) == 'undefined') { category ='Unknown'; }
      if (typeof(label) == 'undefined') { label ='Unknown'; }
      _gaq.push(['_setAccount', 'UA-2478988-1']);
      _gat._getTrackerByName()._trackEvent(category, action, label);
    }catch(err){}
    return true;
  }

  function trackRadioEvent(action,category,label) {
    try {
      if (typeof(action) == 'undefined') { action ='Unknown'; }
      if (typeof(category) == 'undefined') { category ='Unknown'; }
      if (typeof(label) == 'undefined') { label ='Unknown'; }
      _gaq.push(['_setAccount', 'UA-2478988-3']);
      _gat._getTrackerByName()._trackEvent(category, action, label);
    }catch(err){}
    return true;
  }

  var pseudoLink =function(link_href) {
    document.location.href =link_href;
  }
  var pseudoLinkNewTab =function(link_href,link_target) {
    if (typeof(link_target) == 'undefined') {
      link_target ='_blank';
    }
    window.open(link_href, link_target);
  } 

  $('document').ready( function() {
/*
    window.setInterval(function () {
	googletag.pubads().refresh();
    }, 60000);
*/
    $('body').on('click', '.pseudo_link', function() {
      if (typeof($(this).children('a').first().attr('href')) != 'undefined') {
        pseudoLink($(this).children('a').first().attr('href'));
        return false;
      }
      return true;
    });
  });
/* ]]> */
</script>
