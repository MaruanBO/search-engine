	jQuery(window).scroll(function(){
		updateContainer();
	});
}); 
function updateContainer() {
    var containerWidth = jQuery(window).width();
	var aboveHeight = jQuery('#header-wrapper').outerHeight();
	
	if (jQuery(window).scrollTop() > aboveHeight){
	
		jQuery('#logo-wrapper').hide();
		jQuery('#nav-wrapper').addClass('fixed').css('top','0')
		jQuery('#nav-wrapper').next().css('margin-top','52px');
		if (containerWidth >= 1130) { 
			jQuery('#nav-wrapper').css({ width:'1111px' }); 
		}
		if (containerWidth > 983 && containerWidth < 1130) { 
			//jQuery('#logo-wrapper').show();
			//jQuery('#nav-wrapper').removeClass('fixed').next().css('margin-top','0');
			jQuery('#nav-wrapper').css({ width:'' }); 
		}
			
	} else {
		jQuery('#logo-wrapper').show();
		jQuery('#nav-wrapper').removeClass('fixed').next().css('margin-top','0');
		jQuery('.small_fixed_logo').hide().next().css('margin-top','0');

			if (containerWidth >= 1130) {
			jQuery('#nav-wrapper').css({
			width:''
			});
		}
	}
}


//]]>

jQuery( window ).resize(function() {

		if( jQuery(window).width() > 767 && jQuery(window).width() < 1130 ){
		
			var wrap_width		=	jQuery( ".es-carousel" ).width();
		
			jQuery("ul.home-carousel li").css('min-width', wrap_width);
			jQuery("ul.home-carousel li img").css('width', wrap_width);
			
			jQuery("ul.home-carousel li img").css('max-height', 'none');
	
		}
		
		if( jQuery(window).width() >= 1130 ){
			
			jQuery("ul.home-carousel li").css('min-width', '');
			jQuery("ul.home-carousel li img").css('width', 'auto');
			
			jQuery("ul.home-carousel li img").css('max-height', '100%');
			
		}

});



