<script type="text/javascript">
  $(document).ready(function(){
    PointerEventsPolyfill.initialize({});
  });
  videojs("#video-playlist", {"height":"auto", "width":"auto"}).ready(function(event){
      var myPlayer=this;
      myPlayer.playlist({
          'continuous': false,
      });
      //if(typeof myPlayer.L!="undefined") myPlayer.id_=myPlayer.L;
      function resizeVideoJS(){
        var width = document.getElementById(myPlayer.el().id).parentElement.offsetWidth;
        var aspectRatio=8/12;
        myPlayer.width(width).height( width * aspectRatio);
      }
      resizeVideoJS(); // Initialize the function
      window.onresize = resizeVideoJS; // Call the function on resize
  });
  $(document).ready(function(){
  var firstPlay = 0,
      firstSrc = document.querySelectorAll('[id^="video-playlist_"]')[0].currentSrc;
  console.log("firstplay initialized" + firstPlay);
  $("#video-playlist-vjs-playlist").click(function(){
    firstPlay = 1;
    console.log("event will not be sent" + firstPlay);
  })
  $("#video-playlist").click(function(){
    if (firstPlay == 0) {
      __gaTracker('send', 'event', 'Playlist', 'Video Selected', firstSrc.split("/").slice(-1)[0].replace(/\.(\w{3,4})(\?.*)?$/i, ''));
      console.log("event sent" + firstPlay);
    };
    firstPlay = 1;
  })
});
</script>
<script type="text/javascript">
  var openx = videojs('#video-playlist');
  openx.ads();
  openx.vast({
      url: 'http://ox-d.cordillera.tv/v/1.0/av?auid=263993&cc=1&md=1',
      skip: 15
  });
</script>
