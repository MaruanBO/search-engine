<li><div class="thumb-wrapper" style="background-image:url(/storyimage/CP/20150219/CS/150219228/AR/0/AR-150219228.jpg&exactw=140&exacth=240&q=60);"><div class="headline-wrapper CS-border-r"><span class="timestamp"><span class="icon-clock"></span>&nbsp;
	<script type="text/javascript">
		var pubTime = new Date('2/19/2015 15:48:50'),
			  sysTime = new Date(),
			  msecPM = 1000 * 60,
			  msecPH = msecPM * 60,
			  msecPD = msecPH * 24;
		//calculate the interval between system and published time
    var timeInt = sysTime.getTime() - pubTime.getTime();
    //calculate no of days
    var days = Math.floor(timeInt / msecPD);
    timeInt = timeInt - (days * msecPD);
    //calculate no of hours
    var hours = Math.floor(timeInt / msecPH);
    timeInt = timeInt - (hours * msecPH);
    //calculate no of minutes
    var minutes = Math.floor(timeInt / msecPM);
    //output results
    if (days > 0) {
		  document.write(days + " day ago");
		} else if (hours > 1) {
			document.write(hours + " hours ago");
		} else if (hours === 1) {
			document.write(hours + " hour ago");
		} else if (minutes > 1) {
  		document.write(minutes + " minutes ago");
		} else if (minutes === 1) {
  		document.write(minutes + " minute ago");
		} else {
  		document.write("error");
		}
	</script>
