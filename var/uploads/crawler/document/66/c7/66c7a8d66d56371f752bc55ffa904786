		<li id="sourceCode" ondblclick="sourceCodeDblClicked()" ontouchstart="sourceCodeTouched(event)" ontouchend="sourceCodeReleased(event)" >
			<script language="javascript" type="text/javascript">
				var sourceCodeElementDiv = document.getElementById('display_source_code');
				var sourceCode = null;
				if (sourceCodeElementDiv != null) sourceCode = sourceCodeElementDiv.innerHTML;
				if (sourceCode) {
					var labelTmpl = 'Source Code: {0}';
					var sourceCodeMsg = labelTmpl.replace("{0}", sourceCode);
					document.write(sourceCodeMsg);
				}
			</script>
		</li>
	</ul>
