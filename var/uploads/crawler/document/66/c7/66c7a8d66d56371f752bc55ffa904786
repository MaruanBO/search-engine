		function createRegistrationTagIfValidEmail(email){
	 		if (validate_email_without_add_error(email)) {
	 			cmCreateRegistrationTag('69612905',email.value, null, null, null, null,null, null, null, null, null, null,null,null,null,null,null);
	 		}
		}
		function emailSignUpSubmit(form)
		{
			validator_reset();
			
			clear_instructions_emailSignUp();
	
			emailSignUp_frmvalidator.validator_submit();
	
			if (validation_display_errors('') == true){	
				/**
				* @author hsawalhi
				* Per WCSBD-807
				*/
				document.getElementById("emailSignUp_label").removeAttribute("style");
				
				cmCreateRegistrationTag('69612905',$('emailSignUp').value, null, null, null, null,null, null, null, null, null, null,null,null,null,null,null);
				return true;		      
			}
			return false;
		}
		
		var emailSignUp_frmvalidator = new Validator("EmailSignUpForm");
		emailSignUp_frmvalidator.addValidation("emailSignUp",true,"req","emailSignUp_label","Please enter your email address.");
		emailSignUp_frmvalidator.addValidation("emailSignUp",false,"email","emailSignUp_label","The email address you entered is invalid.");
	
	
