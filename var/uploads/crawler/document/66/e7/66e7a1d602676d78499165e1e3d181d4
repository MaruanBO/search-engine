        /*Ignore impression tags tracked by cm_sp parameters using the prefix TOP NAV.*/
        function cmCheckIgnoreImpression(sp,re,cr,me) {
            if(sp){
                sp = sp.match(/%20/i) ? (decodeURIComponent(sp)) : sp;
                if(sp.indexOf("TOP NAV-_-") > -1){
                    return false;
                }
            }
            return true;
