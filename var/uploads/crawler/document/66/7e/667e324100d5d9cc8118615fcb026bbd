        function recordOutboundLink(link, category, action) {
            ga("send", "event", category, action);
            setTimeout('window.parent.location = "' + link.href + '"', 100);
        }

        // Add legacy tracking for custom events and such, just in case.
        var _gaq = _gaq || {};
        if (!_gaq.push) {
            _gaq.push = function(opts) {
                var type;
                var category;
                var action;
                var number;

                if (opts.length > 0) {
                    type = opts[0];
                    category = opts[1] ? opts[1] : "";
                    action = opts[2] ? opts[2] : "";
                    number = opts[3] ? opts[3] : "";

                    ga("send", (type === "_trackEvent" ? "event" : "pageview"), category, action, number);
                }
            }
        }
