<div id="WNCol23" class="wnColConfig2 columns col2 col3"><div id="WNCol23Top"><!--Start of Display Size: Feature - Col 2/3A (29)--><div class="wnBlock displaySize displaySizeId29" id="DisplaySizeId29"><script>Worldnow.Helpers.debug('UB Automatic Newscast Live Link **DO NOT DELETE OR ALTER**(228458)', 'UB_228458');</script><div class="wnDVUtilityBlock"><script type = "text/javascript">

	// Script created by Eagle1295 for exclusive use by FOX4 KDFW. If you are smart enough to view this script to steal it, you are smart enough to change 
	// variable names and arrange it so it doesn't look quite as stolen. At least take some effort to cover your tracks.
	
	function getUTCWithOffset(UTC,TimeZoneOffset)
	{
		return ((TimeZoneOffset*(60*60*1000))+UTC);
	}
	
	function getUTCDayOfWeek(UTC)
	{
		return (Math.floor((UTC%(7*24*60*60*1000))/(24*60*60*1000))+4)%7;
	}
	
	function setUTCHoursFromBaseDay(UTC,hours,mins)
	{
		return ((UTC-(UTC%(24*60*60*1000)))+(hours*60*60*1000)+(mins*60*1000));
	}
	
	function getFOX4TimeZone(FOX4UTC)
	{
		
		var BaseTimeOffset = -6;
		var CalculatedTimeOffset = BaseTimeOffset;
		var FoxBadDate = new Date(FOX4UTC);
		var DSTBeginSeed = new Date(Date.UTC(FoxBadDate.getUTCFullYear(), 2, 1, 8, 0, 0, 0))
		var DSTEndSeed = new Date(Date.UTC(FoxBadDate.getUTCFullYear(), 10, 1, 8, 0, 0, 0))
		var DSTBeginUTC = DSTBeginSeed.getTime();
		var DSTEndUTC = DSTEndSeed.getTime();
		
		var DSTBeginSundayCount = 0;
		if(getUTCDayOfWeek(DSTBeginUTC)==0)
			DSTBeginSundayCount++;
		while(DSTBeginSundayCount<=1)
		{
			DSTBeginUTC+=(24*60*60*1000);
			if(getUTCDayOfWeek(DSTBeginUTC)==0)
				DSTBeginSundayCount++;
		}
		
		var DSTEndSundayCount = 0;
		if(getUTCDayOfWeek(DSTEndUTC)==0)
			DSTEndSundayCount++;
		while(DSTEndSundayCount<=0)
		{
			DSTEndUTC+=(24*60*60*1000);
			if(getUTCDayOfWeek(DSTEndUTC)==0)
				DSTEndSundayCount++;
		}
		
		if(FOX4UTC>=DSTBeginUTC&&FOX4UTC<=DSTEndUTC)
			CalculatedTimeOffset+=1
			
		return CalculatedTimeOffset;
	}
	
	function WeekDayImageRotation(UTCFOX4) 
	{
		var dispT = "Error encountered in Weekday rotation";
		
		// show Good Day image between 4:30 and 9 a.m.
		if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,4,30) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,9,0)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/goodday.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>Good Day on FOX 4</a></div>";
		
		// show Noon News image between noon and 12:30 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,12,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,12,30)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4's News at Noon</a></div>";
		
		// show FOX 4 News image between 5 and 6:30 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,17,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,18,30)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 Evening News</a></div>";
		
		// show FOX 4 News image between 9 and 10:30 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,21,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,22,30)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 News at 9</a></div>";
		
		// all other times show nothing
		else 
			dispT = "";
		
		return dispT;
	}
	
	function SaturdayImageRotation(UTCFOX4) 
	{
		var dispT = "Error encountered in Saturday rotation";
	
		// show Good Day image between 7 and 9 a.m.
		if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,7,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,9,0))  
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/goodday.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>Good Day on FOX 4</a></div>";
			
		// show FOX 4 News image between 6 and 7 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,18,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,19,0))  
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 Evening News</a></div>";
		
		// show FOX 4 News image between 9 and 10 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,21,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,22,0))  
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 News at 9</a></div>";
		
		// all other times show nothing
		else  
			dispT = "";
		
		return dispT;
	}
	
	function SundayImageRotation(UTCFOX4) 
	{
		var dispT = "Error encountered in Sunday rotation";
	
		// show Good Day image between 6 and 8 a.m.
		if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,6,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,8,0)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/goodday.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>Good Day on FOX 4</a></div>";
		
		// show FOX 4 News image between 5 and 6 p.m.
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,17,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,18,0)) 
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 Evening News</a></div>";
			
		// show FOX 4 News image between 9 and 10 p.m	
		else if (UTCFOX4 >= setUTCHoursFromBaseDay(UTCFOX4,21,0) && UTCFOX4 < setUTCHoursFromBaseDay(UTCFOX4,22,0))
			dispT = "<div id='liveNews' style='background:url(http://content.foxtvmedia.com/kdfw/live/news.jpg); width:654px; height:43px;'><a href='/category/264130/stream-1' style='display:block; padding:14px 210px 14px 160px; font: bold 14px Arial, Helvetica, sans-serif; color:#000000; text-decoration:none;'>FOX 4 News at 9</a></div>";
		
		// all other times show nothing
		else 
			dispT = "";
		
		return dispT;
	}
	
	function DisplayRotatingImage()
	{
		var TagOutput = "Error encountered in main";
	
		var localNowDate = new Date();
		var UTCNow = localNowDate.getTime() 
		var FOX4TimeZoneOffset = getFOX4TimeZone(UTCNow);
		var UTCFOX4 = getUTCWithOffset(UTCNow,FOX4TimeZoneOffset);		
		
		switch(getUTCDayOfWeek(UTCFOX4))
		{
			// Sunday
			case 0:
				TagOutput = SundayImageRotation(UTCFOX4); 
				break;
				
			// Saturday
			case 6:
				TagOutput = SaturdayImageRotation(UTCFOX4); 
				break;
				
			// Weekday
			default:
				TagOutput = WeekDayImageRotation(UTCFOX4); 
		}
		
		document.getElementById('imgDisplay').innerHTML = TagOutput;
	}
