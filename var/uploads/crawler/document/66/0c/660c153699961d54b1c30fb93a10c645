<script type="text/javascript">
	googletag.cmd.push(function() {
		var slotid='div-gpt-mpu-4';
		var slotrid='div-gpt-mpu-r-4';
		if(true) {
			slottag = ads.adverttag + '_adSlot4';
		}
		else {
			slottag = ads.adverttag;
		}

		slottag = slottag.toLowerCase();
		ads.toggslots.push(slotid);
		ads.toggslots.push(slotrid);

		if (Modernizr.width >= 400 && Modernizr.width < 960 && $('#'+slotrid).length > 0) {
			slotid =  slotrid;
			$('#'+slotid).toggleClass("hide");
		
		}		
		
		if (Modernizr.width >= 400 ) {
			 googletag.defineSlot(slottag, [[300,250],[300,600]], slotid).addService(googletag.pubads());
		     googletag.display(slotid);
		}
		if($('#'+slotrid).length<=0){
			return;
		}
		
		ads.refslots.push(function() {
			slottag = ads.adverttag + '_adSlot4';
			slottag = slottag.toLowerCase();

			var slotid='div-gpt-mpu-4';
			var slotrid='div-gpt-mpu-r-4';
			
			if (Modernizr.width >= 400 && Modernizr.width > 960) {
				slotrid =  slotid;
				$('#'+slotrid).toggleClass("hide");
				}
			
			if($('#'+slotrid).find('iframe').length>0){
				return;
			}
			if (Modernizr.width >= 400 ) {
				var slot = googletag.defineSlot(slottag, [[300,250],[300,600]], slotrid).addService(googletag.pubads());
				googletag.display(slotrid);
				googletag.pubads().refresh([slot]);
			}
		});
	});
</script>
	






