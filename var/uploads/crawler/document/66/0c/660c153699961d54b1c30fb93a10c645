		/* execute Ads based on the cookie value*/
		if((null != user.getCookie("UserId") && "" != user.getCookie("UserId")) || custType ||
				getCookieStatus("XLOGIN") ||
				getCookieStatus("YREDIRECT") ||
				getCookieStatus("EMAIL_SP")){
			recognised = true;				
		}
		var showAds = false;
		if("loggedout" == 'loggedin' && !recognised){
			showAds = true;
		} else if("loggedin" == 'loggedin' && recognised){
			showAds = true;
		} else if("all" == 'loggedin'){
			showAds = true;
		}
		
