	<script type="text/javascript">	
		$(".phonebook").find("#Business").click(function () {
		    var businesstype = $(".phonebook").find('#BusinessType').val();
		    var businessname = $(".phonebook").find('#BusinessName').val();
		    var location = $(".phonebook").find('#Location').val();
		    var url = 'http://www.thephonebook.bt.com/'; 
		    if ($(".phonebook").find('#BusinessType').val() != 'Business type e.g. Florist etc') {
		        url = 'http://www.thephonebook.bt.com/publisha.content/en/search/business_by_type/search.publisha?BusinessType='+businesstype.replace(/[^\w\s]/gi, '').replace(/\s/g, '-')+"&TownOrPostcode="+location.replace(/[^\w\s]/gi, '').replace(/\s/g, '-'); 
	
		        $(".phonebook").find("#Business").attr("href", url);
		    } else if ($(".phonebook").find('#BusinessName').val() != 'Business name e.g. Interflora etc') {
		        url = 'http://www.thephonebook.bt.com/publisha.content/en/search/business_by_name/search.publisha?BusinessName='+businessname.replace(/[^\w\s]/gi, '').replace(/\s/g, '-')+"&Location="+location.replace(/[^\w\s]/gi, '').replace(/\s/g, '-');
	
		        $(".phonebook").find("#Business").attr("href", url);
		    } else {
		    	url = 'http://www.thephonebook.bt.com/publisha.content/en/search/business_by_type/search.publisha?BusinessType='+businesstype.replace(/[^\w\s]/gi, '').replace(/\s/g, '-')+"&TownOrPostcode="+location.replace(/[^\w\s]/gi, '').replace(/\s/g, '-');
		        $(".phonebook").find("#Business").attr("href", url);
		    }
		});
		$(".phonebook").find("#Residential").click(function () {
			var surname = $(".phonebook").find('#Surname').val(); 
			var reslocation = $(".phonebook").find('#res_location').val();
			url='http://www.thephonebook.bt.com/publisha.content/en/search/residential/search.publisha?Surname='+surname.replace(/[^\w\s]/gi, '').replace(/\s/g, '-')+"&Location="+reslocation.replace(/[^\w\s]/gi, '').replace(/\s/g, '-');
			$(".phonebook").find("#Residential").attr("href",url);
		});
		$('#BusinessType').val("Business type e.g. Florist etc");
		$('#BusinessName').val("Business name e.g. Interflora etc");
		$('#Location').val("Location e.g. BRI 3JA");
		$('#Surname').val("Residential name e.g. Smith");
		$('#res_location').val("Location e.g. BRI 3JA, Bradford");
	</script>		

