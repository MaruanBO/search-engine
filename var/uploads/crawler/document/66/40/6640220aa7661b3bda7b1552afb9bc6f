<script type="text/javascript">
var t;	
var hasWeatherAlerts = false;
var hasBreakingNewsAlerts = false;
var completed = 0;
var numRequests = 2;
var alerts = $('<div id="alerts_ticker_clip"></div>');
  
$(document).ready(function(){
  weather = [];
  ul = $('<ul id="weather_alerts"></ul>');
  if(weather.length > 0){
    $.each(weather, function(i, item){
      counties = "";
      $.each(item.COUNTIES, function(j, county){
        counties += "<span>"+county+"</span>";
      });
      ul.append($.nano('<li><a href="/pages/weather-alerts/?id={id}" title="Alert {index} of {totalItems}">{title} in effect for: {counties}</a></li>', {
           id: item.ID
           ,title: item.TITLE
           ,index: (i+1)
           ,totalItems: weather.length
           ,counties: counties
      }));
    });
    hasWeatherAlerts = true;
  }
  alerts.append(ul);

  breaking = [{"PERMALINK":"news\/education-leaders-discuss-idea-of-free-community-college","TITLE":"Education leaders discuss idea of free community college"},{"PERMALINK":"news\/st-louis-police-officers-fatally-shoot-man-armed-with-gun-64907","TITLE":"St. Louis police: Officers fatally shoot man armed with gun"},{"PERMALINK":"news\/callaway-county-suspects-arrested-for-drugs","TITLE":"Callaway County Suspects Arrested for Drugs"},{"PERMALINK":"news\/schnucks-recalls-devil-s-food-cake","TITLE":"Schnucks Recalls Devil's Food Cake"},{"PERMALINK":"news\/mexico-man-sentenced-in-sexual-assault-case","TITLE":"Mexico Man Sentenced in Sexual Assault Case"}];
  
  breaking = 0;
  ul = $('<ul id="breaking_news_alerts"></ul>');
  if(breaking.length > 0){
    $.each(breaking, function(i, item){
      ul.append($.nano('<li><a href="/{permalink}/" title="{title}">{title}</a></li>', {
           title: item.TITLE
           ,permalink: item.PERMALINK
      }));
    });
    hasBreakingNewsAlerts = true;
  }
  alerts.append(ul);

  if(hasWeatherAlerts || hasBreakingNewsAlerts){
    if(hasWeatherAlerts && hasBreakingNewsAlerts){
      sections = [
          {id: 'breaking_news_alerts', title: 'Breaking News'}
          ,{id: 'weather_alerts', title: 'Weather Alerts'}
      ];
    }
    else if(hasWeatherAlerts){
      sections = [{id: 'weather_alerts', title: 'Weather Alerts'}];
    }
    else if(hasBreakingNewsAlerts){
      sections = [{id: 'breaking_news_alerts', title: 'Breaking News Alerts'}];
    }
    $('#alerts_ticker').append(alerts);
    $('#alerts_ticker').css('display', 'block').animate({top: 0}, 750, function(){
      t = $('#alerts_ticker_clip').ticker({
        sections: sections
        ,progressBarWidth: 186
      });				
      t.start();
      t.loadElement();
    });
  }
});
</script> 
