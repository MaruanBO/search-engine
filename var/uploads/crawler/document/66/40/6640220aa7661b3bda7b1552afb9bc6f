    var newsfader_index = 1;
    var newsfader_interval = 0;
    var newsfader_json = [{"THUMB":"5ee26116e4acfdd7d61f604bdf55c8ad.thumb.jpg","ID":66017,"FILE":"5ee26116e4acfdd7d61f604bdf55c8ad.jpg","URL":"\/news\/holts-summit-police-arrest-explosive-device-suspsect","CREATED":"February, 19 2015 11:36:16","TITLE":"Charges expected in Holts Summit explosive device case"},{"THUMB":"58275145b1f4464ee6aca21d67b6f5ee.thumb.jpg","ID":66010,"FILE":"58275145b1f4464ee6aca21d67b6f5ee.jpg","CREATED":"February, 19 2015 09:10:06","URL":"\/news\/columbia-car-chase-ends-in-fulton","TITLE":"Columbia car chase ends in arrest"},{"THUMB":"58190d38a58b85e81aa09fb67ef6b212.thumb.jpg","ID":66006,"FILE":"58190d38a58b85e81aa09fb67ef6b212.jpg","URL":"\/news\/columbia-police-prepare-for-continued-mardi-gras-festivities-66006","CREATED":"February, 19 2015 07:13:34","TITLE":"Columbia police prepare for continued Mardi Gras festivities"},{"THUMB":"5814edbadd32fa7765edbe3148673256.thumb.jpg","ID":65999,"FILE":"5814edbadd32fa7765edbe3148673256.jpg","CREATED":"February, 19 2015 01:44:01","URL":"\/news\/jefferson-city-officials-to-discuss-possible-tax-increase","TITLE":"Jefferson City officials to discuss possible tax increase"}];

    function newsfader_switch(id) {
      $("li#newsfader-link-" + newsfader_index).css("background-color", "transparent");
      $("li#newsfader-link-" + newsfader_index + " a").animate({opacity: 0.5});
      $("a#newsfader-dot-" + newsfader_index).css("color", "#aaa");
      if(++newsfader_index > 4) { newsfader_index = 1; }
      if(id) { newsfader_index = id; }
      $("li#newsfader-link-" + newsfader_index).css("background-color", "#fffcb0");
      $("li#newsfader-link-" + newsfader_index + " a").animate({opacity: 1});
      $("a#newsfader-dot-" + newsfader_index).css("color", "#b00");
      $("div#newsfader > a").attr("href", newsfader_json[newsfader_index - 1].URL);
      $("div#newsfader > a img").attr("src", "/images/newsfader/" + newsfader_json[newsfader_index - 1].FILE);
      clearInterval(newsfader_interval);
      newsfader_interval = setInterval(function() { newsfader_switch(); }, (id ? 10000 : 4000));
      return false;
    }

    $(document).ready(function() {
      $("div#newsfader li a").css("opacity", "0.5");
      $("div#newsfader li:first-child a").css("opacity", "1");
      $("div#newsfader li:first-child").css("background-color", "#fffcb0");
      $("div#newsfader > div a:first-child").css("color", "#b00");
      newsfader_interval = setInterval(function() { newsfader_switch(); }, 4000);
    });
    </script>
  


