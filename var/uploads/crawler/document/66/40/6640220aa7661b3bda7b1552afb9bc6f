<script type="text/javascript">
function promo_scroller_timer() {
  return setInterval(function() {
    $("#promo-scroller-list li:first-child").slideUp(function() {
      $(this).appendTo($("#promo-scroller-list")).slideDown();
    });
  }, 5000);
}

function promo_scroller_next() {
  clearInterval(PROMO_SCROLLER_TIMER);
  $('#promo-scroller-list li:first').hide().appendTo('#promo-scroller-list').show()
  PROMO_SCROLLER_TIMER = promo_scroller_timer();
  return false;
}

function promo_scroller_prev() {
  clearInterval(PROMO_SCROLLER_TIMER);
  $('#promo-scroller-list li:last').hide().prependTo('#promo-scroller-list').show()
  PROMO_SCROLLER_TIMER = promo_scroller_timer();
  return false;
}

var PROMO_SCROLLER_TIMER = promo_scroller_timer();
</script>

