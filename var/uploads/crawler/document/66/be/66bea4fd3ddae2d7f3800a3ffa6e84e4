<script>
function getTallest(group) {
   tallest = 0 ;
   group.each(function() {
      var anchorLink = $(this).find('.panel-inner-box-six-rows').next('.panel-link').height();
      // add 4px padding to non quick-links 
      if (anchorLink !== null ) anchorLink = anchorLink + 4 ;
      thisHeight = $(this).height() + anchorLink;
      
      if(thisHeight > tallest) {
         tallest = thisHeight;
      }
   });
   return tallest ;
}
function equalHeight(group,buffer) {

   maxHeight = getTallest(group);
   group.height(maxHeight + buffer);
}
$(document).ready(function() {

   equalHeight($(".topgrid"), 2);
   equalHeight($(".carousel"), 2);

   //align anchor links
   var buffer = 54 ;
   var tl = getTallest($(".topgrid:not('.quick-links') .panel-inner-box-six-rows"));

   var notQuickLink = $(".topgrid:not('.quick-links') .panel-link") ;
   notQuickLink.css('top',tl+buffer+'px');
   notQuickLink.css('bottom','0px');
   
});
</script>

