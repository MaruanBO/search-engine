<script type="text/javascript">

function postEmail()
{
  var messageHold = document.getElementById("thankyou");
  var atpos = $('#emailInput').val().indexOf("@");
  var dotpos = $('#emailInput').val().lastIndexOf(".");
  
  if ($('#emailInput').val() === '' || $('#emailInput').val().length == 0 ) 
  {
	messageHold.innerHTML = "Please fill in e-mail";
	return false;
  }
  if (atpos<1 || dotpos<atpos+2 || dotpos+2>=$('#emailInput').val().length) 
  {
	messageHold.innerHTML = "Please enter a valid e-mail";
	return false;
  }
  
  var em = document.getElementById('emailInput').value;
  var url = '/support/emailPost.jsp';
	$.ajax({
		type: "POST",
	    url: url,
	    data: "email="+em, // serializes the form's elements.
	    success: function(data)
	    	{ messageHold.innerHTML = "Thank You!" },
		error: function (jqXHR, textStatus, errorThrown)
			{ messageHold.innerHTML = "Sorry. Please try again later." }
   });
}

$(document).ready(function(){

	// clearing input on focus
	$('#emailInput').live('focus', function()
	{
		if($(this).val()==$(this).attr('title'))
		{
			$(this).val('');
		}
	});
	//restoring input value on blrr if the input is left blank
	$('#emailInput').live('blur', function()
	{
		if($(this).val()=='')
		{
			var inputtitle=$(this).attr('title');
			$(this).val(inputtitle);
		}
	});			
});

</script>

