                        curobj.click(function () {
                            curobj.toggleClass('minusicon');
                            var curobjclass = (curobj.attr('class').indexOf('minusicon') > 0) ? 'minusicon' : 'plusicon';
                            $('.tfhmenubar ul.dropdown-menu').prev().removeClass('minusicon');
                            $('.tfhmenubar ul.dropdown-menu').prev().addClass('plusicon');
                            curobj.addClass(curobjclass);
                        });
                    });
                }
            }

            /* Initialise the Scroller for similar recipes */
            if (typeof (RD) != 'undefined' && typeof (RD.Widget) != 'undefined' && typeof (RD.Widget.Scroller) != 'undefined') {
                scroller = new RD.Widget.Scroller({
                    parentEl: document.getElementById("divSimilarProjects"),
                    /* No animation initially */
                    animation: true
                });
            }

            /* Initialise the Scroller for similar recipes */
            if (typeof (RD) != 'undefined' && typeof (RD.Widget) != 'undefined' && typeof (RD.Widget.Scroller) != 'undefined') {
                scroller = new RD.Widget.Scroller({
                    parentEl: document.getElementById("topvideoscroller"),
                    /* No animation initially */
                    animation: true
                });
            }

            $('#homemarqueecont').on('slid.bs.carousel', function () {
                window.adjustmarqueearrow();
            });

            slidepagecounter = 1;
            window.updateslidecounter();
            $('#slideshowcont').on('slid.bs.carousel', function () {
                setTimeout(function () { window.updateslidecounter() }, 0);
                slidepagecounter++;
                window.refreshad();
                return false;
            });


            $('#viewalltext').on('click', function () {
                window.showviewallthumbs();
            });
            $('#baktoslids').on('click', function () {
                window.showslideshows();
            });
            $('#viewallcont .imgthumb').on('click', function () {
                var parentobj = $('#viewallcont');
                var selectedindex = $(".imgthumb ", parentobj).index(this);
                window.showslideshows();
                $('#slideshowcont').carousel(selectedindex);
                return false;
            });


        });

        function showviewallthumbs() {
            $('#viewallcont').show();
            $('#slideshowcont').hide();
        }
        function showslideshows() {
            $('#viewallcont').hide();
            $('#slideshowcont').show();
        }
        function updateslidecounter() {
            var totalSlides = $('#slideshowcont .carousel-inner .item').length;
            var parentobj = $('#slideshowcont .carousel-inner');
            if (parentobj.length) {
                var activeSlide = $("div.active", parentobj).index();
                $('#slidecounter').text((activeSlide + 1) + ' of ' + totalSlides);
                if (activeSlide == 0) {
                    $('#slideshowcont .left.carousel-control').hide();
                } else {
                    $('#slideshowcont .left.carousel-control').show();
                }
                if (activeSlide == (totalSlides - 1)) {
                    $('#slideshowcont .right.carousel-control').hide();
                } else {
                    $('#slideshowcont .right.carousel-control').show();
                }
            }
        }

        function refreshad() {
            if (((slidepagecounter - 1) % 3) == 0) {
                googletag.pubads().refresh();
            }
        }

        if (window.addEventListener)
            window.addEventListener("load", adjustmobilead, false);
        else if (window.attachEvent)//IE
            window.attachEvent('onload', adjustmobilead);

        window.onresize = adjustmobilead;

        function adjustmobilead() {
            if (($(window).width() < 350)) {
                $("div.AdBlock iframe").contents().find("a img").css({ 'width': '90%', 'margin': '0 auto', 'display': 'block' });
            }
            adjustbrandad();
            adjustmarqueearrow();
        }

        function adjustbrandad() {
            var bodyobj = document.getElementsByTagName("BODY")[0];
            var responsivecss = document.getElementById('theme-responsive');
            if ($(window).width() > 1024) {
                if (bodyobj && responsivecss) {
                    bodyobj.style.width = '990px';
                    bodyobj.style.margin = '0 auto';
                    //responsivecss.disabled = true;
                    $('.container').css('width', '990px');
                }
            } else {
                bodyobj.style.width = '100%';
                responsivecss.disabled = false;
                $('.container').css('width', 'inherit');
            }
        }
        function adjustmarqueearrow() {
            var marqueeimg = $('#homemarqueecont .item.active .marqueeimg');
            var arrowcontrol = $('#homemarqueecont .carousel-control');
            var arrowcontrolleft = $('#homemarqueecont .carousel-control.left');
            var arrowcontrolright = $('#homemarqueecont .carousel-control.right');
            var marqueeslideimg = $('#homemarqueecont .item.active .marqueeimg img');
            var marqueeindicator = $('#homemarqueecont .marqueeindicator');
            if (!marqueeimg[0]) {
                //Ad slide               
                var marqueeimg = $('#homemarqueecont .item.active .thirdslide');
            }
            if (!marqueeimg[0]) {
                return;
            }
            var calctop = (marqueeimg[0].offsetTop) + ((marqueeimg.height() / 2));
            var calcright = (marqueeimg.width()) - (marqueeslideimg.width());
            var indicatortop = (marqueeimg[0].offsetTop) + ((marqueeimg.height() + 10));
            arrowcontrol.css(
                {
                    'top': (calctop - 15)
                }
            );
            arrowcontrolleft.css(
                {
                    'left': (marqueeimg[0].offsetLeft + 15)
                }
            );
            arrowcontrolright.css(
                {
                    'right': calcright
                }
            );
            marqueeindicator.css(
               {
                   'right': calcright,
                   'top': indicatortop
               }
           );
        }

    </script>
</body>

</html>
";i:2;s:3:"690";}