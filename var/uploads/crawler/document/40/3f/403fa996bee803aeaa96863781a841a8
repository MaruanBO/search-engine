                        <script language="javascript" type="text/javascript">
                            var songInfo = null;
                            var showInfo = null;
                            var broadcastService = '//cdn.kloveair1.com/services/broadcast.asmx';

                            function UpdateNowPlaying() {
                            
                                var url = jmsajaxurl({
                                    url: broadcastService,
                                    method: "GetRecentSongs",
                                    data: { SiteId: 1 }
                                });

                                $.ajax({
                                    url: url + "&format=json",
                                    jsonpCallback: "GetRecentSongs",
                                    dataType: "jsonp",
                                    cache: true,
                                    success: function(msg) {

                                        songInfo = $(msg.d)

                                        if (songInfo.length > 0) {

                                            $(".area2 a.songLink").html(ShortenString(songInfo[0].Title, 18));

                                            if(songInfo[0].Lyrics != "")
                                                $(".area2 a.songLink").attr('href', songInfo[0].Lyrics);
                                            else
                                                $(".area2 a.songLink").attr('href', songInfo[0].ArtistLink);

                                            $(".area2 a.artistLink").html(ShortenString(songInfo[0].Artist, 25));
                                            $(".area2 a.artistLink").attr('href', songInfo[0].ArtistLink);
                                        }

                                        setTimeout("UpdateNowPlaying()", 30000);
                                    }
                                });
                            }

                           function UpdateShowInfo() {

                                var url = jmsajaxurl({
                                    url: broadcastService,
                                    method: "GetCurrentShow",
                                    data: { SiteId: 1 }
                                });

                                $.ajax({
                                    url: url + "&format=json",
                                    jsonpCallback: "GetCurrentShow",
                                    dataType: "jsonp",
                                    cache: true,
                                    success: function(msg) {

                                        showInfo = $(msg.d)

                                        if (showInfo.length > 0) {

                                            if (showInfo[0].Title != undefined && showInfo[0].Title.toLowerCase() != 'n/a') {
                                                $(".area2 .OnAir").show();
                                                $(".area2 a.showLink").html(showInfo[0].Title);
                                                $(".area2 a.showLink").attr('href', showInfo[0].Link);
                                              
                                                
                                            }
                                            else
                                                $(".area2 .OnAir").hide();

                                            if (showInfo[0].ImageName != undefined && showInfo[0].Title != undefined && showInfo[0].Title.toLowerCase() != 'n/a') {
                                                $(".area1 img.showImage").show();
                                                $(".area1 img.showImage").attr("src", '//cdn.klove.com/klove/images/shows/' + showInfo[0].ImageName + '.png');
                                                
                                                $(".area1 a.showImageLink").attr('title', showInfo[0].Title);
                                                $(".area1 a.showImageLink").attr('href', showInfo[0].Link);
                                                
                                            }
                                            else
                                                $(".area1 img.showImage").hide();
                                        }

                                        setTimeout("UpdateShowInfo()", 30000);
                                    }
                                });
                            }

                            $(document).ready(function () {
                                //Only update Now Playing if client is not on an SSL page...
                                if (window.location.protocol != "https:") {
                                    setTimeout("UpdateNowPlaying()", 25000);
                                    setTimeout("UpdateShowInfo()", 25000);
                                }
                            });   
                                                       
                            $(window).load(function(){$("#ctl00_ctl00_bodyMaster").live('click',function(e){var target=e.target,$target=$(target),link,title;if(target.nodeName==='A'){if($target.hasClass('ga-outbound-tracking-link')){link=($target.attr('href').indexOf("?h=",0)!=-1)?unescape($target.attr('href').substring($target.attr('href').indexOf("?h=",0)+3,$target.attr('href').length)):$target.attr('href'),title=$target.attr('title');}}else if(target.nodeName==='SPAN'){if(target.parentNode.nodeName==='A'&&$target.parent().hasClass('ga-outbound-tracking-link')){link=($target.parent().attr('href').indexOf("?h=",0)!=-1)?unescape($target.parent().attr('href').substring($target.parent().attr('href').indexOf("?h=",0)+3,$target.parent().attr('href').length)):$target.parent().attr('href'),title=$target.parent().attr('title');}}if(link&&link.length>0){_gaq.push(['_trackPageview',window.location.pathname+'/title/'+title+'/outbound/'+link]);}});});

                        </script>
