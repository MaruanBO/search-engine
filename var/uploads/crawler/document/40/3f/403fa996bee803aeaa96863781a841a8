        <div id="listenOnlineRC">
            
            <script language="javascript" type="text/javascript">

                function UpdateListenRC() {

                    if (songInfo != null) {
                        if (songInfo.length > 0) {
                            $("#listenOnlineRC a.songRCLink").html(ShortenString(songInfo[0].Title, 17));

                            if (songInfo[0].Lyrics != "")
                                $("#listenOnlineRC a.songRCLink").attr('href', songInfo[0].Lyrics);
                            else
                                $("#listenOnlineRC a.songRCLink").attr('href', songInfo[0].ArtistLink);

                            $("#listenOnlineRC a.artistRCLink").html(ShortenString(songInfo[0].Artist, 25));
                            $("#listenOnlineRC a.artistRCLink").attr('href', songInfo[0].ArtistLink);
                        }
                    }

                    if (showInfo != null) {
                        if (showInfo.length > 0) {

                            if (showInfo[0].Title != undefined && showInfo[0].Title.toLowerCase() != 'n/a') {
                                $("#listenOnlineRC .OnAir").show();
                                $("#listenOnlineRC a.showRCLink").html(showInfo[0].Title);
                                $("#listenOnlineRC a.showRCLink").attr('href', showInfo[0].Link);
                            }
                            else
                                $("#listenOnlineRC .OnAir").hide();
                        }
                    }

                    setTimeout("UpdateListenRC()", 30000);
                }

                $(document).ready(function () {
                    setTimeout("UpdateListenRC()", 30000);
                });   
                                  
            </script>
            <a id="ctl00_ctl00_cphMain_cphRightColumn_DropZone2_columnDisplay_ctl00_controlcolumn_ctl02_WidgetHost_WidgetHost_widget_ucPlayerLink_hypPlayerLaunchLink" onclick="launchPlayer();return false;" href="/listen/"><img id="ctl00_ctl00_cphMain_cphRightColumn_DropZone2_columnDisplay_ctl00_controlcolumn_ctl02_WidgetHost_WidgetHost_widget_ucPlayerLink_imgPlayerLaunchImage" src="//cdn.klove.com/klove/images/listen-online-btn2.gif" alt="Listen Online" /></a>
            <p>
                Now Playing:
