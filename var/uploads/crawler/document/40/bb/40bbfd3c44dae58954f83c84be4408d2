$(".typeahead").on("typeahead:selected typeahead:autocompleted", function(e,datum) { 
console.log(datum.value);
var Type;


	if(datum.tokens["Zip"]){
		$('#field_Zip').val(datum.tokens["Zip"]);
		Type = 'Zip';
	}

	if(datum.tokens["State"]){
		$('#field_State').val(datum.tokens["State"]);
		Type = 'State';
	}
	
	if(datum.tokens["City"]){
		$('#field_City').val(datum.tokens["City"]);
		Type = 'City';
	}
	
	if(datum.tokens["PropertyID"]){
		$('#field_PropertyID').val(datum.tokens["PropertyID"]);
		$('#field_Name').val(datum.tokens["Name"]);

		Type = 'ha';
	}
	
	if(datum.tokens["County"]){
		$('#field_County').val(datum.tokens["County"]);
	}
	



$('#field_Type').val(Type);

});


$('#toggle-pagination').click(function(){
	
	
	if($(this).html() == '...')
		$(this).html('&raquo; &laquo;');
	else
		$(this).html('...');
	
	$('.toggle-pagination').each(function(){
		
		if($(this).hasClass('hide'))
			$(this).removeClass('hide');
		else
			$(this).addClass('hide');
			
	});


});


$('.nav-search .twitter-typeahead').removeClass('col-lg-10').addClass('col-xs-12').css('marginRight', '0px').css('paddingRight', '0px');



});


	
		
</script>


	



