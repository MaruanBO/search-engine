<script type="text/javascript">
    window.fbAsyncInit = function() {
        FB.init({
            appId      : '207764379254184', // App ID
            channelURL : '//www.craftsy.com/channel.html', // Channel File
            status     : true, // check login status
            cookie     : true, // enable cookies to allow the server to access the session
            oauth      : true, // enable OAuth 2.0
            xfbml      : true  // parse XFBML
        });

        FB.Event.subscribe('edge.create', function(response) {
            //_gaq.push(['_trackSocial', 'Facebook', 'Like', 'null']);
        });
        FB.Event.subscribe('edge.remove', function(response) {
            //_gaq.push(['_trackSocial', 'Facebook', 'Unlike', 'null']);
        });
        FB.Event.subscribe('message.send', function(response) {
            //_gaq.push(['_trackSocial', 'Facebook', 'Share', 'null']);
        });
        FB.Event.subscribe('auth.login', function(response){
            if ( response && response.status && response.status === "connected" )
            {
                var facebookUserId = 0,
                    authResponse = response.authResponse;
                if ( authResponse && authResponse.userID )
                {
                    facebookUserId = authResponse.userID;
                }
                if ( facebookUserId !== 0 )
                {
                    $.post('/facebook/connected', { facebookUserId: facebookUserId }, function(){});
                }
            }
        });

        
        Craftsy.user.facebookStatus = '';
        
        Craftsy.social.getFacebookLoginStatus();
    };
    (function() {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol +
                '//connect.facebook.net/en_US/all.js';
        document.getElementById('fb-root').appendChild(e);
    }());

    Craftsy.tmplHash = "1424298232786";
</script>

