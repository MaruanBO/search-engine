            $(document).ready(function() {
                var $reg_modal = $('#reg-modal'),
                    is_mobile = typeof(IS_MOBILE_SITE) !== 'undefined' && IS_MOBILE_SITE,
                    $page = $('#page');

                $reg_modal
                    .fadeIn('fast', function() {
                        setTimeout(function() {
                            $('a.close', $reg_modal).fadeIn('fast');
                        }, 3000);
                    })
                    .find('a.close')
                    .click(function(e) {
                        e.preventDefault();
                        $reg_modal.fadeOut('fast');
                        if(is_mobile) {
                            $page.css({
                                height: 'auto',
                                overflow: 'visible'
                            });
                        }
                        if($reg_modal.data('org-lang') !== $reg_modal.data('cur-lang')) {
                            location.reload();
                        }
                    })
                    .end()
                    .find('a.switch-lang')
                    .click(function(e) {
                        e.preventDefault();

                        $reg_modal
                            .toggleClass('spanish-lang')
                            .find('.btn-email')
                            .each(function() {
                                var $this = $(this);
                                if($reg_modal.hasClass('spanish-lang')) {
                                    $this.attr('href', $this.attr('href') + '&user_lang=es');
                                    $reg_modal.data('cur-lang', 'es');
                                } else {
                                    $this.attr('href', $this.attr('href').replace(/\&user_lang\=es/g, ''));
                                    $reg_modal.data('cur-lang', 'en');
                                }
                            })
                            .end()
                            .find('*[data-switch-lang]')
                            .each(function() {
                                var $this = $(this),
                                    alt_lang = $this.data('switch-lang');

                                $this.data('switch-lang', $this.html());
                                $this.html(alt_lang);
                            });

                        var translate_lang = $reg_modal.hasClass('spanish-lang') ? 'es' : 'en';
                        $.get('/actions/set_user_lang.php?user_lang=' + translate_lang + '&ajax=true', function() {});
                    });

                if(is_mobile) {
                    var update_modal_positioning = function() {
                        if($(window).height() <= $('.inner', $reg_modal).height()) {
                            $reg_modal.css('position', 'absolute');
                            var page_height = $('.inner', $reg_modal).height();
                        } else {
                            $reg_modal.css('position', 'fixed');
                            var page_height = $(window).height();
                        }

                        $page.css({
                            height: page_height + 'px',
                            overflow: 'hidden'
                        });
                    };

                    if(window.addEventListener) {
                        window.addEventListener('orientationchange', update_modal_positioning);
                    }
                    update_modal_positioning();
                }
            });
        
    </script>
