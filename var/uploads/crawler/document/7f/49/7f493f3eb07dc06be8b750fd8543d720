	</div><script type="text/javascript">
var cur_home_slide = 0;
var cur_home_time = 5000;
var cur_home_int;
var home_carousel;

$(document).ready(function(){

	$('#posts-list').jcarousel({
		scroll: 5,
		animation: 500,
		visible: 5,
		itemFirstInCallback: carouselTrigger
	});
	home_carousel = $("#posts-list").data("jcarousel");
	
	cur_home_int = setInterval(swapSlides, cur_home_time);	
	
	$('#posts-list > li:first-child').addClass('active');  
	$('#posts-list > li').hover(function(){
		slideHover(this);
			
		var posts = $('#posts-list > li');
		clearInterval(cur_home_int);		
		cur_home_slide = posts.index(this);
		
	}, function(){
		cur_home_int = setInterval(swapSlides, cur_home_time);		
	});
});

function slideHover(obj){
	var dataid = $(obj).data("id");	
	var datastr = "#"+dataid;
	$(obj).parent().find('.active').removeClass('active');
	$(obj).addClass('active');
	$(".posts-title-list li").css("display", "none");
	$(".posts-title-list").find(datastr).css("display", "block");
}
	
function swapSlides(){
	var posts = $('#posts-list > li');
	cur_home_slide++;
	
	if(posts.length < cur_home_slide + 1){
		cur_home_slide = 0;
		home_carousel.scroll(0);
	}
		
	if(cur_home_slide % 5 === 0)
		home_carousel.next();
		
	slideHover($('#posts-list > li:eq('+cur_home_slide+')').get());
}

function carouselTrigger(carousel, item, idx, state) {
	idx--;
	
	if(cur_home_slide >= idx && cur_home_slide < idx + 5){		
		return;
	}
	
    var slide = $('#posts-list > li:eq('+idx+')').get();
    slideHover(slide);			
	cur_home_slide = idx;    
};
</script>

