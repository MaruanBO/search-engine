	<script>

var currentFrame = 0,
	dlDelay = 5000,
	dlTimer,
	dlWidth,
	mobileDevice = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent),
	newsDlInitialized = false,
	resizeTimeout;

function activateDot() {
	jQuery(".dl-dot.active").removeClass("active");
	jQuery(".dl-dot:eq("+currentFrame+")").addClass("active");
	startTimer();
}
function animateDL(input) {
	if (typeof input == "string") {
		var direction = input;
		var offset = direction == "left" ? -dlWidth : dlWidth;
		if (direction == "left" && currentFrame-1 == -1) {
			jQuery("#dl-frames").css("left",-((jQuery(".dl-frame").length-1)*dlWidth));
			currentFrame = jQuery(".dl-frame").length-1;
			activateDot();
		} else if (direction == "right" && currentFrame+1 == jQuery(".dl-frame").length) {
			jQuery("#dl-frames").css("left",0);
			currentFrame = 0;
			activateDot();
		} else {
			jQuery("#dl-frames").css("left",jQuery("#dl-frames").position().left-offset);
			direction == "left" ? currentFrame-- : currentFrame++;
			activateDot();
		}
	} else {
		jQuery("#dl-frames").css("left",-(input*dlWidth));
		currentFrame = input;
		activateDot();
	}
}
function startTimer() {
	if (dlTimer) {
		clearInterval(dlTimer);
	}
	dlTimer = setInterval(function() {
		animateDL("right");
	},dlDelay);
}
jQuery(function() {
	if ($("body").width() < dlWidth) {
		$(".dl-contents").addClass("scrollbar-fix");
		if ($("#dl-ad").length > 0) {
			$("#dl-ad").attr("src",$("#dl-ad").attr("src")+"&sbf=1");
		}
	}
	if (!mobileDevice) {
		var s = document.createElement("script");
		s.src = "http://api.moviefone.com/v2/js/mf2-gravity-trending.js";
		jQuery("body").append(s);
	}
	jQuery(window).resize(function() {
		if (resizeTimeout) {
			clearTimeout(resizeTimeout);
		}
		resizeTimeout = setTimeout(function() {
			dlWidth = window.innerWidth > 1024 && jQuery("body").hasClass("gutters") ? jQuery("#mf2-home-dl-new").width() : window.innerWidth;
			if ($("#dl-ad").length > 0) {
				try {
					$("#dl-ad").get(0).contentWindow.resizeAd(window.innerWidth);
				} catch (error) {} 
			}
			jQuery("#dl-frames").css("width",jQuery(".dl-frame").length*dlWidth);
			jQuery(".dl-frame").css("width",dlWidth);
			jQuery("#dl-frames").css("left",-(currentFrame*dlWidth));
			jQuery("#mf2-home-dl-new").css("visibility","visible");
			if (!mobileDevice) {
				jQuery("#mf2-home-dl-new").unbind().mouseenter(function() {
					jQuery(".dl-control").show();
				}).mouseleave(function() {
					jQuery(".dl-control").hide();
				});
			}
			if (dlWidth >= 768 && !newsDlInitialized) {
				var jsCheckInterval = setInterval(function() {
			            if (typeof mfJsObj !== "undefined") {
			                clearInterval(jsCheckInterval);
			                mfJsObj.dynamicLead.init();
					newsDlInitialized = true;
			            }
			        },200);
			}

		},200);
	}).resize();
	jQuery(".dl-left-button, .dl-right-button").click(function() {
		if (dlTimer) {
			clearInterval(dlTimer);
		}
		animateDL(jQuery(this).hasClass("dl-left-button") ? "left" : "right");
		return false;
	});
	var i = 1;
	jQuery(".dl-frame").each(function() {
		jQuery("#dl-dots").append("<a class='dl-dot dl_dot_"+i+"' href='#'></a>");
		i++;
	});
	jQuery("#dl-dots").each(function() {
		jQuery(this).children().first().addClass("active");
	});
	jQuery(".dl-dot").click(function() {
		if (dlTimer) {
			clearInterval(dlTimer);
		}
		animateDL(jQuery(this).index());
		return false;
	});
	startTimer();

});

</script>
