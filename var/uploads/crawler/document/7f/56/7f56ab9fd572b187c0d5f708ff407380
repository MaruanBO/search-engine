<script>
	var siteDevId = 'ao19852k0fksIu';/**/
var checkFav=0;
function MF2HandleAuth(json,$e) {
    var $elem = $e, /*$(this.authLink),*/
    response = json.response,
    userAttr,
	$reviewWrite = $('.review-write');
	$ratingStars = $('.rating-wrap');

    if (response.statusCode === 200) {
        userAttr = response.data.userData.attributes;
        mfJsObj.showtimes.cookie.set('mftk',response.data.token.a,365);
        $elem.html('SIGN OUT');
        $("[name='favTheaters']").each( function() {
            $(".theater-fav").fadeIn();
        });
		
	$reviewWrite.trigger('reviewWrite:signedIn', { response:response });
	$ratingStars.trigger('ratingStars:signedIn', { response:response });
 	$('body').trigger('ratingsManager:signedIn', { response:response });

        $("[name='mfSignIn']").each( function() {
            var e = this;
            var theaterId = $(e).attr("rel");
            if($(e).hasClass("add-fav-auth")==true) {
                $("#theaterItem"+ theaterId).html('<span class="fav-add-link'+theaterId+'" rel="'+theaterId+'" name="favTheaters"><a id="fav'+theaterId+'" href="javascript:addRemoveFavoriteTheater(\'http://www.moviefone.com/addFavoriteTheaters?theaterId='+theaterId+'\', \'http://www.moviefone.com/deleteFavoriteTheaters?theaterId='+theaterId+'\', document.getElementById(\'fav'+theaterId+'\'), document.getElementById(\'favflyout'+theaterId+'\'), \'\',\''+theaterId+'\');" class="favorite-add lt-gray button">Add to Favorites</a></span>');
            }
        });
        if(checkFav==0) {
            checkFav = 1;
            if($('#mfSignInTheater').length>0) {
		checkFav = 2;
		$("#main-column").load('/ajax/showtimes/favorite-theaters?stamp=1424381580&selectedShowDate=20150219',
		    function(response, status, xhr) {
			if (status == 'success') {
                        	$("#main-column").html(response);
                	        $("#main-column").fadeIn();
        	                $(".favoritetheaters .date-loc").show();
	                        $(".favoritetheaters .showtime-tools").show();
                	        mfJsObj.showtimeSelect.init();
                        	mfJsObj.modulePopup.init();
        	                mfJsObj.watchIt.pageurl="http://www.moviefone.com/theaters/favorites";
	                        mfJsObj.watchIt.type = "favtheaters";
                        	mfJsObj.watchIt.init();
			} else {
				$("#mfUserNameTheater").html("There was an error. Please try again.");
			}
                    });

            }
        }

    } else {
	if (checkFav === 2) {
		document.location.reload();
	}
        $elem.html('Sign In');

                $(".add-fav-auth").text('Add to Favorites');

        $("[name='favTheaters']").each( function() {
            var e = this;
            var theaterId = $(e).attr("rel");
            $(".theater-fav").fadeOut();
        });
	$reviewWrite.trigger('reviewWrite:signedOut');
	$ratingStars.trigger('ratingStars:signedOut');
 	$('body').trigger('ratingsManager:signedOut');

       
        mfJsObj.showtimes.cookie.set('mftk','',365);
    }

	if ($.fn.reviewWrite) {
		$reviewWrite.reviewWrite();
	}
}

MF.fn.push(function(){
	
	var ua = navigator.userAgent.toLowerCase();  
	if (ua.match(/android/i)){
		$("body").addClass("android");
	}
	
	if(mfJsObj.showtimes.cookie.get('locationCookie')==""){
		mfJsObj.mfLocation.init();
	}
	
	mfJsObj.headerScroll.init();
	mfJsObj.replaceImage.init();
	if($('.theater-type-wrap').length>0) {
		mfJsObj.showtimeSelect.init();
	}
	
	$("#facebook").html('<iframe src="http://www.facebook.com/plugins/like.php?app_id=133082040051963&href=http://www.facebook.com/moviefone&send=false&layout=standard&width=260&show_faces=false&action=like&colorscheme=dark&font=arial&height=30" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:260px; height:30px; background: transparent none;" allowTransparency="true"></iframe>');
	
	(function($){
		$("[name='mfSignIn']").multiAuth({
			devId: siteDevId,
			getTokenCallback: function(json){
				MF2HandleAuth(json, $(this.authLink))	
			}
		});
	
	})(jQuery);
	
});


</script>

