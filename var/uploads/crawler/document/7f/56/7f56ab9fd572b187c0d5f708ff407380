}(document, 'script', 'twitter-wjs');
	
com = {}
com.aol = {}
com.aol.jsloader = {}
com.aol.pushloader = {}
com.aol.pushloader = function() {
	for ( var i=0, m = MF.fn, l = MF.fn.length; i < l; i++ ){
		m[i]();
	};
};

com.aol.jsloader.doJSObjectsNow = function(){
	// tagFollow-popcontent_custom
	if($("#tagFollow-popcontent_custom")){
		$("[name='modulePopup_custom']").unbind("mouseover").mouseover(function(){
			$("#tagFollow-popcontent_custom").show();
		}).mouseout(function(){
			$("#tagFollow-popcontent_custom").hide();
		});
	}

	//newsletter
	if ($("#mf-newsletter").length) {
		$("#mf-newsletter").aolNewsletter({
			site: "Moviefone",
			list: "main",
			verify: false
		});
	}

	//INIT NAVIGATION
	if ($("ul.mf-nav-list").length) {
		mfJsObj.navigation.init();
	}

	//SEND US FEEDBACK
	if ($(".mf-feedback").length) {
	  $(".mf-feedback").click(function(e) {e.preventDefault();fBo2('moviefone_beta');});
	}

	//SMART SEARCH
	if ($("#frm-search").length) {
		$("#frm-search").submit(function(e) {
			return mfJsObj.smartSearch.submitForm();
		});
		mfJsObj.smartSearch.ajaxUrl = "http://www.moviefone.com/smart-search";
		mfJsObj.smartSearch.searchUrl = "http://www.moviefone.com/search/";
		mfJsObj.smartSearch.timerInterval = 500;
		mfJsObj.smartSearch.initSearch();
	}
	
	if ($("#search-text").length) {
		$("#search-text").focus(function() {
			$(this.value = '')
		});
	}

	//INIT search page paginations
	if ($("#paginate-bucket").length) {
		mfJsObj.paginateSearch.init();
	}

	//faq page toggle
	if($("#faq-main").length) {
		$("[name='question']").each(function(index) {
			$(this).click(function(e) {
				e.preventDefault();
				$(this).toggleClass('down');
				$("#answer"+(index+1)).toggle();
			});
		});
	}

	
	if(($("#mapShowtimesTheater").length) || ($("#mapQuestMap").length)) {
		Aol.getJS('http://www.mapquestapi.com/sdk/js/v7.0.s/mqa.toolkit.js?key=Amjtd%7Clu6tnu6b2h%2C25%3Do5-lw7su',com.aol.pushloader);
	} else {   
		com.aol.pushloader();
	}	 
};
</script>

