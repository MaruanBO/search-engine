    function ToLoadCTCss4Index(cssname) {
        var dfd = $.Deferred(),
        $divForCheck = $("<div />"),
        loadFail = false,
        timeout,
        ToCheckCTCss = function () {
            if (loadFail) {
                dfd.resolve();
                return;
            }
            setTimeout(function () {
                if ($divForCheck.css("display") === "none") {
                    dfd.resolve();
                    $divForCheck.remove();
                    clearTimeout(timeout);
                } else {
                    ToCheckCTCss();
                }
            }, 200);
        };
        CTCssPromises.push(dfd.promise());
        $("<link>").attr({
            "href": cacheUrl + "/css-main/" + cssname,
            "rel": "stylesheet",
            "type": "text/css"
        }).appendTo($("head").get(0));
        $(function () {
            $divForCheck.attr({
                "class": "css-loaded-check",
                "rel": cssname
            }).appendTo("body");
            ToCheckCTCss();
        });
        timeout = setTimeout(function () {
            loadFail = true;
        }, 10000);

    }
