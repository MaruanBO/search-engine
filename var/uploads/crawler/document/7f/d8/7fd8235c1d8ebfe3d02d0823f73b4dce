          var cfiretime = "1";
          $(function () {
              if ($(".chinesenewyear").length > 0) {
                  $(".btn01 > a").click(function () {
                      $('#chinesenewyear').attr('class', 'chinesenewyear stack open');
                  });
                  $(".btn02 > a").click(function () {
                      $('#chinesenewyear').attr('class', 'chinesenewyear stack close');
                  });
              }
          });
          function newyear() {
              if ($.cookie('CtIdxFire') == null || $.cookie('CtIdxFire') == "" || $.cookie('CtIdxFire') == "1") {
                  if ($.cookie('CtIdxFire') == "1") cfiretime = "2";
                  ToSetFireTime();
                  $('#chinesenewyear').attr('class', 'chinesenewyear stack open');
              }
           }
        
          function ToSetFireTime() {
              var sMAdSetupRestTime = 1000 * 60 * 60 * 1; //1000 * 60 * 60 * 1//1000 * 60 * 1
              var expDy = new Date();
              expDy.setTime(expDy.getTime() + (sMAdSetupRestTime));
              $.cookie('CtIdxFire', cfiretime, { path: '/', expires: expDy });
          }
        
          newyear();
          
      </script>
           
