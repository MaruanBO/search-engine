<script>
    function idx_relate(aid, cid) {
        var sRelateUrl = GetUrl_Cn("/news-issue/") + aid + "-" + cid;

        $('#related_news_iframe').attr("src", sRelateUrl);
        $('#related_news').attr('class', 'related_news on');
        setTimeout(idx_relate_close, 1000);
    }
    function idx_relate_off() { $('#related_news').attr('class', 'related_news'); }
    function idx_relate_close() {
        var rn1 = $('#related_news').attr("class");

        if (rn1 == "related_news on") {
            $("body").click(function () {
                idx_relate_off();
            });
            $(window).scroll(function () {
                idx_relate_off();
            });
            $(window).resize(function () {
                idx_relate_off();
            });
            $(document).keypress(function (e) {
                idx_relate_off();
            });

        }
    }
</script>

     
