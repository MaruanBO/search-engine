    <script type="text/javascript">

        var mainBanner = new Array();
        var flashEnabled = DetectFlashVer(requiredMajorVersion, requiredMinorVersion, requiredRevision);

        function home_load() {
            if (mainBanner.length < 3)
                return;

            //check flash banners
            if(!flashEnabled)
            {
                var divs = new Array();
                divs[divs.length] = document.getElementById('panBannerTop01');
                divs[divs.length] = document.getElementById('panBannerTop02');
                divs[divs.length] = document.getElementById('panBannerBottom01');
                divs[divs.length] = document.getElementById('panBannerBottom02');
                divs[divs.length] = document.getElementById('panBannerBottom03');
                
                for(var i=0;i<divs.length;i++){
                    if(divs[i].getElementsByTagName("object").length > 0) {
                        var objrem = divs[i].getElementsByTagName("object")[0];
                        objrem.parentNode.removeChild(objrem);
                        divs[i].getElementsByTagName("a")[0].style.display = "block";
                    }
                }
            }
            
            var b = document.getElementById('panBannerMain').getElementsByTagName("img")[0];
            var div = document.getElementById('divBannerSwitch');
            var ul = document.getElementById('ulBannerSwitch');

            var cnt = 1;
            for (var i = 0; i < mainBanner.length; i = i + 2) {
                var el = document.createElement("li");
                el.className = "bannerSwitch";
                el.appendChild(document.createTextNode(cnt++));

                el.onclick = function() {
                    var pos = parseInt(this.firstChild.nodeValue);
                    var lookup = 0;
                    if (pos > 1)
                        lookup = pos * 2 - 2;
                    document.getElementById('panBannerMain').getElementsByTagName("img")[0].src = mainBanner[lookup];
                    document.getElementById('panBannerMain').getElementsByTagName("a")[0].href = mainBanner[lookup + 1];
                    home_BannerPos = lookup;
                }

                ul.appendChild(el);
            }

            div.appendChild(ul);
            //div.style.left = 480 - (div.childNodes.length * 25) + "px";
            //div.style.marginLeft = 470 - (div.childNodes.length * 25) + "px";

            slideBanners();
        }

        var home_BannerPos=0;
        var home_timer1=null;
        function slideBanners() {
            var b = document.getElementById('panBannerMain').getElementsByTagName("img")[0];
            var l = document.getElementById('panBannerMain').getElementsByTagName("a")[0];
            clearInterval(home_timer1);

            if (home_BannerPos < (mainBanner.length - 2)) {
                b.src = mainBanner[home_BannerPos];
                l.href = mainBanner[home_BannerPos + 1]
                home_BannerPos += 2;
            }
            else {
                b.src = mainBanner[home_BannerPos];
                l.href = mainBanner[home_BannerPos + 1]
                home_BannerPos = 0;
            }

            home_timer1 = setInterval(slideBanners, 7000);
            
        }
        
    </script>
    
