            <div class="search">
                <form id="globalSearchForm" action="/search.aspx" method="get" name="globalSearchForm">
                    <input type="text" name="q" id="hyundaiSearch" value="Search">
                    <a id="hyundaiSearchSubmit" href="javascript:void(0);"><img src="/ipad/images/navigation/search-button.png"/></a>
                 
                   <script type="text/javascript">
                       $(document).ready(function () {
                           var defaultVal = 'Search';

                           $('#globalSearchForm input[type="text"]').each(function () {

                               $(this).focus(function () {
                                   if ($(this).val() == defaultVal || $(this).val().toLowerCase() == 'search') {
                                       $(this).removeClass('active').val('');
                                   }
                               })
                               .blur(function () {
                                   if ($.trim($(this).val()) === '') {
                                       $(this).addClass('active').val(defaultVal);
                                   }
                               })
                               .blur().addClass('active');
                           });

                           $('#globalSearchForm').submit(function (e) {

                               if ($(this).find('[type="text"]').val() === defaultVal || $(this).find('[type="text"]').val().toLowerCase() == 'search') {
                                   e.preventDefault();

                               }

                           });
                      $('input[type=text]').bind('paste', function (e) {
                         e.preventDefault();
                      }); 
                      
                      //hook up the event here
                        $('input[type=text]').bind('keypress', function (e) {
                        var unicode = e.charCode ? e.charCode : e.keyCode
                        //alert(unicode);
                        if (unicode != 8) { //if the key isn't the backspace key (which we should allow)
                            if (unicode == 60 || unicode == 62) //if not a number
                                return false //disable key press
                        }
                        });
                        
                         //hook up the event here
                        $('input[type=textarea]').bind('keypress', function (e) {
                        var unicode = e.charCode ? e.charCode : e.keyCode
                        //alert(unicode);
                        if (unicode != 8) { //if the key isn't the backspace key (which we should allow)
                            if (unicode == 60 || unicode == 62) //if not a number
                                return false //disable key press
                        }
                        });
                        
                      
                   });
                </script>

                
                
                
                </form>
