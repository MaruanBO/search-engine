<link rel="stylesheet" type="text/css" media="all" href="/myhyundai/content/MyHyundai.lightbox.css" />
<link rel="stylesheet" type="text/css" media="all" href="/myhyundai/content/MyHyundai.notification.css" />
<script src="/myhyundai/content/scripts/MyHyundai.Lightbox.js" type="text/javascript"></script>
<script src="/myhyundai/content/scripts/MyHyundai.Notification.js" type="text/javascript"></script>
<script type="text/javascript">
    navMenu.buildNavMenu("leftNav")
    HN.ownerRegister = function () {
        var link = $("#lightbox-opener").attr("href", "/myhyundai/Registration").get();
        myHyundaiUtils.lightbox.open.call(link, { preventDefault: function () { } });
    }
    HN.ownerLogin = function () {
        var link = $("#lightbox-opener").attr("href", "/myhyundai/Login").get();
        myHyundaiUtils.lightbox.open.call(link, { preventDefault: function () { } });
    }
    $(document).ready(function () {
        var loginInfo = IC.CookieJar.getCookie("MyHyundaiLogin");
        if (loginInfo == undefined || loginInfo == null || loginInfo == '') {
            $('#globalNavUtility .owner_register').css('display', 'inline-block');
            $('#globalNavUtility .owner_login').css('display', 'inline-block');
        } else {
            var parts = loginInfo.split('|');
            $('#globalNavUtility .owner_account').css('display', 'inline-block');
            $('#globalNavUtility .owner_logout').css('display', 'inline-block');
            $('#globalNavUtility .owner_well').css('display', 'inline-block');
            $('#globalNavUtility .owner_well .owner_name').html(parts[1]);

        }

        // rsy 10/28/13 #205 - preselect Equus-only when on Equus pages
        if (window.location.pathname.toLowerCase().indexOf("equus") > -1) {
            $("#chkEquusDealer").attr("checked", "checked");
        }

        $('#topNavWrapper #rightNav .u_dealer_locator .item').bind('click', function () {
            
            if ($(this).hasClass('down')) {
                //fix issue on ie 7

                $(this).removeClass('down')
                $('#navUtility .dealer_locator').find('.drop_down').slideUp({ duration: 700, easing: 'easeInExpo', complete: function () {
                    $('#navUtility').css({ 'top': '0px' });
                }
                });
        } else {
                $(this).addClass('down')
                $('#navUtility').css({ 'top': '74px' });
                $('#navUtility .dealer_locator').find('.drop_down').slideDown({ duration: 700, easing: 'easeOutExpo' });
            }
        });


        
        
        $('#navUtility li.dealer_locator a.close').unbind('click');
        $('#navUtility li.dealer_locator a.close').click(function () {
            $('#topNavWrapper #rightNav .u_dealer_locator .item').removeClass('down');
            $('#navUtility .dealer_locator').find('.drop_down:visible').slideUp({ duration: 700, easing: 'easeOutExpo' });
            return false;
        });
        $('#globalNavUtility #hyundaiSearch').valueHold();
        $('#globalNavUtility #hyundaiSearchSubmit').click(function () {
            $('#globalNavUtility #globalSearchForm').submit();
        });
        //$('#globalNavUtility #globalSearchForm').submit(function () {
        //    var $input = $('#globalNavUtility #hyundaiSearch');
        //    if ($input.val() != '') {
        //        return true;
        //    }
        //    return false;
        //});
        if ($('.tooltip_shell').length > 0) {
            $('.tooltip_shell').hover(function () {
                $('.tooltip_pop_cont').slideDown();
                return false;
            });
            $('.tooltip_pop_top').bind('click', function () {
                $('.tooltip_pop_cont').slideUp();
                return false;
            });
        }
        var title = $('#page #title');
        //title.css({'margin-top':'0px'});
        if (title.css('margin-top') == '-30px') {
            title.css({ 'margin-top': '0px' });
        }
        if (title.length > 0) {
            $('#page #wrapper').prepend(title);
        }

        $('#globalNavUtility .drop_down').click(function () {
            
            var ul = $(this).find('> ul');
            var ind = $(this).find('.indicator');

            if ($(this).hasClass('down')) {
                ul.slideUp({ duration: 700, easing: 'easeOutExpo',
                    complete: function () {
                        ul.parent().removeClass('down');
                    }
                });

            } else {
                ul.slideDown({ duration: 700, easing: 'easeOutExpo',
                    complete: function () {
                        ul.parent().addClass('down');
                    }
                });
            }
        });


        HN.Scroller.inject('miniDealerLocatorResult', 'dealers', '190');
        //HN.Scroller.inject('quoteDealerLocatorResult', 'dealer_list', '170');
    });
</script>
