$(function() {
  var POINTS_PER_MS = 578704 / 1000,
      $counter = $('.homepage-data-counter strong'),
      startTime,
      points;
      
  startTime = new Date();
  startTime.setDate(1);
  startTime = startTime.getTime();
  
  function updateCounter() {
    $counter.text(
      Math.round(
        ((new Date()).getTime() - startTime) * POINTS_PER_MS
      ).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,")
    );
  }

  updateCounter();
  setInterval(updateCounter, 500);
  
  setTimeout(function() {
    $counter.parent().slideToggle();
  }, 500);
  
});
