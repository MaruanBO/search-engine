<script type="text/javascript" >
	$(function() {
		$("#btn-submit").click(function(e) {
			document.getElementById("errorMessagesJavaScript").innerHTML ="";
			
			var email = $("#email").val();
			var errorMessage = 'Please enter a valid email address.';
			
			if(!isValidEmail(document.EmailSignup.email.value))
			{
				//display error message
				
				document.getElementById("errorMessagesJavaScript").innerHTML = errorMessage;
				//document.getElementById("errorMessagesJavaScript").style.display="block";
				
			return false;
			}
			else
			{
				
				$.ajax({
				url: "SignUpForSpecialOffersEmailCmd",
				dataType: 'json',
				type: "get",
				data:{langId:'-1', storeId:'10451', catalogId:'10001', email:email},
				complete: function(response) {
						response = $.parseJSON(response.responseText);
						// If response is success, success dialog window will display with success message.
						if (response['success'] == 'true'){
						
							//e.preventDefault();
		
							//Get the A tag
							//var id = $(this).attr('href');
							var id = "#dialogEmailSignUpThankYou";
						
							//Get the screen height and width
							var maskHeight = $(document).height();
							var maskWidth = $(window).width();
						
							//Set heigth and width to mask to fill up the whole screen
							//$('#mask').css({'width':maskWidth,'height':maskHeight});
							
							//transition effect		
							$('#maskEmailSignUpThankYou').fadeIn(1000);	
							$('#maskEmailSignUpThankYou').fadeTo("slow",0.8);	
							
							//Get the window height and width
							var winH = $(window).height();
							var winW = $(window).width();
							
							var wLeft = window.screenLeft ? window.screenLeft : window.screenX;
						    var wTop = window.screenTop ? window.screenTop : window.screenY;
						
						    var left = wLeft + (window.innerWidth / 2) - (400 / 2);
						    var top = wTop + (window.innerHeight / 2) - (400 / 2);
							
					              
							//Set the popup window to center
							//$(id).css('top',  top);
							//$(id).css('left', left);
							//Set the popup window to center
							$(id).css('top', winH/2-$(id).height()/2);
							$(id).css('left', winW/2-$(id).width()/2);
							
						
							//transition effect
							$(id).fadeIn(1000); 
							document.getElementById("emailBold").innerHTML = email;
							var prefCenterURL = 'http://pages.s6.exacttarget.com/page.aspx?QS=5c591a8916642e733af05c78481df538a0fe048e04feaf87e7caf9e0e999aeb4&email=xxx&brand=Jared&store=2001';
							prefCenterURL = prefCenterURL.replace('xxx',email);
							$("#prefCentURL").attr('href',prefCenterURL);
	
						// If any problem saving email, failure message will display in the  window.	
						} else if (response['exception'] == 'true'){
							document.getElementById("errorMessagesJavaScript").innerHTML = errorMessage;
						}
					}
				});
			}
			return false;
		});
	});
	$(function() {
		$("#close").click(function(e) {
			var id = "#dialogEmailSignUpThankYou";
			//$(id).fadeOut("slow");
			$(id).hide(); 
		});
	});
</script>

