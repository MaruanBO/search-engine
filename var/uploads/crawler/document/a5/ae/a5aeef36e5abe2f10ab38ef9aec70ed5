				// handles keyboard, keeping focus within the dialog
				$("#popup").on ("keydown", modalKeyboardHandler);
			});
		}
		
		function closeReminder()
		{
			$('#popup').hide();
			var cookie = getCookie('specialOfferOptIn_Jared');
			if (cookie == null)
			{
				SetCookie('specialOfferOptIn_Jared', 0);				 
			}			
			$('#emailMask').hide();
			$('body').css("overflow","visible");				
		}
		function openPrivacyPolicyWindow(url)
		{
			openWindow(url, 'privacy_policy', 800, document.body.clientHeight, 'scrollbars=yes,menubar=no,toolbar=no,location=no,status=yes,resizable=yes');
		}
		function confirmOK(message)
		{
			$('div#content').addClass('text');
			//$('div#content').css('color','blue');			
			$('div#content').html(message);
		}
		function error(message)
		{						
			$('div#info').html(message);
		}
		function emptyError()
		{				
			$('div#info').html('');
		}
	
	//ADA
	// This function keeps keyboard focus within the dialog; escape to cancel...
	function modalKeyboardHandler (e) {
		var $dialog = $(this);
		if ($dialog.is(":visible")) {
			//debug ("keypress in " + $dialog[0].id + " keyCode=" + e.keyCode);
			// find first and last focusable items in the dialog
			var first = $('#popup .close');
			var firstId = "closeBtn";
			var last = $('#popup .modalInput:last');
			var lastId = "privacyPolicy";
			//debug ("first=" + first.id + " last=" + last.className );
			
			// keep tabs and escapes from propagating outside this dialog
			e.stopImmediatePropagation();
			e.stopPropagation();
			//debug ("keyCode=" + e.keyCode + " " + " which=" + e.which);
			switch (e.keyCode || e.which) {
				case 27: // escape
					closeReminder();
					return false;
					break;
				
				case 9:  // shift+tab
					if (e.shiftKey) {
						//debug ("- shift+tab");
						if (e.target.id == firstId) {
							// if we fall off the beginning, wrap to end
							//debug ("- fell off beginning");
							e.preventDefault();
							last.focus();
							return false;
						} // if
						break;
					} // if
				
				case 9: // tab key without shift
					//debug ("tab key...");
					if (e.target.id == lastId) {
						//debug ("fell off the end...");
						// if we fall off the end, wrap to beginning
						e.preventDefault();
						first.focus();
						return false;
					} // if
			} // switch
		
		} // if
		
		// if we made it down here, perform default action
		//debug ("-- default action --");
		return true;
	} // modalKeyboardHandler
</script>
