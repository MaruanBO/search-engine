<script type="text/javascript">
$(document).ready(function(){
     var displayName = getDisplayName();
     var storeNumber = getStoreNumber();
     var loggedIn = false;
     var userTypeValue;
     var hostString = window.location.hostname;
          
     if ( displayName == "guest"  ) {
       loggedIn = false;
     } 
     else {
       loggedIn = true;
     }
     
     if (loggedIn) {   
           if (storeNumber != 'null' && storeNumber != null){
                 userTypeValue = 'S';

           }
           else {
                 userTypeValue = 'R';
           }                       
     }
     else {
           userTypeValue = 'G';
     }
   
	footerPerUserType(userTypeValue);
});	
	function footerPerUserType(userType){			
		//var registeredUserLinksToHide = document.getElementsByClassName('usertype_registeredUser');
		//var guestUserLinksToHide = document.getElementsByClassName('usertype_guestUser');
		//var storeUserLinksToHide = document.getElementsByClassName('usertype_storeUser');
		
		if(userType == 'G') {
			$('.usertype_guestUser').each(function(){
					$(this).parent().remove();
				}
			); 
		}		
		else if(userType == 'R') {
			$('.usertype_registeredUser').each(function(){
					$(this).parent().remove();
				}
			); 
		}
		else {
			$('.usertype_storeUser').each(function(){
					$(this).parent().remove();
				}
			);
			
			var socialWidth = $('.footer-link-social').width();
			$('.footer-link-social').remove();
			$('.footer-link-cust-service').width($('.footer-link-cust-service').width() + (socialWidth/3));
			$('.footer-link-our-company').width($('.footer-link-our-company').width() + (socialWidth/3*2));
			$('.footer-links .footer-vert-lines').removeClass('left').addClass('right');
			$('.footer-link-legal .footer-vert-lines').remove();
			$('.footer-link-cust-service .footer-link-modal').children('.left').eq(1).css('margin-left','25px');
		}		
	}
	

</script>

<!-- CMS-END footer links show hide script -->

