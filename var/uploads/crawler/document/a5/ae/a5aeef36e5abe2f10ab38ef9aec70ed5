<script>
var $ = jQuery.noConflict();
$(document).ready(function() {

	//select all the a tag with name equal to modal
	$('a[name=ContactUsModal]').click(function(e) {
		//Cancel the link behavior
		e.preventDefault();
		
		
				$.get("/Jared/Content/CMSContentSpotDisplay.jsp?xmlFileName=contact_us_modal.xml&cmsId=default", function(data) {
					$('#boxesContactUs #dialogCS').html(data);
					// ADA Compliance - handles keyboard, keeping focus within the dialog
					$('#dialogCS').attr("role", "alertdialog");
					$('#dialogCS a:first').attr("id", "closeBtnCC");
					$('#dialogCS a:first').attr("aria-label", "close");
					$('#dialogCS a:last').attr("id", "emailLink");
					$('.contact-chat-container a:first').focus();
					
					if (!($.browser.msie && parseInt($.browser.version, 10) === 7)) {
						var storeNumber = getStoreNumber();
						var PROD = 'true';	
						var availabilityStatus = 'true';
						var recencyAccountInfo = '';
						if (storeNumber == null ) {
							 if(availabilityStatus == "true") {
							 	var chatButton = $('<a class="brown-button"  id="contactUsLiveChatLink"><span>CLICK TO CHAT LIVE NOW</span></a>');			        			
			        			$("#chatAvailabilityButton").append(chatButton);
			        			setContactUsChatLink(PROD, recencyAccountInfo);							 
							 } else {
							 	var chatButton = $('<a class="chatUnavailableBox" id="contactUsLiveChatLink"><span>Sorry, Live Chat is currently offline</span></a>');							 				        			
			        			$("#chatAvailabilityButton").append(chatButton);
			        			setContactUsChatLink(PROD, recencyAccountInfo);	
							 }
						}
					}					
				});
			
		


		
		//Get the A tag
		var id = $(this).attr('href');
	
		//Get the screen height and width
		var maskHeight = $(document).height();
		var maskWidth = $(window).width();
	
		//Set heigth and width to mask to fill up the whole screen
		//$('#mask').css({'width':maskWidth,'height':maskHeight});
		
		//transition effect		
		$('#maskContactUs').fadeIn(1000);	
		$('#maskContactUs').fadeTo("slow",0.8);	
		
		//Get the window height and width
		var winH = $(document).height();
		var winW = $(window).width();
              
		//Set the popup window to center
		$(id).css('top',  '105px');
		$(id).css('left', '9%');
	
		//transition effect
		$(id).fadeIn(1000); 
	
	});
	
	//if close button is clicked
	$('.windowCS .close').live("click", function(e){
		e.preventDefault();		
		$('#maskContactUs').hide();
		$('.windowCS').hide();
	});
	
	
	//if mask is clicked
	$('#maskContactUs').click(function () {
		$(this).hide();
		$('.windowCS').hide();
	});			
	
	// ADA Compliance
	$("#dialogCS").on ("keydown", modalKeyboardHandlerContactUs);
	
});	

	//ECOMDEV-13571 - ADA Compliance
	function closeContactUs() {
		$('#dialogCS').hide();
	}
	// This function keeps keyboard focus within the dialog; escape to cancel...
	function modalKeyboardHandlerContactUs (e) {
		var $dialog = $(this);
		if ($dialog.is(":visible")) {
			//console.debug ("keypress in " + $dialog[0].id + " keyCode=" + e.keyCode);
			// find first and last focusable items in the dialog
			var first = $('#dialogCS #closeBtnCC');
			var firstId = "closeBtnCC";
			var last = $('#dialogCS #emailLink');
			var lastId = "emailLink";
			
			// keep tabs and escapes from propagating outside this dialog
			e.stopImmediatePropagation();
			e.stopPropagation();
			//console.debug ("keyCode=" + e.keyCode + " " + " which=" + e.which);
			switch (e.keyCode || e.which) {
				case 27: // escape
					closeContactUs();
					return false;
					break;
				
				case 9:  // shift+tab
					if (e.shiftKey) {
						//console.debug ("- shift+tab");
						if (e.target.id == firstId) {
							// if we fall off the beginning, wrap to end
							//console.debug ("- fell off beginning");
							e.preventDefault();
							last.focus();
							return false;
						} // if
						break;
					} // if
				
				case 9: // tab key without shift
					//console.debug ("tab key...");
					if (e.target.id == lastId) {
						//console.debug ("fell off the end...");
						// if we fall off the end, wrap to beginning
						e.preventDefault();
						first.focus();
						return false;
					} // if
			} // switch
		
		} // if
		
		// if we made it down here, perform default action
		//console.debug ("-- default action --");
		return true;
	} // modalKeyboardHandlerContactUs
</script>
