<script>
<!--
    function toggle_visibility(id) {
       var e = document.getElementById(id);
       if(e.style.display == 'block')
          e.style.display = 'none';
       else
          e.style.display = 'block';
    }
//-->

 function toggle_visibility(visibleId,hiddenId) {
    var visible = document.getElementById(visibleId);
    var hidden = document.getElementById(hiddenId);
    if(visible != null && hidden != null){
    	visible.style.display = 'none';
	    hidden.style.display = '';
    }
}
 function SetCookie (name, value) {
	var argv = SetCookie.arguments;
	var argc = SetCookie.arguments.length;
	var defaultexp = new Date ();
	defaultexp.setTime (defaultexp.getTime() + (24 * 60 * 60 * 1000 * 31));
	var expires = (argc > 2) ? argv[2] : defaultexp;
	var path = (argc > 3) ? argv[3] : "/";
	var domain = (argc > 4) ? argv[4] : null;
	var secure = (argc > 5) ? argv[5] : false;
	document.cookie = name + "=" + escape (value) +  ((expires == null) ? "" : ("; expires=" + expires.toGMTString())) + ((path == null) ? "" : ("; path=" + path)) + ((domain == null) ? "" : ("; domain=" + domain)) + ((secure == true) ? "; secure" : "");
}
 
 function SetCJCookie (name, value) {
	var defaultexp = new Date ();
	defaultexp.setTime (defaultexp.getTime() + (24 * 60 * 60 * 1000));
	var path =  "/";
	var domain = null;
	var secure = false;
	document.cookie = name + "=" + escape (value) +  ((defaultexp == null) ? "" : ("; expires=" + defaultexp.toGMTString())) + ((path == null) ? "" : ("; path=" + path)) + ((domain == null) ? "" : ("; domain=" + domain)) + ((secure == true) ? "; secure" : "");
}

function getBillingZip(){
	var c = getCookie('BILLINGZIP10451');
	if (c==null){
		c = '';
	}
	return c;
}
 
 function search(){
 	var storeId='10451';
  	var catalogId='10001';
    var langId='-1';
    var searchTerm = document.globalSearchForm.Ntt.value;
    var ntk = document.globalSearchForm.Ntk.value;
    var n=document.globalSearchForm.N.value;
     var ntt=document.globalSearchForm.Ntt.value;
    var ne=document.globalSearchForm.Ne.value;
	  var url = 'search|'+storeId+'|'+catalogId+'|'+langId+'|'+searchTerm+'|'+n+'|'+ne+'|'+ntk+'|'+ntt;
  //   var url = 'SearchResultsView?langId=-1&storeId=10451&catalogId=10001&N=0&Ne=1&Ntk=Products&Ntt=Levian&searchButton.x=0&searchButton.y=0';
    document.location.href = url;
  
   
 }
