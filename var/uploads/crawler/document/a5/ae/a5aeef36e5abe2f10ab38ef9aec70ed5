<script language="javascript">
function getCookie(name) {

	var cookies = document.cookie.split(';');
	var value   = null;
	
	for ( var i=0; i < cookies.length; i++ ) {
		var cookie = cookies[i];
		
		while ( cookie.charAt(0) == ' ' ) {
			cookie = cookie.substring(1, cookie.length );
		}

		if ( cookie.indexOf(name+"=") == 0 ) {
			value = cookie.substring(name.length+1, cookie.length);
			value =	unescape(value);
		}
	}
	return value;
}
function getDisplayName() {
 	var name = getCookie( "DISPLAYNAME" );
	if ( name == null ) {
		name = "guest";
	}
	// this is a fix for IE as IE does not listen to changes in cookie value
	if (navigator.appName.indexOf('Microsoft') > -1) {
		var nameForIE = getCookie("WC_AUTHENTICATION_-1002");	
		var useractivity = getCookie('WC_USERACTIVITY_-1002');
		if (nameForIE == null && name == 'guest' && useractivity == null){
			name = "";
		}

	}
	

	return name;
}
function updateShopCart(quantity, subtotal){
	value = "{quantity:'" + quantity + "', subtotal:'" + subtotal + "'}";
	SetCookie('SHOPPINGCART10451', value);
	$("#mini-cart-qty").text(quantity);
}
function getShopCart(){
	var c = getCookie('SHOPPINGCART10451');
	var mcart = (c != null) ? eval("(" + c + ")") : null;
	var quantity = (mcart != null) ? mcart.quantity : 0;
	//alert("mcart : " + mcart);
	//alert("quantity : " + quantity);
	return quantity;
}
function getShopCartSubtotal(){
	var c = getCookie('SHOPPINGCART10451');
	var mcart = (c != null) ? eval("(" + c + ")") : null;
	var subtotal = (mcart != null) ? mcart.subtotal : 0;
	//alert("mcart : " + mcart);
	//alert("quantity : " + quantity);
	return subtotal;
}
function getOTBAmount(){
	var c = getCookie('OTBAMOUNT10451');
	if (c==null){
		c = 0.00;
	}
	return c;
}
function creditAppWindow(url) {
				openWindow(url, "credit_app", 800, document.body.clientHeight, "scrollbars=yes,menubar=no,toolbar=no,location=no,status=yes,resizable=yes");
}
function getStoreNumber(){
	var number = getCookie('STORENUMBER10451');
	return number;
}

</script>

