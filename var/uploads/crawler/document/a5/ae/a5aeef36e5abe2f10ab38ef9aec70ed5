<script type="text/javascript">
function getDelimitedListForDataLayerItem(name, delimiter) {
  	      
  	      var result = ""

		  if( name && name != null && name != "" &&
		      delimiter && delimiter != null && delimiter != "") {
			  if( sdl_orderItems != null && sdl_orderItems.length > 0 ) {
					for( var i = 0; i < sdl_orderItems.length; i++ ) {
					  var _val = "";
					  if( name == "sku" ) {
					    _val = sdl_orderItems[i].sku;
					  } else if( name == "adjustedPrice" ) {
					    _val = sdl_orderItems[i].adjustedPrice;
					  } else if( name == "description" ) {
					    _val = sdl_orderItems[i].description;
					  } else if( name == "category" ) {
					    _val = sdl_orderItems[i].category;
					  } else if( name == "productClass" ) {
					    _val = sdl_orderItems[i].productClass;
					  } else if( name == "quantity" ) {
					    _val = "1";
					  } else if( name == "rating" ) {
					    _val = sdl_orderItems[i].rating;
					  }
		
					  // remove invalid characters
					  if( typeof _val === "undefined" || _val == null ) {
					    _val = "";
					  }
					  _val = _val.replace(',','');
					  _val = _val.replace('$','');
		
					  if( result == '' ) {
						result += _val;
					  } else {
						result += delimiter + _val;
					  }
					}
			  }
		  }
		  return result;
		}
</script>
	

<!-- Search Ignite BEGIN click tracking script  version 4.0 Jared --><!-- iGoDigital START -->
  <div id="igdRTA">
	
    <script type="text/javascript">
      //iGoDigital RTA Initialization
 	  var delimiter = "|";
 	  var pageName = 'TopCategoriesDisplay';
 	  var rtaRetailer = 'jared';
 	  var rtaEmail = sdl_Email;
 	  var rtaSearch = ""; 
 	  var rtaProductSKU = "";
 	  var rtaRating = sdl_orderItem.rating;
 	  var rtaCategory = "";
	  var rtaCart = "";
	  var rtaCartSku = "";
	  var rtaCartAmounts = "";
	  var rtaCartQuantities = "";
	  var rtaSpecial = "";
	  var rtaConvertCart = "";
  	  var rtaOrderNum = "";
      
 	  if(pageName.indexOf("ProductDisplay") != -1 || pageName.indexOf("ProductDisplayView") != -1)
 	  {
 	    rtaProductSKU = sdl_orderItem.sku;
 	  }
	  if(pageName.indexOf("OrderItemDisplay") != -1)
	  {
	    rtaSpecial = "ownit";
	  }
	  if(pageName.indexOf("OrderConfirmationDisplay") != -1 || pageName.indexOf("OrderOKView") != -1)
	  {
	    rtaConvertCart = "1";
	    rtaOrderNum = sdl_OrderId;
	  }
 
	  rtaCart = getDelimitedListForDataLayerItem("productClass",delimiter);
	  rtaCartSku = getDelimitedListForDataLayerItem("sku",delimiter);
	 
	  rtaCartAmounts = getDelimitedListForDataLayerItem("adjustedPrice",delimiter);
	  rtaCartQuantities = getDelimitedListForDataLayerItem("quantity", delimiter);
	  
	  rtaSearch = getReferrerSearchParameter(); //obtain query term used by search engine
	  if(!rtaSearch || 0 === rtaSearch.length)
	  {
	    rtaSearch = ''; //obtain internal search text
	  }
	  if(sdl_orderItem.category)
	  {
	    rtaCategory = sdl_orderItem.category; //try to set item
	  } 
	  if(!rtaCategory || 0 === rtaCategory.length)
	  {
	    rtaCategory = gtm_pcat;
	  }
	  if(!rtaSearch || 0 === rtaSearch.length)
	  {
	    rtaSearch = rtaCategory;
	  }
	  
    </script>
  </div>
