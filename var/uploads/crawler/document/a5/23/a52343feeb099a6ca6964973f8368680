    <script language="javascript" type="text/javascript">
var l_prm = Sys.WebForms.PageRequestManager.getInstance();
var aPage = 1;
var scrollOffset = 20;
var aScrollY = 0;

function AScroll(a_scrollObject)
{
if (a_scrollObject.clientHeight + a_scrollObject.scrollTop >= a_scrollObject.scrollHeight)
{//load up some more data
aPage++;
__doPostBack('ctl00_m_homeFooter_m_sideBarCNTRL_m_activityPNL',aPage);
}
}

function BeginRequestHandler(sender, args)
{
if ($get('ctl00_m_homeFooter_m_sideBarCNTRL_m_activityPNL') != null)
{
  aScrollY = $get('ctl00_m_homeFooter_m_sideBarCNTRL_m_activityPNL').scrollTop;
}
}

function EndRequestHandler(sender, args)
{
if ($get('ctl00_m_homeFooter_m_sideBarCNTRL_m_activityPNL') != null)
{
   $get('ctl00_m_homeFooter_m_sideBarCNTRL_m_activityPNL').scrollTop = aScrollY;
}
}

l_prm.add_beginRequest(BeginRequestHandler);
l_prm.add_endRequest(EndRequestHandler);
</script>
