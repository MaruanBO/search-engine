        tlium.GetTrafficType = function () {
            var trafficType = "";
            if ("false" == "true") {
                trafficType = "BOT";
            }
            else if ("false" == "true") {
                trafficType = "MON";
            }
            else {
                trafficType = "CUS";
            }
            return trafficType;
        };
        tlium.GetMMCT = function() {
            var mmct = "";
