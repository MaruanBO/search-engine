<script type="text/javascript">
	
	var RequireJSSetup = function () {
		var _siteVersionNumber;

		
		var _getSiteVersionNumber = function()
		{
			//returns _siteVersionConfig if it is not null, otherwise executes a search.
			return _siteVersionNumber ? _siteVersionNumber : function() {
				var scriptNodes = document.getElementsByTagName('script');
				var attributeMatch;

				for(var i=0; i<scriptNodes.length; i++)
				{
					try
					{
						attributeMatch = scriptNodes[i].attributes.src.value.match(/requirejs_prvdcustom.*/i);
						if(attributeMatch.index > -1)
						{
							
							_siteVersionNumber = attributeMatch[0].match(/(\d+\.*)+/)[0];
							return _siteVersionNumber;
						}
					}
					catch (ex)
					{
						
					}
				}

				try {
					
					var d = new Date();
					_siteVersionNumber = d.getFullYear() + '.' + d.getMonth() + '.' + d.getDay() + '.' + d.getHours() + '.' + d.getMinutes();
					return _siteVersionNumber;
				}
				catch(ex)
				{
				}

			}();

		};

		
		var _init = function()
		{
			var scriptRoot;

			if(window.isPRVDLM())
			{
				scriptRoot = PRVD.CT.CONFIG.ABSOLUTEPATHTOJSFILES;
			}
			else
			{
				scriptRoot = "https://" + PRVD.CT.CONFIG.JAVASCRIPTCONTENTHOST + PRVD.CT.CONFIG.ABSOLUTEPATHTOJSFILES;
			}

			require.config({
				baseUrl: scriptRoot,
				useMinified: !window.isPRVDDebug(),
				urlArgs: "siteVersionNumber=" + _getSiteVersionNumber(),
				normalizeModuleCasing: true,
				waitSeconds: 20,
				paths: {
					text: 'lib/requireText_prvdCustom'
				}
			});

			
			require([ 'WWW/PrvdWeb_r' ]);

		}();

	}();

