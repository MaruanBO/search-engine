halloween.today = halloween.getDate().toString() + halloween.getMonth().toString() + halloween.getFullYear().toString();

if (halloween.today === "3192014") {
  console.log("BOO! It's Halloween and we just made the background spooky dark! [pt 727-8465174]");
$(document).ready(function () {jQuery("body").css("background-color", "black")});
}
else if (halloween.getMonth() > 9) {
  console.log("Aw, Halloween is over. Is there any more candy? [pt 727-8465174]");
}
else {
  console.log("It's " + (31 - halloween.getDate()) + " days until Halloween! [pt 727-8465174]");
}
</script>

