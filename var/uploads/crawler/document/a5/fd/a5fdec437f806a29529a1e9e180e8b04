                            function LoginForm_onsubmit(ver) {
                                    var f = document.loginForm;
                                    if (isWhitespace(f.CONTACT_EMAIL.value)) {
                                                alert("Please enter your Email address.");
                                                f.CONTACT_EMAIL.focus();
                                                return false;
                    }
                                else if (isWhitespace(f.USERPASS.value)) {
                                            alert("Please enter a Password.");
                                            f.USERPASS.focus();
                                            return false;
                    }
                                else {
                                            if (ver == 'fbc') {
                                                            _gaq.push(['_trackEvent', 'Engag Assistant', 'EA-FB Login', 'Normal login']);
                    }
                                    return true;
                    }
                    }
                </script>

