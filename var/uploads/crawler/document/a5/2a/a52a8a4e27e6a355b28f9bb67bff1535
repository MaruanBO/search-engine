<script>
    // For Facebook
    jQuery(function() {
        // Offsets to move the popup
        var moveLeft = 55;
        var moveUp = -115;
        jQuery('.socialFollowFacebook').hover(function() {
                jQuery('.socialFollowbarPopUpFB').show()
                    .appendTo('.socialFollowFacebook');
                // Bring div to front to prevent ads/images blocking it
                jQuery('.container-vertical-sharebar').css('z-index', 10000009);
            }, function() {
                jQuery('.socialFollowbarPopUpFB').hide();
                // Undo bring div to front
                jQuery('.container-vertical-sharebar').css('z-index', '');
        }).mousemove(function() {
            jQuery('.socialFollowbarPopUpFB').css('top', moveUp).css('right', moveLeft);
        });
    });
    // For Twitter
    jQuery(function() {
        var moveLeft = 55;
        var moveUp = 27;
        jQuery('.socialFollowTwitter').hover(function() {
            jQuery('.socialFollowbarPopUpTW').show()
                .appendTo('.socialFollowTwitter');
            jQuery('.container-vertical-sharebar').css('z-index', 10000009);
        }, function() {
            jQuery('.socialFollowbarPopUpTW').hide();
            jQuery('.container-vertical-sharebar').css('z-index', '');
        }).mousemove(function() {
            jQuery('.socialFollowbarPopUpTW').css('top', moveUp).css('right', moveLeft);
        });
    })
</script>        </div>

