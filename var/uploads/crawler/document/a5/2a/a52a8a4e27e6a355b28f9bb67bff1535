<script>
    // Adds commas to numbers
    jQuery.fn.digits = function() {
        return this.each(function() {
            jQuery(this).text(
                jQuery(this).text()
                    .replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")
            );
        })
    };
</script>
