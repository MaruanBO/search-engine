		<script type="text/javascript">

			function check_secure() {
				var httpsRE = /^https/i;
				if (!window.location.protocol.match(httpsRE)) {
					
		                $ ('#h13-news-bulletin-form').attr("action","https://" + window.location.hostname + ":443" + "/libs/threeshare/MarketoServlet");
					
		        }
		        $ ('#orig-scheme').val(window.location.protocol);  // note e.g. http: <- includes the ':'
		        $ ('#orig-host').val(window.location.hostname);
		        $ ('#orig-port').val(window.location.port);
		        $ ('#orig-referrer').val(document.referrer);
		    }

		    $(document).ready(function() {

		        //check_secure();

		        // FORM FIELD PLACEHOLDERS:
				$('.form_field input').focus(function() {
				    if (this.value == this.defaultValue){  
				        this.value = '';  
				    }  
				});  
				$('.form_field input').blur(function() {
				    if (this.value == ''){  
				        this.value = this.defaultValue;  
				    }  
				});		        
		        
		        //custom validation rule - text only (LastName)
		        $.validator.addMethod("validValueTwoForty", function(value, element) { return ((value != element.defaultValue)&&(value.length>=2)&&(value.length<=40));}, "Enter a valid value.");

		        //custom validation rule - text only (FirstName)
		        $.validator.addMethod("validValueOneForty", function(value, element) { return ((value != element.defaultValue)&&(value.length>=1)&&(value.length<=40));}, "Enter a valid value.");

		        var h13form = $('#h13-news-bulletin-form'),
		        	h13Success = $('#h13-news-bulletin-success'),
		        	h13Error = $('#h13-news-bulletin-error'),
		        	h13formDiv = $('.h13-news-bulletin-form'),
		        	h13submitDiv = $('.h13-news-bulletin-submit');

		     	// FORM SUBMIT:
		        h13form.validate({
	            	submitHandler: function(form) {

            	    	// post to marketo:
						$.ajax({
							type: 'POST',
							url: h13form.attr('action'),
							data: h13form.serialize(),
							dataType: 'json',
							encode: true
						})
		            	.done(function(data) {
		            		h13formDiv.hide();
		            		h13submitDiv.hide();
		            		if(data.success == true){
		            			h13Success.show();
			            	} else {
			            		h13Error.show();	
				            }
		    				
			            })
		            	.fail(function(data) {
		            		h13formDiv.hide();
		            		h13submitDiv.hide();
		            		h13Error.show();
						});

					}
	            });
	                
			    if (jQuery('#Email').length > 0) {
	                $('#Email').rules('add',
	                    {
		                	email: true,
	                    	required: true
	                    }
	                );
			    }
				if (jQuery('#FirstName').length > 0) {
					$('#FirstName').rules('add',
					    {
					    	required: true,
					 		validValueOneForty: true
					    }
					);
				}
				if (jQuery('#LastName').length > 0) {
					$('#LastName').rules('add',
					    {
					        required: true,
					 		validValueTwoForty: true
					    }
					);
				}		        
		    });
		
		</script>
								
	

	
	
