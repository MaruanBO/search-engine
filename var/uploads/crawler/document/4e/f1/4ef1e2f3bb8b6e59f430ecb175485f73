    					<script type='text/javascript'>
			
			var home_page_middle_col_section01 = {};
			
			home_page_middle_col_section01.table_node;
			home_page_middle_col_section01.interval_delay = 7500;
			home_page_middle_col_section01.interval_reference;
			home_page_middle_col_section01.interval_td_index;
			home_page_middle_col_section01.num_tds = 0;
			home_page_middle_col_section01.td_arr = [];
			home_page_middle_col_section01.overflow_node;
			
			
			$(document).ready(
				function(){
					var ready_arr = [
						'FUNCTION: getJavascript_homePageMiddleCol_section01',
						'CLASS: TemplateContentGenerator',
					];
					var ready_str = ready_arr.join("\n\n");
					//alert(ready_str);
					//console.log(ready_str);
					
					
					home_page_middle_col_section01.init();
					
				}
			); // end ready
			
			
			home_page_middle_col_section01.init = function(){
			    
				// get a jq reference to the table holding all this content
				home_page_middle_col_section01.table_node = $('#homePageMiddleColSection01');
				
				home_page_middle_col_section01.overflow_node = $(home_page_middle_col_section01.table_node).find('.overflowDiv');
				var overflow_parent = $(home_page_middle_col_section01.overflow_node).parent();
				//console.log(overflow_parent.height());
				
				home_page_middle_col_section01.overflow_node = $(home_page_middle_col_section01.table_node).find('.overflowDiv').css(
					{
						'height': overflow_parent.height() + 'px'
					}
				);
				
				
				//$(home_page_middle_col_section01.table_node).find('.leftCol').hover(
				$(home_page_middle_col_section01.table_node).hover(
					function(){
						//console.log('over table');
						home_page_middle_col_section01.stopRotate();
					},
					function(){
						//console.log('out table');
						// turn it back on, in just a second, so as not to be so jarring
						home_page_middle_col_section01.interval_reference = setTimeout("home_page_middle_col_section01.rotate()", 1500);
					}
				);
				
				
				
				// find/loop through all the right columns
				home_page_middle_col_section01.td_arr = $(home_page_middle_col_section01.table_node).find('.rightCol');
				
				//$(home_page_middle_col_section01.table_node).find('.rightCol').each(
				$(home_page_middle_col_section01.td_arr).each(
					function(i){
						//console.log(i);
						home_page_middle_col_section01.num_tds++;
						
						// start out the page with all inactive
						$(this).find('dl').css(
							{
								'border-width': '1px 0 0 3px',
								'border-style': 'solid',
								'border-color': '#e7e7e7',
								'background-color': '#f5f5f5'
							}
						);
						
						// set the on events for the tds on the right side
						$(this).hover(
							function(){
								//console.log('over right cell');
								
								// remove the active class from any/all right cells
								//$(home_page_middle_col_section01.td_arr).removeClass('active').addClass('inactive');
								$(home_page_middle_col_section01.td_arr).find('dl').css(
									{
										'border-width': '1px 0 0 3px',
										'border-style': 'solid',
										'border-color': '#e7e7e7',
										'background-color': '#f5f5f5'
									}
								);
								
								// mark this right cell as active
								//$(this).addClass('active').removeClass('inactive');
								$(this).find('dl').css(
									{
										'border-width': '1px 0 0 3px',
										'border-style': 'solid',
										'border-color': '#e7e7e7 #e7e7e7 #e7e7e7 #73c513',
										'background-color': '#fff',
										//'cursor': 'pointer' // nina asked for this to be removed
									}
								);
								
								
								// hide anything on the left
								home_page_middle_col_section01.hideArticles();
								
								
								// get the class for the dl in this td. it is an article id
								var aid = $(this).find('dl').attr('class');
								//console.log(aid);
								// show the corresponding article in the left col. select by the div's class: aidXXX 
								//$(home_page_middle_col_section01.table_node).find('.aid' + aid).show().addClass('active');
								$(home_page_middle_col_section01.table_node).find('.aid' + aid).addClass('active').show().animate(
				                	{
				                    	'left': 0,
									}, 
									500,
									function(){
										// complete
									}
								);
							},
							function(){
								//console.log('out right cell');
							}
						);
					}
				);
				
				
				home_page_middle_col_section01.interval_td_index = -1;
				home_page_middle_col_section01.rotate();
				
			} // end init
			
			home_page_middle_col_section01.rotate = function(){
				//console.log('call rotate');
				//console.log('home_page_middle_col_section01.interval_td_index: ' + home_page_middle_col_section01.interval_td_index);
				
				var next_index = (home_page_middle_col_section01.interval_td_index + 1);
				if(next_index >= home_page_middle_col_section01.num_tds){
					next_index = 0;
				}
				
				//$(home_page_middle_col_section01.table_node).find('.rightCol').each(
				$(home_page_middle_col_section01.td_arr).each(
					function(i){
						//console.log('loop rightCols');
						//console.log('i: ' + i);
						//console.log('next_index: ' + next_index);
						if(i === next_index){
							//console.log('=========  i == next_index');
							// trigger the td click to show the next one
							$(this).mouseover();
							home_page_middle_col_section01.interval_td_index = i;
						}
					}
				);
				
				home_page_middle_col_section01.interval_reference = setTimeout("home_page_middle_col_section01.rotate()", home_page_middle_col_section01.interval_delay);
					
			} // end autoRotate
			
			home_page_middle_col_section01.stopRotate = function(){
				//console.log('rotate off');
				clearTimeout(home_page_middle_col_section01.interval_reference);
			}
			
			
			home_page_middle_col_section01.hideArticles = function(){
			
				//$(home_page_middle_col_section01.table_node).find('.leftColDiv').hide();
				$(home_page_middle_col_section01.table_node).find('.leftCol .active').each(
					function(index, self) {
		                var $this = $(this);
		                $this.removeClass('active').animate(
		                	{
		                    	left: ($this.width() + 5)
							}, 
							500,
							function(){
								$this.css('left', ( -$this.width() - 10));
							}
						);
					}
	            );

				
			}
		</script>
