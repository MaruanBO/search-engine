function FBLogin(){
	//Calling this method to fire a coremetrics conversion tag during FB click.
	cmCreateConversionEventTag("Facebook Connect","2","Login-Header",null);
	FB.getLoginStatus(function(user) {
	var host = "https://"+document.location.hostname;
	if(user.status =='connected'){
		FB.api('/me', function(response) { 
			if (user.authResponse) {
				if(response.email != null && user.authResponse.userID!=null && user.status=='connected'  ){
					var url = host+"/facebooklogin.do?fbEmail="+response.email+"&fbUid="+user.authResponse.userID+"&preLink="+'welcome.do';
					window.location = url;
				}
			}
		});
	}else{
		FB.login(function(user) {
			FB.api('/me', function(response) { 
			   	if (user.authResponse) {
					if(response.email != null && user.authResponse.userID!=null && user.status=='connected' ){
						var url = host+"/facebooklogin.do?fbEmail="+response.email+"&fbUid="+user.authResponse.userID+"&preLink="+'welcome.do';
						window.location = url;
					}
				} 
			});
		}, {scope:'email,user_birthday,status_update,publish_stream,friends_about_me,user_likes,friends_likes,friends_birthday'});
	}
});
}
function FbConnect(){
FB.getLoginStatus(function(user) {
	if(user.status =='connected'){
		FB.api('/me', function(response) { 
			if (user.authResponse) {
				if(response.email != null && user.authResponse.userID!=null && user.status=='connected'  ){
					document.forms["FaceBookForm"].email.value = response.email;													  													  
					document.forms["FaceBookForm"].fbId.value = user.authResponse.userID;	
					document.forms["FaceBookForm"].prePage.value = 'welcome.do'
					document.forms["FaceBookForm"].facebookStatus.value = 'disconnect';
			 		var imgSRC ="https://graph.facebook.com/"+user.authResponse.userID+"/picture";
					document.getElementById("fbUserImg").setAttribute('src', imgSRC);
					document.getElementById("faceBookEmail").innerHTML=response.email;	
					document.getElementById("faceBookName").innerHTML=response.name;
					document.getElementById("faceBookConnect").style.display="block";
				}
			}
		});

	}else{
		FB.login(function(user) {
			FB.api('/me', function(response) { 
		  		if (user.authResponse) {
		  	 	 if(response.email != null && user.authResponse.userID!=null && user.status=='connected' ){
   			 		document.forms["FaceBookForm"].email.value = response.email;													  													  
					document.forms["FaceBookForm"].fbId.value = user.authResponse.userID;	
					document.forms["FaceBookForm"].prePage.value = 'welcome.do'
					document.forms["FaceBookForm"].facebookStatus.value = 'disconnect';
			 		var imgSRC ="https://graph.facebook.com/"+user.authResponse.userID+"/picture";
					document.getElementById("fbUserImg").setAttribute('src', imgSRC);
					document.getElementById("faceBookEmail").innerHTML=response.email;	
					document.getElementById("faceBookName").innerHTML=response.name;
					document.getElementById("faceBookConnect").style.display="block";
			 	}
			}
		});
	}, {scope:'email,user_birthday,status_update,publish_stream,friends_about_me,user_likes,friends_likes,friends_birthday'});
	
	}
});
	
}

</script>
