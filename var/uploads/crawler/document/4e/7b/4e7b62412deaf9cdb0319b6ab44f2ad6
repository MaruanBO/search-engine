  <script type="text/javascript">
  
   function signinCallback(authResult) 
   {
	  if (authResult['status']['signed_in']) {
	  try{
	 	var gsPageId = document.getElementById("gsPageId");
		if(gsPageId != null && gsPageId != 'undefined' && gsPageId.value != null && gsPageId.value == 'checkoutsignin'){
	  		cmCreateConversionEventTag("GooglePlus","2","Login-Checkout",null);
	  	}else{
	  		cmCreateConversionEventTag("GooglePlus","2","Login-Header",null); 
	  	}
	  }catch(e){
	  }	
	  	var accessToken = authResult['access_token'];
	 	 // Update the app to reflect a signed in user
	    // Hide the sign-in button now that the user is authorized, for example:
		if(document.getElementById('gplus_connect') != null && document.getElementById('gplus_connect') != 'undefined')
		{
	    	document.getElementById('gplus_connect').setAttribute('style', 'display: none');
		} 
	    if(document.getElementById('customBtn') != null && document.getElementById('customBtn') != 'undefined')
		{
	    	document.getElementById('customBtn').setAttribute('style', 'display: none');
		}
	    
		// This sample assumes a client object has been created.
	  	// To learn more about creating a client, check out the starter:
	  	//  https://developers.google.com/+/quickstart/javascript
		gapi.client.load('plus', 'v1', function() {
		gapi.client.plus.people.get({'userId' : 'me'}).execute(function(resp){
		
		var userAuth = document.getElementById("userAuth");
		var prePage = 'welcome.do';
		
		if(userAuth == null || userAuth.value == "")  //not signed in
	   	{
	   		var host = "https://"+document.location.hostname;
			var url = host+"/GooglePlusAction.do?gpEmail="+resp.emails[0].value+"&gpUid="+resp.id+"&gpFirstName="+resp.name.givenName+"&gpLastName="+resp.name.familyName+"&accessToken="+accessToken+"&gpImage="+resp.image.url+"&prePage="+prePage;
			window.parent.location = url;
		}
		else
		{
		document.forms["GooglePlusForm"].email.value = resp.emails[0].value;													  													  
		document.forms["GooglePlusForm"].gpId.value = resp.id;	
		document.forms["GooglePlusForm"].prePage.value = 'welcome.do';
		document.forms["GooglePlusForm"].googleStatus.value = "gconnect";
		document.forms["GooglePlusForm"].accessToken.value = accessToken;
		document.forms["GooglePlusForm"].gpImage.value = resp.image.url;
		document.getElementById("gpUserImg").setAttribute('src', resp.image.url);
		document.getElementById("googleEmail").innerHTML=resp.emails[0].value;	
		document.getElementById("googleName").innerHTML=resp.name.givenName;
		document.getElementById("GooglePlusConnect").style.display="block";
		}
  		})
		});
	  } else {
	    // Update the app to reflect a signed out user
	    // Possible error values:
	    //   "user_signed_out" - User is signed-out
	    //   "access_denied" - User denied access to your app
	    //   "immediate_failed" - Could not automatically log in the user
	   // console.log('Sign-in state: ' + authResult['error']);
	  }
	}
	
