  function render() 
  {
  	var gConnect = document.getElementById('gConnect');
	if(gConnect != null && gConnect != 'undefined' && gConnect.value =='true')
	{
		if(document.getElementById('gplus_connect') != null && document.getElementById('gplus_connect') != 'undefined')
		{
	    document.getElementById('gplus_connect').setAttribute('style', 'display: none');
		} 
	    if(document.getElementById('customBtn') != null && document.getElementById('customBtn') != 'undefined')
		{
	    document.getElementById('customBtn').setAttribute('style', 'display: none');
		}
	    
	}
   // Additional params including the callback, the rest of the params will
   // come from the page-level configuration.
   var additionalParams = {     
      'callback': 'signinCallback',
      'clientid': '316309773155.apps.googleusercontent.com',
      'cookiepolicy': 'single_host_origin',
      'requestvisibleactions': 'http://schemas.google.com/AddActivity',
      'scope': 'https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/userinfo.email'
   };

   // Attach a click listener to a button to trigger the flow.
   var signinButton = document.getElementById('customBtn');
   if(signinButton != null && signinButton !='undefined')
   {
	   signinButton.addEventListener('click', function() {
	   gapi.auth.signIn(additionalParams); // Will use page level configuration
	   });
   }
   
    var gplusHeaderButton = document.getElementById('customBtnHeader');
    if(gplusHeaderButton != null && gplusHeaderButton !='undefined')
    {
	   	gplusHeaderButton.addEventListener('click', function() {
	    gapi.auth.signIn(additionalParams); // Will use page level configuration
	    });
   	}
   	
   	var gplusConnectLink = document.getElementById('gplusConnectLink');
   	if(gplusConnectLink != null && gplusConnectLink !='undefined')
    {
    	gplusConnectLink.addEventListener('click', function() {
	    gapi.auth.signIn(additionalParams); // Will use page level configuration
	    });
   	}
 }
  
  </script>
  
