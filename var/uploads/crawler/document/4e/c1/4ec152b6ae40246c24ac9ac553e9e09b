<script type="text/javascript" src="/js/lib/jquery.hoverIntent.minified.js"></script>
<script type="text/javascript">
	$(document).ready(function(){
		//$('ul.nav li div:first').css('background','#ff0');
		var config = {
			 sensitivity: 3, // number = sensitivity threshold (must be 1 or higher)
			 interval: 200,  // number = milliseconds for onMouseOver polling interval
			 over: doOpen,   // function = onMouseOver callback (REQUIRED)
			 timeout: 400,   // number = milliseconds delay before onMouseOut
			 out: doClose    // function = onMouseOut callback (REQUIRED)
		};

		function doOpen() {
			if(!$(this).find('a.main-button').hasClass('this-seg'))
			{
				var dropdown = $(this).find('.mega-nav');
				if (dropdown.length != 0)
				{
					$(this).addClass('this-seg-over');
					$(this).find('a.main-button').addClass('this-seg-over');
				}
			}
			$('div', this).fadeIn(100);
		}

		function doClose() {
			if($(this).hasClass('this-seg-over'))
			{
				/* var dropdown = $(this).find('.mega-nav');
				if (dropdown.length != 0) */
				{
					$(this).removeClass('this-seg-over');
					$(this).find('a.main-button').removeClass('this-seg-over');
				}

			}
			$('div', this).fadeOut(100);
		}

		$("ul.nav li").hoverIntent(config);
	});
</script>


