        <script type='text/javascript'>
            
            var home_page_carousel_obj = {};
            home_page_carousel_obj.include_right_toc = 0;
            home_page_carousel_obj.num_main_slides = 6;
            home_page_carousel_obj.main_slide_index = 0;
            home_page_carousel_obj.main_carousel_div;
            home_page_carousel_obj.toc_carousel_div;
            
            home_page_carousel_obj.scrollToc = function(){
                //console.log('scroll toc');
                
                $(home_page_carousel_obj.main_carousel_div).find('.carousel-inner .item').each(
                    function(x){
                        //console.log('x: ' + x);
                        if($(this).hasClass('active')){
                            home_page_carousel_obj.main_slide_index = x;
                            //console.log('home_page_carousel_obj.main_slide_index: ' + home_page_carousel_obj.main_slide_index);
                            home_page_carousel_obj.animateToc();
                        }
                    }
                );
            }
            
            home_page_carousel_obj.animateToc = function(){
                //console.log('home_page_carousel_obj.animateToc');
                //console.log('home_page_carousel_obj.main_slide_index: ' + home_page_carousel_obj.main_slide_index);
                
                $(home_page_carousel_obj.toc_carousel_div).find('li').each(
                    function(x){
                        //console.log('li x: ' + x);
                        
                        if(x <= home_page_carousel_obj.main_slide_index){
                            $(this).slideUp();
                        }else{
                            //$(this).show();
                            $(this).slideDown();
                        }
                        
                    }
                );
            }
            
            $(document).ready(function() {
                
                console.log('slideshow ready');
                
                home_page_carousel_obj.main_carousel_div = $('#homePageCarousel');
                
                home_page_carousel_obj.toc_carousel_div = $('#homePageCarouselToc');
                
                // 2014-12-12 NOT NEEDED
                // THE BOOTSTRAP FILE STARTS THIS BY ITSELF via $(window).on('load')
                // see /html/javascript/bootstrap.carousel.js
                //$('.carousel').carousel();
                
                
                // set the event handlers for the bootstrap carousel
                // this DOES affect the bootstrap carousel code running
                $('.carousel').on(
                    'slide.bs.carousel', 
                    function () {
                        //console.log('slide.bs.carousel');
                    }
                );
                
                $('.carousel').on(
                    'slid.bs.carousel', 
                    function () {
                        if(home_page_carousel_obj.include_right_toc == 1){
                            home_page_carousel_obj.scrollToc();
                        }
                    }
                );
                
                /* 
                clone all the toc LIs and add them to the end of the orig LIs
                this allows us to traverse the slides and never run out
                */
                $(home_page_carousel_obj.toc_carousel_div).find('li').clone().appendTo( $(home_page_carousel_obj.toc_carousel_div).find('ul') );
                // remove the first one, it is the same as the first main slide so it only gets shown at the end (which we handled in the clone above)
                $(home_page_carousel_obj.toc_carousel_div).find('li').slice(0, 1).hide();
                
            });
        </script>
        <!-- END home page middle column js -->
