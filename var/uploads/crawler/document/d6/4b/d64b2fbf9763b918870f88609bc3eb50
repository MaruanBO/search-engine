					}
					
				}
			}
		); /// end the code that hijacks the signup anime div
		
		
		console.log('line: 674');
		console.log('dhtml_signup_display_delay: ' + dhtml_signup_display_delay);
		// sometimes delay == 0
		setTimeout('animateDhtmlSignup()', dhtml_signup_display_delay);
		

	} // end main func
); // end ready;

