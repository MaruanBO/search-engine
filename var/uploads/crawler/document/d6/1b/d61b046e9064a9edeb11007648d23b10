    $('.tuesday-time').hover(function(){
        $('img.tuesday-time').each(function() {
           newTimeSrc = $(this).attr('src').replace('.png','-hover.png');   
           $(this).attr('src', newTimeSrc);
        });
    },     
    function(){
        $('img.tuesday-time').each(function() {
           newTimeSrc = $(this).attr('src').replace('-hover.png','.png');     
           $(this).attr('src', newTimeSrc);
        });
    });
	var newMoneySrc = "";
    $('.tuesday-money').hover(function(){
        $('img.tuesday-money').each(function() {
           newMoneySrc = $(this).attr('src').replace('.png','-hover.png');   
           $(this).attr('src', newMoneySrc);
        });
