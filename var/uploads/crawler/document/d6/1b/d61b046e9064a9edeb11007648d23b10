	$('.bugmenot').on('click', function() {
		$.cookie('viewed-ad-pop', 'yes', {
			expires: 10000,
			path: '/'
		});
	});
	if ($.cookie('visited') % 2 === 1 && $.cookie('viewed-ad-pop') == null) {
		setTimeout(function() {
			$('#ad-pop').bPopup({
				zIndex: 10001
			});
		}, 2000);
		visited = $.cookie('visited');
		visited = parseInt(visited, 10);
		visited++;
		$.cookie('visited', visited, {
			expires: 10000,
			path: '/'
		});
	} else {
		$.cookie('visited', '1', {
			expires: 10000,
			path: '/'
		});
	}
});
