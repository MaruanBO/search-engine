jQuery(document).ready(function($) {
	var date = new Date();
	date.setTime(date.getTime() + (8 * 60 * 60 * 1000));
	$("#clickToGiveButtonGGC").click(function() {
		$.cookie('clicked', 'yep', {
			expires: date
		});
	});
	if ($.cookie('clicked') == 'yep') {
		$('#button-ready').hide();
		$('#button-thanks').show();
		$('#thanks-funding').show();
	} else {
		$('#button-ready').show();
		$('#button-thanks').hide();
	}
	$('.email-pop').bind('click', function(e) {
		e.preventDefault();
		$('#email-pop').bPopup({
			zIndex: 10001
		});
	});
	function preload(arrayOfImages) {
	    $(arrayOfImages).each(function(){
	        $('<img/>')[0].src = this;
	        // Alternatively you could use:
	        // (new Image()).src = this;
	    });
	}
	preload([
