    // Set client and delivery
    var _iaq = [
        ['client', 'wcpo'],
        ['delivery', 74],
        ['track', 'consume']
    ];

    // Include the Idio Analytics JavaScript
    (function(d,s){var ia=d.createElement(s);ia.async=1,s=d.getElementsByTagName(s)[0],ia.src=('https:'===d.location.protocol?'https://ssl-':'http://')+'s.idio.co/ia.js',s.parentNode.insertBefore(ia,s)})(document,'script');

    // Custom Idio Tracking
    (function idio(){
        if (typeof jQuery != "undefined" && typeof paywallGtmCookie != "undefined"  && typeof _iaq != "undefined"){
            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for(var i=0; i<ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') c = c.substring(1);
                    if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
                }
                return "";
            }
            
            // Viewed Insider Excerpt 
            if(jQuery('meta[name=isPremium]').attr('content') == "true"){
                _iaq.push(['group', 'Viewed Insider Excerpt']);
                _iaq.push(['track', 'join']);
            }

            // Viewed Subscriber Page
            if(/wcpo.com\/subscribe($|\?|#)/.test(window.location.href)){
                _iaq.push(['group', 'Viewed Subscriber Page']);
                _iaq.push(['track', 'join']);
            }

            // Is a subscriber
            if(paywallGtmCookie.SubscriberStatus == true){
            	_iaq.push(['user','subscriberid', paywallGtmCookie.SubscriberID]);
                _iaq.push(['group', 'Subscriber']);
                _iaq.push(['track', 'join']);
            }

            // Track bundle
            var c = getCookie("paywall_selected_planCode");
            if (c !== ""){

                _iaq.push(['group', 'Bundle: ' + c]);

                _iaq.push(['track', 'join']);

            }


        } else {
            // run again once jQuery has loaded
            setTimeout(idio, 200);
