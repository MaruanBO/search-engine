		<script type="text/javascript">
		jQuery(document).ready(function($) {
					});
		function refresh_news_widget(id, speed) {
			var data = {
				action: 'news_widget_refresh',
				id: id
			};

			jQuery.post('http://www.gunsandammo.com/wp-admin/admin-ajax.php', data, function(response) {
				if(response == '') return;
				jQuery('#' + data.id).replaceWith(response);
				setTimeout(function() {
					refresh_news_widget(id, speed);
				}, speed);
			});
		}
		</script>
