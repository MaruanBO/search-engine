<script>
if( $('ff_sub_tabbar_bookatrip').hasClassName("active") ){
show_ff_sub_tab('bookatrip');
} else if( $('ff_sub_tabbar_redeem').hasClassName("active") ){
show_ff_sub_tab('redeem');
