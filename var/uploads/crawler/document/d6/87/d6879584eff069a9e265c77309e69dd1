				function askTextField()
								{
									if(document.getElementById('txtQuestion').value == '') {
										document.getElementById('txtQuestion').value = "Ask me a question";
									}	
								}
								function askTextFieldFocus()
								{
									if(document.getElementById('txtQuestion').value == "Ask me a question") {
										document.getElementById('txtQuestion').value = "";
									}	
								}

				</script>
