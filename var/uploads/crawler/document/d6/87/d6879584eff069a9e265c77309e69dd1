<style>

#autocomplete_availability_from[style],#discounts_from_auto[style] {
	   
	    left: 10px !important;
	    top: 60px !important;
	   
}
#autocomplete_availability_to[style],#discounts_to_auto[style] {
	   
	    left: 10px !important;
	    top: 105px !important;
	   
