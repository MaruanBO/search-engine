<script type="text/javascript">
var SSAP = window.SSAP = window.SSAP || {};

SSAP.view = "homepage";

SSAP.generateHtml = function(options) {
  var feed_type_class = "";
  if(ACTIVE.ui.controller_name == "search"){
    feed_type_class = " search-item";
  }
  html =  '<article class="ie-activity-list'+ feed_type_class +' sponsored-search-item">';
  html += '<div class="row">';
  html += '<div class="span8 ie-content">';
  html += '<div class="row">';
  html += '<a href="'+options.url+'" class="ie-article-link" data-asset-id="'+options.assetId+'">';
  html += '<div class="span1 feed-left-column">'
  html += '<span>';
  html += '<div class="single-date-row">'
  html += '<span class="h5">'+options.startDate.toString('MMM')+'</span>';
  html += '<span class="h2">'+options.startDate.toString('dd')+'</span>';
  html += '<span class="h5">'+options.startDate.toString('yyyy')+'</span>';
  html += '</div>';
  html += '</span>';
  html += '</div>';
  html += '<div class="span7 feed-center-column">';
  html += '<div class="article-content">';
  html += '<h5 class="title">'+options.title+'</h5>';
  html += '<h6 class="desc-info">';
  html += options.city + ', ' + options.state;
  html += '</h6>';
  html += '<h6 class="secondary-text desc-info">'
  html += '&nbsp';
  html += '<span class="p pull-right inline">';
  html += '<span class="instant-ads-promotion-link" data-href="http://www.activenetwork.com/software/instant-ads.htm?int=29-82" data-asset-id="'+options.assetId+'" data-target="_blank">Promoted Activity</span>';
  html += '</span></h6></div></div></a></div></div></div></article>';
  return html;
}

SSAP.loadA3SearchAds = function() {

    ssap_ads1 = new SSAP_ADS({
        topic : "",
        attribute : "",
        keywords : "",
        pageId : 15003,
        config:[{
                listings_div: "a3search-listings-1",
                adUnits : [399262]
            },
            {
                listings_div: "a3search-listings-2",
                adUnits : []
            },
            {
                listings_div: "a3search-listings-3",
                adUnits : []
            }
        ],
        callback: function() {
            var jsonAds = ssap_ads1.getJsonAds();
            for (var i = 0; i < jsonAds.length; i++) {
                var ad = jsonAds[i];                
                var html = SSAP.generateHtml({
                    assetId: ad.assetId,
                    title: ad.title,
                    city: ad.city,
                    state: ad.state,
                    startDate: Date.parse(ad.startDate),
                    endDate: ad.endDate,
                    dateString: ad.dateStr,
                    description: ad.description,
                    url: ad.url
                });
                                
                if(SSAP.view == 'search') {
                    // div = $('#' + ad.listingsDiv).first();
                    $('#' + ad.listingsDiv).first().before(html);
                } else {
                    // Supporting instant ads on LPF pages
                    // var div = $('#lpf-tabs2-a > #' + ad.listingsDiv).first();
                    $('#lpf-tabs2-a > #' + ad.listingsDiv).first().before(html);
                    // div = $('#lpf-tabs2-b > #' + ad.listingsDiv).first();
                    $('#lpf-tabs2-b > #' + ad.listingsDiv).first().before(html);
                }
            }
            new ACTIVE.powerplant.factory('instant_ads_link_helper');

        }
    });
    ssap_ads1.initAds();
}

SSAP.loadA3HomePageAds = function() {

    ssap_ads1 = new SSAP_ADS({
        topic : "",
        attribute : "",
        keywords : "",
        pageId : 14351,
        config:[{
                listings_div: "a3search-listings-1",
                adUnits : [335345]
            },
            {
                listings_div: "a3search-listings-2",
                adUnits : [335347]
            },
            {
                listings_div: "a3search-listings-3",
                adUnits : [335348]
            }
        ],
        callback: function() {
            var jsonAds = ssap_ads1.getJsonAds();
            for (var i = 0; i < jsonAds.length; i++) {
                var ad = jsonAds[i];

                var html = SSAP.generateHtml({
                    title: ad.title,
                    city: ad.city,
                    state: ad.state,
                    startDate: Date.parse(ad.startDate),
                    endDate: ad.endDate,
                    dateString: ad.dateStr,
                    description: ad.description,
                    url: ad.url
                });
                var div = $('#ab-tabs1-a > #' + ad.listingsDiv).first();
                div.before(html);
                div = $('#ab-tabs1-b > #' + ad.listingsDiv).first();
                div.before(html);
            }
            SSAP.loadA3PersonalizedEvents($('#instant-ads-cycling, #instant-ads-running, #instant-ads-walking').toArray());
        }
    });
    ssap_ads1.initAds();
}

SSAP.loadA3PersonalizedEvents = function(divs) {
    if(divs.length == 0) {
        return;
    }

    var id = $(divs).first().attr('id');
    var topic  = id.substr(id.lastIndexOf('-') + 1);

    ssap_ads1 = new SSAP_ADS({
                            topic : topic,
                            pageId : 15003,
                            config:[{listings_div: id, adUnits : [399262]}],
                            callback: function() {
                                    var jsonAds = ssap_ads1.getJsonAds();
                                    for (var i = 0; i < jsonAds.length; i++)
                                    {
                                        var ad = jsonAds[i];
                                        var html = SSAP.generateHtml({title: ad.title,
                                                                        city: ad.city,
                                                                        state: ad.state,
                                                                        startDate: ad.startDate,
                                                                        endDate: ad.endDate,
                                                                        dateString: ad.dateStr,
                                                                        description: ad.description,
                                                                        url: ad.url});
                                        var div = $(divs).first();//document.getElementById(ad.listingsDiv);
                                        div.html(html);
                                        div.attr('style', 'display')
                                        // div.style.display = '';
                                    }

                                    divs.shift()
                                    if(divs.length > 0) {
                                        SSAP.loadA3PersonalizedEvents(divs);
                                    }
                                }
                            });



    ssap_ads1.initAds();
}
if(SSAP.view != 'homepage') {
    SSAP.loadA3SearchAds();
} else {
    SSAP.loadA3HomePageAds();
}
</script>


