        googletag.defineSlot('/6031/'+ACTIVE.ui.adParams.environment+'/'+ ACTIVE.ui.adTargeting.channel +'//'+ ACTIVE.ui.adTargeting.subchannel +'/' + ACTIVE.ui.adTargeting.feature +'//'+ ACTIVE.ui.adTargeting.subfeature1 +'//' + ACTIVE.ui.adTargeting.subfeature2 + '/' + ACTIVE.ui.adTargeting.subview(), [1, 1], "pop_up")
          .addService(googletag.pubads())
            .setTargeting("position", "pop_up");
      });
      ACTIVE.ad_positions.push("pop_up");
    }
  </script>
</div>


