<script type="text/javascript">
    var n = $("#featured-block div.items > ul > li").length - 1;
    var i = 0;
    if (n > 0) {
        setInterval(function() {animateHeadlines()}, 3000);
    }
    
    function animateHeadlines() {
        if (i < n) {
            i += 1;
            $("#featured-block div.items > ul > li").fadeOut(300);
            $("#featured-block div.items > ul > li:eq(" + i + ")").fadeIn(300);
        }
        else {
            i = 0;
            $("#featured-block div.items > ul > li").fadeOut(300);
            $("#featured-block div.items > ul > li:eq(" + i + ")").fadeIn(300);
        }
    }
</script>

</div></div><div id="tncms-region-top-custom-index" class="tncms-region "></div><div class="clear"></div>

