<script type="text/javascript">
    var d = new Date('Thursday, February 19, 2015 3:32 PM');
    var startDate = new Date('2/22/2015');
    var endDate = new Date('3/09/2015');
    var url;
    var img;
    
    var ad_spot = $("div.tw-leaderboard-200x90");
    
    if (ad_spot.length > 0) {
        if (!ad_spot.find("img").length > 0) {
            display200x90AdSpot();
        }
    }

    function display200x90AdSpot() {
        if ((d > startDate) && (d < endDate)) {
            url = "http://www.tulsaworld.com/site/outlook2015.html";
            img = "http://tulsaworld.com/content/tncms/live/global/resources/images/outlook-200x90.jpg";
            ad_spot.html("<a href=\"" + url + "\"><img src=\"" + img + "\" /></a>");
        }
        else {
            url = "http://www.tulsaworld.com/ultimategaragesale/";
            img = "http://tulsaworld.com/content/tncms/live/global/resources/images/indoor-garage-sale-200x90.gif";
            ad_spot.html("<a href=\"" + url + "\"><img src=\"" + img + "\" /></a>");
        }
    }	
