<script type="text/javascript">
var currentTime = new Date()
var y = currentTime.getFullYear().toString()
var m = currentTime.getMonth()+2
if (m <10) { m = "0"+ m}
if (m > 12) { m = m -12 ; y++}
var firstnext= y+ "-" + m + "-01"
var d = daysInMonth(m,y)
var lastnext= y + "-" + m + "-" + d
var CRITEO_CONF = [[{
    pageType: 'home'
},
