            <script type='text/javascript'>
        function processScroll() {
            var i, scrollTop = $win.scrollTop()
            if ( !isFixed && scrollTop >= navTop && !InSkinPubliOn ) {
                isFixed = true
                $body.addClass('navfixed');
            } else if ( isFixed && scrollTop <= navTop ) {
                isFixed = false
                $body.removeClass('navfixed');
            }
        }
    </script>
