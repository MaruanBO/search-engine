    <script type="text/javascript">
        if(document.cookie.indexOf("cookie_policy") < 0 ){
            var moveAfter = false;
            var div = document.getElementById('container');
            if (!div) {
                div = document.getElementById('menuSuperior');
                moveAfter = true;
            } else {
                if (window.location.href.search('/gallery/2') > 0) {
                    div.className = div.className + " gallery";
                }
            }
            if (div) {
		var el = document.createElement("div");
		el.innerHTML = '	<div id="cookiesBanner">		<div style="position: relative; padding: 15px 0;">			<img src="http://multimedia.hola.com/ns/hola/comunes/infoCookies.png" class="infoCookies" />			<p>This site uses cookies. By continuing to browse the site you are agreeing to our use of cookies.  <a href="http://contact.hellomagazine.com/cookies/" target="_blank">Find out more here</a></p>			<a href="#" onclick="javascript: document.getElementById(\'cookiesBanner\').style.display=\'none\';" class="close"><img src="http://multimedia.hola.com/ns/hola/comunes/close-window-cookies-w.png" /></a>		</div>	</div>';
		div.insertBefore(el, div.firstChild);
                if (moveAfter) {
                    var divCookiesBanner = document.getElementById('cookiesBanner');
                    div.parentNode.insertBefore(divCookiesBanner, div.nextSibling);
                }
            }
        }
        
        
    /* cookie */

	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}
	function readCookie(name) {
	    var nameEQ = name + "=";
	    var ca = document.cookie.split(';');
	    for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	    }
	    return null;
	}
	(function(){
		var d_body = document.getElementsByTagName("body")[0];
		var cookieLinks = false;
		d_body.addEventListener("click",function(ev){
			ev.stopPropagation();
			if(readCookie("cookie_policy") != "true"){			
				if(ev.target == document.querySelectorAll("#cookiesBanner a")[0] || ev.target == document.querySelectorAll("#cookiesBanner a")[1] || ev.target == document.querySelectorAll("#cookiesBanner img")[1]){
					cookieLinks = true;
				}else{
					cookieLinks = false;
				}
				if(!cookieLinks){
					createCookie("cookie_policy", "true", 5*365);
				}				
			}
		},false);
	})();
    </script>
