   <script type="text/javascript">
        (function(){

            var clickCloseBtn = function(){
                document.getElementById("js_Topclose").onclick = function(){
                    document.getElementById("topPop").style.display = "none";
                   localStorage.setItem('topPop',1);
                }
            };
            var fBrowserRedirect = function(){
                var sUserAgent  = navigator.userAgent.toLocaleLowerCase();
                var bIsIpad  =  sUserAgent.match(/ipad/i) == "ipad";
                var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
                var andriodOs = sUserAgent.match(/android/i) == "android";

                if(bIsIphoneOs ){ //$(".js_androidShow").hide();$(".js_iosShow").show();

                    if(localStorage.getItem('topPop') != 1){
                        document.getElementById("topPop").style.display = "block";
                        clickCloseBtn();
                    }
                }else if(andriodOs){
                	$(".js_androidShow").show();$(".js_iosShow").hide();
                	if(localStorage.getItem('topPop') != 1){
                        document.getElementById("topPop").style.display = "block";
                        clickCloseBtn();
                    }
                }
            }
            fBrowserRedirect();
        })()
   </script>
   


