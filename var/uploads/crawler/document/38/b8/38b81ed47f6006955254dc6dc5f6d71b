<script>


  var navToggle = $('.navbar-toggle'),
      navState = navToggle.attr('data-toggle'),
      dropdownToggle = $('.dropdown-toggle'),
      dropdownMenu = $('.dropdown-menu'),
      navbarCollapse = $('.navbar-collapse'),
      navbarHeader = $('.navbar-header');

  /*if (navState == 'collapse') {
    navToggle.addClass('closed');
    navToggle.click(function(){
      navToggle.toggleClass('closed')
    });
  }*/


  if(/Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)){
      dropdownToggle.click(function(){
        var href = $(this).attr('href');
        window.location.href = href;
      });

      //Allow scrolling of nav on mobile
      navbarCollapse.css({ maxHeight: $(window).height() - navbarHeader.height() + 'px' });

      window.addEventListener('orientationchange', function() {
        navbarCollapse.css({ maxHeight: $(window).height() - navbarHeader.height() + 'px' });
      }, false);
  }
  else{
    if (window.matchMedia('(orientation: landscape)').matches) {
      //Products dropdown animation
      dropdownToggle.mouseenter(function(){
        dropdownMenu.slideToggle('fast').show();
      });
      dropdownMenu.mouseleave(function(){
        $(this).slideToggle('fast');
      });
      dropdownToggle.click(function(){
        var href = $(this).attr('href');
        window.location.href = href;
      });
    }
    else{
      dropdownToggle.click(function(){
        var href = $(this).attr('href');
        window.location.href = href;
      });
      navbarCollapse.css({ maxHeight: $(window).height() - navbarHeader.height() + 'px' });
    }
  }


  //Implement smooth scroll for anchor tags
  $('a[href*=#]:not([href=#])').click(function() {
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      var target = $(this.hash);
      target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      if (target.length) {
        $('html,body').animate({
          scrollTop: target.offset().top
        }, 1000);
        return false;
      }
    }
  });
</script>

