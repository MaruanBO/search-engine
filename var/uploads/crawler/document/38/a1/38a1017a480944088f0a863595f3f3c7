	<!-- incentivized sharing -->
	<script>
	/* ------- Trigger when sharing buttons are clicked ------- */
		$(window).load(function() {
			$( ".share-buttons a" ).on( "click", { }, asGetUniqueCode );
		});

		/* ------- check if there is already a unique code if not pull one ------- */
		function asGetUniqueCode(){
			if(checkCookie("asSideShowShareCode")){
				var asTheCode = getCookie("asSideShowShareCode");
				asAssignCouponCode(asTheCode);
			} else {
				$.get( "https://www.sideshowtoy.com/cgi-bin/gcauto-AddShoppers-IncentivizeSharing.pl?1424381326", function( data ) {
					var asTheCode = data;
					setCookie("asSideShowShareCode",asTheCode,1);
					asAssignCouponCode(asTheCode);
				});
			}
		}

		/* ------- assign unique code to social reward value ------- */
		function asAssignCouponCode(asTheCode){
			console.log(asTheCode);
			AddShoppersWidget.coupons.facebook.code = asTheCode;
			AddShoppersWidget.coupons.twitter.code = asTheCode;
			AddShoppersWidget.coupons.pinterest.code = asTheCode;
			AddShoppersWidget.coupons.email.code = asTheCode;
			AddShoppersWidget.coupons.google.code = asTheCode;
		}

		/* ------- Cookie Monster! - check/set/get cookies ------- */
		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays*24*60*60*1000));
			var expires = "expires="+d.toGMTString();
			document.cookie = cname + "=" + cvalue + "; " + expires + ";path=/";
		}
		//gets the cookie value returned
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i=0; i<ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1);
				if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
			}
			return "";
		}
		//checks if cookie exists and returns true or false
		function checkCookie(ascscname) {
			var ascscval = getCookie(ascscname);
			if (ascscval != "") {
				//cookie is present
				return true;
			} else {
				ascscval = "";
				if (ascscval != "" && ascscval != null) {
					//setCookie(ascscname, ascscval, 30);
				}
				return false;
			}
		}
	</script>
	
