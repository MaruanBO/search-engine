<script type='text/javascript'>
var pagetitle = "";
//check the page
 var url = window.location.href.split("compandsave.com");
      var current_url = url[url.length-1];
      if (url[0].match("search")) {
        pagetitle = "search.compandsave";
      }
      else if (current_url.toUpperCase().match("SHOPPINGCART.ASP")) {
        pagetitle = "shoppingcartpage";
     }
      else if (current_url.match("one-page-checkout.asp")) {
        pagetitle = "checkout";
      }
      else if (typeof global_Current_ProductCode != "undefined") {
        brand = global_Current_ProductCode.split("-");
   
     if (brand.length > 2)
          pagetitle = brand[1].toUpperCase() + "productPage";   
      }
      else if (current_url.match(/(.*_s\/){1,}(\d+)\.htm+.*/ig) || current_url.match(/SearchResults\.asp\?Cat=/)) {

        if (printer_tag) {
var heading1_html = [''];
          $('.heading1').each(function() {
            heading1_html = $(this).html().split(" ");
            heading1_html[0] = heading1_html[0].toUpperCase();
          });
          if (heading1_html[0] != "HP" && heading1_html[0] != "EPSON" && heading1_html[0] != "CANON" && heading1_html[0] != "BROTHER" && heading1_html[0] != "LEXMART" && heading1_html[0] != "SAMSUNG" && heading1_html[0] != "DELL" && heading1_html[0] != "KODAK")
          heading1_html[0] = "OTHER";
          pagetitle = heading1_html[0] + "printerPage";
        } else {

        var imagename="default";
        var imageURL=current_url.replace(/(.*_s\/){1,}(\d+)\.htm+.*/ig, "$2");
        var flag=imageURL.indexOf(".");
        if(flag!=-1){
          imageURL=current_url.replace(/(.*\/searchresults.asp\?cat=){1}(\d+).*/i,"$2");
          flag=imageURL.indexOf(".");
          if(flag!=-1){
            //do nothing
          }
          else{
            imagename=imageURL;
          }
        }
        else{
          imagename=imageURL;
        }
        if (imagename != "default") {
          if (imagename == "167") pagetitle = "HPcategoryPage";
          else if (imagename == "168") pagetitle = "EPSONcategoryPage";

else if (imagename == "169") pagetitle = "CANONcategoryPage";
else if (imagename == "205") pagetitle = "BROTHERcategoryPage";
          else if (imagename == "212") pagetitle = "LEXMARTcategoryPage";
          else if (imagename == "19") pagetitle = "SAMSUNGcategoryPage";
          else if (imagename == "220") pagetitle = "DELLcategoryPage";
          else if (imagename == "114") pagetitle = "KODAKcategoryPage";   
          else pagetitle = "OthercategoryPage";       
        }}
      }
      else {
        pagetitle = current_url;
      }


