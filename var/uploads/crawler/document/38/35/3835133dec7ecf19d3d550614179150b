function validateEmail(email) { 
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
} 

$("#slide_subscribe_button").click(function() {
    var email = jQuery("#slide_subscribe_email").val();
    if (validateEmail(email)) {
		$.post("/v/dc/yy/email_update/active_camp.asp", {email:email, type:'subscribe'}, function(data) {
var html_temp = "";
			if (data == "Y") {

html_temp = "<a onclick='subscribe_hide()' class='cas-hid-close'><p>Close this</p></a><p>Thank you for signing up to our email<br>newsletter! You can expect our emails<br>once a week and can manage your<br>preferences <a class='cas-cms-blueTxt'>here</a> at any time.</p><p><span class='cas-cms-small-font'>Look for our email in your inbox! Don't see it?<br>Make sure we're on your <a href='http://www.compandsave.com/MailingList_unsubscribe.asp' target='_blank'>safe senders</a> list.</span></p>";
       			  $('#cas-hid-signup').html(html_temp);
			}
			else if (data == "N") {
html_temp = "<a onclick='subscribe_hide()' class='cas-hid-close'><p>Close this</p></a><p>The e-mail address you entered has already been subscribed<br>to our newsletter list. However, we would like to still offer you<br>a surprise discount. Use '<span class='cas-cms-blueTxt'>GETX</span>' at checkout to reveal your<br>hidden discount! You can expect our emails once a week and<br>can manage your preferences <a class='cas-cms-blueTxt'>here</a> at any time.</p><p><span class='cas-cms-small-font'>Look for our email in your inbox! Don't see it?<br>Make sure we're on your <a href='http://www.compandsave.com/MailingList_unsubscribe.asp' target='_blank'>safe senders</a> list.</span></p>";
$('#cas-hid-signup').html(html_temp);        	}	 
        }).error(function() {console.log("goes wrong");});

ga('send', 'event', 'Slidebar', 'signup-slider', pagetitle, {'nonInteraction': 1});
} 
	else {$('#slide_error_message').html("Email Address not valid");console.log("email not valided");}
});

