<!--Start checking criteo tag of listing and product page -->
<script type="text/javascript">

$(document).ready(function(){
var hreftext = window.location.href;
var reg1=/_s\/\d*\.htm/i;
var reg2=/SearchResults\.asp\?Cat=/i;
var reg3=/.*(?=\.htm)/;

//var reg=/[1-9]\d*(?=\.htm)/gi;
var result1 = reg1.exec(hreftext);
var result2 = reg2.exec(hreftext);

if (result1||result2)

{   var criteo_q = window.criteo_q || [];
    var productid = new Array();
	var hrefarray = new Array();
	var temp = new Array();
	var num=0;
	hrefarray = document.getElementsByClassName('productnamecolor colors_productname');
	if(hrefarray[0])
    {
    	for (var i=0;i<hrefarray.length;i++)
    	{
    		if (hrefarray[i].href)
    		{
                 temp = hrefarray[i].href.split("/");
                 productid[num] = reg3.exec(temp[temp.length-1]);
                 productid[num] = decodeURI(productid[num]);
                 num++;
                 
    		}
    	}


criteo_q.push(
    { event: "setAccount", account: 4737 },
    { event: "setSiteType", type: "LISTING"},
    { event: "viewList", product: productid, keywords: "" }
);

    }

}
else if(typeof global_Current_ProductCode != "undefined")
{
    var productcode = global_Current_ProductCode;
    var criteo_q = window.criteo_q || [];
criteo_q.push(
    { event: "setAccount", account: 4737 },
    { event: "setSiteType", type: "PRODUCT"},
    { event: "viewItem", product: productcode } );
}
});

</script>
<!--End of checking criteo tag of listing and product page -->



