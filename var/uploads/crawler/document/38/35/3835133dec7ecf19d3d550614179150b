    setTimeout(function testQuery(){
        if (getParameterByName("googlebasepid") == null || getParameterByName("googlebasepid") == "" || displayProductName() == null || displayProductName() == "" || displayProductPrice() == null || displayProductPrice() == "")
	{

////////////////////////// for displaying the printer picture in iframe //////////////////////////////////
var xmlhttp;

if (window.XMLHttpRequest){
// code for IE7+, Firefox, Chrome, Opera, Safari
    xmlhttp=new XMLHttpRequest();
}
else{
// code for IE6, IE5
    xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
}

var URLstr = window.location.href;
var imagename="default";
var imageURL=URLstr.replace(/(.*_s\/){1,}(\d+)\.htm+.*/ig, "$2");
var flag=imageURL.indexOf(".");

if(flag!=-1){
    imageURL=URLstr.replace(/(.*\/searchresults.asp\?cat=){1}(\d+).*/i,"$2");
    flag=imageURL.indexOf(".");
    
	if(flag!=-1){
        //do nothing
	}
    else{
		imagename=imageURL;
    }
}
else{
	imagename=imageURL;
}
	 
try {
if(imagename!="default"){
    imageURL= "//www.compandsave.com/v/vspfiles/photos/categories/printers/" + imagename + ".jpg";
    xmlhttp.open("GET", imageURL, true);
    xmlhttp.send();
console.log('xml sent');
    xmlhttp.onreadystatechange=function(){  
        if (xmlhttp.readyState==4 && xmlhttp.status==200){
           document.getElementById("pidresult").innerHTML = "<div style='border:0; top:115px; padding:0; box-shadow:none;' id='pidresult-inner'><img style='max-width:290px;max-height:213px; float:right;' src=" + imageURL + "></img></div>";
        }
	}
}
}catch(err) {console.log("can't find image");}
////////////////////////// for displaying the printer picture in iframe //////////////////////////////////



	}
	else
	{
		document.getElementById("pidresult").innerHTML = "<div style='z-index:1;' id='pidresult-inner'><img src='/v/vspfiles/photos/" + getParameterByName("googlebasepid") + "-2.jpg'></br>" + "<div id='buy-now-pid' ><span id='pid-price'>Price: $" + displayProductPrice()+"</span><br><form target='_top' style='display: inline;' action='//www.compandsave.com/ShoppingCart.asp' method='post'><table border='0' cellpadding='3' cellspacing='0' ><tbody><tr><td valign='top' align='left'>Qty: <input name='QTY." + getParameterByName("googlebasepid")+ "' size='3' maxlength='8' onkeydown='javascript:QtyEnabledAddToCart();' value='1' type='textbox'></td></tr><tr><td align='left'><input src='//www.compandsave.com/v/mp/btn_addtocart_2.png' name='btnaddtocart' alt='Add to cart' border='0' type='image' width='140px' height='42px' id='btn-shadow'><input name='ReplaceCartID' value='' type='hidden'><input name='ProductCode' value='" + getParameterByName("googlebasepid") + "' type='hidden'><input name='e' value='' type='hidden'><input name='ReturnTo' value='ShoppingCart.asp' type='hidden'></td></tr></tbody></table></form></div>"+"<div id='pid-text'>"+ displayProductName() + "</div></div>";
