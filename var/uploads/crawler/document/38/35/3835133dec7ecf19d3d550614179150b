<script type="text/javascript">

/***************************************/
/*                                                                */
/* Description: on product page  check          */
/*  the url, scroll html to   product description */
/* Author: Shuoran   09/25/2014                     */
/* for productdetail page, scoll down to the printer model */
/**************************************/if(typeof global_Current_ProductCode != "undefined") {
	//get parameters from URL
    var scrolldown = getUrlVars()["scolltoprinter"];
    //check if the page needs to scroll down
    if (typeof scrolldown != "undefined" && scrolldown != "") {
    	jQuery(document).ready(function() {
    		scroll();
		});
	}
}
function scroll() {
  e = document.getElementsByClassName("colors_descriptionbox");
  var offset = e[0].offsetTop;
  var current = e[0].offsetParent;
  while(current!=null) {  
    offset += current.offsetTop;
    current = current.offsetParent; 
  }
    jQuery('html,body').animate({
    scrollTop: offset-20},
    'slow');
}
</script>
