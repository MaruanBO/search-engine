<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script language="javascript">
    function handleEnter(inField, e) {
        var charCode;
        if(e && e.which){
           charCode = e.which;

        }else if(window.event){
           e = window.event;
           charCode = e.keyCode;
        }

        if(charCode == 13) {
           submitSearch();
           return false;
        }
    }
     
    function submitSearch() {
        if (document.getElementById("search_v5_1").value != "") {
            var searchUrl = "http://search.compandsave.com/search?keywords=";
            if (getUrlVars()["cus_id"]) cb_customer_id = getUrlVars()["cus_id"];  
           else cb_customer_id = getCookie("CustomerID");
            
            window.location = searchUrl + escape(document.getElementById("search_v5_1").value) + "&cus_id=" + cb_customer_id;
        }
        return false;
    }
</script>


