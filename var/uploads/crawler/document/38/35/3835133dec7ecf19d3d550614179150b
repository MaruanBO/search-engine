<!-- Start of Customer Behavior Tracking Code !-->
<script type="text/javascript">
/************************************/
/* customer behavior tracking code  */
/* send customerid, url, to db      */
/* Author: Yiyang 4/2               */
/************************************/
(function() {
  	var cb_customer_id = getCookie("CustomerID");
  	if (typeof cb_customer_id != "undefined" && cb_customer_id != "") {

      //if customer has email cookie and customerid cookie, delete email cookie and save the email in our database
      var cas_email_address = getCookie("cas_CustomerID");
      if (typeof cas_email_address != "undefined" && cas_email_address != "" && cas_email_address != null) {
        setCookie("cas_CustomerID", '', -1);
        jQuery.post("//www.compandsaves.com/code/yy/customer_behavior/customer_email.php", {id:cb_customer_id, web:'cas', email:cas_email_address});
      }
    
  		var url = window.location.href.split("compandsave.com/");
  		var current_url = url[url.length-1];
  		var detail = "";
  		var cart_item = getShoppingCartItems();
  		if (typeof global_Current_ProductCode != "undefined") detail = global_Current_ProductCode;
  		else if (current_url.match("OrderFinished.asp")) detail = "Order" + Order[0];
  		//console.log(current_url);
  		//console.log(customerid);
  		//console.log(detail);
