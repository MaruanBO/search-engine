function subscribe_slideout() {
 jQuery('#cas_subscriber_box_A').animate({'margin-left':'0px'}, 100);  
jQuery('#cas-hid-hoverHide').css('visibility', 'hidden');
slide_flag = false;
ga('send', 'event', 'Slidebar', 'seemore-slider', pagetitle, {'nonInteraction': 1}); 
setCookie('Subscriber_Box', '', 180);}

function subscribe_hide() {

jQuery('#cas_subscriber_box_A').hide();
setCookie('Subscriber_Box', 'N', 180);
}

function subscribe_slidein(value) {
value = "-" + value + "px";
 jQuery('#cas_subscriber_box_A').animate({'margin-left': value}, 100);  

jQuery('#cas-hid-hoverHide').css('visibility', 'visible');

}


function subscribe_slidein2() {
