<script type="text/javascript">
window.onload = $(function(){
				$.get("/v/dc/gpid/product_tracking.asp?ProductCode=" + getParameterByName("googlebasepid"),function(data){
					$("#test").append(data);
					var name=$('#pname').val();
					var cost=$('#price').val();
					$('#get_name').val(name);
					$('#get_price').val(cost);
					//alert("Test Name: " + name + " Test Cost "+ cost);//result to see but when real code make line comment
					
				});
				return false;
});

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}


function displayProductName()
{
var x=document.getElementById("get_name").value;
return x;
}

function displayProductPrice()
{
var x=document.getElementById("get_price").value;
return x;
}

</script>

