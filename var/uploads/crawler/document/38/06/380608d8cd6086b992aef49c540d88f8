    var anvp = {};
	anvp.listener = function(e){
		var androidDialog;
		if (e.name == "ANDROID_PLAYER_REQUESTED" && e.args && e.args.length) {
			androidDialog =window.open(e.args[0], "anvIntentPlayer");
			if (androidDialog && androidDialog.focus) androidDialog.focus();
				console.log(e.name, e.args);
		}
