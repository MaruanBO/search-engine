<script type="text/javascript">
	if (typeof customAlert === undefined) {
	customAlert = function(msg) { alert(msg); }; // safe loader in case we are before colorbox load
}

	function loginBox() {
		$("#login_box").toggle();
		$("#signin").toggleClass("active");
	}
	$("#signin, .signin").bind("click", function(e) {
		e.stopPropagation();
		e.preventDefault();
		loginBox();
	});
	$("#login_email, #login_password, #forgot_link, #login_button").click(function(e) {
		e.stopPropagation();
	});
	 $("html").click(function() {
	 	$("#login_box").hide();
	 	$("#signin").toggleClass("active", false);
	 });

	function forgot() {
		if ($("#login_email").val()) {
			var oldtext = $("#forgot_link").html();
			$("#forgot_link").html('Please wait…');
			$.ajax({
				url: document.location.protocol + '//batchgeo.com/api/forgot/?email=' + escape($("#login_email").val()), 
				success: function(data) {
					data = parseInt(data);
					
					if (data == 0) {
						$.ajax({ 
							url: document.location.protocol + '//batchgeo.com/api/emailMaps/?email=' + escape($("#login_email").val()), 
							success: function(edata) {
								customAlert(edata);
								$("#forgot_link").html(oldtext);
							}
						});					
					} else if (data > 0) {
						customAlert('Ok, you should receive the email shortly');
						$("#forgot_link").html(oldtext);
					}
				}
			});
		} else {
			customAlert('Ok, just enter your email address into the field on the sign-in box and click "Forgot Password" again.')
		}
	}
	function sendLogin() {
		$.getScript('https://batchgeo.com/api/login/?email=' + encodeURIComponent($("#login_email").val()) + '&password=' + encodeURIComponent($("#login_password").val()), function() {} );
	}
</script>

<script type="text/javascript">
	if (devicePixelRatio && devicePixelRatio > 1) {
		var r = document.createElement('script'); r.type = 'text/javascript'; r.async = true;
		r.src = '//d27ixrd8sdmf11.cloudfront.net/js/retina.js';
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(r, s);
	}
</script>

