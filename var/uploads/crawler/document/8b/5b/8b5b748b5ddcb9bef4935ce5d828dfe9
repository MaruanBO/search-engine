	$(document).ready(function(){
		var $rows_house = $('.newsgroup--row:gt(0)');
		$rows_house.each(function(e){
			var nav_title = $('.nav-title',$(this)).text();
			var ad_house = $(this).find('.house-ad');
			if(ad_house.length > 0){
				var src;
				switch(nav_title) {
					case 'Page Two':
						src=('ontouchstart' in window)?'/doubleclick/div-gpt-ad-mobile_middle_2.php':'/doubleclick/div-gpt-ad-middle.php';
						break;
					default:
						src='/house_ads.php';
					break;
				}
				$('iframe',ad_house).attr('src',src);
			}
		});
	});
})(jQuery);
</script><script>
	// This script detects more than two large items 
	// and hides the ad within the large items row if so
	jQuery('.newsgroup--large_items').each(function(e){		
		if(jQuery(this).children('.newsgroup--regular_item').length > 2){
			jQuery(this).find('.house_ad').remove();
		}
	});
</script><script>
	var $twitter_accounts = jQuery('.newsgroup--columnist .author-r h2');	
	
	$twitter_accounts.each(function(e){
		var str = jQuery(this).html();
		var sanitized_str = str.replace("@","").trim();
		var a = document.createElement('a');
		jQuery(a).attr({
			'href':'https://twitter.com/'+sanitized_str,
			'title':str,
'target':'_blank'
		})
		.html(str);
		
		jQuery(this).html(a);
	}); 
