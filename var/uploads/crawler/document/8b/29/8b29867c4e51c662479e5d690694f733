                        <div><a href="_blank"><img src ="
/image/view/-/21984502/highRes/1/-/bsggyx/-/Gas-Tracker-Banner.png
" align="middle" height="100" width="320" id="banner"/></a>
</div>
<script language="javascript" type="text/javascript">
document.getElementById("banner").parentNode.onclick = newlocation;
rotate();
var ad = 0;
function newlocation ()
 {
     var url = new Array ("http://www.abc17news.com/gas-prices/","http://www.abc17news.com/recalls","http://www.abc17news.com/road-conditions");
     document.location.href = url [ad-1];
     return false;
 }
function rotate()
 {
     var images = new Array ("/image/view/-/21984502/highRes/1/-/bsggyx/-/Gas-Tracker-Banner.png", "/image/view/-/23093242/highRes/1/-/tk4xnhz/-/Recall-Alerts-Banner.png", "/image/view/-/23093368/highRes/1/-/9rw8sd/-/Road-Conditions-Map-Banner.png");
     if (ad == images.length)
      {
        ad = 0;  
      }
     document.getElementById("banner").src = images [ad] ;
         ad++;
     setTimeout(rotate, 4000);
 }
    
</script>
</section>
