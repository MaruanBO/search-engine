        div.showcaseGallery object#kaltura_dynamic_player_ {
            border-color: #EBEBEB;
        }
    </style>

    <script>
        $ib(function() {
            $ib("div.showcaseScrollable").scrollable();
            var playerDiv = $ib('div#showcaseGallery-19265732 .player');
            $ib("#showcaseGallery-19265732 .items .slide").click(function(){

                var currentId = $ib('#showcaseGallery-19265732 .kalturaVideoPlayerAssoc').attr('id').split('kalturaVideoPlayer_')[1],
                        playerId = 'kaltura_dynamic_player_'+currentId,
                        newId = $ib(this).data('entry').entryId;

                try {
                    if (currentId === newId) {
                        return false;
                    }

                    // clear any existing Livestream timers
                    ibsys.kdp.liveProgram.clearTimers();

                    //video tracker object
                    var videoTrack = {
                        name: "videoViewStart",
                        label: "abandonment",
                        value: ibsys.kdp.players[playerId].get(0).evaluate('{video.player.currentTime}'),
                        vidID: playerId,
                        override: false //set override to avoid tracking page view for abandonment
                    };
                    // track user left previous player
                    ibsys.kdp.track(videoTrack);

                    delete ibsys.kdp.players[playerId];
                    playerDiv.load($ib(this).data('ajax-url'));
                    return false;
                } catch(msg) {
                    console.log("showcaseGallery changeMedia error! " + msg);
                    return true;
                }
            });
        });
    </script>
