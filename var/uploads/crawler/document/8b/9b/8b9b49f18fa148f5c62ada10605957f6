        var searchWaterMark = "Search";
        var locateWaterMarkCity = "Enter City";
        var locateWaterMarkState = "State";
        var locateWaterMarkZip = "Zip Code";

        var navoff_x = 92;
        var navoff_y = 150;


        if (($.browser.msie) && ($.browser.version == 6.0)) {
            //alert('ie6');
            //var i = 0;
        }
        else {
            /*CUFON -should be outside docready*/
            Cufon.replace('#main-nav li a.main-nav', {
