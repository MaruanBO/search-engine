        function browserInWhiteList(replaceHtml) {
            try {

                if ($.browser.msie);
                {

                    if ($.browser.version >= 7.0) {
                        //alert(true);
                        return true;
                    }
                }

                if ($.browser.webkit) {

                    return true;
                }

                if ($.browser.mozilla) {
                    //alert($.browser.version.substr(0, 3));
                    if ($.browser.version.substr(0, 3) > 1.8)//Greater than 2.0
                        return true;

                    else if ($.browser.version.substr(0, 3) == 1.8) {
                        //alert($.browser.version.substr(2, 3));
                        if ($.browser.version.substr(2, 3) > 8.0)//GREATER THAN 1.0
                            return true;
                    }


                }

                if (replaceHtml) {
                    var noJSDiv = "<div id='divNonSupported'";
                    noJSDiv = noJSDiv + "</div>";





                    $('.altContent').after(noJSDiv);

                    if ($(".altContent").html().toString().substr(0, 10).indexOf("&lt;") == -1) {
                        $('#divNonSupported').html($(".altContent").html());
                    }
                    else {
                        $('#divNonSupported').html(htmlDecode($(".altContent").html()));
                    }




                    $('.altContent').remove();
                }
            }
            catch (Error) {
                //alert('error');
                return false;
            }


            return false;
        }


        //used to decode/encode html
        function htmlDecode(value) {
            return $('<div/>').html(value).text();
        }

        function htmlEncode(value) {
            return $('<div/>').text(value).html();
        }
        var socialNav = '<div id="whatsNewShareSide"><a class="facebook" target="_blank" href="http://www.facebook.com/bobevans">Facebook</a><a class="twitter" target="_blank" href="http://twitter.com/#!/bobevansfarms">Twitter</a>    <a class="youtube" target="_blank" href="http://www.youtube.com/user/BobEvansFarms">        Youtube</a> <a class="getApps" href="/WhatsNew.aspx#apps">Get Apps</a> <a class="mail" target="_blank" href="http://bobevans.com/bemail/default.aspx">Mail</a></div>';





        function setSocialNavPosition() {
            var x = $('#header').position().left - navoff_x;
            var y = $('#header').position().top + navoff_y;



            //bug with jquery .position().left in IE9
            if (jQuery.browser.msie && jQuery.browser.version == "9.0")
                x = 0;

           // console.log(x,y);
            $('#whatsNewShareSide').css('left', x.toString() + 'px');
            $('#whatsNewShareSide').css('top', y.toString() + 'px');

        }
        $(function () {

            var carryOutHref = $('.carry_out_li a').eq(0).attr('href');
            $('.carry_out_li a').eq(0).bind('click', function () { mmConversionTag(301665, this, '_self', '' + carryOutHref + ''); return false; });
            $('.carry_out_li a').eq(0).attr('href', '');

            $('.bemail_li .dropdown-container').remove();
            $(window).resize(function () {
                setSocialNavPosition();

            });

            $('#header').before(socialNav);
            setSocialNavPosition();


            //alert('');
            if (browserInWhiteList(false)) {
                $(".search").val(searchWaterMark);

                $(".search").focus(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == "" || $(this).val() == searchWaterMark;
                    }).removeClass("watermarkOn").val("");

                });

                $(".search").blur(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == ""
                    }).addClass("watermarkOn").val(searchWaterMark);

                });



                $(".txtLocateCity").val(locateWaterMarkCity);

                $(".txtLocateCity").focus(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == "" || $(this).val() == locateWaterMarkCity;
                    }).removeClass("watermarkOn").val("");

                });

                $(".txtLocateCity").blur(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == ""
                    }).addClass("watermarkOn").val(locateWaterMarkCity);

                });

                $(".txtLocateState").val(locateWaterMarkState);

                $(".txtLocateState").focus(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == "" || $(this).val() == locateWaterMarkState;
                    }).removeClass("watermarkOn").val("");

                });

                $(".txtLocateState").blur(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == ""
                    }).addClass("watermarkOn").val(locateWaterMarkState);

                });

                $(".txtLocateZip").val(locateWaterMarkZip);

                $(".txtLocateZip").focus(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == "" || $(this).val() == locateWaterMarkZip;
                    }).removeClass("watermarkOn").val("");

                });

                $(".txtLocateZip").blur(function () {

                    $(this).filter(function () {
                        // We only want this to apply if there's not
                        // something actually entered
                        return $(this).val() == ""
                    }).addClass("watermarkOn").val(locateWaterMarkZip);

                });


                $(".locate").keyup(function (e) {


                    if (e.keyCode == 13) {
                        e.returnValue = false;
                        e.preventDefault();

                        $('.btn-locate').click();
                    }
                    return false;
                });

                //$('.dropdown-container').prepend('<div class="ddc-rightborder"></div>');
            }
            else {
                $('.main-nav').css("font-size", "13px");

            }
        });

        function RemoveWatermarks() 
        {
            $('.watermarkOn').each(function () {

                $(this).removeClass('watermarkOn');

                $(this).val('');
            });            
        }   
    </script>




    
