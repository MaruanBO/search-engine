    $(window).load(function () {
        var topBannerCont = $('header .topBanner'),
            bannerFrame = $('header .topBanner div.gptAdContainer div iframe');
        if (bannerFrame && !bannerFrame.contents().find('img').length && !bannerFrame.contents().find('object').length) {
            topBannerCont.remove();
            $('#metaDescription, .addContacts').hide().show(0); //used to redraw section for Chrome. APS-691.
        } else {
            topBannerCont.removeClass('nobanner');
        }
        $('.imgBoxCropped img').each(function (ind, el) {
            $(el).addClass('cropped');
        });
    });


    window.onbeforeunload = function () {
        var page = window.location.href;
        if (page.indexOf("faadsearch") == -1 && page.indexOf("/login") == -1) {
            SetCookie("page", window.location.href, 1);
        }
    };
