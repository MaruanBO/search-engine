		<script>
			var explorePopularDestMsg1 = "Explore Our Neighborhoods";
			var explorePopularDestMsg2 = "Stay Like a Local";
			
			jq(document).ready(function(){

				/* Explore Popular Destinations Toggle - Homepage */
				var initPopularDest = function(){
					var staticContentWrapper = jq('#staticContentWrapper');
					var topContainer = jq('#staticContentWrapper .topContainer');
					
					// Add toggle control to DOM
					var popDestCtrl = jq('<div class="popDestControl"><div class="leftHandle"><div class="rightHandle"><a class="expanded"><span class="epdMsg1">'+explorePopularDestMsg1+'</span>\\<span class="epdMsg2">'+explorePopularDestMsg2+'</span></a></div></div></div>');
					jq(popDestCtrl).insertAfter(topContainer);
					
					// Opens collapsable content on page load
					var drawerExpanded = true;
					jq(topContainer).toggleClass('accessibleHide'); 

					// Add animated toggle behavior
					popDestCtrl.click(function(e){
						e.preventDefault();
						jq(topContainer).toggleClass('accessibleHide');
						jq(this).find('a').toggleClass('expanded');
						if(!drawerExpanded){
							var tcTop = topContainer.offset().top;
							jq('html,body').animate({scrollTop: tcTop}, 1000);
							drawerExpanded = true;
						}
						else {
							jq('html,body').animate({scrollTop: 0}, 1000);
							drawerExpanded = false;
						}
					});
				}();
			});
		</script>
	





    
    
    
    
	
		
		
			
				
			
			
		

		
				
    
        
        
            
                
                    
                
                
            
        
    

    
    	
    	
	    
	    	
	    	
	    
