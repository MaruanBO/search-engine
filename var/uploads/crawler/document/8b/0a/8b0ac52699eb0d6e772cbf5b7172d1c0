	var env_uri = "/hotelindigo/hotels/us/en/reservation";
	var alongARoute = false;
	/*<sl:translate>*/
	var favHotelLink = "Favorites"; 
	var noAvailabilityErrMsg = "There were no hotels found using your specified search criteria. Please try again by changing your selections.";
	/*</sl:translate>*/
	var destinationLabel = "Enter City, Address, Zip Code, Airport or Point of Interest"; 
	var isClosedSearch="false";
	var isOpenSearch = "";
	searchLocation = "";
	var brandId = "in";
    var languageId = "en";
	var labelCheckInDate = jq("<div/>").html("Check In").text();
	var labelCheckOutDate = jq("<div/>").html("Check Out").text();
	var expressTimeOutMillis = 5000;
	var expressTimedOutMsg = jq("<div/>").html("Your operation has timed out; this may be caused by a slow network connection, a server outage or an application error. Please try again.").text();
	var label_express_booking_openshell_toggle_alt = jq("<div/>").html("Select to access Express Booking.").text();
	var label_express_booking_closeshell_toggle_alt = jq("<div/>").html("Select to hide Express Booking.").text();
	var DESTINATION_TYPE_LABEL = {
			
			
			
        hotel_name:'IC.HOTELS',
        other:'CITIES/REGIONS'
	};
	var imgprefixpath='http://prodcache.internal.ihg.com';

    var PET_FRIENDLY_HOTELS = "";
	var error_recentlyViewedHotels_ServiceUnavailable='Your Recently Viewed Hotels are temporarily unavailable.';
	
	
    jq(document).ready(function() {
        if(PET_FRIENDLY_HOTELS == PET_FRIENDLY_HOTELS_TITLE){
            findAmenity(AMENITY_PET_FRIENDLY_HOTELS).attr('checked', true);
            jq("#moreSearchOptions").click();
        }
        
        
        jq('[placeholder]').focus(function() {
        	  var input = jq(this);
        	  if (input.val() == input.attr('placeholder')) {
        	    input.val('');
        	    input.removeClass('placeholder');
        	  }
        	}).blur(function() {
        	  var input = jq(this);
        	  if (input.val() == '' || input.val() == input.attr('placeholder')) {
        	    input.addClass('placeholder');
        	    input.val(input.attr('placeholder'));
        	  }
        	}).blur();
        
        jq('[placeholder]').parents('form').submit(function() {
        	// wipe out placeholder
        	jq(this).find('[placeholder]').each(function() {
        	    var input = jq(this);
        	    if (input.val() == input.attr('placeholder')) {
        	      input.val('');
        	    };
    });
      	    
	   		// when finding route - wipe out the default label, if any, this can be undone if/when input element changed to placeholder html5 style
	   		jq('#routeBegin').each(function() {
	       	    var input = jq(this);
	       	    if (input.val() == input.attr('title')) {
	       	      input.val('');
	       	    }
	       	  });
	   		
	   		// when finding route - wipe out the default label, if any, this can be undone if/when input element changed to placeholder html5 style
	   		jq('#routeEnd').each(function() {
	       	    var input = jq(this);
	       	    if (input.val() == input.attr('title')) {
	       	      input.val('');
	       	    }
	       	  });        	  
     	});
        
    });    
</script>
