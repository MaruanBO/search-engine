jQuery(function ($) {
$('#earth').data('countToOptions', {
        
	onUpdate: function (value) {
		alert("UPDATE: "+value);
          count.call(this, {
            from: (value-10000000),
            to: value
          });
        },
	onComplete: function (value) {
		//alert("Complete: "+value);
          count.call(this, {
	    from: (value-10000000),
            to: value
          });
        }
      });

 function count(options) {
        $.ajax({url:"/messageCount.php",success:testproc});	
	function testproc(data) {
	$('.timer').countTo({
		from: (data-10000000),
		to: data,
		speed: 20000000,
		refreshInterval: 100,
		formatter: function (value, options) {
		  return value.toFixed(options.decimals).replace(/\B(?=(?:\d{3})+(?!\d))/g, ',');
		},
		onComplete: function (value) {
			//alert("Complete: "+value);
		  count.call(this, {
		    to: value
		  });
		}
    });
	}
      }

	$('.timer').each(count);
//***************************
    });

</script>

 
