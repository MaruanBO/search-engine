<script type="text/javascript">
    $(function(){
		
		$('.popup-close-btn').click(function(evt) {
            evt.preventDefault();
            hs.close();
        });
		
		
	        // handle HighSlide's onBeforeExpand event to load the context chooser via ajax/webmethod
            hs.Expander.prototype.onBeforeExpand = function() {
                
                $('.popup-close-btn').click(function(evt) {
                    evt.preventDefault();
                    hs.close();
                });
            
                // only call the ajax method when we want to launch the localizer
                if ($(this.a).hasClass('launch-localizer')) {
                    $.ajax({
                        type: 'POST',
                        url: '/Services.aspx/GetLocalizationChooserHtml',
                        data: "{}",
                        contentType: 'application/json; charset=utf-8',
                        dataType: "json",
                        success: function(response) {
                            $('#localizer-content').html(response.d);
                        },
                        error: function(xhr, status) {
                            window.location.href = '/choose-country.aspx';
                        }
                    });
                }
                else if($(this.a).hasClass('track-order')){                    

                    var modalValidator = new FormValidator('frmTrackOrder', [{
                        name: 'orderNumber',
                        display: 'Order Number',
                        rules: 'required|numeric'
                    }], function(errors, event) {
                        var $error = $('#trackOrderErrors');
                        if (errors.length > 0) {
                            var errorString = '';

                            for (var i = 0, errorLength = errors.length; i < errorLength; i++) {
                                errorString += errors[i].message + '<br />';
                                $('#' + errors[i].id).addClass('required');
                            }
                            var modalError = document.getElementById("trackOrderErrors");
                            resizeHighSlidePopup(modalError, errorString);
                        }
                        else {
                            $error.hide();
                            $error.html('');
                            
                            var url = "order-status-1.aspx/GetTrackingUrl";
                            var pageUrl = window.location.pathname;
                            if (pageUrl.indexOf("/myroom/") > -1) {
                                url = '/order-status-1.aspx/GetTrackingUrl'
                            }
                                            
                            $.ajax({
                                type: "POST",
                                url:  url,
                                data: '{"orderNo":"' + $('#orderNumber').val() + '","zipCode":"' + $('#zipCode').val() + '"}',
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function(response) {
                                    if (response.d.IsValidOrder) {                                
                                        //Set the url
                                        $('a#trkOrder').attr('href', response.d.TrackingUrl);
                                        //Set the anchor tag html
                                        $('a#trkOrder').text(response.d.TrackingNumber);
                                        //Set the shipping method
                                        $('span#shipMethod').html('Your order has been shipped via <b>' + response.d.ShipVia + '</b>');

                                        //Hide the action buttons
                                        $('div.button-area > .actions').hide();
                                        //Show the tracking results
                                        $('div.button-area > .results').show();

                                        var results = document.getElementById("trackingResults");
                                        resizeHighSlidePopup(results, response.d.Message);
                                    }
                                    else {
                                        if (response.d.Message == 'international') {
                                            //Hide the action buttons
                                            $('div.button-area > .actions').hide();
                                            $('div.button-area > div#international-order').show();

                                            var borderFreeOrder = document.getElementById("international-order");
                                            resizeHighSlidePopup(borderFreeOrder, '');
                                        }
                                        else{                            
                                            var modalError = document.getElementById("trackOrderErrors");
                                            resizeHighSlidePopup(modalError, response.d.Message);
                                        }
                                    }
                                }
                            });
                        }
                    });


                    //Handler for form submission ( submit button click event ).
                    $('#frmTrackOrder').submit(function(e) {
                        e.preventDefault();            
                    });

                    //Handler for track order link in the pop up
                    $('a#trkOrder').click(function(evt){
                        $('a#closePopup').click();
                    });

                    $('input').focus(function(evt) {
                        $(this).removeClass("required");
                    });
                }
            }
        
	});
</script>

