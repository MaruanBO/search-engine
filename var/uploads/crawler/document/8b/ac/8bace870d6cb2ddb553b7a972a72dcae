		  	<script>
(function($){
	$(function() {

		var validateEmail = function(email) {
			return /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/.test( email );
		};

		var myForm = $("#infoForm form");
		/*
		switch ( window.location.hostname ) {
			case 'neatologie.staging.wpengine.com':
				myForm.attr('action', 'https://leads.neat.com/staging/formProcessor.php');
				myForm.find('input[name="_ri_"]').val('X0Gzc2X%3DWQpglLjHJlYQGpgzelzbRzdl3zgXOSHfXzd5XAIqe1KmlVwjpnpgHlpgneHmgJoXX0Gzc2X%3DWQpglLjHJlYQGjzgcOyFzbzgFTFYTczadnAYR6WNzaWmh');
			break;
		}
		*/
		myForm.submit(function(e) {
			e.preventDefault();
			var form = $(this);
			if ( !validateEmail(form.find('.email input').prop('value')) ) {
				alert('Invalid Email Address');
				return;
			}
			form.find('[type=submit]').html("Processing...");
			var message = $('<div/>', { 'class' : 'leads-response text-center' }).html('Thanks for your interest.');
			message.insertAfter(form.slideUp());
			message.hide().slideDown();
			$.ajax({
				url : form.attr('action'),
				data : form.serialize(),
				type : 'post'
			});
		});
	}); 
})(jQuery);
