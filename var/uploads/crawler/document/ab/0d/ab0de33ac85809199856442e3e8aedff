<script>
	$(document).ready(function() 
	{
		base();
		$('#menu-toggle').click(function (e) 
		{
		  $('body').toggleClass('active');
		  e.preventDefault();
		});

		function isScrolledTo(elem) 
		{
		  var docViewTop = $(window).scrollTop(); //num of pixels hidden above current screen
		  var docViewBottom = docViewTop + $(window).height();
		
		  var elemTop = $(elem).offset().top; //num of pixels above the elem
		  var elemBottom = elemTop + $(elem).height();
		
		  return ((elemTop <= docViewTop));
		}
   
   	if($(window).width() > 480)
   	{
			var header_nav = $("#header-nav");
			var footer_nav = $("#footer-nav");
			
			var header_pos = header_nav.offset().top;
			var header_height = header_nav.height();
			var footer_pos = footer_nav.offset().top;
			var result_height = footer_pos - header_pos - header_height - 150; // 150 allows for ad + breadcrumbs + spacing

			if(result_height > 3000)
			{
				var catcher = $('#catcher');
				var sticky = $('#sticky');
				
				if (sticky.length) 
				{ 
					// make sure "#sticky" element exists
					var stickyTop = sticky.offset().top; // returns number
					var stickyHeight = sticky.height();
	
		      $(window).scroll(function()
		      { 
		      	// scroll event
						console.log("scrolling....");
						var footerTop = $('#bouncer').offset().top;
						console.log("footerTop = " + footerTop);
						
						var limit = footerTop - stickyHeight;
						console.log("limit = " + limit);
						
						if (isScrolledTo(sticky))
						{
						   sticky.css({ position: 'fixed', top: 0 });
						}
						else {
						   sticky.css('position','static');
						}
		
						var windowTop = $(window).scrollTop(); // returns number
		        if (limit < windowTop) 
		        {
				      var diff = limit - windowTop;
				      sticky.css({top: diff});
		        }
		        
		        var stopHeight = catcher.offset().top + catcher.height();
		        if ( stopHeight > sticky.offset().top) 
		        {
		            sticky.css('position','absolute');
		            sticky.css('top',stopHeight);
		        }
		      });
		   	}
		  }
		}
	});
</script>
