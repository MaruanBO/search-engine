	<script type="text/javascript" charset="utf-8">
		function populateBrand()
		{
			var load_php;
			
			var query_report = $("#compare-report").val();
			if(query_report != "")
			{
				if((query_report == 3) || (query_report == 8) || (query_report == 9))
				{
					$("#compare-year").show();
				}
				else
				{
					$("#compare-year").hide();
				}				
				
				if((query_report == 1) || (query_report == 2)) // reliability, lemon odds
				{
					load_php = "inc/add_brands_reliability.php";
				}
				else
				{
					if(query_report == 6)
					{
						load_php = "inc/add_brands_mk.php";
					}
					else
					{
						if(query_report == 7)
						{
							load_php = "inc/add_brands_photo.php";
						}
						else
						{
							if((query_report == 8) || (query_report == 9))
								load_php = "inc/add_brands_specs.php";
							else
								load_php = "inc/add_brands_rh.php";
						}
					}
				}

				$.getJSON(load_php, {report: $("#compare-report").val()}, function(data)
				{
					var select = $('#compare-make');
					var options = select.attr('options');
					$('option', select).remove();
					options[options.length] = new Option("Select Make", "");
					
					$.each(data, function(index, array)
					{
						options[options.length] = new Option(array["brand"], array["brand_code"]);
					});
				});
			}
		}
		function populateModel() 
		{
			var model_year;
			
			model_year = $("#compare-year").val();
			
			if($("#compare-make").val() != "")
			{
				var load_php;
				var query_report = $("#compare-report").val();
				if(query_report != "")
				{
					if(query_report == 1) // reliability
					{
						load_php = "inc/add_models_reliability.php";
					}
					else
					{
						if(query_report == 2)
						{
							load_php = "inc/add_models_lemon.php";
						}
						else
						{
							if(query_report == 3)
							{
								if($("#compare-year").val() == "")
								{
									return -1;
								}
								load_php = "inc/add_models_rh.php";
							}
							else
							{
								if(query_report == 4)
								{
									load_php = "inc/add_models_for_fuel_econ.php";
								}
								else
								{
									if(query_report == 6)
									{
										load_php = "inc/add_models_mk.php";
										model_year = "";
									}
									else
									{
										if(query_report == 7)
										{
											load_php = "inc/add_models_photo.php";
										}
										else
										{
											if((query_report == 8) || (query_report == 9))
											{
												load_php = "inc/add_models_specs.php";
											}
											else
											{
												load_php = "inc/add_models.php";
											}
										}
									}
								}
							}
						}
					}
				}

				$("#submit_query").removeAttr("disabled");
				$("#submit_query").attr("style", "background:#ffab00");
				
				$.getJSON(load_php, {brand: $("#compare-make").val(), year: model_year, rp: query_report}, function(data) 
		    {	    	
	        var select = $('#compare-model');
	        var options = select.attr('options');
	        $('option', select).remove();
	 				options[options.length] = new Option("Select Model", "");
	 				
	      	$.each(data, function(index, array) 
	      	{
	          options[options.length] = new Option(array["model"], array["model_code"]);
	     		});
	    	});
			}
		}
		$(document).ready(function() 
		{
			$('#compare-year').hide();
			populateBrand();
			$('#compare-report').change(function()
			{
				populateBrand();
			});
	    populateModel();
	    $('#compare-make').change(function() 
	    {
	       populateModel();
	    });
		});
	</script>
