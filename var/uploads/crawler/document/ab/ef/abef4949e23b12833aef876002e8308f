<script type="text/javascript">
	function isZip(field) {
		var myZip = document.getElementById(field).value;
		if(myZip.length != 5) {
			alert('Please enter ZIP code');
			return false;
		} else {
			myWidth = screen.width;
			myHeight = screen.height;
			affid = "";click = "";subid = "";			$('#zip-form').attr('action', '/fas/');
			if(affid == "" || click == "")
			{
				window.open('/form4/?zip='+myZip,'','fullscreen=yes, top=0, left=0, menubar=0, scrollbars=yes, status=0, width='+myWidth+', height='+myHeight);
			}
			else
			{
				window.open('/form4/?zip='+myZip+'&affid='+affid+'&click='+click+'&subid='+subid,'','fullscreen=yes, top=0, left=0, menubar=0, scrollbars=yes, status=0, width='+myWidth+', height='+myHeight);				
			}
			$('#zip-form').attr('onsubmit', '');
			$('#zip-form').submit();
		}
	}
	function isZipSubmit( field )
	{
		var is_apple = $('input[name=apple]').val();
		var myZip = document.getElementById(field).value;

		if(myZip.length != 5)
		{
			alert('Please enter ZIP code');
			return false;
		}
		else
		{
			myWidth = screen.width;
			myHeight = screen.height;
			var affid = "";
			var click = "";
						if ( is_apple == "true" )
			{
				if ( affid == "" || click == "" )
				{
					var url = '/form4/?zip='+myZip;
				}
				else
				{
					var url = '/form4/?zip='+myZip+'&affid='+affid+'&click='+click;
				}
				$('#zip-form').attr('onsubmit', '');
				$('#zip-form').attr('action', url);
				$('#zip-form').submit();
			}
			else
			{
				isZip( field );
			}
		}
	}
</script> 
