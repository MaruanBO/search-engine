      $(function() {
        $('ul.calendar-tabs li').click(function() {
          var from = $(this).data('from-date')
          var to = $(this).data('to-date')
          var href = '/arts-and-entertainment/find-an-event?to_date=' + encodeURI(to) + '&from_date=' + encodeURI(from)
          $('a#more-calendar-picks').attr('href', href)
        });
      });
