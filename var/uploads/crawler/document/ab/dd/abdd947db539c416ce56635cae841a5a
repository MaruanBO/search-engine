<script type="text/javascript">
$(function() {
  $.ketchup.message('email', 'Must be a valid Email');
  $.ketchup.createErrorContainer(function(form, el) {
    divtop = $(el).parent();
    return $('<ul/>', {
           'class': 'ketchup-error'
         }).insertBefore(divtop);
    })
    .addErrorMessages(function(form, el, container, messages) {
      container.html('');
      for(i = 0; i < messages.length; i++) {
        $('<li/>', {
          text: messages[i]
        }).appendTo(container);
      }
    })
    .showErrorContainer(function(form, el, container) {
        container.slideDown('fast');
    })
    .hideErrorContainer(function(form, el, container) {
        container.slideUp('fast');
    });
    $('form.validatable').ketchup();
});
</script><div class="content-box facebook-page-like" id="">
  
  <div class="header ">
	<div class="title"> 
  	
    SEATTLE MET ON<br>FACEBOOK
