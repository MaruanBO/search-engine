      switch (r.pagetype) {
        case "product": return { ecomm_prodid : 'PRODUCT-' + r.productView.id, ecomm_pagetype : 'product', ecomm_totalvalue : r.productView.price};
        case "category": return { ecomm_prodid : '', ecomm_pagetype : 'category', ecomm_totalvalue : '', ecomm_catid: r.categoryView.code};
        case "searchresults": return { ecomm_prodid : '', ecomm_pagetype : 'searchresults', ecomm_totalvalue : ''};
