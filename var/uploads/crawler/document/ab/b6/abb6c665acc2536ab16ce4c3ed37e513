        case "orderUpsell": return { ecomm_prodid : getIds(r.orderView.items), ecomm_pagetype : 'purchase', ecomm_totalvalue : r.orderView.total};
        case "orderCoachOffer": return { ecomm_prodid :  getIds(r.orderView.items), ecomm_pagetype : 'purchase', ecomm_totalvalue : r.orderView.total};
        case "orderPaymentOption": return { ecomm_prodid :  getIds(r.orderView.items), ecomm_pagetype : 'purchase', ecomm_totalvalue : r.orderView.total};
