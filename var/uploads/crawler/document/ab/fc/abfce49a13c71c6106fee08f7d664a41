        <script>
        requireModule('hldesktop/app').send("enableHolidayBackground");
    </script>


    <script>
        requireModule('hldesktop/app').initializer({
            name: 'rotating-promo',
            initialize: function(container, application) {
                var promoData = window.rotatingPromoData;
                var content = Ember.A();

                if ( !promoData ) {
                    return;
                }

                for (var i = 0; i < promoData.promos.length; i++) {
                    var promo = promoData.promos[i];
                    content.pushObject(Ember.Object.create( {
                        index: i,
                        imageSource: promo.imageSource,
                        linkURI: promo.linkURI,
                        altText: promo.altText,
                        promoPosition: promo.promoPosition
                    }));
                }

                container.lookup('controller:application').send('setupRotatingPromo', content);
            }
        });
    </script>

    </body>
</html>
