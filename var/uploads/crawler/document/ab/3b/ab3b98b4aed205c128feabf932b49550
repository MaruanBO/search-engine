<script type="text/javascript">
	function bindhover() {
		$("#menu-main>li").hover(
			function () {
				$("#menu-main>li>ul.sub-menu").hide();
				$(".sub-menu", this).show();
			},
			function () {
				//$(".sub-menu", this).hide();
			});
		}
	function bindsubhover() {
		$("#menu-main>li>ul.sub-menu, #header").hover(
			function () {
				//$(".sub-menu", this).show();
			},
			function () {
				//$(this).hide();
				$("#menu-main>li>ul.sub-menu").hide();
			});
		}
	bindhover();
	bindsubhover();
	$('#s').val('Search...');
	$('#s').focusout(function() {
		if ($('#s').val() == '') {
			$('#s').val('Search...');
			}
		});
	$('#s').focusin(function() {
		if ($('#s').val() == 'Search...') {
			$('#s').val('');
			}
		});
	$('#searchform').css('position','relative');
	$('#mercsrchbutton').appendTo('#searchwrap');
	$('#mercsrchbutton').click(function() {
		$('#searchform').submit();
		});
</script><script type="text/JavaScript">
