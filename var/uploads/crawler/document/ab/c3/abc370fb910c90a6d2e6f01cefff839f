<script type="text/javascript">
function showEmailThanks(text){
         $("#emailThanks").show(50);
         $("#emailThanksBoxText").html(text);
}
function closeEmailThanks(){
         $("#emailThanks").hide(50);
         $("#emailSending").hide();
}
$(document).ready(function () {
        $("#email-alert-signup2").submit(function (e) {
               $("#emailSending").show();
                var email = $('#Email').val();
               if(email != ""){
                    var reg  = /^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$/;
                          if(!reg.test(email)){
                               showEmailThanks("<h2 style='color:red;'>Error</h2><p>Wrong Email Format</p>");
                           }else{
        
                               var url = "http://email.sleepcountry.com/Portal/forms/Process.ashx"; // the script where you handle the form input.
                               $.ajax({
                                     url:url,
                                     dataType: 'jsonp',
                                     data: $("#email-alert-signup2").serialize(),
                                     success:function(json){
                                     },
                                     error:function(data){
                                          ga('send', 'pageview', 'mailinglist-signup-complete');
                                          showEmailThanks("<h2>Thank You!</h2><p>Thank you for signing up to receive exclusive news and information from Sleep Train.</p><p>A welcome email will be arriving soon and you will be on your way to a better nightâ€™s sleep!</p>");

                                     }   
  
                          });
                      }
              }else{
                    showEmailThanks("<h2 style='color:red;'>Error</h2><p>Email can not be empty.</p>");
             } 
    return false; // avoid to execute the actual submit of the form.
});
});

</script>
