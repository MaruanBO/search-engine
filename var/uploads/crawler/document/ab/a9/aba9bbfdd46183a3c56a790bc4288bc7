<script type="text/javascript">
        if (s.pageName != undefined) {
        var pageName = s.pageName;
        baseURL = location.protocol + "//" + location.hostname + (location.port && ":" + location.port) + "/";
        var queryStr = pageName.replace(baseURL, "");
        queryStr = queryStr.replace("#", "");
        var evar1Pages = "home,clearance,retailflyer,gift-cards,hft-new-items.html";
        if (baseURL == s.pageName && (evar1Pages.indexOf("home") >= 0)) {
            s.eVar1 = "Home";
        }

        if ((queryStr != "") && (evar1Pages.indexOf(queryStr) >= 0)) {
            s.eVar1 = queryStr;
        }
        var s_code = s.t();
        if (s_code)document.write(s_code);
    }
</script>
