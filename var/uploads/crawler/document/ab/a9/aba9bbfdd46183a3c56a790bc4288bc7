<script type="text/javascript">

    /**
     * The reason to keep this function at the footer is:
     * it is used from customer login page and also from footer ads track order section.
     * Aim is to make a common function.
     */
    function goTrack()
    {
        inputOrder = arguments[0];
        orderNo = document.getElementById(inputOrder).value;
        var baseUrl = "http://www.harborfreight.com/";
        if(orderNo != "" && orderNo != "Enter Order #") {
            url  = baseUrl + '/track?order='+orderNo;
            window.location = url;
            return true;
        } else {
            alert("Please enter the Order Number.");
            return false;
        }
    }

    function clearText()
    {
        inputOrder = arguments[0];
        orderNo = document.getElementById(inputOrder).value;
        if(orderNo == "Enter Order #") document.getElementById(inputOrder).value = "";
    }

</script>

