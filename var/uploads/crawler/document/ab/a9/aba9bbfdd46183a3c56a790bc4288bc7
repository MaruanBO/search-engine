<script type="text/javascript">
    function storeLocatorSubmit(EnteredZip)
    {
        var isValidZipCode = isValidUSZip(EnteredZip);
        if(isValidZipCode) {
            var redirectTo = "/storelocator/location/map?zipcode_input="+EnteredZip;
            window.location = redirectTo;
        } else {
            //alert("Sorry, we are not able to recognize your address");
            window.location = "/storelocator/location/map";
        }
    }
    function clearDefault(field,defaultText)
    {
        if (field.value == defaultText) {
            field.value = "";
        }
    }
    function isValidUSZip(zipcode){
        return /^\d{5}(-\d{4})?$/.test(zipcode);
    }
</script>        </div>
