<script type="text/javascript" src="https://ea.harborfreight.com:9143/EasyAsk/js/eascriptcomplete.js"> </script>
<script type="text/javascript"> 
    function initAutoComplete(){
        document.getElementById('search_mini_form').onsubmit = trimResultCount;
        EAComplete.init({
            url : 'https://ea.harborfreight.com:9143/EasyAsk/AutoComplete.jsp',
            id: 'searchInputField',
            dict: 'Ecomm',            
            num: 10,
            submitFctn: function() { submitSearchForm(); } });
        }
        
        if(document.getElementById('search_mini_form') != null){
            if (window.attachEvent) window.attachEvent('onload',initAutoComplete); else window.addEventListener("load",initAutoComplete,false);
        }
        function submitSearchForm(){
            trimResultCount();
            document.getElementById('search_mini_form').submit();
        }
        
        function trimResultCount(){
            var searchText = document.getElementById('searchInputField').value;
            var searchTextArray = searchText.split(" ");
            if(searchTextArray.length > 1 || searchText.indexOf('\t') != -1){

                searchText = searchText.replace('\t','');
                searchText = searchText.replace('\t',' ');
                searchArray = searchText.split(" ");
                if(isNumeric(searchArray[searchArray.length-1])){
                    searchArray.pop();
                    var newSearchText = searchArray.join(" ");
                    document.getElementById('search').value = newSearchText;
                }
            }
        }

    /* no-route CMS page search box */
    function initAutoCompleteNoRoute(){
        document.getElementById('search_mini_form_no_route').onsubmit = trimResultCountNoRoute;
        EAComplete.init({
            url : 'https://ea.harborfreight.com:9143/EasyAsk/AutoComplete.jsp',
            id: 'searchInputFieldNoRoute',
            dict: 'Ecomm',
            num: 10,
            submitFctn: function() { submitSearchFormNoRoute(); } });
    }

    if(document.getElementById('search_mini_form_no_route') != null){
        if (window.attachEvent) window.attachEvent('onload',initAutoCompleteNoRoute); else window.addEventListener("load",initAutoCompleteNoRoute,false);
    }
    function submitSearchFormNoRoute(){
        trimResultCountNoRoute();
        document.getElementById('search_mini_form_no_route').submit();
    }

    function trimResultCountNoRoute(){
        var searchText = document.getElementById('searchInputFieldNoRoute').value;
        var searchTextArray = searchText.split(" ");
        if(searchTextArray.length > 1 || searchText.indexOf('\t') != -1){

            searchText = searchText.replace('\t','');
            searchText = searchText.replace('\t',' ');
            searchArray = searchText.split(" ");
            if(isNumeric(searchArray[searchArray.length-1])){
                searchArray.pop();
                var newSearchText = searchArray.join(" ");
                document.getElementById('search').value = newSearchText;
            }
        }
    }
    
    function isNumeric(n) {
        return !isNaN(parseFloat(n)) && isFinite(n);
    }
</script><script type="text/javascript">
    var $j = jQuery.noConflict();
    jQuery(function($j) {
        if (!$j.cookie('hftref') && findcjparam(document.URL)) {
            $j.cookie('hftref', 'cj', {expires:90, domain:'.harborfreight.com'});
        }
    });

    function findcjparam(url) {
        if(url.search('hftref=cj')>0)
            return true;
        else {
            return false;
        }
    }
</script>
