<!-- div container_welcome -->


<script type="text/javascript">
    
    var welcomeoverlayForm = new VarienForm('welcomeoverlayForm', true); 
    var $j = jQuery.noConflict();   



    jQuery(function($j) {
        if (!$j.cookie('new2hft')) {
            $j.cookie('new2hft', '1',  {expires:60});
            $j('#container_welcome').removeClass('no-display');
            $j('<div id="welcome_overlay"></div>').insertAfter('#container_welcome');
        }
    });

    $j('#welcomeoverlayForm').submit(function () {
        CallAddEmailWebService();
        return false;
    });

    $j(document).keyup(function(e) {
        if (e.keyCode == 27) { CloseDialog(); }   // esc
    });


    function CloseDialog()
    {
        $j('#container_welcome').remove();
        $j('#welcome_overlay').remove();
    }

    function CallAddEmailWebService()
    {
        var pageUrl="http://pref.harborfreight22.com/hftpostemail/quicksignupadd.asmx/PostEmail";

        $j.ajax({
            type:"GET",
            url:pageUrl,
            crossDomain:true,
            contentType: "application/json; charset=utf-8",
            data:{"Source":"QuickSignup4", "Email":document.getElementById("emailAddress").value, "ZipCode":"", "FirstName":"" },
            dataType:"jsonp",
            success:OnSuccessCall,
            //error:OnErrorCall
            error:function (xhr, ajaxOptions, thrownError){
               // alert(xhr.status);
               // alert(thrownError);
               setTimeout("CloseDialog()", 3000);
            }       
        });      
    }

    function OnSuccessCall(data) {
        var email 			= "";
        var message 		= "";
        var emailResultCode = "";
        var msg = "";


        $j.each(data, function(i, item) {
            email = item.Email;
            message = item.Message;
            emailResultCode = item.ResultCode;
        });

        if (emailResultCode == "0") {
            msg = "Thank you for signing up.  Your email has been added to our list.";
            $j('#responseMessage').removeClass('validation-advice').addClass('validation-success');
            setTimeout("CloseDialog()", 3000);
        }
        else
        // negative result code is missing or invalid source - this should never occur
        // note: look at variable 'message' for valid sources
            if (emailResultCode < "0") {
                msg = "We were not able to process your email.";
            }
        else
            if (emailResultCode == "1") {
                msg = "Invalid email domain.  Please re-enter.";
            }
        else
            if (emailResultCode == "2") {
                msg = "Email address is missing.  Please re-enter.";
            }

        $j('#responseMessage').html(msg);
    }

</script>

                <!--footer-->
            </div>
            <!--wrap-conten-->
        </div>
        <!--end conten-->
        
