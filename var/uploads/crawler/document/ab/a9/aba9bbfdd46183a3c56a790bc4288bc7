<script>
    var googleMapsScripLoaded = 0;
    function loadGoogleMapsScript() {
        if ((googleMapsScripLoaded == 0)
            && (window.location.pathname != '/storelocator/location/map')
            && (window.location.pathname != '/storelocator/location/search')) {
            var script = document.createElement("script");
            script.type = "text/javascript";
            script.src = "http://maps.googleapis.com/maps/api/js?client=gme-harborfreighttools&sensor=false&v=3.exp&libraries=places&async=2&callback=initialize_nostorelocator";
            document.body.appendChild(script);
            googleMapsScripLoaded = 1;
        } else {
            initialize_nostorelocator();
        }
    }

    jQuery("#store-change").click(function() {
        loadGoogleMapsScript();
        jQuery("#zip-search-popup").toggle(100);
    });
    jQuery("#close-zip-search-popup").click(function() {
        jQuery("#zip-search-popup").toggle(100);
    });

    function locationSubmit(address) {
    geocoder = new google.maps.Geocoder();
        geocoder.geocode( {'address': address}, function(results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                window.location = "/storelocator/location/map?zipcode_input="+decodeURI(address);
            } else {
                document.getElementById('zipcode_input_popup').innerHTML = '';
                //alert("Sorry, we are not able to recognize your address. Please enter valid address.");
                window.location = "/storelocator/location/map";
            }
        });
    }
</script>
