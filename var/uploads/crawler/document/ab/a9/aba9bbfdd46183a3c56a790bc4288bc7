<script>
    //<![CDATA[
    var redesignfeedbackForm = new VarienForm('redesignfeedbackForm', true);
    //]]>

    jQuery.noConflict();
    (function($) {
       $("#btn_feedback").colorbox({
            inline:true,
            href:"#redesignfeedbackForm-overlay",
            width:710,
            height: 1190,
            close:'',
            display:"block",
            onOpen:function(){
                $('#redesignfeedbackForm')[0].reset();
                $('#colorbox').css('background','none');
                $('#cboxWrapper').css('background','none');
                $('#cboxContent').css('background','none');
                $('#recaptcha_response_field').addClass('required-entry');
                $('#feedbackWindow li input').removeClass('validation-passed');
            },
            onComplete: function(){
                $('#cboxClose').css('display','none');
            },
            onClosed: function(){
                $('#colorbox').css('background','white');
                $('#cboxWrapper').css('background','white');
                $('#cboxContent').css('background','white');
                $('#advice-required-entry-captcha').css('display', 'none');
                $('#recaptcha_response_field').removeClass("validation-failed");
            }
        });
        $('#feedbackWindowClose').click(function(){
            $("#btn_feedback").colorbox.close();
        });
        $('#found1').click(function(){
            $('#notFound').val('');
            $('#notFound').removeClass('required-entry');
            $('#notFound').removeClass('validation-failed');
            $('#advice-required-entry-notFound').hide();

        });
        $('#found2').click(function(){
            $('#notFound').addClass('required-entry');
        });
        $('#btnFeedbackSubmit').click(function(){
            $('#error_captcha').css("display", "none");
            var validator = new Validation(redesignfeedbackForm.form);
            if (validator.validate()) {
                var url = "http://www.harborfreight.com/redesignfeedback/index/send/";
                var reason = $('input:radio[name=reason]:checked').val();
                var visited = $('input:radio[name=visited]:checked').val();
                var found = $('input:radio[name=found]:checked').val();
                var recaptcha_response_field = $('#recaptcha_response_field').val();
                var notFound = $('#notFound').val();;
                var feebackSuggest = $('#feebackSuggest').val();
                var emailFeedback = $('#emailFeedback').val();
                var recaptcha_challenge_field = $('#recaptcha_challenge_field').val();
                $("#redesignfeedbackForm :input").attr("disabled", true);
                $.ajax({
                    url: url,
                    type: 'POST',
                    dataType: "json",
                    data: {
                            "reason":reason,
                            "visited":visited,
                            "found":found,
                            "notFound":notFound,
                            "feebackSuggest":feebackSuggest,
                            "recaptcha_response_field": recaptcha_response_field,
                            "recaptcha_challenge_field": recaptcha_challenge_field,
                            "emailFeedback": emailFeedback
                        },
                    success: function(html){
                        $("#redesignfeedbackForm :input").attr("disabled", false);
                        if(html.captcha == false) {
                            Recaptcha.reload();
                            $('#advice-required-entry-captcha').css("display", "block");
                            $('#recaptcha_response_field').addClass("validation-failed");
                        }
                        else {
                            $("#btn_feedback").colorbox.close();
                        }
                    }
                });
            }
        });
    })(jQuery);
</script>
