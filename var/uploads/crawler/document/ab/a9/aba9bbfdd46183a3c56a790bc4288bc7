 <script>
    var NumberOfCartItem = jQuery.cookie("cart_qty");
    cartText = "";
    if(NumberOfCartItem !=null) {

        if(NumberOfCartItem == 1) {
            cartText = "(1 Item)"
        }
        else if(NumberOfCartItem > 1) {
            cartText = "(" + NumberOfCartItem + " Items)"
        }
    }

    var cartHtml = jQuery("#my-cart").html();
    cartHtml = cartHtml.replace('title="My Cart"','title="My Cart ' + cartText + '"');
    jQuery("#my-cart").html(cartHtml);
</script>

