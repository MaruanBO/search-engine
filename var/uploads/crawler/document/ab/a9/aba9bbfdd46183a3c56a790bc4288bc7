<script type="text/javascript">
    function storeFinderSubmit()
    {
        var validZip = isValidUSZip(document.getElementById("store_finder_zip_input").value);
        if(validZip)
        {
            var zipcodeInput = document.getElementById("store_finder_zip_input").value;
            var redirectTo = "/storelocator/location/map?zipcode_input="+zipcodeInput;
            window.location = redirectTo;
        } else {

            alert("Please enter valid zipcode");
        }
    }

    function clearDefault(field,defaultText)
    {
        if (field.value == defaultText) {
            field.value = "";
        }
    }
    function isValidUSZip(zipcode){
        return /^\d{5}(-\d{4})?$/.test(zipcode);
    }
    if(window.location.protocol.indexOf("s") != -1){
        document.getElementById("httpshidefirst").style.display = "none";
        document.getElementById("httpshidesecond").style.display = "none";
        document.getElementById("httpshidethird").innerHTML = "hidden";
        document.getElementById("httpshidethird").style.display = "none";
    } else {
        document.getElementById("httpshidethird").innerHTML = "<iframe src='http://images.harborfreight.com/ecom/html/QuickSubscribe.htm?src=HFT_footer' width='225' height='45' marginheight='0' marginwidth='0' frameborder='0' scrolling='no'><p>Your browser does not support iframes.</p></iframe>";
            }
</script>
