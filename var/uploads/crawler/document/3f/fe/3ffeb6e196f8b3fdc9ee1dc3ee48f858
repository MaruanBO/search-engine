<script type="text/javascript">
	//HANDLE POPUP CLOSE
	$(document.body).on('click','.popupclose',function (e) {
		$('#ovrlybg').fadeOut(200,'linear');
		startCarousel();
	});
	$('#popup_innercont').on('click', function(e){
	    e.stopPropagation();
	});
	//HANDLE POPUP OPEN
	$(document.body).on('click', '.popupitm', function(){
		killCarousel();
		handlePopup('#popup_innercont',$(this).attr('controller'),'#ovrly_cont','#ovrlybg','','','','','');
		return false;
	});
</script>

