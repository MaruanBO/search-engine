$(document).ready(function() {	
	//collapsible google area
	$("#collapsible").hide();
	$("#collapse").click(function () {
		$("#collapsible").slideToggle("600"); //when #collapse is clicked, slide down and show #collapsible
		$("html,body").animate({				// and also scroll the screen down to that div
			scrollTop: $("#collapsible").offset().top
			},600);
		$(this).text($(this).text() == 'Learn More' ? 'Hide' : 'Learn More'); // and change text to 'hide'

	});
});

