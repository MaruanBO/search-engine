<script>

    $(document).mouseup(function(e) {
        var container = $('.modal-dialog');
        if (!container.is(e.target)
            && container.has(e.target).length === 0)
        {
            $('#vimeoModal').hide();
            $('#vimeoModal iframe').attr("src", jQuery("#vimeoModal iframe").attr("src"));
            $('#vimeoModal-2').hide();
            $('#vimeoModal-2 iframe').attr("src", jQuery("#vimeoModal-2 iframe").attr("src"));
        }
    });
    $('#close-vid-1').click(function () {
        $('#vimeoModal').hide();
        $('#vimeoModal iframe').attr("src", jQuery("#vimeoModal iframe").attr("src"));
    });
    $('#close-vid-2').click(function () {
        $('#vimeoModal-2').hide();
        $('#vimeoModal-2 iframe').attr("src", jQuery("#vimeoModal-2 iframe").attr("src"));
    });

</script>


