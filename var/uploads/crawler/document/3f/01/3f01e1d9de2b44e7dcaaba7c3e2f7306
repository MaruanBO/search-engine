		        $("#txtSearch").keypress(function(e) {		        
		            if (e.shiftKey) {
		                // prevent the entry of "<" (keycode 60) or ">" (keycode 62) 
		                if (e.keyCode == 60 || e.which == 60 || e.keyCode == 62 || e.which == 62) {
		                    return false;
		                }
		            } 

		            //check if the enter key is pressed in the search textbox, handles all browsers
		            if (e.keyCode == 13 || e.which == 13) {
		                $("#ctl00_btnSearch").click();
		                return false;
		            }

		            return true;
		        });        
		    
			    
