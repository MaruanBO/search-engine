      <script type="text/javascript">  		
        var prodId = '' 
        var pageType = 'other';
        var totalValue = '';
        var pathName = window.location.pathname;  
        
        if(pathName.toLowerCase().indexOf("/item--") !== -1){
            prodId = pageItemNo; 
            pageType = 'product';
        } else if (pathName.toLowerCase().indexOf("/cartpage") !== -1 || pathName.toLowerCase().indexOf("/recap") !== -1 || pathName.toLowerCase().indexOf("/addressentry") !== -1){
            totalValue = $('td [id*="_cart_lblsubtot"]').attr('rel'); //-order subtotal	
            pageType = 'cart';
            prodId = getCartProducts(products);          
        } else if(pathName.toLowerCase().indexOf("/ordercomplete") !== -1){
            totalValue = $('td [id*="_cart_lblsubtot"]').attr('rel'); //-order subtotal	
            pageType = 'purchase';
            prodId = getCartProducts(products);
        } else if(pathName.toLowerCase().indexOf("/itemsearch") !== -1){
            pageType = 'searchresults';
        } else if(pathName.toLowerCase().indexOf("/index") !== -1 || pathName === "/"){
            pageType = 'home';
        } else if(typeof navigationPage != "undefined" && navigationPage){
            pageType = 'category';
        } 
                
        var google_tag_params = {			
          ecomm_prodid: prodId, 			
          ecomm_pagetype: pageType,	
          ecomm_totalvalue: totalValue		
         };
         
         function getCartProducts(prodArray){
            var prodString = "[";
            for( i=0; i < prodArray.length; i++){
                prodString += "'" + prodArray[i] + "'";
                // if this is not the last one then add a comma.
                if(prodArray.length > i + 1) prodString += ", ";
            }
            prodString += "]";
            return prodString;
         }
      </script>
