<div id="NewsletterSignupModal" class="GlobalHeader Home ">

  <a class="newsletterSignupModal" href="#emailSignUpForm" style="display:none;">
    Newsletter Sign Up
  </a>

  <div style="display:none">
    <iframe id="emailSignUpForm" style="border:none;" src="https://app.e2ma.net/app2/audience/signup/1776463/1742937/?v=a" width="530" height="560"></iframe>
  </div>

</div>

<script>
Foundation.documentReady.then(function () {
  jQuery( function(){
    var modalCookieName = "newsletterSignupDisplayed"
    var modalCookie = modalCookieName+"="+Date();

    var tickSpan = 86400000;
    var displayFrequency = 1; // every so many tickSpans

    displayModalAndSetCookie = function(){
      document.cookie = modalCookie;
      jQuery("a.newsletterSignupModal").fancybox().click();
    }

    if(document.cookie.indexOf(modalCookieName) == -1){
      displayModalAndSetCookie();
    }else{
      var cookieList = document.cookie.split('; ');
      for( mrIndex in cookieList){
        //console.log(cookieList[mrIndex]);
        //console.log(cookieList[mrIndex].indexOf(modalCookieName) != -1);
        if(cookieList[mrIndex].indexOf(modalCookieName) != -1){
          var lastDate = cookieList[mrIndex].split("=")[1]; // grab the date of the cookie
          var tickSpansSinceLastCookie = (Date.parse(Date()) - Date.parse(lastDate))/tickSpan;
          //console.log(lastDate);
          //console.log(tickSpansSinceLastCookie);
          //console.log((Date.parse(Date()) - Date.parse(lastDate))/tickSpan);
          //console.log(tickSpansSinceLastCookie >= displayFrequency);
          if(tickSpansSinceLastCookie >= displayFrequency){
            console.log("it's been enough time");
            displayModalAndSetCookie();
          }
          break; // no need to look at the rest
        }
      }
    }
  });
});
</script>
<!-- End #NewsletterSignupModal -->


