            var curDateTime = new Date();
            var timezone_offset = -(curDateTime.getTimezoneOffset()/60);
            document.getElementById('timezone_offset').value = timezone_offset;

            /**
             * DHTML email validation script. Courtesy of SmartWebby.com (http://www.smartwebby.com/dhtml/)
             */
            function email_check(str) {
                    var at="@"
                    var dot="."
                    var lat=str.indexOf(at)
                    var lstr=str.length
                    var ldot=str.indexOf(dot)

                    if (str.indexOf(at)==-1){
                            return false;
                    }
                    if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
                            return false;
                    }
                    if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
                        return false;
                    }
                    if (str.indexOf(at,(lat+1))!=-1){
                        return false;
                    }
                    if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
                        return false;
                    }
                    if (str.indexOf(dot,(lat+2))==-1){
                        return false;
                    }
                    if (str.indexOf(" ")!=-1 || str.indexOf(",")!=-1){
                        return false;
                    }
            }

            function da_submit() {
                    var brands = [];
                    var num_brands = 0;
                    elems = document.saleform.elements;
                    for(var i = 0; i < elems.length; i++) {
                            if (elems[i].name.indexOf('designers') != -1 && elems[i].checked) {
                                    brands[num_brands] = elems[i].value;
                                    num_brands++;
                            }
                    }

                    if (num_brands < 5) {
                            alert('Please select at least five brands.');
                            return false;
                    } else {
                            document.getElementById('selected_brands').value=brands;
                    }

                    var email = document.getElementById('email');

                    if (email.value == '') {
                            alert('Please provide an email address.');
                            return false;
                    } else if (email_check(email.value) == false) {
                            alert('Please provide a valid email address.');
                            return false;
                    }

                    document.getElementById('action').value='Submit';
                    document.saleform.submit();
            }
