    var gtm = {
        trackPageView: function (host, path, title) {
            dataLayer.push({
                'event': 'backbonePageView', // Name of the event which will trigger the tag in GTM
                'backbonePageURL': path,
                'backbonePageTitle': title
            });
        },

        trackVirtualPageView: function (host, path, title) {
            dataLayer.push({
                'event': 'backboneVirtualPageView', // Name of the event which will trigger the tag in GTM
                'backboneVirtualPageURL': path,
                'backboneVirtualPageTitle': title
            });
        },

        trackEvent: function (category, action, label) {
            if (label === "currentUrl") {
                label = window.location.pathname;
            }

            dataLayer.push({
                'event': 'analyticsEvent',
                'eventCategory': category,
                'eventAction': action,
                'eventLabel': label
            });
        },

        trackDataEvent: function (src) {
            var category = src.getAttribute('data-event-category');
            var action = src.getAttribute('data-event-action');
            var label = src.getAttribute('data-event-label');

            if (category != undefined && action != undefined && label != undefined) {
                this.trackEvent(category, action, label);
            }
        }
    };

