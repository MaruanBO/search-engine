<script type="text/javascript">
    $(document).ready(function($) {
        $("#id_destination_0").autocomplete({
            source: function( request, response ) {
            $.ajax({
                url: "/locations/search/",
                dataType: "json",
                data: {term: request.term},
                success: function(data) {
                            response($.map(data, function(item, i) {
                                return {
                                    label: item[0],
                                    id: item[1],
                                };
                             }));
                        }
                });
            },
            minLength: 2,
            autoFocus: true,
            select: function(event, ui) {
                $('#id_destination_1').val(ui.item.id);
            }
        });

    <!-- styling -->
    $('#id_destination_0').addClass("search-mag-glass");

    <!-- placeholders -->
    $('#id_destination_0').attr("placeholder" ,"select a destination");

    });
</script>
<!-- END: Autocomplete -->

