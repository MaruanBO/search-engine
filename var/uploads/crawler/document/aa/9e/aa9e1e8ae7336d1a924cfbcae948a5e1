        <script type="text/javascript">
          	var tempHTML = jQuery("div.pr-stars").length, loadingStars;
            jQuery(document).ready(function(e) {
				loadingStars = setInterval(function(){checkDOM()}, 100);
            });
			var failSafeDelay = 0;
			function checkDOM() {
				failSafeDelay++;
				if(jQuery("div.pr-stars").length > 0 || failSafeDelay > 10) {
					clearInterval(loadingStars);
					loadCarousels();
				}
			}
			
			function loadCarousels() {
				if(jQuery("div.carouselContent")) {
					jQuery("ul.carousel").each(function(index, element) {
						jQuery(this).cycle({
							fx:     'scrollHorz',
							delay:  -2000,
							fit: 0,
							next: jQuery(this).parent("div.carouselContent").next("div.carouselCtrls").children("div.rightArrow"),
							prev: jQuery(this).parent("div.carouselContent").next("div.carouselCtrls").children("div.leftArrow"),
							timeout: 0,
							width: 718
						});
                    });
				}
			}
