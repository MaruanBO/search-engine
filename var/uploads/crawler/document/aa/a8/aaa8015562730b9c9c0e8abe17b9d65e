	<script type="text/javascript">
			function setPuDate(date) {
				var newDate = new Date(date.getFullYear() , date.getMonth(), (date.getDate() + 1));
				if (getDoDate() <= date) setDoDate(newDate);
				var updateDays = false;
				if ((date.getMonth() != getPuDate().getMonth()) || (date.getFullYear() != getPuDate().getFullYear())) updateDays = true;
				$("input[name='puDay']").val(date.getDate());
				$("input[name='puMonth']").val(date.getMonth() + 1);
				$("input[name='puYear']").val(date.getFullYear());
				setPuDisplayDate(updateDays);
			}
		</script>
	<script type="text/javascript">
		var chineseLicenseInfo = ("To be eligible to hire a car in China, the driver must hold a valid driving licence issued by People\'s Republic of China (Mainland). No other driving licences will be accepted, even when accompanied by an International Driving Permit (IDP).");
		var isChina = false;
		$(document).ready(function(){
			$("#pu-country, #do-country").bind("change", function(){
				if ($(this).val() == "China") {
					$("#chinese-driver-license").show();
					isChina = true;
				} else {
					$("#chinese-driver-license").hide();
					isChina = false;
				}
			});
			
			$("#SearchResultsForm").bind("submit", function(){
				if (isChina && (!$("#chinese-license").is(":checked"))) {
					alert(chineseLicenseInfo);
					return false;
				}
			});
		});
