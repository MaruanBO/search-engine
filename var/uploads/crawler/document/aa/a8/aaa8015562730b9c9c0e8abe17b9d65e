        <script type="text/javascript">
       		function initPu() {
       			var foundDay = false;
       			
       			var dow = getDoDate().getDay();
       			if (dow == 0) { setDoDate(new Date(getPuDate().getFullYear(), getPuDate().getMonth(), (getPuDate().getDate() + 1))); }
       			
       			var year = getPuDate().getFullYear();
       			var month = parseInt(getPuDate().getMonth()) + 1;
	       		var day = getPuDate().getDate();
	       		var monthYear = month + "-" + year;
	       		var firstDay = 1;
	       		var nextDay = new Date(year, (month-1), 1);
	       		var lastDay = 32 - new Date(year, (month - 1), 32).getDate();
	       		
	       		$("#puMonth_select").val(monthYear);
	       		$("#puDay_select option").remove();
       			$("#puDay_select").append("<option value=\"-1\" disabled=\"disabled\">" + dayTxt + "</option>");

	       		for (var i = firstDay; i <= lastDay; i++) {
		       		nextDay.setDate(i);
		       		var formattedDate = $.datepicker.formatDate("D d", nextDay, { dayNamesShort: dayNamesShort });
		       		$("#puDay_select").append("<option value=\"" + i + "\">" + formattedDate + "</option>");
		       		if (i == day) foundDay = true;
	       		}

       			if (foundDay) {
		       		$("#puDay_select").val(day);
		       		if (dow == 0) {
		       			$("input[name='puDay']").val($("#puDay_select").val());
		       			var newDoDate = new Date(getDoDate().getFullYear(), getDoDate().getMonth(), (getDoDate().getDate() + 1));
		       			setDoDate(newDoDate);
		       		}
	       		} else {
		       		$("input[name='puDay']").val($("#puDay_select").val());
	       		}
       		}
       		
       		function initDo() {	
       			var foundDay = false;
       			
       			var dow = getDoDate().getDay();
       			if (dow == 0) { setDoDate(new Date(getDoDate().getFullYear(), getDoDate().getMonth(), (getDoDate().getDate() + 1))); }
       			
       			var year = getDoDate().getFullYear();
       			var month = parseInt(getDoDate().getMonth()) + 1;
       			var day = getDoDate().getDate();
       			var monthYear = month + "-" + year;
       			var firstDay = 1;
       			var nextDay = new Date(year, (month-1), 1);
       			var lastDay = 32 - new Date(year, (month - 1), 32).getDate();
       			
       			$("#doMonth_select").val(monthYear);
       			$("#doDay_select option").remove();
       			$("#doDay_select").append("<option value=\"-1\" disabled=\"disabled\">" + dayTxt + "</option>");
				
       			for (var i = firstDay; i <= lastDay; i++) {
       				nextDay.setDate(i);
       				var formattedDate = $.datepicker.formatDate("D d", nextDay, { dayNamesShort: dayNamesShort });
       				$("#doDay_select").append("<option value=\"" + i + "\">" + formattedDate + "</option>");
       				if (i == day) foundDay = true;
       			}

       			if (foundDay) {
       				$("#doDay_select").val(day);
       				if (dow == 0) {
		       			$("input[name='doDay']").val($("#doDay_select").val());
		       			var newDoDate = new Date(getDoDate().getFullYear(), getDoDate().getMonth(), getDoDate().getDate());
		       			setDoDate(newDoDate);
		       		}
       			} else {
       				$("input[name='doDay']").val($("#doDay_select").val());
       			}

       		}
       	</script>
   	<script type="text/javascript">
	
        function formSubmit()
        {
        if( $('#returntolocation').attr('checked') == true ) {
            showReturnDropDown();
        }

        $('input[name=locationName]').val( $( droplists[2].jqSelector ).find('option:selected').html() );
        $('input[name=dropLocationName]').val( $( droplists[5].jqSelector ).find('option:selected').html() );
        
        
        if( $( droplists[2].jqSelector ).val() == -1 && $( droplists[5].jqSelector ).val() == -1 ) {
            
            if( $( droplists[0].jqSelector ).val() == $( droplists[3].jqSelector ).val() && $( droplists[1].jqSelector ).val() == $( droplists[4].jqSelector ).val() ) {
                $('form[name=SearchResultsForm] input[name=searchType]').val('allareasgeosearch');
                $('form[name=SearchResultsForm] input[name=doFiltering]').val('true');
                $('form[name=SearchResultsForm] input[name=filterFrom]').val('0');
                $('form[name=SearchResultsForm] input[name=filterTo]').val('20');
				
						$('form[name=SearchResultsForm] input[name=filterTo]').val('49');
					
            } else {                          
                $('form[name=SearchResultsForm] input[name=searchType]').val('manytomanysearch');
                $('form[name=SearchResultsForm] input[name=doFiltering]').val('true');
                $('form[name=SearchResultsForm] input[name=filterFrom]').val('0');
                $('form[name=SearchResultsForm] input[name=filterTo]').val('20');
            }
    
        } else if(  $( droplists[2].jqSelector ).val() == -1 && $( droplists[5].jqSelector ).val() != -1 ) {
            $('form[name=SearchResultsForm] input[name=searchType]').val('manytoonesearch');
            $('form[name=SearchResultsForm] input[name=doFiltering]').val('true');
            $('form[name=SearchResultsForm] input[name=filterFrom]').val('0');
            $('form[name=SearchResultsForm] input[name=filterTo]').val('20');
        } else if(  $( droplists[2].jqSelector ).val() != -1 && $( droplists[5].jqSelector ).val() == -1 ) {
            $('form[name=SearchResultsForm] input[name=searchType]').val('onetomanysearch');
            $('form[name=SearchResultsForm] input[name=doFiltering]').val('true');
            $('form[name=SearchResultsForm] input[name=filterFrom]').val('0');
            $('form[name=SearchResultsForm] input[name=filterTo]').val('20');
        } else {
            $('form[name=SearchResultsForm] input[name=searchType]').val('');
            
                    $('form[name=SearchResultsForm] input[name=doFiltering]').val('false');
                    $('form[name=SearchResultsForm] input[name=filterFrom]').val('0');
                    $('form[name=SearchResultsForm] input[name=filterTo]').val('1000');
                
            
            
        }
        

        if(validateSearchResultsForm( document.SearchResultsForm ) ) { 
            
            saveInput();
            $('form[name=SearchResultsForm]').append(
                    $( document.createElement('input') )
                        .attr('type','hidden')
                        .attr('name','fromLocChoose')
                        .val(true)
                ).submit();      
        }else {
            
            
            
            return false;
        }
    }
	
        
    function droplistPopulate( data, droplistJSON, callback ) {
        
        $.ajax({
                url: '/AjaxDroplists.do;jsessionid=FDD28F030F58741CD2762245647DF2DF.node224a',
                
                data: data  + '&wrapNonAirports=true' ,
                	
                dataType: 'json',
                success: function( result ) {            
                    $( droplists ).each( function() {                 
                        if( this.index >= droplistJSON.index ) {                  
                            var element; 
                            
                            element = $( this.jqSelector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) );
                            	
                        }
                        if( this.index != droplistJSON.index )
                            $( element ).attr('disabled',true);
                    });
                    
                    
                   
                   
                    	if(fillTopLocsOptions( droplistJSON.jqSelector, result )){
                    	
                    		$(droplistJSON.jqSelector).attr('disabled', false);
                    	}
                    
                    	else {
                    		
                    	
                   
                    		$( result ).each(function() {      
                    			
								
                    			
                    			if( typeof this.timeZone != 'undefined' ) {
                                    
                                    $( droplistJSON.jqSelector ).append( 
                                            $( document.createElement('option') )
                                                .html( this.name  )
                                                .attr('timeZone',this.timeZone)
                                                .val( this.id ) )
                                            .attr('disabled',false);
                                } else {
                                    
                                    $( droplistJSON.jqSelector ).append( 
                                            $( document.createElement('option') )
                                                .html( this.name  )
                                                .val( this.id ) )
                                            .attr('disabled',false);                      
                                }
                            }); 
                    		
                     } 
                        
                    if( typeof callback != 'undefined' ) callback.call(); 
                }
            });
        	
        }
    
        function droplistPopulateWithData( data ) {
        	
            $.ajax({
                url: '/AjaxDroplists.do;jsessionid=FDD28F030F58741CD2762245647DF2DF.node224a',
                
                data: data  + '&mode=fillAll&wrapNonAirports=true' ,
                	
                dataType: 'json',
                success: function( result ) {   
                    $( droplists ).each(function() {                  
                        var selector = this.jqSelector;
                        
                        var enable = false;  
                        var element; 
                        
                        element = $( this.jqSelector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) ).attr('disabled',true);
                        	
                       
                       // $( selector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) ).attr('disabled',true);
                                        
                            
                            
                            
                            
                            	var enableDefaultSelection = false;
								
                            	if(this.fillAllName === 'locationList' && fillTopLocsOptions(selector, result[this.fillAllName])) {
                            		
                            		enable = true;
                            		enableDefaultSelection = true;
                            	}
                            	else {
                            		
                            
                                                        
	                            if (typeof(this)=='object') { 
		                            $( result[ this.fillAllName ] ).each(function() {
		                                
										
		                            	
		                            	if( typeof this.timeZone != 'undefined' ) {
		                                    
		                                    $( selector ).append(
		                                            $( document.createElement('option') )
		                                                .html( this.name  )
		                                                .attr('timeZone',this.timeZone)
		                                                .val( this.id )
		                                        ); 
		                                        enable = true;
		                                    
		                                } else {
		                                    
		                                    $( selector ).append(
		                                            $( document.createElement('option') )
		                                                .html( this.name  )
		                                                .val( this.id )
		                                        ); 
		                                        enable = true;                              
		                                }
		                                
		                            });
	                            }
                            		 
                           		
                            		} 
	                            	
	                            		if(enableDefaultSelection) {
			                    			$('#pu-location').val('0');
			                    			$('#do-city').attr('disabled', true);
	                            		}
	                    			
							var JSEXPtj_landing_page_city_default_location = false;
							
                            
	                        
                            if( enable == true ) $( selector ).attr('disabled', false);
                            if( typeof result[ this.fillAllSelect ] != 'undefined' && result[ this.fillAllSelect ] != ''  ) {
                            	if (!JSEXPtj_landing_page_city_default_location) {
                            		
									$( selector ).val( result[ this.fillAllSelect ] ).find('option[value=0]').attr('disabled',true);
										
                            	}
                            }
                            
                    });                
                }
            
            });
        }
        
      
       
        function hasTopLocsAttributes(selectorName, list){
        	
        	var returnValue = false;
        	if(selectorName && list) {
        		
        		switch(selectorName){
        		case 'location':
        		case 'dropLocation':
        			break;
        			
        			default: return false;
        		}
        		
        		$(list).each(function(){
        			
        			if(this.type) {
        				switch(this.type) {
        				case 'other':
        				case 'railstation':
        					returnValue = true; break;
        				}
        			}
        			
        			if(returnValue) return false;
        			
        		});
        	}
        	
        	return returnValue;
        }
        
        function fillTopLocsOptions(selector, json) {
        	
        	if(!hasTopLocsAttributes($(selector).attr('name'), json)) return false;
        	
        	
        	
        	var foundAirport = false, foundcity = false;
        	var airportGroup = $('<optgroup></optgroup>').attr('label', 'Airport location');
        	var citiesGroup = $('<optgroup></optgroup>').attr('label', 'City location');
        	
        	$(json).each(function (){
        		
        		var appender = null;
        		
        		if (this.type === 'airport') {
        			appender = airportGroup;
        			foundAirport = true;
        		}
        		
       			
        		
        		else if (this.type === 'other'  || this.type === 'railstation'  || this.type === 'allareas') {
        			appender = citiesGroup;
        			foundcity = true;
        		}
       			
        		if(appender){
					
        		
        			var option = $('<option></option>').html(this.name ).attr('value', this.id);
        			$( appender ).append( option );
        		}
        	});
        	
        	if(foundAirport) $(selector).append(airportGroup);
       	
	       	
   		
        	if(foundcity) $(selector).append(citiesGroup);
        	
        	return foundAirport || foundcity  ;
        }
        
   

    $(document).ready(function () {
    	
    	
        $('#returntolocation').click( function (){
            $('#do-location-fieldset').slideToggle();
        });
        
        $('form[name=SearchResultsForm] input[type=submit]').click( function(){
            if(!formSubmit()){ 
                return false ;
            }
        });
    });
    
    </script>

