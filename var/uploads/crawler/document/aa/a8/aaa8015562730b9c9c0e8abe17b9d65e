		        <script type="text/javascript">
		             $(document).ready(function(){
		                 $('#morePopularDestinations').hide();
		                 
		                 $('#showMorePopularDestinations').bind('click',function(){
		                     $('#morePopularDestinations').toggle();
		                 });
		             });
		        </script>
		    <script type="text/javascript">
	$(document).ready(function(){
		$("#subscribe").click(function(){
			if ($("#email").val().search(/^\w+((-\w+)|(\.\w+)|(\.\-\w+)|(\-\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) != -1) {
				doAjaxSubscribe();
			} else {
				$("#subscribeFail").show();
			}
		});
		
		var minHeight = 199;
		$(".sub_search_section_footer").each(function(){
			if ($(this).outerHeight() > minHeight) {
				minHeight = $(this).outerHeight();
			}
		});
		$(".sub_search_section_footer").css("min-height", minHeight);
	});

	function doAjaxSubscribe() {
		var email = $("#email").val();
		$.ajax({
			type: "POST",
			url: "/ResponsiveProcessOfferSubscription.do",
			data: "email=" + email + "&fsFirst=Australia",
			success: function(response){
				$("#subscribe, #email, #subscribeFail").hide();
				$("#subscribeSuccess").show();
			},
			error: function(e){
				alert("Error: " + e);
			}
		});
	}
</script>

