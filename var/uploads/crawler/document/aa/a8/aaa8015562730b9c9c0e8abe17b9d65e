	$(window).load(function(){
		$("#formsubmit").bind("click", function(){
			location.hash = "#" + $("#SearchResultsForm").serialize();
		});
		var previousValues = location.hash;
		previousValues = previousValues.substring(1);
		
		if (previousValues != "") {
			
			var sameLocation = false;
			var locations = "country=#0&city=#1&locationId=#2&dropCountry=#3&dropCity=#4&dropLocationId=#5";
			var puDay = "";
			var puMonth = "";
			var puYear = "";
			var doDay = "";
			var doMonth = "";
			var doYear = "";
			var params = previousValues.split("&");
			for (var i = 0; i < params.length; i++) {
				var param = params[i].split("=");
				var key = param[0];
				var value = param[1];
				var $item = $("[name='" + key + "']");
				if (key == "country") { locations = locations.replace(/#0/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if (key == "city") { locations = locations.replace(/#1/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if (key == "location") { locations = locations.replace(/#2/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if (key == "dropCountry") { locations = locations.replace(/#3/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if (key == "dropCity") { locations = locations.replace(/#4/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if (key == "dropLocation") { locations = locations.replace(/#5/, value); $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				else if ((key == "puSameAsDo") && (value == "on")) { sameLocation = true; }
				
				else if (key == "puDay") { $item.val(value); puDay = value; }
				else if (key == "puMonth") { $item.val(value); puMonth = value; }
				else if (key == "puYear") { $item.val(value); puYear = value; }
				else if (key == "doDay") { $item.val(value); doDay = value; }
				else if (key == "doMonth") { $item.val(value); doMonth = value; }
				else if (key == "doYear") { $item.val(value); doYear = value; }
				else { $item.val((decodeURIComponent(value)).replace(/\+/g, ' ')); }
				
				
			}
			
			if (puDay != "") { $("#puDay_select").val(puDay); }
			if ((puMonth != "") && (puYear != "")) { $("#puMonth_select").val(puMonth + "-" + puYear); }
			if (doDay != "") { $("#doDay_select").val(doDay); }
			if ((doMonth != "") && (doYear != "")) { $("#doMonth_select").val(doMonth + "-" + doYear); }
			
			droplistPopulateWithData(locations);
			if (!sameLocation) {
				$("#do-location-fieldset").show();
			}
			
			
			location.hash = "";
		}
	});
</script>
						
<script type="text/javascript">
            $(document).ready(function(){
                var puLocSelector = $('select#pu-location');
                var datepicker = $('.datepicker');
                
                puLocSelector.bind('change',function(){
                	
                	var visibility = datepicker.css('display');
                	
                	
                	
                    var locValue = puLocSelector.val();
                    if(locValue !== 0 && visibility !== 'none'){
                        $('#dp_pu_img').datepicker('show');
                    }
                });
            });
