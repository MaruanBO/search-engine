	 <script type="text/javascript">
	 $(document).ready( function () {
		  $('.opt-toggle').bind('click', function () { 
			   if ($('#options-site').css('display') == 'none'){
				    $('#options-site, .opt-label, .opt-arrow').css('display', 'block'); 
			   }else{
				   $('#options-site, .opt-label, .opt-arrow').css('display', 'none');
			   }
		  });
		  
		    $('html').click(function() {
			    if ($('.btn-dd').hasClass('active')){
			        $('.btn-dd').removeClass('active');
			    }
			});
		    
			$('#btn-my-booking').click(function(event){
			    $('#btn-language, #btn-currency').removeClass('active');
			    $(this).toggleClass('active');
			    event.stopPropagation();
			});			
			
			$('#btn-currency').click(function(event){
                $('#btn-language, #btn-my-booking').removeClass('active');
                $(this).toggleClass('active');
                event.stopPropagation();
            });
			
			$('#btn-language').click(function(event){
                $('#btn-currency, #btn-my-booking').removeClass('active');
                $(this).toggleClass('active');
                event.stopPropagation();
            });
			
			$('.dd-menu').click(function(event){
			    event.stopPropagation();      
			});
	 });
	 </script>
	
