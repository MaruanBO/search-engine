    <script type="text/javascript">
			$(window).load(function(){
				$(".destination_img").each(function(){
					var $item = $(this);
					var destImgSrc = $item.css("background-image");
					destImgSrc = destImgSrc.replace("url(", "");
					destImgSrc = destImgSrc.replace(")", "");
					destImgSrc = destImgSrc.replace(/\"/g, "");
					var $destImg = $("<img/>");
					$destImg.attr("src", destImgSrc);
					$destImg.error(function(){ $item.css("background-image", "url(/images/site_graphics/newsite/location_default_img.jpg)"); });
				});
			});
		</script>
