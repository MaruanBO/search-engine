			<script>
			if (!jQuery("#mainNav").hasClass("outlet-nav")) {
				// Loop through main navigation elements for treatment
				var mainNavElement = document.getElementById("mainNav");
				var mainNavElements = jQuery("#mainNav > li");
				var mainNavWidth = 0;
				// Zero out main nav link padding to get correct width measurement
				//jQuery("#mainNav li.mainNavItem > a").attr("style", "padding : 0")
				for (var i = 0; i < mainNavElements.length; i++) {
					// Get width of main navigation elements except for clearance (assumed last element)
					mainNavWidth += jQuery(mainNavElements[i])[0].offsetWidth;
					// Reposition drop nav if too far right
					if ((jQuery(mainNavElements[i]).position().left + 502) > jQuery(".primary-nav").width()) {
						jQuery(mainNavElements[i]).find(".sub-nav").addClass("sub-nav-right");
						//$(mainNavElements[i]).find(".sub-nav").css("left", ($(".primary-nav").width() - ($(mainNavElements[i]).position().left + 502)) + "px");
					}
				}

				// Navigation: Using total width of prior nav elements, calculate width of clearance to make any fine adjustments to fit nav width properly.	
				jQuery("#mainNav li.mainNavItem > a").each(function() {
					jQuery(this).attr("style", "width : " + (Math.floor((jQuery(".primary-nav")[0].offsetWidth - mainNavWidth)/(mainNavElements.length)) + jQuery(this).width()) + "px;");
				});

				var mainNavWidth2 = 0;
				for (var i = 0; i < mainNavElements.length; i++) {
					// Get width of main navigation elements except for clearance (assumed last element)
					mainNavWidth2 += jQuery(mainNavElements[i])[0].offsetWidth;
					// Reposition drop nav if too far right
					if ((jQuery(mainNavElements[i]).position().left + 502) > jQuery(".primary-nav").width()) {
						jQuery(mainNavElements[i]).find(".sub-nav").addClass("sub-nav-right");
					}
				}

				jQuery(".primary-nav").find('[title="Clearance"]').parent().addClass("clearanceNav");
				jQuery(".primary-nav").find('[title="Clearance"]').attr("style", "width : " + (jQuery(".primary-nav").find('[title="Clearance"]').width() + jQuery(".primary-nav")[0].offsetWidth - mainNavWidth2) + "px");

				// Split price ranges
				var mainNavElementLink = mainNavElement.getElementsByTagName("a");
				for (var i=0; i<mainNavElementLink.length; i++) {
					if (mainNavElementLink[i].innerHTML.indexOf(" - ") > -1) {
						zales.splitPriceRange(mainNavElementLink[i]);
						var priceWrap = jQuery(mainNavElementLink[i]).find('.price-range');
						priceWrap.formatCurrency();
					}
				}
				for (var i=0; i<mainNavElementLink.length; i++) {
					if (mainNavElementLink[i].innerHTML.indexOf("Over ") > -1) {
						zales.splitPriceRange(mainNavElementLink[i]);
						var priceWrap = jQuery(mainNavElementLink[i]).find('.price-range');
						priceWrap.formatCurrency();
					}
				}
				var mainNavElementItem = mainNavElement.getElementsByTagName("li");
				for (var i=0; i<mainNavElementItem.length; i++) {
					if (mainNavElementItem[i].className.indexOf("mainNavItem") > -1) {
						jQuery(mainNavElementItem[i]).find(".subNavGroup:nth-child(4)").addClass("subNavGroupLast");
					}
				}

				//strip zeros from price range in drop downs
				var mainNavPriceRanges = jQuery('#mainNav span.price-range');
				zales.stripZeros(mainNavPriceRanges);

				// Drop navigation
				jQuery("#mainNav > li").live("mouseover", function () {
					clearTimeout(jQuery.data(this, 'delayDrop'));
					var currDrop = jQuery(this);
					var showDrop = setTimeout(function () {
						if (!jQuery(currDrop).hasClass("active")) {
							jQuery(currDrop).addClass("active");
						}
					}, 300);
					jQuery(this).data('delayDrop', showDrop);
				});
				jQuery("#mainNav > li").live("mouseout", function () {
					clearTimeout(jQuery.data(this, 'delayDrop'));
					var currDrop = jQuery(this);
					var showDrop = setTimeout(function () {
						if (jQuery(currDrop).hasClass("active")) {
							jQuery(currDrop).removeClass("active");
						}
					}, 200);
					jQuery(this).data('delayDrop', showDrop);
				});

				
				if ((typeof browser != 'undefined') && (browser != null && browser.mobilesafari)) {
					jQuery('body:first').addClass('mobile-safari');
					// remove desktop browser events
					jQuery("#mainNav > li").die('mouseover').die('mouseout');
	                // hide nav dropdowns when body element is touched
					jQuery("#borderWrapper").bind("mouseup", function(){return;});
				} 

				// Now widths are calculated and behaviors are set, show main nav
				jQuery("#mainNav").css("visibility", "visible");
			}
			</script>

		</div>   	

	
