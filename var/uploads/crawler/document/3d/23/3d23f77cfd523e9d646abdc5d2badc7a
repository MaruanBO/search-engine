<script type="text/javascript">
	$(document).ready(function() {
		// if email input field lost focus we do an ajax request to check if that email is not already used by an other account
		$('#PartnerUserEmail').blur( function() {
			if($('#PartnerUserEmail').val() != '') {
				$.post('https://www.plista.com/de/account/checkEmailExists',
					{email: $('#PartnerUserEmail').val()},
					function(data) {
						if(data != '') {
							$('#error_div_reg').html(data);
							$('#error_div_reg').css('display', 'block').fadeTo(1200, 0.9);
							setTimeout( function() {
								$('#error_div_reg').css('display', 'none').fadeTo(1200, 0);
							}, 5000);
						} else {
							$('#error_div_reg').fadeTo(1200, 0);
						}
					});
			}
		});

			});
</script>


