<script type="text/javascript">
    $(document).ready(function () {
        function doSearch() {

            var url = '/recipes/find/_/_/_/_/__keywords__';
            url = url.replace('__keywords__', encodeURIComponent($("#globalSearchKeywords").val()));
            window.location.href = url;
            $('#aspnetForm').attr("action", url);
            $('#aspnetForm').submit();

        }

        $("#btnSearch").click(function () {

            doSearch();

        });

        $("#globalSearchKeywords").keypress(function (e) {

            if (e.which == 13) {

                doSearch();

            } // end if

        });
    });
</script>




