        <script>
    
            var datenow = new Date();
            var downow = datenow.getDay();
        
            if (downow < 4)
                {
                document.getElementById('bestofnny_hype_container').style.display = 'block';
                }
            else
                {
                document.getElementById('nightlife-promo-container').style.display = 'block';
        
                if (downow == 4)
                    {
                    if (getcookie('nightlifepromo-thurs') != 1)
                        {
                        document.getElementById('nightlife-promo').style.display='none';
                        document.getElementById('nightlife-animation').style.display='block';
                        setcookie('nightlifepromo-thurs', 1, 1);
                        setTimeout(function(){document.getElementById('nightlife-promo').style.display='block';document.getElementById('nightlife-animation').style.display='none';},45000);
                        }
                    }
                else if (downow == 5)
                    {
                    if (getcookie('nightlifepromo-fri') != 1)
                        {
                        document.getElementById('nightlife-promo').style.display='none';
                        document.getElementById('nightlife-animation').style.display='block';
                        setcookie('nightlifepromo-fri', 1, 1);
                        setTimeout(function(){document.getElementById('nightlife-promo').style.display='block';document.getElementById('nightlife-animation').style.display='none';},45000);
                        }
                    }
                else if (downow == 6)
                    {
                    if (getcookie('nightlifepromo-sat') != 1)
                        {
                        document.getElementById('nightlife-promo').style.display='none';
                        document.getElementById('nightlife-animation').style.display='block';
                        setcookie('nightlifepromo-sat', 1, 1);
                        setTimeout(function(){document.getElementById('nightlife-promo').style.display='block';document.getElementById('nightlife-animation').style.display='none';},45000);
                        }
                    }
                else
                    {
                    // clear cookie
                    setcookie('nightlifepromo-thurs', 0, 7);
                    setcookie('nightlifepromo-fri', 0, 7);
                    setcookie('nightlifepromo-sat', 0, 7);
                    }
            
                }
            
        </script>
        
