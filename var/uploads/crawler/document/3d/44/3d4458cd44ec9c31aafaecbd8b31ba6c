<script type="text/javascript">
    var caro_height = 300;
    var caro_advancer = '';
    function caro_init()
        {
        caro_item_current = 1;
        caro_display_id = 'caro_display_a';
        caro_waiting_id = 'caro_display_b';
        caro_state = 1;
        var el_up = document.getElementById(caro_display_id);
        var el_next = document.getElementById(caro_waiting_id);
        var el_up_src = document.getElementById('caro_item' + caro_item_current);
        var numthumb_string = '';
        if (caro_item_current == caro_item_count)
            {
            var el_next_src = document.getElementById('caro_item1');
            }
        else
            {
            var caro_item_next = caro_item_current + 1;
            var el_next_src = document.getElementById('caro_item' + caro_item_next);
            }
        change_opacity(0, caro_waiting_id);
        el_up.innerHTML = el_up_src.innerHTML;
        el_next.innerHTML = el_next_src.innerHTML;
        for (i = 1; i <= caro_item_count; i++)
            {
            el_this = document.getElementById('caro_item' + i);
            if (el_this.offsetHeight > caro_height)
                {
                caro_height = el_this.offsetHeight;
                }
            numthumb_string += '<span id="caro_numthumb' + i + '" class="caro_numthumb_item" onclick="caro_advance(' + i + ');">' + i + '</span>';
            }
        numthumb_string += '<span style="margin-left: 10px;">';
        numthumb_string += '<span class="caro_numthumb_item" onclick="caro_advance(1);"><img src="/images/carousel/first.png" style="display: inline; width: 9px; heigh: 9px;"></span>';
        numthumb_string += '<span class="caro_numthumb_item" onclick="caro_advance(-1);"><img src="/images/carousel/prev.png" style="display: inline; width: 13px; heigh: 9px;"></span>';
        numthumb_string += '<span class="caro_numthumb_item" onclick="caro_setstate(0);" id="caro_pauseplay"><img src="/images/carousel/pause.png" style="display: inline; width: 8px; heigh: 9px;"></span>';
        numthumb_string += '<span class="caro_numthumb_item" onclick="caro_advance(0);"><img src="/images/carousel/next.png" style="display: inline; width: 13px; heigh: 9px;"></span>';
        numthumb_string += '<span class="caro_numthumb_item" onclick="caro_advance(' + caro_item_count + ');"><img src="/images/carousel/last.png" style="display: inline; width: 9px; heigh: 9px;"></span>';
        numthumb_string += '</span>';
        caro_play_image = new Image();
        caro_play_image.src = '/images/carousel/play.png';
        document.getElementById('caro_numthumb').innerHTML = numthumb_string;
        document.getElementById('caro_numthumb' + caro_item_current).setAttribute('class','caro_numthumb_item caro_numthumb_itemactive');
        document.getElementById('caro_display').style.height = caro_height + 'px';
        caro_advancer = setTimeout("caro_advance(0)", 8000);
        }
    function caro_setstate(thestate)
        {
        clearTimeout(caro_advancer);
        caro_state = thestate;
        if (thestate == 1)
            {
            caro_advancer = setTimeout("caro_advance(0)", 200);
            document.getElementById('caro_pauseplay').innerHTML = '<img src="/images/carousel/pause.png" style="display: inline;">';
            document.getElementById('caro_pauseplay').setAttribute('onclick','caro_setstate(0);');
            }
        else
            {
            document.getElementById('caro_pauseplay').innerHTML = '<img src="/images/carousel/play.png" style="display: inline;">';
            document.getElementById('caro_pauseplay').setAttribute('onclick','caro_setstate(1);');
            }
        }
    function caro_advance(item)
        {
        clearTimeout(caro_advancer);
        var el_up = document.getElementById(caro_display_id);
        var el_next = document.getElementById(caro_waiting_id);
        if (item == 0)
            {
            if (caro_item_current == caro_item_count)
                {
                caro_item_current = 1;
                }
            else
                {
                caro_item_current++;
                }
            if (caro_state == 1)
                {
                caro_advancer = setTimeout("caro_advance(0)", 8000);
                }
            }
        else if (item < 0)
            {
            if (caro_item_current == 1)
                {
                caro_item_current = caro_item_count;
                }
            else
                {
                caro_item_current--;
                }
            if (caro_state == 1)
                {
                caro_advancer = setTimeout("caro_advance(0)", 8000);
                }
            }
        else
            {
            caro_item_current = item;
            if (caro_state == 1)
                {
                caro_advancer = setTimeout("caro_advance(0)", 24000);
                }
            }
            var el_src = document.getElementById('caro_item' + caro_item_current);
            el_next.innerHTML = el_src.innerHTML;
            document.getElementById(caro_display_id).style.zIndex = 1;
            document.getElementById(caro_waiting_id).style.zIndex = 2;
            fade(caro_waiting_id, 0, 100, 500);
            fade(caro_display_id, 100, 0, 500);
            el_next.style.zIndex = parseInt(el_up.style.zIndex) + 1;
            caro_temp_id = caro_display_id;
            caro_display_id = caro_waiting_id;
            caro_waiting_id = caro_temp_id;
        for (i = 1; i <= caro_item_count; i++)
            {
            document.getElementById('caro_numthumb' + i).setAttribute('class','caro_numthumb_item');
            }
        document.getElementById('caro_numthumb' + caro_item_current).setAttribute('class','caro_numthumb_item caro_numthumb_itemactive');
        }
    function fade(id, opacstart, opacend, ms)
        {
        var speed = Math.round(ms / 100);
        var timer = 0;
        if (opacstart > opacend)
            {
            for (var i = opacstart; i >= opacend; i--)
                {
                setTimeout("change_opacity(" + i + ", '" + id + "')", (timer * speed));
                timer++;
                }
            }
        else if (opacstart < opacend)
            {
            for (var i = opacstart; i <= opacend; i++)
                {
                setTimeout("change_opacity(" + i + ", '" + id + "')", (timer * speed));
                timer++;
                }
            }
        }
    function change_opacity(opacity, id)
        {
        var object = document.getElementById(id).style;
        object.opacity = (opacity / 100);
        object.MozOpacity = (opacity / 100);
        object.KhtmlOpacity = (opacity / 100);
        object.filter = "alpha(opacity=" + opacity + ")";
        }
</script>
