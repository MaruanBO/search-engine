                <div id="JSoDElement"></div>
                <script type='text/javascript'>gnu_live = false;</script>
                <script type="text/javascript">
                    //gnu_live = true;
                    responseActive = 0;
                    var last_id = new Array();
                    last_id['all'] = 1;
                    last_id['youtube'] = 1;
                    last_id['gnuvisions'] = 0;
                    var max_video = new Array();
                    max_video['all'] = 10;
                    max_video['youtube'] = 10;
                    max_video['gnuvisions'] = 10;
                    
                    function video_advance(channel, id){
                        id = (id == 0 ? (gnu_live ? (max_video['all']) : 1) : id);
                        for(var i=1;i<=max_video[channel];i++){
                            document.getElementById('video_'+channel+'_numthumb'+i).className = "caro_numthumb_item";
                        }
                        document.getElementById('video_'+channel+'_numthumb'+id).className = "caro_numthumb_item caro_numthumb_itemactive";
                        document.getElementById('video_all_source').innerHTML = "";
                        if(responseActive == 0 && last_id[channel] != id){
                            document.getElementById("video_panel_"+channel).innerHTML = "<div style='width:413px;padding-top:108px;text-align:center;'><img style='display:inline;' src='http://watertowndailytimes.com/images/ajax_loader2.gif' width='32' height='32' /></div>"
                            if(document.getElementById("JSoDElement").getElementsByTagName("script")[0]){
                                document.getElementById("JSoDElement").removeChild(document.getElementById("JSoDElement").getElementsByTagName("script")[0]);
                            }
                            last_id[channel] = id;
                            responseActive = 1;
                            var scriptEle = document.createElement("script");
                            scriptEle.src = "http://webapps.wdt.net/videofeed/wdt_"+channel+".php?i="+id.toString()+"&w=413&h=232&showtitle=1&js=1";
                            document.getElementById("JSoDElement").appendChild(scriptEle);
                        }
                    }
                </script>
