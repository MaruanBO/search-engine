    <!-- school alerts code -->
    <script type="text/javascript">
    
        var alerts_counties = "'Jefferson','St. Lawrence','Lewis','Franklin','Oneida','Oswego','Clinton'";
        var alerts_site = "WDT";
        var alerts_datenow = new Date();
        var alerts_hournow = alerts_datenow.getHours();
        if (alerts_hournow >= 4 && alerts_hournow < 11)
            {
            var alerts_container = "webapps_school_alerts_am";
            }
        else
            {
            var alerts_container = "webapps_school_alerts_pm";
            }
            
        var alerts_script = "http://watertowndailytimes.com/section/schoolalerts?omniture=0&wd_module=" + encodeURIComponent(alerts_site) + "&wd_container=" + encodeURIComponent(alerts_container) + "&wd_counties=" + encodeURIComponent(alerts_counties);
    
        var head = document.getElementsByTagName('head')[0]
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = alerts_script;
        script.id = 'webapps_alert_script';
        head.appendChild(script);
        
        setInterval(function()
            {
            head.removeChild(script);
            script = document.createElement('script');
            script.type = 'text/javascript';
            script.src = alerts_script + '&cachebuster=' + new Date().getTime();
            script.id = 'webapps_alert_script';
            head.appendChild(script);
            }, 12000);
        
    </script>
