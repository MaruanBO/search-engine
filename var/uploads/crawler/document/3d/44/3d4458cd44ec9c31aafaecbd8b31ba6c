    <script type="text/javascript">
        function blogsort(a, b)
            {
            return (a.isopubdatetime > b.isopubdatetime) ? -1 : 1;
            }
        blogobj.sort(blogsort);
        var bloglistcontent = '';
        for (i=0; i<blogobj.length; i++)
            {
            bloglistcontent += '<div class="bloglist_block">';
            bloglistcontent += '<a href="/section/' + blogobj[i].category + '"><img src="/images/columnsandblogs/icon_' + blogobj[i].category + '.png" width="25" height="25" class="bloglist_icon dropshadow_shallow"></a>'
            bloglistcontent += '<div class="bloglist_text">';
            bloglistcontent += '<div class="bloglist_headline"><a href="/section/' + blogobj[i].category + '">' + blogobj[i].title + '</a></div>';
            bloglistcontent += '<div class="bloglist_title"><a href="/section/' + blogobj[i].category + '">' + blogobj[i].categoryname + '</a></div>';
            for (j=0; j<blogdata.length; j++)
                {
                if (blogdata[j].category == blogobj[i].category)
                    {
                    bloglistcontent += '<div class="bloglist_author">By ' + blogdata[j].author + '</div>';
                    break;
                    }
                }
            bloglistcontent += '<div class="bloglist_pubdatetime">' + blogobj[i].pubdatetime + '</div>';
            bloglistcontent += '</div>';
            bloglistcontent += '<div class="break"></div>';
            bloglistcontent += '</div>';
            }
        document.getElementById('bloglist').innerHTML = bloglistcontent;
    </script>
