// show/hide functions for Menu display
$(document).ready(function () {
    $(".showMenu").click(function () {
        $(".displayMenu").show();
        $(".hideMenu").show();
        $(".showMenu").hide();
    });
    $(".hideMenu").click(function () {
        $(".displayMenu").hide();
        $(".hideMenu").hide();
        $(".showMenu").show();
    });
});
