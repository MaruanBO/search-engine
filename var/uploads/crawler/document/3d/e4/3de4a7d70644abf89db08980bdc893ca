  if(chatBox != undefined) {
    var acctId = chatBox.id.split('-')[2]; 
    var deptId = chatBox.id.split('-')[3]; 
    /* load check image */
    $("<img>").attr('src', 'http://admin.instantservice.com/resources/smartbutton/' + acctId + '/' + deptId + '/available.gif?' + Math.floor(Math.random()*10001))
              .load(function() {
                $(chatBox).click(function() { window.open('http://www.speakeasy.net/includes/chat_popup.php?di=' + deptId, 'custclient', 'width=500, height=320'); })
                          .fadeTo(2000, 1.0);              
              })
              .error(function() { /* custom error function */ });           
  }
});
