(function($) {
	$().ready(function() {
		var $container = $('#examples').imagesLoaded(function() {
			$container.isotope({
				itemSelector: '.m09'
			})
		});

		$('.m27').on('click', 'a', function() {
			var filterValue = $(this).data('filter');
			if (filterValue) filterValue = '.' + filterValue;
			$container.isotope({filter: filterValue});
		});
	});
})(ps$);

