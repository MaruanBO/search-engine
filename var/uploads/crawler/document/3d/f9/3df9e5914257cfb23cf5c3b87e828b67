			$(function(){
				//when the page loads
				$("#breaking_news").delay(500).slideDown("slow");
				$(".home #first_content .contents").delay(500).animate({ paddingTop: 5}, "slow");	

				//when you close Breaking News
				$("#breaking_news_close").click(function() {
					$("#breaking_news").slideUp("slow");
					$(".home #first_content .contents").animate({ paddingTop: 0}, "slow");
