<script>
	var theInt = null;
	var curclicked = 0;
	var stop = 0;

	rotateContent = function(cur){
		clearInterval(theInt);
		theInt = setInterval(function(){
			$(".gallery_image").eq(curclicked).trigger('nextImage');
	    	curclicked++;
	    	if( 2 == curclicked ) curclicked = 0; //first number should be rotators.length - 1
			$(".changing_content").mouseover(function(){
				if (stop == 0) {
					clearInterval(theInt);
					stop = 1;
				}
			});
		}, 7000); //interval time
	};
</script>
