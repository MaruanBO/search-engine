<script language="Javascript">
	var gbInternalLink=false; 


	function makeDoubleDelegate(pobjFunction1, pobjFunction2)
	{
		return function()
			{	if (pobjFunction1)
					pobjFunction1();
				if (pobjFunction2)
					pobjFunction2();
			}
	}

	function DocumentPrep()
	{
	//	alert("btnClick");


	// LINKS FIRST
	//
		var ln=document.links.length; 

		for(var i=0;i<ln;i++)
		{ 
			var	sDynamicJavascriptCode =						  "	var sTestURL=/roadscholar.org/i; ";
				sDynamicJavascriptCode = sDynamicJavascriptCode + "	var sTestForm=/submit/i; ";
				sDynamicJavascriptCode = sDynamicJavascriptCode + "	var sThisURL=document.links[" + i + "].href; ";
				sDynamicJavascriptCode = sDynamicJavascriptCode + "	gbInternalLink = sTestURL.test(sThisURL) || sTestForm.test(sThisURL); ";
				sDynamicJavascriptCode = sDynamicJavascriptCode + "	return true; ";

			var objNewFunction = new Function(sDynamicJavascriptCode);

			document.links[i].onclick = makeDoubleDelegate(objNewFunction, document.links[i].onclick);
		} 

	// FORM ELEMENTS SECOND
	//

		for(var i=0;i < document.forms.length ;i++)
		{
			var frm = document.forms[i];

			for (var x=0; x < frm.elements.length; x++)
			{
				var ctl = frm.elements[x];

				if (ctl.type.toUpperCase() == "BUTTON")
					ctl.onclick = makeDoubleDelegate(function(){ gbInternalLink = true; }, ctl.onclick);

				else if (ctl.type.toUpperCase() == "SELECT-ONE")
					ctl.onchange = makeDoubleDelegate(function(){ gbInternalLink = true; }, ctl.onchange);
				
				else
				{ // nothing to do - ignore everything else 
				}
				
			}
		}

	}



	window.onload = makeDoubleDelegate(window.onload,  function(){ DocumentPrep(); return true; });




	window.onunload=function(){ 
		if(!gbInternalLink)
		{
           
            if (document.getElementById("signupthree").value == "2")
            {            
    		  //  window.open('http://www.roadscholar.org/popups/SGN.asp','','toolbar=0,scrollbars=0,location=no,statusbar=0,menubar=0,resizable=0,width=409,height=257,top=100,left=100');

            }

        }
	} 

</script>


