        function SizeImage(sImageName) {
            var maxWidth = 100;
            var maxHeight = 200;
            var newWidth = 100;
            var newHeight = 200;
            var modX, modY;
            var img = document.getElementById(sImageName);

            modX = img.width / maxWidth;
            modY = img.height / maxHeight;

            if (modX > modY) {
                newWidth = (img.width / modX);
                newHeight = (img.height / modX);
            }
            else {
                newWidth = (img.width / modY);
                newHeight = (img.height / modY);
            }
            img.height = newHeight;
            img.width = newWidth;
            img.style.visibility = "visible";
        }
-->
