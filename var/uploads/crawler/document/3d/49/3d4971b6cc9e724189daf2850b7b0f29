	<script language="JavaScript">
	    function showHideLocations(thelocal) {
            var Locations = document.getElementById(thelocal);
	        if (Locations != null) {
                Locations.style.display = Locations.style.display == 'none' ? 'block' : 'none';
	        }
	    }


	    function submit_this() {

	        var buildlist;
	        var build2;

	        buildlist = "";

	        if (document.getElementById("thePROGRAM_TYPE").selectedIndex != 0) {
	            //alert (document.getElementById("thePROGRAM_TYPE").value);
	            var selectedTypes = "";
	            var typeCond = "";
	            var i

	            for (i = 0; i < document.getElementById('thePROGRAM_TYPE').options.length; i++) {
	                if (document.getElementById('thePROGRAM_TYPE').options[i].selected) {

	                    selectedTypes += document.getElementById('thePROGRAM_TYPE').options[i].value + ",";

	                }

	            }

	            buildlist = "?ProgDesc=" + escape(selectedTypes.substr(0, (selectedTypes.length - 1))) + "&TypeCond=" + typeCond;
	        }

	        if (document.getElementById("theCOUNTRY_CODE").selectedIndex != 0) {
	            //alert (document.getElementById("theCOUNTRY_CODE").value);
	            if (buildlist == "") {
	                buildlist = "?";
	            }
	            else {
	                buildlist = buildlist + "&";
	            }

	            buildlist = buildlist + "CountryCode=" + document.getElementById("theCOUNTRY_CODE").value;
	        }

	        if (document.getElementById("theSTATE_PROVINCE_CODE").selectedIndex != 0) {
	            //alert (document.getElementById("theSTATE_PROVINCE_CODE").value);
	            if (buildlist == "") {
	                buildlist = "?";
	            }
	            else {
	                buildlist = buildlist + "&";
	            }
	            buildlist = buildlist + "StateCode=" + document.getElementById("theSTATE_PROVINCE_CODE").value;
	        }

	        var datelen = document.getElementById("StartDate").value.length;

	        if ((datelen > 2) && (document.getElementById("StartDate").value != "[Any Date]")) {
	            //            alert(document.getElementById("StartDate").value);
	            if (buildlist == "") {
	                buildlist = "?";
	            }
	            else {
	                buildlist = buildlist + "&";
	            }
	            buildlist = buildlist + "StartDate=" + document.getElementById("StartDate").value;
	        }


	        build2 = "../programs/search_res.asp" + buildlist;
	        if (buildlist.length > 2) {
	            document.location = "../programs/search_res.asp" + buildlist;
	        }

	    }



	    var startDate
	    $(function () {
	        startDate = new dhtmlxCalendarObject('StartDate', true, { isWinHeader: true, headerButtons: "X" });
	        startDate.setDateFormat("%m/%d/%Y");
	    });

	    function closeCals() {
	        startDate.hide();
	    }

	</script>





    

