<script type="text/javascript">
function DefValue(elId, defVal){
var el = document.getElementById(elId);
var empty = false;
var first = true;
var defaultvalue = defVal;
if (el.value == '' || el.value == undefined || el.value == null)
{
el.value = defaultvalue;
empty = true;
}
//el.setAttribute('autocomplete', 'off');
el.focus();
el.onblur = function()
{
if (el.value == '')
{
empty = true;
el.value = defaultvalue;
}
}
el.onfocus = function()
{
if (empty && !first)
{
el.value = '';
empty = false;
}
first = false;
}
var actFunc = function()
{
if (empty)
{
empty = false;
el.value = '';
}
}
el.onclick = actFunc;
el.onkeydown = actFunc;
}
</script>
