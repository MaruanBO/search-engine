	}
	
	GetOtherPages();
	
}
function GetCookieMobile(c_name)
{
	var i,x,y,ARRcookies=document.cookie.split(";");
	for (i=0;i<ARRcookies.length;i++)
	{
	  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
	  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
	  x=x.replace(/^\s+|\s+$/g,"");
	  if (x==c_name)
		{
		return unescape(y);
		}
	 }
}
function GetURLSKU(){
	var pattern = /p\/(.*)\.htm/;
	var re = new RegExp(pattern);
  	var m = re.exec(top.location.href);
	if(m){
		return m[1];
	}else{
		return false;	
	}
}
function GetURLCatID(){
	var pattern = /s\/(.*)\.htm/;
	var re = new RegExp(pattern);
  	var m = re.exec(top.location.href);
	if(m){
		return m[1];
	}else{
		return false;	
	}
}
function GetURLArticleID(){
	var pattern = /a\/(.*)\.htm/;
	var re = new RegExp(pattern);
  	var m = re.exec(top.location.href);
	if(m){
		return m[1];
	}else{
		return false;	
	}
}
function GetURLProductID(){
	var pattern = /p\/(.*)\.htm/;
	var re = new RegExp(pattern);
  	var m = re.exec(top.location.href);
	if(m){
		return m[1];
	}else{
		var pattern = /ProductCode=(.*)/;
		var re = new RegExp(pattern);
		var m = re.exec(top.location.href);
		if(m){
			return m[1];
		}else{
			return false;
		}
	}
}
function GetOtherPages(){
	if( !GetURLArticleID() && !GetURLCatID() ){
		
		var path = location.pathname;
		switch (path.toLowerCase()){
			case "/terms_privacy.asp" :
				top.location.href='/v/m/terms_privacy.asp'+query_string('');
			break;
			case "/returns.asp" :
				top.location.href='/v/m/returns.asp'+query_string('');
			break;
			case "/help_answer.asp?ID=21" :
				top.location.href='/v/m/shipping-info.asp'+query_string('');
			break;
			case "/" :
				top.location.href='/v/m/'+query_string('');
			break;
			case "/default.asp" :
				top.location.href='/v/m/'+query_string('');
			break;
			default :
				return false;
			break;	
		}
		return true;
	}	
}

function query_string(delimiter){
	if(delimiter=="&"){
		return location.search.replace("?","&");
	}else{
		return location.search;	
	}
}
