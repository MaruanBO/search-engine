	var MobileArticleID = "";
	if(GetURLArticleID()){
		MobileArticleID = GetURLArticleID();
		//alert(MobileArticleID);
		switch (MobileArticleID){
			case "1":
				top.location.href='/v/m/about-us.asp'+query_string('');
