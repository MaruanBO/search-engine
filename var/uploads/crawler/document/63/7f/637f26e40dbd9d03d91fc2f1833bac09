    <script>
      // celeb scroller control module
      var celebScroller = (function() {
        var slides = ["#doctors-img", "#hgn-img", "#hgtv-img"], /* need at least 2 IDs here */
            lastSlideIndex = slides.length - 1,
            currentSlide = 0,
            nextSlide = 1,
            previousSlide = lastSlideIndex,
            animating = false;

        var incrementSlide = function(slide) {
          if (slide == lastSlideIndex) {
            return 0;
          }
          else {
            return (slide + 1);
          }
        };

        var incrementAllSlides = function() {
          currentSlide = incrementSlide(currentSlide);
          previousSlide = incrementSlide(previousSlide);
          nextSlide = incrementSlide(nextSlide);
        };

        var decrementCurrentSlide = function(slide) {
          if (slide == 0) {
            return lastSlideIndex;
          }
          else {
            return (slide - 1);
          }
        };

        var decrementAllSlides = function() {
          currentSlide = decrementCurrentSlide(currentSlide);
          previousSlide = decrementCurrentSlide(previousSlide);
          nextSlide = decrementCurrentSlide(nextSlide);
        };

        return {
          next: function() {
            if (!animating) {
              animating = true;
              $(slides[nextSlide]).fadeIn(500);
              $(slides[currentSlide]).fadeOut(500, function() {
                incrementAllSlides();
                animating = false;
              });
            }
          },

          previous: function() {
            if (!animating) {
              animating = true;
              $(slides[previousSlide]).fadeIn(500);
              $(slides[currentSlide]).fadeOut(500, function() {
                decrementAllSlides();
                animating = false;
              });
            }
          },

          init: function() {
            $(".left-arrow-celeb").click(function() {
              celebScroller.previous();
            });

            $(".right-arrow-celeb").click(function() {
              celebScroller.next();
            });
          }
        };
      })();

      celebScroller.init();
    </script>
    <!-- END: CELEB SCROLLER -->


