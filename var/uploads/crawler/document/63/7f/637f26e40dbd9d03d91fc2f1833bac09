			if (started) {
				if(ref_url.indexOf("/v/m/")>0 && GetCookieMobile("SwitchDesktop")){
					history.pushState({}, null, "/");
				}
			}
		};
		setTimeout(function () {
			started = true;
		}, 1);
	}
