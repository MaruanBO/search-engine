<script type="text/javascript">
	// Utag Tealium
	var utag_data = {
			customer_id: "",
			customer_email: ""
		};
	
	// *************** Get User Data ************** //
	var UserData = "";
	if(get_user_session('CustomerID')){
		$.ajax({
		   url : '/api/v1/users/current',
		   dataType : "json",
		   contentType: "application/json; charset=utf-8",
		   headers: { "cache-control": "no-cache", "X-MicrosoftAjax" : "Delta=true" },
		   success : function(data) {
				UserData = data.data;
				
				// Add User data Values
				utag_data.customer_id=UserData.id;
				utag_data.uid=UserData.id;
				utag_data.customer_email=UserData.email;
		   }
		});
	}
	function get_user_session(c_name){
		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + c_name + "=");
		if (c_start == -1) {c_start = c_value.indexOf(c_name + "=");}
		if (c_start == -1){c_value = null;}else{
			c_start = c_value.indexOf("=", c_start) + 1;
			var c_end = c_value.indexOf(";", c_start);
			if (c_end == -1){c_end = c_value.length;}
			c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;
	}
</script>	
