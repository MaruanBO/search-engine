var xyz = 'http://www.dvdtalk.com/reviews/list.php?orderBy=Date&reviewType=All&searchText=';

function d(e, z) {
	var sb = document.getElementById('sb');
	var px = document.getElementById('px');
	if(e.id == '') sb.value = 'Search ' + e.innerHTML; else sb.value = e.id;
	var t = px.getElementsByTagName('A');
	for(var i = 0; i < t.length; i++) t[i].className = '';
	e.className = 'b';
	xyz = z;
}

function ds(f) {
	var si = document.getElementById('si');
	document.location = xyz + si.value;
}
</script>

