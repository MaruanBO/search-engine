// JavaScript Document - For FLIGHTS PAGES - First 3 are to Override -
$(window).load(function() {
  //This sets Phone number for AARP Page if Cookie is Set to 156
  if ($.cookie("leadsourceId") == 156){
      $('.leadsourcePhone').html('1.866.823.5793');
  }
  if ($.cookie("leadsourceId") == 402){
      $('.leadsourcePhone').html('1.800.862.1398');
  }

//PAGE SPECIFIC !!!!!!!!!!!!!!!!!!!!!
  //This is for all-flights-offer pages
  if (window.location.href.indexOf("all-flights-offer") > -1) {
    $('.leadsourcePhone').html('1.866.930.8529');
  }
  //This is for flights list page
  if (window.location.href.indexOf("flight-list") > -1) {
    $('.leadsourcePhone').html('1.866.930.8529');
  }
  //This is for Offer Pages 
  if (document.title.indexOf("Flights ") > -1) {
    $('.leadsourcePhone').html('1.866.930.8529');
  }     
  //This is for Groups page
  if (window.location.href.indexOf("groups") > -1) {
    $('.leadsourcePhone').html('1.866.280.4719');
  }

  //This is for Groups page
  if (window.location.href.indexOf("group-travel") > -1) {
    $('.leadsourcePhone').html('1.866.280.4719');
  }

  //This is for AARP  page
  if (window.location.href.indexOf("aarp") > -1) {
    $('.leadsourcePhone').html('1.866.823.5793');
  }
  //This is for Business Travel  page
  if (window.location.href.indexOf("business-travel") > -1) {
    $('.leadsourcePhone').html('1.866.332.7165');
  }

// LEADSOURCE OVERRIDES! 
  if ($.cookie("leadsourceId") == 615  && window.location.href.indexOf("group-travel") > -1) {
      $('.leadsourcePhone').html('1.877.987.8463');
