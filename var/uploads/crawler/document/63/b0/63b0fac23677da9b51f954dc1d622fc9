              (function() {
                var abCookieKey = "coamzab",
                        abCookieRegEx,
                        abCookieValue;
                abCookieRegEx = new RegExp("(?:(?:^|.*;\\s*)" + abCookieKey + "\\s*\\=\\s*([^;]*).*$)|^.*$");
                abCookieValue = document.cookie.replace(abCookieRegEx, "$1");
                if (abCookieValue === "B") {
                  document.getElementById("abe-only-logo").style.display = "none";
                  document.getElementById("abe-amzn-logo").style.display = "block";
                }
              }());
