<script>
    (function() {
        var abCookieKey = "coamzab",
                abThreshold = 90,
                tossResult,
                abCookieVal,
                toss,
                expireDate
                ;
        toss = function(min,max) {
            return Math.floor(Math.random()*(max-min+1)+min);
        }
        if (document.cookie.indexOf(abCookieKey) < 0) {
            tossResult = toss(1, 100);
            if (tossResult > abThreshold) {
                abCookieVal = "B";
            } else {
                abCookieVal = "A";
            }
            expireDate = new Date();
            expireDate.setTime(+ expireDate + (90 * 24 * 60 * 60 * 1000));
            document.cookie = abCookieKey + "=" + abCookieVal + ";expires=" + expireDate.toGMTString() + ";path=/";
        }
    }());
</script>


