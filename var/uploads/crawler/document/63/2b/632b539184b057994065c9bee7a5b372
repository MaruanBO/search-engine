<script src="//cdn.foodservicewarehouse.com/Content/bundles/bundle.js?v=Y_WiZNs6k32kTfy_K7VlUcA85PjJK3UTrDm-ibZINI41"></script>
    
    <script>

    $('.slider4').bxSlider({
        slideWidth: 130,
        minSlides: 2,
        maxSlides: 6,
        moveSlides: 3,
        slideMargin: 45
    });

    function advanceToSlide(index, wait, callback) {
        var $newImage = $($('.orbital img.orbital-item').get(index) || $('.orbital img.orbital-item').get(0));
        var $otherImages = $('.orbital img.orbital-item').not($newImage);
        $otherImages.css("z-index", "1");
        if (wait) {
            $($('.orbital-tab-container .orbital-tab-progress').get(index - 1)).animate({ width: '100%' }, wait || 0, function() {
                if (window.orbitalRunning === true) {
                    $newImage.css("z-index", "2").animate({ opacity: 1 }, 300, function () {
                        $otherImages.css("opacity", "0").removeClass("active");
                        $newImage.addClass("active");                    
                        if(typeof callback === 'function' && window.orbitalRunning === true) { callback(index + 1); }
                    });
                    var $tabs = $('.orbital-tab-container li');
                    $tabs.removeClass('active');
                    $tabs.find('.orbital-tab-title').attr('href', null);
                    var $newTab = $($tabs.get(index));
                    $newTab.addClass('active');
                    var $tabTitle = $newTab.find('.orbital-tab-title');
                    $tabTitle.attr('href', $tabTitle.attr('data-href'));
                }
            });
        } else {
            $newImage.css("z-index", "2").animate({ opacity: 1 }, 200, function () {
                $otherImages.css("opacity", "0").removeClass("active");
                $newImage.addClass("active");
                var $tabs = $('.orbital-tab-container li');
                $tabs.removeClass('active');
                $tabs.find('.orbital-tab-title').attr('href', null);
                var $newTab = $($tabs.get(index));
                $newTab.addClass('active');
                var $tabTitle = $newTab.find('.orbital-tab-title');
                $tabTitle.attr('href', $tabTitle.attr('data-href'));
            });
        }
    }

    var slideAutoAdvance = function(index) {
        if (index > 3) {
            advanceToSlide(0);
            $('.orbital-tab-container .orbital-tab-progress').css({ background: 'none'});
            if (localStorage) {
                localStorage.setItem('orbitalViewed', true);
            }
            return;
        }
        advanceToSlide(index, 2500, function(next) {
            slideAutoAdvance(next);
        });
    }

    if (localStorage && !localStorage.getItem('orbitalViewed')) {
        window.orbitalRunning = true;
        advanceToSlide(1, 3000, function(next) {
            slideAutoAdvance(next);
        });
    }

    $(".orbital-tab-container li").on('click', (function (e) {        
        if (window.orbitalRunning === true) {
            window.orbitalRunning = false;
            $('.orbital-tab-container .orbital-tab-progress').css({ background: 'none'});
            if (localStorage) {
                localStorage.setItem('orbitalViewed', true);
            }
        }
        var $el = $(e.currentTarget);
        if (!$el.hasClass("active")) {
            if (!$el.attr('data-tracked')) {
                var $title = $el.find('.orbital-tab-title');
                ga('ec:addPromo', {
                    'id': 'Home_Orbital',
                    'name': $title.text(),
                    'creative': $title.attr('data-href').split('?')[0],
                    'position': 'orbital_item_' + ($el.index()+1)
                });                
                ga('send', 'event');
                $el.attr('data-tracked', true);
            }
            advanceToSlide($el.index());
        }
    }));
    
    $(".orbital-slides a").click(function(e) {
        var $el = $(e.currentTarget);
        var title = $($(".orbital-tab-container li").get($el.index())).find(".orbital-tab-title").text();        
        ga('ec:addPromo', {
            'id': 'Home_Orbital',
            'name': title,
            'creative': $el.attr('href').split('?')[0],
            'position': 'orbital_item_' + ($el.index()+1)
        });
        ga('ec:setAction', 'promo_click');
        ga('send', 'event');
    });

    $(".orbital .nolink").click(function (e) {
        e.preventDefault();
        return false;
    });

</script>

