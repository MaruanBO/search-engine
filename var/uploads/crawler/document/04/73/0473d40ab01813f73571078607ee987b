<script type="text/javascript">
    var zip = "";

    $("#divAccuWeather").load("AccuWeather.aspx", { 'zip': zip });
    $("#ulLatestAdded > li:last").addClass("noline");
    $('input[name=section]').live('change', function () {
        var val = $('input[name=section]:checked').val();
        $(".categorylink").each(function (i) {
            var paramVal = getParam("setype", $(this).attr('href'));
            if (paramVal == "") {
                $(this).attr('href', $(this).attr('href') + '&setype=' + val);
            } else {
                $(this).attr('href', $(this).attr('href').replace('&setype=' + paramVal, '&setype=' + val));
            }
        });
    });
    function getParam(name, tmpURL) {
        var regexS = "[\\?&]"+name+"=([^&#]*)";
        var regex = new RegExp( regexS );
        var results = regex.exec( tmpURL );
        if( results == null )
            return "";
        else
            return results[1];
    }
    function SubmitQuickFind() {
        if ($("#txtQuickFindCode").val() != '') {
            location.href = "/list/searchredirect.aspx?fulltext=" + $("#txtQuickFindCode").val();
        }
    }

    function toggle_visibility(id) {
        var e = document.getElementById(id);
        if (e.style.display == 'block')
            e.style.display = 'none';
        else
            e.style.display = 'block';
    }

    function hitEnter(elem, e) {
        var nKeyCode = 0;

        if (e.keyCode) {
            nKeyCode = e.keyCode;
        } else if (e.which) {
            nKeyCode = e.which;
        }

        if (nKeyCode == 13) {
            if (elem.id == "txtQuickFindCode") {
                SubmitQuickFind();
                return false;
            }
        } else {
            return true;
        }
    }
</script>
