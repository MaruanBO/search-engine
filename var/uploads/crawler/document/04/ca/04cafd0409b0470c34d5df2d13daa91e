		function validate() {
			var store = document.form1.store.value;
			var ecookiecnt = "";
			var ecookiepart = "";
			if(readCookie("pizzahutmyentrycookie")!=null){
				ecookiecnt = readCookie("pizzahutmyentrycookie");
				ecookiepart = ecookiecnt.replace("\"","").split("|");
			}	
			
			
			if(store !=null) {
				
				store=zeroPad(store,6);
				if(locations.indexOf(store)==-1){
					return messages.error.certcode();
				}
				
				if(ecookiecnt!= ""){
					if(parseInt(ecookiepart[0])>=2){
						window.location = "message.do";
						return false;
					}
				}
				
				document.form1.store.value = store;
				return true;
			} else {
				return messages.error.certcode();
			}
		}
		
