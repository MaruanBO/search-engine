		function zeroPad(ostring,tlength){
			var y;
			var nval=ostring.length;
			var nstring=ostring;
			if(nval<tlength){
					for(y=nval;y<tlength;y++){
						nstring="0"+nstring;
					}
			}
			return nstring;
		}
