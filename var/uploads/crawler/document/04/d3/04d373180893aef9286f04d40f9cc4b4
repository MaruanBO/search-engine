		<script>
         var alerts = [];

            //check to see if the placeholder attribute is supported
            function placeholder() {
				var test = document.createElement('input');
				return ('placeholder' in test);
			}
	
			$(document).ready(function() {
					
				$('body').scrollTop(0);
				
				
				//Detect mobile devices and redirect fall landing to fall blog post
					
					
								
				
								
				
				/**
				 *  Incase placeholder is not supported
				 */
				if (!placeholder) {
					$('input').each(function(index, item) {
						$(this).val($(this).attr('placeholder')).addClass('placeholder');
					});
				}
	
				$('.placeholder').on('click focus', function() {
					if ($(this).val() == '' || $(this).val() == $(this).attr('placeholder')) {
						$(this).val('');
					}
				}).on('blur', function() {
					if ($(this).val() == '' || $(this).val() == $(this).attr('placeholder')) {
					$(this).val($(this).attr('placeholder'));
					}
				});
		
				/**************************************/
				
				$('.media-carousel').jcarousel();
				$('.featured-video-carousel').jcarousel({
					vertical: true
				});



				/* Affiliate Tab Hover Drop Down */
				var $affiliate_list = $('.affiliate-list');
				$('.wtw > ul > li').hoverIntent(function() {
					$affiliate_list.slideToggle('fast');
				});

		
				if ($(window).width() > 979) {
					/* Main Nav Flyouts */
					function mainNavFlyout(flyoutSection, flyoutTab) {
						var $flySec = $(flyoutSection);
						/*$flySec.hide();*/
						$(flyoutTab).hoverIntent(function() {
							$flySec.fadeIn();
						}, function() {
							$flySec.fadeOut();
						});
					}
			
					mainNavFlyout('ul.schedule-flyout', '.sched-nav-tab'); /* Schedule Flyout */
					mainNavFlyout('ul.programs-flyout', '.prog-nav-tab'); /* Programs Flyout */
					mainNavFlyout('ul.videos-flyout', '.vid-nav-tab'); /* Videos Flyout */
					
				}
				
				/* Makes header stick when scrolling */
				$("#header").sticky({
					topSpacing: 0,
					onStick: function(s) {
						if (s.stickyId == 'header') {
						}
					},
					onUnStick: function(s) {
						if (s.stickyId == 'header') {
						}
					}
				});
                
				$(window).resize(function() {
					//console.log($(window).width());
					//$(".is-sticky").sticky().update;
					if ($(window).width() < 785) {
					//                        var cnt = $('#undefined-sticky-wrapper').contents();
					//                        $('#undefined-sticky-wrapper').replaceWith(cnt);
					//$('#undefined-sticky-wrapper').remove();
					}
				});

				/* Expand sub nav */
				$('.menu-tab').click(function() {
					$('.sub-nav').slideToggle('fast');
				});
	
				/* Expand about dropdown */
				$('li.about-us').hoverIntent(function(){
					$('ul.about-us-dropdown').slideToggle('fast');
				});

				/* Disqus */
				var disqus_shortname = 'metvnetwork';
				(function() {
					var dsq = document.createElement('script');
					dsq.type = 'text/javascript';
					dsq.async = true;
					dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
					(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
				})();
			
				$('.scroll_to').click(function(e) {
					e.preventDefault();
					window.location.hash = '!' + $(this).data('target');
				});

				$(window).hashchange(function(e) {
					e.preventDefault();
					var new_hash = window.location.hash.replace('#!', '');
					var aTag = $("a[name='" + new_hash + "']");
				
					if (new_hash && aTag.length) {
						$('body,html,document').animate({
							scrollTop: aTag.offset().top - $('.header-inner-wrap').outerHeight()
						}, 'slow', function() {
				
						});
					}
				});
				$(window).hashchange();
				
				/* Hide form input values on focus*/ 
				$('input:text').each(function(){
					var txtval = $(this).val();
					$(this).focus(function(){
						if($(this).val() == txtval){
							$(this).val('')
						}
					});
					$(this).blur(function(){
						if($(this).val() == ""){
							$(this).val(txtval);
						}
					});
				});
				
				
				/* Makes iframe videos responsive */
				// Find all YouTube / Vimeo videos
				var $allVideos = $("iframe[src^='http://player.vimeo.com'], iframe[src^='//www.youtube.com']"),
				
					// The element that is fluid width
					$fluidEl = $("#content-col");
				
				// Figure out and save aspect ratio for each video
				$allVideos.each(function() {
				
				  $(this)
					.data('aspectRatio', this.height / this.width)
				
					// and remove the hard coded width/height
					.removeAttr('height')
					.removeAttr('width');
				
				});
				
				// When the window is resized
				$(window).resize(function() {
				
				  var newWidth = $fluidEl.width();
				
				  // Resize all videos according to their own aspect ratio
				  $allVideos.each(function() {
				
					var $el = $(this);
					$el
					  .width(newWidth)
					  .height(newWidth * $el.data('aspectRatio'));
				
				  });
				
				// Kick off one resize to fix all videos on page load
				}).resize();
				
				
				/* Makes embeded object / embed videos responsive */
				// Find all Limelight videos
				var $allVideos = $("object, embed"),
				
					// The element that is fluid width
					$fluidEl = $(".video-page-main-vid");
				
				// Figure out and save aspect ratio for each video
				$allVideos.each(function() {
				
				  $(this)
					.attr('data-aspectRatio', this.height / this.width)
				
					// and remove the hard coded width/height
					.removeAttr('height')
					.removeAttr('width');
				
				});
				
				// When the window is resized
				$(window).resize(function() {
				
				  var newWidth = $fluidEl.width();
				
				  // Resize all videos according to their own aspect ratio
				  $allVideos.each(function() {
					newWidth = $(this).parent().parent().width();	
					var $el = $(this);
					$el
						.width(newWidth)
						.height(newWidth * $el.attr('data-aspectRatio'));
				
				  });
				
				// Kick off one resize to fix all videos on page load
				}).resize();
				var hasStorage =false;
            if(typeof(Storage)!== void(0)){
               hasStorage = true;
            }
            
            for(i in alerts){
               msg = alerts[i];
               var html_alert = '<div class="system-alert" id="alert_'+msg.id+'"><span class="system-alert-close">x</span><p>'+msg.message+'</p><input type="hidden" value="'+msg.id+'"></div>';
            
               if(hasStorage && localStorage.getItem('alert_'+msg.id)==msg.id){
                  //$('#wrapper').prepend(html_alert);
               }else{
                  $('#wrapper').prepend(html_alert);
               }
            }
            
				$('.system-alert-close').click(function(e){
					
					if(e.target) targ = e.target;
               else if (e.srcElement) targ = e.srcElement;
               if(targ.nodeType==3) targ = targ.parentNode;
               
               alt_box = $(targ.parentNode);
               id = alt_box.children('input').val();
               
               alt_box.fadeOut();
               if(hasStorage){
                  localStorage.setItem('alert_'+id,id);
               }
				});
            
			});/* end $(document).ready(); */
		</script>
       	 
