<script>
    var processing = false;
    var homepage_item_next = 0;
    var homepage_item_limit = 6;
    var fia_offset;
    var stickyMenu = false;
    var timeout;
    var $homepage_tiles;

    $(window).load(function() {
        if ($(window).width() >= 1000) {
        /* Makes featured ads stick */
        $("#featured-internal-ads").sticky({
            topSpacing: 100,
            wrapperClassName: 'sticky-ads',
            onStick: function(s) {
                if (s.stickyId == 'featured-internal-ads') {
                    featured_ads('stick');
                }
            },
            onUnStick: function(s) {
                if (s.stickyId == 'featured-internal-ads') {
                    featured_ads('unstick');
                }
            }
        });

        $('.featured-internal-ads').css('z-index', '9997');
		}
		
		/* Initial load of first 6 tiles */
		homepage_item_loader();

        $(window).scroll(function() {
            if ($(window).scrollTop() >= ($(document).height() - $(window).height() - 100)) {
                processing = true;
                homepage_item_loader();
            }
        });
	
//        $.ajax({
//            url: '/homepage_cache.json',
//            data: null,
//            type: 'GET',
//            dataType: 'json',
//            success: function(response) {
//                console.log(response);
//            },
//            error: function(xhr, ajaxOptions, thrownError) {
//                console.log(xhr);
//            }
//        });
        
        /* Masonry - Creates floating grid layout */
        $homepage_tiles = $('.tile-wrap');
        $homepage_tiles.masonry({
            itemSelector: '.tile',
            columnWidth: 330
        });
		
        /*$homepage_tiles.imagesLoaded(
            $homepage_tiles.masonry('reload')
        );*/

    });

    function featured_ads(status) {
        var $featured_internal_ads = $('.featured-internal-ads');
        var $featured_int_ad_text = $('.feature-int-ad-text');
        if (status == 'stick') {
            $featured_internal_ads.addClass('fia-height');
            $featured_int_ad_text.show();
        } else {
            $featured_internal_ads.removeClass('fia-height');
            $featured_int_ad_text.hide();
        }
    }
    
    function time_diff(epoch){
        var now = new Date();       
        var then = new Date();
        if (epoch <10000000000) epoch *= 1000; //js uses milliseconds
        then.setTime(epoch);
        
        //When saved on server it used New York Time (4 hours UTC)
        //Now account for that time offset
        var offsetHours = 4; 
        then.setTime(then.getTime() - (offsetHours*60*60*1000)); 
        
        timediff = now - then;
        timediff = Math.floor(timediff/1000); //back to seconds
        hour_diff = Math.floor(timediff/3600); //to hours
        min_diff = Math.floor(timediff/60);
        if(hour_diff> 47){
            return Math.floor(hour_diff / 24) + ' Days Ago';
        }
      
        if(hour_diff <= 0)
        {
            if(min_diff == 1)
                return min_diff + ' Minute Ago';
            else
                return min_diff + ' Minutes Ago';
        }
        
        if(hour_diff == 1)
            return hour_diff + ' Hour ago';
        else
            return hour_diff + ' Hours ago';            
    }

    function homepage_item_loader() {
        $.get('homepage_cache.json?pickles=' + Math.random(), function(data) {
		if(!data) return false;

            data = data.splice(homepage_item_next, homepage_item_limit);
            homepage_item_next = (homepage_item_next + homepage_item_limit);
            $.each(data, function(index, item) {
		if($('.'+item.type+'-skeleton').length === 0){
                    console.log('error: no skeleton('+item.type+')');
                    console.log($(this));
                    return false;
                }
                
		console.log(item);
                $clone = $('.'+item.type+'-skeleton').clone();

                var html = $clone.html();
				
                var blogImg = '';

                if(item.thumbnail != ''){
                        blogImg = '<a href="/monitor/'+item.url+'"><img src="'+item.thumbnail+'" width="'+item.width+'" height="'+item.height+'"></a>';
                }
                if(item.type == "twitter")
                {
                    if(item.thumbnail == ''){
                        item.thumbnail = '<div class="twitter-header clearfix">' + 
                                            '<a href="https://twitter.com/metv"><img src="images/twitter_avatar.jpg" width="30" height="30" /></a>' + 
                                            '<span class="twitter-username"><a href="https://twitter.com/metv">metv</a></span>' + 
                                        '</div>';
                    }
                    else
                        item.thumbnail = '<a href="' + item.link + '"><img class="twitter_thumb" src="' + item.thumbnail + '" width="130"></a>';
                }
                //item.since uses the time at the time of cache, so we need to use time_diff instead
                var since_time = item.epoch ? time_diff(item.epoch) : item.since;
                //var regexp = new RegExp('#([^\\s]*)','g');
                var content = item.content;
                html = html/*.replace(/#title#/, (item.title != null) ? item.title : '')*/
				.replace(/#title#/,capitaliseFirstLetter(item.title))
				.replace(/#url#/g, item.url)
				.replace(/#link#/g, item.link)
                                .replace(/#imagesize#/,item.imagesize ) 
				.replace(/#content#/, content)
				.replace(/#date#/g, item.date)
				.replace(/#since#/, since_time)
				.replace(/#thumbnail#/, item.thumbnail)
				.replace(/#placeholder#/, blogImg)
				;
                                
                //console.log(html);
                $clone.html(html).show().removeClass(item.type+'-skeleton skeleton').addClass(item.type + '-tile');
                $('.tile-wrap').append($clone);       
            });
            
            //hack fixes overflowing twitter tiles
            //because imagesLoaded fires too early... 
            setTimeout(function(){         
                $('.tile-wrap').masonry('reload'); 
            }, 100)
            
            $homepage_tiles.imagesLoaded( 
                $homepage_tiles.masonry('reload')
            );

            processing = false;
        });
    }
	
	// Used to capitalize the first letter of the social media types
	function capitaliseFirstLetter(string){
		if(!string) return false;
    	return string.charAt(0).toUpperCase() + string.slice(1);
	}
	
	/* Responsive Slides */
	$(function() {
		$("#homepage-slides").responsiveSlides({
			timeout: 8000,
			pager: true,
			pause: true,
			maxwidth: '1000',
			namespace: 'homepage_slides'
		});
	});
</script>

