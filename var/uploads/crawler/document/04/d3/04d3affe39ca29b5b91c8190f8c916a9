    (function () {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true; 
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js'; 
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
    
    function GATrackEvent(category, action, label, value) {
        if (label != null && value != null) {
            _gaq.push(['_trackEvent', category, action, label, value]);
        }
        else if(label != null && value == null) {
            _gaq.push(['_trackEvent', category, action, label]);
        }
        else if(label == null && value != null) {
            _gaq.push(['_trackEvent', category, action, '', value]);
        }
        else {
            _gaq.push(['_trackEvent', category, action]);
        }
    }

    $(document).ready(function () {
        $('.ga-track').each(function () {
            var link = $(this);
            var cat;
            var act;
            var lbl;
            var val;

            if (link.attr('ga-cat') != null) {
                cat = link.attr('ga-cat');
                act = link.attr('ga-act');
                lbl = link.attr('ga-lbl');
                val = link.attr('ga-val');
            }
            else {
                cat = link.children('.ga-cat').first().text();
                act = link.children('.ga-act').first().text();
                lbl = link.children('.ga-lbl').first().text();
                val = link.children('.ga-val').first().text();
                if (lbl == "") {
                    lbl = undefined;
                }
                if (val == "" || isNaN(val)) {
                    val = undefined;
                }
                else {
                    val = Number(val);
                }
            }
            link.click(function () {
                GATrackEvent(cat, act, lbl, val);
            });
        });
    });
</script>
