<script>
    var bSetIFrameSrcFooter = false;
    function showEmailSignupInFooterIfVisible() {
        var el = $("#frameFooter");
        
        //Make sure the element is present
        if (el.length) {
            if (el.visible(true)) {
                if (bSetIFrameSrcFooter === false) {
                    el.attr("src", "https://www.schwans.com/secure/account/signUpForEmail.aspx?ssefrmnossl=Y");
                    bSetIFrameSrcFooter = true;
                }
            }
        }
    }

    $(document).ready(function () {
        showEmailSignupInFooterIfVisible();

        if (bSetIFrameSrcFooter === false) {
            $(window).scroll(function (event) {
                showEmailSignupInFooterIfVisible();
            });
        }
    });
</script>

