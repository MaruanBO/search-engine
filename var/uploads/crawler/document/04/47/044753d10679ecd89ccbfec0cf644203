<script type="text/javascript">
    $(function () {

        //Menu Styles and Fallback
        $(".menuDrop ul.child").removeClass("child");
        $("ul.myAcct.menuDrop li:last-child ul").addClass("right");
        $("ul.myCart.menuDrop li:last-child ul").addClass("right");
        $("ul.mainNav.menuDrop li:last-child ul").addClass("right");

        //My Account Dropdown
        $(".myAcct.menuDrop li").has("ul").hover(function () {
            $(this, "ul").addClass("current").children("ul.myAcctOptions").stop(true, true).slideDown({
                duration: 200,
                easing: "easeInExpo"
            });
        }, function () {
            $(this, "ul").removeClass("current").children("ul.myAcctOptions").hide();
        });

        //My Cart Dropdown
        $(".myCart.menuDrop li").has("ul").hover(function () {
            $(this, "ul").addClass("current").children("ul.cartItems").stop(true, true).slideDown({
                duration: 200,
                easing: "easeInExpo"
            });
        }, function () {
            $(this, "ul").removeClass("current").children("ul.cartItems").stop(true, true).hide();
        });

        //Main Nav Dropdown
        $(".mainNav.menuDrop li").hover(function () {
            $(this, "ul").addClass("current").children("ul").stop(true, true).slideDown({
                duration: 200,
                easing: "easeInExpo"
            });
        }, function () {
            $(this, "ul").removeClass("current").children("ul").stop(true, true).hide();
        });

        //Sub Nav Dropdown
        $(".subNav.menuDrop li").hover(function () {
            $(this, "ul").addClass("current").children("ul").stop(true, true).slideDown({
                duration: 200,
                easing: "easeInExpo"
            });
        }, function () {
            $(this, "ul").removeClass("current").children("ul").stop(true, true).hide();
        });

        //Input Watermarks
        $(".searchInput").Watermark("Product Name or Item #");

        //search bar selected option
        $("div.select").text($("#searchBar select.styled option:selected").text());

        $("#searchBar select.styled").change(function () {
            $("div.select").text($("#searchBar select.styled option:selected").text())
        });

        //add italic class to LiveSmart links
        $('div.headerBox a:contains("LiveSmart")').each(function () {
            $(this).addClass("italic");
        });
    });
</script>
