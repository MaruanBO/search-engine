		<div class="content-asset ca-homepage-header-getnavtest">
			<style>
	@import url("http://demandware.edgesuite.net/aaeo_prd/on/demandware.static/Sites-MichaelsUS-Site/Sites-MichaelsUS-Library/default/v1424335496893/css/ca-homepage-header-getnavtest-open.css");
</style>
<script type="text/javascript" src="http://demandware.edgesuite.net/aaeo_prd/on/demandware.static/Sites-MichaelsUS-Site/Sites-MichaelsUS-Library/default/v1424335496893/js/jquery.hoverIntent.minified.js"></script>

<script>
	$(document).ready(function(){
		$('.tn-main a.jsswitch').on('click', function(){
			// remove active state form both tn-main and tn-sub-main-bar
			$('.tn-main a, .tn-sub-main-bar').removeClass('active');
			// update both tn-main and tn-submain-bar active state
			$(this).addClass('active');
			var target = $(this).attr('data-target');
			if (target) {
				$(target).addClass('active');
			}
			return false;
		});
		$("#navigation2 .navigation2-top-bg nav.navigationtest .tn-sub-main-bar .tn-sub-main li.dropdown").hoverIntent({
			over: makeTall,
			out: makeShort, 
			timeout: 200
		});
	});
	function makeTall(){
		$(this).find('.tn-sub-main-items').fadeIn(200).parent().css({
			"background": "url(http://demandware.edgesuite.net/aaeo_prd/on/demandware.static/Sites-MichaelsUS-Site/Sites-MichaelsUS-Library/default/v1424335496893/images/menubg-level3.png) repeat top left #fff",
			"height": 40
		}).find('a:first-child').css({
			"color":"#333"
		});
	}
    function makeShort(){
    	$(this).find('.tn-sub-main-items').fadeOut(200).parent().css({
			"background": "transparent",
			"height": 33
		}).find('a:first-child').css({
			"color":"#fff"
		});
    }
</script>

