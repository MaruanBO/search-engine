<script>
// Check for subdomain
function subDomain(url) {

	// IF THERE, REMOVE WHITE SPACE FROM BOTH ENDS
	url = url.replace(new RegExp(/^\s+/),""); // START
	url = url.replace(new RegExp(/\s+$/),""); // END
	 
	// IF FOUND, CONVERT BACK SLASHES TO FORWARD SLASHES
	url = url.replace(new RegExp(/\\/g),"/");
	 
	// IF THERE, REMOVES 'http://', 'https://' or 'ftp://' FROM THE START
	url = url.replace(new RegExp(/^http\:\/\/|^https\:\/\/|^ftp\:\/\//i),"");
	 
	// IF THERE, REMOVES 'www.' FROM THE START OF THE STRING
	url = url.replace(new RegExp(/^www\./i),"");
	 
	// REMOVE COMPLETE STRING FROM FIRST FORWARD SLASH ON
	url = url.replace(new RegExp(/\/(.*)/),"");
	 
	// REMOVES '.??.??' OR '.???.??' FROM END - e.g. '.CO.UK', '.COM.AU'
	if (url.match(new RegExp(/\.[a-z]{2,3}\.[a-z]{2}$/i))) {
	      url = url.replace(new RegExp(/\.[a-z]{2,3}\.[a-z]{2}$/i),"");
	 
	// REMOVES '.??' or '.???' or '.????' FROM END - e.g. '.US', '.COM', '.INFO'
	} else if (url.match(new RegExp(/\.[a-z]{2,4}$/i))) {
	      url = url.replace(new RegExp(/\.[a-z]{2,4}$/i),"");
	}
	 
	// CHECK TO SEE IF THERE IS A DOT '.' LEFT IN THE STRING
	var subDomain = (url.match(new RegExp(/\./g))) ? true : false;

	// Results into object
	var parts = url.split('.');
	var status = {subDomainStatus:subDomain,urlString:url,urlParts:parts};
	 
	return(status);
 
}

	$(document).ready(function(){
		// Adding additional copy to additional material section on project detail pages **Temp Solution**
		$('.additional-material-title').html("You’ll Also Need <span class='small'><br>* available in store at Michaels</span>").css({'text-align':'center','line-height':'1.1em', 'width':'50%', 'margin':'6.5% auto 5%'});
		$('.additional-material-title span.small').css({'font-size':'.65em','line-height':'1em'});

		// Variables for main navigation dropdown and mobile drop down
		var windowWidth = window.screen.width < window.outerWidth ?
			window.screen.width : window.outerWidth;
		var mobile = windowWidth < 768;
		
		//Variables for 3rd party mobile search bar
		var locHost = location.hostname,
			locStatus = subDomain(locHost);

		if (mobile != true) {
			// Main navigation dropdown function non mobile device
			$('.custom-dropdown').hover(
				function(){
				$(this).find('div.level-2').css({'display':'block'});
			}, function(){
				$(this).find('div.level-2').css({'display':'none'});
			});
		} else {
			// Main navigation drop down function on mobile devices
			$('.custom-dropdown').on('click',function(){
				$(this).find('div.level-2').css({'display':'block'}).animate({'left':0},500);
			});
			$('.level-2-link').on('touchstart click',function(){
				var link = $(this).attr('href');
				console.log(link)
				window.location.assign(link);
			});

			// Reseting the search bar for 3rd parties
			console.log(locStatus);
			if (locStatus.subDomainStatus == true && locStatus.urlParts[0] === 'weeklyad') {
				console.log('BAM');
				$('#free-shipping-50').append('<style>#header, .pt-checkout.checkout-process header {height: 85px;}#main {margin-top: 85px;}.promo-add {display: block;}.textbox-search {display: none;height: 72px;}#mobile-searchgo {display: block;}.top-nav-header {right: 52px;}</style>');
			}
		}
	});	
</script>
	
 
