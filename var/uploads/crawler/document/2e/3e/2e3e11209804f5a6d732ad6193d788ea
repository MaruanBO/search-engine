<script type="text/javascript">
    function sv_validate_vote(){
        var oElm = document.getElementById('ctl00_Content_SV_plVoteQuestions')
        var oInputs = oElm.getElementsByTagName('input');
        var iInputs = oInputs.length;
        var x = 0;
        
        while(x < iInputs){
            if(oInputs[x].checked){
                return true;
            }
            x++;
        }
        return false;
    }
</script>

