<script type="text/javascript">
     function btnSearch_press(event){
        var oEvent;
        var iKey;
      
        oEvent = window.event ? window.event : event;
        iKey = oEvent.srcElement ? oEvent.keyCode : oEvent.which;
        
        if(iKey == 13){
            if(txtZip_validate()) {
                var oBtn = document.getElementById('ctl00_Content_GBZS_btnSearch');
                oBtn.click();
            }
            return false;
        }else{            
            return true;
        }
    }
    
    function btnSearch_click(event){
        if(txtZip_validate()) {
            return true;
        }else{
            return false;
        }
    }

    function txtZip_validate(){        
        var oElm = document.getElementById('ctl00_Content_GBZS_txtZip');        
        var sWM = '';     
      
        if(oElm.getAttribute('wm') != null){            
            sWM = oElm.getAttribute('wm');
            oElm.value = oElm.value.replace(sWM, '');
        }
        
        if(oElm.value == "" || oElm.value == sWM){
            oElm.className = 'ac_warn';
            return false;
        }
        
        oElm.className = 'ac_text';
        pageTracker._trackEvent('GasBuddy.com', 'Home Page Search', oElm.value);
        return true;
    }
    
</script>

<div id="zs" onkeydown="return btnSearch_press(event);">
