<script language="javascript" type="text/javascript">    
    var oHB_AC = new cls_ac();
        
    oHB_AC.getdata = function(){
        GasBuddy_ASPX.GBAjax['ac_ga'](this.getLastSearch(), '', '', this.ondata.bind(this));
    }
    oHB_AC.displayItem = function(oRow){
        return oRow.a + ", " + oRow.s;
    }    
    oHB_AC.dosort = function(a,b){
        if (b[1] == 1 && b[1] - a[1] == 0) {
            return b[2] - a[2];
        } else {
            return b[1] - a[1];
        }        
    }
    oHB_AC.onEnter = function(){        
        btnSearch_press(event);
    }
    
    oHB_AC.initialize('ctl00_Content_GBZS_txtZip', 'p');  
    
    function gbz_t_c(oElm){
        var aItems = [];
        var oSearchElm = document.getElementById('ctl00_Content_GBZS_txtZip');
        
        if(oSearchElm != null && oSearchElm != 'undefined'){
            aItems.push(oSearchElm.value);            
        }
        
        aItems.push(oElm.innerHTML);        
        pageTracker._trackEvent('GasBuddy.com', 'Home Page Suggestion', aItems.join('|'));
    }
    
    function gbz_t(){
        var oElm = document.getElementById('zs');
        
        if(oElm != null && oElm != 'undefined'){
            var oElms = oElm.getElementsByTagName('a');
            var iLength = oElms.length;
            var iX = 0;
            
            for(iX = 0; iX < iLength; iX++){
                oElms[iX].onclick = function(){
                    gbz_t_c(this);
                }
            }
        }
        
        oElm = document.getElementById('ctl00_Content_GBZS_hlLocation');
        
        if(oElm != null && oElm != 'undefined'){
            oElm.onclick = function(){
                var s = this.innerHTML;
                s = s.replace('Get ', '');
                s = s.replace(' Gas Prices', '');                
                
                pageTracker._trackEvent('GasBuddy.com', 'Local IP Suggestion', s);
            }
        }
    }       
    gbz_t();   
</script>
	
		    
