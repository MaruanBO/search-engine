				lightbox_content += '</ol>';
				lightbox_content += '<button type="submit" onclick="_gaq.push([\'_trackEvent\', \'YEG\', \'Click\', \'autismspeaks.org 10 Years of Progress Lightbox\']);">Donate Now</button>';
				lightbox_content += '</fieldset>';
				lightbox_content += '</form>';
				// More Complicated HTML End
				/*** Linked Image or Other HTML End ***/

				/*** Do Not Change Start ***/
				$('body').append('<div id="popup"><div class="lightbox">' + lightbox_content + '<div class="buttons clearfix"><button type="button" class="close">skip this [ X ]</button></div></div><div class="overlay"></div></div>');

				$('#popup').find('.close').click(function() {
					$('#popup').fadeTo(200, 0, function(){
						$('#popup').remove();
					});
					return false;
				});
				/*** Do Not Change End ***/

				/*** Specific Scripts Start ***/

				$('.donate-lightbox').submit(function() {
					$('.donate-lightbox').attr('action', function(i, currval) {
						var amt = $('.donate-lightbox').find('input[name="amount"]:checked').val() || '';

						return currval + '&amt=' + amt;
					});
				});
				/*** Specific Scripts End ***/

			}
		}
	}
