<script>
	function lightBox() {
		var	today = new Date(),
				start_date = new Date('02/19/2015'),
				end_date = new Date('03/06/2015'),
				cookie_will_change = true,
				change_cookie_date = new Date('02/25/2015'),
				change_cookie = today.setHours(0,0,0,0) >= change_cookie_date.setHours(0,0,0,0),
				lightbox_content = '';

		if (today.setHours(0,0,0,0) >= start_date.setHours(0,0,0,0) && today.setHours(0,0,0,0) < end_date.setHours(0,0,0,0)) {
			if ($.cookie('ty_a') != '1' || (cookie_will_change && change_cookie && $.cookie('ty_b') != '1')) {
				$.cookie('ty_a', '1', { expires: 3, path: '/' });
				if (cookie_will_change && change_cookie) {
					$.cookie('ty_b', '1', { expires: 1, path: '/' })
				};

				/*** Linked Image or Other HTML Start ***/
				// One Big Image Start
				/*
				lightbox_content += '<a href="https://secure.autismspeaks.org/site/c.8hKPL7NMLpJ4G/b.8861233/k.963A/Giving_Tuesday.htm?utm_source=autismspeaks.org&utm_medium=Lightbox&utm_campaign=14YEGivingTuesday&msource=14GivTueL" onclick="_gaq.push([\'_trackEvent\', \'GivingTuesday\', \'Click\', \'autismspeaks.org GivingTuesday 2014 Lightbox\']);">';
				lightbox_content += '<img src="http://www.autismspeaks.org/sites/default/files/images/lightboxes/givingtuesday_feature_685x385-compressor.jpg" width="685" height="385" style="height: auto; max-width: 98%;">';
				lightbox_content += '</a>';
				*/
				// One Big Image End

				// More Complicated HTML Start
				lightbox_content += '<form class="donate-lightbox" action="https://secure.autismspeaks.org/site/c.8hKPL7NMLpJ4G/b.8844069/k.C240/Donate.htm?utm_source=autismspeaks.org&utm_medium=Lightbox&utm_campaign=10YearsProgress&msource=10YLightBox" method="post">';
				lightbox_content += '<img src="/sites/default/files/images/lightboxes/10-year-donation-page-logo.png" class="intro">';
				lightbox_content += '<p class="intro">Our first 10 years of progress are just the beginning. Donate today to help us bring a lifetime of hope to those living with autism.</p>';
				lightbox_content += '<fieldset>';
				lightbox_content += '<legend class="visuallyhidden">Select a donation amount</legend>';
				lightbox_content += '<ol class="amounts">';
				lightbox_content += '<li><label for="amount1">$25</label><input type="radio" name="amount" id="amount1" value="25"></li>';
				lightbox_content += '<li><label for="amount2">$50</label><input type="radio" name="amount" id="amount2" value="50"></li>';
