<script>
$(function() {  

	// slider options
	var o = {  
		animateSpeed: 1300,
		animateDelay: 70, // amount of time to incrimentally add to the animateSpeed of each animation
		sliderTimeout: 4000, // time before proceeding to next slidepx
		sliderSpeed: 1300, // speed of slide transition
		sliderFx: 'fade', 
		sliderEasing: 'easeInOutExpo',
		bgImgDelay: 800,
		bgImgSpeed: 1000,
		log: false
	}

	// set vars
	var $sliderParent = $('.bigpic_slider');
	var $slider = $('ul', $sliderParent);
	var $next = $('.bigpic_right');
	var $prev = $('.bigpic_left');
	var i;


	// flying animation 
	function fly(el, origin, reversed, callback) {  

		// prep variables
		var $el = $(el);
		var position = $el.css('position');
		var offset, flyposition, speed, delay, easing;

		// set start and finishing styles. reset left and right
		var startingStyles = {  'visibility':'visible'};
		var finishingStyles = {  };

		// position:absolute elements cannot currently fly
		if (position!=='absolute') {  
			// console.log(position);
			startingStyles['position'] = 'relative';

			// calculate direction of animation based on origin
			if (reversed && origin==='left') {  
				flyposition = 'right';
			} else if (reversed && origin==='right') {  
				flyposition = 'left';
			} else {  
				flyposition = origin;
			}

			// calculate offset between element and edge of screen for animation
			var pre = reversed ? '' : '-';
			offset = pre+$(window).width()+'px';
			// if (origin==='left') {  
			// 	offset = pre+( $(window).width() - $el.offset().left)+'px';
			// } else {  
			// 	offset = pre+( $(window).width() )+'px';
			// };

			// set starting position and finishing position variables
			startingStyles['left'] = 'auto';
			startingStyles['right'] = 'auto';
			startingStyles[flyposition] = reversed ? 0 : offset;
			finishingStyles[flyposition] = reversed ? offset : 0;

		};

		// set starting and finishing opacity
		startingStyles['opacity'] = reversed ? 1 : 0;
		finishingStyles['opacity'] = reversed ? 0 : 1;

		// set animation behaviour
		if (reversed) {  
			easing = 'easeInOutExpo';
			speed = o.animateSpeed+(o.animateDelay*i);
			delay = 0;
		} else {  
			easing = i==0 ? 'easeInOutExpo' : 'easeInOutBack';
			speed = o.animateSpeed;
			delay = o.animateDelay*i;
		};

		// ANIMATE!
		$el.css(startingStyles).delay(delay).animate(finishingStyles, speed, easing, function() {  
			if (callback!==undefined) callback();
			// console.log('animation done', direction, reversed);
		});
	}

	// animate all .bigpic_animate elements in a slide
	function animate (el, inout, callback) {  
		i = 0;
		var $els = $('.bigpic_animate', el);
		var count = $els.length;
		var direction = $(el).hasClass('bigpic_layout_alt') ? 'right' : 'left';
		$els.each(function(event) {  
			if (count==(i+1) && callback !== undefined) {  
				var call = callback;
			};
			if (inout == 'in') {  
				fly(this, direction, false, call);
			} else {  
				fly(this, direction, true, call);
			}
			i++;
		});
		if (inout=='in') {  
			$('.bigpic_background_img', el).delay(o.bgImgDelay).fadeIn(o.bgImgSpeed);
			// console.log('now');
		};
	}

	// reset .bigpic_animate elements
	function resetSlide (slide) {  
		var $slide = $(slide);
		$slide.find('.bigpic_animate').each(function(index) {  
			if ($(this).css('position')!=='absolute') {  
				$(this).removeAttr('style').css('visibility', 'hidden');
			};
		});
		$slide.find('.bigpic_background_img').hide();
	}

	// easily play/pause slider
	var playSlider = '';
	function slider(action) {  
		if (action === 'play' && playSlider === '' && modalOpen !== true) {  
			playSlider = setTimeout(function(){  $next.trigger('click')}, o.sliderTimeout);
			// console.log('play triggered');
		} else if (action === 'pause') {  
			clearTimeout(playSlider);
			playSlider = '';
			// console.log('pause triggered');
		} else {  
			return false;
		}
	}

	$slider
		// setup slider animations on initialize
		.on('cycle-initialized', function(event) {  
			var $first_slide = $('li:first', this);
			resetSlide($first_slide);
			animate($first_slide, 'in', function(){  
								if (sliderPaused !== true) slider('play');
							});
		})
		// initialize cycle
		.cycle({  
			slides: '> li',
			fx: o.sliderFx,
			paused: true,
			speed: o.sliderSpeed,
			easing: o.sliderEasing,
			log: false

		})
		// reset animations for next slide
		.on('cycle-before', function(event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {  
			resetSlide(incomingSlideEl);
			animate(incomingSlideEl, 'in', function(){  
				if (!sliderPaused) slider('play');
			});
		})
		// initiate animations for next slide
		.on('cycle-after', function(event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {  

			// setTimeout(function() {  $('.bigpic_background_img').fadeIn(500)}, 1000);
		});

		// set next/prev triggers
		$next.on('click', function(event) {  
			$('.bigpic_animate').stop(true, true);
			animate('.cycle-slide-active', 'out', function(){  
				slider('pause');
			});
			$slider.cycle('next');
		});
		$prev.on('click', function(event) {  
			$('.bigpic_animate').stop(true, true);
			animate('.cycle-slide-active', 'out', function(){  
				slider('pause');
			});
			$slider.cycle('prev');
		});


		// play/pause slider on hover
		var sliderPaused = false;
		$('.action_button', $sliderParent).hover(function() {  
			slider('pause');
			sliderPaused = true;
			// console.log(sliderPaused);
		}, function() {  
			slider('play');
			sliderPaused = false;
			// console.log(sliderPaused);
		});

		// pause slider when modal is active
		var modalOpen = false;
		$('.modal_link').on('click', function(event) {  
			slider('pause');
			modalOpen = true;
			//console.log('slider paused');
		});
		$(document).on('click', '.new_modal_overlay, .new_modal_close', function(event) {  
			modalOpen = false;
			slider('play');
			// console.log('slider play');
		});


		// before: function(currSlideElement, nextSlideElement, options, forwardFlag) {  
		// 	resetSlide(nextSlideElement);
		// },
		// after: function(currSlideElement, nextSlideElement, options, forwardFlag) {  
		// 	animateSlide(nextSlideElement);
		// }
});
</script>

