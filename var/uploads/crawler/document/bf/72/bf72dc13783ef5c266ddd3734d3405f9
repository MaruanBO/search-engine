		<script type="text/javascript">

			$(document).ready(function() {

				// start menu tabs
				changeTabs();

				$('#v_vrForm').each(function(){this.reset()});

				$('#v_vrForm').formUtils({ initialLocation: 'e.g., New York, 11020' });
				$('#v_reForm').formUtils({ initialLocation: 'e.g., New York, 11020'});
				$('#v_rentalsForm').formUtils({ initialLocation: 'e.g., New York, 11020'});

				$('#what').formInitial({ initialValue: 'e.g., red honda accord' });
				$('#where').formInitial({ initialValue: 'e.g., New York, 11020' });

				$('#citystateorzipcode').formInitial({ initialValue: 'city state OR zip code' });

				$('#price_from, #rent_from').formInitial({ initialValue: '$min' });
				$('#price_to, #rent_to').formInitial({ initialValue: '$max' });

				$('#available_from').formInitial({ initialValue: 'in' });
				$('#available_to').formInitial({ initialValue: 'out' });


				// image fadeOut/fadeIn when tab is changed
				$(".tabNavigation li a").click(function(){

					// image name based on href attribute
					$id = $(this).attr("href");
					var imageId = $id + "_bg";

					// check if id exist
					if($(imageId).length !== 0){
						// prevent multiple image transition on current tab
						if(!$(imageId).hasClass("activeBg")){

							if(jQuery.browser.msie){
								$("#promoPic").find('.activeBg').removeClass('activeBg').hide();
								$(imageId).addClass('activeBg').show();
							}else{
								$("#promoPic").find('.activeBg').removeClass('activeBg').fadeOut('slow');
								$(imageId).addClass('activeBg').fadeIn('slow');
							}
						}
					}else{
						if(!$(imageId).hasClass("activeBg") && $("#real_estate_bg").css('display') == "none"){

							if(jQuery.browser.msie){
								$("#promoPic").find('.activeBg').removeClass('activeBg').hide();
								$("#real_estate_bg").addClass('activeBg').show();
							}else{
								$("#promoPic").find('.activeBg').removeClass('activeBg').fadeOut('slow');
								$("#real_estate_bg").addClass('activeBg').fadeIn('slow');
							}
						}
					}
				});

				// on submit - strip all commas and remove not needed fields like submit:
				$('#frm_re').submit(function(e) {
					var locationField = $(this).find('input[name=location]');
					var location = $(locationField).val().replace(/\,/g, '').toLowerCase();
					$(this).attr('action', $(this).attr('action') + '/location-' + location);
					$(locationField).remove();
					$(this).find('input[name=submit]').remove();
				});
			});

			function changeTabs() {
				var tabContainers = $('.nav_tab_cont');
				tabContainers.hide().filter(':first').show();

				$('div.tabs ul.tabNavigation a').click(function () {
					tabContainers.hide();
					tabContainers.filter(this.hash).show();
					$('div.tabs ul.tabNavigation a').removeClass('selected');
					$(this).addClass('selected');
					return false;
				}).filter(':first').click();
			}
		</script>

