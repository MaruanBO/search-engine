<script type="text/JavaScript">
	addEvent(window, 'load', initCorners);
	function initCorners() {
	var settingA = {
		tl: { radius: 0 },
		tr: { radius: 0 },
		bl: { radius: 10 },
		br: { radius: 10 },
		antiAlias: true
	}
	
	var settingB = {
		tl: { radius: 10 },
		tr: { radius: 10 },
		bl: { radius: 10 },
		br: { radius: 10 },
		antiAlias: true
	}
	
	var settingC = {
		tl: { radius: 10 },
		tr: { radius: 0 },
		bl: { radius: 10 },
		br: { radius: 0 },
		antiAlias: true
	}
	
	
	curvyCorners(settingA, ".login");
	curvyCorners(settingB, ".headline_news");
	curvyCorners(settingC, ".headline_news-header");
}
</script>

