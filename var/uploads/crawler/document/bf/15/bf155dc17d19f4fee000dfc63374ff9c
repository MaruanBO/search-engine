		<!-- JS FOR HEAD SUBMENU -->
		<script type="text/javascript">
			var submenuTimer;
			
			$(document).ready(function(){
				$('#submenu-lifestyle-button,#submenu-lifestyle').hover(function(){
					submenu_show_item('lifestyle');
					$('#submenu-lifestyle-button').addClass('submenu-main-hover');
				},
				function(){
					submenu_hide_item('lifestyle');
				});

				$('#submenu-travel-button,#submenu-travel').hover(function(){
					submenu_show_item('travel');
					$('#submenu-travel-button').addClass('submenu-main-hover');
				},
				function(){
					submenu_hide_item('travel');
				});

				$('#submenu-fashion-button,#submenu-fashion').hover(function(){
					submenu_show_item('fashion');
					$('#submenu-fashion-button').addClass('submenu-main-hover');
					
				},
				function(){
					submenu_hide_item('fashion');
					
				});

				$('#submenu-bestofluxe-button,#submenu-bestofluxe').hover(function(){
					submenu_show_item('bestofluxe');
					$('#submenu-bestofluxe-button').addClass('submenu-main-hover');
					
				},
				function(){
					submenu_hide_item('bestofluxe');
					
				});
				
			});
			
			function submenu_show_item(item){
				clearTimeout(submenuTimer);
				submenu_hideall();
				$('#submenu-'+item).css('display','block');
				
			}
			
			function submenu_hide_item(item){
				submenuTimer =setTimeout(function(){submenu_hideall();},300);
			}
			
			function submenu_hideall(){
				$('#submenu-travel').css('display','none');
				$('#submenu-lifestyle').css('display','none');
				$('#submenu-fashion').css('display','none');
				$('#submenu-bestofluxe').css('display','none');
				$('#submenu-fashion-button').removeClass('submenu-main-hover');
				$('#submenu-travel-button').removeClass('submenu-main-hover');
				$('#submenu-lifestyle-button').removeClass('submenu-main-hover');
				$('#submenu-bestofluxe-button').removeClass('submenu-main-hover');
				$('#allsections-main').css('display','none');
			}
								
		</script>
		
		
		<!-- JS FOR PLACEHOLDER SUPPORT -->
		<script type="text/javascript">
			
			// This adds 'placeholder' to the items listed in the jQuery .support object. 
		jQuery(function() {
		   jQuery.support.placeholder = false;
		   test = document.createElement('input');
		   if('placeholder' in test) jQuery.support.placeholder = true;
		});
		// This adds placeholder support to browsers that wouldn't otherwise support it. 
		$(function() {
		   if(!$.support.placeholder) { 
			  var active = document.activeElement;
			  $(':text').focus(function () {
				 if ($(this).attr('placeholder') != '' && $(this).val() == $(this).attr('placeholder')) {
					$(this).val('').removeClass('hasPlaceholder');
				 }
			  }).blur(function () {
				 if ($(this).attr('placeholder') != '' && ($(this).val() == '' || $(this).val() == $(this).attr('placeholder'))) {
					$(this).val($(this).attr('placeholder')).addClass('hasPlaceholder');
				 }
			  });
			  $(':text').blur();
			  $(active).focus();
			  $('form:eq(0)').submit(function () {
				 $(':text.hasPlaceholder').val('');
			  });
		   }
		});
		
		  
		</script>
		
