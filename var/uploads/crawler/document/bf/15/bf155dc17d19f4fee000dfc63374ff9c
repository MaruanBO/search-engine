<link rel="stylesheet" href="/includes/js/fancybox/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
<script type="text/javascript" src="/includes/js/fancybox/jquery.fancybox.pack.js?v=2.1.5"></script>
<script type="text/javascript">
	$(document).ready(function() {
		$("a.fancybox").fancybox();
	});
</script>

<script>
	
	$(document).ready(function(){
		$('.bottombar .rightbox img').click(function(){
			$('.small').removeClass('small');
			$('.small input'). css({
				display:"block"
			});
			$('.bottombar input[type=text]').select();
			$(this).hide();
			$('.bottombar form').show();
		});
		
		$('#menu').click(function(){
			$('.bottombar .leftbox ul').toggle();
		});
		var menu = $('.bottombar .leftbox ul');
		$(window).resize(function(){  
		    var w = $(window).width();  
		    if(w > 500 && menu.is(':hidden')) {  
		        menu.removeAttr('style');  
		    }  
		});   

		$('.allSectionButton').click(function(){
			$('.all_section').css('display','block');
		});
		$('#allsections-close-btn').click(function(){
			$('.all_section').css('display','none');
		});
		//close allsections menu if user clicks anywhere else on the page
		$('html').click(function (e) {
			if (e.target.id != 'allsections-wrapper' && e.target.id != 'submenu-allsections-button') {
				$('.all_section').css('display','none');
			}
		});

		$('#submenu-fashion-button').hover(function(){
			//$('#submenu-fashion').css('display','block');
		},
		function(){
			//submenu_hide_item('fashion');	
		});
		function submenu_hide_item(item){
				submenuTimer =setTimeout(function(){submenu_hideall();},300);
		}
		function submenu_hideall(){
				$('#submenu-travel').css('display','none');
				$('#submenu-lifestyle').css('display','none');
				$('#submenu-fashion').css('display','none');
				$('#submenu-bestofluxe').css('display','none');
				$('#submenu-fashion-button').removeClass('submenu-main-hover');
				$('#submenu-travel-button').removeClass('submenu-main-hover');
				$('#submenu-lifestyle-button').removeClass('submenu-main-hover');
				$('#submenu-bestofluxe-button').removeClass('submenu-main-hover');
				$('#allsections-main').css('display','none');
			}
		});
</script>	 
<script type="text/javascript">
	function openUploadWindow(strPage){
		newwindow=window.open(strPage,'uploadarticle','width=760,height=700,toolbar=0,scrollbars=1,resizable=1,location=0');
		if (window.focus) {newwindow.focus()}
	}
</script>

