      <span property="dc:title" content="Office Sway lets you add video from your camera roll in latest update" class="rdf-meta element-hidden"></span><span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span>  
      <div class="meta-wrap"> 
                <div class="meta-author submitted">
          <span property="dc:date dc:created" content="2015-02-19T13:19:14-05:00" datatype="xsd:dateTime" rel="sioc:has_creator">By <a href="/users/jkeller" title="View user profile." class="username" xml:lang="" about="/users/jkeller" typeof="sioc:UserAccount" property="foaf:name" datatype="">Joseph Keller</a>, Thursday, Feb 19, 2015 a	 1:19 pm EST</span>        </div><!-- meta-author -->
                
