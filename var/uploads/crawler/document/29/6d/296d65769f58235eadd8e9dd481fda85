        <script type="text/javascript">
            function addLoadEvent(func) {
                var oldonload = window.onload;
                if (typeof window.onload != 'function') {
                    window.onload = func;
                } else {
                    window.onload = function() {
                        if (oldonload) { oldonload(); }
                        func();
                    }
                }
            }

            function closeModalOverlay() {
                $("#modalOverlay").click();
                killTheModalOverlay();
            }					
        </script>
