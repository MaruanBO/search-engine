         this.style.color = 'black';
         this.style.backgroundColor = 'white';
         this.innerHTML = '';
         this.innerHTML = '<img src="http://mediaassets.kitsapsun.com/html/data/seahawks_x27.jpg" alt="Seahawks">';
      }
   });
});

