<script type="text/javascript">
function resizeDirColumns() {
		setTimeout(function() {
	        var test = document.createElement('div');
        	jQuery(test).css({'width': '1px','min-width': '2px'});
	        jQuery('body').append(test);
	        var nativeSupport = ( test.offsetWidth && test.offsetWidth == 2 );
	        jQuery(test).remove();
			var anonh = jQuery("div#anonDir").height();
			var newh = 1000;
			if( anonh > 515) { newh = 485 + anonh; }
			if(nativeSupport) {
				jQuery("div#rest_details").css("min-height",newh);
				jQuery("div#rest_details_col1").css("min-height",newh);
				jQuery("div#rest_details_col2").css("min-height",newh);
				jQuery("div#rest_details_col3").css("min-height",newh);
			} else {
				jQuery("div#rest_details").css("height",newh);
				jQuery("div#rest_details_col1").css("height",newh);
				jQuery("div#rest_details_col2").css("height",newh);
				jQuery("div#rest_details_col3").css("height",newh);
			}
		},800);
}
/* delete cookie by setting expiration 1 second into the past */
function deleteCookie (cookie_name)
{
  var cookie_date = new Date ( );  // current date & time
  cookie_date.setTime ( cookie_date.getTime() - 1 );
  document.cookie = cookie_name += "=; expires=-5";
}

function clearSearchCookie (inDomain)
{
	inDomain = inDomain.replace(/[\"\'][\s]*javascript:(.*)[\"\']/g, "\"\"");
    inDomain = inDomain.replace(/script(.*)/g, "");
    inDomain = inDomain.replace(/eval\((.*)\)/g, "");
	var name = "searchCookie";
	var value = "";
	var domain = "." + escape(inDomain);
	var path = "/";
	var cookie_date = new Date ( );  // current date & time
	var expires = 1000 * 60 * 60 * 24 * 365;
  	cookie_date.setTime ( cookie_date.getTime() + expires );
	var curCookie = name + "=" + escape(value) + "; expires=" + cookie_date + "; path=" + path + "; domain=" + domain;
  document.cookie = curCookie;
  document.getElementById('cookies').style.display='none';
  if (document.getElementById('cookies2') != null) {
  	document.getElementById('cookies2').style.display='none';
  }
}
</script>

<script type="text/javascript">
/* get cookie */
function parseCookie() {		
cookie = readCookie('searchCookie');	

if(!cookie) { return false; }
	
var results = new Array();
if(cookie.indexOf('"') == 0 && (cookie.lastIndexOf('"') == cookie.length-1))
 cookie = cookie.substring(1, cookie.length-1)
var list = cookie.split(':');
for(i = 0; i < list.length; i++) {
	var parsedItem = list[i].split('|');
	
	if (parsedItem != '') {	
		parsedItem[1] = parsedItem[1].replace(/\+/g, ' ');
		
		var exists = false;
		for(j = 0; j < results.length; j++) {
			if(results[j][1] == parsedItem[1]) {
				exists = true;
				break;
			}
		}
		if(!exists) {					
			results.push(parsedItem); 
		}
	}
}
return results;
}

function readCookie(name) {

var cookie = document.cookie;
var str = name + '=';
var start = cookie.indexOf(str);
if(start != -1) {
	var end = cookie.indexOf(';', start);
	if(end != -1) {
		return unescape(cookie.substring(start + str.length, end));
	} else {
		return unescape(cookie.substring(start + str.length));
	}
} else {
	return false;
}		

}
</script>









