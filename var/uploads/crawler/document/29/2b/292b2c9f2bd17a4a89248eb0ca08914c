<script type="text/javascript">
var cityAjaxSelected = false;
var overlayDisplayedOnce = false;

jQuery.noConflict();
jQuery(document).ready(function($) {


$('#GenRssButton').click(function() {


		var cityState = $('#CityAjaxRss').val();								
		var city = '';
		var state = '';
		if(cityState == 'City, State' || jQuery.trim(cityState) == '' || cityState.indexOf(',') < 0) {
			$("#rss_results").hide();
			$("#rss_error").show();
		} else {
			$("#rss_error").hide();
			cityState = cityState.split(',');
			city = jQuery.trim(cityState[0]);
			state = jQuery.trim(cityState[1]);
			$("#rss_new_link").text(city + ', ' + state);	
			$("#rss_coming_link").text(city + ', ' + state);	
			$("#rss_leaving_link").text(city + ', ' + state);		
			
			$("#rss_new_link").attr("href", "http://feeds.rewardsnetwork.com/feeds/new/rn/"+ escape(city) +"/"+ escape(state) + "/");
			$("#rss_coming_link").attr("href", "http://feeds.rewardsnetwork.com/feeds/comingsoon/rn/"+ escape(city) +"/"+ escape(state) + "/");
			$("#rss_leaving_link").attr("href", "http://feeds.rewardsnetwork.com/feeds/leavingsoon/rn/"+ escape(city) +"/"+ escape(state) + "/");			
			$("#rss_results").show();
		} 					

});

var cityAjaxSubmitted = false;
var restBCText = 'Restaurant, Bar or Club Name';
$('#CityAjax').autocomplete('/ajax-one-box.htm',	{
	minChars: 3,
	max: 10,
	delay: 30,
	highlight: false,	
	formatItem: formatItem,
	formatResult: formatResult
}).result(function(event, data, formatted) {
	var hidden = $(this).find("..+/:input");
	hidden.val( (hidden.val() ? hidden.val() + ";" : hidden.val()) + data[1]);
	cityAjaxSubmitted = true;
	deleteCookie("backfromdetails");
	if(document.getElementById('merchantSearch').value == restBCText)
		document.getElementById('merchantSearch').value = '';
	$('#CityAjax').parents('form').submit();
	
});
$('#merchantSearch').keydown(function(e){
	var code = (e.keyCode ? e.keyCode : e.which);
	deleteCookie("backfromdetails");
	if(document.getElementById('merchantSearch').value == restBCText)
		document.getElementById('merchantSearch').value = '';
	if(code == 13)
		$('#CityAjax').parents('form').submit();
});

