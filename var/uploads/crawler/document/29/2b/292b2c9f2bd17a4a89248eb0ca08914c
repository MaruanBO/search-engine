function handleManAddrResponse(responseXml) {
	var response = $('message', responseXml).text();
	//alert("1");
	if(response == 'true') {
		//alert("2");
		$("#manaddrsubmit").hide();
	}
}

function handleLoginResponse(responseXml) {
	var response = $('message', responseXml).text().split('|');

	var status = response[0];
	var message= response[1];
	var page   = response[2];

	if (status == 'false') {
	document.getElementById("error_login").innerHTML=message;
	}  else {
	window.location = page;
	}
}
});
