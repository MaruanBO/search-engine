<script>
	var isLoggedIn = false;

	function showPaymentFailedDialog() {
		$('#ysi_dlg_payment_failed').dialog('open');
	}
	function closePaymentFailedDialog() {
		$('#ysi_dlg_payment_failed').dialog('close');
	}


	$(document).ready(function () {

		// Post upload Pro Signup dialog
		$("#ysi_dlg_payment_failed").dialog({
			height: 'auto',
			autoOpen: false,
			width: 540,
			open: function (event, ui) {
				$(".ui-dialog-titlebar-close").hide();
				$(".ui-dialog .ui-dialog-titlebar").show().css('margin','20px 20px 0px 15px');
				$(".ui-dialog-content").css('padding','0px 20px 20px');
				$('input:radio[name="verify_cc_info"]').filter('[value="1"]').attr('checked',true);
				var psModalData = {
					pagename:getPageName(),
					modalname:'checkPaymentFailedDialog',
					triggertype:'s',
					keyframe:1
				}
				trackmodaldetailed (psModalData);
			}
		});

		$("#ysi_btn_payment_failed_continue").click(function(){
			if($("#verify_cc_info_radio:checked").val()) {
				window.location = "/ccdetails";
			} else if (typeof(loginData) != 'undefined') {
				if (loginData.msg == 'v2'){
					window.location = "https://www.hightail.com/?myaccount=true";
					return;
				} else if(loginData.msg=='v3'){
					window.location = redirect_url;
					return;
				} else if (loginData.msg=='hasWorkspace'){
			  		window.location = "overview"; 
					return;
				} else if(loginData.msg=='v2NotActive'){
					window.location = "https://www.hightail.com/resend_activate_auto.php";
					return;
				} else if(loginData.msg=='201'){
					$("#ysi_btn_hd_login, #basic-modal-content-cancel, #ysi_btn_hd_login_dis").toggle();
					$("#basic-modal-content").dialog('close');
					$('#ysi_dlg_login_ad_only').dialog('open');
					return;
				} else if((typeof loginData.fromAPI != 'undefined') && loginData.fromAPI){
					$("#ysi_exists_msg").hide();
					$("#ysi_verify_msg").show().html(loginData.msg);
					$("#ysi_btn_hd_login, #ysi_btn_hd_login_dis, #basic-modal-content-cancel").toggle();
					return;
				} else {
					window.location = "/overview";
				}
			} else {
				$('#ysi_dlg_payment_failed').dialog('close');
				return;
			}
		});
	});
</script>
  

