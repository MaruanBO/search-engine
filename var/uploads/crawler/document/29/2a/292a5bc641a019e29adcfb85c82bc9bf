    <script>
        function sendOmSuccess() {
            var modalData = {
                pagename: getPageName(), accttype: getCookieAcctType(), modalname: 'login_success', events: "event1"
            }
            trackmodaldetailed(modalData);
        }

        var dropdownEmailMessage = /*<sl:translate>*/ 'Your email' /*</sl:translate>*/;
        $(document).ready(function () {
            $("#ht_login").click(function () {
                if (location.protocol.toLowerCase() == 'http:') {
                    location.href = 'https://' + location.hostname + '/login';
                    return;
                }
                if ($('#ht_email').val() == '') {
                    $('#ht_email').addClass('not_touched_input');
                    $('#ht_email').css('color', '#aeaeae');
                    $('#ht_email').val(dropdownEmailMessage);
                }
                setTimeout(function () {
                    if (typeof centerContent2014Hp == 'function') {
                        $(".bottom").css('position','relative');
                    }
                    $("#login-panel-container").slideDown(500, function () {
                        if (typeof centerContent2014Hp == 'function') {
                            centerContent2014Hp();
                        }
                    });
                    $('.global-top-message').css('top', '-292px');
                }, 1);
            });

            $("#ht-login-signup-close").click(function () {
                $("#login-panel-container").slideUp(500, function () {
                    if (typeof centerContent2014Hp == 'function') {
                        centerContent2014Hp();
                    }
                });
                $('.global-top-message').css('top', '-80px');
            });

            $('#ht_email').unbind('focus');
            $('#ht_email').focus(function (e) {
                $(this).css('color', 'black');
                if ($(this).hasClass('not_touched_input')) {
                    $(this).removeClass('not_touched_input');
                    $(this).css('color', 'black');
                    $(this).val('');
                }
            });

            $('#ht_email').focusout(function (e) {
                if ($(this).val() == '') {
                    $(this).addClass('not_touched_input');
                    $(this).css('color', '#aeaeae');
                    $(this).val(dropdownEmailMessage);
                }
            });

            $('#ht_pass_prompt').focus(function (e) {
                $('#ht_pass_prompt').hide();
                $('#ht_pass').show();
                $('#ht_pass').focus();
            });

            $('#ht_pass').focusout(function (e) {
                if ($(this).val() == '') {
                    $('#ht_pass').hide();
                    $('#ht_pass_prompt').show();
                }
            });

            $('#remember, .css-label').click(function () {
                if ($('#remember').is(':checked')) {
                    $('#remember').attr('checked', false);
                    $('.css-label').css('background-position', '0 0');
                } else {
                    $('#remember').attr('checked', true);
                    $('.css-label').css('background-position', '0 -15px');
                }
            });

            
            $('#ht_email').keypress(function (e) {
                if (e.which == 13) {
                    $('#ht_login_btn').click();
                    e.preventDefault();
                }
            });

            $('#ht_pass').keypress(function (e) {
                if (e.which == 13) {
                    $('#ht_login_btn').click();
                    e.preventDefault();
                }
            });
            
            $('#ht_login_btn').click(function () {
                if (!areCookiesEnabled()) {
                    window.location = "/no-cookies";
                    return;
                }

                $('#ht_login_btn').hide();
                $('#ht_login_btn_disa').show();
                var loginEmail = $.trim($('#ht_email').val());
                $.post(
                    "hightailHomeFoot?phi_action=app/getLoginInfo&",
                    {ysi_email: loginEmail,
                        ysi_password: $('#ht_pass').val(),
                        remember_me: $('#remember').is(':checked')},
                    function (data) {
                        var toVal = 1000; // Omniture timeout value

                        if (typeof data != 'object' || typeof data.msg == 'undefined') {
                            $("#errorDisplay").show().html("Log in system error. Please try again later.");
                        } else if (typeof data.orgUrl == 'undefined') {
                            redirect_url = "/";
                        } else {
                            redirect_url = data.orgUrl;
                        }
                        if (data.transFailure) {
                            if (typeof showPaymentFailedDialog == "function")
                                showPaymentFailedDialog();
                            else
                                showError('Error:', 'There is a potential problem with your payment method on file.  Please go to My Account and check your Billing information.');
                            return;
                            // Now check all the cases that require a redirect to complete a login
                            // Some of these may have an error code (e.g., AD, SAML), but will succeed on
                            // the redirected login.  A setTimeout is added to allow the Omniture call to complete
                        } else if (data.msg == 'v2') {
                            sendOmSuccess();
                            setTimeout(function () {
                                window.location = "https://www.hightail.com/?myaccount=true";
                            }, toVal);
                            return;
                        } else if (data.msg == 'hasWorkspace') {
                            sendOmSuccess();
                                                        setTimeout(function () {
                                window.location = "overview";
                            }, toVal);  //overview page
                                                        return;
                        } else if (data.msg == 'v3') {
                            sendOmSuccess();
                            if (typeof existingUserUpgradeRedirect === 'function') {
                                if (existingUserUpgradeRedirect(loginEmail)) return;
                            }
                                                        setTimeout(function () {
                                window.location = redirect_url.length > 1 ? redirect_url : "";
                            }, toVal);
                                                        return;
                        } else if (data.msg == 'v2NotActive') {
                            window.location = "https://www.hightail.com/resend_activate_auto.php";
                            return;
                        } else if (data.msg == '912') {
                            window.location = '/samlLogin?phi_action=app/samlLogin&subAction=makeSamlRequest&emailAddress=' + $("#ht_email").val();
                            return;
                        } else if (data.msg == '201') {
                            $("#errorDisplay").show().html("Your account requires Hightail Express or Outlook Plug-in to send files.  For more information please contact your helpdesk.");
                            $("#ysi_btn_login, #ysi_btn_login_dis").toggle();
                            return;
                        } else if ((typeof data.fromAPI != 'undefined') && data.fromAPI) {
                            $("#errorDisplay").show().html(data.msg);
                            $("#ysi_btn_login, #ysi_btn_login_dis").toggle();
                            return;
                        } else if (data.succ == false) {
                            // Catch any remaining errors
                            $("#errorDisplay").show();
                        }
                        // Rest buttons to start point
                        $('#ht_login_btn_disa').hide();
                        $('#ht_login_btn').show();
                    },
                    'json'
                );
            });

			$('#len-nav-sign-in').click(function() {
				omniTrackPartnerLogin('lenovo');
				setTimeout(function() {
					window.location.href="https://ypi.hightail.com/ypi/a1/partner/login?uiLogin=true&apiKey=a0a592fdbebebda5f0ce969031f3b0f9fb1c9d06";
	   	     	},500);
			});

        }); // end onload

    </script>
