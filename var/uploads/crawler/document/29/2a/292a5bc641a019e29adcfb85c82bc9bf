<script type="text/javascript">
var YSI = YSI || {};
YSI.Track = (function() {
	var ppuSkus = null;
	var orderId = null;
	var sid = null;
	var cid = null;
	var category = null;
	var events = null;
	
	function setPpuSkus(skus) {
		ppuSkus = skus;
	}

	function getPpuSkus() {
		return ppuSkus;
	}
	
	function setOrderId(newOrderId) {
		orderId = newOrderId;
	}
	
	function getOrderId() {
		return orderId;
	}
	
	function setSIDCID(c_sid, c_cid) {
		sid = c_sid;
		cid = c_cid;
	}
	
	function getSID() {
		return sid;
	}
	
	function getCID() {
		return cid;
	}
	
	function setOmnitureData(dat) {
		orderId = null;
		events = null;
		if('object' != typeof dat) return;
		if(dat.omnitureCategory && dat.orderId) {
			var eventIds = [];
			switch(dat.omnitureCategory) {
				case 'SUB':
				eventIds = [1,3,13,14,42,43];
				break;
				case 'TRIAL':
				eventIds = [1,3,13,14,44,45];
				break;
				default: 				eventIds = [14,46,47];
				break;
			}
			if(eventIds.length) {
				orderId = dat.orderId;
				events = 'event' + eventIds.join(':' + orderId + ',event') + ':' + orderId + ',purchase';
			}
		}
	}
	
	function setUploadEvent() {
		if(ppuSkus && orderId) {
			var eventIds = [7,14,46,47];
			events = 'event' + eventIds.join(':' + orderId + ',event') + ':' + orderId + ',purchase';
		} else {
			events = 'event7';
		}
	}
	
	function getEvents() {
		return events;
	}

	return {
		setPpuSkus:setPpuSkus,
		getPpuSkus:getPpuSkus,
		setOrderId:setOrderId,
		getOrderId:getOrderId,
		setSIDCID:setSIDCID,
		getSID:getSID,
		getCID:getCID,
		setOmnitureData:setOmnitureData,
		getEvents:getEvents,
		setUploadEvent:setUploadEvent
	}
})();


function trackmodal (pagename, modalname) {
	var oldpagename = s.pageName;
	s.pageName=pagename+":"+modalname;
	s.t();
	s.pageName = oldpagename;
}

function getOmnitureProductInfo (data) {
	switch (data.omnitureCategory) {
		case "TRIAL":
			omnitureEventType1="event44="+data.omnitureAccountPrice+"|event45=1";
			break;
		case "SUB":
			omnitureEventType1="event42="+data.omnitureAccountPrice+"|event43=1";
			break;					
		default:
			omnitureEventType1="";
			break;
	}
	return omnitureEventType1;
}
function loadURL (destinationURL) {
                document.location = destinationURL;
}

function loadURLNewWindow (destinationURL) {            
	popupWin = window.open(destinationURL);
}



function trackLinkLoadURL (domObj, linkname, destinationURL) {
                s.tl(domObj,'d', linkname);
                setTimeout ("loadURL('"+destinationURL+"')", 500);
                return false;
}

function conversionTopOfFunnel (domObj, nameOfFunnel, destinationURL) {
                s.linkTrackVars="events";
                s.linkTrackEvents="event61";
                s.events="event61";
                s.tl(domObj,'o', nameOfFunnel);
                setTimeout ("loadURL('"+destinationURL+"')", 500);
                return false;
}

function getUserIdFromAuth () {
	var auth = $.cookie ("auth");
	var authArray = auth.split (".");
	var userId = authArray[0];
	return userId;
}

function trackmodaldetailed (modaldata) {
	var oldpagename = s.pageName;
	var modal = "OTH";
	if (typeof modaldata.triggertype != 'undefined') {
		switch(modaldata.triggertype.toLowerCase()) {
			case "s":
				modal = "SWL";
				break;
			case "h":
				modal = "HWL";
				break;
			case "u":
				modal = "USE";
				break;
		}

		s.pageName=modaldata.pagename+":"+modaldata.accttype+":"+modal+":"+modaldata.modalname;

		s.prop1=omniturePrefix+"|"+modaldata.pagename+":"+modaldata.accttype+":"+modal+":"+modaldata.modalname;
		s.prop2=omniturePrefix+"|"+modaldata.modalname;

	} else {
		s.pageName=modaldata.pagename+":"+modaldata.accttype+":"+modaldata.modalname;
		s.prop1=omniturePrefix+"|"+modaldata.pagename+":"+modaldata.accttype+":"+modal+":"+modaldata.modalname;
		s.prop2=omniturePrefix+"|"+modaldata.modalname;
	}


	/* Set eVars */
	if(typeof modaldata.incid != 'undefined') s.eVar2 = modaldata.incid;
	s.eVar5=omniturePrefix+"|"+modaldata.pagename; /* prefix|pagename */
	s.eVar7=s.prop22=""; /* Customer Type - legacy */

    s.eVar9=s.prop3=modaldata.accttype; /* Account Type */

	s.eVar11=s.prop11=(s.prop3 != 'GST' && $.cookie ("Ysi_session_id")) ? "Logged In" : "Logged Out"; /* Logged in Status */
	s.prop18=omnitureSubBusiness; /* Sub Business Unit */
	s.eVar22=""; /* Promo ID TBD */
	s.eVar23=""; /* Purchase Type - Upgrade/Downgrade/New TBD */
	s.eVar24=s.prop2; /* prefix|Simple page name */
	s.eVar25=s.prop1; /* prefix|Detailed page name */

	if (typeof modaldata.userId != 'undefined') {
		s.eVar55=modaldata.userId;
	}

	if(typeof modaldata.keyframe != 'undefined' && modaldata.keyframe > 0) {
		s.eVar41=s.pageName;
	}
	/* End set eVars */

	/* Set sProps */
	s.prop19=""; /* First level navigation */
	s.prop20=""; /* Second level navigation */
	s.prop23=""; /* Price type */
	s.prop24=""; /* Site name */
	s.prop46=omnitureLanguage; /* Language */

	/* End set sProps */

	/* Set Events */
	if(typeof modaldata.events != 'undefined') {
		s.events = modaldata.events;
	} else {
		var events = YSI.Track.getEvents();
		if(events) s.events = events;
	}
	/* End set events */

	/* Set products */
	if(typeof modaldata.products != 'undefined') s.products = modaldata.products;
	/* End set products */

	/* Set purchaseID */
	if(typeof modaldata.purchaseID != 'undefined') s.purchaseID = modaldata.purchaseID;

	/* Set other */
	if(typeof modaldata.state != 'undefined') s.state = modaldata.state;
	if(typeof modaldata.zip != 'undefined') s.zip = modaldata.zip;
	
	if(modaldata != 'undefined' && modaldata.cid) {
		if (modaldata.cid.length > 0) {
			s.campaign = s.eVar10 = s.eVar8 = s.eVar30 = modaldata.cid;
		}
	}
	
	// Set Source ID for backend processing
	if(typeof modaldata.sid != 'undefined') {
		if (String(modaldata.sid).length > 0) {
			if( $.cookie('osid') == null ) { 
				$.cookie ("osid",modaldata.sid, { expires: 60, 
                                                  path   : '/',
                                                  domain  : 'yousendit.com'
                                                });
			}
			s.eVar17 = s.prop9 = modaldata.sid;
			$.cookie ("tsid",modaldata.sid, { expires: 60,
                                              path   : '/',
                                              domain  : 'yousendit.com'
                                            });
		}
	}
	/* End set other */

	s.t();
	s.pageName = oldpagename;

	/* Reset trackmodaldetailed specific variables */
	if(typeof modaldata.events != 'undefined') s.events = "";
	if(typeof modaldata.products != 'undefined') s.products = "";
	if(typeof modaldata.purchaseID != 'undefined') s.purchaseID = "";
	/* End Reset trackmodaldetailed specific variables */

	/* Reset eVars */
	s.campaign = s.eVar10 = s.eVar8 = s.eVar30 = "";
	s.eVar2 = "";
	s.eVar5=""; /* prefix|pagename */
	s.eVar7=""; /* Customer Type - legacy */
	s.eVar9=""; /* Account Type */
	s.eVar11=""; /* Logged in Status TBD */
	s.eVar17=""; /* SID */
	s.eVar22=""; /* Promo ID TBD */
	s.eVar23=""; /* Purchase Type - Upgrade/Downgrade/New TBD */
	s.eVar24=""; /* prefix|Simple page name */
	s.eVar25=""; /* prefix|Detailed page name */
	s.eVar55=""; /* userId */
	s.state=""; /* state */
	s.zip=""; /* zip */
	if(typeof modaldata.keyframe != 'undefined' && modaldata.keyframe > 0) { s.eVar41='';}

	/* Reset sProps */

}

function sendDownloadLinkLoadTracking (domObj,linkData) {
		// Set events for link tracking
		var trackType = 'd';
		if(linkData && linkData.trackType && 'o' == linkData.trackType) {
			trackType = 'o';
		} else {
			s.linkTrackVars = "prop1,prop2,prop5,,eVar73,events";
			s.events = "event67";
			s.linkTrackEvents = "event67";
			s.eVar73 = linkData.omnitureAppName;
			s.prop5 = location.protocol+"//"+location.hostname+location.pathname;
			s.prop1 = linkData.omnitureAppName;
			s.prop2 = "D=c1";
		}
		s.tl(domObj, trackType, linkData.omnitureAppName);
}

function trackDownloadLinkLoadURL (domObj, linkData) {
// Required linkData parameters
// linkData.domObj - DOM object - pass "this" into linkData.domObj
// linkData.omnitureAppName - Omniture application name.
// linkData.destinationURL - The full URL of the file being downloaded
// linkData.newWindow - value of 1 opens the destinationURL to a new window

	if (typeof domObj != 'undefined' || typeof linkData.linkname != 'undefined' || typeof linkData.destinationURL != 'undefined' || typeof linkData.cid != 'undefined' ) {

		sendDownloadLinkLoadTracking(domObj,linkData);

		if (typeof linkData.newWindow == 'undefined') {
			setTimeout ("loadURL('"+linkData.destinationURL+"')", 1000);
		} else {
			setTimeout ("loadURLNewWindow('"+linkData.destinationURL+"')", 1000);
		}
	} else {
		if(document.URL.indexOf('www.yousendit.com') < 0) {
			alert ("trackDownloadLinkLoadURL needs all parameters for tracking");
		}
	}
}

function trackdownloadlink (pagename, linkname) {
  s.products=';Download';
  s.events='event8';
  s.linkTrackVars='events';
  s.linkTrackEvents='event8';
  s.tl(this,'o',pagename+':'+linkname);
}

function trackContactsImportLink (domObj, provider) {
	s.linkTrackVars='prop56,events';
	s.events='event82';
	s.linkTrackEvents="event82";
	s.prop56='Open Import Contacts:'+provider;
    s.tl(domObj,'o', s.prop56);

	s.linkTrackVars='';
	s.events='';
	s.linkTrackEvents="";
	s.prop56='';
}


function trackSocialLink (socialData) {
    
     s.linkTrackVars='prop56,prop58,events';

     if (socialData.socialChannel.toLowerCase() == "email") {
          s.events="event81";
          s.linkTrackEvents="event81";
          s.prop56 = socialData.socialChannel+":"+socialData.pageName+":"+socialData.totalEmailsSent;
          s.prop58 = socialData.socialServiceName+":"+socialData.totalNativeImported;
     } else if (socialData.socialChannel.toLowerCase() == "facebook") {
          s.events="event79";
          s.linkTrackEvents="event79";
          s.prop56 = socialData.socialChannel+":"+socialData.pageName;
     } else if (socialData.socialChannel.toLowerCase() == "twitter") {
          s.events="event80";
          s.linkTrackEvents="event80";
          s.prop56 = socialData.socialChannel+":"+socialData.pageName;
     } else {
          alert ("trackSocialLink value is required");
     }
         
     s.tl(this,'o',socialData.socialChannel+":"+socialData.pageName);
    
     // Reset SC variables
     s.linkTrackEvents="";
     s.events="";
     s.prop56="";
     s.prop58="";
}

function tracklink (pagename, linkname) {
	if (pagename != "guest_send" && linkname != "ysi_bindYahooSignin" && linkname != "ysi_bindFacebookSignin" && linkname != "ysi_bindGoogleSignin")
		s.tl(this,'o',pagename+":"+linkname);
}

function trackexitlink (pagename, linkname) {
	s.tl(this,'e',pagename+":"+linkname);
}

var YSI = YSI || {};

YSI.mBox = (function() {
	function track(ysiTrackLinkData) {
		if('object' != typeof ysiTrackLinkData) return;
		var dynamicDivValue = "Not set";
		if (typeof ysiTrackLinkData.dynamicDiv != 'undefined') {
			dynamicDivValue = ysiTrackLinkData.dynamicDiv;
		}
	
		var mboxDivValue = "Not set";
		if (typeof ysiTrackLinkData.mboxDiv != 'undefined') {
			mboxDivValue = ysiTrackLinkData.mboxDiv;
		}
	
		var orderTotalValue = "Not set";
		if (typeof ysiTrackLinkData.orderTotal != 'undefined') {
			orderTotalValue = ysiTrackLinkData.orderTotal;
		}
	
		var productPurchaseIdValue = "Not set";
		if (typeof ysiTrackLinkData.productPurchaseId != 'undefined') {
			productPurchaseIdValue = ysiTrackLinkData.productPurchaseId;
		}
	
		var mboxTypeValue = "Not set";
		if (typeof ysiTrackLinkData.mboxType != 'undefined') {
			mboxTypeValue = ysiTrackLinkData.mboxType;
		}
	
		/* Check to see if div already exists */
		if ($("#"+ysiTrackLinkData.dynamicDiv).length == 0) {
	    var divTag = document.createElement("div");
	    divTag.id = dynamicDivValue;   
	    divTag.setAttribute("display","none");
		}
		var orderId = ysiTrackLinkData.orderId ? ysiTrackLinkData.orderId : '';
	  mboxDefine(dynamicDivValue, mboxDivValue);
	  mboxUpdate(mboxDivValue, "orderTotal="+orderTotalValue, "productPurchasedId="+productPurchaseIdValue, "type="+mboxTypeValue, 'orderId='+orderId);
	}

	return {track:track}
})();


