              <script type="text/javascript">
                //Why go pro dropdown animation script
                $(document).ready(function(){
                  var frametotal = 6,
                    curframe = 1,
                    lastframe = -1,
                    timerid = -1,
                    direction = true, //true == forward; false == backward
                    isMouseEntered = false;

                  $('#ysi_upgrade').on('mouseenter', function(){
                    if(isMouseEntered){return;}
                    curframe = 1;
                    lastframe = -1;
                    direction = true;
                    isMouseEntered = true;
                    $('li#ysi_upgrade>a').css('color', '#000');
                    $('#wgp-slide1').show();
                    set_navspots(1);
                    window.clearInterval(timerid);
                    window.clearTimeout(timerid);
                    timerid = window.setTimeout(delay_over, 1000);
                  }).on('mouseleave', function(){
                    if(!isMouseEntered){return;}
                    isMouseEntered = false;
                    window.clearInterval(timerid);
                    window.clearTimeout(timerid);
                    $('li#ysi_upgrade>a').css('color', '#fff');
                    $('.wgp-slide').hide();
                  });

                  function show_frame(idx) {
                    if(!direction){
                      var oldslide = $('#wgp-slide' + lastframe);
                      oldslide.hide("slide", {direction: "right"}, 300);
                      var slide = $('#wgp-slide' + idx);
                      slide.show("slide", { direction: "left" }, 300);
                      set_navspots(idx);
                    }
                    else {
                      if(lastframe > -1) {
                        var oldslide = $('#wgp-slide' + lastframe);
                        oldslide.hide("slide", {direction: "left"}, 300);
                      }
                      var slide = $('#wgp-slide' + idx);
                      slide.show("slide", { direction: "right" }, 300);
                      set_navspots(idx);
                    }
                  };

                  function set_navspots(idx) {
                    $('#navspots span').css('opacity', '0.1');
                    $('#navspot' + idx).css('opacity', '1.0');
                  }

                  function bullet_click(){
                    var idx = parseInt($(this).attr('data-idx'));
                    if(idx === curframe) {return;}
                    lastframe = curframe;
                    curframe = idx;
                    if(lastframe === 1 && curframe === 6) {
                      direction = true;
                    }
                    else if(lastframe === 6 && curframe === 1) {
                      direction = false;
                    }
                    else {
                      direction = (lastframe > curframe ? false : true);
                    }
                    show_frame(idx);
                    window.clearInterval(timerid);
                    window.clearTimeout(timerid);
                    timerid = window.setTimeout(delay_over, 7000);
                  };

                  function slide_click(){
                    var slideId = '',
                      slideNum = '',
                      slideNext = 0;

                    var pattern = /wgp-slide/;
                    slideId = $(this).attr('id');
                    slideNum = parseInt(slideId.replace(pattern,''));
                    lastframe = slideNum;
                    slideNext = (slideNum === frametotal ? 1 : slideNum + 1);
                    curframe = slideNext;
                    direction = true;
                    show_frame(slideNext);
                    window.clearInterval(timerid);
                    window.clearTimeout(timerid);
                    timerid = window.setTimeout(delay_over, 7000);
                  };

                  function timer_tick() {
                    lastframe = curframe;
                    curframe === frametotal ? curframe = 1 : ++curframe;
                    direction = true;
                    show_frame(curframe);
                  };

                  function delay_over() {
                    window.clearTimeout(timerid);
                    timerid = window.setInterval(timer_tick, 5000);
                  };

                  $('#navspots span').on('click', bullet_click);
                  $('.wgp-slide').on('click', slide_click);
                });
              </script>
              <script type="text/javascript">
                $(document).ready(function(){
                  function trackElementClick(elementName){
                    s.events=s.linkTrackVars=s.linkTrackEvents = "";
                    s.linkTrackVars = "prop1,prop2,prop3,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40";
                    s.linkTrackEvents="None";
                    s.tl(true,'o','UpgradeNav' + '  :  ' + elementName);
                    s.events=s.linkTrackVars=s.linkTrackEvents = "";
                  };
                  $('#ysi_upgrade > a').add('#learnMore').click(function(){
                    trackElementClick($(this).text());
                  });
                });
              </script>
                      </li>
                  </ul>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function(){
    function trackElementClick(elementName){
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
      s.linkTrackVars = "prop1,prop2,prop3,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40";
      s.linkTrackEvents="None";
      s.tl(true,'o','UpgradeNav' + '  :  ' + elementName);
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
    };
    $('#ysi_upgrade li a').click(function(){
      trackElementClick('TopNav:Upgrade:Dropdown:' + $(this).text());
    });
  });
</script>
<script type="text/javascript">
  function TopNavElementClick(elementName){
    s.events=s.linkTrackVars=s.linkTrackEvents = "";
    s.linkTrackVars = "prop1,prop2,prop3,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40";
    s.linkTrackEvents="None";
    s.tl(true,'o','TopNavElementClick' + '  :  ' + elementName);
    s.events=s.linkTrackVars=s.linkTrackEvents = "";
  };
  $('#ysi_myworkspace').add('#ysi_dashboard').add('#ysi_send').add('#ysi_sign').click(function(){
    TopNavElementClick($(this).text());
  });
</script>

