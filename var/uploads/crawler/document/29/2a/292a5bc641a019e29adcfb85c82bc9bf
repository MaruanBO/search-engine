	//function to get the current time
	function orderTime()
	{
		var today=new Date();
	          return  "-"+(today.getMonth()+1) + today.getDate()+today.getFullYear();
	}

	s.pageName=$.trim($('#omniturePageName').val());
	var orderID = '';
	var skuID = '';
	var userID = "";
	var orderQty = 1;
  var seats = 1; // seats (units) -- NEW FOR TEAMS

	if('undefined' != typeof omnitureAccountPrice) {
		var totalPrice = omnitureAccountPrice * orderQty;
		omnitureAccountPrice = totalPrice.toFixed(2);
	}

	if (skuID.length > 0) {
		var serializationID;
	
		if (orderID) {
			serializationID = orderID;
		} else {
			serializationID = "436800a895"+orderTime();
		}
	
		var purchaseID = serializationID;
	
		// Event driven metrics
		if (s.pageName == 'lite_confirmation' || s.pageName == 'vri_signup_confirmation' || s.pageName == 'liteconfirmationWS' || s.pageName == 'ws_new_user_signup') {
