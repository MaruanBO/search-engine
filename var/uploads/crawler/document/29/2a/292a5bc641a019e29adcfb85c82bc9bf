<script type="text/javascript">
var YSI = this.YSI || {};
YSI.isThisGroupAdmin = 0;
$(function(){
   $("nav.top-navigation ul#nav li.dir").click(function(e) {
      $("nav.top-navigation ul#nav li.dir ul").removeClass("active");
      $("nav.top-navigation ul#nav li.dir").removeClass('hoverEffect');
      $(this).addClass('hoverEffect');
      $('ul',$(this)).toggleClass('active');
   });
   $(document).click(function(e){
      if(!$(e.target).is('li.dir a')){
         $("nav.top-navigation ul#nav li.dir").removeClass('hoverEffect');
         $("nav.top-navigation ul#nav li.dir ul").removeClass("active");
      }
   }); 
    $("a[rel].title").overlay({
      expose: '#000000',
      loadSpeed: 500,
      left: 'center',
      top:'center'
   });
}); 
</script>
