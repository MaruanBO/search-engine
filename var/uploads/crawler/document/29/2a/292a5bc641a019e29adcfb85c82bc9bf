	<script type="text/javascript">
   function resizeLogoImage(logoImgId,addMargin){
      var maxHeight = 50;
      var maxWidth = 160;;
      var maxRatio = maxHeight/maxWidth;
      var height = $(logoImgId).height();
      var width = $(logoImgId).width();
      var imgRatio = height/width;

      if (imgRatio < maxRatio) { // too wide
         var newHeight = Math.round(imgRatio * maxWidth);
         var topMargin = (maxHeight - newHeight)/2 + addMargin;
         $(logoImgId).css('width',maxWidth);
         $(logoImgId).css('margin-top',parseInt(topMargin) + 'px');
      } else { // too tall
         var newWidth = Math.round(maxHeight/imgRatio);
         var leftMargin = (maxWidth - newWidth)/2;
         $(logoImgId).css('height',maxHeight);
         $(logoImgId).css('margin-left',parseInt(leftMargin) + 'px');
      }
      $(logoImgId).css('visibility','visible');
   }
</script>
