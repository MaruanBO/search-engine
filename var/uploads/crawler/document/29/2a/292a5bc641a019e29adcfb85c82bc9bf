// Partner Login 
function omniTrackPartnerLogin(partnerId){
	s.events = s.linkTrackVars = s.linkTrackEvents = '';
	s.prop67 = s.eVar67 = partnerId;
	s.linkTrackVars = 'prop1,prop2,prop3,prop5,prop11,prop40,prop67,eVar5,eVar6,eVar24,eVar25,eVar40,eVar67';
	s.linkTrackEvents = 'None';
	s.tl(true,'o',s.pageName+' : partner login : ' + partnerId);
}
	
// Lenovo tracking routines
function omniLenovoTrack(clickCode) {
	s.events = s.linkTrackVars = s.linkTrackEvents = '';
	s.linkTrackVars = 'prop1,prop2,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40';
	s.linkTrackEvents = 'None';
	switch(clickCode) {
		case 1:
			msg = "lenovo:topnav:sign in:click";
		break;
		case 2:
			msg = "lenovo:get started:click";
		break;
		case 3:
			msg = "lenovo:link account cancel:click";
		break;
		case 4:
			msg = "lenovo:link account:click";
		break;
		default:
			msg = "lenovo:tracking error:" + clickCode;
	}
	s.tl(true,'o',s.pageName+':'+msg);
	s.events=s.linkTrackVars=s.linkTrackEvents='';
}
