<script type="text/javascript">
$(document).ready(function(){
  function hmPgTopNavClick(elementName){
    var currURL = window.location;
    currURL = currURL.toString().split("?")[0].split("#")[0].replace(/\/$/,'');
    if(currURL === 'https://www.hightail.com'){
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
      s.linkTrackVars = "prop1,prop2,prop3,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40";
      s.linkTrackEvents="None";
      s.tl(true,'o','topNavClick' + '  :  ' + s.pageName +  '  :  ' + elementName);
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
    }
  };

  $('ul.loginJoin li a').add("ul#nav_menu_guest li a").each(function(){
    $(this).click(function(){
      hmPgTopNavClick($(this).text());
    });
  });
});
</script>

<script type="text/javascript">
  $(document).ready(function(){
    function hmPgElementClick(elementName){
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
      s.linkTrackVars = "prop1,prop2,prop3,prop5,prop11,prop40,eVar5,eVar6,eVar24,eVar25,eVar40";
      s.linkTrackEvents="None";
      s.tl(true,'o','hmPgBodyClick' + '  :  ' + elementName);
      s.events=s.linkTrackVars=s.linkTrackEvents = "";
    };
    $('#buttonTryNow').add('#buttonBuyNow').click(function(){
      hmPgElementClick($(this).text());
    });
    $('#enterprise-ad a').click(function(){
      hmPgElementClick('enterprise-ad');
    });
    $('#enterprise-advertise a').click(function(){
      hmPgElementClick('enterprise-advertise');
    });
  });
</script>

<script>
$(document).ready(function(){
  window.onkeydown = function(e) { 
  return !(e.keyCode == 32);
  };	
  // Window resize
  $(window).resize(function(){
  var height = $(window).height();
  if (height <= 500) {
  $(".bottom").css('position','relative');
  $(".policy").css('bottom','-50px');
  $("body").css('overflow','scroll').css("min-height","500px");
  $(".body-wrapper").css('margin','1% auto');
  }
  if ((height > 501) && (height <= 800)) {
  $(".bottom").css('position','relative');
  $(".policy").css('bottom','0');
  $("body").css('overflow','hidden').css("min-height","800px");
  $(".body-wrapper").css('margin','11% auto');
  }
  if (height > 801) {
  $(".bottom").css('position','fixed');
  $(".policy").css('bottom','0');
  $("body").css('overflow','hidden').css("min-height","1300px");
  $(".body-wrapper").css('margin','15% auto');
  }
  });
  // For IE8
  var uaString = navigator.userAgent;
  if (uaString.match('MSIE 8.0')) {
  $('body').css("background","#3A383A url(/web/en_US/theme_default/images/content/bg_simple.jpg) center 0 no-repeat");	
  }
});
</script>

