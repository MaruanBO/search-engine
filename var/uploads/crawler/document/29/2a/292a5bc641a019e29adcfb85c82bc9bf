<script>
	$(document).ready(function(){
		$("#ysi_dlg_login_ad_only").dialog({
			height: 'auto',
			width: 500,
			position: 'center',
			closeOnEscape: true,
			autoOpen: false,
			hide: 'slide',
			open: function (event, ui) {
				$(".ui-dialog-titlebar-close").hide();
				$(".ui-dialog .ui-dialog-titlebar").css('margin','5px 20px');
				$(".ui-dialog-content").css('margin','0px 20px');
				$("a").blur();
			},
			close: function (event, ui) {
			}
		});

		// Failure resend of activation buttons
		$("#ysi_btn_login_ad_only_cancel").click(function () {
			$("#ysi_dlg_login_ad_only").dialog('close');
		});

		$(".login_popup_close").click(function() {
			$('#ysi_su_pwd').val('');
			$('#ysi_btn_send_processing').hide();
			$('#ysi_btn_send').show();
			$("#basic-modal-content, #ysi_dlg_lrg20").dialog('close');
		});


	});

	function doUserLogin(){

		if(! $("#ysi_hd_login").valid() ) {
			return false;
		}

		if(typeof login_dialog.customizedLogin == 'function') {
			login_dialog.customizedLogin();
			return false;
		}

		$("#ysi_btn_hd_login, #basic-modal-content-cancel, #ysi_btn_hd_login_dis").toggle();
		$.post(
		"/login?phi_action=app/getLoginInfo",
		{ysi_email: $('#ysi_su_email').val(),
		ysi_password: $('#ysi_su_pwd').val()},
		function(data){

			if (data && data.msg && data.msg=='6325'){
				window.location = "https://ypi.hightail.com/ypi/a1/partner/login?uiLogin=true&loginRequired=true&apiKey=a0a592fdbebebda5f0ce969031f3b0f9fb1c9d06&email=" + $('#ysi_su_email').val() + "&retPage=" + escape(document.URL);
				return;
			}
			// Do we have a custom login callback function?  If so, call it
			if(typeof customizedLoginSubmit == 'function') return customizedLoginSubmit(data);

			redirect_url = ''; // yes, it is global.  It's needed in the payment failure dialog
			loginData = data; // set global loginData variable
			if(typeof data.orgUrl == 'undefined') {
				redirect_url = "/";
			} else {
				redirect_url = data.orgUrl;
			}

			do{
				if(typeof data != 'object' || typeof data.msg == 'undefined') break;

				if (data.transFailure){
					login_dialog.dialog('close');
					if (typeof showPaymentFailedDialog == "function") {
						showPaymentFailedDialog();
					}
					else {
						showError('Error:', 'There is a potential problem with your payment method on file.  Please go to My Account and check your Billing information.');
						return;
					}
				} else if (data.msg == 'v2'){
					window.location = "https://www.hightail.com/?myaccount=true";
					return;
				} else if(data.msg=='v3'){
					window.location = redirect_url;
					return;
		        } else if (data.msg=='hasWorkspace'){
		          window.location = "overview"; 
					return;
				} else if(data.msg=='v2NotActive'){
					window.location = "https://www.hightail.com/resend_activate_auto.php";
					return;
				} else if(data.msg=='201'){
					$("#ysi_btn_hd_login, #basic-modal-content-cancel, #ysi_btn_hd_login_dis").toggle();
					$("#basic-modal-content").dialog('close');
					$('#ysi_dlg_login_ad_only').dialog('open');
					return;
				} else if((typeof data.fromAPI != 'undefined') && data.fromAPI){
					$("#ysi_exists_msg").hide();
					$("#ysi_verify_msg").show().html(data.msg);
					$("#ysi_btn_hd_login, #ysi_btn_hd_login_dis, #basic-modal-content-cancel").toggle();
					return;
				}
			}while(false);
			$("#ysi_exists_msg").hide();
			$("#ysi_verify_msg").show().html("Email address or password is invalid. Please try again.");
			$("#ysi_su_pwd").val('');
			$("#ysi_btn_hd_login, #ysi_btn_hd_login_dis, #basic-modal-content-cancel").toggle();
		},'json');
	}
</script>

<script type="text/javascript">
	// Global var to set a PHP value in js for doLogout
	var htlDomain = "www.hightail.com";

    function UpdateJoinLinkRID() {
        var rid = "si-16";
        var pageName = getPageName().toLowerCase();
        if(pageName.indexOf("homepage") >= 0) {
            rid = "si-18";
        }
        else if(pageName.indexOf("compare") >= 0) {
            rid = "si-19";
        }
        else if(pageName.indexOf("downloadready") >= 0) {
            rid = "si-20";
        }

        $("#signupLink a").attr("href", "/join?rid="+rid);
    }


  function toggleAdmin(){
    if($.cookie("ADMIN_MENU_OPEN")==null || $.cookie("ADMIN_MENU_OPEN") == 0){
      $.cookie("ADMIN_MENU_OPEN", 1, { path: '/' });
      $("#topSubHeaderDivParent").show();
      $("#topSubHeaderDiv").slideDown(500);
      $("#header").slideDown(800);

      $("#global-nav-wrapper").addClass("changeHeight");
      $("#sec-nav-admin a").addClass("changeBg");
    }else{
      $.cookie("ADMIN_MENU_OPEN", 0, { path: '/' });
      $("#topSubHeaderDivParent").css('background-color',"#f2f2f2");
      $("#header").slideUp(200);
      $("#sec-nav-admin a").removeClass("changeBg");
      $("#topSubHeaderDiv").slideUp(500,function(){
        $("#topSubHeaderDivParent").hide();
        $("#global-nav-wrapper").removeClass("changeHeight");
      });
    }
  }

  $(document).ready(function(){
    if($.cookie("ADMIN_MENU_OPEN")==null || $.cookie("ADMIN_MENU_OPEN") == 0){
      $.cookie("ADMIN_MENU_OPEN", 0, { path: '/' });
      $("#topSubHeaderDiv").hide();
      $("#topSubHeaderDivParent").hide();
      $("#sec-nav-admin a").removeClass("changeBg");
      $("#header").hide();
      $("#global-nav-wrapper").removeClass("changeHeight");
    }else{
      $.cookie("ADMIN_MENU_OPEN", 1, { path: '/' });
      $("#topSubHeaderDivParent").show().css('background-color',"transparent");
      $("#topSubHeaderDiv").show().css('background-color',"#2e2d2e");
      $("#header").show();
      $("#sec-nav-admin a").addClass("changeBg");
      $("#global-nav-wrapper").addClass("changeHeight");
    }

    var navAcctType = getCookieAcctType();  
    if (navAcctType == 'BAS' || navAcctType == 'LITE' || navAcctType == 'MPL') {
      $('#uplinkNav').remove();
    }

    $('#sec-nav-admin a').click(function() {
      var acctType = getCookieAcctType();  
      if (acctType == 'PPP' || acctType == 'BPL') {
        window.location = "/adminDash";
        return;
      } else {
        toggleAdmin();
      }
    });
    UpdateJoinLinkRID();
  });
  function fitTextInElememnt(el, wd) {
    if(el.width() <= wd) return;
    var txt = el.text();
    var l = Math.round(txt.length * wd / el.width());
    do {
      l -= 3;
      txt = txt.substr(0,l);
      el.text(txt + '...');
    } while(el.width() > wd);
  }

  if($('#partnerlogo').length) {
    $('#noLogoWelcome, #logoWelcome').toggle();
    fitTextInElememnt($('#logoWelcome'), 360);
  } else {
    fitTextInElememnt($('#noLogoWelcome'), 460);
  }
  $('#logo_img').css({'visibility':'visible'});

  function hideSecNav() {
    $('.secondaryNav-dropdown').hide();
  }

  $('#sec-nav-base').mouseover(function() {
    $('#sec-nav-base a').css('background-color','#3a383a');
    $('.secondaryNav-dropdown').show();
  });
  $('#sec-nav-base').mouseout(function() {
    secNavHide = setTimeout('hideSecNav()',100);
    $('#sec-nav-base a').css('background-color','');
  });

  $('.secondaryNav-dropdown').mouseover(function(){
    clearTimeout(secNavHide);
    $('#sec-nav-base a').css('background-color','#3a383a');
    $(this).css('background-color','#f15b41');
    $('.secondaryNav-dropdown').show();
  });
  $('.secondaryNav-dropdown').mouseout(function(){
    //if ($(this).hasClass('secondaryNav-logout')) {
    //  $(this).css('background-color','#4e4c4e');
    //} else {
      $(this).css('background-color','#2e2d2e');
    //}
    $('#sec-nav-base a').css('background-color','');
    $('.secondaryNav-dropdown').hide();
  });
  $('.divider').mouseover(function(){
    clearTimeout(secNavHide);
    $('#sec-nav-base a').css('background-color','');
    $('.secondaryNav-dropdown').show();
  });

</script>


</div>

<script type="text/javascript">
var partnerLinks = {"a0a592fdbebebda5f0ce969031f3b0f9fb1c9d06":{"PARTNER_TRAFFIC_ID":"lenovo","DISPLAY_NAME":"Lenovo","LOGO":"lenovo-logo.png","APPS_PAGE":"lenovo_application-trike","PLANS_PAGE":"lenovo_compare-plans","APP_LICENSE_KEY":"a0a592fdbebebda5f0ce969031f3b0f9fb1c9d06"}}; 

function getPartnerCookieVal() {
	if (partnerIdCookie = $.cookie('app_license_key')) {
		if (partnerLinks[partnerIdCookie]) {
			return partnerLinks[partnerIdCookie];
		}
	}
	return null;
}
function getPartnerPlansLink() {
	var partnerLinks = getPartnerCookieVal();
	if (partnerLinks != null && partnerLinks['PLANS_PAGE']) {
		return partnerLinks['PLANS_PAGE'];
	}
	return null;
}
function getPartnerAppsLink() {
	var partnerLinks = getPartnerCookieVal();
	if (partnerLinks != null && partnerLinks['APPS_PAGE']) {
		return partnerLinks['APPS_PAGE'];
	}
	return null;
}
function getPartnerLogoLink() {
	var partnerLinks = getPartnerCookieVal();
	if (partnerLinks != null && partnerLinks['LOGO']) {
		return partnerLinks['LOGO'];
	}
	return null;
}
function setNavStatus(targetNav) {
	if (targetNav == 'loggedIn') {
		if ($("#navContainerDiv").hasClass('loggedInNav')) return;
		$("body").removeClass("loggedOutNav");
		$("body").addClass("loggedInNav");
		$("#navContainerDiv").removeClass("loggedOutNav").addClass("loggedInNav");
		$(".toggleNav").toggleClass("hide");
		var acctType = getCookieAcctType();
		if (acctType == 'BAS') {
			$("#uplinkNav").hide();
			$("#uplinkNav").removeClass('secondaryNav-dropdown');
		} else if (acctType == 'ENT' || acctType == 'PPP' || acctType == 'BPL') {
			var adminFlag = getCookieAdminFlag();
			if (adminFlag == 'e' || adminFlag == 't') {
				$('#sec-nav-admin').show();
				$('#sec-nav-myacct').removeClass('myacct-pos');
				$('#sec-nav-myacct').addClass('myacct-admin-pos');
			} 
		    $.post('/e?phi_action=app/getBranding',
		        function(retDat) {
		            if (retDat.brandLogo) {
		            	$('#ht_logged_in_banner').before(''+
							'<div class="span-6">'+
								'<div class="span-7" style="position:relative; z-index: 1000; line-height:19px; overflow:hidden;">'+
									'<img id="partnerlogo" src="'+retDat.brandLogo+'" style="width:160px; margin-top:4px;" />'+
								'</div>'+
							'</div>'
							);
						$('#ht_logged_in_banner, #ht_logged_out_banner').remove();
		            }
		        },'json');
			$('#ysi_upgrade').hide();
			$('#upgradeNav').remove();
		}
		if (partnerPlansLink = getPartnerPlansLink()) {
			$('#ysi_upgrade a').attr('href','/' + partnerPlansLink);
			$('#upgradeNav a').attr('href','/' + partnerPlansLink);
		}
		if (partnerAppsLink = getPartnerAppsLink()) {
			$('#appsNav a').attr('href','/' + partnerAppsLink);
		}
		if (partnerLogoLink = getPartnerLogoLink()) {
			$('#logo_img').attr('src','https://fastly.hightailcdn.com/web/en_US/theme_default/images/' + partnerLogoLink);
			$('#ht_logged_in_banner').toggleClass('span-2').toggleClass('span-6');
		}
	} else {
		if ($("#navContainerDiv").hasClass('loggedOutNav')) return;
		$("body").removeClass("loggedInNav");
		$("body").addClass("loggedOutNav");
		$("#navContainerDiv").removeClass("loggedInNav").addClass("loggedOutNav");
		$(".toggleNav").toggleClass("hide");
	}
}

</script>
    

