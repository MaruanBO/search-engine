function HIDE_STUFF(){

	if(GetCookie(FBCookieName) == "true"){

	document.getElementById("fb_like_box").className += " hidden";

	document.getElementById("fb_like_button").className += " hidden";

	document.getElementById("email_sub_box").className += " rounded";

	}

	if(GetCookie(NewsletterCookieName) == "true"){

	document.getElementById("fb_like_box").className += " rounded";

	document.getElementById("email_sub_box").className += " hidden";

	}

	if(GetCookie(NewsletterCookieName) == "true" && (GetCookie(FBCookieName) == "true")){

	document.getElementById("follow-subscribe").className += " hidden";

	document.getElementById("email_sub_box").className += " hidden";

	document.getElementById("fb_like_box").className += " hidden";

	document.getElementById("fb_like_button").className += " hidden";

	document.getElementById("like-subscribe-heading").className += " hidden";

	}

}

HIDE_STUFF();

</script>
