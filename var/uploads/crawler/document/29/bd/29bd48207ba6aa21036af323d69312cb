<script type="text/javascript">

    jQuery(document).ready(function () {
        var $target = jQuery('#webcomm_carousel_1771200');
         
         function swipeRight(){
             $target.carousel('prev');
         }
         
         function swipeLeft(){
             $target.carousel('next');
         }
         
         $target
             .find('.webcomm-carousel-item')
             .removeClass('active');
           
         $target
             .find('.webcomm-carousel-item:first')
             .addClass('active');  

         $target
                .carousel("pause")
                .removeData()
                .carousel(0)
                ;
      });

</script></div>
