<script>
$(document).ready(function() {

    // start with tabbedSearch hidden
    $('#tabbedSearch').hide();
    
    // show tabbed search when people try searching
    $('#dummySearch').on('click', function(event) {
         if( $('#tabbedSearch').is(":hidden") ) {
             $('#tabbedSearch').show();
             setTimeout( function(){
                 $('div.active input.searchbox').focus();
             }, 300);
             event.stopPropagation(); 
         }                              
    });  
    
     // hide it again when people click something else    
    $("#tabbedSearch").bind( "clickoutside", function(event){           
            if( $('#tabbedSearch').is(":visible") ) {
                $('#tabbedSearch').hide();
                $('#mobileSearchButton').removeClass("active");   
            }
    });

       
        $("#mobileSearchButton a").on("click", function(event){
            if( $('#tabbedSearch').is(":hidden") ) {            
                $('#tabbedSearch').show();
                 setTimeout( function(){
                        $('div.active input.searchbox').focus();
                    }, 300); 
                 $('#mobileSearchButton').addClass("active");   
                 event.stopPropagation();
            } else if (! $('#tabbedSearch').is(":hidden") ) {
                 $('#mobileSearchButton').removeClass("active");   
                 $('#tabbedSearch').hide();                          
            }
        });
                
        
        $("#mobileNavButton a").on("click", function(event){
            if( $('.aboutmenu').is(":hidden") ) {                        
                $('.audiencenav').slideDown('fast'); 
                $('.aboutmenu').slideDown('fast');
                $('#mobileNavButton').addClass("active");                      
            } else if (! $('.aboutmenu').is(":hidden") ) {
                $('.aboutmenu').slideUp("fast");
                $('.audiencenav').slideUp("fast"); 
                $('#mobileNavButton').removeClass("active");   
            }
        });
        
    });
    
    
    
</script>
     


