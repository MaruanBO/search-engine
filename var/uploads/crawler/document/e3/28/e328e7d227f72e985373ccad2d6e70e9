if(set==items_set_total-1){$('.d-fwd a').addClass('disabled');}else{$('.d-fwd a').removeClass('disabled');}};NewsTray.getTrayData=function(){var trayCollection=new NewsTray.TrayResultsCollection();this.trayView=new NewsTray.TrayView({collection:trayCollection});trayCollection.fetch();};NewsTray.normalizeHeights=function(group){if(tallest_item>0){group.removeAttr('style');$('.nt-container, .nt-control a').removeAttr('style');}
group.each(function(){thisHeight=$(this).outerHeight();if(thisHeight>tallest_item){tallest_item=thisHeight;}});if(tallest_item){group.height(tallest_item);$('.nt-container, .nt-control a').height(tallest_item);}};NewsTray.TrayResultModel=Backbone.Model.extend({});NewsTray.TrayResultsCollection=Backbone.Collection.extend({model:NewsTray.TrayResultModel,url:function(){return $('[data-tray-list-uri]').data('tray-list-uri');},parse:function(response){if(response.error){console.log(response.error);return[];}
