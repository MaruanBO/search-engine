<script type="text/javascript">
var rrGlobals={
rrk:false,							// kill switch - nocalls to rr
rrNoInvCall:false,					// no inventory checks will be performed
rrNoRR:false,						// temp switch - not used
rrNoInvCheckDebug:false,			// debug flag only --> ignore products not returned in stock check ajax call --> show all products
rrShowOnFail:false,					// show products if inv check fails
rrSetListen:false,					// sets listen mode
rrEarlyResult:[],
rrmanuals:[],
rrmissedpub:[],
rrItemObjArray:[],
rrCacheICheckArray:[]
};

rrGlobals.rrEarlyResult=null;
	function jsCallBack(itemObjArray)
	{
		mns.msg.pub('/log', 'mns.richRelevance callback',itemObjArray);
		if(mns.msg.pub('/richRelevance/ajaxresponse',itemObjArray,'RR')==false)
		{
			mns.msg.pub('/log', 'mns.richRelevance callback fail',itemObjArray);
			rrGlobals.rrEarlyResult=itemObjArray;  
		}
	}
	function rrAddManual(id,strategy)
	{
		rrGlobals.rrmanuals[id-1]=strategy;
	}
	function rrCacheICheck(idArray)
	{
		var ar2=[];
		for (id in idArray)
		{
			if (idArray.hasOwnProperty(id))
			{
				ar2[idArray[parseInt(id,10)]-1]=true;
			}
		}
		rrGlobals.rrCacheICheckArray[rrGlobals.rrCacheICheckArray.length]=ar2;	
	}
	
	


	rrGlobals.rrNoInvCheckDebug=false; // debug flag only, true --> ignore products not returned in stock check ajax call --> show all products
	// true - no inventory check performed - show all products
	if(false){
		rrGlobals.rrNoInvCall = false;
	}else{
		rrGlobals.rrNoInvCall = true;
	}
	rrGlobals.rrShowOnFail=false;	// true - show all products on inventory check failure , json error,404 etc.
	
	rrGlobals.rrk=0;			// true - recomendations kill switch	
	
	
	rrGlobals.rrNoRR=false;		// true - with above switch - allow manual recommendations
	rrGlobals.rrSetListen=false;		// sets listen only mode
	</script>

	<script type="text/javascript"
		src='//media.richrelevance.com/rrserver/js/1.0/p13n.js'>
	</script>
	
<script type="text/javascript" >
		if(!Modernizr.touch || true){
			if (true || (false && (!rrGlobals.rrk || rrGlobals.rrk == 0) && !rrGlobals.rrSetListen))
			{
			
				function createGuid()
				{
				    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
				        var r = Math.random()*16|0, v = c === 'x' ? r : (r&0x3|0x8);
				        return v.toString(16);
				    });
				}
							
				if(mns.head.readCookie('GUID') == "" || mns.head.readCookie('GUID') == undefined){					
				document.cookie='GUID='+createGuid();
				}	
							
				var R3_COMMON = new r3_common();
				var userType = 'G';
				R3_COMMON.addContext({},true);
				R3_COMMON.setBaseUrl('//recs.richrelevance.com/rrserver/');
				R3_COMMON.setApiKey('1a855e51d76188dd');
				R3_COMMON.setClickthruServer(window.location.protocol+"//"+window.location.host);
				var cookie_GuId = mns.head.readCookie('GUID');
				if( cookie_GuId !== undefined ){
					var rrUserJID = cookie_GuId;
					R3_COMMON.setSessionId(rrUserJID);
					R3_COMMON.setUserId(rrUserJID);
					var rrUserInfo = mns.head.readCookie('MS_USER_COOKIE');
					if((rrUserInfo !== undefined) && (rrUserInfo !== ""))
					{
						var userIds = rrUserInfo.split('_');
						var externalUserId = userIds[1];
						R3_COMMON.setUserId(externalUserId);
					}
				}
		
					R3_COMMON.addPlacementType('home_page.promo_1');
				
				R3_COMMON.addPlacementType('home_page.rcnt1');
				var R3_HOME = new r3_home();
			
		if (rrGlobals.rrNoRR==false && cookie_GuId !== undefined)
			{
				rr_flush_onload();
				r3();
			}
			}
		}
</script>
