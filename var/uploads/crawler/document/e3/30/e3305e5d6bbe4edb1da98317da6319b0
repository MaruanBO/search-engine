                var exception = ";;end";
                    $(document).ready(function() {
                        var url = window.location.href;
                        if (url.indexOf("lang=") != -1) {
                            var part1
                            if (url.indexOf("&lang=") != -1) {
                                part1 = url.substring(0, url.indexOf("lang=")-1);
                            }
                            else {
                                part1 = url.substring(0, url.indexOf("lang="));
                            }
                            var part2 = "";
                            var count;
                            if (url.indexOf("pt/br") != -1) {
                                count = 11;
                            }
                            else {
                                count = 8;
                            }
                            part2 = url.substring(url.indexOf("lang=") + count);
                            url = part1 + part2;
                        }

                        if (url.indexOf("?") != -1) {
                            url += "&";
                        }
                        else {
                            url += "?";
                        }
                        $('[id*="langLi"]').each(function() {

                            var hrf = $(this).attr("href");
                            $(this).attr("href", url + hrf);
                        });
                        var isOpen = false;
                        $(function() {
                            $("a#lang_selector_link").click(function(event) {

                                event.preventDefault();
                                if (!isOpen) {

                                    $("ul#languages_available").css("display", "block");

                                    isOpen = true;
                                }
                                else {

                                    $("ul#languages_available").css("display", "none");

                                    isOpen = false;

                                }
                            });
                        });



                    })
                
                
                
                
                
                
