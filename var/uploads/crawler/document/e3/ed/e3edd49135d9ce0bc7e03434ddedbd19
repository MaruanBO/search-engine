<script language="javascript" data-strip-mobile="true">
 function stcountdown(obj) {
	this.obj = obj;
	this.span = 'stlearnmorecopy';
	this.countdown_active = true;
	this.countdown_string;
	this.countdown_format = '<span>%%D%% DAYS LEFT</span>';
	this.calculate = countdown_calculate_length;
	this.countdown_message = countdown_message_string;
	this.setup = countdown_setup;
 }
 function countdown_calculate_length(secs, num1, num2) {
	s = ((Math.floor(secs/num1))%num2).toString();
	if (s.length < 2) s = s;
	return (s);
 }
 function countdown_message_string(secs) {
	if (secs < 0) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">THIS OFFER IS EXPIRED</span>'; 
	} else if (secs < 86400) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">ENDS TODAY</span>'; 
	} else if (secs < 172800) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">ENDS TOMORROW</span>'; 
	} else if (secs < 259200) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">2 DAYS LEFT</span>'; 
	} else if (secs < 345600) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">3 DAYS LEFT</span>'; 
	} else if (secs > 345600) { 
	 document.getElementById(this.span).innerHTML = '<span id="stbanner" class="stbanner yellow">LIMITED TIME</span>'; 
	} else {
	 this.countdown_string = this.countdown_format.replace(/%%D%%/g,  this.calculate(secs,86400,100000));
	 document.getElementById(this.span).innerHTML = this.countdown_string;
	 if (this.countdown_active) setTimeout(this.obj +'.countdown_message(' + (secs-1) + ')', 990);
	}
 }
 function countdown_setup(){
	var d_final = new Date(this.final_date);
	var d_today = new Date();
	d_diff = new Date(d_final-d_today);
	g_secs = Math.floor(d_diff.valueOf()/1000);
	this.countdown_message(g_secs);
 }
 var end_date = new stcountdown('end_date');
 end_date.final_date = '02/28/2015 11:59 PM';
 end_date.setup();
</script>                           </div>
