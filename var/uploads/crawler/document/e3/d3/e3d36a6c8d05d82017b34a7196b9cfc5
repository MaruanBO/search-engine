	<script type="text/javascript">
		cy.FunnelLevel="0";			
		

// Pass Email from URL parameter if exists
var emailParamName = "cyEmail";

function getQueryString(){   
	var result = {}, queryString = location.search.substring(1), re = /([^&=]+)=([^&]*)/g, m;  
	while (m = re.exec(queryString)){     
		result[decodeURIComponent(m[1])] = decodeURIComponent(m[2]);
	}   
	return result; 
} 

function processMarketingLink(){
	if(!cy.UserId || cy.UserId == ''){
		//candidate, so test url
		var id = getQueryString()[emailParamName]; 
		if(id){
			cy.UserId = getQueryString()[emailParamName]; 
			cy_getImageSrc();
		}
	}else{
		cy_getImageSrc();
	}
}

processMarketingLink();


// Capture Email from input fields
function CYEmailCapture(elId){
	var CYEmailReg = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,6}$/i;
	if (!cy.UserId || cy.UserId == "" && document.getElementById(elId) && document.getElementById(elId).value.match(CYEmailReg)){
		var CyEmailValue = document.getElementById(elId).value;
		cy.UserId=CyEmailValue;
		cy_getImageSrc(); 
	}
}

jQuery("#emailsignupinput").change(function(){
	CYEmailCapture("emailsignupinput");
});
jQuery("#email_exclusive").change(function(){
		CYEmailCapture("email_exclusive");
});
jQuery("#dwfrm_guestEmail_emailAddress").change(function(){
	CYEmailCapture("dwfrm_guestEmail_emailAddress");
});
jQuery("#dwfrm_login_username").change(function(){
	CYEmailCapture("dwfrm_login_username");
});
jQuery("#dwfrm_profile_customer_email").change(function(){
	CYEmailCapture("dwfrm_login_username");
});
jQuery("#dwfrm_billing_billingAddress_email_emailAddress").change(function(){
	CYEmailCapture("dwfrm_login_username");
});
	</script>
