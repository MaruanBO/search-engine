<script type="text/javascript">
	//Further CPO Customization:
	
	function appendText(that, text, i){
		var currentHref = jQuery(that).attr('href');
		if(currentHref.match(/\?/)){
			currentHref = currentHref + '&';
		}else{
			currentHref = currentHref + '?';
		}
		currentHref = currentHref + text + i;
		jQuery(that).attr('href',currentHref);
	}
		
	jQuery(document).ready(function(){

	 	
			//append parameter for other cross-sell links
		 	jQuery(".recCont a[href]").each(function(i){
		 			var text = '';
					
					
					appendText(this, text, i);
		 	});
		 	
		 	
		 	
		 	
	 	
	});


	//append parameter on carousel links
	jQuery(".hpcarousel a[href]").each(function(i){		
		appendText(this, 'carousel=', i);
	});

</script>



