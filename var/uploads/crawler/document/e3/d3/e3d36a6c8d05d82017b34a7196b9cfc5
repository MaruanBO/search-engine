		<script type="text/javascript">
		
		function miniitemcheck(e) {
			//If function is run when Item is Added to Cart or On Page load w/ Items Avail in Basket
			if(e == 'add' || 0.0 > 0) {
				jQuery("a.linkminicart").unbind("click");
				
				jQuery("#minicart").removeClass('noitems');
				
				jQuery('.minicarttotal').mouseenter( function() {
					jQuery('.minicart .minicarttotal').addClass('hoverminicrt');
					jQuery('.minicart .hoverminicrt').removeClass('minicarttotal');
				});
				jQuery('.minicarttotal').mouseleave( function() {
					jQuery('.minicart .hoverminicrt').addClass('minicarttotal');
					jQuery('.minicart .minicarttotal').removeClass('hoverminicrt');						
				});
			} 
			//If function is run on Page load and No Items are Avail in Basket
			else {
				jQuery("a.linkminicart").bind('click', function(e) {
					e.preventDefault();
				});
			}
		};
		
		jQuery(document).ready(function() {
			miniitemcheck();
		});			
		</script>
		<div class="clear"></div>
	</div><!-- END:minicarttotal -->
	
