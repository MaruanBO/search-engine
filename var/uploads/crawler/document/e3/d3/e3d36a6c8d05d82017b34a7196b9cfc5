<script type="text/javascript">
//Prepares Main HP Images


var hpSlider = document.getElementById("hp-slider"),
    imgs = hpSlider.getElementsByTagName("img");
ssm.addStates([{
    id: "under470",
    maxWidth: 470,
    onEnter: function() {
        for (var e = 0; e < imgs.length; e++)
            if (0 == e) {
                var t = imgs[e].getAttribute("data-src-sm");
                imgs[e].src = t, imgs[e].className = "loaded"
            }
    },
    onLeave: function() {
        for (var e = 0; e < imgs.length; e++)
            if (e > 0) {
                var t = imgs[e].getAttribute("data-src-lg");
                imgs[e].src = t
            }
    }
}, {
    id: "over470",
    minWidth: 471,
    onEnter: function() {
        for (var e = 0; e < imgs.length; e++)
            if (0 == e) {
                var t = imgs[e].getAttribute("data-src-lg");
                imgs[e].src = t, imgs[e].className = "loaded"
            }
    },
    onLeave: function() {
        for (var e = 0; e < imgs.length; e++)
            if (e > 0) {
                var t = imgs[e].getAttribute("data-src-sm");
                imgs[e].src = t
            }
    }
}]).ready();

// Add a script element as a child of the body
function downloadJSAtOnload(){var e=document.createElement("script");e.src="http://demandware.edgesuite.net/aaea_prd/on/demandware.static/Sites-cpooutlets-Site/-/default/v1424344833005/js/homepagejs.js";document.body.appendChild(e)}if(window.addEventListener)window.addEventListener("load",downloadJSAtOnload,false);else if(window.attachEvent)window.attachEvent("onload",downloadJSAtOnload);else window.onload=downloadJSAtOnload;
	

jQuery(document).ready(function(){
	if(typeof overlay != 'undefined'){
 		overlay({'selector':'div.producttile'});
	}
});

jQuery(window).load(function() {
	//Responsive JS Breakpoints 
	(function(){
		ssm.addStates([          
		    {		    	
		    	maxWidth: 875,
		    	onEnter: function() {
		    		jQuery(".promoWrap").touchCarousel({
		    			itemsPerMove: 1,
						pagingNav: true,
		    			dragUsingMouse: false,
						scrollbarAutoHide: true,
						transitionSpeed: 200
		    		});	
		    	},
		    	onLeave: function() {
		    		carouselDestroy(jQuery(".promoWrap"));
		    	}
		    }
		]).ready();
	}());
});

