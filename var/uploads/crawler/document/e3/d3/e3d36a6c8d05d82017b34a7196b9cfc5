var recProdHtml = decodeURIComponent(recJsonObj[i].products);
var recScheme = '.' + recJsonObj[i].scheme;
var recDiv = '.' + recJsonObj[i].div;
var recClass = recJsonObj[i].render;
var recTitle = decodeURIComponent(recJsonObj[i].title);
// select certona div with appropriate scheme & div class names
// populate with product data
// add class, then make visible
jQuery(recScheme + recDiv).html(recProdHtml).addClass(recClass).css('display', 'block');
}
}
// resx and resx.links are for Certona resonance(.isml) reporting
if(typeof resx =="undefined") {
var resx = new Object();
}
resx.links = "";
resx.pageid = recJsonObj[0].pageid;

</script>
		
