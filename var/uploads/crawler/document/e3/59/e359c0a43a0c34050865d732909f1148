<form action="/" method="post"><input class="search-modelNumber" data-val="true" data-val-required="Model Number required." id="ProductCode" name="ProductCode" placeholder="model number" type="text" value="">            <input type="submit" class="button small" value="Go" id="guidesAndManualsSubmit">
