<script>
    $(function () {
        $("#guidesAndManualsSubmit").parents('form').validate();

        $('#guidesAndManualsSubmit').click(function (e) {
            if (!$(this).parents('form').hasClass('submitting')) {
                e.preventDefault();
                $('form').addClass('submitting');
                frigidaire.sendTracking('homepage', 'click_manual', document.getElementById('ProductCode').value);

                if ($("#guidesAndManualsSubmit").parents('form').valid()) {
                    setTimeout(function () {
                        $("#guidesAndManualsSubmit").parents('form').submit();
                    }, 200);
                }
            }
        });
    });
</script>
