<script type="text/javascript">
var totalCartItemsValue ="0";                                   
function getTotalCartItemsData()
{

	var country = "SG";
	var lang = "en";
	var dynUrl ="//buy.norton.com";
	
	var trfid = "nortoncom";
	scsguid = "";
	
if(scsguid!=null && scsguid!=""){
	
	
		var url = dynUrl + '/redirector/seo?ptype=cartpopover&COUNTRY=' + country + '&LANGUAGE=' + lang +'&scsguid='+ scsguid + '&trf_id='+ trfid + '&callback=?';
		
	
		
		
		jQuery.getJSON(url, function(totalCartItemsResponseData) {
			totalCartItemsValue ="0";
			for (var totalCartItems in totalCartItemsResponseData) {
				if (totalCartItemsResponseData.hasOwnProperty(totalCartItems)) {
					
					totalCartItemsValue = totalCartItemsResponseData[totalCartItems];
					if(totalCartItemsValue!='0' && totalCartItemsValue!=''){
						
						
						document.getElementById("cart").innerHTML="(" + totalCartItemsValue+ ")";
						
					}
				
				break;
				}
			}
		}
		);
		}
}                
function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
	var c = ca[i];
	while (c.charAt(0)==' ') c = c.substring(1,c.length);
	if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}
function getCookie(c_name)	{
	var i,x,y,ARRcookies=document.cookie.split(";");
	for (i=0;i<ARRcookies.length;i++)
		  {
		  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
		  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
		  x=x.replace(/^\s+|\s+$/g,"");
		  if (x==c_name)
		    {
		    return unescape(y);
		    }
	}
}                           
