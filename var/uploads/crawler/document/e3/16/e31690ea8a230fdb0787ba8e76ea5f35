        $('#adowl-order-form > div.mainView > fieldset > legend:contains("Online Content")').after("<p>To enhance your online ad, please fill out the sections below. To add one or more photos, use the link at the bottom. Photos must be uploaded separately for online ads even if they have already been uploaded for a print ad.</p>");
 });
</script>

<script>
$(window).load(function() {
var sAuthtoken = $.cookie('tncms-authtoken');
var sUrl = 'https://service.freelancestar.com/cgi-bin/pbs/process/custservice/web/addrfind.html';
if(typeof(sAuthtoken)!='undefined'){
    $("a#notice-subscribe,a#notice-subscribe-manager-pop").attr('href', sUrl);
}else{
    $("#subscribe-area").find("a").each(function(){
        var sHref = $(this).attr("href").replace(/\&referer_url\=.*?\&/g,"&referer_url="+encodeURIComponent(sUrl)+"&");
        var sHref = sHref.replace("&driver=subscription","");
        var sHref = sHref.replace(/serviceid\=.*?\&/g,"");
        $(this).attr("href",sHref);
    });
}    
});
</script>


