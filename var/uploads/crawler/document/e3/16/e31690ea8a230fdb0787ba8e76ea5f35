        function onAfter1001(curr1001, next1001, opts1001) {
            // commented out by cricketts 2.11.11
            //var index1001= opts1001.currSlide;
            //$('#prev1001')[index1001== 0 ? 'hide' : 'show']();
            //$('#next1001')[index1001== opts1001.slideCount - 1 ? 'hide' : 'show']();
            
                imageItem = $('#slideshow1001').find('.business').eq(opts1001.currSlide).find('.logo-clip img');
                iWidth = imageItem.width();
                iHeight = imageItem.height();
                
                if(iWidth && iHeight){
                    if(iWidth > iHeight){
                        imageItem.css("width", "150px");
                        imageItem.css("height", "auto");
                    } else {
                        imageItem.css("width", "auto");
                        imageItem.css("height", "80px");
                    }
                }
        }
        function pagerFactory1001(idx1001, slide) {
            var s1001= idx1001> 6 ? ' style="display:none"' : '';
            return '<li'+s1001+'><a href="#">'+(idx1001+1)+'</a></li>';
        };
    
    
