		<!-- Establish overlay trigger click handler and run the Orbit plugin. -->
		<script type="text/javascript">	
			$(window).load(function() {
				// In the overlayTrigger click handler, iterate through set of overlay triggers.  If the 
				// current trigger is 'this', then toggle() the associated overlay (associated as an 
				// attribute of overlayTrigger elements right now).  If current trigger is not 'this' 
				// object, then hide() the associated overlay.
				var overlayCloseButtons = $('.overlayCloseButton');
				if (overlayCloseButtons.length) {
					overlayCloseButtons.click(function() {
						$('.overlay').each(function() {
							$(this).hide();
						});
					});
				}
				var overlayTriggers = $('.overlayTrigger');
				if (overlayTriggers.length) {
					overlayTriggers.click(function() {
						var overlayTriggers = $('.overlayTrigger');
						for (var i = 0; i < overlayTriggers.length; i++) {
							if ($(overlayTriggers[i]).attr('overlay') != undefined) {
								if ($(this).is(overlayTriggers[i])) {
									if ($($(overlayTriggers[i]).attr('overlay')).is(':hidden')) {
										var gaCategory = $(overlayTriggers[i]).attr('ga_category');
										var gaAction = $(overlayTriggers[i]).attr('ga_action');
										var gaLabel = $(overlayTriggers[i]).attr('ga_label');
										if (gaCategory != undefined && gaAction != undefined && gaLabel != undefined) {
											trackGaEvent(gaCategory, gaAction, gaLabel);
										}
									}
									$($(overlayTriggers[i]).attr('overlay')).toggle();
								} else {
									$($(overlayTriggers[i]).attr('overlay')).hide();
								}
							}
						}
						return false;
					});
				}
			
				$('#featured').orbit({
					animation: 'fade', 
					bullets: true,
					afterSlideChange: function(){ // fires when Slider changes slides
						pauseVideo(); //pause slide's video
					}
				});
				
				var sliderPauseButton = $('.pause');
				if (sliderPauseButton.length) {
					sliderPauseButton.click(function() {
						trackGaEvent('homepage_pause', 'pause_click', 'pause_click');
					});
				}
				var sliderRightButton = $('.right');
				if (sliderRightButton.length) {
					sliderRightButton.click(function() {
						trackGaEvent('homepage_right-arrow', 'right_click', 'right_click');
					});
				}
				var sliderLeftButton = $('.left');
				if (sliderLeftButton.length) {
					sliderLeftButton.click(function() {
						trackGaEvent('homepage_left-arrow', 'left_click', 'left_click');
					});
				}
			});
			
			
			// This code loads the IFrame Player API code asynchronously.			
			var tag = document.createElement('script');
			tag.src = "//www.youtube.com/iframe_api";
			var firstScriptTag = document.getElementsByTagName('script')[0];
			firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);					
			
			var player;
			function onYouTubeIframeAPIReady() {
					var $contentdivs = $('#videoSliderDiv').find('#videoFrameDiv');
					$contentdivs.each(function(i){ // loop through the content divs
						var $contentdiv = $(this);							
							player = new YT.Player('ytiframe', {
								height: '281',
								width: '500',
								videoId: 'o-lp8RBcFVk',
								playerVars: {'rel': 0, 'wmode':'transparent', 'origin':'', loop:1},
								events: {
									'onReady': onPlayerReady,
									'onStateChange': onPlayerStateChange
								}
							});
							$contentdiv.data("youtubeplayer", player) // store Youtube API player inside contentdiv object
					})
			}
			
			function onPlayerReady(event) {
				//player.playVideo();
			}
 
			function onPlayerStateChange(event,element) { 
				var _pauseFlag = false;

				if (event.data ==YT.PlayerState.PLAYING){
					//alert('playing');
					//_pauseFlag = false;
					if (!player.cmTracked) {
						var elementCategory = "";
						var attrString = "built-to-work";
						attrString += "-_--_--_--_--_--_--_--_--_--_--_--_-2-_--_-";
						elementCategory = "HOME-SLIDER-VIDEO";
						cmCreateElementTag(elementCategory, "YOUTUBE", attrString);
						player.cmTracked = true;
					}
				} 
 
				if (event.data ==YT.PlayerState.ENDED){
					//alert('Ended');
				} 
 
				if (event.data ==YT.PlayerState.PAUSED && _pauseFlag == false){
					//alert('paused');
					//_pauseFlag = true;
				}
 
				if (event.data ==YT.PlayerState.BUFFERING){
					//alert('buffering');
				}
 
				if (event.data ==YT.PlayerState.CUED){
					//alert('Cueing');
				}
				
			} 
			
			function resetvideo(){
				if (YT.PlayerState.PLAYING == 1) {
					//The conclusion I've come to is that the only way to stop a video
					//in all browsers is to reset the embedded src="" with the same URL.
					var video = $("#ytiframe").attr("src");
					$("#ytiframe").attr("src","");
					$("#ytiframe").attr("src",video);
				}
			}

			function pauseVideo(){
				if (typeof player != "undefined"){
					player.pauseVideo();
				}
			}
			
		</script>		

