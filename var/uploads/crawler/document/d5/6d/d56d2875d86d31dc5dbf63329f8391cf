<script id="coremetricsScript" data-mobile-site="false" type="text/javascript">



var WebAnalytics = { Page: { PageIdentifier: "" } };

webanalytics = {};

cmSetupOther({"cm_TrackImpressions":""});

var CM_SKU_FLAG_GUARANTEED_IN_STOCK_INDEX = 5;
var CM_SKU_FLAG_GUARANTEED_IN_STOCK_NAME = "skuFlagGuaranteedInStock";

var CM_PRODUCT_CATEGORY_INDEX = 9;
var CM_PRODUCT_CATEGORY_NAME = "productCategory";

var CM_MOBILE_PAGE_ID_PREFIX = "m:";
var CM_SITE_INDICATOR_INDEX = 19;
var CM_SITE_INDICATOR_NAME = "siteIndicator";
var CM_DESKTOP_SITE = "w";
var CM_MOBILE_SITE = "m";

// Determine if this is www or mobile site utilizing flag on this script element.
var siteIndicator = CM_DESKTOP_SITE;
var cmScript = $("#coremetricsScript");
if (cmScript.length > 0) {
    if ("true" === cmScript.attr("data-mobile-site")) {
        siteIndicator = CM_MOBILE_SITE;
    }
}


	
appendPageGroupId("allpages");  // Define constant for this.


var pageViewAttrHash = [];
pageViewAttrHash[CM_SITE_INDICATOR_INDEX] = {name: CM_SITE_INDICATOR_NAME, value: siteIndicator};
var pageViewAttributes = createAttributeString(pageViewAttrHash);
var pageIdStr = siteIndicator == CM_MOBILE_SITE ? CM_MOBILE_PAGE_ID_PREFIX + location.pathname : location.pathname;
webanalytics.page = {pageid: pageIdStr, categoryid:"9852723696502240", searchterm:"", searchresults:"", attributes:pageViewAttributes, extrafields:""};

    
function createAttributeString(hash) {
    var attrString = "";
    
    // Iterate through hash.
    if (hash) {
        for (i = 0; i < hash.length && i < 50; i++) {
            if (i > 0) {
                attrString += "-_-";
            }
            if (hash[i] && hash[i].value) {
                attrString += hash[i].value;
            }
        }
    }
    
    return attrString;
}


function appendPageGroupId(pageGroupId) {
    if ("" != WebAnalytics.Page.PageIdentifier) {
    	WebAnalytics.Page.PageIdentifier += "_"
    }
    WebAnalytics.Page.PageIdentifier += pageGroupId;
}

</script>


