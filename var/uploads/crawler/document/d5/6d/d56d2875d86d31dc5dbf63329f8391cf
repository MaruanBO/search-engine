	<script>
    $(document).ready(function() {
    	$("#searchFieldSubmitLink").click(function() {
    		// return checkSearchField(this, 'Please enter search criteria.', 'Enter Keywords');
    		if (checkSearchField({}, 'Please enter search criteria.', 'Enter Keywords')) {
    			var searchField = document.getElementById("searchfield");
    			// console.log("this.href: " + this.href + "?query=" + searchField.value);
    			// var searchUrl = this.href + "?query=" + searchField.value;
    			// console.log("searchUrl: " + searchUrl);
    			window.location.href = this.href + "?query=" + searchField.value;
    			// window.location.href = searchUrl;
    			return false;
    		}
    	});
		// TODO: Get this handler tested on a non-US keyboard.
		$("#searchfield").keypress(function(e) {
			if (e.which === 13) {
				e.preventDefault();
				// $("#searchFieldSubmitLink").click();
				var searchFieldSubmitLink = document.getElementById("searchFieldSubmitLink");
	    		// if (checkSearchField(searchFieldSubmitLink, 'Please enter search criteria.', 'Enter Keywords')) {
	    		if (checkSearchField({}, 'Please enter search criteria.', 'Enter Keywords')) {
	    			// window.location.href = searchFieldSubmitLink.href;
	    			window.location.href = searchFieldSubmitLink.href + "?query=" + this.value;
	    		}
			}
		});
	});
	</script>

