            <!-- primary-category-nosh -->


			<div id="index-post-first" class="post-184228 post type-post status-publish format-standard has-post-thumbnail hentry category-food-nosh category-food-featured category-nosh category-people-nosh tag-albany-high-school tag-based-god tag-brandon-mccartney tag-follow-your-heart tag-lil-b tag-the-pack tag-veganaise tag-vegemoji clearfix primary-category-nosh">

								<div class="primary-category-icon"></div>
				<div class="post-categories">
                	<div class="post-categories-links">
						<a href="http://www.berkeleyside.com/category/nosh" rel="tag">Nosh</a>                    </div>
