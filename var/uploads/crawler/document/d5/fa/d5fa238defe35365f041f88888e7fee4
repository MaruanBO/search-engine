<script>
var XO = XO || {};

XO.TNScrollingShareBar = (function(){
  var windowY = window.pageYOffset,
  showing = true,
  _dependencies = [{
    name: 'jquery',
    url: '//www.xoedge.com/jscripts/jquery/jquery-1.7.1.min.js',
    is_included: function() {
      return typeof jQuery !== 'undefined' && _dotVersionToInt(jQuery().jquery) >= 171;
    }
  }],
  _IELessThan10 = function() {
    var div = document.createElement("div");
    div.innerHTML = "<!--[if lt IE 10]><i></i><![endif]-->";
    return (div.getElementsByTagName("i").length == 1);
  },
  _dotVersionToInt = function(version_number) {
    return parseInt(version_number.replace(/\./g, ''), 10);
  },
  _loadDependencies = function(callback) {
    if (_dependencies.length > 0) {
      var dependency = _dependencies.shift();
      if (!dependency.is_included()) {
        $.getScript(dependency.url, function(data, textStatus, jqxhr) {
          _loadDependencies(callback);
        });
      } else {
        _loadDependencies(callback);
      }
    } else { /* all loaded */
      callback();
    }
  };

  return{
    shareBarDependencies: function(){
      _loadDependencies(this.readyScroll);
    },
    getDistance: function(topID, bottomID){
      var topBar = $(topID);
      var bottomBar = $(bottomID);
      if (topBar.length==0||bottomBar.length==0) return Number.MAX_VALUE;
      var topBarY = topBar.offset().top;
      var bottomBarY = bottomBar.offset().top;
      var distance = bottomBarY - topBarY;
      return distance;
    },
    scrollFunc: function(topID, bottomID){
      var topBar = $(topID);
      var bottomBar = $(bottomID);
      if (topBar.length==0||bottomBar.length==0) return;
      var topBarY = topBar.offset().top;
      var topBarHeight = topBar.height();
      var bottomTopBarY = topBarY + topBarHeight;
      if(windowY>bottomTopBarY && showing==false){ //when top bar disappears fully, show the bottom bar
        bottomBar.animate({height:'show'}, "slow", function(){bottomBar.show()});
        showing = true;
      }
      if(windowY<topBarY && showing){//when top bar appears fully, hide bottom bar
        bottomBar.animate({height:'hide'}, "slow", function(){bottomBar.hide()});
        showing = false;
      }
    },
    readyScroll: function(){
      var bars = $(".sharingArticle");
      $(bars.get(0)).attr("id", "topSharingArticleBar");
      $(bars.get(1)).attr("id", "bottomSharingArticleBar");
      if (bars.length<2) return;
      $("#bottomSharingArticleBar").animate({height:'hide'}, "fast", function(){$("#bottomSharingArticleBar").hide()});
      $(window).scroll(function(){
        if (window.innerHeight > XO.TNScrollingShareBar.getDistance('#topSharingArticleBar', '#bottomSharingArticleBar')){
          newWindowY = window.pageYOffset;
          if (Math.abs(windowY-newWindowY)/window.innerHeight * 100 > 2.5){
            windowY = newWindowY;
            XO.TNScrollingShareBar.scrollFunc("#topSharingArticleBar", "#bottomSharingArticleBar");
          }
        }else{
          $("#bottomSharingArticleBar").animate({height:'show'}, "fast", function(){$("#bottomSharingArticleBar").show()});
          return;
        }
      })
    }
  }
})();

$(document).ready(XO.TNScrollingShareBar.readyScroll);
</script>
