</div>
<div style="position: relative; padding: 6px; width: 656px;" class="panel"><a href="http://www.nhl.com/ice/gallerylanding.htm?id=7059&amp;navid=mod-left3"><img width="656" height="150" border="0" src="http://3.cdn.nhle.com/nhl/en/v3/ext/frozenmoment/frozen_banner.jpg" alt="" /></a></div></div></div></div><div id="medCol"><script language="javascript" type="text/javascript">
	jQuery(document).ready(function() {
	  	initRightRailTabs();
	});
	function initRightRailTabs() {
		jQuery("#rrLeftTab").bind("click", function(e) {
			toggleRightRailTab("left")
		});
		jQuery("#rrRightTab").bind("click", function(e) { 
			toggleRightRailTab("right");
		});
	}
</script><script language="javascript" type="text/javascript">
	function toggleRightRailTab(tab) {	
		switch(tab) {
			case "left":
				jQuery('#rrLeftTab').addClass('active');
				jQuery('#rrLeftTabBody').show();
				jQuery('#rrRightTab').removeClass('active');
				jQuery('#rrRightTabBody').hide();
			break;
			case "right":
				jQuery('#rrLeftTab').removeClass('active');
				jQuery('#rrLeftTabBody').hide();
				jQuery('#rrRightTab').addClass('active');
				jQuery('#rrRightTabBody').show();
			break;
		}
	}
</script><div id="tabbedRightRail"><h3 class="rrTab active" id="rrLeftTab">TOP HEADLINES</h3><h3 class="rrTab" id="rrRightTab">NHL GREEN</h3><div class="tabModContainer" id="rrLeftTabBody"><div id="c_mrmTbLft"><div id="mrmTbLft" class="medColMod"><div class="modContainer topHeads">
