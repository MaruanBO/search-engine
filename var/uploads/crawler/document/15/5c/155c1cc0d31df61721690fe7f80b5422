<script>



$(document).ready( function(){
    var subnav_timeout;
    $("#gallery-nav li.top").hover(function () {
        var dropdown = $('.dropdown[id="' + $(this).attr('data-id') + '"]');
        dropdown.css('left', + $(this).parent().position().left - 20);
        if ($(window).width() < 1100 ) {
            var last_dropdown = $('#gallery-nav li.top:last-of-type ul.dropdown');
            last_dropdown.css('left', + $(this).parent().position().left - 115)
            last_dropdown.find('li').css('textAlign', 'right')

        }
        dropdown.stop(true,true);
        clearTimeout(subnav_timeout);
        subnav_timeout = setTimeout( function() {
                $('.dropdown').not(dropdown).slideUp(50)
                dropdown.slideDown(50)}
            , 30
        )
    }, function () {
        var dropdown = $('.dropdown[id="' + $(this).attr('data-id') + '"]');
        dropdown.parent().removeClass('active');
        dropdown.stop(true,true);
        clearTimeout(subnav_timeout);
        subnav_timeout = setTimeout( function() {
            dropdown.slideUp(50)}
            , 30
        )
    });
});
</script>

