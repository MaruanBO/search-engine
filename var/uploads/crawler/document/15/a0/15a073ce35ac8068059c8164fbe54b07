    <script type="text/javascript">

        $(document).ready(function () {

            var widthForPanels;
            $('.leftContainer').css("white-space", "nowrap");

            //To modify style of controls when invalid login error is shown
            if ($("span[id$=lblSorry]").is(':visible')) {
                modifyStyleForLoginPage();
            }

            //To modify style of controls when Reset password information message is shown
            if ($("div[id$=dvWarning]").is(':visible')) {

                var heightOfWarningMsg = parseInt($('div[id$=dvWarning]').height());
                modifyStyleForLoginPage();
                if (heightOfWarningMsg > 100) {  //If Height of warning message is making it overflow the panel ,handle padding only at that time
                    var paddingForControls = Math.round(heightOfWarningMsg / 4.2);
                    $('.form.panelLeft').css("padding-bottom", paddingForControls + 'px');
                    $(".form.panelRight").css("padding-bottom", paddingForControls + 'px');
                    $('.advertisement').css("padding-top", Math.round(paddingForControls / 2 - 5) + 'px');
                }
            }
        });

        function modifyStyleForLoginPage() {
            widthForPanels = calculateWidth();
            $('.form.panelLeft').css("width", (widthForPanels + 230) + 'px');
            $('.form.panelRight').css("width", (widthForPanels + 230) + 'px');
            $('.form.panelRight').css("text-align", 'center');
            $('.loginContainer').css("max-width", ((widthForPanels + 230) * 2 + 30) + 'px');
            $('.leftContainer').css("width", (widthForPanels + 5) + 'px');
        }

        function calculateWidth() {
            var widthRemember = parseInt($('span[id$=ltRememberMeText]').width());
            var widthForgotPassword = parseInt($('a[id$=aForgotPassword]').width());
                if (widthRemember >= widthForgotPassword) {
                    return (widthRemember + 24);
                }
                else
                    return (widthForgotPassword + 4);
        }

        function resetThePassword() {
            if (document.getElementById('cphForgotPassword_txtForgotPass').value.trim() == '' || document.getElementById('cphForgotPassword_txtForgotPass').value.trim() == document.getElementById('cphContent_hdnUserName').value.trim()) {
                alert(document.getElementById('cphContent_hdnPleaseEnterUserName').value.trim());
                return false;
            }
            else {
                return true;
            }
        }

        function resetPasswordOnEnter(e) {
            if (e.keyCode == 13) {
                document.getElementById('cphForgotPassword_lnkBtnForgotPass').click();
                return false;
            }
            return true;
        }

        function onFocus() {
            if (document.getElementById('cphForgotPassword_txtForgotPass').value.trim() == document.getElementById('cphContent_hdnUserName').value.trim()) {
                document.getElementById('cphForgotPassword_txtForgotPass').value = '';
            }
        }

        function onBlur() {
            if (document.getElementById('cphForgotPassword_txtForgotPass').value == '') {
                document.getElementById('cphForgotPassword_txtForgotPass').value = document.getElementById('cphContent_hdnUserName').value.trim();
            }
        }
    </script>
