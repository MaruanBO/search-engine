<script type="text/javascript">

    function setFocus() {
        if (document.getElementById('cphContent_txtUserName') != null)
            document.getElementById('cphContent_txtUserName').focus();
        else {
            if (document.getElementById('cphContent_txtNewPassword') != null)
                document.getElementById('cphContent_txtNewPassword').focus();
        }
    }
    
    String.prototype.trim = function() {
        return this.replace(/^\s+|\s+$/g, "");
    }

    //Script for assigning a height to .loginContainer so that the footer always aligns to the bottom of the page.
    $(document).ready(calcHeight);

    $(window).resize(calcHeight);

    function calcHeight() {
        var docheight = parseInt($(window).height());
        var $loginPanel = $('.loginPanel'), $loginContainer = $('.loginContainer'), tempWidth, count = 0;
        docheight = docheight - 145;
        $loginContainer.css({ 'height': docheight, 'min-height': 310 });

        //Get year from the current date and display it in the footer. Reduce manual updates.
        var curYear = new Date().getFullYear();
        $('#currentYear').text(curYear);
    }
    function ForgotPassword() {
        $('#ForgotPassword').show();
        $('#SignIn').hide();
    }
    function SignIn() {
        $('#ForgotPassword').hide();
        $('#SignIn').show();
    }
    // Facebook and Twitter image hover
    $(document).ready(function () {
        $('img#facebook').hover(function () {
            $(this).attr('src', '//static.allegiancetech.com/Login/images/facebook_active.png');
        }, function () {
            $(this).attr('src', '//static.allegiancetech.com/Login/images/facebook.png');
        });

        $('img#twitter').hover(function () {
            $(this).attr('src', '//static.allegiancetech.com/Login/images/twitter_active.png');
        }, function () {
            $(this).attr('src', '//static.allegiancetech.com/Login/images/twitter.png');
        });
    }); 
</script>

