    function appendBrowserEventFrame() {
		if ($("#BrowserEventFrame").length <=0 ){
			$("<iframe>").attr("id","BrowserEventFrame").attr("src","about:blank").attr("style","display:none").appendTo("body");	
			}
	}
	
	function BrowserEvent(event,cgi){
		appendBrowserEventFrame();
		
		var BrowserEventUrl = "/?do=BrowserEvent&event=" + event + "&" + decode_utf8(cgi);
		document.getElementById("BrowserEventFrame").setAttribute("src", BrowserEventUrl);
	}
	
	function BrowserEventPageView(cgi){
		appendBrowserEventFrame();
		
		var BrowserEventUrl = "/browserevent/pageview?" + decode_utf8(cgi);
		document.getElementById("BrowserEventFrame").setAttribute("src", BrowserEventUrl);
	}
	
	function decode_utf8( s ){
	  return decodeURIComponent( escape( s ));
	}
