        $('.recent-videos-block .more-videos .btn-load-more').click(function(){
          data.offset = offset;
          if (loading) {
            return false;
          }
          var more_button = $(this);
          var videos = more_button.closest('.videos-block').find('.videos');
          var videos_header = more_button.closest('.videos-block').find('.videos-header');
          $.ajax({
            data: data,
            type: 'GET',
            dataType: 'json',
            url: '/bizutv-video/ajax',
            beforeSend: function() {
              loading = true;
              more_button.parent().find('.btn-label').hide();
              more_button.parent().find('.arrow-down').hide();
              more_button.parent().find('.ajax-loader').css({display: 'block'});
            },
            complete: function() {
              loading = false;
              more_button.parent().find('.ajax-loader').hide();
              more_button.parent().find('.btn-label').show();
              more_button.parent().find('.arrow-down').show();
            },
            success: function(data){
                             var oas_ad_dev = '</ul><div class="oas-top"></div><ul class="channelcat-detail clearfix">';
                $('.videos').last().append(oas_ad_dev);
                            videos.find('.channelcat-detail').last().append(data.html).hide().fadeIn(700, 'swing');

              ajax_update_ads();
              offset += limit;
              
                            
              if (offset >= total_count) {
                more_button.hide();
              }
            }
          });
          return false;
        });
                var totalScrollPages = Drupal.settings.bizutv.auto_load_limit;
        var scrollCount = 0;
        var footerOffset = $('.footer').height();
        var more_button = $('.recent-videos-block .more-videos .btn-load-more');
        $(document).scroll(function() {
          if (scrollCount < totalScrollPages && $(window).scrollTop() >= $(document).height() - ($(window).height() + footerOffset)) {
            more_button.trigger('click');
            scrollCount++;
          }
        });
        
      function ajax_update_ads() {
        if (typeof OAS_listpos != 'undefined') {
          $positions = OAS_listpos.split(',');
          $rnds = '1234567890';
          $rnds = new String (Math.random()); 
          $rnds= $rnds.substring (2, 11);
          var hostname = Drupal.settings.bizutv.oas_hostname;
          var url = 'http://oascentral.ibtimes.com/RealMedia/ads/adstream_mjx.ads/' + OAS_sitepage + '/'+$rnds+'@'+OAS_listpos+'?';
          $.getScript(url, function(){
            $.each($positions, function(key, position) {
                pos = position.toLowerCase();
                $oas_data = '<scrip'+'t type="text/javascript">OAS_AD(\''+position+'\');</s'+'cript>';
                $('.oas-'+pos).writeCapture().html($oas_data);
              });
          });
        }
      } /* End ADS Function*/



      })(jQuery);
