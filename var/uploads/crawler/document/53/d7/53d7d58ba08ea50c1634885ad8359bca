(function ($, document, window) {
$(document).ready(function() {
  var $programs = $('.intro-program'),
      $loyalty_program = $('#loyalty-program'),
      $refer_program = $('#refer-program'),
      $fans_program = $('#fans-program'),
      $buttons = $('.intro-button'),
      $loyalty_button = $('#loyalty-button'),
      $refer_button = $('#refer-button'),
      $fans_button = $('#fans-button');

  ui_reset = function() {
    $programs.hide();
    $buttons.removeClass('selected').addClass('select');
  };

  $loyalty_button.click(function(evt) {
    if($loyalty_button.hasClass('select')) {
      ui_reset();
      $loyalty_program.fadeIn();
      $loyalty_button.removeClass('select').addClass('selected');
    }

    return false;
  });

  $refer_button.click(function(evt) {
    if($refer_button.hasClass('select')) {
      ui_reset();
      $refer_program.fadeIn();
      $refer_button.removeClass('select').addClass('selected');
    }

    return false;
  });

  $fans_button.click(function(evt) {
    if($fans_button.hasClass('select')) {
      ui_reset();
      $fans_program.fadeIn();
      $fans_button.removeClass('select').addClass('selected');
    }

    return false;
  });
});

$(document).ready(function() {
  var $header = $('.marketing-header'),
      $footer = $('.marketing-footer'),
      $intro = $('.section-intro'),
      $testimonial = $('.section-testimonial'),
      $section_container = $('.section-container');

  after_move = function(index) {
    if ($intro.hasClass('active')) {
      $header.fadeIn(300);
    }

    if ($testimonial.hasClass('active')) {
      $footer.fadeIn(600);
    }
  };
  before_move = function(index) {
    if ($intro.hasClass('active')) {
      $header.fadeOut(1000);
    }
    if ($testimonial.hasClass('active')) {
      $footer.fadeOut(1000);
    }
  };

  if (!window.Modernizr.touch) {
    $footer.fadeOut(0);
    $section_container.css({height: '100%'});
    
    $('head').append('<link rel="stylesheet" href="//s.sloyalty.com/site_media/static/3rdparty/jquery-onepage-scroll/onepage-scroll.css">');

    $('.marketing-body').onepage_scroll({
      sectionContainer: ".opscroll",
      afterMove: after_move,
      beforeMove: before_move
    });
  }
});
}(jQuery, document, window));
