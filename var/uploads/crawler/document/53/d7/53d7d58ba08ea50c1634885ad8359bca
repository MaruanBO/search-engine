(function($, document, window) {
  'use strict';

  $(document).ready(function() {
    var basic_settings = {
          dropboxID:   "20178661",
          url:         "https://sloyalty.zendesk.com",
          tabTooltip:  "Questions",
          tabImageURL: "https://assets.zendesk.com/external/zenbox/images/tab_questions_right.png",
          tabColor:    "#494a4b",
          tabPosition: "Right",

          hide_tab: false,
          request_description: '',
          request_subject: '',
          requester_email: ''
        },
        mailing_list_settings = {
          dropboxID:   "20242496",
          url:         "https://sloyalty.zendesk.com",
          tabTooltip:  "Support",
          tabImageURL: "https://assets.zendesk.com/external/zenbox/images/tab_support.png",
          tabColor:    "black",
          tabPosition: "Left",

          hide_tab: true,
          request_description: 'Join the mailing list for other platform',
          request_subject: 'Join the mailing list for other platform',
          requester_email: ''
        },
        enterprise_settings = {
          dropboxID:   "20178671",
          url:         "https://sloyalty.zendesk.com",
          tabTooltip:  "Support",
          tabImageURL: "https://assets.zendesk.com/external/zenbox/images/tab_support.png",
          tabColor:    "black",
          tabPosition: "Left",

          hide_tab: true,
          request_description: '',
          request_subject: '',
          requester_email: ''
        },
        $basic_buttons = $('.zendesk-contact-us'),
        $enterprise_buttons = $('.zendesk-enterprise-contact'),
        $zenbox_iframe,
        require_reset = false;

    window.Zenbox.init(basic_settings);
    $zenbox_iframe = $('#zenbox_body');

    $enterprise_buttons.click(function(evt) {
      window.Zenbox.update(enterprise_settings);
      window.Zenbox.show();
      require_reset = true;

      return false;
    });

    $basic_buttons.click(function(evt) {
      window.Zenbox.update(basic_settings);
      window.Zenbox.show();
      require_reset = false;

      return false;
    });

    $(document).on('joinMailingList', function(evt) {
      window.Zenbox.update(mailing_list_settings);
      window.Zenbox.show();
      require_reset = true;

      return false;
    });

    // The zenbox iframe will be reloaded each time the form shows or hides.
    // The iframe loads source from different URLs in the two states.
    $zenbox_iframe.load(function(evt) {
      if ($zenbox_iframe.attr('src').indexOf('//assets.zendesk.com') === 0) {
        // the feedback form is closed
        if (require_reset) {
          window.Zenbox.update(basic_settings);
          require_reset = false;
        }
      }
    });
  });
}(jQuery, document, window));
