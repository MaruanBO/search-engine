(function($) {
  'use strict';
  $(document).ready(function() {
    var $signup_dialog = $('#signup-dialog'),
        dialog_options = {
          autoOpen: false,
          draggable: false,
          modal: true,
          resizable: false,
          width: 600,
          height: 350,
          zIndex: 10001,
          dialogClass: 'alert'
        },
        selected_app;

    $signup_dialog.dialog(dialog_options);

    /* TODO(lxiang): combine the following 3 methods */
    $('.rewardsapp-install').click(function() {
      selected_app = 'rewardsapp';
      $signup_dialog.dialog('open');

      return false;
    });

    $('.referralsapp-install').click(function() {
      selected_app = 'referralsapp';
      $signup_dialog.dialog('open');

      return false;
    });

    $('.fansapp-install').click(function() {
      selected_app = 'fansapp';
      $signup_dialog.dialog('open');

      return false;
    });

    $('.ui-widget-overlay').live('click', function() {
      $signup_dialog.dialog('close');

      return false;
    });

    $('button.install-button').click(function() {
      var $select_input = $(this).siblings('.signup-platform'),
          platform = $select_input.val(),
          url_base = 'https://' + window.location.host,
          bigcommerce_path, shopify_path;

      if (platform === '') {
        $('.errtxt').show();
        return false;
      }

      if (selected_app === 'rewardsapp') {
        bigcommerce_path = '/bigcommerce/rewardsapp/install';
        shopify_path = '/shopify/rewardsapp/install';

      } else if (selected_app === 'referralsapp') {
        bigcommerce_path = '/bigcommerce/referralsapp/install';
        shopify_path = '/shopify/referralsapp/install';
      } else if (selected_app === 'fansapp') {
        bigcommerce_path = '/bigcommerce/fansapp/install';
        shopify_path = '/shopify/fansapp/install';
      }

      $.event.trigger({
        type: 'trialSubmit',
        app: selected_app,
        platform: platform
      });

      if (platform === 'bigcommerce') {
        setTimeout(function() {
          window.location.href = url_base + bigcommerce_path;
        }, 100);

      } else if (platform === 'shopify') {
        setTimeout(function() {
          window.location.href = url_base + shopify_path;
        }, 100);

      } else {
        $('.errtxt').hide();
        $signup_dialog.dialog('close');
        $.event.trigger({ type: 'joinMailingList' });
      }

      return false;
    });
  });
}(jQuery, document, window));
