        window._ghearst_vars = (function() {
            var
                metaKeyWords = (function(){
                    var i = 0,
                        metaKeywordContent = '',
                        nameAttribute      = null,
                        metaCollection     = document.getElementsByTagName('meta');
                        nameAttribute = metaCollection[i].name.search(/keywords/);
                      if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                      }
                    return encodeURIComponent(metaKeywordContent);
                })();
        
                // If not www, set subdomain to 'stag'.
                var _hearstSubdomain = 'stag';
                if (location.host.indexOf('www') === 0) {
                    _hearstSubdomain = 'www';
                }
        
                var _hearstSplit = location.pathname.split('/');
        
                // Grab first element.
                var _hearstAdCategoryPrefix = _hearstSplit[1];
        
                // Grab last element.
                var _hearstUrlName = _hearstSplit[_hearstSplit.length-1];
                
                // pass path 
                var _hearstBrowserPath = encodeURIComponent(location.pathname);
        
                // Pass path again, but replace / with ,.
                var _hearstCatPrefixes = encodeURIComponent(location.pathname.replace(/\//g, ','));
        
                if( (navigator.userAgent.match(/iPad/i) !== null) && (CN.cookie.get('ipadTakeover') !== 'true') ){
                    return {
                        'ams_ads_script_src': 'http://subscribe.epicurious.com/ams/page-ads.js?ad_category_prefix=' + _hearstAdCategoryPrefix + '&browser_path=' + _hearstBrowserPath + '&cat_prefixes=' + _hearstCatPrefixes + '&keywords='+metaKeyWords+'&position_list=&site_prefix=epicurious&subdomain=' + _hearstSubdomain + '&url_name=' + _hearstUrlName
                    }
                } else {
                    return {
                        'ams_ads_script_src': 'http://subscribe.epicurious.com/ams/page-ads.js?ad_category_prefix=' + _hearstAdCategoryPrefix + '&browser_path=' + _hearstBrowserPath + '&cat_prefixes=' + _hearstCatPrefixes + '&keywords='+metaKeyWords+'&position_list=&site_prefix=epicurious&subdomain=' + _hearstSubdomain + '&url_name=' + _hearstUrlName
                    }
                };
