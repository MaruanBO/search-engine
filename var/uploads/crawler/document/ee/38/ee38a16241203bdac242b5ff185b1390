	<script type="text/javascript">
		$(function () {			
			var startTime = '';			

			if (startTime > 0) {
				var label = "";
				var d = new Date();
				var duration = (d.getTime() - startTime * 1);
				
				WebLogMultiple({ "redirectTime": duration });

				if(duration < 1000) {
					label = "RedirectTime_0-1secs";
				} else if(duration < 2000) {
					label = "RedirectTime_1-2secs";
				} else if(duration < 3000) {
					label = "RedirectTime_2-3secs";
				} else if(duration < 4000) {
					label = "RedirectTime_3-4secs";
				} else if(duration < 5000) {
					label = "RedirectTime_4-5secs";
				} else if(duration < 10000) {
					label = "RedirectTime_5-10secs";
				} else if(duration < 15000) {
					label = "RedirectTime_10-15secs";
				} else {
					label = "RedirectTime_15+secs";
				}
				_gaObj.GATrackEvent(null, "pageload", label, Math.round(duration), true, null, false);
			}
		});
	</script>





	 
