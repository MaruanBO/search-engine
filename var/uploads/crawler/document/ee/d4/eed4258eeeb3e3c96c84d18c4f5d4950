<script type="text/javascript" language="javascript">
    function getUrl()
    {
      var url = location.pathname + location.search;
      var canonicalUrl = jQuery("link[rel='canonical']").attr('href');
      return typeof(canonicalUrl) == 'undefined' ? url : "/" + canonicalUrl.replace(BaseURL, '');  
    }
	
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-18060934-1']);
    _gaq.push(['_setDomainName', 'kingsizedirect.com']);
    _gaq.push(['_trackPageview', getUrl()]);

    if (typeof(pagetype) != 'undefined' && pagetype != '') {
        _gaq.push(['_setCustomVar', 3, 'PageType', pagetype, 3]);
    }
    else { 
        var file = location.href.substring(location.href.lastIndexOf('/') + 1); 
        var fileName = file.split('.'); 
        _gaq.push(['_setCustomVar', 3, 'PageType', fileName[0], 3]); 
    }

    //Start: Cookie functions
    function getCookieValGA(offset) {
        var endstr = document.cookie.indexOf(";", offset);
        return unescape(document.cookie.substring(offset, endstr == -1 ? document.cookie.length : endstr));
    }

    function GetCookieGA(name) {
        var arg = name + "=";
        var alen = arg.length;
        var clen = document.cookie.length;
        var i = 0;
        while (i < clen) {
            var j = i + alen;
            if (document.cookie.substring(i, j) == arg)
                return getCookieValGA(j);
            i = document.cookie.indexOf(" ", i) + 1;
            if (i == 0) break;
        }
        return null;
    }


    function GetSubCookieValueGA(cookieName, subCookieName) {
        var cookieValue = GetCookieGA(cookieName);
        if (cookieValue != null) {
            if (cookieValue.length > 0) {
                var arrSubCookies = cookieValue.split('&');
                for (var i = 0; i < arrSubCookies.length; i++) {
                    if (arrSubCookies[i].indexOf(subCookieName) == 0) {
                        return arrSubCookies[i].substring(subCookieName.length + 1, arrSubCookies[i].length);
                    }
                }
            }
        }
        return null;
    }


    function GetCookieCustomerIDGA() {
        if (GetSubCookieValueGA('User', 'Indy.CustomerId') != null)
            return decode(GetSubCookieValueGA('User', 'Indy.CustomerId'));
    }
    //End: Cookie functions

    var customerID = GetCookieCustomerIDGA();

    if (typeof(customerID) != 'undefined' && customerID != '') {
        _gaq.push(['_setCustomVar', 4, 'UserID', customerID, 1]);

    }
	
    (function () {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
</script>
<!-- End :: Google analytics -->

