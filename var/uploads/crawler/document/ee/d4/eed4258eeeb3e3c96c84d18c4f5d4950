    <script type="text/javascript">
        $(function () {
            var rotateNext = function () {
                var $spinner = $('#international-shipping-link');
                var posY = $spinner.css('top');

                if (posY === '0px') {
                    $spinner.animate({ top: -55 }, 250);
                    $spinner[0].href = '/Account/Apply_CreditCard.aspx';
                } else if (posY === '-55px') {
                    $spinner.animate({ top: 0 }, 500);
                    $spinner[0].href = '/Catalog/email_catalog_management.aspx?action=emailsignup';
                } 
            };


            $('#placeholderDropdown').mouseleave(function () {
                $('#placeholderDropdown').fadeOut();
            });

            window.setInterval(rotateNext, 4000);
        });
    </script>
