<script type="text/javascript">
        // script for Live Chat
        function track_chat() {
            var s = s_gi(s_account);
            s.linkTrackVars = 'events';
            s.linkTrackEvents = 'event27';
            s.events = 'event27';
            s.tl(true, 'o', 'Live Chat Used');
            s.events = '';
        } 
    </script>
