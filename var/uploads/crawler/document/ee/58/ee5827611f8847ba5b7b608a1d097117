if( n > bannerCount && n < bannerCount + 30){ bannerNumber = 1 ; } 
bannerCount = bannerCount + 30;
if( n > bannerCount && n < bannerCount + 1){ bannerNumber = 2 ; } 
bannerCount = bannerCount + 1;
if( n > bannerCount && n < bannerCount + 1){ bannerNumber = 3 ; } 
bannerCount = bannerCount + 1;
if( n > bannerCount && n < bannerCount + 39){ bannerNumber = 4 ; } 
bannerCount = bannerCount + 39;
if( n > bannerCount && n < bannerCount + 29){ bannerNumber = 5 ; } 
bannerCount = bannerCount + 29;
change(bannerNumber);
//menuBarHeight();
//$('.top_block').fadeIn(500);
searchbarSize();
$('.slider').css("display","");
