function goLink(){
var n = nowSlideNumber;
if(n==1){ goLink1(); }
else if (n ==2){ goLink2();}
else if (n ==3){ goLink3();}
else if (n ==4){ goLink4(); }
else if (n ==5){ goLink5();}
else if (n ==6){ goLink6();}
else if (n ==7){ goLink7();}
else if (n ==8){ goLink8();}
}
function left (){
var number = $("#slideShowNumber").val();
if (number ==1 ){
number=5;}
else{
number--;
}
change(number);
}function right(){
var number = $("#slideShowNumber").val();
if (number ==5 ){
number=1;}
else{
number++;
}
change(number);
}
