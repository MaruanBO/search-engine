<script type="text/javascript">
function loginMobile(){
if($('#isMember').val() ==1){
window.location=("https://membership.cyberlink.com/prog/member/service/index.do?lang=ENU");
}else{
forMobileDropDown('.mobile_login','#loginLock','login','/prog/bar/img/icon_1.png','/prog/bar/img/icon_red_1.png','#loginImg','#mobileLoginImg');
}
}
function login(){
if($('#isMember').val() ==1){
window.location=("https://membership.cyberlink.com/prog/member/service/index.do?lang=ENU");
}else{
var loginHeight=$(".login_content").height();
loginHeight = loginHeight +40;
topMenubar('login','#loginLock',loginHeight,'.login','/prog/bar/img/icon_1.png','/prog/bar/img/icon_red_1.png','#loginImg','#mobileLoginImg');
}
}
var memberObj = new CLMemberObj();
var memberPageLang = "ENU";
var MemberLoginStatusCookie = readCookie("MemberLoginStatus");
$( document ).ready(function() {
if(MemberLoginStatusCookie=="YES"){
memberObj = createMemberDataObjFromJSON();
var msgSign= 'Hi, %firstName% %lastName% ';
msgSign = msgSign.replace("%firstName%", memberObj.firstName);
msgSign = msgSign.replace("%lastName%", memberObj.lastName);
$('#person_name').html(msgSign + " (<a href='https://membership.cyberlink.com/prog/member/service/index.do?lang=ENU' style='color:white;text-decoration:underline'>My Account</a>)");
//$("#loginImg").attr("src","img/icon_1.png");
//$("#mobileLoginImg").attr("src","img/icon_red_1.png");
$('#isMember').val(1);
}else{
}
});	
function loginVerify(){
var email = document.getElementsByName("email")[0].value;;
var password = document.getElementsByName("password")[0].value;;
var emailMobile = document.getElementsByName("emailMobile")[0].value;;
var passwordMobile = document.getElementsByName("passwordMobile")[0].value;;
if(email == ''){
email = emailMobile;
password = passwordMobile;
}
if(email =='' ){
alert('Please enter your email address.');
}else if(password==''){
alert('Please enter your password.');
}
else{
$.ajax({
type: "POST",
url: "/prog/store/sign-in-action.jsp",
data: "email="+email+"&password="+password,
async: false,
success: function(msg){
if(msg.indexOf("login_no_account")>=0){
alert('The email or password you entered is incorrect.');
}else if(msg.indexOf("login_no_unactivate")>=0){
alert('please activate your account');
}else if(msg.indexOf("login_password_error")>=0){
alert('The email or password you entered is incorrect.');
}else if(msg.indexOf("login_ok")>=0){
var data_tmp = $.trim(msg).split("&");
var msgSign= 'Hi, %firstName% %lastName% ';
msgSign = msgSign.replace("%firstName%", data_tmp[1]);
msgSign = msgSign.replace("%lastName%", data_tmp[2]);
$('#person_name').html(msgSign + " (<a href='https://membership.cyberlink.com/prog/member/service/index.do?lang=ENU' style='color:white;text-decoration:underline'>My Account</a>)");
login();
cleanAll('all');
//$("#loginImg").attr("src","/prog/bar/img/icon_red_1.png");
//$("#mobileLoginImg").attr("src","/prog/bar/img/icon_red_1.png");
$('#isMember').val(1);
}
}
});
}
}
function getOption(nLangIdAndLocale){
var result = 0; 
var nLangIdAndLocaleArray = new Array();
nLangIdAndLocaleArray[0] = '"1,en_US"';
nLangIdAndLocaleArray[1] = '"1,en_GB"';
nLangIdAndLocaleArray[2] = '"1,en_AU"';
nLangIdAndLocaleArray[3] = '"1,en_CA"';
nLangIdAndLocaleArray[4] = '"1,en_NZ"';
nLangIdAndLocaleArray[5] = '"1,en_EU"';
nLangIdAndLocaleArray[6] = '"7,de_DE"';
nLangIdAndLocaleArray[7] = '"7,de_CH"';
nLangIdAndLocaleArray[8] = '"4,es_ES"';
nLangIdAndLocaleArray[9] = '"4,es_US"';
nLangIdAndLocaleArray[10] = '"8,fr_FR"';
nLangIdAndLocaleArray[11] = '"8,fr_CH"';
nLangIdAndLocaleArray[12] = '"9,it_IT"';
nLangIdAndLocaleArray[13] = '"3,ja_JP"';
nLangIdAndLocaleArray[14] = '"2,zh_TW"';
nLangIdAndLocaleArray[15] = '"2,zh_HK"';
nLangIdAndLocaleArray[16] = '"6,zh_CN"';
nLangIdAndLocaleArray[17] = '"5,ko_KR"';
for(var i=0 ; i<nLangIdAndLocaleArray.length; i++){
if(nLangIdAndLocaleArray[i]==nLangIdAndLocale){
result = i;
}	
}
return result;
}
var nLangIdAndLocale = readCookie("ENU_nLangIdAndLocale");
var langIndex = 0;
try{
langIndex = getOption(nLangIdAndLocale);
if(nLangIdAndLocale.indexOf(",")>0){
langIndex = getOption(nLangIdAndLocale);
}
}catch(err){
langIndex = 0;
nLangIdAndLocale ="";
}
var nLangIdAndLocaleNameArray = new Array();
nLangIdAndLocaleNameArray[0] = "United States";
nLangIdAndLocaleNameArray[1] = "United Kingdom";
nLangIdAndLocaleNameArray[2] = "Australia";
nLangIdAndLocaleNameArray[3] = "Canada";
nLangIdAndLocaleNameArray[4] = "New Zealand";
nLangIdAndLocaleNameArray[5] = "Europe";
nLangIdAndLocaleNameArray[6] = "Deutschland";
nLangIdAndLocaleNameArray[7] = "Schweiz";
nLangIdAndLocaleNameArray[8] = "Espa&ntilde;a";
nLangIdAndLocaleNameArray[9] = "Latinoamérica";
nLangIdAndLocaleNameArray[10] = "France";
nLangIdAndLocaleNameArray[11] = "Suisse";
nLangIdAndLocaleNameArray[12] = "Italia";
nLangIdAndLocaleNameArray[13] = "&#x65e5;&#x672c;";
nLangIdAndLocaleNameArray[14] = "&#x53f0;&#x7063;";
nLangIdAndLocaleNameArray[15] = "&#x9999;&#x6e2f;";
nLangIdAndLocaleNameArray[16] = "&#x4e2d;&#x56fd;";
nLangIdAndLocaleNameArray[17] = "&#xb300;&#xd55c;&#xbbfc;&#xad6d;";
$("#locale_lang").html(nLangIdAndLocaleNameArray[langIndex]);
</Script>
