<script type="text/javascript">	
var topNumber = 0;
var downNumber = 0;
var topNumberCount = 0;
var downNumberCount = 0;
var topRandom = Math.floor(Math.random() * (100 + 1)) + minNum; 
var downRandom = Math.floor(Math.random() * (100 + 1)) + minNum; 
topNumber = 1 ; 
if( topRandom > topNumberCount && topRandom < topNumberCount + 100){
topNumber = 1;
}
topNumberCount = topNumberCount + 100 ;
downNumber=2; 
if( downRandom > downNumberCount && topRandom < downNumberCount + 100){
downNumber = 2;
}
downNumberCount = downNumberCount + 100 ;
$("#spotlight" + topNumber).css("display","");	
$("#spotlight" + downNumber).css("display","");	
</script>
