	(function($){
		$(document).ready(function() {
			
			function getType(){return $.cookie('type');}
			
			function getPrm2(href){
				return href.replace('b2cms_',$.cookie('prm2')+'_');				
			}
			
			function setRegisterJoin(){
				if( $('#RegisterJoin').length == 0)
					return;
				
				if($.cookie('prm2') != null ) {
					try{
						$('#plus_top').attr('href', getPrm2( $('#plus_top').attr('href') ) );
						$('#plus_bottom').attr('href', getPrm2( $('#plus_bottom').attr('href') ) );
					}catch(e){}
				}				
				
				var type=getType();
				
				if( type != null &&  (type == "1" || type == "8" || type == "9" )){ 
					var tlink=$('#RegisterJoin').attr('href');
 					$('#RegisterJoin').attr('href', tlink+'-now');
					$('#home-join-but').attr('href', tlink+'-now');
					$('.b-join-but').attr('href', tlink+'-now');
				}			
			}					
			
			setRegisterJoin();
						
			//fix broken image under cdn
			function toogleCaptcha(show){
				$('img.wpcf7-captcha-captcha-133').each(function(i, n) {										
					 if(show == true){
					 	$('#kLoadingAjax').hide();
						$(this).show();
					 }else{
						$(this).hide();	
					 }
				});	
			}

			var img=$('<img/>', {
				id: 'kLoadingAjax',
				src: '/wp-content/plugins/contact-form-7/images/ajax-loader.gif',
				width: '16px',
				height: '16px'
			});
			$('img.wpcf7-captcha-captcha-133').after(img);

			$('div.wpcf7 > form').each(function(i, n) {
				var url = $(this).attr('action');
				if (0 < url.indexOf('#'))
					url = url.substr(0, url.indexOf('#'));
				
				url+='?r='+Math.random()
				
				var id = $(this).find('input[name="_wpcf7"]').val();
				var unitTag = $(this).find('input[name="_wpcf7_unit_tag"]').val();
					
				$.getJSON(url,
					{ _wpcf7_is_ajax_call: 1, _wpcf7: id },
					function(data) {
						if (data && data.captcha)
							$('#' + unitTag).wpcf7RefillCaptcha(data.captcha);
	
						if (data && data.quiz)
							$('#' + unitTag).wpcf7RefillQuiz(data.quiz);
											
						 setTimeout(toogleCaptcha(true), 4000);
					}
				);
			});
            
		});
	})(jQuery);
</script>
       
