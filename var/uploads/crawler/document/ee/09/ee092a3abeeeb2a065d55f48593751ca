<!--/fi:/csp/mediapool/sites/BendBulletin/assets/csp/adBox.csp?type=5&adposition=rail, e:.000309 -->

	



<script type="text/javascript">

	// ***********************************************************************************************
	//  sticky right rail code
	// ***********************************************************************************************
	$(function () {
   
		var msie6 = $.browser == 'msie' && $.browser.version < 7;
		if (!msie6 && $('.rtRail').offset()!=null) {
			var top = $('.rtRail').offset().top - parseFloat($('.rtRail').css('margin-top').replace(/auto/, 0));
			var winHeight = $(window).innerHeight(); 
			var footerTop = $('#footer').offset().top - parseFloat($('#footer').css('margin-top').replace(/auto/, 0));
			var gap = 7;
			var currentWidth = $('.rtRail').width();  // 15px margin on each side
			var railPosition = $('.rtRail').position();
			
			var bodyPosition = $('.mainBody').position();
			var bodyWidth = $('.mainBody').width();
			var currentXPos = railPosition.left + bodyWidth + bodyPosition.left + 30;
			//console.log("currentXPos = "+currentXPos+" bodyWidth = "+bodyWidth);
			
			function handleRail () {
				if($(window).width() >= 992) {
					var height = $('.rtRail').height();
					//var winHeight = $(window).innerHeight();
					// what the y position of the scroll is
					var y = $(this).scrollTop();
					var bodyPosition = $('.mainBody').position();
					var bodyWidth = $('.mainBody').width();
					var currentXPos = railPosition.left + bodyWidth + bodyPosition.left + 30;
					//console.log("currentXPos = "+currentXPos+" bodyWidth = "+bodyWidth);
				   
					// whether that's below the form
					if (y+winHeight >= top+ height+gap && y+winHeight <=footerTop) {
						$('.rtRail').css({ position: 'fixed', top:winHeight-height-gap+'px', width: currentWidth, left: currentXPos});
					} 
					else if (y+winHeight>footerTop) {
						$('.rtRail').css({ position: 'fixed', top:footerTop-height-y-gap+'px', width: currentWidth, left: currentXPos});
					} 
					else {
						$('.rtRail').css({ position:'static', width: currentWidth, left: currentXPos});
					}
				}
			}
			
			$(window).scroll(function (event) {
				handleRail();
				
			});
			
			$(window).resize(function (event) {
				handleRail();
			});
		 }  
	});
</script>


<!--/fi:/csp/mediapool/sites/BendBulletin/assets/csp/rtRail.csp, cache(1), e:.0007 -->

							</div>
						</div>		
						
				</div>
			</div><!-- //.container (the main body) -->
			
			<div id="footer" style="clear:both">
				
					
<!-- fi:/csp/mediapool/sites/BendBulletin/assets/csp/scrollingAddBanner.csp -->
<style type="text/css">
	#homeBannerContainer {
		text-align: center;
		//left: -1000px;
		padding: 0.25em 0;
	}
	
	#homeBannerContainer h3 {
		margin: 0 0 0.25em 0;
		padding: 0;
	}
	
	.animationControls {
		width: 25%;
		background-color: #dddddd;
		border-radius: 10px;
		border: solid 3px #555555;
		margin: 0 auto;
	}
	.aniLeftControl {
		text-align: center;
		cursor: pointer;
	}
	.aniRightControl {
		text-align: center;
		cursor: pointer;
	}
	.pausePlay {
		cursor: pointer;
	} 
	
</style>




<div id="homeBannerContainer">
	<h3><img src="http://statics.bendbulletin.com/images/541graphicBug.png" width="40px" style="margin-right: 10px; vertical-align:top"/>541 Clip it! Coupon Savings<img src="http://statics.bendbulletin.com/images/541graphicBug.png" width="40px" style="margin-left: 10px; vertical-align:top"/></h3>
	<div class="photobanner" id="first2">
	
		
		<!-- todayDate = 2015-02-20 -->
		
		<!-- add and ad.stopDate > ?  to the query when we go live and have actual ads -->
		
		
		
		
		<!-- repeat and get the first 8 again to make scroll seam infinite -->
		
		
		
		
		
	</div><!-- photoBanner -->
	<div class="row animationControls">
		<div class="col-xs-5 aniLeftControl"><img src="http://statics.bendbulletin.com/images/couponRewind.png" width="32"/></div>
		<div class="col-xs-2 pausePlay"><img src="http://statics.bendbulletin.com/images/couponPause.png" width="32"/></div>
		<div class="col-xs-5 aniRightControl"><img src="http://statics.bendbulletin.com/images/couponForward.png" width="32"/></div>
	</div>
</div><!-- bannerContainer -->

<script type="text/javascript">
	var currMargin;
	var anim;
	var playState = 1;
	var normSpeed = 50000;
	var margStart;
	var adCount = 0;
	
	// Actually start the normal jquery animation.  
	function doAnimation(animVal, speed) {
		console.log("animVal = "+animVal+" currMargin = "+currMargin); 
		$('#first2').animate(
		{
			marginLeft: animVal
		}, 
		{
			duration: speed,
			easing: 'linear',
			complete: function() {
				scrollAdBanner(anim);
			}
		});
	}
	
	// function to set up the animation - reset margin if not first time through and then call to animate
	function scrollAdBanner(moveVal) {
		moveVal = "-="+moveVal;
		$('#first2').css("margin-left", "-10px"); //set the margin back to zero so the animation can run again.
		doAnimation(moveVal, normSpeed);
		
	}
	function isScrolledIntoView()
	{
		var docViewTop = $(window).scrollTop();
		var docViewBottom = docViewTop + $(window).height();
	
		var elemTop = $('#homeBannerContainer').offset().top;
		var elemBottom = elemTop + $('#homeBannerContainer').height();
	
		if ((elemBottom <= docViewBottom) && (elemTop >= docViewTop))
		{
			resumeAni();
		}
		else
		{
			pauseAni();
		}
	}
	
	// pauses the animation, and stores the current margin value in currMargin in case we start again
	function pauseAni() {
		$('#first2').stop(true);
		currMargin = $('#first2').css("margin-left");
		currMargin = parseInt(currMargin);  // set currMargin global for resumeAni to use
	}
	
	// resumes the animation by calculating how much further it has to go - than starting a new animation
	//  of the distance left to go.
	function resumeAni() {
		if(playState == 1)
		{
			var resVal = anim + currMargin;
			var newSpeed = normSpeed * (resVal/anim);
			resVal = "-="+resVal;
			doAnimation(resVal, newSpeed);
		}
	}
	
	// toggles the pausePlay control for the animation
	$('.pausePlay').click(function() {
		if(playState == 1)
		{
			playState = 0;
			pauseAni();
			$('.pausePlay > img').attr("src", "http://statics.bendbulletin.com/images/coupPlay.png");
		}
		else
		{
			playState = 1;
			resumeAni();
			$('.pausePlay > img').attr("src", "http://statics.bendbulletin.com/images/couponPause.png");
		}
	});
	
	function doJumpAnimation(dir, jumpVal) {
		if (dir == 'left') // move right
		{
			if ((-currMargin) >= anim)
			{
				var diff = anim+"px";
				$('#first2').css("margin-left", "-100px");
				currMargin = anim;
				animVal = "-="+jumpVal;
			}
			else
			{
				animVal = "-="+jumpVal;
			}
		}
		else // move left
		{
			if ( (currMargin)+ jumpVal > 0)
			{

				var diff = -anim+"px";
				console.log("diff = "+diff);
				$('#first2').css("margin-left", diff); 
				currMargin = -anim;
				animVal = "+="+jumpVal;
			}
			else
			{
				animVal = "+="+jumpVal;
			}
		}
			
		$('#first2').animate(
		{
			marginLeft: animVal
		}, 
		{
			duration: 200,
			easing: 'linear'
		});
	}
	$('.aniRightControl').click(function() {
		var scrollJump = 500;
		pauseAni();
		doJumpAnimation('left', scrollJump);
		currMargin = currMargin - scrollJump;
		//console.log("currMargin = "+currMargin+" anim = "+anim);
		resumeAni();
	});
	
	$('.aniLeftControl').click(function() {
		var scrollJump = 700;
		pauseAni();
		doJumpAnimation('right', scrollJump);
		currMargin = currMargin + scrollJump;
		//console.log("currMargin = "+currMargin+" anim = "+anim);
		resumeAni();
	});
	
	$(window).scroll(function() {
		isScrolledIntoView();
	});
	
	$(window).focusout(function() {
		pauseAni();
	});
	$(window).focus(function() {
		isScrolledIntoView();
	});

	$(document).ready(function() {
		if (adCount > 0)
		{
			if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) // mobile view
			{
				// resize control buttons for mobile view
				$('.animationControls').css("width", "80%");
			}
			
			var banWid = (175 + 15) * (0 + 9) + 175 + 200;
			console.log("banWid = "+banWid+" adCount = "+0);
			$('.photobanner').css("width", banWid);
			
			margStart = $(window).width();
			margStart = -margStart;
			$('.photobanner').css("margin-left", margStart+"px");
			
			anim = (175 + 15) * 0 + 70;
			normSpeed = (normSpeed * 0)/20;		// normSpeed of 50,0000 is set for 20 ads, need to adjust for more or less
			
			scrollAdBanner(anim);
			isScrolledIntoView();
		}
		else
		{
			$('#homeBannerContainer').hide();
			console.log("no ads from query");
		}
	});
</script>
<!--/fi:/csp/mediapool/sites/BendBulletin/assets/csp/scrollingAddBanner.csp, e:.000779 -->

				
				
				
