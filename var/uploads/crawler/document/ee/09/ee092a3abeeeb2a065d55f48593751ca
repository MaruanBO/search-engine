	$('.ytubeTitle > a').attr("href", urlString);
	
	//$(document).ready(function() {
		var winWid = $(window).innerWidth();
		if (winWid > 768) {
			var maxHt = 0;
			$('.breaking-media > .media-body').each(function() {
				newHt = $(this).height();
				console.log("newHt = "+newHt);
				if (newHt > maxHt)
					maxHt = newHt;
			});
			console.log("maxHt = "+maxHt);
			$('.breaking-media').each(function() {
				$(this).height(maxHt);
			});
		}
	//});

