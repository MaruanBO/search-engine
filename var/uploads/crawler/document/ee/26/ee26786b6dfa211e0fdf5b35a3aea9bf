						thumbAmount:5,

						navigationType:"bullet",
						navigationArrows:"solo",
						navigationStyle:"round",

						touchenabled:"on",
						onHoverStop:"on",

						swipe_velocity: 0.7,
						swipe_min_touches: 1,
						swipe_max_touches: 1,
						drag_block_vertical: false,
						
						
						keyboardNavigation:"off",

						navigationHAlign:"right",
						navigationVAlign:"top",
						navigationHOffset:10,
						navigationVOffset:20,

