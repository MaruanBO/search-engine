(function(){
	var ooyalaPlayerShells = $('.ooyala-player');

	var ooyalaScriptLink = "//player.ooyala.com/v3/3fb9ae8bc2074ccb9b530f384f74a6f5";
	if ( ! Playboy.Video.hasFlash() ) {
		ooyalaScriptLink += "?platform=html5-priority";
	}

	if ( ooyalaPlayerShells ) {
		$script(ooyalaScriptLink, function(){
			Playboy.Defer.untilReady(Playboy.Video.initialize);
		});
	} else {
		Playboy.Defer.untilReady(Playboy.Video.initialize);
	}

})();
</script>

