      $("#lastNameSearch").autocomplete({
        source: members_of_congress,
        select: function( event, ui ) {
          $(".lastNameSearchForm").attr('action','http://www.heritageactionscorecard.com/members/member/'+ui.item.mcid);
          
          $("#lastNameSearch").keyup(function(){
            if($("#lastNameSearch").val() == ""){
              $(".lastNameSearchForm").attr('action','http://www.heritageactionscorecard.com/members/search/name');
            }
          })
        }
      });
      $(".lastNameSearchForm .searchSubmit").click(function(){
        $(this).parent().submit();
      })

  });
  </script>
