    function getTealiumPageType(){
        var url = document.URL,
                tmp = url.split('/'),
                ret = "section";

        if(tmp[tmp.length - 2] == document.domain) {
            ret = "home";
        }
        if(url.indexOf("accessories") > -1){
            ret = "category";
        }
        if(url.indexOf("product") > -1){
            ret = "product";
        }
        if(url.indexOf("search") > -1){
            ret = "search";
        }
        if(url.indexOf("cart") > -1){
            ret = "cart";
        }
        if(url.indexOf("checkout") > -1){
            ret = "checkout";
        }
        if(url.indexOf("checkout/confirmation") > -1){
            ret = "order";
        }
        if(url.indexOf("account") > -1){
            ret = "account";
        }
        if(url.indexOf("service") > -1){
            ret = "service"
        }

        return ret;
    }
</script>

