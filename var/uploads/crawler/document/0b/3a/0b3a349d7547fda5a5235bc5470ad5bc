function updateHeaderForm()
{
	$j.get('/csrf', function(data) {
		  $j("#inputsearch").html(data);
		  setTimeout(submitHeaderSearch, 500);
	});
}

function submitHeaderSearch() {
	$j('#hdrSearchForm').submit();
}

var whitelist = ['www.pepboys.com', 'pepboys.com', 'facebook.com', 'www.facebook.com', 'www.pepboyspdat.com', 'pepboyspdat.com', 'localhost'];
var isTop = false;
var isPdat = false;

if (top.location!= self.location) {
	var match = document.referrer.match(/^https?:\/\/([^:\/\s]+)\/?.*/);

	if (match) {
    	var domain = match[1];
        for (var i=0; i < whitelist.length; i++) {
        	var re = new RegExp('.' + whitelist[i] + '$');
        	if (domain == whitelist[i] || re.test(domain)) {
	        	isTop=true; 
	        	if(whitelist[i] == 'pepboyspdat.com' || whitelist[i] == 'www.pepboyspdat.com' || whitelist[i] == 'localhost') {
	        		isPdat = true;	
	        	}
	        }
        }
    }

    if(!isTop) {
		top.location = self.location.href;
    }
    
    if(isTop && isPdat) {
    	$j(document).ready(function() { 
    		$j('a').bind("click.myDisable", function() { return false; });
    	});
    }
    
}

//MD5 Hash Passwords on Login Form Submit
$j(document).ready(function() {
	$j("form[action='/processLogin']").on('submit', function(e) {
		var $password = $j(this).find("input[name='j_password']");
		var password = $password.val();
		if (password) {
			$password.val(CryptoJS.MD5(password));
		}	
	});	
});
</script>
