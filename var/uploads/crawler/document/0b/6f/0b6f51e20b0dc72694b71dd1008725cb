		<script type="text/javascript">
		
			var prizeKeyArray = new Array();			
			var prizePrefixArray = new Array();			
			var prizeList = '';
			
			if('a $1000 Sheng Siong' != ''){prizeKeyArray[0] = 'a $1000 Sheng Siong'};
			if('FairPrice' != ''){prizeKeyArray[1] = 'FairPrice'};
			if('Cold Storage Voucher' != ''){prizeKeyArray[2] = 'Cold Storage Voucher'};
			if('' != ''){prizeKeyArray[3] = ''};
			if('' != ''){prizeKeyArray[4] = ''};
			if('' != ''){prizeKeyArray[5] = ''};
			
			
			for (i=0; i<prizeKeyArray.length; i++){
				
				if('no'=='no'){
					prizePrefixArray[i] = /^[aeiouAEIOU]/.test(prizeKeyArray[i]) ? "" : "";	
				}else{
					prizePrefixArray[i] = /^[aeiouAEIOU]/.test(prizeKeyArray[i]) ? "an" : "a";	
				}
				if(prizeKeyArray.length == 1){					
					prizeList = prizeList + prizePrefixArray[i] + ' <strong>' + prizeKeyArray[i] + '<sup>®</sup></strong>';
				}else if((i+1)<prizeKeyArray.length){					
					prizeList = prizeList + prizePrefixArray[i] + ' <strong>' + prizeKeyArray[i] + '<sup>®</sup></strong>, ';
				}else{
					prizeList = prizeList + 'or ' + prizePrefixArray[i] + ' <strong>' + prizeKeyArray[i] + '<sup>®</sup></strong>';
				}
				
			}
								
			$(document).ready(function (){
				initForm('','','');
				initStock('', '', '');
				initPage();
			});
		</script>
		
		
	

