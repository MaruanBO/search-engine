  <script src="/setup.js" type="text/javascript"></script>
  <script type="text/javascript">
  //<![CDATA[
  // ]]>


  window.quirky.shared = {};

  var requireConfig = {
    baseUrl: '//p1.q-static.com/javascripts/require',
    urlArgs: 'bust=' +  (new Date()).getTime(),
    paths: {
      underscore: 'lib/underscore',
      backbone: 'lib/backbone',
      'backbone.wreqr': 'lib/backbone.wreqr',
      'backbone.babysitter': 'lib/backbone.babysitter',
      marionette: 'lib/backbone.marionette',
      handlebars: 'lib/handlebars',
      i18nprecompile: 'lib/i18nprecompile',
      json2: 'lib/json2',
      hbs: 'lib/hbs'
    },
    waitSeconds: 30,
    hbs: {
      i18nDirectory: '/i18n/',
      templateExtension: 'hbs.html',
      originalKeyFallback: true
    },
    locale: 'en'
  };

    requireConfig.baseUrl = '//p1.q-static.com/assets/require-en';
    requireConfig.urlArgs = '1424258678201';

  require.config(requireConfig);

  /**
   * External Interface.
   * Add to cart button in the shop needs to work in conjunction with the async module.
   */

  // We can't be sure this will load before the button, so create a queue.

  var cartItemsController;
  var cartItemDataQueued;

  $(document).on('shop-legacy:add-cart-item', function(ev, cartItemData) {
    if (cartItemsController) {
      cartItemsController.createCartItem(cartItemData);
    } else {
      cartItemDataQueued = {
        item: cartItemData
      }
    }
  });

  require(['common/common'], function() {
    require(['controllers/embedded/cart_items_controller'],
        function(CartItemsController) {
      $(document).ready(function() {
        cartItemsController = new CartItemsController();
        // If any data is queued then send it in.
        if (cartItemDataQueued) {
          cartItemsController.createCartItem(
            cartItemDataQueued.item,
            cartItemDataQueued.companionSuggestions);
        }
      });
    });
  });

</script>


<script type="text/javascript">
  (function() {
  $(document).ready(function(){
    analytics.track('Shop: Viewed Home', { 'Variant' : '' });

    $('.shop.home a').on('click',function(ev) {
      ev.preventDefault();

      var properties = {
        'Unit Title': $(this).attr('data-title')
      };

      var href = $(this).attr('href');

      analytics.track('Shop: Home Page Clicked', properties, function() {
        window.location.href = href;
      }, 300);
    });
  });
})();

  (function() {
    $(document).ready(function() {

      var mixpanelProperties = { 'Appeared On Page': window.location.pathname };

      $('.ad_unit').each(function() {

        mixpanelProperties['Ad Unit'] = $(this).find('img').data('mp');

        // track appearance of the ad-units
        // analytics.track('Ad: Ad Unit Displayed', mixpanelProperties); // disabled due to volume

        // track clicks on ad-units
        $(this).find('a').on('click',function(ev) {
          ev.preventDefault();
          var href = $(this).attr('href');

          analytics.track('Ad: Ad Unit Clicked', mixpanelProperties, function() {
            window.location.href = href;
          }, 300);
        });
      });

    });
  })();


