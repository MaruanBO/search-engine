<script>
  var ad_grouping_js = function() {
    if(!window.$) {
      window.setTimeout(ad_grouping_js, 250);
    }

    $(document).ready(function() {
      // start carousel
      $('#most-popular-grouping').owlCarousel({
        navigation : true,
        navigationText : ["<i class='icon left arrow'></i>","<i class='icon right arrow'></i>"],
        itemsDesktop: 3,
        itemsDesktopSmall: 3,
        items: 4,
        afterInit: function() {
          try{
            ga('send', 'event', 'ad grouping: Most popular products', 'shown', 'Most popular products');
            ga('send', 'event', 'ad grouping: Most popular products', 'all products', '587,418,848,572,84,624,765,633,426,31,187,405,752,235');
            ga('send', 'event', 'ad grouping: Most popular products', 'shown products', '587,');
          }
          catch(e) {};

          $('#most-popular-grouping a.grouping.product').click(function() {
            try {
              ga('send', 'event', 'ad grouping: Most popular products', 'clicked product position: ' + $(this).data('title'), String(parseInt($(this).data('index')) + 1));
              ga('send', 'event', 'ad grouping: Most popular products', 'clicked product', $(this).data('title'));
            }
            catch(e) {};
          });

          $('#most-popular-grouping .owl-next').click(function() {
            try {
              ga('send', 'event', 'ad grouping: Most popular products', 'scrolled right');
            }
            catch(e) {};
          });

          $('#most-popular-grouping .owl-prev').click(function() {
            try {
              ga('send', 'event', 'ad grouping: Most popular products', 'scrolled left');
            }
            catch(e) {};
          });
        },
        afterMove: function(e) {
        },
        autoPlay: 10000
      });
    });
  }
  window.setTimeout(ad_grouping_js, 250);
</script>

