        <script type="text/javascript">
            $(function() {
                // Adjust all main nav items so they are all the same height
                // IE < 8 doesn't support css tables
                box_height = 0;
                $('.nav_item').each(function() {
                    var height = $(this).outerHeight();
                    if (height>box_height) {
                        box_height = height;
                    }
                });
                $('.nav_item').each(function() {
                    if($(this).outerHeight()<box_height) {
                        var outerHeight = $(this).outerHeight();
                        $(this).css('padding-top',parseInt($(this).css('padding-top'))+Math.ceil((box_height-outerHeight)/2)+'px');
                        $(this).css('padding-bottom',parseInt($(this).css('padding-bottom'))+Math.floor((box_height-outerHeight)/2)+'px');
                    }
                });
            });
        </script>
        <![endif]-->
        <script type="text/javascript">
            $(function() {
                // Clicking on a nav wrapper cell will load the link inside
                $('#main_nav').on('click','div.wrapper > ul > li:not(.bg-Popup)',function(e){
                    e.preventDefault();
                    document.location = $(this).children('a[href]').attr('href');
                });

                // Load the RIP tab box
                var lastTab = null;
                $("#recently_issued_tabs").tabs({ fx: { opacity: 'toggle', duration:'fast' },
                    select: function(e, ui) {
                        $('#'+lastTab).data('scroll',$('#recently_issued_tabs .inner').scrollTop());
                        $('#recently_issued > h2 > a').attr('href',$(ui.tab).attr('data'));
                    },
                    show : function(e ,ui) {
                        lastTab = ui.panel.id;
                        $('.bg-scroll').resetScroller({scrollTop:$('#'+ui.panel.id).data('scroll')});
                    },
                    cookie: {name: 'rip cookie'}
                });

                // Adjust Top Download Box size to be consistent across tabs
                /*
                var box_height = 0;
                $('[id*=top_download_box_tabs] ol').each(function() {
                    var height = $(this).outerHeight();
                    if (height>box_height) {
                        box_height = height;
                    }
                });
                $('[id*=top_download_box_tabs] ol').each(function() {
                    $(this).css('min-height',box_height+10+'px');
                });

                // Load Top Download Tab box
                $("#top_download_box").tabs({fx: { opacity: 'toggle', duration:'fast' }});
                */

                // Add Scroller to RIP box
                $('.bg-scroll').addScrollButtons();

                // Load the Featured Slider
                $('#slider').nivoSlider({
                    pauseTime:5000,
                    effect:'fade'
                });

                // Play/Pause button for the Slider
                $('#nivoSlider-control').click(function(){
                    if ($(this).attr('data')=='stop') {
                        $('#slider').data('nivoslider').stop();
                        $(this).attr('src','img_library/slider/play.png');
                        $(this).attr('data','play');
                    } else {
                        $('#slider').data('nivoslider').start();
                        $(this).attr('src','img_library/slider/pause.png');
                        $(this).attr('data','stop');
                    }
                });

                // Line Key Resource bottom up with the bottom of the RIP box
                $('#recently_issued .inner').alignBottoms({parent_offset:31});

                // Line Featured Topics box top and bottom up with Top Download box
                //$('#top_download_box').alignBottoms();
                $('#top_download_box_tabs-4').alignBottoms();

                // Create the nav popups
                $('.bg-Popup').createPopup({show_speed:200,hide_speed:200,animation_speed:300});

                // Create search box placeholder for browsers that don't support it
                $('#search_box').simulatePlaceholder();

                // Remove Resources href if javascript is running
                $('#main_nav li.bg-Popup a.nav_item').attr('href','#');

                // Featured Collections Scroller
                $('.scroll_container').scrollable({
                    /*circular: true,*/
                    onBeforeSeek: function(event, index) {
                        if (index > this.getSize()-3) {
                            return false;//disable scrollable api
                        }
                    },
                    onSeek: function(event, index) {
                        if (index == this.getSize()-3) {
                            $('a.next').addClass('disabled');//hide 'next' button
                        } else {
                            $('a.next').removeClass('disabled');
                        }
                    }
                });
            });
        </script>
