var enableTooltips = function (selector) {
    var $tooltip, $element;
    var title;
    $(selector).hover(function (event) {
        var tooltip = '<div class="tooltip"><span class="content"></span><div class="arrow-down"></div></div>';
        $element = $(event.currentTarget);
        $tooltip = $(tooltip);
        title = $element.data('title');
        $tooltip.find('.content').html(title);
        $tooltip.stop().hide();
        $tooltip.appendTo('body');
        $tooltip.css('top', (($element.offset().top - parseInt($tooltip.css('height'), 10)) - 15) - $(window).scrollTop());
        $tooltip.css('left', ($element).parent().offset().left + 69);
        $tooltip.stop().show();
    }, function (event) {
        event.preventDefault();
        $tooltip.stop().remove();
    });
};
enableTooltips('.help[data-title]');</script>


