			var local, lang, loc, region;
			(function () {
				var m = window.location.pathname.match(/^\/([a-z]{2})\/([a-z]{2})/);

				if (m.length == 3) {
					local = region = m[1] + '/' + m[2];
					lang = m[2];
					loc = m[1];
				}
			})();
