    <script type="text/javascript">
    application.register('comment.added', function(dataObj) {
        var $notice = jQuery('#prompt-notice-7'),
            message = $notice.html();


        jQuery.prompt({
            title: $notice.attr('title'),
            message: message,
            width: '400',
            onShow: function($modal_content) {
                $modal_content.find('a').bind('click', function() {
                    application.cookie('hideSiteNotice7', true);
                });
            },
            onClose: function() {
                application.cookie('hideSiteNotice7', true);
            }
        });
    });
    </script>

    <script type="text/javascript">
    application.register('issue.reported', function(dataObj) {
        application.trigger('share.item', [{
            url: 'http://seeclickfix.com/',
            text: 'SeeClickFix',
            counts: 'twitter,facebook,google',
            type: 'vertical'
        }, "Now spread the word!" ,"You\'ve created an issue, now share it with your friends and contacts so they can help you get it fixed."]);
    });
    </script>



