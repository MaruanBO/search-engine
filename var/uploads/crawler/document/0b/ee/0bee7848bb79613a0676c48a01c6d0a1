<script type="text/javascript">
$(function() {
$('.tabPane li a').bind('click', function (e) {
var s=s_gi(s_account), tabId, tab,
type = $('meta[property=og:type]').attr('content') , ctrObj = $(this).closest('.tabPane');
if( typeof type === 'undefined' ) {
type = 'homepage';
}
tabId = "tabs-1-" + (ctrObj.index() + 1);
tab = $('#' + tabId).find('span').html();
s.linkTrackVars='prop30,prop31,events'; s.linkTrackEvents='event38';
s.prop30=tab + ' - ' + s.channel + ' ' + type + ' - position ' + ($(this).closest('li').index() + 1);
s.prop31=tab + ' - ' + s.channel + ' ' + type + ' - ' + $(this).html().replace(/'/g, "\\'");
s.events='event38';
s.tl(this,'o', tab + ' Tab - ' + location.hostname);
s.prop30 = s.prop31 = s.events = '';
});
// Custom link tracking for IV promo widget, bind only if we find node in the DOM.
$('.ivPromo-miniCarousel .voicescarousel').bind('click', function(e) {
var s=s_gi(s_account), type = $('meta[property=og:type]').attr('content');
if( typeof type === 'undefined' ) {
type = 'homepage';
}
s.linkTrackVars='prop30,prop31,events'; s.linkTrackEvents='event38';
s.prop30='IV promo carousel - ' + s.channel + ' ' + type + ' - position ' + ($(this).index() + 1);
s.prop31='IV promo carousel - ' + s.channel + ' ' + type + ' - ' + $(this).find('.heading a').html().replace(/'/g, "\\'");
s.events='event38';
s.tl(this,'o', 'IV promo - ' + location.hostname);
s.prop30 = s.prop31 = s.events = ''; });
$('.ivPromo-articleList .ivPromo-article').bind('click', function(e) {
var s=s_gi(s_account), type = $('meta[property=og:type]').attr('content');
if( typeof type === 'undefined' ) {
type = 'homepage';
}
s.linkTrackVars='prop30,prop31,events'; s.linkTrackEvents='event38';
s.prop30='IV promo list - ' + s.channel + ' ' + type + ' - position ' + ($(this).index() + 1);
s.prop31='IV promo list - ' + s.channel + ' ' + type + ' - ' + $(this).find('.title').html().replace(/'/g, "\\'");
s.events='event38';
s.tl(this,'o', 'IV promo - ' + location.hostname);
s.prop30 = s.prop31 = s.events = ''; });
$('#galleriaLoadId-7804719').bind('click', function(e) {
var s = s_gi(s_account);
s.event = 'event8';
});
$('.esi-gallery').click(function(e) {
//console.log('esigallery');
if ($(e.target).hasClass('esi-gallery-rightarrow')) {
var s=s_gi(s_account);
s.linkTrackVars='prop28';
s.prop28=$('.esi-gallery').attr('data-galleryid');//location.href.substr(location.href.lastIndexOf('/') + 1);
//console.log('click28 '+location.href.substr(location.href.lastIndexOf('/') + 1));
}
});
});
</script>
