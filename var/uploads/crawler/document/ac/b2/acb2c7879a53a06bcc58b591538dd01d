		<script type='text/javascript'>
		googletag.cmd.push(function() {
			googletag.pubads().enableAsyncRendering(); // enable async loading
			googletag.pubads().collapseEmptyDivs(); // if no ads loaded, kill their containers
			googletag.enableServices();
		});

		// check viewport for resize events and reload ads (250ms delay)
		var resizeTimer;
			function resizer() {
				googletag.pubads().refresh([window.MNT_Leaderboard_Top]);
				googletag.pubads().refresh([window.MNT_MREC_Right]);
				googletag.pubads().refresh([window.MNT_Flex_Right]);
			}
	
		window.addEventListener("resize", function(){ 
			clearTimeout(resizeTimer);
			resizeTimer = setTimeout(resizer, 1000);
		});
		</script>


