    }, function(){});
});

$('#modal-login-goog').click(function(e){
    e.preventDefault();
    razoo.analytics.push({
        _gaq: ['_trackEvent', 'Login', 'Google', document.location.pathname],
        _mix: {
            event: "Login",
            props: {
                "login_method": "Google",
                "page": document.location.pathname
            }
        }
    }, function(){
        var target = decodeURIComponent($('#modal-login-goog').attr('href'));
        document.location = target;
    });
});


$('#modal-login-submit').click(function(e){
    razoo.analytics.push({
        _gaq: ['_trackEvent', 'Login', 'Email', document.location.pathname],
        _mix: {
            event: "Login",
            props: {
                "email": $('input[name="user_session[email]"]').val(),
                "login_method": "Email",
                "page": document.location.pathname
            }
        }
    });
});

$('#non_modal_login_button, #portable-login-submit').click(function(e){
    e.preventDefault();
    razoo.analytics.push({
        _gaq: ['_trackEvent', 'Login', 'Email', document.location.pathname],
        _mix: {
            event: "Login",
            props: {
                "email": $('input[name="user_session[email]"]').val(),
                "login_method": "Email",
                "page": document.location.pathname
            }
        }
    }, function(){
        $(e.target).closest('form').submit();
    });
});

$('#new_user_session').keydown(function(e){
    if (e.keyCode === 13) {
        var self = $(this);
        e.preventDefault();

        razoo.analytics.push({
            _gaq: ['_trackEvent', 'Login', 'Email', document.location.pathname],
            _mix: {
                event: "Login",
                props: {
                    "email": $('input[name="user_session[email]"]').val(),
                    "login_method": "Email",
                    "page": document.location.pathname
                }
            }
        }, function(){
            self.submit();
        });
    }
});

$('#logout').click(function(e){
    var route = $(this).attr('href');

    e.preventDefault();
    razoo.analytics.push({
        _gaq: ['_trackEvent', 'Logout', 'Success', document.location.pathname],
        _mix: {
            event: "Logout",
            props: {
                "page": document.location.pathname
            }
        }
    }, function(){
        document.location.pathname = route;
    });
});



      
