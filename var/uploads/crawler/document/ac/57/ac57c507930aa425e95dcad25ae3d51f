//<![CDATA[
    $(function(){
      
        $('#fb_register').dialog(
          { autoOpen: false,
          modal: true,
          width: 700,
          title: 'Create a free account'
          }
        );
    

      $('#modal-login-fb').click(function(e){
    razoo.analytics.push({
        _gaq: ['_trackEvent', 'Login', 'Facebook', document.location.pathname],
        _mix: {
            event: "Login",
            props: {
                "login_method": "Facebook",
                "page": document.location.pathname
            }
