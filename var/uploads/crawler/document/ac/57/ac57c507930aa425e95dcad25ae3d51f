<script>
  function load_fb_api(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s);
    js.id = id;
    js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=250457581338";
    fjs.parentNode.insertBefore(js, fjs);
  }
  load_fb_api(document, 'script', 'facebook-jssdk');
</script>


