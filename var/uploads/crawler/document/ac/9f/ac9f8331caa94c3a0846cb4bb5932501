<script type="text/javascript">
function show_fb_logout()
{
	document.getElementById("fb_logout_down").style.display="inline";
}

function pre_logout(href)
{
	if (confirm('Click OK to also log out of Facebook. Click Cancel to only log out of PaperBackSwap.'))
	{
		href +="?fblo=yes";
	}
	return false;
}
</script></div>
