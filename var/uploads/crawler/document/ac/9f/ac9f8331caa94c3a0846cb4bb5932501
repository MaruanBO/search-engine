		<script type="text/javascript">
			var fb_button;

			window.fbAsyncInit = function() { FB.init({ appId: 464725376882909,
		        	version    : 'v2.2',
		            status: true,
		            cookie: true,
		            xfbml: true,
		            oauth: true,
		            channelUrl: 'http://www.paperbackswap.com/channel.php'});


		        function updateButton(response) {
					//alert('updateButton');
					showLoader(true);
		            fb_button    =   document.getElementById('fb-auth');

		            if (response.authResponse) {
						//alert('connected');
		                //user is already logged in and connected
		                FB.api('/me', function(info) {
							email = info.email;
							fb_id = info.id;

							$.ajax({
								url: '/scripts/login_fb_user.php',
								cache: false,
								type: 'GET',
								data: { 'email' : email, 'fb_id' : fb_id },
								dataType: 'json',
								success: function(data) {
									//alert('success');
									if (data.success=='true')
									{
										//alert('success==true');
										if ((typeof(data.redirect) != 'undefined'))
										{
											//alert('redirect:true');
											showLoader(false);
											window.location = decodeURI(data.redirect);
										}
									}
								},
								error: function( error )
								{
									//alert ('An error occurred during the Facebook log in process. Please reload the page and try again.');
								}
							});
							//alert('login(response, info)-inside');
		                    login(response, info);
		                });

		                fb_button.onclick = function() {
		                    FB.logout(function(response) {
								//alert('FB.logout');
		                        logout(response);
		                    });
		                };
		            } else {
		                //user is not connected to PBS Login app or logged out
						//alert('connected-else');
		                fb_button.onclick = function() {
		                    showLoader(true);
		                    FB.login(function(response) {
								//alert('FB.login');
		                        if (response.authResponse) {
		                            FB.api('/me', function(info) {
		                                login(response, info);
		                            });
		                        } else {
		                            //user cancelled login or did not grant authorization
									//alert('FB.login-else');
		                            showLoader(false);
		                        }
		                    }, {scope:'email'});
		                }
		            }
		        }

		        FB.Event.subscribe('auth.statusChange', updateButton);
		    };

			(function(d, s, id){
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) {return;}
				js = d.createElement(s); js.id = id;
				js.src = "//connect.facebook.net/en_US/sdk.js";
				fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));

            function login(response, info){
				//alert('login(response, info)-outside');
	            fb_button    =   document.getElementById('fb-auth');
                if (response.authResponse) {
    				//alert('login(response, info)-outside-if-true');
                    var accessToken = response.authResponse.accessToken;
    				//showLoader(false);

	                FB.api('/me', function(info) {
						email = info.email;
						fb_id = info.id;

						$.ajax({
							url: '/scripts/login_fb_user.php',
							cache: false,
							type: 'GET',
							data: { 'email' : email, 'fb_id' : fb_id },
							dataType: 'json',
							success: function(data) {
								//alert('success');
								//if (data.success=='true')
								//{
									//alert('success==true');
									if ((typeof(data.redirect) != 'undefined'))
									{
										//alert('redirect:true');
										window.location = decodeURI(data.redirect);
										showLoader(false);
									}
								//}
								//else
								//{

								//};
							},
							error: function( error )
							{
								//alert ('An error occurred during the Facebook log in process. Please reload the page and try again.');
							}
						});
						//alert('login(response, info)-inside');
	                    //login(response, info);
	                });

                } else {
    				//alert('login(response, info)-outside-else');
					uri = 'https://secure.paperbackswap.com/register/fb_index.php';
					window.location=uri;
                }
            }

            function logout(response){
				//alert('logout(response)-outside');
                showLoader(false);
            }

            function showLoader(status){
                if (status)
                    document.getElementById('loader').style.display = 'block';
                else
                    document.getElementById('loader').style.display = 'none';
            }
