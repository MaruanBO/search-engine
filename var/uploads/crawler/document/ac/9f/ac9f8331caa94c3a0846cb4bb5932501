	<script type="text/javascript">

	function modify_club_fav_selector(obj)
		{
			var cache_id = dcui('favorite_genres', obj.value);
			swap_carousel('member_favs','', obj.value, null, cache_id, 8);
		}
		function swap_selection(obj, id, name, value, header, view_size)
		{
			var cache_id = dcui('favorite_genres', value);

			var elements = YAHOO.util.Dom.getElementsByClassName("member_favs_link","a");
			for (i=0; i<elements.length; i++)
			{
				elements[i].style.fontWeight="400";
				elements[i].style.fontSize="100%";
			}

			var h = document.getElementById("books_posted_strip_h2");

			if (typeof(h)!='undefined')
			{
				if (typeof(header)!='undefined' && header != null)
				{
					h.innerHTML = header;
				}
				else
				{
					h.innerHTML = "Books Available for Swapping in " + name;
				}
			}

			if (typeof(obj)!='undefined')
			{
				obj.style.fontWeight="bold";
				obj.style.fontSize="100%";
			}

			if (id=='genre')
			{
				swap_carousel('member_favs', 'genre_id='+value, null, cache_id, view_size);
			}
			else if (id=='favs')
			{
				swap_carousel('member_favs', '', value, cache_id, view_size);
			}
		}
		// Determine Carousel Unique ID
		function dcui(name, genre)
		{
			var session_id = '3094398346';

			var output = '';

			if (genre.indexOf("/") !== -1)
			{
				var temp = genre.split("/");
				var fn = temp.pop();
				temp = fn.split(".");
				genre = temp[0];

				output = 'cv4_' + session_id + '_' + genre + '_0';
			}
			else
			{
				output = 'cv4_' + session_id + '_' + name + '_' + genre;
			}

			return output;
		}

		var filters = ['m'];
		if (document.getElementById('market_search_field_0')) {
			AutoComplete_Create('market_search_field_0', 'market_search_0', '/scripts/search_suggest.php', 'k', 'get', 2, 'market_search_form_0',filters);
		}
	</script>

