						<script type="text/javascript">

							var hide_header_stats = function() { document.getElementById("header_stats_main").style.visibility = "hidden"; }
							
							function update_site_book_total()
							{
								current = document.getElementById('site_wide_total_books').innerHTML;
								var idx = current.indexOf(',');

						        while (idx > -1)
						        {
						            current = current.replace(',', '');
						            idx = current.indexOf(',');
						        }

						        current = parseInt(current) + 1;
				                current += '';

				                var rgx = /(\d+)(\d{3})/;
				                while (rgx.test(current))
				                {
				                        current = current.replace(rgx, '$1' + ',' + '$2');
				                }

				                document.getElementById('site_wide_total_books').innerHTML = current;

				                if (document.getElementById('site_wide_total_books_HP'))
				                {
				                	document.getElementById('site_wide_total_books_HP').innerHTML = current;
				                }

				                setTimeout("update_site_book_total();", 20000);
							}

							function update_member_savings()
							{
								savings = document.getElementById('total_money_saved').innerHTML;
								var hdx = savings.indexOf(',');

						        while (hdx > -1)
						        {
						            savings = savings.replace(',', '');
						            hdx = savings.indexOf(',');
						        }

				                savings = format_currency(parseFloat(savings) + 1);
				                savings += '';

				                var rgx = /(\d+)(\d{3})/;
				                while (rgx.test(savings))
				                {
				                	savings = savings.replace(rgx, '$1' + ',' + '$2');
				                }

				                document.getElementById('total_money_saved').innerHTML = savings;

				                if (document.getElementById('total_money_saved_HP'))
				                {
				                	document.getElementById('total_money_saved_HP').innerHTML = savings;
				                }

								setTimeout("update_member_savings();", 5000);
							}

							update_site_book_total();
							update_member_savings();
						</script>
				</div>
