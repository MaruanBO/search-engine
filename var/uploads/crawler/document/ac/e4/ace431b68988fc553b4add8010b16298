    function validateContactForm() {
        valid = false;

        // Captcha
        var answer = ($('#num1').val() * 1) + ($('#num2').val() * 1);
        captcha = ($('#captcha').val() == answer) ? true : false;

        if (!captcha) {
            alert('Invalid answer for human test.');
            $('#captcha').focus();
        }
        else {
            valid = true;
        }
        return valid;
    }
