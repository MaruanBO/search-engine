<script type="text/javascript">
    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

    $(function() {
        var rdn = getParameterByName("__src");
        if (rdn == "") {
            rdn = getParameterByName("RDN");
        }
        if (window.location.hash != "") {
            rdn = window.location.hash.substring(1);
            window.location.hash = "";
        }

        rdn = rdn.toLowerCase();
        rdn = rdn.replace(/\.com\./g, '.');
        rdn = rdn.replace(/^www\./, '');
        rdn = rdn.replace(/^www\./, '');
        rdn = rdn.replace(/^m\./, '');

        if (rdn.slice(-4) != ".xyz") {
            rdn += ".xyz";
        }

        if (rdn != ".xyz") {
            $.getJSON(
                "http://gen.xyz/data/check_name.php?name="+rdn,
                function(data){
                    if (data.status == 0) {
                        $('#mce-RDN').val(rdn);
                    }
                }
            );
        }

    });
</script>


<script type="text/javascript">
    function getParameterByNameSuccess(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }
	clickedDomain='';
    $(function() {
    	
        var rdn = getParameterByNameSuccess("__src");
        if (rdn == "") {
            rdn = getParameterByNameSuccess("RDN");
        }
        if (window.location.hash != "") {
            rdn = window.location.hash.substring(1);
            window.location.hash = "";
        }

        rdn = rdn.toLowerCase();
        rdn = rdn.replace(/\.com\./g, '.');
        rdn = rdn.replace(/^www\./, '');
        rdn = rdn.replace(/^www\./, '');
        rdn = rdn.replace(/^m\./, '');

        if (rdn.slice(-4) != ".xyz") {
            rdn += ".xyz";
        }
        rdn = punycode.ToUnicode(rdn);

        if (rdn != ".xyz") {
        	clickedDomain = rdn;
			 $('.selected-success-domain').html(rdn);
			 $('.registrar-link').each(function(){
				 linkWithDomain = $(this).attr('href') + rdn;
				 $(this).attr('href', linkWithDomain);
			 });
			 // for DomainMonaster on page3378 only
			 if ($('body').hasClass('page-id-3378')) {
				 $('#dm-name').attr('value', rdn);
			 }
        }

    });
    
	$('.registrar-link').click(function(event) {
		if (clickedDomain!='')
		{
			event.preventDefault();
			link =  $(this).attr('href');		
			window.open(link.replace('{domain}', clickedDomain) );			
		}
	});    
    
    
</script>



