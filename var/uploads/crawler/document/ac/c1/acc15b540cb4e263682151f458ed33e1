<script>
/* begin popup  */
var sinit = false;
  function initshadowbox() {
    if (sinit) return true;
  if(typeof(Shadowbox) == "object") {
    sinit = true;
    var options = {
      skipSetup: true,
      displayNav:         false,
      handleUnsupported:  'remove',
      keysClose:          ['c', 27], // c or esc
      autoplayMovies:     false,
      resizeDuration:0,
      fadeDuration:0
    };

    Shadowbox.onReady = function() {
      //setTimeout( function() { opengcal() },1000);
    }
    Shadowbox.init(options);
    return true;
  }
  return false;
}
function opengcal() {
  if (!initshadowbox()) return;
//  console.log("open");
//  console.log(Shadowbox);
  Shadowbox.open({
    player:     'iframe',
    content:    '/googlecalendar.html',
    height:     380,
    width:      730
    });
}
function closebox() {
  Shadowbox.close();
}
/* end popup  */
</script>
