    <script>
var _gaq = _gaq || [];

	 $(document).ready(function(){

		// TRACK EVENTS - NETWORK HEADER
		$(".nethead li a").click(function() {
			wclick = $(this).attr("href");
			if(wclick.indexOf("?") != -1) {
				wclicka= wclick.split("?");
				if (wclicka.length > 0) {
					wclick = wclicka[0];
				}
			}
			console.log("home click: " + wclick);
			//_trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
			_gaq.push(['_trackEvent', 'Site', 'NetworkHeader',wclick]);
		});

		// TRACK EVENTS - MAIN NAV
		$("#main-nav a").click(function() {
			wclick = $(this).attr("href").toLowerCase();
			if(wclick.indexOf("?") != -1) {	wclicka= wclick.split("?");	if (wclicka.length > 0) {	wclick = wclicka[0];}}  // REMOVE QSTRING
			console.log("Main Nav click: " + wclick);
			//_trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
			_gaq.push(['_trackEvent', 'Site', 'MainNav',wclick]);
		});

		// TRACK EVENTS - SIDE BAR
		$(".sidebar-item a,.sidebar-itemcal a").click(function() {
			aclick = $.trim($(this).parents(".sidebar-item").prev("h3").text().toLowerCase());
			if (aclick == "") {aclick = $.trim($(this).parents(".sidebar-itemcal").prev("h3").text().toLowerCase());}
			if (aclick == "") {aclick = $(this).parents(".sidebar-item").find(".widgettitle").text().toLowerCase(); }
			wclick = $(this).attr("href");
			if (wclick.toLowerCase().indexOf('http://t.co') == -1) {
				wclick = wclick.toLowerCase();
			}
			if(wclick.indexOf("?") != -1) {	wclicka= wclick.split("?");	if (wclicka.length > 0) {	wclick = wclicka[0];}}  // REMOVE QSTRING
			console.log("Side Bar click: " + 'SideBar - ' + aclick + " / " + wclick);
			//_trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
			_gaq.push(['_trackEvent', 'Site', 'SideBar - ' + aclick,wclick]);
		});

		// TRACK EVENTS - FOOTER
		$("#pageFooter a, #pageContribute a").click(function() {
			wclick = $(this).attr("href").toLowerCase();
			if(wclick.indexOf("?") != -1) {	wclicka= wclick.split("?");	if (wclicka.length > 0) {	wclick = wclicka[0];}}  // REMOVE QSTRING
			console.log("Footer click: " + wclick);
			//_trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
			_gaq.push(['_trackEvent', 'Site', 'Footer',wclick]);
		});
//		alert("hi");
			// TRACK EVENTS - SUBSCRIBE CAL
		$(".savepop a").click(function() {

			wclick = $(this).text().toLowerCase();
			wval = $.trim($(this).parents(".savepop").attr("id").replace("pid",""));
			if (wval == "") { wval = "-Full"; } else { wval = "-" + wval;}

			console.log("Cal click: " + wclick + " " +  wval);
			//_trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
			_gaq.push(['_trackEvent', 'Calendar', 'FeedClick',wclick + wval]);
		});

	})

</script>
