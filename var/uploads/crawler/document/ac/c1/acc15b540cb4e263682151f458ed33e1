$(document).ready(function(){
  var searchint = setInterval(function() {
    console.log("Google Search Still Loading....");
    if ($("#search form.gsc-search-box").size() > 0) {
        clearInterval(searchint);
        customsearchinit();
    }
  }, 200);
});


function customsearchinit() {
 // console.log("..customsearchinit");

  
  var searchtextval = "February in Philadelphia";
  $("input.gsc-input").attr("placeholder",searchtextval);
  $("#search form.gsc-search-box").before("<div class='searchlabel'>Search Uwishunu.com</div>");

  $("form.gsc-search-box").submit(function() {
    console.log("go1");

    return true;
  });

  // on go click
  $(".gsc-search-button input").click(function() {
    setsearchval();
  });

 // on enter press click
  $("input.gsc-input").keydown(function(e) {
    if(e.which == 13) {
      setsearchval();
    }
  });
}

function setsearchval() {

    var thisinput = $("input.gsc-input");
    if ($.trim(thisinput.val()) === "") {
      if (!empty(thisinput.attr("placeholder"))) {
        thisinput.val(thisinput.attr("placeholder"));
        //$("form.gsc-search-box").trigger("submit"); //submit();
         $(".gsc-search-button input").trigger("click");
      }
    }

}


</script>
