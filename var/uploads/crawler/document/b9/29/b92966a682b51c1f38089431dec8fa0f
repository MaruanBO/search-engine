		    			<!-- x = 3 -->
    			<script type="text/javascript">
        	var dhtmlSubCenter = {};
        	dhtmlSubCenter.email;
        	dhtmlSubCenter.main_obj;
        	
        	dhtmlSubCenter.trigger = function(){
            	console.log('dhtmlSubCenter trigger');
            	$('body').append('<div id="dhtmlSubCenter"><div class="ajaxContent"></div></div>');
            	dhtmlSubCenter.main_obj = $('#dhtmlSubCenter');
            	$(dhtmlSubCenter.main_obj).show();
            	dhtmlSubCenter.getContent();
        	}
        	
        	dhtmlSubCenter.getContent = function(){
            	console.log('dhtmlSubCenter.getContent');
            	$(dhtmlSubCenter.main_obj).find('.ajaxContent').html('<div class="loading"><p>Please wait while we retrieve your newsletter subscription information.</p><img src="/images/ajax-loader-bar.gif" alt="Loading..."></div>');
            	
            	if(is_mustang){
                	if(dhtmlSubCenter.email == undefined  ||  dhtmlSubCenter.email == ''){
                    	dhtmlSubCenter.email = 'ddd@ddd.com';
                	}
            	}
            	
            	$.ajax({
                    url: "/ajax_process.php",
                    type: 'GET',
                    cache: false,
                    dataType: "html",
                    timeout: 20000,
                    data: {
                        task: 'dhtml_sub_center_get_base',
                        email: dhtmlSubCenter.email
                    },
                    complete: function(){
                        
                    },
                    success: function(html){
                        // add the html to the page
                        $(dhtmlSubCenter.main_obj).find('.ajaxContent').html(html);
                        
                        
                        $(dhtmlSubCenter.main_obj).find('.close').click(
                            function(){
                                console.log('click');
                                $(dhtmlSubCenter.main_obj).slideUp();
                            }
                        );
                        
                        
                        // set the reveal link click
                        $(dhtmlSubCenter.main_obj).find('.contentTypeDiv h5 a').click(
                            function(){
                                console.log('click');
                                var reveal = $(this);
                                var parent_div = $(this).parents('.contentTypeDiv');
                                $(parent_div).find('ul').slideToggle(
                                    'normal',
                                    function(){
                                        //console.log('done');
                                        //console.log($(this).css("display"));
                                        var dec_val = '&#9650;';
                                        switch($(this).css("display")){
                                            case 'none':
                                                dec_val = '&#9660;';
                                                break;
                                        }
                                        //$(this).css('background-color', 'blue');
                                        //$(reveal).css('background-color', 'red');
                                        $(reveal).html(dec_val);
                                    }
                                );
                                
                                return false;
                            }
                        );
                        
                        // set click for checkboxes
                        $(dhtmlSubCenter.main_obj).find('.contentTypeDiv input[type=checkbox]').click(
                            function(){
                                //console.log('click');
                                //console.log($(this).val());
                                //console.log($(this).prop('checked'));
                                
                                //var form_index = $(this).siblings('.form_index').val();
                                //console.log('form_index: ' + form_index);
                                
                                // get the box's special offer
                                var nltrSpecialOfferID = $(this).siblings('.nltrSpecialOfferID').val();
                                //console.log('nltrSpecialOfferID: ' + nltrSpecialOfferID);
                                
                                // if there is a corresponding solo (this click might have actually been a solo, too)
                                if(nltrSpecialOfferID > 0){
                                    //console.log('go find special offer');
                                    
                                    // find the special checkbox
                                    var special_box = $('#checked' + nltrSpecialOfferID);
                                    // get the box's LI
                                    var special_li = $(special_box).parents('li');
                                    
                                    // was the box we initially clicked checked
                                    if($(this).prop('checked')){
                                        //console.log('show the special');
                                        
                                        $(special_li).slideDown();
                                        
                                        var special_ul = $(special_box).parents('ul');
                                        
                                        var special_content_type_div = $(special_box).parents('contentTypeDiv');
                                        
                                        // is the ul holding the special hidden
                                        switch($(special_ul).css("display")){
                                            case 'none':
                                                // get the containing div's headline and reveal triangle
                                                $(special_content_type_div).find('h5 a').click();
                                                break;
                                        }
                                        
                                        // make sure special offer block is show
                                        $(dhtmlSubCenter.main_obj).find('.specialOffers').show();
                                            
                                    }else{
                                        //console.log('uncheck and hide the special');
                                        
                                        $(special_box).prop('checked', false);
                                        $(special_li).slideUp();
                                        
                                    }
                                    
                                }
                                
                            }
                        );
                        
                        
                        // hijack the form
                        $(dhtmlSubCenter.main_obj).find('form').ajaxForm(
                            {
                                timeout: 30000,
    							dataType: 'json',
                                beforeSerialize: function(){
                                    //console.log('beforeSerialize');
                                },
                                beforeSubmit: function(){
                                    console.log('beforeSubmit');
                                    var result = true;
                                    
                                    if(result){
                                        $(dhtmlSubCenter.main_obj).find('.feedback').html('Updating Your Newsletters<img src="/images/ajax-loader-bar.gif" alt="Loading...">').show();
                                        $(dhtmlSubCenter.main_obj).find('form').fadeTo('normal', 0.3);
                                        $('html,body').animate({scrollTop: 0},'slow');
                                    }
                                    
                                    //result = false; console.log('result: ' + result);
                                    return result;
                                },
    							success: function(json){
        							
        							if(json.success != undefined){
            							$(dhtmlSubCenter.main_obj).find('.feedback').html('Your Newsletters Have Been Updated').show();
            							
            							setTimeout('dhtmlSubCenter.getContent()', 3000);
        							}else{
            							$(dhtmlSubCenter.main_obj).find('.feedback').html('There was an issue updating your newsletters. Please try again.').show();
        							}
        							
                                }
                            }
                        );
                        
                        
                    } // end success
                });

        	}
        	
