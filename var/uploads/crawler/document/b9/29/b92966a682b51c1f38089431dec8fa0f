    					<script type='text/javascript'>
		
			var footer_search_obj = {};
			footer_search_obj.form_id = 'footerSearch';
			footer_search_obj.form_obj;
			footer_search_obj.search_field_name = 'search_term';
			footer_search_obj.search_field_obj;
			footer_search_obj.search_field_default_val = 'Search EverydayDiabeticRecipes.com';
			
			
			$(document).ready(
				function(){
					
					var ready_arr = [
						'FUNCTION: getJavascript_footerSearch',
						'CLASS: Search',
						'footer_search_obj.form_id: ' + footer_search_obj.form_id,
						'footer_search_obj.search_field_name: ' + footer_search_obj.search_field_name,
						'footer_search_obj.search_field_default_val: ' + footer_search_obj.search_field_default_val,
						
					];
					var ready_str = ready_arr.join("\n\n");
					//alert(ready_str);
					//console.log(ready_str);
					
					footer_search_obj.init();
				}
			); // end ready
			
			footer_search_obj.init = function(){
				
				footer_search_obj.form_obj = $('#' + footer_search_obj.form_id);
				$(footer_search_obj.form_obj).submit(
					function(){
						//console.log('submit form');
						//alert('submit form');
						
						var result = true;
						
						var search_str = $(footer_search_obj.search_field_obj).val();
						var search_str2 = search_str.replace(footer_search_obj.search_field_default_val, '');
						//console.log('search_str2: ' + search_str2);
						
						$(footer_search_obj.search_field_obj).val(search_str2);
						if(search_str2 == ''){
							alert('Please enter a search term.');
							$(footer_search_obj.search_field_obj).select();
							result = false;
						}
						
						//console.log('result: ' + result);
						//return false;
						
						if(result == true){
							$(this).find('input[type=submit]').fadeTo('normal', 0.2);
						
							$(footer_search_obj.search_field_obj).addClass('waiting');
						}
						
						
						return result;
					}
				);
				
				footer_search_obj.search_field_obj = $(footer_search_obj.form_obj).find('input[name=' + footer_search_obj.search_field_name + ']');
				
				$(footer_search_obj.search_field_obj).click(
					function(){
						//console.log('click - ' + $(this).val() );
						var current_val = $(this).val();
						// is this the default value passed to the form
						if(current_val == footer_search_obj.search_field_default_val){
							$(this).val(''); // unset the default value
						}else{
							$(this).select();
						}
						
						
						$(this).css(
							{
								'color': '#333'
							}
						);
						
					}
				);
			}
			
		</script>
