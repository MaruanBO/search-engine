			window.___gcfg = {
				lang: 'en-US',
				//parsetags: 'onload'	// All plugins on the page are automatically rendered after the page loads.
				parsetags: 'explicit'	// Plugins are rendered only with explicit calls to their respective go or render methods. When you use the explicit load in conjunction with go and render calls that point to specific containers in your page, you prevent the script from traversing the entire DOM, which can improve button rendering time.
			}
			
			var google_api_obj = {};
			google_api_obj.loaded = false;
			
			function onLoadCallbackGoogleAPI(){
				//console.log('onLoadCallbackGoogleAPI');
				google_api_obj.loaded = true;
				//console.log('google_api_obj.loaded: ' + google_api_obj.loaded + ' (23711)');
			}
			
			
