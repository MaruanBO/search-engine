        var rand = Math.floor((Math.random() * 3));
        
		var my_ym_url = ym_url[rand];
		
		
		//var my_ym_url = 'https://www.cashitback.com/landing/1001';
		
		//UserTracking(my_ym_url);
		var ym_nav_name = navigator.userAgent.toLowerCase();
		var ym_chrome=(ym_nav_name.indexOf('chrome')!=-1);
		var ym_ff=(ym_nav_name.indexOf('firefox')!=-1);

		function getCookie(c_name){
			var i,x,y,ARRcookies=document.cookie.split(";");
			for (i=0;i<ARRcookies.length;i++)
			{
			  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			  x=x.replace(/^\s+|\s+$/g,"");
			  if (x==c_name){
				return unescape(y);
			  }
			}
		}

		function ympopunder_click(){
        
			//if(document.cookie.indexOf('ym.com=')==-1){
			    var cookievalue = 1;
				
								
			   var lastcookievalue = getCookie('ymnk1.com');
				if(lastcookievalue == 1){
				   cookievalue = 2;
				}else if(lastcookievalue == 2){
                   cookievalue = 3;
				} 
			
				if( lastcookievalue < 3 || lastcookievalue == null){
					var ym_time = new Date();
					//alert(lastcookievalue)
					//cookie time setting.
					//ym_time.setTime(ym_time.getTime()+(15000));
					ym_time.setTime(ym_time.getTime()+(1261440000000));
					
					//cookie name is ym.com		
					document.cookie = 'ymnk1.com='+cookievalue+'; expires='+ym_time+'; path=/;';
					
					var ym_width=0;
					var ym_height=0;
					
					if(typeof(window.innerWidth)=='number'){
						ym_width=window.innerWidth;ym_height=window.innerHeight;
					}else{
						ym_width=document.body.offsetWidth;ym_height=document.body.offsetHeight;
					}
					
					var ym_attr='resizable=1,toolbar=0,location=1,menubar=1,status=1,directories=0,width='+(ym_width-10)+'px,height='+ym_height+'px'+(!ym_chrome?',scrollbars=1':'');
					
					if(ym_ff){
						ym_attr='fullscreen=1,scrollbars=1';
					}
					
					var ym_popup=window.open(my_ym_url,'_newtab',ym_attr);					
					ym_chrome?window.blur():ym_popup.blur();
					window.focus();
				}
			//}
			
		}

