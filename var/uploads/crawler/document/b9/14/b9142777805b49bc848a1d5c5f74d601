	  function login(){

		FB.api({
			method: 'fql.query',
			query: 'SELECT  email FROM user WHERE uid=me()'
			},
			function(response) { 
				console.log(response);
				var url =  urlpath+"checkfbemail.php?email="+response[0].email;
				httpObj10.open("POST",url,true);
				httpObj10.onreadystatechange = function(){
					if(httpObj10.readyState==4){
						 if(httpObj10.responseText == '--1--'){
							window.location.href = urlpath + 'email_mobile_verification.php';
						 }else if(httpObj10.responseText == '--2--'){
							window.location.href = landingpage;
						 }else if(httpObj10.responseText == '--3--'){
							window.location.href = urlpath + 'facebook_register.php';
						 }else if(httpObj10.responseText == '--4--'){
							window.location.href = urlpath + 'login/';
						 }
					}
				}
				httpObj10.send(null);
			}
		);
		
	}
