        ['_setCustomVar', 3, 'ContentType', contentType, 3]
    );

    if (userId) {
        _gaq.push(['_setCustomVar', 5, 'RegisteredUser', userId, 1]);
    }

    _gaq.push(['_trackPageview']);

    (function () {
