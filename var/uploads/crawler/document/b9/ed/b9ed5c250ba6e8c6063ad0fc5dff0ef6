    var userId;
    var memberId = 'Unassociated';
    var segmentType = 'Leisure';
    var contentType = 'Home';
    var contentId = '110d559f-dea5-42ea-9c1c-8a5df7e70ef9';
    var registrationStatus = 'non-member';

    if (jCore.readCookie('MemberNet')) {
        $.ajax({ url: "/handlers/getuser.aspx", async: false, dataType: "json" })
            .done(function (data) { userId = data.memberId; });
    }

    if (userId) {
        segmentType = 'RegisteredMembers';
        registrationStatus = 'member';
    }
    else {
        if ((contentType == 'Article' || contentType == 'Event' || contentType == 'Offer') && (document.referrer)) {
            if (document.referrer.indexOf("/meetings-in-san-diego") > 0) {
                segmentType = 'MeetingPlanners';
            }
            else if (document.referrer.indexOf("/traveltrade") > 0) {
                segmentType = 'TravelTrade';
            }
            else if (document.referrer.indexOf("/press") > 0) {
                segmentType = 'Press';
            }
        }
    }

    var _gaq = _gaq || [];
    _gaq.push(
