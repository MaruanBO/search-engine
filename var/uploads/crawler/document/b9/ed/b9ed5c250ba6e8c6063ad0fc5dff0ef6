    $(function () {
        $(".member-website").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'ClickMemberWebsite', contentId, 1]);
            profileAction('ClickMemberWebsite', contentId);
        });
        $(".member-email").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'ClickMemberEmail', contentId, 0]);
            profileAction('ClickMemberEmail', contentId);
        });
        $(".member-mapit").live("click", function () {
            contentId = getValue($(this).data("contentid"));
            // _gaq.push(['_trackEvent', 'ProfileActions', 'Directions', contentId, 0]);
            profileAction('Directions', contentId);
        });
        $("#print-directions").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'PrintDirections', contentId, 0]);
            profileAction('PrintDirections', contentId);
        });
        $("#get-directions").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'GetDirections', contentId, 0]);
            profileAction('GetDirections', contentId);
        });
        $(".member-facebook, .member-twitter, .member-linkedin, .member-youtube").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'SocialShare', contentId, 0]);
            profileAction('SocialShare', contentId);
        });
        $(".member-tickets").click(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'CTAProfileClick', contentId, 0]);
            profileAction('CTAProfileClick', contentId);
        });
        $(".member-booknow").live("click", function () {
            contentId = getValue($(this).data("contentid"));
            //_gaq.push(['_trackEvent', 'ProfileActions', 'CTAProfileClick', contentId, 0]);
            profileAction('CTAProfileClick', contentId);
        });
        $("div.slide.pager .tile > a", $("#article-slides")).each(function () {
            contentId = getValue($(this).data("contentid"));
            //_gaq.push(['_trackEvent', 'MemberAds', 'Impression_Article', contentId, 0, true]);
            $(this).click(function () {
                contentId = getValue($(this).data("contentid"));
                //_gaq.push(['_trackEvent', 'MemberAds', 'Click_Article', contentId, 1]);
                memberContentClick('Article', contentId);
                //_gaq.push(['_trackEvent', 'ProfileActions', 'AdClick', contentId, 0, true]);
                profileAction('AdClick', contentId);
            });
        });
        $(".event-link").each(function () {
            contentId = getValue($(this).data("contentid"));
            //_gaq.push(['_trackEvent', 'MemberAds', 'Impression_Event', contentId, 0, true]);
            memberContentImpression('Event', contentId);
            $(this).click(function () {
                contentId = getValue($(this).data("contentid"));
                //_gaq.push(['_trackEvent', 'MemberAds', 'Click_Event', contentId, 1]);
                memberContentClick('Event', contentId);
                //_gaq.push(['_trackEvent', 'ProfileActions', 'AdClick', contentId, 0, true]);
                profileAction('AdClick', contentId);
            });
        });
        $(".offer-link").click(function () {
            contentId = getValue($(this).data("contentid"));
            //_gaq.push(['_trackEvent', 'MemberAds', 'Click_Offer', contentId, 1]);
            memberContentClick('Offer', contentId);
            //_gaq.push(['_trackEvent', 'ProfileActions', 'AdClick', contentId, 0, true]);
            profileAction('AdClick', contentId);
        });
        $(".offers-module .offer").each(function () {
            contentId = getValue($(this).find("> a").data("contentid"));
            //_gaq.push(['_trackEvent', 'MemberAds', 'Impression_Offer', contentId, 0, true]);
            memberContentImpression('Offer', contentId);
        });
        $("body.article, body.event-landing-detail, body.offer-landing-detail, body.member-detail").each(function () {
            //_gaq.push(['_trackEvent', 'ProfileActions', 'Content_View', contentId, 0, true]);
            profileAction('Content_View', contentId);
        });
    });

    function getValue(value) {
        return (value) ? value + '' : 'Unassociated';
    }
</script>


