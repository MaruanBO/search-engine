// <![CDATA[

    function toggleWebCamera(cam)
    {
        // hide cameras    
        $$('.webcam').each(
           function (e) {
              e.hide()
           } 
        );        

        // show camera
        $(cam).show();        
        
    }
    
                        // default to
                    toggleWebCamera('webcam_homecoming');                
                
// ]]>
</script>
        

