var tracking={
	'countryName':null,
	'searchTerm':null,
	'pathName':window.location.pathname,
	'domain':'com',
	'isDev':false
}

if (document.location.href.indexOf('vonagenetworks.net')>=0) {
tracking.isDev=true;
}
 
if (getQueryVariable('q')) {
  tracking.searchTerm=encodeURIComponent(getQueryVariable('q').replace(/(<([^>]+)>)/ig,""));
}

if (document.location.pathname.indexOf('phone-plans/country')>0) {
  tracking.countryName=document.location.pathname.slice(document.location.pathname.lastIndexOf('/')+1);
}
</script>

