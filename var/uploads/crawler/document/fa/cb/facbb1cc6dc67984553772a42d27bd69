	 function _quantgc(n) {
	 var c=document.cookie;if(!c)return '';
	 var i=c.indexOf(n+"=");if(-1==i)return '';
	 var len=i+n.length+1;
	 var end=c.indexOf(";", len);
	 return c.substring(len,end<0?c.length:end);
	 }
	 quantSegs = "";
	 var _qsegs = _quantgc('__qseg').split('|');
