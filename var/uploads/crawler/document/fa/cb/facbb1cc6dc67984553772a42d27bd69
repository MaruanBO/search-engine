<script>
   function getBreakingCookie(c_name)
   {
   var i,x,y,ARRcookies=document.cookie.split(";");
   for (i=0;i<ARRcookies.length;i++)
   {
     x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
     y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
     x=x.replace(/^\s+|\s+$/g,"");
     if (x==c_name)
       {
       return unescape(y);
       }
     }
   }
   function setBreakingcookie(idname) {
     var date = new Date();
     date.setTime(date.getTime()+(4*60*60*1000));
     var expires = "; expires="+date.toGMTString();
   	var value = "hidden"
     document.cookie = idname+"="+value+expires+"; path=/";
   }
   function readBreakingcookie(idname){
   var cookiename=getBreakingCookie(idname);
   	if (cookiename=="hidden") {
   	    $("#"+idname).toggle();
   	}
   }
   function hideMe(e){
   		$("#"+e).toggle("slow");
   		setBreakingcookie(e);
    	  return false; //Prevent the browser jump to the link anchor
   }
   var ids = $('.liveAlert').get();
   for(var i in ids) {
     var idvar = $('.liveAlert')[i].id;
   	readBreakingcookie(idvar);
   }
</script>
 



