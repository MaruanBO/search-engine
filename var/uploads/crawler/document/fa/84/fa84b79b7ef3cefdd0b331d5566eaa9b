                        <script>
$(document).ready(function() {
    $('#sign_in_button').click(function(e) {
        
        e.preventDefault();
        $('#sign_in_div').fadeIn();
        
    });

    $('.x').click(function(){
        $('#sign_in_div').fadeOut();
        return false
    });

    $('#login_form').validate({
        invalidHandler: function() {
            $('#sign_in_error').show();
            $('#login_form #email_div, #login_form #password_div').addClass('error');
        },
        submitHandler: function(form) {
            var form_data = formToJson('#login_form');

            $.ajax({
                url: 'https://www.michiganlottery.com/v1/milotto/players/profile/post/auth_token.json',
		type: 'POST',
                data: {ng: 1, ajax: 1, username: form_data.email, password: form_data.password, sk: 1},
                success: function(data) {
                    console.log(data);
                    $('#webkey').val(data.result.webkey);
                    $('#sk').val(data.result.session_key);
                    $('#sign_in_link').submit();
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    console.log(XMLHttpRequest.responseJSON, textStatus, errorThrown);

                    if(XMLHttpRequest.responseJSON.result.error == 'invalid-state') {
                        triggerInvalidStateModal();
                    } else {
                        $('#sign_in_error').show();
                        $('input[name=email]').addClass('error');
                        $('input[name=password]').addClass('error');
                        $('#password_div').addClass('error');
                        $('#email_div').addClass('error');
                    }
                }
            });

            return false;
        }
    });
});
</script>

