<script>
	var channel = $.cookie('lotto_chan2');

	

	

	lottery.wurfl(function(wurfl) {
		if(!channel) {
			channel = wurfl.is_wireless_device && !wurfl.is_tablet? 'm' : 'def';
			$.cookie('lotto_chan2', channel, {expires: 1});
		}

		$(document).ready(function(){
			
			if((channel == 'def' && (wurfl.is_wireless_device && !wurfl.is_tablet))) {
				$('#divEmbedLoginWrapper').hide();
				$('.tablet_show').show();
				$('.tablet_hide').hide();
			}
		});

		var halt = false;
		
		if(wurfl.is_wireless_device && !wurfl.is_tablet) {
			

			
		}

		if(!halt && (wurfl.is_tablet || false) && false) {
			window.top.location = 'https://www.michiganlottery.com/html_keno?vpage=intro';
			halt = true;
		}

		if(!halt && channel == 'm' && true && true) {
			window.top.location = 'http://m.michiganlottery.com/?c=m';
		}

                if(wurfl.is_wireless_device) {
			$(document).ready(function() {
				$('#back_to_mobile').show();
			});
		}
	});
</script>



