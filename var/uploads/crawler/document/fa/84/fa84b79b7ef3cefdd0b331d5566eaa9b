                                var game_div = $(this).parent().attr("id");
                                var game = game_switch[game_div];

                                $('#results_' + game + ' .focus').addClass('after').removeClass('focus');

                                lottery.previous(game, function(data) {

                                	data.details = data.result.detail;
                                	data.numbers = data.result.winning_numbers[0];

                                if (game == 3 || game == 4) {
                                    data.winning_numbers = data.result.winning_numbers;
                                    var grouped = lottery.group_daily34(data.result.winning_numbers);
                                    data.mid = grouped[0].mid;
                                    data.eve = grouped[0].eve;
                                }
                                    if (data.numbers != undefined) {
                                        $('#results_' + game).jqotepre(".results" + game, data, '$');
                                    } else {
                                        lottery.next(game);
                                        $('#results_' + game + ' .after').first().addClass('focus').removeClass('after');
								        $("div.game_panel[name='" + game + "'] > .leftArrow").hide();
                                    }
                                });

								$(this).closest(".game_panel").find(".rightArrow").show();
                            });

                            $('.rightArrow').click(function(e){
                                var game_div = $(this).parent().attr("id");
                                var game = game_switch[game_div];
                                var r = $('#results_' + game + ' .focus');
                                var next_date = $(this).parent().find('.focus').next().attr('id');

                                if (next_date != undefined) {
                                    r.removeClass('focus');
                                    r.next('.result').addClass('focus').removeClass('after');
                                    lottery.next(game);

									if(!$(this).parent().find('.focus').next().attr('id'))
									{
										$(this).closest(".game_panel").find(".rightArrow").hide();
									}
                                }

								$(this).closest(".game_panel").find(".leftArrow").show();
                            });

			    $('#kj_content').fastJax({template: '#keno_jack', url: 'https://www.michiganlottery.com/v1/milotto/players/jackpots.json'});
