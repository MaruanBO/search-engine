		<script>
			$(document).ready(function() {
				try {
					OnRewardPointsBalanceUpdate(0);
					KeepAlive();
				} catch(err) {
					console.log(err);
				};

				
				try {
					var login_details = ng_fw.getSessionDetails();

					if(login_details) {
						OnSessionStatusUpdate('Login', login_details.PlayerID, login_details.SessionToken, true);
					}
				} catch(err) {
					console.log(err);
				};
				

				

				$('a:not(.bound)').addClass('bound').bind('click', function(e) {
					if(!$(this).attr('href').match(/logout/)) {
						try {
							KeepAlive();
						} catch(err) {
							console.log(err);
						};
					}

					var wurfl = lottery.boolify(JSON.parse($.cookie('wurfl')));

					
					if($(this).attr('href').match(/\b(online_game_horoscope_demo|online_game_unwrap_the_cash|online_game_cashword_demo|online_game_wild_time_doubler_demo|online_games|online_game_blazing_red_7s|join_now|online_game_super_money_match_demo|online_game_bingo_doubler|online_game_instant_keno|online_game_diamond_payout_demo|online_game_pure_gold_demo|online_game_pure_gold|online_game_bingo_doubler_demo|online_game_blazing_red_7s_demo|online_game_instant_keno_demo|update_my_profile|online_game_alfredo|ilottery_lobby|online_new_games|online_keno_games|instant_online_games|online_game_diamond_payout|online_game_unwrap_the_cash_demo|online_game_10X_the_cash_demo|online_game_cashword|online_game_alfredo_demo|online_game_instant_riches_demo|online_game_super_money_match|online_game_10X_the_cash|online_game_instant_riches|online_game_horoscope|online_game_wild_time_doubler)\b/) && $.cookie('tablet_modal_interrupt_suppress') != 'true' && (!wurfl.is_tablet && wurfl.is_wireless_device)) {
						if($(this).hasClass('main_nav_item') && $(this).parent('li').children('.nav_panel').css('display') != 'block') {
							return true;
						}

						$('body').prepend('<div id="tablet_modal" class="overlay"></div>');
						$('#tablet_modal').fadeIn(function(){
							$('#tablet_modal').append($('#tablet_modal_content'));
							$('#tablet_modal_content').fadeIn();
						});

						$('#tablet_modal_continue').attr('href', $(this).attr('href'));
						return false;
					}

					var href = $(this).attr('href');

					if(href.match(/michiganlottery.com/) && $('#ng_id').val() && !$(this).attr('href').match(/ng_id/)) {
						var params = 'ng_id=' + $('#ng_id').val() + '&ng_token=' + $('#ng_token').val();

						$(this).attr('href', href.replace('?', '?' + params + '&'));
					}

					return true;
				});
			});
		</script>
