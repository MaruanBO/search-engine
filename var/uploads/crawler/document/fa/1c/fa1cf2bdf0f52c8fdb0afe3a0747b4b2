		<script type="text/javascript">
			var newspopTimer;
			function popupNewsletterSubmit(theForm) {
				$('#popupEmailError').hide();
				$('#popupConfirmError').hide();
				if (EmailAddressIsInvalid(theForm.emailFrom.value)) {theForm.emailFrom.focus(); $('#popupEmailError').show(); return false;}
				if (theForm.emailFrom.value.toUpperCase() != theForm.emailConfirm.value.toUpperCase()) {theForm.emailConfirm.focus(); $('#popupConfirmError').show(); return false;}
				return true;
			}
			function popupNewsletterFirstEmail(theForm,theButton) {
				if (EmailAddressIsInvalid(theForm.emailFrom.value)) {theForm.emailFrom.focus(); $('#popupEmailError').show(); }
				else {$('#popupEmailConfirm').slideDown(); $(theButton).hide();}
			}
			function popupNewsletterClose() {
				$('#popupNewsletterDiv').animate({'left':'-500px'},500);
				clearTimeout(newspopTimer);
			}
			var newsletterCookie = decodeURIComponent($.cookie("newsletterPopupShown"));
			$(document).ready(function () {
				$("#popupNewsletterDiv").bind("click",function(e) {
					clearTimeout(newspopTimer);
				});
				$('#popupNewsletterDiv').css('top',($(window).height()-$('#popupNewsletterDiv').height()-150) + 'px');
				if (newsletterCookie == 'undefined') {
					$.cookie("newsletterPopupShown", 'true', { expires : 365, path: '/', domain: '.omahasteaks.com' });
					$('#popupNewsletterDiv').animate({'left':($(window).width()-$('#popupNewsletterDiv').width()-60)/2 + 'px'},1500);
					newspopTimer = setTimeout(function() {popupNewsletterClose();}, 15000);
					s.linkTrackVars = 'prop15'
					s.prop15 = 'Newsletter Popup Slider shown';
					s.tl(true,'o');
				}
			});
		</script>
	</div>


