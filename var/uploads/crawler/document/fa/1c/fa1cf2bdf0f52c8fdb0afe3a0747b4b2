		<script type="text/javascript">
			$('#viewCartTab').bind('click',function(e){
			  e.preventDefault();
				popupCart(document.getElementById('viewCartLink'));
			});

			function popupCart(anchor) {
				$('#viewCartTab').unbind('click');
				$('#scrollMinicart').css('height',Math.min(260,$('#scrollMinicart')[0].offsetHeight) + 'px');
				if ($('#scrollMinicart').height() >= 260) { $('#scrollMinicart').attr('class','scrollMinicartBorder'); $('#cartTable').css('width',(393 - (ie?30:0)) + 'px'); }
					else { $('#cartTable').css('width',410 - (ie?8:0) + 'px'); }
				$('#popupMinicart').css('height',Math.min(360,$('#popupMinicart').height() + (ie?20:0)) + 'px');
				var width=$('#popupMinicart').width();
				// $('#modalPopup').bind('mouseleave',function(e){ hidePopupMinicart(); });
				$('#modalPopup').html($('#hiddenMinicart').html());
				$('#modalPopup').css('left', (getPageSize()[0]/2 + 494 - width) + 'px');
				$('#modalPopup').css('top', Math.max(getScrollXY()[1], (findPosition(anchor)[1] + 22)) + 'px');
				$('#modalPopup').show();
				return false;
			}

			function hidePopupMinicart() {
				$('#modalPopup').unbind('mouseleave');
				$('#viewCartTab').bind('click',function(e){ e.preventDefault(); popupCart(document.getElementById('viewCartLink')); });
				hideModalPopup();
				$('#modalPopup').html("");
			}
		</script>


