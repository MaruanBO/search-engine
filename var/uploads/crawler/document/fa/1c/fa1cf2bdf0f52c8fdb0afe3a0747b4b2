	<script>

	selectedLayer = 1;
	var numRotations = $("#anchors a").length;
	function flipLayer(i,setNextFlip) {
		var nextLayer = (i%numRotations)+1;
		clearTimeout(flipBanner);
		if (setNextFlip===undefined) { setNextFlip=false; }
		$("#promo" + selectedLayer).fadeOut(300, function() {
			$("#anchors a").removeClass()
			document.getElementById('anchor' + nextLayer).className = 'linkon';
			$("#rotateBullets").appendTo("#promo" + nextLayer);
			$("#rotateBullets img").removeClass().addClass('bulletOff');
			document.getElementById('rotate' + nextLayer).className = 'bulletOn';
			$("#morepages > div").hide();
			$("#promo1").hide();
			$("#promo" + nextLayer).fadeIn(200);
			selectedLayer = nextLayer;
			if (!setNextFlip) { nextLayer = 0; }
			setNextFlipLayer(nextLayer);
		});
		return false;
	}

	function setNextFlipLayer(nextLayer) {
		if (nextLayer == 0) { // no next flip, pause
			clearTimeout(flipBanner);
		} else {
			clearTimeout(flipBanner);
			flipBanner = setTimeout("flipLayer("+nextLayer+",true);",8000);
		}
	}

	$(document).ready(function () {
		if ( $("#morepages").text() == "" ){
			$("#morepages").load(AddOsUrlParms('/servlet/OnlineShopping?Dsp=325'), { force: 'post' }, function(){
				$("#morepages").show();
				flipBanner = setTimeout("flipLayer(selectedLayer,true);",10000);
			});
		}
		$('#rotateBullets img, #anchors a').mouseover(function() {
			if ( $("#morepages").text() == "" ){ return false; }
			var nextLayer = ((this.id.substr(6,1)-1)%numRotations)+1;
			clearTimeout(flipBanner);
			$("#promo" + selectedLayer).hide();
			$("#rotateBullets img").removeClass().addClass('bulletOff');
			document.getElementById('rotate' + nextLayer).className = 'bulletOn';
			$("#rotateBullets").appendTo("#promo" + nextLayer);
			$("#anchors a").removeClass()
			document.getElementById('anchor' + nextLayer).className = 'linkon';
			$("#morepages > div").hide();
			$("#promo1").hide();
			$("#promo" + nextLayer).show();
			selectedLayer = nextLayer;
		}).mouseout(function() {
			var nextLayer = ((this.id.substr(6,1)-1)%numRotations)+1;
			setNextFlipLayer(nextLayer);
		});
	});

	</script>

	
