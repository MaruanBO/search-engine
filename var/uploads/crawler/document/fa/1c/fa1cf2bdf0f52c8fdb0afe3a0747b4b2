	$(".componentsHover li").bind('mouseenter',function(e){
		$(this).siblings("li").removeClass('componentsHoverDefault');
		$("#"+this.id.substr(0,4)+"Image").attr('src','' + window[this.id.substr(0,4)][this.id.substring(4)]);
