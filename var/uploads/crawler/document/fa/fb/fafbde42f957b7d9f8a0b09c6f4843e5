			});

			function onPlayerStateChange(frame, slider) {
				return function(event) {
			        if(event.data == YT.PlayerState.PLAYING) {
			            jQuery(slider).flexslider("pause");
			        }
			        if(event.data == YT.PlayerState.PAUSED) {
			        	jQuery(slider).flexslider("play");
			        }
		    	}
			}
