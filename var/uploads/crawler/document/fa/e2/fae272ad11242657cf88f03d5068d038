    <script>
    var objItem = document.getElementsByTagName("ie:menuitem");
jQuery(document).ready(function ($) {
    if (objItem != null) {
        for (var i = 0; i < objItem.length; i++) {
            var itm = objItem[i];
            if (itm != null) {
                var itemID = $('#' + itm.id);
                if (itemID != null) {
                    if (itemID.attr("text") == "Sign Out" || itemID.attr("text") == "My Settings"
                      || itemID.attr("text") == "Personalize this Page" || itemID.attr("text") == "Request Access"
                      || itemID.attr("text") == "Sign in as Different User") {
                        itemID.remove();
                    }
                }
            }
        }
    }
});

$(document).ready(function () {
    if (objItem != null) {
        for (var i = 0; i < objItem.length; i++) {
            var itm = objItem[i];
            if (itm != null) {
                if ($('#' + itm.id).attr("text") == "Sign in as Different User") {
                    $('#' + itm.id).remove();
                }
            }
        }
    }
});

$(document).ready(function () {
    if (objItem != null) {
        for (var i = 0; i < objItem.length; i++) {
            var itm = objItem[i];
            if (itm != null) {
                if ($('#' + itm.id).attr("text") == "Sign Out") {
                    $('#' + itm.id).remove();
                }
            }
        }
    }
});
