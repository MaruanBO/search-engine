<script type="text/javascript">
    function SearchRedirect() {
        var txtSearchword = document.getElementById('ctl00_Search_txtSearch');
        if (txtSearchword.value != 'SEARCH' && txtSearchword.value != null && txtSearchword.value.match('script>') == null) {
            //            window.location = '/search/Pages/searchresults.aspx?k=' + encodeURIComponent(txtSearchword.value);
            window.location = '/search/searchresults/default.aspx?k=' + encodeURIComponent(txtSearchword.value);
        }
        else {
            window.alert('Sorry, please enter a search keyword.');
        }
    }
    function SearchcontrolEnter(txtSearch, ImgbtnObj, txtboxevent) {
        var keyCode = (txtboxevent.keyCode ? txtboxevent.keyCode : txtboxevent.which);
        if (keyCode == 13) {
            var txtSearchbox = document.getElementById(txtSearch);

            if (txtSearchbox.value != null && txtSearchbox.value != 'SEARCH' && txtSearchbox.value != '' && txtSearchbox.value.match('<script>') == null && txtSearchbox.value.match('<\/script>') == null) {
                //window.location = "/search/Pages/searchresults.aspx?k=" + encodeURIComponent(txtSearchbox.value);
                window.location = '/search/searchresults/default.aspx?k=' + encodeURIComponent(txtSearchbox.value);
                return false;
            }
            else {
                alert('Sorry, please enter a search keyword.');
                return false;
            }
        }
        else
            return true;
    }

    function SearchFocus(txtElem, strDefault) {

        if (txtElem.value == strDefault) {
            txtElem.value = '';
            txtElem.select();
        }
    }

    function SearchOnblur(txtElem, strDefault) {
        if (txtElem.value == '') {
            txtElem.value = strDefault;
        }
    }
</script>
