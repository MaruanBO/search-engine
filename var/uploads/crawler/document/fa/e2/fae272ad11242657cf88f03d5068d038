<script type="text/javascript">
    $(function () {
        try {
            var prm = Sys.WebForms.PageRequestManager.getInstance();
            if (!(prm)) return;
            document.orginalTitle = document.title;
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function (s, e) {
                if (document.title.replace(/\s/g, "").length == 0)
                    document.title = document.orginalTitle;
            });
        }
        catch (err) {

        }
    });
    function AddWatermark() {
        if ($(".signUser").val() == "") {
            $(".signUser").addClass("watermarkClass");
        }
        if ($(".signPass").val() == "") {
            $(".signPass").addClass("PassWatermarkClass");
        }
        if ($(".signUserEmail").val() == "") {
            //$(".signUser").WaterMark();
            $(".signUserEmail").addClass("watermarkClass");
        }
    }
    function AddPasswordWater() {
        if ($(".signPass").val() == "") {
            $(".signPass").addClass("PassWatermarkClass");
        }
    }
    function FocusUserName() {
        var Username = document.getElementById('ctl00_lv1_SignInUC_SignInControl_UserName');

        if (Username.value == "") {
            $(Username).removeClass("watermarkClass")
            $(Username).addClass("signUser");
        }
    }
    function BlurUserName() {
        var Username = document.getElementById('ctl00_lv1_SignInUC_SignInControl_UserName');
        if (Username.value == "") {
            $(Username).removeClass("signUser")
            $(Username).addClass("watermarkClass");
        }
    }
    function Focuspassword() {
        var Password = document.getElementById('ctl00_lv1_SignInUC_SignInControl_Password');

        if (Password.value == "") {
            $(Password).removeClass("PassWatermarkClass")
            $(Password).addClass("signPass");
        }
    }
    function BlurPassword() {
        var Password = document.getElementById('ctl00_lv1_SignInUC_SignInControl_Password');
        if (Password.value == "") {
            $(Password).removeClass("signPass")
            $(Password).addClass("PassWatermarkClass");
        }
    }
    function AddEmailwatermark() {
        var ForgetPassEmail = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserName');
        var UserNameRequired = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserNameRequired');
        var clrfailure = document.getElementById('divFailuretxt');
        UserNameRequired.errormessage = '';
        UserNameRequired.innerHTML = '';
        if (ForgetPassEmail.value == "") {
            $(ForgetPassEmail).removeClass("signUserEmail")
            $(ForgetPassEmail).addClass("watermarkClass");
            UserNameRequired.value = "";
            if (clrfailure != null)
                clrfailure.innerHTML = "";
        }
    }
    function Removewatermark() {

        var ForgetPassEmail = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserName');
        if (ForgetPassEmail.value == "") {
            $(ForgetPassEmail).removeClass("watermarkClass")
            $(ForgetPassEmail).addClass("signUserEmail");
        }
    }
    function ShowsigninPop() {
        var lblUserError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblUserName');
        var lblPwdError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblPassword');
        var Username = document.getElementById('ctl00_lv1_SignInUC_SignInControl_UserName');
        var Password = document.getElementById('ctl00_lv1_SignInUC_SignInControl_Password');

        if (Username.value != 'Email address' && Password.value != 'Password') {
            lblUserError.innerHTML = '';
            lblPwdError.innerHTML = '';
        }

        $("#ctl00_lv1_SignInUC_LoginArea").css("display", "block");
    }

    $(document).mouseup(function (e) {
        var lblUserError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblUserName');
        var lblPwdError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblPassword');
        var lblPwdlogError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblLoginError');
        var ForgetPassEmail = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserName');
        var clrfailure = document.getElementById('divFailuretxt');
        var UserNameRequired = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserNameRequired');

        lblUserError.innerHTML = '';
        lblPwdError.innerHTML = '';
        UserNameRequired.errormessage = '';
        UserNameRequired.innerHTML = '';
        ForgetPassEmail.innerHTML = '';
        // Check if the click is outside the popup
        if ($(e.target).parents("#ctl00_lv1_SignInUC_divForgetpwd").length == 0 && !$(e.target).is("#ctl00_lv1_SignInUC_divForgetpwd") && !$(e.target).is("#ctl00_lv1_SignInUC_divForgetpwd")) {          
            lblUserError.innerHTML = '';
            lblPwdError.innerHTML = '';
            lblPwdlogError.innerHTML = '';
            ForgetPassEmail.value = "";
            AddEmailwatermark();
            if (clrfailure != null)
                clrfailure.innerHTML = "";
            // Hide the popup
            $("#ctl00_lv1_SignInUC_divForgetpwd").css("display", "none");
        }
    });
    $(document).mouseup(function (e) {
        var lblUserError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblUserName');
        var lblPwdError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblPassword');
        var lblPwdlogError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblLoginError');
        var lblEmailError = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_lblUserNameError');
        lblUserError.innerHTML = '';
        lblPwdError.innerHTML = '';

        // Check if the click is outside the popup
        if ($(e.target).parents("#ctl00_lv1_SignInUC_LoginArea").length == 0 && !$(e.target).is("#ctl00_lv1_SignInUC_LoginArea") && !$(e.target).is("#ctl00_lv1_SignInUC_loginLink")) {
            lblUserError.innerHTML = '';
            lblPwdError.innerHTML = '';
            lblPwdlogError.innerHTML = '';
            lblEmailError.innerHTML = '';
            // Hide the popup
            $("#ctl00_lv1_SignInUC_LoginArea").css("display", "none");
        }
    });

    function Login() {
        var Username = document.getElementById('ctl00_lv1_SignInUC_SignInControl_UserName').value;
        var Password = document.getElementById('ctl00_lv1_SignInUC_SignInControl_Password').value;
        var lblUserError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblUserName');
        var lblPwdError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblPassword');
        var lblPwdError = document.getElementById('ctl00_lv1_SignInUC_SignInControl_lblLoginError');

        lblPwdError.innerHTML = '';
        lblUserError.innerHTML = '';

        if (!Username && !Password) {
            lblUserError.innerHTML = 'Please enter your email address';
            lblPwdError.innerHTML = 'Please enter your password';
            return false;
        }
        else if (!Username) {
            lblUserError.innerHTML = 'Please enter your email address';
            lblPwdError.innerHTML = "";
            return false;
        }
        else if ((!validateEmail(Username)) || (!validateEmail(Username) && Password)) {
            if (!validateEmail(Username) && !Password) {
                lblUserError.innerHTML = 'Invalid email address or password';
                lblPwdError.innerHTML = "Please enter your password";
                return false;
            }
            else {
                lblUserError.innerHTML = 'Invalid email address or password';
                lblPwdError.innerHTML = "";
                return false;
            }
        }
        else if (Username && !Password) {
            lblPwdError.innerHTML = 'Please enter your password';
            lblUserError.innerHTML = "";
            return false;
        }
        else if (!Username && Password) {
            lblUserError.innerHTML = 'Please enter your email address';
            lblPwdError.innerHTML = "";
            return false;
        }
        else if (!Password) {
            lblPwdError.innerHTML = 'Please enter your password';
            lblUserError.innerHTML = "";
            return false;
        }
    }

    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }
    function CheckValidity() {        
        var UserNameRequired = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_UserName').value;
        var lblUserError = document.getElementById('ctl00_lv1_SignInUC_PasswordRecovery1_UserNameContainerID_lblUserNameError');
        if (!UserNameRequired) {
            lblUserError.innerHTML = 'Enter Email Address';
            return false;
        }
    }
</script>
