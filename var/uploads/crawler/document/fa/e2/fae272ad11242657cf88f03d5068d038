 $(document).ready(function () {
var href = window.location.href.toLowerCase();
$('#topNavMenu li').each(function () {
var lnkHref = $(this).find('a').attr('href').trim().toLowerCase();
if ((href.trim() == lnkHref || href.indexOf(lnkHref)) > 0)
if($(this).attr('class') == 'menuTop'){
$(this).removeClass('menuTop');
$(this).addClass('activemenuTop');
}
});
 var timeoutId;
 $("ul#topNavMenu li").hover(function () {
 var topid = $(this).attr('id'); 
if (!timeoutId) {
timeoutId = window.setTimeout(function() {
   timeoutId = null;
 $("#"+topid).find("div").stop().fadeIn(700);
 }, 400);
 }
},
function () {
var topid = $(this).attr('id');
 if (timeoutId) {
 window.clearTimeout(timeoutId);
 timeoutId = null;
}
else {
$(this).find("div").stop().fadeOut("fast");
}
});
});
</script>

