<script type="text/javascript">
    $(document).ready(function () {
       
        var cookie = getCookie("weeklyAddUrl");
        if (cookie == "" || cookie == null) {
            initGeolocation();
        }
        else {
            $("#weeklyAddLink").attr("href", cookie);
        }
        $("#ctl00_Toplinks_txtStrLocator").blur(function () {
            var divID = document.getElementById('Locatordiv');
            divID.style.display = 'none';
        });

        $("#ctl00_Toplinks_txtStrLocator").focus(function () {
            var divID = document.getElementById('Locatordiv');
            divID.innerHTML = 'Enter Zip or City, State';
            divID.style.display = 'block';
        });
        $("#ctl00_Toplinks_submit").click(function () {
            var valEx = /^[0-9]{1,6}|[a-zA-Z]+,\s?[a-zA-Z]+$/
            var searchText = document.getElementById("ctl00_Toplinks_txtStrLocator").value.trim();
            if (searchText != 'LOCATOR' && searchText != "" && searchText.match(valEx)) {
                window.location = "/_layouts/WDCorp/Locator.aspx?search=" + $("#ctl00_Toplinks_txtStrLocator").val().trim();
            }
            else if (searchText == "" || searchText == 'LOCATOR') {
                var divID = document.getElementById('Locatordiv');
                divID.innerText = 'Enter Zip or City, State';
                divID.style.display = 'block';
                $("#ctl00_Toplinks_txtStrLocator").focus();
                alert("Please enter a city, state or ZIP code");
            }
            else {
                var divID = document.getElementById('Locatordiv');
                divID.innerText = 'Enter Zip or City, State';
                divID.style.display = 'block';
                $("#ctl00_Toplinks_txtStrLocator").focus();
                alert("Please enter both City and State separated by a comma. Ex: Jacksonville, FL");
            }
            return false;
        });
    });
    function initGeolocation() {
        
        if (navigator.geolocation) {
            var optn = {
                enableHighAccuracy: false,
                maximumAge: Infinity,
                timeout: 2000
            };
            navigator.geolocation.getCurrentPosition(success, Failure);
        }
        else {
            
        }
    }
    function callApi(latlng) {
        $.ajax({
            type: 'POST',
            data: "{latlng:'" + latlng + "'}",
            url: 'https://www.winndixie.com/_layouts/WDCorp/GetPincode.aspx/Geturl',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (datas) {
                setCookie("weeklyAddUrl", datas.d.url, 1);
                $("#weeklyAddLink").attr("href", datas.d.url);
            }
        });
        return false;
    }
    function Failure(error) {
       
        if (error.code != 1) {
            
        }
        else {
            setCookie("weeklyAddUrl", "/Savings/WeeklyAds/default.aspx");
            $("#weeklyAddLink").attr("href", "/Savings/WeeklyAds/default.aspx");
        }
    }
    function success(position) {
        
        var x = position.coords.longitude;
        var y = position.coords.latitude;
        callApi(x + "," + y);
    }
    function fail() {
        
        var lat = geoip_latitude();
        var lon = geoip_longitude();
        callApi(lat + "," + lon);
    }
    function setCookie(cname, cvalue, exdays) {

        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }
    function getCookie(cname) {

        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }
</script>
