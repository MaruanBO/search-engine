// BIMreg Hooks

bim_reg.subscribeToChannel("bim_reg_ready", function(message) {
    if (!bim_reg.m.user.isLoggedIn()) {
    	$("li#login-item.top-level-item, li#login-item.top-level-item #login-button").removeClass("hide");
    }
});

