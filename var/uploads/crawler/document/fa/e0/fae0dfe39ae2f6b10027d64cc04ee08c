		     $(".beloFBShareButton").unbind("click").bind("click", function(){
		     	id=$(this).attr("id").split("-");
		     	if($("#beloFBShare"+id[1]).width()<80){    	
		    		$("#beloTwShare"+id[1]).css("width","0px");
		    		$("#topStoryShare"+id[1]).css("width","210px");
		    		$("#beloFBShare"+id[1]).css("width","128px");
		     	}else{
		     		$("#beloTwShare"+id[1]).css("width","0px");
		    		$("#beloFBShare"+id[1]).css("width","0px");
		    		$("#topStoryShare"+id[1]).css("width","78px");
		     	}
		        return false;
		    });
		    $(".beloTwShareButton").unbind("click").bind("click", function(){
		    	id=$(this).attr("id").split("-");
		    	if($("#beloTwShare"+id[1]).width()<80){ 
		    		$("#beloFBShare"+id[1]).css("width","0px");
		    		$("#topStoryShare"+id[1]).css("width","168px");
		    		$("#beloTwShare"+id[1]).css("width","88px");
		    	}else{
		    		$("#beloTwShare"+id[1]).css("width","0px");
		    		$("#beloFBShare"+id[1]).css("width","0px");
		    		$("#topStoryShare"+id[1]).css("width","78px");
		    	}
		        return false;
		    });
		});
	</script>
</div>

	</div><!-- end og bigStorydiv -->
