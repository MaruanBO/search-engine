<!--This script is used only for dev purpose.-->
<script>
        function LoadWidgetsForDev($) {
                $("#flightWidget").flightSearch({
                    flightHolder: "flightWidget",
                    travelersTemplateID: "num-travelers-template",
                    adultsdropdownID: "adult-travelers",
                    seniorsdropdownID: "senior-travelers",
                    minorsdropdownID: "minor-travelers",
                    minorAgeSelectorID: "childrenPanel",
                    serviceClassDropdownID: "ddClass",
                    errMsgHolderID: "errMsg",
                    prefAirlineID: "preferred-airline",
                    tripTemplateID: "single-trip-template",
                    formID: "flight-search-form",
                    tripTypeGroupID: "trip-type-group",
                    submitButtonID: "search-btn",
                    flexiDateID: "chkFlexiDts",
                    directFlightID: "chkDirectFlt",
                    openJawID: "open-jaw-switch",
                    serviceClassDropdownHolderID: "select_class",
        	isPrimary:true,
            timeSwitchHolderID: "time-ctrl-holder",
            timeSwitchID: "time-switch"
                });
                $("#hotelWidget").hotelSearch({
                    tripTemplateID: "single-hotel-template",
