<script type="text/javascript">
    try {
        $(document).ready(function () {
            var sessionIdLs = localStorage.getItem("sidls");
            var sessionId = "773346501";
            if (sessionIdLs != null && sessionIdLs != sessionId) {
                localStorage.removeItem('sidls');
            }

            $("#recentSearch").ShowRecentSearch();
        });

        $("#widget-tabs > li").click(function () {
            var selectedTabId = $(this).attr("id")
            if (selectedTabId != "showFlights") {
                $("#recentSearchSection").hide();
                $(".cro-section__mktmsg, .cro-section__newsletter").show();
                $('.cro-section__promo-code').css('margin-top', '75px');
            }
            else {
                var showRecentSearch = localStorage.getItem("sidls");
                var recentSearchOrder = localStorage.getItem("orrs");

                if (showRecentSearch == null && recentSearchOrder != null && recentSearchOrder.length > 0) {
                    $("#recentSearchSection").show();
                    var totalHeight = $('#recentSearches').outerHeight();
                    totalHeight = 485 - totalHeight;
                    $(".cro-section__mktmsg, .cro-section__newsletter").hide();
                    $('.cro-section__promo-code').css('margin-top', totalHeight + 'px');
                }
                else {
                    $(".cro-section__mktmsg, .cro-section__newsletter").show();
                    $('.cro-section__promo-code').css('margin-top', '75px');
                }
            }
        });
    }
    catch (err) { }

</script>                    
