        <script type="text/javascript">
            function getCookieData(name) {
                var pairs = document.cookie.split("; "),
                count = pairs.length, parts;
                while (count--) {
                    parts = pairs[count].split("=");
                    if (parts[0] === name)
                        return parts[1] == 'true';
                }
                return false;
            }
            var preferFullSite = getCookieData('preferfullsite');
        </script>

        <script type="text/javascript">
            $(function () {
                $('.popover-link').popover({
                    html: true,
                    title: function () {
                        return $($(this).data('titlewrapper')).html();
                    },
                    content: function () {
                        return $($(this).data('contentwrapper')).html();
                    }
                });
            });

            $(':not(.header-section)').mouseup(function (e) { //use $(':not(.header-section)') instead of $(document) to avoid ios bliking
                $('.popover-link').each(function () {
                    if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0 || $(e.target).is('.popover-close')) {
                        $('.popover').toggleClass('in').remove(); //$(this).popover('hide') -- popover does indeed hide but the elements remain in the DOM
                        return;
                    }
                });
            });

            $(function () {
                $('.tooltip-link').tooltip({
                    html: true,
                    title: function () {
                        return $($(this).data('tooltipwrapper')).html();
                    }
                });
                $('.tooltip-link02').tooltip({
                    html: true,
                    title: function () {
                        return $($(this).data('tooltipwrapper')).html();
                    }
                });
            });

            function checkWidth() {
                if ($(window).width() > 992) {
                    $('.row-offcanvas').removeClass('active'); // reset mobile sidebar activation
                    //$('.popover.in').css('display', 'none'); // reset popover activation
                    $('.nav-main__menu--dropdown').removeClass('open'); // reset megamenu activation
                }
                if ($(window).width() > 550) {
                    $('.popover.in').css('display', 'none'); // reset popover activation
                }
            }

            $(window).resize(checkWidth);


            if (typeof isIE7 !== "undefined" && isIE7) {
                // Add z-index for ie7
                $(function () {
                    var zIndexNumber = 1000;
                    // Put your target element(s) in the selector below!
                    $("div").each(function () {
                        $(this).css('zIndex', zIndexNumber);
                        zIndexNumber -= 10;
                    });
                });
            }

        </script>

