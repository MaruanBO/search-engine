        <script type="text/javascript">
            window.onerror = function (message, url, line, column, error) {
                var e = encodeURIComponent;
                (new Image()).src = '//www.fareportal.com:8080/error?message=' + e(message) +

                        '&url=' + e(url) +
                        '&line=' + e(line) +
                        (error && error.stack ? '&stack=' + e(error.stack) : '') +
                        (column ? '&column=' + e(column) : '');
            };
            function getQueryString() {
                var assoc = new Array();
                var queryString = unescape(location.search.substring(1));
                var keyValues = queryString.split('&');
                for (var i in keyValues) {
                    var key = keyValues[i].split('=');
                    assoc[key[0]] = key[1];
                }
                return assoc;
            }
            var sessionmsg = getQueryString()["sessiontimeout"];

            if (sessionmsg === '1') {
                $('#sessionmsg').removeClass("hidden");
                $('#mktmsg').addClass("hidden");

            }


        </script>

