    <script type="text/javascript">
        function convertToEntities_login() {
          var tstr = jQuery('#gn_navbar_username_display').val();
          var bstr = '';
          for (i = 0; i < tstr.length; i++) {
            if (tstr.charCodeAt(i) > 127) {
            bstr += '\&#' + tstr.charCodeAt(i) + ';';
            } else {
              bstr += tstr.charAt(i);
            }
          }
          jQuery('#gn_navbar_username').val(bstr);
          return true;
        }

