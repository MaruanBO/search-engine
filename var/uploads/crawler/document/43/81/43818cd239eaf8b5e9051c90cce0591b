  function changeBg(info){
   $(".homebgbox img").css('visibility','hidden');
   $(".homebgbox ."+info+"").css('visibility','visible');
   if(info=="onephoto"){
        $(".btnbox").find('img').css('visibility','visible');
        $(".btnbox").find('.hide').css('visibility','hidden');
   }
  }
 

