        $(".fotor_func_list li").on({
            mouseenter: function () {
                $(this).addClass("on");
                $(this).find(".txt_box").stop().animate({ "top": 0 }, 600);
                $(this).find(".icon_box").stop().animate({ "top": 238 }, 600);
            },
            mouseleave: function () {
                $(this).removeClass("on");
                $(this).find(".txt_box").stop().animate({ "top": "-238px" }, 600);
                $(this).find(".icon_box").stop().animate({ "top": 0 }, 600);
            }
        });

        $(".next_btn").on("click", function () {
            w_height = $(window).height();
            $("html, body").animate({ scrollTop: w_height - 65 }, 400);
        });

        $(window).on("scroll", function () {
            var st = $(document).scrollTop();
            if (st > 130) {
                $('#homepage_header').css({ "position": "fixed", "top": "0" });
            } else if (st < 130) {
                $('#homepage_header').css({ "position": "absolute", "top": "130px" });
            }
        });

		

        $("#lang").mouseenter(function () { $("#index-lang").slideDown("fast"); });
        $("#lang").parent().mouseleave(function () { $("#index-lang").slideUp(); })
        
    })

    function otherdoAction() {
        $(".fotorbar").attr("href", "http://forum.fotor.com/User/Login?sig=" + user.token);
