    <script type="text/javascript">

    var eventsapiurl =  "next?picture=yes";
    eventsapiurl  = eventsapiurl.replace(/&amp;/,"&");

    var lasttimestamp = "1424345680";
    var firsttimestamp = "";

    var lasteventid = "IA1247095";

    var eventurl;
    var gettingevents = 0;

    var ecount=0;

    var bLazy ;

    function showMore(id) {

        $(".boxMenuAddTag").hide()
        $("#cmdMenu").removeClass("cmdMenuActive")

        $(".expand .capsContent").not("#" + id).animate({
            top: "0px"


        }, 400);
        $(".expand .linkMore").not("#" + id).text("more")
        $(".expand .moreText").not("#" + id).fadeOut("Blind")
        $(".expand").not("#" + id).removeClass("expand")





        $("#" + id).toggleClass("expand")

        if ($("#" + id).hasClass("expand")) {
            $("#" + id + " .capsContent").animate({
                top: "-=" + ($("#" + id + " .capsImg").height() - 50) + "px"


            }, 400);
            $("#" + id + " .linkMore").text("less")
            $("#" + id + " .moreText").fadeIn("Blind")
            if ($("#" + id).parents(".boxTrendingSlider").length == 1)
                $(".boxTrendingSlider").height(850)
        }
        else {
            $("#" + id + " .capsContent").animate({
                top: "-0px"


            }, 400);
            $("#" + id + " .linkMore").text("more")
            $("#" + id + " .moreText").fadeOut("Blind")
            if ($("#" + id).parents(".boxTrendingSlider").length == 1)
                $(".boxTrendingSlider").height(500)


        }
        return false
    }


    $(document).ready(function(){

        bLazy = new Blazy({
            offset: 100 // Loads images 100px before they're visible
        });

        bindCapsItemHover();

        startSliding($(".boxTrendingSlider > div").length)

        $("div[id=description]").each(function(index) {
