        _.templateSettings = {
            interpolate : /\{\{(.+?)\}\}/g,
            evaluate : /\<\@(.+?)\@\>/g
        };

        var sEvent = Backbone.Model.extend();

        var sEventList = Backbone.Collection.extend({
            model: sEvent,
            url : eventsapiurl,
            firstload :true,
            initialize: function() {
                _.bindAll(this, 'processChanges');
                this.bind('reset', this.processChanges);
                this.bind("change:url", function(){
                    eventurl = this.url;
                });
                firstload=true;
                eventurl = this.url;
                //setInterval(getNextEvents, 2*1000);
            },
            processChanges: function() {
                if(this.length > 0){
                    if(firsttimestamp < this.at(0).get("date")){
                        firsttimestamp = this.at(0).get("date");
                    }
                    if(firstload == true){
                        lasttimestamp  = this.at((this.length)-1).get("date");
                        lasteventid  = this.at((this.length)-1).get("id");
                        firstload= false;
                    }else{
                        if(lasttimestamp > this.at((this.length)-1).get("date")){
                            lasttimestamp  = this.at((this.length)-1).get("date");
                            lasteventid  = this.at((this.length)-1).get("id");
                        }
                    }

                }
                eventurl = this.url;
            },
            changes: function() {
                this.fetch({data:{after:firsttimestamp}});
            },
            cleanup:function() {
                lasttimestamp = -1;
                firsttimestamp = -1;
                firstload=true;
                url = eventsapiurl;
                lasteventid ='';

            },
            parse:function(response){
                return response.events;
            }
        });

        var events = new sEventList();


        var EventView = Backbone.View.extend({
            el: $('#eventlist'),
            template: _.template($('#event-template').html()),

            //template: $("#event-template"),
            initialize: function() {
                _.bindAll(this, 'render','appendItem');
                this.collection.bind('reset', this.render, this);
                this.collection.bind('add' , this.prependItem,this);
                this.elems=[];

            },
            render: function(p) {
                this.isPrepend = p;
                this.collection.each(this.appendItem);



                //$container.masonry( 'appended', this.elems);

                return this;
            },
            cleanup: function(){

                $('.event_list_item').remove();
            },
            prependItem : function(model){
                // var template = $("#event-template");
                var tempDate = new Date(model.get("date")*1000);
                //alert(tempDate.toString("dddd, mmmm dS, yyyy, h:MM:ss TT"));
                //var cuteDate =  $.cuteTime({ refresh: 10000 },tempDate.toString("dddd, mmmm dS, yyyy, h:MM:ss TT"));
                //var cuteDate =  $.cuteTime({ refresh: 10000 },"2009/10/16 22:11:19");
                model.set({date:tempDate.toString("dddd, mmmm dS, yyyy, h:MM:ss TT")});
                var descriptionhtml = activateLinks(model.get("description"));
                model.set({description:descriptionhtml});
                //var html = template.tmpl(model.toJSON());
                var html = this.template(model.toJSON());
                this.el.prepend(html);
                $('.cute_time').cuteTime({ refresh: 5000 });
                //addthis.toolbox(".sharediv");

            },
            appendItem: function(model){
                //  $(this.el).html(this.template(model.toJSON()));
                //$('#eventlist').append("<span>"+model.get('title')+" "+model.get('id')+"</span>");
                //var template = $("#event-template");
                var tempDate = new Date(model.get("date")*1000);

                //alert(tempDate.toString("dddd, mmmm dS, yyyy, h:MM:ss TT"));
                //var cuteDate =  $.cuteTime({ refresh: 10000 },"2009/10/16 22:11:19");
                model.set({date:tempDate.toString("dddd, mmmm dS, yyyy, h:MM:ss TT")});
                var descriptionurl = model.get("event_pic_link");
                model.set({showImage:false});
                model.set({"celebImageUrl":""})
                model.set({"placeImageUrl":""})
                if(descriptionurl)
                    descriptionurl=descriptionurl.replace("_large.jpg","_300.jpg");
                /*
                if(!descriptionurl){
                    descriptionurl = "";
                    if(celebString.indexOf(model.get("person")[0].id) != -1 && placeString.indexOf(model.get("location").place_id) != -1){
                        var celebImageUrl = "http://d2srvtdub1i1qz.cloudfront.net/celebs/"+model.get("person")[0].id+"/"+model.get("person")[0].id+"_medium.jpg"
                        var placeImageUrl = "http://d2srvtdub1i1qz.cloudfront.net/places/"+model.get("location").place_id+"/"+model.get("location").place_id+"_medium.jpg"

                        model.set({showImage:true});
                        model.set({"celebImageUrl":celebImageUrl});
                        model.set({"placeImageUrl":placeImageUrl});

                    }else{
                        model.set({showImage:false});
                    }

                }else{
                    descriptionurl=descriptionurl.replace("_large.jpg","_300.jpg");
                }
                */
                model.set({url_in_description:descriptionurl});
                var descriptionhtml = activateLinks(model.get("description"));
                model.set({description:descriptionhtml});
                if(model.get("person")){
                    var cleanPersonName = cleanName(model.get("person")[0].name);
                }else{
                    var cleanPersonName = '';
                }

                model.set({persontagcleanname:cleanPersonName})

                var html = $(this.template(model.toJSON()));
                // var html = template.tmpl(model.toJSON());
                $(html).css({ opacity: 0 });


                if(this.isPrepend == "true"){
                    this.el.prepend(html);
                }else{
                    $container.append(html);
                    html.imagesLoaded(
                            function(){
                                $(html).animate({ opacity: 1 });
                                $container.masonry( 'appended', html, true );
                                ecount = ecount+1;
                                /*
                                if(ecount == 3 || ecount == 15 ){
                                    var div = $("#ad-sidebar-tmpl").clone();
                                    div.show();
                                    div.attr('id','');
                                    div.addClass("post_col");
                                    div.addClass("ad-sidebar");
                                    div.width(300).height(280);
                                    div.data('waypoint',true).waypoint(initwaypoint, {
                                        triggerOnce: true
                                    });
                                    $container.append(div);
                                    $container.masonry( 'appended', div, true );
                                }
                                */
                            });
                    //this.el.append(html).masonry('reload');
                }

                //$container.append($output).masonry('reload');
                //this.elems.push(html);


                $('.cute_time').cuteTime({ refresh: 5000 });

                // addthis.toolbox(".sharediv");
            }
        });

        var eView = new EventView({collection:events});



        function eventLoadSuccess(p){
            gettingevents=0;
            $("#overlay").hide();
            bindCapsItemHover();
            bLazy.revalidate();
        }

        function getEvents(lastId){
            $("#overlay").fadeIn();
            if(lastId){
                events.fetch({data:{before:lastId},success: eventLoadSuccess});
            }else{
                events.fetch({success: eventLoadSuccess});
            }
        }

        $(window).scroll(function() {
            // We check if we're at the bottom of the scrollcontainer
            //pixelsFromWindowBottomToBottom = 0 + $(document).height() - ($(window).scrollTop() ) - $(window).height();
            if($(window).scrollTop() + $(window).height() > $(document).height() - 1200) {
                if(gettingevents == 0 ){
                    gettingevents = 1;
                    ecount=0;

                    
                        getEvents(lasttimestamp);
                    

                    var href = window.location.href;
                   _gaq.push(['_trackEvent', 'pageview', 'scroll', href]);
                }
            }

        });

    });



    function activateLinks(text){
        var linkhtml = linkify(text,{callback:function(text, href){
            return href ? '<a href="' + href + '" target="_blank" >' + text + '</a>' : text;
        }});

        return linkhtml;
    }

    function cleanName(name){

        name=name.replace(/ /g,"-");
        name = name.replace(/,/g,"");
        name=name.replace(/'/g,"");
        return name;
    }



    function follow(personid,event_id){
        //	var popover = $(".popbtn").data('popover').$tip;
        //   var email = popover.find('#emailaddr').val();
        // var popover =  $("#popover-content");
        //var emailelm = popover.find("#emailaddr");

        var popover = $("#"+event_id).data('popover').$tip;
        var email = popover.find('#emailaddr').val();


        // alert('emailaddress '+ email + ' person '+ person_id + "event id " + event_id);
        if(email != ''){

            $.ajax({
                type: 'POST',
                url: "http://api.smarp.com/smarp/api/post/subscribe",
                data: { emailaddress: email, person_id: personid },
                success: function(data){
                    $("#"+event_id).text("Following");
                    clsfollowdialog();
                },
                error : function(data){
                    $("#"+event_id).text("Following");
                    clsfollowdialog();
                }
            });
        }

    }


    function shareFb(event_id,event_title){
        var facebookshare = 'http://api.addthis.com/oexchange/0.8/forward/facebook/offer?url=http://smarp.com/event/'+event_id+'&pubid=ra-4f417c5241a2ce8c&title='+event_title;
        window.location.href= facebookshare;
        return true;
    }

    function shareTwitter(event_id,event_title){
        var twittershare = 'http://api.addthis.com/oexchange/0.8/forward/twitter/offer?url=http://smarp.com/event/'+event_id+'&pubid=ra-4f417c5241a2ce8c&title='+event_title;
        window.location.href= twittershare;
        return true;
    }

</script>


