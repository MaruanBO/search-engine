        window.fbAsyncInit = function() {
            FB.init({
//       appId      : '565857800132860', // Dev App ID
                appId      : '330197143750310', // Prod App ID
                channelUrl : '', // Channel File
                status     : true, // check login status
                cookie     : true, // enable cookies to allow the server to access the session
                xfbml      : true  // parse XFBML
            });

            // Additional init code here

        };


        function createuser(fb_id,name,emailaddress,profilepic_url){
            $.ajax({
                type: 'GET',
                url: '/join/createuser',
                data: { fb_id: fb_id, name: name, emailaddress:emailaddress,profilepic_url:profilepic_url},
                datatype:'json',
                success: function(data){
                    console.log("Data from createuser "+data);
                    var jsonobj = JSON.parse(data);
                    if(jsonobj.post_login.usertype == "new"){
                        window.location = '/follow';
                    }else{
                        window.location = '/';
                    }
                },
                error : function(data){
//                    window.location = '<g:createLink controller="eventsHomeNew" action="index"/>';
                    console.log("error createuser "+data);
                    hideFormLogin();
                    hideFormSignup();
                }
            });
         }

        function fblogin(){
            clicky.log('#join/login','login');

            FB.login(function(response) {
                if (response.status === 'connected') {
                    // The response object is returned with a status field that lets the app know the current
                    // login status of the person. In this case, we're handling the situation where they
                    // have logged in to the app.
                    var fb_id , name, emailaddress,profilepic_url;
                    FB.api('/me', function(response) {
                        fb_id = response.id;
                        name = response.name;
                        FB.api('/me?fields=picture', function(response) {
                            profilepic_url = response.picture.data.url ;
                            FB.api('/me?fields=email', function(response) {
                                emailaddress = response.email;
                                createuser(fb_id,name,emailaddress,profilepic_url);
                            });
                        });
                    });
                }
                // handle the response
            }, {scope: 'email'});

            return false;
        }

        function doLike(eventid){
            if(eventid.indexOf("IA") == 0){
                $.ajax({
                    type: 'GET',
                    url: "/follow/like",
                    data: {event_id: eventid },
                    success: function(data){
                        console.log("Liked "+JSON.stringify(data));
                        $.growl.notice({title:"", message: "Event  liked" });
                    },
                    error : function(data){
                        console.log("Liked Error "+JSON.stringify(data));
                    }
                });
            }

        }

