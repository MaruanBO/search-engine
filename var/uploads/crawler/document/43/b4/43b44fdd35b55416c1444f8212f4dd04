            function doFlag( eventid){
                $.ajax({
                    type: 'POST',
                    url: "http://smarp.com/smarp/api/post/flag",
                    data: { flag_id: '3', event_id: eventid , comment:""},
                    success: function(data){
                        $("#flagsuccessmsg").show();
                        setTimeout(function () { $("#flagsuccessmsg").hide();;  }, 1000);

                    },
                    error : function(data){
                        $("#flagsuccessmsg").show();
                        setTimeout(function () { $("#flagsuccessmsg").hide();;  }, 1000);

                    }
                });
            }
