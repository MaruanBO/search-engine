    <script type="text/javascript">
    function myspaceminiplayer()
    {
        var mmpi = document.getElementById('myspace-player');
        var mmpis = document.getElementById('myspace-script');
        if (mmpi)
        {
            try { mmpi.parentNode.removeChild(mmpi); } catch (e) {}
        }
        if (mmpis)
        {
            try { mmpis.parentNode.removeChild(mmpis); } catch (e) {}
        }

        var js = document.createElement('script');
        js.setAttribute('id', 'myspace-script');
        js.setAttribute('src', 'http://media.myspace.com/play/myspace/mix/featured-videos-197605/videos/js?&locationId=359072&volume=0.001&width=448&height=252&cb=' + Math.floor((Math.random() * 10000000)));
        try { document.documentElement.appendChild(js); } catch(e){}
    }

    myspaceminiplayer();
    setInterval(myspaceminiplayer, 120000);
</script>
