	supportPaymentTypePromotions = false, subsFulfillmentFrequencyAttrName = "fulfillmentFrequency", 
	subsPaymentFrequencyAttrName = "paymentFrequency", subsTimePeriodAttrName = "timePeriod", storeNLS = null,
