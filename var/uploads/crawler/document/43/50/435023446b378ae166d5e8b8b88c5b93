	var ModelOpen = false; 
function browserCompatabilityMsg() 
{    
	var agentStr = navigator.userAgent; 
    var isBroswerVersionIE8Lower = true; 
    if (agentStr.indexOf("Trident")> -1) { 
        isBroswerVersionIE8Lower = false; 
	} 
    if ((BrowserDetect.browser=='Explorer' && isBroswerVersionIE8Lower ) ||  
            (BrowserDetect.browser=='Firefox' && BrowserDetect.version < 4 ) ||  
            (BrowserDetect.browser=='Chrome' && BrowserDetect.version < 1) ||  
            (BrowserDetect.browser=='Safari' && BrowserDetect.version < 1) ||  
            (BrowserDetect.browser=='Opera' && BrowserDetect.version < 7.54) ||  
            (BrowserDetect.browser=='Netscape' && BrowserDetect.version < 7))   
    {    
        var strDesc = "<div class='LoginModalFirstPara'><p>The browser you are using doesn't fully support the AT&T website. You can continue to explore the site but certain features and layout elements may not function properly.</p></div><div class='LoginModalFirstPara'><p>Please ensure your browser version is supported to get full functionality of the AT&T website.</p></div><div class='LoginModalLastPara'><p>Current supported browsers:-</p><ul class='LoginModalFirstUL'><li>Safari v4.0 and up</li><li>Firefox v4.0 and up</li><li>Chrome v7.0 and up</li><li>Internet Explorer v7.0 and up</li></ul></div>"; 
        var strHTML = "<!--[if lte IE 9]><div id='ie'><![endif]--><div id='md'>" + 
                            "<div class='modalHeader'>" + 
                                "<h1>Outdated browser</h1>" + 
                                "<a href='#Close' class='closeModal' onclick='jQuery.colorbox.close(); return false'>Close</a>" + 
                            "</div>" + 
                            "<div class='LoginModalContent'>"  + 
                                "<div class='LoginModalFirstPara'> " + 
                                   strDesc +  
                                "</div>" + 
                                "<div class='LoginModalButtonDiv'>" + 
                                    "<div class='LoginModalBotLinks'><a href='http://www.att.com/esupport/article.jsp?sid=KB402059'>Get more help with browser updates</a></div>" + 
                                    "<div class='LoginButtonDiv'></div>" + 
                                    "<div class='clear'></div>" + 
                                "</div>" +                                 
                            "</div>" + 
                        "</div><!--[if lte IE 9]></div><![endif]-->";                                    
        if(!ModelOpen){ 
            jQuery.colorbox({ height:430, width:650, scrolling:false, close:"",html:strHTML });  
            ModelOpen = true; 
        } 
    }     
} 
	</script> 
