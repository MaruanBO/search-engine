<script> 
        var URL = "https://www.att.com/olam/unauth/performAjaxToGetSavedUserId.myworld"; 
        var userIdVal; 
        var saveUserIdVal; 
        var splittedIds; 
        jQuery.ajax( 
        {  
            type : 'POST',  
            url : URL,  
            async : true,  
            cache: false,  
            crossDomain:true,  
            dataType: 'jsonp text',  
            jsonpCallback: null,  
            jsonp: null  
         });   
        function colamCookieResponse(data){  
            userIdVal = data.userId; 
            saveUserIdVal = data.saveUserIDCheck; 
            if(typeof(userIdVal) != "undefined"){ 
                splittedIds = userIdVal.split(","); 
            }             
            if(typeof(userIdVal) != "undefined"){ 
                if(splittedIds.length == 1){ 
                    jQuery("#userid").val(userIdVal); 
                } 
                if(splittedIds.length > 1){ 
	                for(i=0;i<splittedIds.length;i++) {             
	                   var listItem = "<dd class='opt PadTop5 Padbot5 MarLeft0 PadLeft20Force MarTopNeg3' style='cursor:pointer;' id='"+i+"'><a href='#'>"+splittedIds[i]+"</a></dd>"; 
	                   jQuery("#dropdownID").append(listItem);    
	                   jQuery("#divShortcut").click(function(){ 
	                       jQuery("#userid").val(""); 
	                   }); 
	                } 
               } 
               if(splittedIds.length == 1 || splittedIds.length < 1){ 
            	   jQuery("#ddShortcutList").removeClass("box no-top-round"); 
               } 
            } 
		    if(saveUserIdVal == 'Y') { 
		        jQuery("#remMe").attr('checked','checked'); 
				jQuery("#uniform-remMe span").addClass("checked"); 
				jQuery("input#remMe").val('YES');                       
		    } 
		    else { 
		    	jQuery("#remMe").removeAttr('checked'); 
				jQuery("#uniform-remMe span").removeClass("checked"); 
		        jQuery("input#remMe").val("NO"); 
		    } 
            if(typeof(userIdVal) == "undefined"){ 
                jQuery("#ddShortcutList").removeClass("box no-top-round"); 
            } 
        }   
        jQuery(window).load(function(){ 
            jQuery('#ddShortcutBox').hide(); 
            var ddOpened = false; 
            jQuery('#ddShortcut.enabled input').focus(function () { 
                var scrollTop     = jQuery(window).scrollTop(); 
                //alert(scrollTop); 
                var ht     = jQuery(window).height(); 
                //alert("ht"+ht); 
                var   elementOffset = jQuery('#divShortcut').offset().top; 
                //alert(elementOffset); 
                var  distance      = (elementOffset - scrollTop); 
                //    alert(distance); 
                var bot = ht - distance - 30; 
            //  alert(bot); 
                var top = 130; 
                if (bot < 120) 
                { 
                    jQuery("#ddShortcutBox").css("margin-top",-115); 
                    jQuery('#ddShortcutList').addClass('inputDropdownscrollup'); 
                } 
                else{ 
                    jQuery('#ddShortcutList').removeClass('inputDropdownscrollup');      
                    jQuery("#ddShortcutBox").css("margin-top",0); 
                } 
                jQuery('#ddShortcutBox').slideDown(400); 
            }); 
                jQuery("#ddShortcutBox").click(function(){ return false; }); 
            jQuery("#divShortcut").click(function(){ return false; }); 
            jQuery('body').click(function(e){ 
          if ( !jQuery(e.target).is('#divShortcut') ) { 
          jQuery('#ddShortcutBox').slideUp(400);  
                // do something 
          } 
            }); 
            jQuery("dd.opt").click(function(){ 
                var temp = jQuery(this).text(); 
                //jQuery("#ddShortcut p.head").html(temp); 
                jQuery("#divShortcut input#userid").attr("value",temp); 
                jQuery("#ddShortcutBox").slideUp(400); 
                return false; 
            }); 
            /*START:Script to change color and background of option on HOVER*/ 
                jQuery("dd.opt").hover( 
                        function () { 
                        jQuery(this).css('background','#3090C7'); 
                        jQuery(this).find("a").css('color','#FFFFFF'); 
                        jQuery("dd.opt a").css('text-decoration','none'); 
                        }); 
                jQuery("dd.opt").mouseleave( 
                        function () { 
                        jQuery(this).css('background','none'); 
                        jQuery(this).find("a").css('color','#005B95'); 
                        }); 
            /*END:Script to change color and background of option on HOVER*/ 
			jQuery("#userid").bind("keyup",function(){if(jQuery("#userid").val().length>0){jQuery("#ddShortcutBox").css('display','none');}else{jQuery("#ddShortcutBox").css('display','block');}}); 
            }); 
</script> 
<script> 
jQuery(document).ready(function(){ 
    jQuery("a.openModalHp").click(function(e){          
    	var pageLanguage = jQuery("html").attr("lang"); 
		  var modalHref = jQuery(this).attr('href'); 
		  var modalURL = "/homepage/" + pageLanguage + "/" + modalHref;		    
		  //jQuery.colorbox({iframe:true, height:550, width:720, scrolling:false, href: modalURL, close:""}); 
		  var l_height = 545; // default height     
            if (jQuery.browser.msie) {                
                if(pageLanguage=="en"){ 
                    if(parseInt(jQuery.browser.version)<9){ 
                    l_height = 509; 
                     } 
                 else { 
                    l_height = 504;  
                  }                                    
                    jQuery.colorbox({iframe:true, height:l_height, width:720, scrolling:false, href: modalURL, close:""}); 
                } 
            else { 
                if(parseInt(jQuery.browser.version)<9){ 
                 l_height = 545; 
              } 
              else { 
                 l_height = 532;  
              }           
             jQuery.colorbox({iframe:true, height:l_height, width:720, scrolling:false, href: modalURL, close:""}); 
            } 
        } 
        else {        
         if(pageLanguage=="en"){                 
            l_height = 515; 
            jQuery.colorbox({iframe:true, height:l_height, width:720, scrolling:false, href: modalURL, close:""}); 
         } 
         else{ 
             jQuery.colorbox({iframe:true, height:l_height, width:720, scrolling:false, href: modalURL, close:""}); 
         } 
       } 
		  return false; 
    }); 
    jQuery(".closeModal").click(function(){ 
        jQuery.fn.colorbox.close(); 
    }); 
    jQuery("#cboxMiddleLeft").append('<div></div>'); 
    jQuery("#cboxMiddleRight").append('<div></div>'); 
    jQuery(function(){ 
    	jQuery("#remMe").focus(function(){jQuery(this).parent().css("border","1px dotted #000");}); 
    	jQuery("#remMe").blur(function(){jQuery(this).parent().css("border","none");}); 
   	}); 
}); 
</script> 
