             jQuery("#_content_att_homepage_en_index_jcr_content_homepageftrs_featureteaser").bind("teaserLoaded", function(event) { 
                var hname = window.location.hostname; 
                var pname = window.location.pathname; 
                if((pname.indexOf("/homepage/index.html")>=0 || pname.indexOf("/homepage/es/index.html")>=0 || pname=='' || pname=='/') && hname.indexOf(".att.com")>=0){ 
