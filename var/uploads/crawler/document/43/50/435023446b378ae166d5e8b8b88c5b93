<script> 
var cookieRME = ""; 
var cookieUID = ""; 
jQuery(document).ready(function(){ 
	var globalNavCookie = GlobalNav.util.getGNCookie(); 
	var values=globalNavCookie.UG; 
	var authState="Unauth"; 
	var unauth="Unauth"; 
	var auth="Auth"; 
	for ( i = 0; i < values.length; i++ ) { 
		if(values[i] == auth) 
		{ 
			authState = auth; 
		} 
		if(values[i] == unauth) 
		{ 
			authState = unauth; 
		} 
	}  
	if (authState == auth) 
	{ 
		jQuery("#loginContainer").addClass("hideLoginModule");         
	} 
	else if (authState == unauth) 
	{   
		jQuery("#loginContainer").removeClass("hideLoginModule"); 
	} 
	function onMessage (event) { 
		var theIFrameElement= document.getElementById("ssoIframe"); 
		if (event.source != theIFrameElement.contentWindow) return; 
		var message = jQuery.parseJSON(event.data); 
		cookieRME = message.data.rme; 
		cookieUID = message.data.uid; 
		ATT.ssologin.LoginManager.cookieCheck(); 
		//fix for jira ticket b2c12-4246   attuid ar5204 
		ATT.ssologin.LoginManager.removePasswordFromIDField(); 
	} 
	if (window.attachEvent) 
		window.attachEvent('onmessage', onMessage); 
	else if (window.addEventListener) 
		window.addEventListener('message', onMessage, false); 
//	Need to read myATT response for saved user id instead of reading the cookies, hence commenting this out so that it should not invoke rememberme.jsp	 
//	jQuery("#ssoIframe").attr({src:"https://www.att.com/homepage/index.rememberme.html"}); 
}); 
</script> 
