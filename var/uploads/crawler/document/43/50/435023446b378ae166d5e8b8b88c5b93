<script> 
jQuery(document).ready(function(){ 
    var globalNavCookie = GlobalNav.util.getGNCookie(); 
    var values=globalNavCookie.UG; 
    var authState="Unauth"; 
    var unauth="Unauth"; 
    var auth="Auth"; 
    for ( i = 0; i < values.length; i++ ) { 
        if(values[i] == auth) 
        { 
        	authState = auth; 
        } 
        if(values[i] == unauth) 
        { 
        	authState = unauth; 
        } 
    }	 
	jQuery(".homeproducttray_Both").show(); 
    if (authState == auth) { 
        jQuery(".homeproducttray_Auth").show(); 
        jQuery(".homeproducttray_Unauth").hide(); 
    } 
    else if (authState == unauth) { 
        jQuery(".homeproducttray_Unauth").show(); 
        jQuery(".homeproducttray_Auth").hide(); 
    }     
}); 
</script> 
