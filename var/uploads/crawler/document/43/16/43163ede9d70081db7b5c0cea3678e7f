function FetchHeatContent(config_obj){var config_obj=config_obj||config,heat_index_url='http://bootstrap.'+config_obj.domain+'/api/v3.0/hottest/';if(ie_version<10){HandleXdrRequest(heat_index_url);}else{$.ajax({type:'GET',url:heat_index_url,data:config_obj.heat_index_params,crossDomain:true,success:function(response){HandleCollections(response);},error:function(jqXHR,status,error){throw'There was an error ('+error+') fetching '+heat_index_url;}});}}
function HandleXdrRequest(url){var xdr=new XDomainRequest();xdr.open('GET',url);xdr.onload=function(){var response=$.parseJSON(xdr.responseText);return HandleCollections(response);};xdr.onerror=function(){};xdr.ontimeout=function(){};xdr.onprogress=function(){};xdr.send();}
function HandleCollections(response){if(response.data&&response.data.length>0){var deferred=function(){var deferred_array=$.map($(response.data),function(el,i){if(el.id&&el.title&&el.url&&el.url.indexOf(chatter_site_name+'.com')!==-1){if(typeof chatter_tags!=='undefined'&&chatter_tags.length>0&&chatter_tags.indexOf('all')===-1){for(var i=0;i<chatter_tags.length;i++){el.tags=$.map(el.tags,function(value){return value.replace(/_/g,' ').replace(/-/g,' ');});var chatter_tag=chatter_tags[i].replace(/_/g,' '),chatter_tag_upper=chatter_tag.charAt(0).toUpperCase()+chatter_tag.slice(1);if($.inArray(chatter_tag,el.tags)>-1||$.inArray(chatter_tag_upper,el.tags)>-1){AddCollectionToArray(el);return el;}}}else{AddCollectionToArray(el);return el;}}});if(deferred_array.length==0){deferred_array=$.map($(response.data),function(el,i){if(el.id&&el.title&&el.url&&el.url.indexOf(chatter_site_name+'.com')!==-1){AddCollectionToArray(el);}});}
return deferred_array;};$.when.apply(null,deferred()).then(function(){LfStreamWrapper(config);$widget.find('.cm-lc-view').css({display:'block',overflow:'auto'});setTimeout('cmg.utility.latest_chatter.UpdateTimestamps()',60000);});}}
function AddCollectionToArray(el){config.collection_ids.push(el.id.toString());config.collections_content.push([{articleId:el.articleId,siteId:el.siteId,title:el.title,url:el.url}]);}
function TruncateContent(html){var $existing_content=$widget.find('.cm-lc-body').html(),text_words;if(typeof $existing_content!=='undefined'&&$existing_content!==null&&$existing_content.indexOf(html)>-1){return false;}
html=html.replace(/<p.*?>/gi,'').replace(/<\/p>/gi,' ').replace(/<[B-Zb-z].*?>/g,'').replace(/<\/?[B-Zb-z].*?>/g,'').replace(/ {2,}/g,' ');text_words=html.replace(/<[Aa]w.*?>/g,'').replace(/<\/[Aa].*?>/g,'').split(' ');if(text_words.length>config.truncate_word_count){text_words=text_words.slice(config.truncate_word_count);var text_to_replace=text_words.join(' ');if(html.indexOf(text_to_replace)==-1){var snippet=html.substr(0,html.indexOf(text_words[0]+' '+text_words[1]));if(snippet.lastIndexOf('>')<snippet.lastIndexOf('<')){html=snippet.substr(0,snippet.lastIndexOf('<'));}else{html=snippet;}}else{html=html.replace(text_to_replace,'');}
html+='...';}
return html;}
function LfStreamWrapper(config){Livefyre.require(['streamhub-sdk/content/views/content-list-view','streamhub-sdk/content/views/content-view','streamhub-sdk/collection','streamhub-sdk/content','inherits','hogan'],function(ListView,ContentView,Collection,Content,inherits,hogan){var list_view=new ListView({el:$widget.find('.cm-lc-view')});HasCustomContentView.call(list_view);function CustomContentView(opts){ContentView.apply(this,arguments);}
