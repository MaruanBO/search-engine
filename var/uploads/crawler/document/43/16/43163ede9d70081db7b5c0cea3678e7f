            local_counties = cmg._.map(local_counties_raw, function (county) {
            return county.trim();
        });

        try {
            var user_cookie_data_raw = $.cookie('_cmg_weather_location'),
                user_cookie_data = JSON.parse(user_cookie_data_raw),
                user_zip_code = user_cookie_data.zip_code,
                user_county = user_cookie_data.county;
