    cmg.query(document).on('ready', function($) {

        $('.cm-ttd-filter-dates, .cm-ttd-filter-locations').on('click', function(e) {
            e.preventDefault();

            var $filter = $(this),
                callback = function() {},
                $document = $(document),
                $modal_container = $('.cm-modal-container'),
                $modal_content = $('.cm-modal-content'),
                $modal_close = $('.cm-close-modal'),
                util = function() {
                    $('.cm-navbar-container').hide();
                    cmg.utility.modal_manager.PositionCloseButton();
                    $document.on('modal-closed', function() {
                        $modal_content.attr('style', '');
                        $('.cm-navbar-container').show();
                    });
                };
            $modal_close.on('click', function(e) {
                e.preventDefault();
                $modal_container.fadeOut();
                $modal_content.empty();
                $document.trigger('modal-closed');
            });
            if ($filter.hasClass('cm-ttd-filter-locations')) {
                // locations filter
                callback = function() {
                    var $locations = $('.cm-ttd-facet.cm-ttd-locations').clone(),
                        $link = $locations.find('.cm-ttd-facet-footer a');
                    $modal_content
                        .html($locations.show())
                        .css({
                            width: $locations.css('maxWidth'),
                            maxHeight: $(window).height() - 20,
                            overflow: 'auto'
                        });

                    $link.on('click', function(e) {
                        $modal_close.trigger('click');
                    });

                    $locations.find('input').on('click', function(e) {
                        var $link_href = $link.attr('href'),
                            $loc = $(this).val();
                        if ($(this).is(':checked')) {
                            $link.attr('href', $link_href + ($loc.indexOf('maponics') > -1 ? '&n=' : '&c=') + $loc);
                        } else {
                            $link.attr('href', $link_href.replace(($loc.indexOf('maponics') > -1 ? '&n=' : '&c=') + $loc, ''));
                        }
                    });

                    util();
                };
            } else if ($filter.hasClass('cm-ttd-filter-dates')) {
                var $start_date_container = $('.cm-ttd-date-start .cm-ttd-dp-placeholder'),
                    $end_date_container = $('.cm-ttd-date-end .cm-ttd-dp-placeholder');

                
                $start_date_container.datepicker({
                    prevText: 'prev',
                    nextText: 'next',
                    dateFormat: 'yymmdd12',
                    altField: '.cm-ttd-dates-container input[name="cm-date-start"]',
                    minDate: 0
                });
                $end_date_container.datepicker({
                    prevText: 'prev',
                    nextText: 'next',
                    dateFormat: 'yymmdd12',
                    altField: '.cm-ttd-dates-container input[name="cm-date-end"]',
                    minDate: 0
                });

                // dates filter
                callback = function() {
                    var $dates = $('.cm-ttd-dates-container'),
                        $parent = $('.cm-ttd-facet.cm-ttd-dates'),
                        $link = $dates.find('.cm-ttd-facet-footer a');

                    $modal_content
                        .html($dates.show())
                        .css('width', $dates.css('maxWidth'));

                    $document.on('modal-closed', function() {
                        if ($parent.html() === '') {
                            $parent.html($dates);
                            // destroy the datepicker instances so as to avoid dupes post-modal-close.
                            $start_date_container.datepicker('destroy');
                            $end_date_container.datepicker('destroy');
                        }
                    });

                    $link.on('click', function(e) {
                        var $link_href = $(this).attr('href'),
                            $start_date = $('.cm-ttd-dates-container input[name="cm-date-start"]').val(),
                            $end_date = $('.cm-ttd-dates-container input[name="cm-date-end"]').val();
                        if ($start_date * 1 <= $end_date * 1) {
                            $(this).attr('href', $link_href + '&when=' + $start_date + '-' + $end_date);
                        } else if ($link_href.indexOf('&') > -1) {
                            $(this).attr('href', $link_href.substr(0, $link_href.indexOf('&')));
                        }
                        $modal_close.trigger('click');
                    });

                    util();
                };
            }
            $modal_container.fadeIn();
            callback();
        });

        $.getScript('http://media.cmgdigital.com/shared/media/2015-02-19-11-59-47/web/common/v2newspaper/javascript/components/ttd.js', function(data, textStatus, jqxhr) {
            //script loaded
            if (jqxhr.status === 200) {
                cmg.utility.ttd.RenderEvents = function(data, $, is_default_search) {
                    var $ = $ || cmg.query,
                        $event_title = $('.cm-ttd-event-title'),
                        $event_image = $('.cm-ttd-event-image'),
                        $event_date = $('.cm-ttd-event-date'),
                        i = 0,
                        is_default_search = is_default_search || false,
                        months_of_year = [
                            'Jan',
                            'Feb',
                            'Mar',
                            'Apr',
                            'May',
                            'Jun',
                            'Jul',
                            'Aug',
                            'Sep',
                            'Oct',
                            'Nov',
                            'Dec'
                        ],
                        ttd = cmg.utility.ttd;
                    if (is_default_search) {
                        events = data.events.event;
                    } else {
                        events = data.events;
                    }
                    for (i; i < events.length; i++) {
                        var event = events[i];
                        if (event.image && event.image !== '') {
                            if (event.title) {
                                var event_title = event.title.replace('Â', '');
                                if (event.title.length > 25) {
                                    event_title = event.title.substr(0, 25) + '...';
                                }
                                $event_title
                                    .attr('title', event.title.replace('Â', ''))
                                    .text(event_title);
                            }
                            if (event.image) {
                                if (is_default_search && typeof event.image[ttd.AppConfig.image_size] !== 'undefined') {
                                    // public api
                                    $event_image.append($('<img />', {
                                        'src': event.image[ttd.AppConfig.image_size].url
                                    }));
                                } else if (typeof event.image !== 'undefined' && event.image && event.image !== '') {
                                    // ehp api
                                    $event_image.append($('<img />', {
                                        'src': event.image
                                    }));
                                }
                            }
                            if (event.url) {
                                // public api
                                $event_image.attr('href', event.url);
                                $event_title.attr('href', event.url);
                                $event_date.attr('href', event.url);
                            } else if (event.permalink) {
                                // ehp api
                                $event_image.attr('href', event.permalink);
                                $event_title.attr('href', event.permalink);
                                $event_date.attr('href', event.permalink);
                            }
                            if (event.start_time) {
                                if (typeof event.start_time === 'number') {
                                    event.start_time = event.start_time.toString();
                                }
                                if (event.start_time.indexOf('-') > -1) {
                                    var start_date = new Date(event.start_time.replace(/-/g, '/'));
                                } else {
                                    var start_date = Date(event.start_time);
                                    start_date = new Date(start_date);
                                }
                                $event_date.text(months_of_year[start_date.getMonth()] + ' ' + start_date.getDate());
                            }
                            break;
                        } else if (i === events.length - 1) {
                            // end of the results and none have images, run a new search
                            if (ttd.ApiMethod !== 'search') {
                                // the featured method returned no suitable events,
                                // retry with the API's search method
                                ttd.ApiMethod = 'search';
                            } else {
                                // already running a generic search, increase # of results
                                ttd.AppConfig.page_size = '10';
                            }
                            ttd.SearchInProgress = false;
                            ttd.EventSearch();
                        }
                    }
                };

                cmg.utility.ttd.Init(
                    {
                        app_key: 'XBpcswMDXnPsSq8T',
                        api_token: 'a4658bdfccfd959930b93a02aeeefba3',
                        image_size: 'perspectivecrop176by124'
                    },
                    'thingstodo.palmbeachpost.com',
                    'carousel'
                );
            }
        });
    }(cmg.query));
