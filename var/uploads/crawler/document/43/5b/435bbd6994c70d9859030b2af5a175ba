            <script type="text/javascript">
                if (!isIe7 && isLocalStorageNameSupported()) {
                    if (!amplify.store.types.sessionStorage) {
                        var section = amplify.store('ACTIVE_SECTION');
                        if (section == null || section == '') {
                            amplify.store('ACTIVE_SECTION', goToSection);
                            section = goToSection;
                        }
                        InfusionViewModel.MakeSectionActive(section);
                        amplify.store('ACTIVE_SECTION', '');
                    } else {
                        var section = amplify.store.sessionStorage('ACTIVE_SECTION');
                        if (section == null || section == '') {
                            amplify.store.sessionStorage('ACTIVE_SECTION', goToSection);
                            section = goToSection;
                        }
                        InfusionViewModel.MakeSectionActive(section);
                        amplify.store.sessionStorage('ACTIVE_SECTION', '');
                    }
                } else {
                    InfusionViewModel.MakeSectionActive(goToSection);
                }
                InfusionViewModel.UpdateScrollbar();
            </script>
    
