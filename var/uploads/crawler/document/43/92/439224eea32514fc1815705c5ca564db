<script type="text/javascript">
jQuery(document).ready(function() {
    Date.firstDayOfWeek = 0;
    jQuery('#datepicker-search')
        .datePicker({inline:true,startDate:'01/01/1996'})
        .bind(
            'dateSelected',
            function(e, selectedDate, $td) {
                var urlday = selectedDate.getDate();
                var urlmonth = selectedDate.getMonth() + 1;
                var urlyear = selectedDate.getFullYear();
                var urldate = urlmonth + '%2F' + urlday + '%2F' + urlyear;
                
                location.href = '/calendar/search/?byDate=1&d1=' + urldate + '&d2=' + urldate + '&month=' + urlmonth + '&day=' + urlday + '&year=' + urlyear + '&l=20';
                
            }
        );
});
</script>
