function addEvent(el, eventName, fn) {
    if (el.addEventListener) {
        el.addEventListener(eventName, fn, false);
    } else if (el.attachEvent) {
        el.attachEvent('on' + eventName, fn);
    } else {
        el['on' + eventName] = fn;
    }
}

function setCookie(name, value, expiresInYears, path) {
    // set time, it's in milliseconds
    var today = new Date();
    today.setTime(today.getTime());
    if (expiresInYears) {
        expiresInYears = expiresInYears * 1000 * 60 * 60 * 24 * 365;
    }
    var expires_date = new Date(today.getTime() + (expiresInYears));

    document.cookie = name + "=" + escape(value) +
((expiresInYears) ? ";expires=" + expires_date.toGMTString() : "") +
((path) ? ";path=" + path : "");
}

function setCPUCookie() {
       setCookie("nextvisitsite", "uk", 10, "/");
}

addEvent(window, "load", setCPUCookie);
</script></div></td>
