	</div>
	<div class="modal-footer">
		<button class="btn" data-dismiss="modal" aria-hidden="true">Abbrechen</button>
		<button id="buttonSendChangeRequest" class="btn btn-primary hidden">Mitteilung Abschicken</button>
	</div>
</script>


<script id="changeAddressOpeningHoursInputRow" type="text/template">
	<div>
		<select class="daySelect">
			<option <%= (selectedDay === "Montag") && "selected" %>>Montag</option>
			<option <%= (selectedDay === "Dienstag") && "selected" %>>Dienstag</option>
			<option <%= (selectedDay === "Mittwoch") && "selected" %>>Mittwoch</option>
			<option <%= (selectedDay === "Donnerstag") && "selected" %>>Donnerstag</option>
			<option <%= (selectedDay === "Freitag") && "selected" %>>Freitag</option>
			<option <%= (selectedDay === "Samstag") && "selected" %>>Samstag</option>
			<option <%= (selectedDay === "Sonntag") && "selected" %>>Sonntag</option>
		</select>
		<input type="time" class="timeInput timeFrom" value="<%= timeFrom %>"/>
		<span> - </span>
		<input type="time" class="timeInput timeUntil" value="<%= timeUntil %>"/>
		<a class="removeOpeningHoursLine">entfernen</a>
	</div>
</script>

