        <script type="text/javascript">

            var isMobile = {
        Android: function() {
            return navigator.userAgent.match(/Android/i);
        },
        BlackBerry: function() {
            return navigator.userAgent.match(/BlackBerry/i);
        },
        iOS: function() {
            return navigator.userAgent.match(/iPhone/i);
        },
        Opera: function() {
            return navigator.userAgent.match(/Opera Mini/i);
        },
        Windows: function() {
            return navigator.userAgent.match(/IEMobile/i);
        },
        any: function() {
            return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
        }
    };
    if(isMobile.any()) {

    }
    else {
   
            $(function() {
                $('#delivery-popup-holder').load('/ajax/deliverydatepopup', function() {
                    $('#delivery-popup-background').css('height', $(window).height()+'px').fadeIn(200);
                    var $top = 0;
                    if ($('#delivery-popup-holder').height() >= $(window).height()) {
                        $top = $(window).scrollTop() + 20;
                    }
                    else {
                        $top = ( $(window).height() - $('#delivery-popup-holder').height() ) / 2+$(window).scrollTop(); 
                    }
                    $('#delivery-popup-holder').css("top", $top + "px"); 
                    $('#delivery-popup-holder').show(); 
                });
            });
        }

    </script>
