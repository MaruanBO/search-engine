<script type="text/javascript">
var animLen = 700, lastIndx = 0;

function show_tab(indx)
{
    lastIndx = indx = ((indx-1)%3)+1;

    $(".indx2_cont_images .indx2_cont_img"               ).stop().animate({opacity: 0}, animLen);
    $(".indx2_cont_images .indx2_cont_img[indx="+indx+"]").stop().animate({opacity: 1}, animLen);

    $(".indx2_cont_images a").removeClass("selected");
    $(".indx2_cont_images a[indx="+indx+"]").addClass("selected");

    return false;
}

$( document ).ready( function() { show_tab(3); setInterval("show_tab(lastIndx+1)", 5000); });


</script>

