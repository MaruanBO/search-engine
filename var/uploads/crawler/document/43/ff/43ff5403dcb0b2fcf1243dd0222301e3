<script type="text/javascript">

function show_optin()
{
    $("#optin_bk").css({display: "block", opacity: 0})
        .animate({opacity: 1});
}

function hide_optin()
{
    $("#optin_bk")
        .animate({opacity: 0}, {
            complete: function() {
                $(this).css("display", "none");
            }
        });
    return false;
}

</script>

