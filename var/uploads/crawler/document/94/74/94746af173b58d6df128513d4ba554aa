function responsiveSlider() {
//responsive code begin
function ScaleSlider() {
if (getWidth())
jssor_slider1.$ScaleWidth(Math.max(getWidth(), 1024));
else
window.setTimeout(ScaleSlider, 30);
}
ScaleSlider();
$(window).bind("load", ScaleSlider);
$(window).bind("resize", ScaleSlider);
$(window).bind("orientationchange", ScaleSlider);
//responsive code end
}
var stepsLoaded = false;
$(window).on('DOMContentLoaded load resize scroll',
function() {
var step = 1;
var totalSteps = 4;
if($("#steps-1").is(":in-viewport") && !stepsLoaded) {
stepsLoaded = true;
var totalSteps = 4;
processStep = function(step) {
$(".step-" + step).css({ opacity: 0, display: 'inline-block'}).animate({opacity:1},600).css("visibility","visible");
if(step != totalSteps) {
$(".arrow-" + step).fadeIn(600).css({ opacity: 0, display: 'inline-block'}).delay(600).animate({opacity:1},600).css("visibility","visible");
}
step++;
if(step <= totalSteps) {
setTimeout(function() { processStep(step) }, (300 * step));
}
}
processStep(step);
}
}
