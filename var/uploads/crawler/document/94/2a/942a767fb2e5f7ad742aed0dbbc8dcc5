<script>
    Rent.dynamicReady(function (s) {

        Rent.trackCubePageView();
        Rent.domReady(function () { Rent.PageViews.track(); });

        var c, params, r = ['unknown-state'], a = [];
        if (s.logged_in === true && s.shadow_acct === false) {
            r.push('tmpl-lo-state', 'signed-out', 'lo-li-pdp');
            a.push('tmpl-li-state', 'logged-in');
        } else {
            a.push('logged-out');
        }
        if (!s.lease_status) {
            a.push('can-report-lease');
        }
        c = document.body.className.replace(new RegExp('(?:^|\\s)('+ r.join('|') +')(?!\\S)', 'g'), '');
        a.length && (c += ' ' + a.join(' '));

        params = {};

        if (s.testPicks) {
            var picks = s.testPicks,
                len = picks.length;

            for (var i = 0; i < len; i++) {
                var pick = picks[i],
                    baseClass = ' mvt-'+pick.baseClass.replace(/_/g,'-');

                /* override the pick if a parameter has been set for it */
                if (params.hasOwnProperty(pick.baseClass)) {
                    picks[i].pick = params[pick.baseClass];
                }

                /* add baseClass and pick to body */
                c += baseClass + baseClass+'-'+pick.pick; 
            }
        }

        document.body.className = c;
    });
</script>
