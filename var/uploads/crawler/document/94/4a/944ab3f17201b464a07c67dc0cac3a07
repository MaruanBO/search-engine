<script type="text/javascript">

    function showModalLogin(modal) {

        if (!modal.hasClass('loggedin')) {
            if (modal.data('loginstep') === "register" && $('input#regToken').val() == "") {
                genRegToken();
            }
            //To hide the back to all ... link on detail/landing pages
            $('.back-bt').css('z-index', '0');
            //Only show sign in module for non-logged in users
            $('.mck-modal-overlay').addClass('on');
            $('.mck-modal-login').addClass('on');
            $('.mck-show-view').removeClass('mck-show-view');
            $('.mck-modal-login').find('.' + modal.data('loginstep')).addClass('mck-show-view');
        }
    }

    function getItemID() {
        var itemID = $('#header_0_hid_sid').val();
        itemID = itemID.substring(1, itemID.length - 1);
        return itemID.toUpperCase();
    }

    function getEDesc() {
        return $('#header_0_hid_edesc').val();
    }

    function getIsFpLoggedIn() {
        return $('#header_0_hid_isfploggedin').val();
    }

    function fpClickSave_Add(guid, elemid, name) {
        fpAJClickSave_Add(guid, elemid, name);
    }

    function fpClickSave_Del(guid, elemid, name) {
        fpAJClickSave_Del(guid, elemid, name);
    }

    var resizeCanvas = function (_instance) {
        //get the canvas container
        var recipe_canvas_container = $('#' + _instance.local_canvas.targetId);
        var _new_size = Number(recipe_canvas_container[0].offsetWidth);
        var is_ie8 = ($.browser.msie === true && $.browser.version.slice(0, 1) === "8");
        /* ie8 doesn't render the canvas to the flash element if it has fixed size or float applied
        * set the container*/
        if (is_ie8) {
            //IE8- resizes multiple times as the page renders
            if (_instance.resize_ready === false)
                return false;
            _instance.resize_ready = false;

            var ie8_resize_delay = 300;
            recipe_canvas_container.css({
                width: '42%',
                float: 'left'
            });
            _new_size = Number(recipe_canvas_container[0].offsetWidth);
            recipe_canvas_container.css({
                width: 'auto',
                float: 'none'
            });
        }

        recipe_canvas_container.children('canvas').remove();
        _instance.size = _new_size;
        MCK.Mark.drawChart(_instance);
        //$('#'+_instance.toolTipId).css('min-height',_instance.size);

        //need timeout for internet explorer
        if (is_ie8) {
            var ie_t = setTimeout(function () {
                recipe_canvas_container.css({
                    width: '42%',
                    float: 'left'
                });
                _instance.resize_ready = true;
            }, ie8_resize_delay);
        }
    };

</script>

