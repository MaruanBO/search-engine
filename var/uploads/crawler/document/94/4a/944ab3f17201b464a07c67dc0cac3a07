<script type="text/javascript">
    var addErrorMessage = function(message) {
        $('#errorMessagesContainer ul').append('<li>'+message+'</li>');
    }

    var clearErrorMessages = function() {
        $('#errorMessagesContainer ul').empty();
    }

    var hideErrorMessages = function(callback) {
        if (typeof callback == 'function')
            $('#errorMessagesContainer').slideUp(function(){
                callback();
            });
        else
            $('#errorMessagesContainer').slideUp();
    }

    var showErrorMessages = function (callback) {
        if (typeof callback == 'function')
            $('#closeErrorMessages').click(function () {
                callback();
            });
        $('#errorMessagesContainer').slideDown();
    }

    $(document).ready(function(){
        $('#closeErrorMessages').click(function(){
            hideErrorMessages(clearErrorMessages);
        });

        
    });
</script>

<div id="errorMessagesContainer" style="display:none;">
	<div class="ui-state-error ui-corner-all" style="margin: 8px; padding: .7em ;">
