    function onLoadHandlerSSOButtons(eventObj) {
        

       $("#gigyaDiv div[gigid='facebook']").click(function() { dataLayer.push({ 'event': 'GenRegStartTag' }) });
       $("#gigyaDiv div[gigid='twitter']").click(function() { dataLayer.push({ 'event': 'GenRegStartTag' }) });
       $("#gigyaDiv div[gigid='google']").click(function() { dataLayer.push({ 'event': 'GenRegStartTag' }) });
