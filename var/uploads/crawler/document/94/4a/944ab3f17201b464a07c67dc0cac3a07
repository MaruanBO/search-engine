<script>
    function getAnonSavesLength() {
        var items = JSON.parse(localStorage.getItem('mccormickSaves'));
        if (items !== null) {
            return items.length;
        }
        else {
            return '0';
        }
    }
        document.getElementById("anonSavesLength").innerHTML = getAnonSavesLength();
</script>
 
