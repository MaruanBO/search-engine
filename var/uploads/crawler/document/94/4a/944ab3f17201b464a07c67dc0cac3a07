    //TODO Add more brands here
function searchClick(obj) {
    var qshold = $(obj).parent().children('#txtInpSearch').val().replace('&','and');
    if(qshold != '' && qshold != 'Recipe Search')
    {
        var productSearch = '';

        if (searchType == 'Product') { productSearch = '&type=' + searchType; }

        if(window.location.href.toLowerCase().indexOf("/lawrys") > -1) {
            window.location = '/Lawrys/Search?qs=' + encodeURI(qshold.trim() + productSearch);
        }
        else if(window.location.href.toLowerCase().indexOf("/gourmet") > -1) {
            window.location = '/Gourmet/Search?qs=' + encodeURI(qshold.trim() + productSearch);
        }
         else if(window.location.href.toLowerCase().indexOf("/zatarains") > -1) {
             window.location = '/Zatarains/Search?qs=' + encodeURI(qshold.trim() + productSearch);
         }
         else if(window.location.href.toLowerCase().indexOf("/grill-mates") > -1) {
             window.location = '/Grill-Mates/Search?qs=' + encodeURI(qshold.trim() + productSearch);
         }
        else {
             window.location = '/Search?qs=' + encodeURI(qshold.trim() + productSearch);
        }
    }
}

</script>
