    }

    function gigyaSetup() {
    var login_params =
        {
            showTermsLink: 'false'
	        , height: 67
            , width: 396
	        , containerID: 'gigyaDiv'
            , buttonSize: 67
	        , buttonsStyle: 'standard'
	        , enabledProviders: 'facebook,Twitter,google,yahoo'
            , UIConfig: '<config><body><controls><snbuttons buttonsize="33"></snbuttons></controls></body></config>'
            , hideGigyaLink: 'true'
            , showWhatsThis: 'false'
            , onLoad: onLoadHandlerSSOButtons
        }
        var url = window.location;
    var login_params_mobile =
        {
            showTermsLink: 'false'
	        , height: 67
            , width: 371
	        , containerID: 'gigyaDivMobile'
            , buttonSize: 67
	        , buttonsStyle: 'standard'
	        , enabledProviders: 'facebook,Twitter,google,yahoo'
            , UIConfig: '<config><body><controls><snbuttons buttonsize="33"></snbuttons></controls></body></config>'
            , hideGigyaLink: 'true'
            , showWhatsThis: 'false'
            , onLoad: onLoadHandlerSSOButtons
            , redirectURL: 'http://www.mccormick.com//'
            , authFlow: 'redirect'
        }
    gigya.socialize.addEventHandlers({
        context: { str: 'congrats on your' }
				    , onLogin: onLoginHandler
    });

    gigya.socialize.showLoginUI(login_params);
    gigya.socialize.showLoginUI(login_params_mobile);
    
