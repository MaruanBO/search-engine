    <label id='ideoclick_dd'></label>

    <script>
        function handleResponse(response) {
            if (window.location.protocol == 'http:') {
                var data = eval(response);
                var sentData = data.dataReceived;
                $('#ideoclick_dd').html(sentData);
            }
        }
        $(document).ready(function () {
            $(function () {
                jQuery.ajax({
                    url:
                    'https://tools.ideoclick.info/demanddriver/testresponse.ashx?data=603&key=821ee658-fe04-424f-bec6-b4976c77a5e0',

                    dataType: 'jsonp',
                    type: 'GET',
                    cache: true,
                    jsonpCallback: 'handleResponse'
                });
            });
        });
    </script>
