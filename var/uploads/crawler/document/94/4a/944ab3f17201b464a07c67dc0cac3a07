        $(document).ready(function () {
            var blockUIConfig = {
                theme: true,
                message: $('#loadingMessageContainer').html()
            };
            if ($(window).width() < 480) {
                blockUIConfig = {
                    theme: true,
                    message: $('#loadingMessageContainer').html(),
                    themedCSS: {
                        width: '60%',
                        top: '40%',
                        left: '20%'
                    }
                };
            }

            //handle ajax request blocking
            $(document).ajaxStart(function () {
                $.blockUI(blockUIConfig);
            }).ajaxStop($.unblockUI);

            //handle page unload blocking
            $(window).on('beforeunload', function () {
                $.blockUI(blockUIConfig);
                var image = document.getElementById('loadingMessageIcon');
                setTimeout(function () {
                    image.src = image.src;
                }, 1);
            });

            if ($.browser.msie && +$.browser.version < 9 && $.cookies && !$.cookies.get('browserWarningClosed')) {
                clearInfoMessages();
                addInfoMessage('Your Internet Explorer is out of date.  To get the best possible experience using our website we recommend that you upgrade to a newer version or other web browser.');
                showInfoMessages(function ($) {
                    var expiresAt = new Date();
                    expiresAt.setDate(expiresAt.getDate() + 1);
                    $.cookies.set('browserWarningClosed', true, { expiresAt: expiresAt });
                } (window.jQuery));
            }

            $('a.top-link').attr('href', 'javascript:void(0);').click(function (e) {
                e.preventDefault();
                $('html, body').animate({
                    scrollTop: 0
                }, 750);
            });
        });
    </script>



