<script type="text/javascript">
    require(['get!merchandising/productBlock'],
        function (productBlock) {
            var getModelCount = function () {
                var modelCountToDisplay = 6;
                if ($(window).width() >= 768) {
                    modelCountToDisplay = 8;
                }

                if ($(window).width() >= 960) {
                    modelCountToDisplay = 10;
                }

                return modelCountToDisplay;
            };

            productBlock().init('HOME_RR', getModelCount);
        });
</script>



