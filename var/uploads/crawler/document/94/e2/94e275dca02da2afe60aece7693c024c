    require(["get!core/headerNavInit", "get!core/screenInit", "get!core/slider", "get!core/flyawayInit", "get!core/headerNavFlyaway", "get!core/headerNavFlyawayTouch"],
        function (headerNavInit, screenInit, slider, flyawayInit, headerNavFlyaway, headerNavFlyawayTouch) {
            screenInit(headerNavInit(), { minWidth: 768 });
            screenInit(slider({ selector: '.navSlider' }), { maxWidth: 767 });
            screenInit(flyawayInit(), { minWidth: 768 });
            screenInit(headerNavFlyaway({ menuItemSelector: '#MainNavItems > li', contentSelector: '.navFlyaway' }), { minWidth: 768, checkTouch: true, forTouch: false });
            screenInit(headerNavFlyawayTouch({ menuItemSelector: '#MainNavItems > li', contentSelector: '.navFlyaway' }), { minWidth: 768, checkTouch: true, forTouch: true });
        }
    );
</script>

