<script type="text/javascript" language="javascript">
    $(document).ready(function() {
        $('#username').focus();

        $('#loginForm').submit(function(evt) {
        var name = $('#username');
        var pass = $('#password');
           var errorArray = new Array();

        if(pass.val() == "" || pass.val() == "Password") {
            errorArray.push($('#password'));
            pass.focus();
        }
        
        if(name.val() == "" || name.val() == "Username"){
            errorArray.push($('#username'));
            name.focus();
        }

        if(errorArray.length > 0) {
            $.each(errorArray, function(index, value) {
            $(this).css('color', '#900');
            });
            evt.preventDefault();
            return false;
        }
        });
    });
</script>
