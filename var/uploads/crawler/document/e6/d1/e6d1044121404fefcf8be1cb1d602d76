<script type="text/javascript">
    CheckMobile();

    function LoadCookieData() {
        var isShowDownload = getCookie("ShowDownloadLink");
        if (isShowDownload != "") {
            if (isShowDownload.toLowerCase() == "true") {
                jQuery('#nmDownloadMobileLink').show();
            }
            else if (isShowDownload.toLowerCase() == "false") {
                jQuery('#nmDownloadMobileLink').hide();
            }
        }
    }

    function HideDownloadLink() {
        var vrTime = jQuery('#plc_lt_zoneLeft_ScheduleGrid_hfDayOfCookie').val();

        setCookie("ShowDownloadLink", false, parseInt(vrTime));
        jQuery('#nmDownloadMobileLink').hide();
    }
    function setCookie(a, d, c) {
        var e = new Date();
        e.setDate(e.getDate() + c);
        var b = escape(d) + ((c == null) ? "" : "; expires=" + e.toUTCString());
        document.cookie = a + "=" + b;
    }
    function getCookie(c_name) {
        try {
            var i, x, y, ARRcookies = document.cookie.split(";");
            for (i = 0; i < ARRcookies.length; i++) {
                x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
                y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
                x = x.replace(/^\s+|\s+$/g, "");
                if (x == c_name) {
                    if (unescape(y) == '' || unescape(y) == null) {
                        return "";
                    }
                    return unescape(y);
                }
            }
        }
        catch (e) {
        }
        return "";
    }

    function CheckMobile() {

        var isMobile = {
            Android: function () {
                return navigator.userAgent.match(/Android/i) ? true : false;
            },
            BlackBerry: function () {
                return navigator.userAgent.match(/BlackBerry/i) ? true : false;
            },
            iOS: function () {
                return navigator.userAgent.match(/iPhone|iPad|iPod/i) ? true : false;
            },
            Windows: function () {
                return navigator.userAgent.match(/IEMobile/i) ? true : false;
            },
            any: function () {
                return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Windows());
            }
        };

        if (isMobile.Android()) {
            jQuery('#aDownloadLink').attr('href', 'https://play.google.com/store/apps/details?id=com.fli.android.newsmaxapp');
            LoadCookieData();
        }
        else if (isMobile.iOS()) {
            jQuery('#aDownloadLink').attr('href', 'https://itunes.apple.com/us/app/newsmax-tv/id666513919?mt=8');
            LoadCookieData();

        }
        else {
            jQuery('#nmDownloadMobileLink').hide();
        }

    }


</script>

