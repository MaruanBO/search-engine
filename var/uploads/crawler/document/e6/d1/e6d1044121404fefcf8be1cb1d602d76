<script type="text/javascript">
    function ShowScheduleTab(TabID, PnlID) {
        jQuery('div[id^="tabSchedul"]').addClass('nmSegmentMainTab');
        jQuery('div[id^="tabSchedul"]').removeClass('nmSegmentMainTabSelected');
        jQuery('#' + TabID).removeClass('nmSegmentMainTab');
        jQuery('#' + TabID).addClass('nmSegmentMainTabSelected');
        jQuery('div[id^="pnlSchedule"]').hide();
        jQuery('#' + PnlID).show();
    }
    function LoadSegment(vrLiveShowID) {
        jQuery.ajax({
            type: "POST",
            url: "/CMSPages/NewsMax/Handlers/LiveShow.ashx",
            data: "operation=GetSegment&LiveShowID=" + vrLiveShowID,
            dataType: "json",
            cache: false,
            success: onSuccessGetSegment,
            error: onErrorGetSegment
        });
    }
    function onSuccessGetSegment(result) {
        var o = eval(result);
        var vrdata = "";
        var vrdataHeader = "";
        if (o.Success == "true") {
            for (var i = 0; i < o.data.length; i++) {

                var classNameHeader = "";
                if ((i == 0) || (o.data[i].IsDisplayHeader == "False")) {
                    classNameHeader = 'displayNone';
                }
                else if (o.data[i].BroadcastType == "Live") {
                    classNameHeader = 'segmentBlock LiveShowHeader';
                }
                else {
                    classNameHeader = 'segmentBlock ReplayShowHeader';
                }
                vrdata += '<div class="' + classNameHeader + '">';
                vrdata += '<div class="LiveShowHeaderText">';
                vrdata += o.data[i].ShowHost;
                vrdata += '</div>';
                vrdata += '<div class="LiveShowHeaderAiringTime">';
                vrdata += o.data[i].AiringTimes;
                vrdata += '</div>';
                vrdata += '</div>';
                var classNameNowPlaying = 'segmentBlock';
                var classNameNowPlayingShow = 'displayNone';

                if (i == 0) {
                    if (o.data[0].IsNowPlay.toLowerCase() == "true") {
                        classNameNowPlaying = "segmentBlock NOWPlaying";
                        classNameNowPlayingShow = "nmPlayingNow";
                    }
                }
                vrdata += ' <div class="' + classNameNowPlaying + '">';
                /*vrdata += '<div class="' + classNameNowPlayingShow + '">Now Playing...</div>';*/
                vrdata += '<div class="row">';
                vrdata += '<div class="nmTodayScheduleLeft">';
                vrdata += '<div class="nmTodayTime">';
                vrdata += o.data[i].TimeShort;
                vrdata += '<span class="nmTodayEST"> ';
                vrdata += o.data[i].IsAMPM;
                vrdata += ' (EST)</span>';
                vrdata += '</div>';
                vrdata += '<div class="nmBrodcast">';
                if (o.data[i].BroadcastType == "Live") {
                    vrdata += '<img src="/App_Themes/NewsmaxNew/images/NewsmaxTV/airinglive_small.png" />';
                }
                else {
                    vrdata += '<img src="/App_Themes/NewsmaxNew/images/NewsmaxTV/replay_small.png" />';
                }
                vrdata += '</div>';
                vrdata += ' </div>';
                vrdata += '<div class="nmTodayScheduleRight">';
                vrdata += '<div class="nmTodayScheduleRightText">';
                var vrDescription = jQuery('<div/>').html(o.data[i].Description).text();
                vrdata += vrDescription;
                vrdata += '</div>';
                vrdata += '</div>';
                vrdata += '</div>';
                vrdata += '</div>';

                /*Load Header Titel*/

                if (i == 0) {

                    vrdataHeader += '<div class="row nmAiringLiveMain">';

                    vrdataHeader += '<div class="nmSegmentAiringLiveImage">';
                    if (o.data[0].BroadcastType == "Live") {
                        vrdataHeader += '<img src="/App_Themes/NewsmaxNew/images/NewsmaxTV/airinglive_large.png" />';
                    }
                    else {
                        vrdataHeader += '<img src="/App_Themes/NewsmaxNew/images/NewsmaxTV/replay_large.png" />';
                    }
                    vrdataHeader += ' </div>';
                    vrdataHeader += '<div class="nmSegmentForumText">';
                    vrdataHeader += ' <b>';
                    vrdataHeader += o.data[0].ShowName;
                    vrdataHeader += '</b> with ';
                    vrdataHeader += o.data[0].ShowHost;
                    vrdataHeader += ' </div>';
                    vrdataHeader += '</div>';
                }
            }
        }
        else {
            vrdata += '<div class="nmEmptySegment">Loading Show Data.</div>';
        }
        vrdata += '<div class="dvFooterBoder">&nbsp;</div>';
        jQuery('#nmSegmentDisplay').html(vrdata);
        jQuery('#NMdataHeader').html(vrdataHeader);

    }
    function onErrorGetSegment(result) {
        if (result.status == 200) {
            onSuccessGetSegment(result);
        }
    }
</script>
