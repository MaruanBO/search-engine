<!-- Ooyala V3 Player Module -->
<script type="text/javascript">
    var OoyalaV3PlayerModule = (function (parent, ns_, $) {

        var module = parent = parent || {},
            self = this;
            players = (typeof module.getPlayers === 'function') ? module.getPlayers() : [];

        module.init = function (params) {
            self.createPlayer(params);
        };

        module.getPlayer = function (id) {
            return $.grep(players, function (e) { console.log(e); return e.pid == id; });
        };

        module.getPlayers = function () {
            return players;
        };

        self.createPlayer = function (params) {

            // Default params
            var default_params = {
                onCreate: window.onCreate,
                pid: 'nmx_vplayer_' + Date.now(),
                cid: 'RqcGJ4YzqP1btsdPQA-BnlWDUpdtiLMD',
                autoplay: true,
                width: '520px',
                height: '326px'
            };

            params = jQuery.extend({}, default_params, params);

            // Get url hash if available
            var hash = location.hash.match(new RegExp('#ooid=([^&]*)'));
            if (hash) {
                params.cid = hash[1];
            }

            // Perhaps we have the ooid as a url param
            var urlparam = location.href.match(new RegExp('[?]ooid=([^&]*)'));
            if (urlparam) {
                params.cid = urlparam[1];
            }

            // Prepare Player
            var player = OO.Player.create(params.pid, params.cid,
            {
                onCreate: params.onCreate,
                autoplay: params.autoplay,
                width: params.width,
                height: params.height
            });

            // Ooyala Video V3 w/ StreamSense
            (function (pl) {
                
                var clips = [];
                var streamSense = new ns_.StreamSense({}, 'http://b.scorecardresearch.com/p?c1=2&c2=9248945');
                streamSense.setPlaylist({ ns_st_pl: "NewsmaxTV" });

                // Custom Labels
                streamSense.setLabels({
                    ns_site: 'newsmax',
                    name: 'nmxwebpart',
                    category: 'video',
                    page_type: 'index',
                    content_type: 'index',
                    content_title: 'nmax_video_webpart',
                    content_id: '',
                    content_source: 'nmaxvideowebpart'
                });

                pl.mb.subscribe(OO.EVENTS.CONTENT_TREE_FETCHED, 'newsmaxtv', function (e, v) {

                    clips[0] = {
                        ns_st_cn: 1, // The clip number
                        ns_st_ci: v.embedCode, // Internal content id
                        ns_st_pr: v.title, // Program title
                        ns_st_cl: v.time, // Length of the stream (milliseconds) always 0 since its live
                        ns_st_pn: 1, // Part Number
                        ns_st_tp: 0, // Total Parts
                        ns_st_pu: 'Newsmax', // Publisher Brand Name
                        ns_st_ge: 'News', // Content genre
                        ns_st_li: 0 // Live
                    }

                    streamSense.setClip(clips[0]);

                });

                pl.mb.subscribe(OO.EVENTS.PLAYING, 'newsmaxtv', function () {
                    streamSense.notify(ns_.StreamSense.PlayerEvents.PLAY, {}, 0);
                });

                pl.mb.subscribe(OO.EVENTS.BUFFERING, 'newsmaxtv', function () {
                    streamSense.notify(ns_.StreamSense.PlayerEvents.BUFFER, {}, 0);
                });

                pl.mb.subscribe(OO.EVENTS.PAUSED, 'newsmaxtv', function () {
                    streamSense.notify(ns_.StreamSense.PlayerEvents.PAUSE, {}, 0);
                });

                pl.mb.subscribe(OO.EVENTS.DESTROY, 'newsmaxtv', function () {
                    streamSense.notify(ns_.StreamSense.PlayerEvents.END, {}, 0);
                });
                
                players.push({ pid: params.pid, player: pl });

            })(player);

        }

        return module;

    }(OoyalaV3PlayerModule || {}, ns_, jQuery));

    jQuery(function () {
        OO.ready(function () {
            var cid = 'RqcGJ4YzqP1btsdPQA-BnlWDUpdtiLMD';
            if (cid != '') {
                // Initialize Player if we have content id only
                // the player can be handled by outside sources as well
                OoyalaV3PlayerModule.init(
                    {
                        onCreate: window.onCreate,
                        pid: 'NM_TV_Player_home',
                        cid: 'RqcGJ4YzqP1btsdPQA-BnlWDUpdtiLMD',
                        autoplay: 'true',
                        width: '580px',
                        height: '326px'
                    });
            }

        });
    });
    
</script>

