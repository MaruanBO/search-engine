LF.CommentCount({
    replacer: function(element, count){
        MVN.queue(function(){
            if(count == 0){
                $(element).closest('span.comments').remove();
            }else{
                $(element).html('(' + count +') Comment'+ (count === 1 ? '' : 's'));
            }
        });
    }
});
