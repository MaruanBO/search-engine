<script type="text/javascript">
	$(document).ready(function(){
		var cookie_text = "This site uses cookies <a rel=\"cookie_reg\" class=\"colorset_personal\" href=\"#cookie_reg_lb\">more info</a>";
		$("#cookie_reg_footer_container").html(cookie_text);
		$("#cookie_reg_content").html(cookie_text);
		
		var cookie_reg_viewed = false;
		$(window).scroll(function(){
			if($(window).scrollTop()>0) $("#cookie_reg").fadeOut();
			else if(!cookie_reg_viewed)$("#cookie_reg").fadeIn();
		});
		setTimeout(function(){$("#cookie_reg").fadeOut(); cookie_reg_viewed = true;},20000);
		
		$(".cookie_reg_selector").change(function(){
			if($(this).val()=="necessary"){
				$(".targeting").each(function(){ $(".disabled").append($(this)); });
			}
			else if($(this).val()=="targeting"){
				$(".targeting").each(function(){ $(".enabled").append($(this)); });
			}
		});
		
		$("a[rel='cookie_reg']").click(function(){
			$($(this).attr("href")).fadeIn();
		});
		
		$("#cookie_reg_lb").click(function(e){ 
			if($(e.target).attr("id")=="button_cookie_cancel" || $(e.target).attr("id")=="cookie_reg_close" || $(e.target).attr("id")=="cookie_reg_lb") $(this).fadeOut(); 
			if($(e.target).attr("id")=="button_cookie_save"){
				var d = new Date();
				d.setTime(d.getTime()+(24*60*60*1000));
				document.cookie = "cookiesetting=" + $(".cookie_reg_selector").val() + "; expires=" + d.toGMTString() + "; path=/";
				$(this).fadeOut();
				location.reload();
			}
		});
		
		$(".cookie_reg_selector").val("targeting");
		$(".cookie_reg_selector").trigger("change");
		
	});
</script>

