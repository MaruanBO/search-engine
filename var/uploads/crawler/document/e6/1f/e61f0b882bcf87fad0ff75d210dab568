<script type="text/javascript">
    // Call slider on Index Home Users Div
    function slider() {
        function animate_slider() {
            $('.slider_home_users_wrapper #' + shown).hide();
            $('.slider_home_users_wrapper #' + next_slide).fadeIn(1000);
            shown = next_slide;
        }
        function choose_next() {
            next_slide = (shown == sc) ? 1 : shown + 1;
            animate_slider();
        }
        $('.slider_home_users_wrapper #1').css({display: "block"});

        var shown = 1;
        var next_slide;
        var sc = $('.slider_home_users').length;
        var iv = setInterval(choose_next, 5000);

        $('.slider_home_users').hover(function () {
            clearInterval(iv);
        }, function () {
            iv = setInterval(choose_next, 5000);
        });

        $('.slider_home_users_nav').click(function (e) {
            var n = e.target.getAttribute('name');
            if (n == 'prev') {
                next_slide = (shown == 1) ? sc : shown - 1;
            } else if (n == 'next') {
                next_slide = (shown == sc) ? 1 : shown + 1;
            } else {
                return;
            }
            animate_slider();
        });
    }
    window.onload = slider;
</script>

