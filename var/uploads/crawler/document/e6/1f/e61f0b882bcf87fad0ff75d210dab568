<script type="text/javascript">
    $(document).ready(function () {
        $('input').placeholder();
        $('a[href="#"]').click(function (e) {
            e.preventDefault();
        });
        var lb_index = $("#lb_wrapper_index");
        var lb_message = $("#lb_message");
                    // show lightbox after a few seconds
                setTimeout(function () {
                    lb_index.fadeIn();
                }, 500);
    
        // lightbox close call
        $(".lb_close").click(function () {
            $("#lb_wrapper_index").fadeOut("fast", function () {
            });
            $("#lb_message").fadeOut("fast", function () {
            });
            var now = new Date();
            var time = now.getTime();
            time += 3600 * 1000;
            now.setTime(time);
            document.cookie =
                    'close_lightbox=true'
            '; expires=' + now.toUTCString() +
                    '; path=/';
        });
        $("#lb_wrapper_index").click(function (e) {
            if ($(e.target).attr("id") == "lb_wrapper_index")
                $("#lb_wrapper_index").fadeOut("fast", function () {
                });
            var now = new Date();
            var time = now.getTime();
            time += 3600 * 1000;
            now.setTime(time);
            document.cookie =
                    'close_lightbox=true'
            '; expires=' + now.toUTCString() +
                    '; path=/';
        });
    });
</script>

<script type="text/javascript">
	$(document).ready(function(){
		// lightbox close call
		$(".lb_close").click(function(){ $("div[class*='lavasoft_lb']").fadeOut(); });
		$(".lavasoft_lb").click(function(e){ if($(e.target).attr("class")=="lavasoft_lb") $("div[class*='lavasoft_lb']").fadeOut(); });
		
		// lightbox load call
		$("a[rel*='lavasoft_lb']").click(function(){
			var lb = $("div[rel='"+ $(this).attr('rel') +"']");
			if(lb.find(".lb_content").find("iframe").width()!=null){
				var width = lb.find(".lb_content").find("iframe").width()+20;
			}
			lb.find(".lb_wrapper").css("width", width);
			lb.find("div[class*='_middle']").css("width", width-28);
			lb.fadeIn();
		});
		
		// adjust Width of Lightbox accordingly 
		var width = 940;
		$(".lb_wrapper").css("width", width);
		$("div[class*='_middle']").css("width", width-28);
	});
</script>
