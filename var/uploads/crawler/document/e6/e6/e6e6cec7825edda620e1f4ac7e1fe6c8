<script>


	var refreshDisabled = false;
	
	if(jQuery.cookie('refreshOff')) {
		refreshDisabled = true;
	}	

	var isHome = true;;

	var myTimeOut = (isHome == true) ? 600000 : 600000;

	
	var isMobile = {
	    Android: function() {
	        return navigator.userAgent.match(/Android/i);
	    },
	    BlackBerry: function() {
	        return navigator.userAgent.match(/BlackBerry/i);
	    },
	    iOS: function() {
	        return navigator.userAgent.match(/iPhone|iPad|iPod/i);
	    },
	    Opera: function() {
	        return navigator.userAgent.match(/Opera Mini/i);
	    },
	    Windows: function() {
	        return navigator.userAgent.match(/IEMobile/i);
	    },
	    any: function() {
	        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
	    }
    };  
    

	var time = new Date().getTime();

	/* you must check the footer for the jQuery timer reset function */
	
	
	function refresh() {

		if((new Date().getTime() - time >= myTimeOut && !isMobile.any()) && refreshDisabled == false) {
			window.location.reload(true);
		} else {
			setTimeout(refresh, 10000);
		}

			
	}
	
		
	setTimeout(refresh, 10000);
	





	jQuery(document.body).bind("mousemove keypress", function(e) {
		//time = new Date().getTime();
	});
	
	jQuery(document).ready(function() {

		jQuery(".hideMe").css("display","none");

	
	    if(!(jQuery.cookie('refreshOff'))){
			
			jQuery(".hideMe").css("display","block");
			
		    jQuery("a#disableRefreshButton").on("click",function(){
			    	refreshDisabled = true;
					jQuery.cookie('refreshOff', 'true', { expires: 7, path: '/' });
					jQuery(this).html("Success");	
					jQuery(".hideMe").delay(1000).fadeOut();	    	
		    })
		
		} 


	});	
	
</script>
