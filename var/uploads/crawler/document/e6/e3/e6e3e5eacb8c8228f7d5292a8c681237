    <script>
        $(function () {
            //////////////
            //isPlaying is defined in slideshow macro.
            //////////////
            var isClick = false;
            $("#slide-nav a").click(function () {
                // $('iframe').get(0).stopVideo();
                var slide = $(this).attr("data-slide");
                $("#slide-nav a").removeClass("active");
                $(this).addClass("active");
                isPlaying = false;
                isClick = true;
            });

            $('#slide-inspirations').flexslider({
                animation: "slide",
                slideshow: false,
                controlNav: true,
                start: function (slider) {
                    $("#slide-inspirations").css("display", "block");
                },
                before: function (s) {

                }
            });

            // Initialize Slides
            $('.slider').slides({
                preload: false,
                preloadImage: '/img/ajax-loader.gif',
                generatePagination: true,
                play: 5000,
                pause: 1,
                hoverPause: true,
                // Get the starting slide
                // start: startSlide,
                animationStart: function () {
                    PauseVideos();
                    //stopVideo();
                    //$(slideEvent).trigger('click');
                },
                animationComplete: function (current) {
                    $("#slide-nav ul li a").removeClass("active");
                    $("#slide-nav ul li:nth-child(" + current + ") a").addClass("active");

                    isPlaying = isClick;
                    isClick = false;
                    // Set the slide number as a hash
                    //window.location.hash = '#' + current;
                }
            });




        });
	</script>

	


