<script language="javascript">

function setColumns() {
	var columns = 3;
	var max1 = 0;
	var columnsDifs = new Array();
	for (i=1;i<=columns;i++){
		max1 = Math.max(max1,document.getElementById('column_'+i).offsetHeight);
	}
	for (i=1;i<=columns;i++){
		columnsDifs.push(max1 - document.getElementById('column_'+i).offsetHeight);
	}

	for (i=1;i<=columns;i++){
		var changedHeight = document.getElementById('columne_'+i).offsetHeight + columnsDifs[i-1];
		document.getElementById('columne_'+i).style.height = changedHeight+'px';
	}
}

function changediv (divname) {
	for (i=0; i<=9; i++) {
		divname.style.backgroundImage = 'none';
	}
	newHeight = divname.parentNode.offsetHeight;
	divname.parentNode.style.height = newHeight; // IE has issues hovering over the whole div if we don't do this trick
	divname.style.padding = "0px";
	divname.style.width = "auto";
	divname.style.height = "auto";
	divname.style.margin = "0px";
	divname.style.backgroundImage = 'url(ui/templates/gray/first_page-shadows.jpg)';
	divname.style.backgroundPosition = 'right bottom';
	divname.style.backgroundRepeat = 'no-repeat';
	document.body.style.cursor = 'pointer';

}
function cleardiv (divname) {

	divname.style.backgroundImage = 'none';
	document.body.style.cursor = 'default';

}

</script>



