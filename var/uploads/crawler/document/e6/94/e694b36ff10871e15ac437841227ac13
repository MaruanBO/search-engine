 <script type="text/javascript">

function automcomplete_bind(form_element, form_name,extra_filters)
{
   $(function() 
   {
    	if(($("#AutoSuggestArgs").length > 0) )
     	{
		    var val = document.getElementById('AutoSuggestArgs').value.replace('amp;','')
			var tablename = (val.substr(val.indexOf("table=") + 6));
			var matches = val.match(/a=(.+)\&/);
			if (matches.length > 1) 
			{
				var account_id = matches[1].substring(0, 4);
			}
		}
       $("#as_filterChart").autocomplete({
            source: "/script/jquery-ui-1.8/driver.php?account_id="+account_id+"&tablename="+tablename+"&extra_filters="+extra_filters,
            minLength: 3,
            select: function(event, ui) 
            {
            	$("#as_filterChart").val(ui.item.value);
                document.forms[form_name].submit();

            }
        });
       $('#as_filterChart').keypress(function(e) {
		    if (e.which == 13) {
		        $(this).autocomplete("destroy");
		    }
		});

    });

}

    </script>
