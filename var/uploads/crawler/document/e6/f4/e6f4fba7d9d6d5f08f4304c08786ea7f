function cmVCCatId(defaultCatId, oiid, vcString)
{
cmCatId = defaultCatId;
if ( vcString == null || vcString == '' )
return cmCatId;
ind1 = vcString.indexOf(oiid+'~~~');
if ( ind1 >= 0 ) {
ind1 = ind1 + oiid.length + 3;
ind2 = vcString.indexOf("@", ind1 );
if ( ind2 > ind1 ) {
cmCatId = vcString.substring(ind1, ind2);
}
}
return cmCatId;
}
