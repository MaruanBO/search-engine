pageViewAttr[1] = '';
pageViewAttr[2] = '';
pageViewAttr[3] = '';
for (var i = 0; i < vehicleMeta.length; i++) {
var ymm = vehicleMeta[i].split('|');
var temp = vehicleMeta[i].replace(/\|/g, ' ');
var engine = vehicleDesc[i].slice(temp.length);
while (engine.charAt(0) == ' ' ) {
engine = engine.slice(1);
}
if (i != (vehicleMeta.length - 1)) {
pageViewAttr[0] += ymm[0] + '|';
pageViewAttr[1] += ymm[1] + '|';
pageViewAttr[2] += ymm[2] + '|';
pageViewAttr[3] += engine + '|';
} else {
pageViewAttr[0] += ymm[0];
pageViewAttr[1] += ymm[1];
pageViewAttr[2] += ymm[2];
pageViewAttr[3] += engine;
}
}
} else {
var ymm = tempMeta.split('|');
var vehicleDesc = jsVehicleDesc[0];
var temp = tempMeta.replace(/\|/g, ' ');
var engine = vehicleDesc.slice(temp.length);
while (engine.charAt(0) == ' ' ) {
engine = engine.slice(1);
}
pageViewAttr[0] = ymm[0];
pageViewAttr[1] = ymm[1];
pageViewAttr[2] = ymm[2];
