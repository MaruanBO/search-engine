		function getDestPageName(href) {
	    	var linksMap = {
	    			'javascript:showCartContent(this);'        : 'Shopping Cart',
                    'http://stage.anntaylor.com/loft/home/index.jsp' : 'LOFT Home Page',
                    'http://liveloveloft.com/'                 : 'Loft Blog',
                    'http://teachers.anntaylorloft.com/'       : 'Loft Loves Teachers',
                    '/loft/catalog/common/editorial.jsp'       : 'Weddings',
                    '/loft/profile/orderStatusNotLoggedIn.jsp' : 'Orders Status (not logged in)',
                    '/loft/profile/login.jsp'                  : 'Account Login and Setup',
                    '/loft/stores.jsp'                         : 'Store Locator',
                    '/loft/common/home.jsp'                    : 'Home Page',
                    '/loft/siteMap.jsp'                        : 'Site Map',
                    'pageName=ATCard'                          : 'Customer Service:Credit Card',
                    'pageName=Privacy'                         : 'Customer Service:Privacy & Security',
                    '/loft/giftCard.jsp'                       : 'Gift Cards',
                    '/loft/customer-service'             : 'Customer Service:Contact us',
                    'http://www.anntaylorstorescorp.com/'      : 'Corporate',
                    'http://careers.anntaylor.com/'            : 'Careers',
					'/loft/department/LOFT_Apparel/catl00002'       : 'Apparel Landing Page',
					'/loft/department/LOFT-Shoes-%26-Accessories/catl00003'   : 'Accessories Landing Page',
					'/loft/department/LOFT-Style-Studio/catl00004'  : 'Style-Studio Landing Page',
					'/loft/department/LOFT-Petite/catl00005'        : 'Petite Landing Page',
					'/loft/department/LOFT-Maternity/catl00006'     : 'Maternity Landing Page',
					'/loft/department/LOFT-Beach/catl00007'         : 'Beach Landing Page',
					'/loft/department/LOFT-Sale/catl00008'          : 'Sale Landing Page',
					'/loft/editorial/Petite/Petite-Fit-Guide/catl000067' : 'Petite:LOFT Petite Fit Guide',
					'/loft/findinstore/findInStore.jsp'		  : 'Style Finder Page',
					'/findinstore/findInStoreResults.jsp'	  : 'Style Finder Results Page',
		            '/findinstore/inputDirections.jsp'		  : 'Style Finder Directions',
		            '/international/internationalIndex.jsp'	  : 'Checkouts',
		            '/international/envoyOrderSuccess.jsp'	  : 'Thank you',
		            '/cwm/instantcredit/cbcc/instantCreditCBCCRegistration.jsp':'CBCC Application Page',
					'/cwm/instantcredit/cbcc/instantCreditCBCCApproval.jsp':'CBCC Approval Page ',
					'/cwm/instantcredit/cbcc/instantCreditCBCCApprovalNeedMoreInfo.jsp':'CBCC Not Approved Page- Under Review',
					'/cwm/instantcredit/instantCreditRegistration.jsp':'PLCC Application Page',
					'/cwm/instantcredit/instantCreditApproval.jsp':'PLCC Approval Page ',
					'/cwm/instantcredit/instantCreditApprovalNeedMoreInfo.jsp':'PLCC Not Approved Page- Under Review'
					}


	    	var dest = linksMap[href];
	    	if (dest == null) {
		    	if (href.indexOf('/loft/custserv/custserv.jsp') == 0) {
			    	var s = href.split('?');
			    	if (s[1] != null) {
				    	var t = s[1].split('&');
				    	dest = linksMap[t[0]];
				    	if (dest == null) {
					    	dest = href;
				    	}
			    	}
		    	} else if (href.indexOf('/loft/common/popupContent.jsp?contentId=') == 0) {
			    	dest = 'Marketing Content';
			    } else {
		    		var arr = href.split('/');
		        	if (arr[2] != null && arr[2] == 'sale' && arr[3] != null) {
		        		dest = unescape(arr[3]);
		        	} else if (arr[2] != null &&
		                	(arr[2] == 'cat' || arr[2] == 'outfit' || arr[2] == 'editorial' || arr[2] == 'product') &&
		                	arr[3] != null && arr[4] != null) {
		        		dest = unescape(arr[3]) + ":" + unescape(arr[4]);
		        	} else {
		        		dest=href;
		        	}
		    	}
	    	}
	    	return dest;
		}
