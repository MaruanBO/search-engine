        <script>
    (function () {
        if (location.pathname !== "/error/upgradebrowser") {
            var N = navigator.appName, ua = navigator.userAgent, tem;
            var M = ua.match(/(opera|chrome|safari|firefox|msie)\/?\s*(\.?\d+(\.\d+)*)/i);
            if (M && (tem = ua.match(/version\/([\.\d]+)/i)) != null) M[2] = tem[1];
            var browserDetails = M ? [M[1], M[2]] : [N, navigator.appVersion, '-?'];
            var browserDetailsName = [browserDetails[0]];
            var browserDetailsVersion = [browserDetails[1]];
            var redirectUrl = "/error/upgradebrowser?status=browser";
            var isAndroid = ua.match(/(android)\/?\s*(\.?\d+(\.\d+)*)/i);

            if ((browserDetailsName == "MSIE" && Number(browserDetailsVersion) <= 7) || (browserDetailsName == "Firefox" && Number(browserDetailsVersion) <= 3.6) || (browserDetailsName == "Opera" && Number(browserDetailsVersion) <= 10.6) || (browserDetailsName == "Safari" && Number(browserDetailsVersion) <= 4 && !isAndroid) || (browserDetailsName == "Netscape" && Number(browserDetailsVersion) <= 9)) {
                window.location = redirectUrl;
            }
        }
    })();
</script>

