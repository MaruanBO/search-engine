    <script type="text/javascript">
    var _ajaxReport = {};
        document.observe('dom:loaded', function () {
            checkCartQty();
            //detectCustomerStatus();
            detectMarketingParams();
            //getSalesruleDefultCoupon(); detectMarketingParams calls this onComplete.
            retrieveProductData();
        });

        function in_array(needle, haystack) {
            for(var i in haystack) {
                if(haystack[i] == needle) return true;
            }
            return false;
        }

        jQuery('#get_code').on('click', respondToClick);
        if(document.getElementById("get_code2")) {
            $('get_code2').observe('click', respondToClick);
            function in_array(needle, haystack) {
                for(var i in haystack) {
                    if(haystack[i] == needle) return true;
                }
                return false;
            }
        }
        function respondToClick(event) {
            var element = Event.element(event);
            var uri = 'http://www.ldproducts.com/servicecode/?url=http%3A%2F%2Fwww.ldproducts.com%2Fcheckout%2Fcart%2F';
            if (location.protocol == 'https:' && !uri.match('https:')) {
                uri = 'https://www.ldproducts.com/servicecode/?urlhttp%3A%2F%2Fwww.ldproducts.com%2Fcheckout%2Fcart%2F/'
            }
            element.update("Loading..." );
            jQuery.ajax({
                type: 'get',
                url: uri
            }).done(function(transport) {
                var response = transport;
                if (!response.match(/html/i)) {
                    element.update("Service code: " + response);
                } else {
                    element.update("Error with generating service code!");
                }
            });
        }
        function detectCustomerStatus() {
            _ajaxReport['detectCustomerStatus'] = 0;
            var uri = 'http://www.ldproducts.com/ldpcheckout/cart/customerStatus/';
            if (location.protocol == 'https:' && !uri.match('https:')) {
                uri = 'https://www.ldproducts.com/ldpcheckout/cart/customerStatus/'
            }
            new Ajax.Request(uri, {
                method: 'get',
                asynchronous: true,
                onSuccess: function (transport) {
                    _ajaxReport['detectCustomerStatus'] = 1;
                    updateCustomerStatus(parseInt(transport.responseText));
                },
                onComplete: function(transport){
                    //detectMarketingParams();
                }
            });
        }

        function updateCustomerStatus(status) {
            var htmlData = '<a href="https://www.ldproducts.com/customer/account/create/">Create Account</a> or <a class="blue heavy" href="https://www.ldproducts.com/customer/account/login/">Sign In</a>';
            if (status == 1) {
                var htmlData = '<a href="https://www.ldproducts.com/customer/account/">Manage Account</a> or <a class="blue heavy" href="https://www.ldproducts.com/customer/account/logout/">Sign Out</a>';
            }
            $('customer_status').innerHTML = htmlData;
            if (typeof(PageInfo) !== 'undefined'){
                PageInfo.login = status;
            }
        }

        function queryStringArgs() {
            var qs = (location.search.length > 0 ? location.search.substring(1) : "");
            var args = {};

            var items = qs.split("&");
            var item = null, name = null, value = null;

            for (var i = 0; i < items.length; i++)
            {
                item = items[i].split("=");
                if (item[0])
                    args[decodeURIComponent( item[0].toLowerCase())] = decodeURIComponent(item[1]);
            }

            return args;
        }

        function detectMarketingParams() {
            _ajaxReport['detectMarketingParams'] = 0;
            var currentUrl = window.location.href.split('?')[0];
            var getargs = queryStringArgs(); // from js.js
            var uri = "http://www.ldproducts.com/ldpmarketing/ajax/params/?";
            var requestParams = [];
            var config = ["xid","aid","kid","sid","coupon","pid","cid"];
            for (var param in getargs) {
                param = param.toLowerCase();
                if (getargs[param] != 'undefined' && in_array(param, config)) {
                    requestParams.push(encodeURIComponent(param) + "=" + encodeURIComponent(getargs[param]));
                }
            }
            if (requestParams.length > 0) {
                //requestParams.push('source=' + encodeURIComponent(currentUrl));
                new Ajax.Request(uri + requestParams.join('&'), {
                    method: 'get',
                    asynchronous: true,
                    onSuccess: function (transport) {
                        _ajaxReport['detectMarketingParams'] = 2;
                        // do something there
                    },
                    onComplete: function (transport){
                        _ajaxReport['detectMarketingParams'] = 2;
                        getSalesruleDefultCoupon();
                    }
                });
            } else {
                _ajaxReport['detectMarketingParams'] = 1;
                getSalesruleDefultCoupon();
            }
        }

        function getSalesruleDefultCoupon() {
            _ajaxReport['getSalesruleDefultCoupon'] = 0;
            var reportlog = {};
            reportlog['getSalesruleDefultCoupon'] = true;
            var url = '';
            var params = {};
            if (typeof moduleController === 'undefined') {
                var moduleController = 'checkout|cart|index';
            }
            var allowedModuleController = ['cms|page|view', 'cms|page|noRoute', 'catalog|product|view', 'catalogsearch|result|index'];
            reportlog['allowedModuleController'] = in_array(moduleController, allowedModuleController);
            if (moduleController && in_array(moduleController, allowedModuleController)) {
                if (typeof _printerIdsForCoupon != "undefined") {
                    url = "http://www.ldproducts.com/ldpsalesrule/ajax/getDefaultCouponForPrinters/";
                    params['product_ids'] = _printerIdsForCoupon;
                    reportlog['path'] = '_printerIdsForCoupon';
                    reportlog['url'] = url;
                    reportlog['params'] = params['product_ids'];
                } else if (typeof printerIdsForCoupon != "undefined") {
                    if (moduleController == 'catalogsearch|result|index') {
                        url = "http://www.ldproducts.com/ldpsalesrule/ajax/getDefaultCouponForPrinters/";
                        params['product_ids'] = printerIdsForCoupon;
                        reportlog['path'] = 'printerIdsForCoupon - catalogsearch';
                        reportlog['url'] = url;
                        reportlog['params'] = params['product_ids'];
                    } else {
                        url = "http://www.ldproducts.com/ldpsalesrule/ajax/getDefaultCouponForPrinter/";
                        params['product_id'] = printerIdsForCoupon;
                        reportlog['path'] = 'printerIdsForCoupon';
                        reportlog['url'] = url;
                        reportlog['params'] = params['product_id'];
                    }
                } else if (typeof currentProductId != "undefined") {
                    if ($('coupon_price')) {
                        url = "http://www.ldproducts.com/ldpsalesrule/ajax/getDefaultCouponForProduct/";
                        params['product_id'] = currentProductId;
                        reportlog['path'] = 'currentProductId';
                        reportlog['url'] = url;
                        reportlog['params'] = params['product_id'];
                    } else {
                        reportlog['path'] = 'currentProductId sans #coupon_price';
                        reportlog['url'] = false;
                        reportlog['params'] = false;
                    }
                }
                var requestParams = [];
                for (var param in params) {
                    requestParams.push(encodeURIComponent(param) + "/" + params[param]);
                }
                reportlog['requestParams'] = requestParams;
                if (url) {
                    new Ajax.Request(url + requestParams.join('/'),
                        {
                            method: 'get',
                            asynchronous: true,
                            onSuccess: function(transport) {
                                _ajaxReport['getSalesruleDefultCoupon'] = 1;
                                reportlog['transport'] = transport;
                                var data = transport.responseText.evalJSON(true);
                                reportlog['transportData'] = data;
                                if ($('coupon_price')) {
                                    reportlog['markup'] = '#coupon_price';
                                    reportlog['couponCode'] = data['couponCode'];
                                    reportlog['percentOff'] = data['percentOff'];
                                    if (data && ('couponCode' in data) && ('percentOff' in data)) {
                                        if (data['couponCode'].split(",").length > 1 ){
                                            $('coupon_price').innerHTML = "The Coupons In Your Cart Will Give You An Additional <span class=\"coupon_discount\">"+data['percentOff']+"% Off</span> This Product";
                                        }else{
                                            $('coupon_price').innerHTML = "The Coupon <span class=\"coupon_code\">'"+data['couponCode']+"'</span> Will Give You An Additional <span class=\"coupon_discount\">"+data['percentOff']+"% Off</span> This Product";
                                        }
                                    }
                                } else {
                                    reportlog['markup'] = '#coupon_price_key';
                                    if (data && ('couponCode' in data) && ('products' in data)) {
                                        reportlog['products'] = data.products;
                                        for (var key in data.products) {
                                            if (($('coupon_price_'+key) != undefined) && (data.products[key]['percentOff'] > 0)) {
                                                if (data['couponCode'].split(",").length > 1 ){
                                                    $('coupon_price_'+key).innerHTML =  "The Coupons In Your Cart Will Give You An Additional <span class=\"coupon_discount\">"+data.products[key]['percentOff']+"% Off</span> This Product";
                                                }else{
                                                    $('coupon_price_'+key).innerHTML =  "The Coupon <span class=\"coupon_code\">'"+data['couponCode']+"'</span> Will Give You An Additional <span class=\"coupon_discount\">"+data.products[key]['percentOff']+"% Off</span> This Product";
                                                }
                                            }
                                        }
                                    }
                                }
                                _ajaxReport['couponReport'] = reportlog;
                            },
                            onComplete: function(transport){
                                //retrieveProductData();
                            }

                        });
                }else{
                    _ajaxReport['couponReport'] = reportlog;
                    _ajaxReport['url'] = url;
                    //retrieveProductData();
                }
            }else{
                _ajaxReport['couponReport'] = reportlog;
                _ajaxReport['moduleController'] = moduleController;
                _ajaxReport['allowedModuleControllers'] = allowedModuleController;
                //retrieveProductData();
            }
        }

        function retrieveProductData() {
            _ajaxReport['retrieveProductData'] = 0;
            var _productIds = _ldpGlobalProducts;
            if (typeof currentProductId === 'undefined') {
                currentProductId = '';
            }
            if (typeof moduleController === 'undefined') {
                moduleController = '';
            }
            var allowedModuleController = ['catalog|category|view','catalog|product|view','catalogsearch|result|index'];

            var url = 'http://www.ldproducts.com/ldpcatalog/ajax/retrieveProductData/';

            if (!(_productIds.length)) {
                if (moduleController
                    && in_array(moduleController, allowedModuleController)) {
                    jQuery('.price').each(function (e) {
                        if (this.id.match(/^product-price/)) {
                            _productIds.push(this.id.split('-')[2]);
                        }
                    });
                }
                if (moduleController == 'catalogsearch|result|index') {
                    jQuery('.priceblock').each(function (e) {
                        if (this.id.match(/^product-priceblock/)) {
                            _productIds.push(this.id.split('-')[2]);
                        }
                    });
                }
                if (currentProductId) {
                    _productIds.push(currentProductId);
                }
            }
            if (_productIds.length > 0) {
                new Ajax.Request(url, {
                    method: 'get',
                    asynchronous: true,
                    parameters: {'ids': _productIds.join(),'module': moduleController, 'product_id': currentProductId},
                    onSuccess: function (transport) {
                        _ajaxReport['retrieveProductData'] = 1;
                        var result = transport.responseText.evalJSON();
                        for (var i =0 ; i < _productIds.length; i++) {
                            productId = _productIds[i];
                            if (result[productId] != undefined) {
                                if ((result[productId]['your_price'] != undefined)
                                    && (result[productId]['your_price'] != "")){
                                    $('product-priceblock-' + productId).innerHTML = result[productId]['price_block'];
                                } else {
                                    // Ink & Toner regular price
                                    if (result[productId]['price'] != undefined
                                        && $('old-price-' + productId) != undefined) {
                                        $('old-price-' + productId).innerHTML = '$' + result[productId]['price'];
                                    }
                                    // Ink & Toner sale price
                                    if (result[productId]['special_price'] != undefined
                                        && (result[productId]['is_office_product'] == false || moduleController == 'catalogsearch|result|index')
                                        && $('product-price-' + productId) != undefined) {
                                        $('product-price-' + productId).innerHTML = '$' + result[productId]['special_price'];
                                    }

                                    // office-product sale price
                                    if (result[productId]['special_price'] != undefined
                                        && result[productId]['is_office_product'] == true
                                        && $('product-minimal-price-' + productId) != undefined) {
                                        $('product-minimal-price-' + productId).innerHTML = '$' + result[productId]['special_price'];
                                    }
                                    // Office product regular price
                                    if (result[productId]['special_price'] != undefined
                                        && result[productId]['is_office_product'] == true
                                        && $('product-price-' + productId) != undefined
                                        && moduleController != 'catalogsearch|result|index') {
                                        $('product-price-' + productId).innerHTML = '$' + result[productId]['special_price'];
                                    }

                                    // Office product sale price
                                    if (result[productId]['tier_price_short'] != undefined
                                        && $('tierprice-short-' + productId) != undefined) {
                                        $('tierprice-short-' + productId).innerHTML = result[productId]['tier_price_short'];
                                    }
                                    // Office product sale price
                                    if (result[productId]['tier_price_full'] != undefined
                                        && $('tierprice-full-' + productId) != undefined) {
                                        $('tierprice-full-' + productId).innerHTML = result[productId]['tier_price_full'];
                                    }
                                }
                                if (result[productId]['name'] != undefined
                                    && $('product-name-' + productId) != undefined) {
                                    $('product-name-' + productId).innerHTML = result[productId]['name'];
                                }

                                if (result[productId]['long_name'] != undefined
                                    && $('product-longname-' + productId) != undefined) {
                                        $('product-longname-' + productId).innerHTML = result[productId]['long_name'];
                                }

                                if (result[productId]['info_line'] != undefined
                                    && $('product-infoline-' + productId) != undefined) {
                                    $('product-infoline-' + productId).innerHTML = result[productId]['info_line'];
                                }

                                /* Office Supplies Grid Add to Cart */
                                if (result[productId]['add_to_cart'] != undefined
                                    && $('addtocart-' + productId) != undefined) {
                                    // If product is currently shown as sellable but is not sellable
                                    if (jQuery('#addtocart-'+productId).find('#qty').length) {
                                        if (!result[productId]['is_sellable']) {
                                            $('addtocart-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    // If product is currently shown as not sellable but is sellable
                                    } else {
                                        if (result[productId]['is_sellable']) {
                                            $('addtocart-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    }
                                }

                                /* Printer Page Add to Cart Block */
                                if (result[productId]['add_to_cart'] != undefined && $('printer-list-add-'+productId) != undefined) {
                                    // if product is currently shown as sellable but is not sellable
                                    if (jQuery('#printer-list-add-'+productId).find('.qty-val').length) {
                                        if (!result[productId]['is_sellable']) {
                                            $('printer-list-add-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    // if product is currently show as not sellable but is sellable
                                    } else {
                                        if (result[productId]['is_sellable']){
                                            $('printer-list-add-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    }
                                }

                                /* Related Products Add to Cart */
                                if (result[productId]['add_to_cart'] != undefined && $('related-'+productId) != undefined) {
                                    // If product is currently shown as sellable but is not sellable
                                    if (jQuery('#related-'+productId).find('#qty').length) {
                                        if (!result[productId]['is_sellable']) {
                                            $('related-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    // If product is currently show as not sellable but is sellable
                                    } else {
                                        if (result[productId]['is_sellable']) {
                                            $('related-'+productId).innerHTML = result[productId]['add_to_cart'];
                                        }
                                    }
                                }

                                if (moduleController == 'catalogsearch|result|index') {
                                    if (result[productId]['is_sellable'] == true
                                        && result[productId]['price_block'] != undefined
                                        && $('product-priceblock-' + productId) != undefined) {
                                        $('product-priceblock-' + productId).innerHTML = result[productId]['price_block'];
                                    }
                                    if (result[productId]['is_sellable'] == false
                                        && result[productId]['add_to_cart'] != undefined
                                        && $('product-priceblock-' + productId) != undefined) {
                                        $('product-priceblock-' + productId).innerHTML = result[productId]['add_to_cart'];
                                    }
                                }
                            }
                        }
                    },
                    onComplete: function(transport){
                    }
                })
            }
        }

        function checkCartQty() {
            _ajaxReport['checkCartQty'] = 0;
            var doNotAllowedModuleController = ['checkout|cart|index','checkout|onepage|index', 'checkout|onepage|success'];
            var uri = 'http://www.ldproducts.com/checkout/cart/qty';
            if (location.protocol == 'https:' && !uri.match("https")) {
                uri = 'https://www.ldproducts.com/checkout/cart/qty/';
            }
            if (typeof moduleController === 'undefined') {
                moduleController = '';
            }
            if ($('item_in_cart') && !in_array(moduleController, doNotAllowedModuleController)) {
                new Ajax.Request(uri,
                    {
                        method: 'get',
                        asynchronous: true,
                        onSuccess: function(transport) {
                            _ajaxReport['checkCartQty'] = 1;
                            var qty = parseInt(transport.responseText);
                            if (!isNaN(qty) && qty == 1) {
                                $('item_in_cart').innerHTML = qty + " item";
                            } else if (qty == 0) {
                                $('item_in_cart').innerHTML = '0 items';
                            } else if (!isNaN(qty)) {
                                $('item_in_cart').innerHTML = qty + " items";
                            }
                        },
                        onComplete: function(transport){
                            detectCustomerStatus();
                        }
                    });
            }else{
                detectCustomerStatus();
            }
        }
    </script>

    
