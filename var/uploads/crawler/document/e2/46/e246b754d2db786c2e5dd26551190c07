<script type="text/javascript">

    jQuery(document).ready(function() {
        var frame = 1;
        var frameWidth = 859;
        var frameCount = 5;
        var indicatorContainerWidth = jQuery(".indicator_container").parent().width()

        function sliderButtonColors()
        {
            if (frame == 1)
            {
                jQuery('.buttons .next').css({'background-color':'#78b5ed', 'cursor':'default'});
                jQuery('.buttons .prev').css({'background-color':'#ccc', 'cursor':'pointer'});
            }
            else if (frame == frameCount)
            {
                jQuery('.buttons .prev').css({'background-color':'#78b5ed', 'cursor':'pointer'});
                jQuery('.buttons .next').css({'background-color':'#ccc', 'cursor':'default'});
            }
            else
            {
                jQuery('.buttons .prev').css({'background-color':'#78b5ed', 'cursor':'pointer'});
                jQuery('.buttons .next').css({'background-color':'#78b5ed', 'cursor':'pointer'});
            }
        }


        jQuery('.buttons .next').on('click', function() {
            if (frame == frameCount) {
                jQuery('.product_slider .indicator').stop().animate({left: 0 + "px"},300);
                jQuery('.product_slider ul').stop().animate({marginLeft: 0},300, function() {
                    frame = 1;
                    sliderButtonColors();
                });
            }
            else {
                jQuery('.product_slider .indicator').stop().animate({left: (indicatorContainerWidth / frameCount * frame) + "px"},300);
                jQuery('.product_slider ul').stop().animate({marginLeft: -Math.abs(frameWidth * frame) + 'px'},300, function() {
                    frame += 1;
                    sliderButtonColors();
                });
            }
        });


        jQuery('.buttons .prev').on('click', function() {
            if (frame == 1) {
                jQuery('.product_slider .indicator').stop().animate({left: (indicatorContainerWidth - (indicatorContainerWidth/frameCount)) + "px"},300);
                frame = frameCount;
                jQuery('.product_slider ul').stop().animate({marginLeft: -Math.abs(frameWidth * (frameCount - 1))},300, function() {
                    sliderButtonColors();
                });
            }
            else {
                frame -= 1;
                jQuery('.product_slider ul').stop().animate({marginLeft: -Math.abs(frameWidth * (frame - 1)) + 'px'},300, function() {
                    sliderButtonColors();
                });
                jQuery('.product_slider .indicator').stop().animate({left: (indicatorContainerWidth / frameCount * (frame - 1)) + "px"},300);
            }
        });
    });
    var setProducts = [];
    jQuery('.slider_container').find('input[type="hidden"]').each(function() { setProducts.push(jQuery(this).val()) });
            new Ajax.Request('ldpcatalog/ajax/retrieveProductData', {
                    method: 'get',
                    asynchronous: true,
                    parameters: {
                        'ids': setProducts.join(),
                        'module': 'catalog|category|view',
                        'product_id': ''
                    },
                    onSuccess: function(transport) {
                        var result = transport.responseText.evalJSON();
                        for (var i = 0; i < setProducts.length; i++) {
                            var setProductId = setProducts[i];
                            if (result[setProductId] != undefined) {
                                var newPrice = result[setProductId]['price'];
                                var newSpecialPrice = result[setProductId]['special_price'];
                                jQuery('#product_'+setProductId)
                                    .find('.old_price strike').html('$' + newPrice).end()
                                    .find('.current_price').html('Now $' + newSpecialPrice).end();
                            }
                        }
                    }
                }
            );
</script>
