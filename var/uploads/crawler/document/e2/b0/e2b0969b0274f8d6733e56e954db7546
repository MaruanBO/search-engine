          <script language="javascript">
              $('.LI_Parent').hover(
                  function (e) {
                      $('#Header').find('.NAV_Sub').css('display', 'none');
                      $(this).find('.NAV_Sub').css('display', 'block');
                  },
                  function (e) {
                      var hideDefault = !$(e.originalTarget).parent('li').hasClass('NAV_Current');
                      if (hideDefault)
                          $(this).find('.NAV_Sub').css('display', 'none');
                  }
              );

              $('#Header').hover(function () {
                  },
                  function () {
                      $('#Header').find('.NAV_Sub').css('display', 'none');
                      $('.NAV_Current').find('.NAV_Sub').css('display', 'block');
                  }
              );
          </script>

