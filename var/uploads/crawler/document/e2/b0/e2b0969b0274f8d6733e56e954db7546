			<script language="javascript">
			$(function() {
				$('.FRM_Newsletter').submit(function() {
					$('.XH_Return').html('').hide();
					$.post('/', {IDX: 'newsletter_signup', action: 'subscribe', email: $('.FRM_Newsletter').find('.NewsEmailInput').val()}, function(data) {
						$('.XH_Return').html(data.message).show();
					},'json');
					return(false);
				});
				$('.NewsEmailInput').focus(function() {
						$(this).val(($(this).val() == 'Enter email address here') ? '' : $(this).val());
				});

				$('.NewsEmailInput').blur(function() {
					$(this).val(($(this).val() == '') ? 'Enter email address here' : $(this).val());
				});

				$('.XH_Return').click(function() {
					$(this).hide();
				});
			});
