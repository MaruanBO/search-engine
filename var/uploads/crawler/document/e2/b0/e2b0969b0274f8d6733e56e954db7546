			<script language="javascript">
			$(function() {
				if (!$('.MAIN_ButtonsOn').find('.BTN_Current').length) {
					$('.MAIN_ButtonsOn').find('a:eq(0)').addClass('BTN_Current');
					$('.MAIN_Show:eq(0)').css('display','block');
				}

				$('.MAIN_Tabs li a').click(function() {
					$('.MAIN_Buttons').find('.BTN_Current').removeClass('BTN_Current');

					var visible = $('.MAIN_Buttons ul:visible');
					if ($(visible).hasClass($(this).attr('rel')))
						return(false);

					$('.MAIN_Tabs li a').removeClass('TAB_Current');
					$(this).addClass('TAB_Current');

					$(visible).slideUp('normal');
					$('.' + $(this).attr('rel')).slideDown('normal');

					if ($(this).attr('rel') == 'MAIN_ButtonsOn') {
						if ($('.MAIN_Slide:visible .ITEM_Media video').length)
							$('.MAIN_Slide:visible .ITEM_Media video').get(0).pause();

						$('.MAIN_Slides').css('display', 'none');
						$('.MAIN_Shows').css('display', 'block');
						$('.BTN_Default:eq(0)').click();
						if ($('.MAIN_Show:visible .SHOW_Video video').length)
							$('.MAIN_Show:visible .SHOW_Video video').get(0).play();
					} else {
						if ($('.MAIN_Show:visible .SHOW_Video video').length)
							$('.MAIN_Show:visible .SHOW_Video video').get(0).pause();

						$('.MAIN_Shows').css('display', 'none');
						$('.MAIN_Slides').css('display', 'block');
						$('.MAIN_ButtonsNew li a:eq(0)').click();
						if ($('.MAIN_Slide:visible .ITEM_Media video').length)
							$('.MAIN_Slide:visible .ITEM_Media video').get(0).play();
					}

					return(false);
				});


				$('.MAIN_ButtonsOn li a').click(function() {
//					if ($(this).hasClass('BTN_Current'))
//							return(false);

					if ($('.MAIN_Show:visible .SHOW_Video video').length)
						$('.MAIN_Show:visible .SHOW_Video video').get(0).pause();

					var nodeID = $(this).attr('href');
					$('.MAIN_ButtonsOn li a').removeClass('BTN_Current');
					$(this).addClass('BTN_Current');
					$('.MAIN_Show:visible').css('display', 'none');
					$('#mainplayerWrapper').css('display', 'none');
					if ($('.MAIN_Show:visible .SHOW_Video video').length) {
						$('.MAIN_Show:visible .SHOW_Video video').get(0).play();
					} else if ($(nodeID).find(':input[name=vFlashvars]').length) {
						$('#mainplayerWrapper').css({'display': 'block'});
						loadJWVideo({
							"target": "mainplayer",
							"file_flv": $(nodeID).find(':input[name=vFlashvars]').val(),
							"file_h264": $(nodeID).find(':input[name=vMP4]').val()
						});
					}
					$(nodeID).css('display', 'block');
					return(false);
				});

				$('.MAIN_ButtonsNew li a').click(function() {
					if ($(this).hasClass('BTN_Current'))
							return(false);

					if ($('.MAIN_Slide:visible .ITEM_Media video').length)
						$('.MAIN_Slide:visible .ITEM_Media video').get(0).pause();

					var nodeID = $(this).attr('href');
					$('.MAIN_ButtonsNew li a').removeClass('BTN_Current');
					$(this).addClass('BTN_Current');
					$('.MAIN_Slide:visible').css('display', 'none');
					$('#mainplayerWrapper').css('display', 'none');
					if ($('.MAIN_Slide:visible .ITEM_Media video').length) {
						$('.MAIN_Slide:visible .ITEM_Media video').get(0).play();
					}/* else if ($(nodeID).find(':input[name=uFlashvars]').length) {
						$('#mainplayerWrapper').css({'display': 'block'});
						flashvars = {config: 'http://r.unicornmedia.com/embed/b5e7a882-5843-4a1c-826f-6cfe313896fa?view=item%26view_id=' + $(nodeID).find(':input[name=uFlashvars]').val()};
						swfobject.embedSWF('http://r.unicornmedia.com/content.aspx?uid=AC26FE85-334B-4A21-B72C-154F743F5739&at=b5e7a882-5843-4a1c-826f-6cfe313896fa', 'mainplayer', '540', '344', '8', '/swf/express_install.swf', flashvars, params);
					}*/ else if ($(nodeID).find(':input[name=vFlashvars]').length) {
						$('#mainplayerWrapper').css({'display': 'block'});
						loadJWVideo({
							"target": "mainplayer",
							"file_flv": $(nodeID).find(':input[name=vFlashvars]').val(),
							"file_h264": $(nodeID).find(':input[name=vMP4]').val(),
							"show_preroll": true
						});
					} else {
						 $('#mainplayerWrapper').html('<div id="mainplayer"></div>');
					}
					$(nodeID).css('display', 'block');
					return(false);
				});
			});
		</script>

