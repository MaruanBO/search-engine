          <script>
              $('input[name=q]').focus(function () {
                  $(this).val(($(this).val() == 'Search DrPhil.com') ? '' : $(this).val());
              });
              $('input[name=q]').blur(function () {
                  $(this).val(($(this).val() == '') ? 'Search DrPhil.com' : $(this).val());
              });
              var submitSearch = function () {
                  var searchString = $('.HDR_Search').find(':input[name=q]').val();
                  if (searchString != '' && searchString != 'Search DrPhil.com') {
                      window.location.href = '/search/results/' + searchString;
                  }
                  return(false);
              };
          </script>

