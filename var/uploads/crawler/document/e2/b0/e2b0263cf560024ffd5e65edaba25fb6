				//console.log(e, data, $parent,$icon,options,validators);

				//console.log(validators.notEmpty, options.feedbackIcons, options.feedbackIcons.required);
            if (validators.notEmpty && options.feedbackIcons && options.feedbackIcons.required) {
                // The field uses notEmpty validator
                // Add required icon
				//console.log('Add required icon to ' + data.field);
                $icon.addClass(options.feedbackIcons.required).show();
            }
        }).bootstrapValidator({
