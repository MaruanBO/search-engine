				// Get the form instance
				var $form = $(e.target);
				//if($form.attr('submitted') == 'true') return false;
				$form.attr('submitted', 'true');
				// Get the BootstrapValidator instance
				var bv = $form.data('bootstrapValidator');
				jQuery('#GSF').slideUp();
				$('#resultgs').hide().html('<div class="warning_class"><i class="fa fa-spinner fa-spin"> </i>&nbsp;Submit in progress...</div>').slideDown();

				$('#submit_btn_gs').hide();
				// Use Ajax to submit form data
				$.post('/Specific/Cappture/_Action/GSAction.asp', $form.serialize(), function(result) {
					// ... Process the result ...
					if(result && result.ReturnCode == 1){
						$('#resultgs').hide().html('<div class="success"><i class="fa fa-check"> </i>&nbsp;Your request has been submitted.You will receive an email with details shortly</div>').slideDown();

					}
					else
					{
