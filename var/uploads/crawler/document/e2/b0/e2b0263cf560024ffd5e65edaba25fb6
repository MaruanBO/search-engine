<script type="text/javascript">// <![CDATA[
// 

function Submitform(){
  var go_EmailRegexp = /^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
  //validate form
  
if(jQuery('#Contact_Address_Email').val().trim() == '' || !go_EmailRegexp.test(jQuery('#Contact_Address_Email').val())){
  alert('Please enter a valid email address.');
  jQuery('#Contact_Address_Email').focus();
  return false;
}
if(jQuery('#Company_Name').val().trim() == ''){
  alert('Please enter your company name.');
  jQuery('#Company_Name').focus();
  return false;
}
    
 if(jQuery('#Contact_Address_Name_Last').val().trim() == ''){
  alert('Please enter your name.');
  jQuery('#Contact_Address_Name_Last').focus();
  return false;
} 
if(jQuery('#Contact_Address_Phone').val().trim() == ''){
  alert('Please your phone.');
  jQuery('#Contact_Address_Phone').focus();
  return false;
}
    

    
	
  
  }
//
// ]]></script>
	<script type="text/javascript">
// <![CDATA[
function hide(plan) {

	if(plan != 0)
	{
		jQuery('#register').show();
		jQuery('#gettingstarted').hide();
		jQuery('#PackSelected').val(plan);
		var aTag = jQuery("a[name='RegisterAnchor']");
		jQuery('html,body').animate({scrollTop: aTag.offset().top},'slow');
	}
	else
	{
		jQuery('#register').hide();
		jQuery('#gettingstarted').show();
		var aTag = jQuery("a[name='GettingStartedAnchor']");
		jQuery('html,body').animate({scrollTop: aTag.offset().top},'slow');
	}
}


jQuery(document.body).ready(function(){
	
try{window.resize()}catch(ee){}
	});
// ]]>


