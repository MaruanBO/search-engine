    $("#contact_form input, #contact_form textarea").keyup(function() { 
        $("#contact_form input, #contact_form textarea").css('border-color',''); 
        $("#result").slideUp();
    });
    

			/* Form validaton */
			/*Register*/
			$('#form-partner-registration').on('init.field.bv', function(e, data) {
            // data.bv      --> The BootstrapValidator instance
            // data.field   --> The field name
            // data.element --> The field element

            var $parent    = data.element.parents('.pdiv'),
                $icon      = $parent.find('.form-control-feedback[data-bv-icon-for="' + data.field + '"]'),
                options    = data.bv.getOptions(),                      // Entire options
                validators = data.bv.getOptions(data.field).validators; // The field validators

            if (validators.notEmpty && options.feedbackIcons && options.feedbackIcons.required) {
                // The field uses notEmpty validator
                // Add required icon
                $icon.addClass(options.feedbackIcons.required).show();
            }
        }).bootstrapValidator({live:'enabled'}) .on('success.form.bv', function(e) {
				// Prevent form submission
				
				console.log(this, arguments);
				e.preventDefault();

				// Get the form instance
				var $form = $(e.target);

				if($form.attr('submitted') == 'true') return false;
				$form.attr('submitted', 'true');
				// Get the BootstrapValidator instance
				var bv = $form.data('bootstrapValidator');
				
				$('#resultpr').hide().html('<div class="warning_class"><i class="fa fa-spinner fa-spin"> </i>&nbsp;Submit in progress...</div>').slideDown();

				$('#submit_btn_pr').hide();

				$('#RPF_Form').slideUp();
				// Use Ajax to submit form data
				$.post('/Specific/Cappture/_Action/PCA.asp', $form.serialize(), function(result) {
					// ... Process the result ...
					//console.log(result);

					if(result && result.ReturnCode == 1){
						$('#resultpr').hide().html('<div class="success"><i class="fa fa-check"> </i>&nbsp;Your partner account has been created.You will receive an email with connection details shortly</div>').slideDown();

					}
					else
					{
						$('#resultpr').hide().html('<div class="error"><i class="fa fa-times"> </i>&nbsp;An error occurred.Please try again later</div>').slideDown();
						$form.attr('submitted', 'false');
						
						$('#submit_btn_pr').show();

						$('#RPF_Form').slideDown();
					}
				}, 'json');
			}).on('status.field.bv', function(e, data) {
            // Remove the required icon when the field updates its status
            var $parent    = data.element.parents('.pdiv'),
                $icon      = $parent.find('.form-control-feedback[data-bv-icon-for="' + data.field + '"]'),
                options    = data.bv.getOptions(),                      // Entire options
                validators = data.bv.getOptions(data.field).validators; // The field validators

            if (validators.notEmpty && options.feedbackIcons && options.feedbackIcons.required) {
                $icon.removeClass(options.feedbackIcons.required).addClass('fa');
            }
        });

			/* Getting Started */
			$('#form-gettingstarted').on('init.field.bv', function(e, data) {
            // data.bv      --> The BootstrapValidator instance
            // data.field   --> The field name
