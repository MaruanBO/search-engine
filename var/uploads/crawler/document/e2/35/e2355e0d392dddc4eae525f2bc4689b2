<~#~>shortcutsList<~#~> <div class="shortcuts_modal">

    <h3 class="colorset_hard">Gallery Shortcuts</h3>

    <div class="shortcut_list">
        <% 
            var shortcutsList = null,
                shortcut = null,
                group = null; 
            for (var groupName in it.shortcuts) { 
                if (it.shortcuts.hasOwnProperty(groupName)) {
                    shortcutsList = it.shortcuts[groupName];
                    group = it.shortcutGroups[groupName];
        %>
                    <div>
                        <%? group.header %>
                            <h4><%= group.header %></h4>
                        <%?%>
                            <%
                                for (var key in shortcutsList) {
                                    if (shortcutsList.hasOwnProperty(key)) {
                                        shortcut = shortcutsList[key];
                            %> 
                                    <dl>
                                        <dt>
                                            <span>
                                            <% if (shortcut.textDisplay) { %>
                                                <%= _.escape(shortcut.textDisplay) %>
                                            <% } else { %>
                                                <%= key %>
                                            <% } %>
                                            </span>
                                        </dt>
                                        <dd><%= shortcut.description %></dd>
                                    </dl>
                            <%
                                    }
                                }
                            %> 
                    </div>
        <% 
                }
            }
        %>
    </div>

