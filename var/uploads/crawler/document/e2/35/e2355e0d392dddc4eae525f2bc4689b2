<script type="text/javascript">
$(document).ready(function() {
  bind_sign_in_button();
  

  $('#create_account_form input[name=username]').keyup(function(e) {
    var curr_val = $(this).val();
    $('#create_account_form .curr_username').text(curr_val);
    $('#create_account_form .username_error').hide();
	});

  $('div.login_dialog_package form#recover_password_form').submit(function(event) {
    recoverpass_user($(this), true);
    event.preventDefault();
    return false;
  });
});
</script>




