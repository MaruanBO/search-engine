function UmbrellaOmniture(sptagName) {
	var requesteddirectTagName=urldecode(sptagName);
    document.getElementById('umbrellaspotTag').innerHTML = '<iframe src="' + requesteddirectTagName + '" width="1" height="1" frameborder="0"></iframe>';       
}

function DelayUmbrellaOmniture(sptagName,targeturl) {
	var requestedTagName=urldecode(sptagName);
    document.getElementById('umbrellaspotTag').innerHTML = '<iframe src="' + requestedTagName + '" width="1" height="1" frameborder="0"></iframe>';	
	var t = setTimeout('UmbrellaOmnitureWithRedirect(\'' + sptagName + '\',\'' + targeturl + '\')', 3000);
}
function urldecode (str) {  
    return decodeURIComponent((str + '').replace(/\+/g, '%20'));
 }
function UmbrellaOmnitureWithRedirect(sptagName,targeturl)
{  
		var requestedRedirecturl=urldecode(targeturl);		
		window.location.href=requestedRedirecturl;              
}
function HomeTabRedirect(targeturl)
{  
		var isHcp= window.location.href;
		var isLantusconnection= window.location.href.toLowerCase();
	    if (isHcp.indexOf("hcp") != -1) {
			targeturl = targeturl+"/hcp";
		}
		if (isLantusconnection.indexOf("/lantus-connection/") != -1)
		{
			targeturl = targeturl+"/lantus-connection/default.aspx";
		}		
		var requestedRedirecturl=urldecode("http://"+targeturl);	
		window.location.href=requestedRedirecturl;              
}

function OmnitureAndHomeTabClick(targeturl) {
	var sptagName= "Home";
	var trackingTag= "//intouchsolutions.112.2o7.net/b/ss/intouchsanofiumbrella/4/redir/%3Fpe%3dlnk_e%26pev1%3d"+document.location.hostname+"%26c25%3dHomeTab%26c26%3d"+document.location.hostname;
	var requestedTrackingTag=urldecode(trackingTag);
	var requestedTagName=urldecode(targeturl);
    //var s=s_gi('intouchsanofiumbrella');s.linkTrackVars='prop6,eVar6,events';s.linkTrackEvents='event6';s.prop6=document.location.hostname;s.eVar6=document.location.hostname;s.events='event6';s.tl(this,'o','Home Tab Clicks');
	document.getElementById('umbrellaspotTag').innerHTML = '<iframe src="' + requestedTrackingTag + '" width="1" height="1" frameborder="0"></iframe>';	
	var t = setTimeout('HomeTabRedirect(\'' + targeturl + '\')', 3000);
}
</script>

