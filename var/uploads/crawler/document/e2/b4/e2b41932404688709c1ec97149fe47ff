<script type="text/javascript">
	
jQuery(function($) {
function parseInstagramData(data) {
        $.each(data, function(index, element) {
        	if (index === 0) {
        		var createdTime = element.created_time;
        		var timeAgo = $.timeago(createdTime);
        		var userName = element.user.username;

            var src = element.images.thumbnail.url;
            if(window.location){
                if(window.location.toString().indexOf('https://') == 0){
                    src = src.replace('http://','https://');
                }
                else{
                    src = src.replace('https://','http://');
                }
            }

            $('.tw-feed').append(
                $('<a>', {
                    href: element.link
            	}).append(
                $('<img>', {
                    'class':'feed-img',
                    src: src
            	})),
            	$('<p>', {
            		'class':  'btm-info',
            		html:timeAgo
            	})
            );

          };
          return false;

        });
}

$.getJSON('//tweetriver.com/PDSDevTeam/p2-instagram.json?callback=?&limit=1',null,parseInstagramData);

	function parseTwitterDate($stamp)
{			
	var date = new Date(Date.parse($stamp));
	// Use the timeago plugin to return date instead of parsing
	return $.timeago(date.getTime());

// get the two digit hour //
	var hour = date.getHours();
// convert to AM or PM //
	var ampm = hour<12 ? ' AM' : ' PM';
	if (hour>12) hour-= 12;
	if (hour==0) hour = 12;
// return the formatted string //
	return hour + ':' + $stamp.substring(8,10) + ampm + ' - ' + (date.getMonth()+1) + '/' + date.getDate() +  '/' + $stamp.substring(28) ;
}


function parseTwitterData(data) {
 $.each(data, function(index, element) {
        	if (index === 0) {
        		var createdTime = parseTwitterDate(element.created_at);
        		var userName = element.user.screen_name;
            $('.center-cnt').append(
            	$('<h6>', {
            		html: linkifyTwitter(element.text)

            	}),
            	$('<p>',{
            		'class': 'btm-info',
            		html: createdTime + '<br />'
            	})
            );

          };
          return false;

        });
}
$.getJSON('//tweetriver.com/PDSDevTeam/p2-twitterfooter.json?callback=?&limit=1',null,parseTwitterData);});

function linkifyTwitter(text) {
    text = text.replace(/(https?:\/\/\S+)/gi, function (s) {
        return '<a href="' + s + '">' + s + '</a>';
    });

    text = text.replace(/(^|)@(\w+)/gi, function (s) {
        return '<a href="http://twitter.com/' + s + '">' + s + '</a>';
    });

    text = text.replace(/(^|)#(\w+)/gi, function (s) {
        return '<a href="http://search.twitter.com/search?q=' + s.replace(/#/,'%23') + '">' + s + '</a>';
     });
    return text;
}



</script>



