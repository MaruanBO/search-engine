				userToken:'',
				doShowSubmission:
				 function() {
						$("#RevContainer").dialog("close");
						$("#BVSubmissionContainer").dialog({width:960,position: { my: "center top", at: "center top", of: $('#primary')}});
						
						var bodyheight = $('#wrapper').css('height');
