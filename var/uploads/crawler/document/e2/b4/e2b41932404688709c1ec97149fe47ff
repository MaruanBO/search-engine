	<script type="text/javascript">
	
	(function($) {
		var accountDropdownClosingTimeout = null;
		var dropdown = $('.account-menu .dropdown');
		if (dropdown.length > 0) {
			// home page account menu drop down
			function startClosingAccountMenu() {
				accountDropdownClosingTimeout = window.setTimeout(function() {
					dropdown.data('original-height',dropdown.height());
					dropdown.stop().animate({height:0},1000,'swing',
						function() {
							$('.account-menu').removeClass('open')
											.find('.dropdown')
											.removeAttr('style');
						});
				}, 10000.0);
			}
			startClosingAccountMenu();
			$(document).on('mouseenter','.account-menu[locked!="true"] .dropdown', function() {
				if (accountDropdownClosingTimeout) {
					dropdown.stop().animate({height:dropdown.data('original-height')},1000,'swing');
					window.clearTimeout(accountDropdownClosingTimeout);
					accountDropdownClosingTimeout = null;
				}
			});
			$(document).on('mouseleave','.account-menu[locked!="true"] .dropdown', function() {
				startClosingAccountMenu();
			});
		}
	})(jQuery);
	</script>
	
	
	



