<script language="javascript">
	try{
		
		SBS.Recs.setRenderer(strands_home_renderer, "HOME-1");
		
		SBS.Worker.go("Kx7AmRMCrW");
	}
	catch (e){ }
	
	function strands_home_renderer(rec_info) {
		//alert(JSON.stringify(rec_info));
		var rec = rec_info.recommendations;
		var lap = 0;
		var content = "";
		lap = 0;
		for(i=0;i<=rec.length-1;i++){
			var itemid = rec[i]["itemId"];
			var itemPic = rec[i]["metadata"]["picture"];
			var price_retail = rec[i]["metadata"]["price"];
			var price = rec[i]["metadata"]["properties"]["saleprice"];
			var itemDesc = rec[i]["metadata"]["name"];
			var productLink = rec[i]["metadata"]["link"];
	//			if (itemDesc.length > 50) itemDesc = itemDesc.substring(0,46) + '...';
			if (!isNaN(price_retail) && !isNaN(price)) {
				if (itemid != "241013") {
					lap = lap + 1;
					content = content + "<div class='fl' align='center' style='width:140px; padding:10px 0px 10px 5px;'>";
					content = content + "	<div><a href='" + productLink + "' title='" + rec[i]["metadata"]["name"] + "' onclick='SBS.Tracking.onRecClick(\"" + itemid + "\",\"" + rec_info.tpl + "\",\"" + rec_info.rrq + "\");'><img src='" + itemPic + "' border='0' width='100' alt='" + rec[i]["metadata"]["name"] + "' /></a></div>";
					content = content + "	<div style='height:45px; line-height:15px; overflow:hidden;' title='" + rec[i]["metadata"]["name"] + "'><a href='" + productLink + "' title='" + rec[i]["metadata"]["name"] + "' style='font-size:12px; color:#000;' onclick='SBS.Tracking.onRecClick(\"" + itemid + "\",\"" + rec_info.tpl + "\",\"" + rec_info.rrq + "\");'>" + itemDesc + "</a></div>";
					content = content + "	<div style='width:100%; padding-top:10px; font-size:11px; font-weight:bold; color:#666; text-decoration:line-through;' align='center'>Retail Price <span style='text-decoration:line-through;'>$" + price_retail + "</span></div>";
					content = content + "	<div style='width:100%; padding-top:10px; font-size:11px; font-weight:bold; color:#000;' align='center'>Wholesale Price: <span style='color:#f00;'>$" + price + "</span></div>";
					content = content + "	<div style='width:100%; padding-top:10px; height:13px;' align='center'>" + getRatingAvgStar(rec[i]["rating"]) + "</div>";
					content = content + "</div>";
					if (lap >= 10) break;
				}
			}
		}
		document.getElementById("featuredProducts").innerHTML = content;
//		$(".strandsRecs").html(content);
	}
	
	function getRatingAvgStar(rating) {
		var stars = "";
		if (rating > 0) {
			for (starLap=1; starLap<=5; starLap++) {
				if (starLap <= rating) {
					stars = stars + "<img width='14' height='13' border='0' src='/images/review/greenStarFull.gif' />"
				}
				else {
					stars = stars + "<img width='14' height='13' border='0' src='/images/review/grayStar.gif' />"
				}
			}
		}
		return stars;
	}
	
