<script>	
	function forcePromoDetails() {
		document.getElementById('promoZone').innerHTML = "0";
		checkPromo('no');
	}
	
	function floatApplyPromo(promoID) {
		savePromo(promoID);
	}
	
	function showPromoDetails() {
		document.getElementById('promoZone').innerHTML = "";
		ajax('/ajax/showPromoPopup.asp?ajaxID=' + Math.floor((Math.random() * 10000) + 1),'promoZone');
		checkPromo('no');
	}
	
	function freeItemPop() {
		document.getElementById('popBox').innerHTML = document.getElementById('freePenPromoPop').innerHTML;
		document.getElementById('popCover').style.display = '';
		document.getElementById('popBox').style.display = '';
	}
	
	function checkPromo(force)
	{
		if (force == "yes") {
			document.getElementById('popBox').innerHTML = document.getElementById('promoDetailBox').innerHTML;
			document.getElementById('popCover').style.display = '';
			document.getElementById('popBox').style.display = '';
		}
		else {
			if ("" == document.getElementById('promoZone').innerHTML) setTimeout('checkPromo()', 50);
			else if ("0" == document.getElementById('promoZone').innerHTML) {
				document.getElementById('popBox').innerHTML = document.getElementById('promoDetailBox').innerHTML;
				document.getElementById('popCover').style.display = '';
				document.getElementById('popBox').style.display = '';
			}
		}
	}

	$(document).ready(function(){
	   	
		ajax('/ajax/newsletter.asp?semail=checkCouponUsed','emailDumpZone');
		checkWidgetExpand('checkCouponUsed');
		showPromoDetails();
		
	});		
	
	function checkWelcome() { 
		dumpZone = "";
		if(document.getElementById('welcomeDumpZone')){
			dumpZone = document.getElementById('welcomeDumpZone').innerHTML;
		}
		
		if (dumpZone.indexOf("html") > 0) {
			ajax('/ajax/newsletter.asp?semail=welcome','welcomeDumpZone');
			setTimeout('checkWelcome()', 50);
		}
		else {
			if ("" == dumpZone) setTimeout('checkWelcome()', 50);
			else if ("" != dumpZone) {
				if (dumpZone == "alert_welcome") {
					document.getElementById("popBox").innerHTML = document.getElementById("welcomeEmail").innerHTML;
					document.getElementById("popBox").style.position = "absolute";
					document.getElementById("popCover").style.display = "";
					document.getElementById("popBox").style.display = "";
				} else {
				
				}
			}
		}
	}
	
	function savePromo(promoID) {
		if (promoID == 1) { ajax("/ajax/savePromo.asp?utm_promoCode=BLKFRI40CO","dumpZone"); }
		if (promoID == 2) { ajax("/ajax/savePromo.asp?utm_promoCode=CM40CO","dumpZone"); }
		if (promoID == 3) { ajax("/ajax/savePromo.asp?utm_promoCode=COXMASB","dumpZone"); }
		if (promoID == 4) { ajax("/ajax/savePromo.asp?utm_promoCode=COYESA","dumpZone"); }
		closeReviewPopup();
		alert("Promo code added!");
		showEmailOfferTab();
	}
</script>
