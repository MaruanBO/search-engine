								<script>
									$(document).ready(function() {
										var delay = 5000;
										var speed = 2000;
										var sel = ".module.image-carousel .image-carousel-container";

										var container = $(sel);
										var imgs = container.find("img");
										var current = 0;
										$(imgs.slice(1)).remove();
										imgs.css("position", "absolute");

										window.setInterval(function() {
											var current_img = $(imgs[current]);
											var new_img = $(imgs[(current + 1) % imgs.length]);
											new_img
												.css("left", container.outerWidth())
												.appendTo(container);
											current_img.animate({"left": 0 - current_img.outerWidth()}, speed, function() { current_img.remove(); });
											new_img.animate({"left": 0}, speed);
											current = (current + 1) % imgs.length;
										}, delay)
									});
								</script>	
	
