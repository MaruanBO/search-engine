<div class="module html parallax ">
			<script>$(document).ready(function(){
								var offset = 0;
								// var offset_increment = -400; //used to be required for chrome? works fine now
								var offset_increment = 0;

								var instances = [
									[
										".image-recipe img",
										[
											[true, "bottom-bottom", "bottom:0px"],
											[true, "top-bottom", "bottom:-600px"],
											[false, "anchor-target", ".image-recipe"],
										]
									],
									[
										".image-ingredients img",
										[
											[true, "bottom-top", "bottom:0px"],
											[true, "top-bottom", "bottom:-600px"],
											[false, "anchor-target", ".image-ingredients"],
										]
									],
									[
										".image-pizza img",
										[
											[true, "bottom-top", "bottom:0px"],
											[true, "top-bottom", "bottom:-600px"],
											[false, "anchor-target", ".image-pizza"],
										]
									],
									[
										".image-tips img",
										[
											[true, "bottom-top", "bottom:-100px"],
											[true, "top-bottom", "bottom:-700px"],
											[false, "anchor-target", ".image-tips"],
										]
									],
									[
										".image-restaurant img",
										[
											[true, "bottom-top", "bottom:0px"],
											[true, "top-bottom", "bottom:-600px"],
											[false, "anchor-target", ".image-restaurant"],
										]
									],
								];

								for (var instance_i = 0; instance_i < instances.length; instance_i++) {
									var selector = instances[instance_i][0];
									var directives = instances[instance_i][1];
									var instance = $(".module.parallax" + selector);
									for (var directive_i = 0; directive_i < directives.length; directive_i++) {
										var apply_offset = directives[directive_i][0];
										var attr_name = directives[directive_i][1];
										var attr_value = directives[directive_i][2];

										if (apply_offset) {
											attr_name = offset + "-" + attr_name;
										}

										if (attr_name === "anchor-target") {
											attr_value = ".module.parallax" + attr_value;
										}

										instance.attr("data-" + attr_name, attr_value);
									}
								}

								window.skrollr.init({
									"smoothScrolling": false,
									"forceHeight": false,
								});
							});</script>	
	
</div>

			</div>
		</div>
	</div>
	<div id="wrapper">
