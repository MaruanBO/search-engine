$(function()
{
	$('.map_nav a').click
	(		
		function()
		{
			if(!$(this).hasClass('active')){
				$('#map_iframe').attr("src", "/solo/detail/local/map/solo?instance=instance0&keyword="+$(this).attr("class"));
				$('.map_nav a.active').attr("class", $('.map_nav a.active').attr("class").split(' ')[0]);
				$('.'+$(this).attr("class")).attr("class", $(this).attr("class") + ' active');
			}
		}
	);
});
</script>
