  		<div class="showtimes_holder"><script type="text/javascript">
	$(document).ready(function() {
		$('#day').change(function() {
			$('#movies').html('<option>Loading...</option>');
			var sel_date = $('#day').val();
			$.ajax({
				type: "POST",
				url : 'wp-content/themes/movieguide/getmovies.php',
				data : 'sel_date='+sel_date,
			
				success: function(res){
					$('#movies').html('');
					$('#movies').html('<option value="">Select movies</option>'+res);
				}
			});
		});
		/*
		$('#movies').change(function(){
			var sel_movie = $('#movies').val();
			var sel_movie_length = $('#movies').val().length;
			if(sel_movie_length > 10) {
				var text_show = sel_movie.substr(0,10) +'...';
				$('.movielist').html(text_show);
			} else {
				$('.movielist').html(sel_movie);
			}
		});
		*/
	});
</script>
