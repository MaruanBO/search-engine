<script type="text/javascript">
	$(document).ready(function() {
		$('.fancybox').fancybox();
		$('.fancybox-media')
			.attr('rel', 'media-gallery')
			.fancybox({
				openEffect : 'none',
				closeEffect : 'none',
				prevEffect : 'none',
				nextEffect : 'none',

				arrows : false,
				helpers : {
					media : {},
					buttons : {}
				}
			});
	});

//script to call tooltip with time interval//
var timer;	
var tt_val1;
var tt_val2;
var tt_val3;
function setintimer(v1,v2,v3) {
	 tt_val1 = v1;
	 tt_val2 = v2;
	 tt_val3 = v3;
	timer = window.setTimeout('call_tooltip()', 2000);		
}	

function setouttimer() {
	window.clearTimeout(timer);
	ajax_hideTooltip();
}

function call_tooltip() {
	ajax_showTooltip(tt_val1,tt_val2,tt_val3);
	return false;
}		
//script to call tooltip with time interval ends here//
	
</script>

