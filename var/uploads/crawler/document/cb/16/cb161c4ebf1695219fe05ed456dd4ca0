<script type="text/javascript">

	jQuery(function ()
	{
        function beforeFlyout(self)
		{
            if(jQuery(self).parents("#siteHeaderFlexWrapper").hasClass("gn-expanded")) { return false; }
        }

		if (Modernizr.touch || Modernizr.ie10touch) {
            jQuery(".globalNavigation > li > a").globalNavigation({ click: true, beforeFlyoutOpen: beforeFlyout });
        }
        else
		{
            jQuery(".globalNavigation > li").globalNavigation({ beforeFlyoutOpen: beforeFlyout });
        }

		jQuery("#siteHeaderSkipToNav").globalNavigation({ target: "#siteHeaderFlexWrapper", click: true });
        jQuery("#searchGlassLink").globalNavigation({ target: "#siteHeaderSearch", click: true });
		jQuery(".SearchLinkButton").html("Go");

		jQuery("div#siteHeaderNavigation").on("click", "a#searchGlassLink", function()
		{
			if($(this).hasClass("gn-on"))
			{
				// Check for default value
				if($("input.search-box").val().toLowerCase() == "help me find...")
				{
				    $("input.search-box").val("");
				}

				//$("input.search-box").focus();
			}
			else
			{
				$("input.search-box").blur();

				// Check for default value
				if($("input.search-box").val().length == 0)
				{
					$("input.search-box").val("Help me find...");
				}
			}
		});
    });

</script>
