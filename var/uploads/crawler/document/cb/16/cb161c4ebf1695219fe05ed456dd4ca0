<!-- Custom Code added the following if (messageEvent.data.substring(0, 1) === "{") { below to fix an issue with REG -->
<script type="text/javascript" language="javascript">
    if (window.addEventListener) {
        window.addEventListener('message', function (messageEvent) {
            if (messageEvent.origin === 'http://' + location.host || messageEvent.origin === 'https://' + location.host) {
                if (messageEvent.data.substring(0, 1) === "{") {
                    var object = JSON.parse(messageEvent.data);
                    if (object.requestType === "registration") {
                        window.top.reg.userRegistration.invokeModalResize(object.data);
                    }
                }
            }
        }, false);
    } else if (window.attachEvent) {
        window.attachEvent('onmessage', function (messageEvent) {
            if (messageEvent.origin === 'http://' + location.host || messageEvent.origin === 'https://' + location.host) {
                if (messageEvent.data.substring(0, 1) === "{") {
                    var object = JSON.parse(messageEvent.data);
                    if (object.requestType === "registration") {
                        window.top.reg.userRegistration.invokeModalResize(object.data);
                    }
                }
            }
        }, false);
    }
</script>




