<script type="text/javascript">
var messageInput = function(obj, message){
	if(!obj.data('error')) { obj.data('error', $('<span class="error message">'+message+'</span>')); }
	if (message) {
	  obj.addClass('error').before(obj.data('error').text(message));
	  if(obj.is('input')){
		obj.keyup(function(){ additionalValidation(["#"+obj.attr('id')]); });    
	  } else {
		obj.find('input').change(function(){ additionalValidation(["#"+obj.attr('id')]); });
	  }
	} else {
	  obj.removeClass('error').unbind('keyup').data('error').remove();
	}
};
var additionalValidation = function(search) {
	var valid = true;
	for (var i = search.length - 1; i >= 0; i--) {
	  var el = $(search[i]);
	  // ZIP Validation
	  if(/zip/i.test(el.attr('id'))) {
		if(!/^$/.test(el.val()) && !/(^\d{5}(-\d{4d})?$)|(^[ABCEGHJKLMNPRSTVXY]{1}\d{1}[A-Z]{1} *\d{1}[A-Z]{1}\d{1}$)/.test(el.val())) {
		  valid = false;
		  messageInput(el, 'invalid post code');
		} else {
		  messageInput(el, '');
		}
	  } else {
		// Checkbox validation
		if(/check/i.test(el.attr('id')) && parseInt($.cookie('FFCA-EMAIL'))) {
			var isChecked = (el.is('input')) ? el.is(':checked') : el.find('input:checked').length;
			if(!isChecked) {
				valid = false;
				messageInput(el, 'please confirm');
			} else {
				messageInput(el, '');
			}
		}
	  }
	}
	return valid;
}
var canadaOptIn = function(){
	$('.optInCheck').parent().addClass('canada').find('input:checked').attr('checked', '');
};
caInit.push(function(){
	$.cookie('FFCA-EMAIL', 1);
	canadaOptIn();
});
if(parseInt($.cookie("FFCA-EMAIL"))) {
	canadaOptIn();
}
