<script type="text/javascript">
/* SEARCH SUPPORT */
$('form[name=endeca1]').submit(function() {
	var searchEl = $(this).find('input[name=Ntt]');
	if (searchEl.val() == '' || searchEl.val() == searchEl.attr('placeholder')) { return false; }
	else {
		return true; 
	}
});


// Hides price info until the DOM has finished loading (or one second has passed 
if(/nike/i.test(document.location)){
	var detailsPriceHider = function(hide){
		var visibility = (hide) ? 'hidden' : 'visible';
		if(hide){
			document.write('<style>.productdetailsPage .style-price {visibility: hidden}</style>')
		} else {
			$('.productdetailsPage .style-price').css('visibility', visibility);
		}					
	};

	// hide price
	detailsPriceHider(true);

	// set timeout to show price if dom fails
	var removeHideStyle = setTimeout(function(){
		detailsPriceHider(false);
	}, 2000);	

	// show price when the dom loads and clear timeout
	$(function(){
		clearTimeout(window.removeHideStyle);
		detailsPriceHider(false);
	});
}
// end price hider
</script>

<script type="text/javascript">
jQuery.support.placeholder = (function(){
    var i = document.createElement('input');
    return 'placeholder' in i;
})();
if (!jQuery.support.placeholder) {
    $('#topSearch').val($('#topSearch').attr('placeholder')).addClass('placeholding');
    $('#topSearch').focus(function () {
        if ($(this).val() == $(this).attr('placeholder')) {
            $(this).val('').removeClass('placeholding');
        }
    }).blur(function () {
        if ($(this).val() == '') {
            $(this).val($(this).attr('placeholder')).addClass('placeholding');
        }
    });
}


/* AUTO SET WIDTH FOR INPUT-SELECT */
(function(){
	var selEl = $('#TopMargin .input-select select');
	var setSelWidth = function() {
		var txt = jQuery.trim(selEl.find('option:selected').text());
		var m = 15;
		var wm = $('#TopMargin .input-select .widthMaker');
		wm.text( txt );
		$('#TopMargin .input-select').width( wm.width() );
		selEl.blur();
	};
	selEl.change(function(){ setSelWidth(); });
	setSelWidth();
})();

/* CART COUNT */
if (parseInt($.cookie('CART_TOTAL_QUANTITY_20000')) > 0) {
	$('.cart-link').addClass('not-empty').find('.cart-count').addClass('not-empty').text( $.cookie('CART_TOTAL_QUANTITY_20000') );
}

/* SCRIPTS SUPPORTING CHOOSE-YOUR-STORE */
if ($.cookie('StoreInfo')) {
	var storeInfo = jQuery.parseJSON($.cookie('StoreInfo'));
	$('.choose-your-store').addClass('selected').find('.store-name .store').text( storeInfo.AddrLine1 );
}

function isValidStoreZip()
{
	var zipRegEx = /^\d{5}$/;
	var valid = zipRegEx.test($('#storeSelectorZipCode').val());
	$('#zipErrorMessage').toggle(!valid);
	return valid;
}

$('.choose-your-store > a, .choose-your-store .gray > a').click(function(){
	$(this).closest('li').toggleClass('active');
	return false;
});

$('#showStoreSelectorLink').click(function(event){	
	if(isValidStoreZip()) {		
		openStoreSelector();		
		$('#storeSelectorZipCodeContainer').hide();
	}
	return false;
});

$('#storeSelectorZipCode').bind('keypress', function(e){
	var code = (e.keyCode ? e.keyCode : e.which);
	 if(code == 13) { if(isValidStoreZip()) { openStoreSelector(); } }
});

$('#storeSelectorZipCode').focus(function() {
	var thisVal = $(this).val();
	if (thisVal == 'Enter Zip Code') { $(this).val(''); }
});

function openStoreSelector()
{
	$('#storeSelectorZipCode').blur();
	if (DetectIpad())
	{
		document.cookie = 'FindAStorePrevPage=' + escape(location.href.substring(location.href.indexOf(location.pathname))) + '; path=/; domain=' + siteDomain;
		window.location.href = '/Community/FindAStore2.aspx?location=' + $('#storeSelectorZipCode').val();
	}
	else
	{
		var tb_pathToImage = "/Images/loadingAnimation.gif";
		imgLoader = new Image();// preload image
		imgLoader.src = tb_pathToImage;	
		var url = '/Community/FindAStore.aspx?location=' + $('#storeSelectorZipCode').val() + '&page=shipping&TB_iframe=true&modal=true&height=600&width=500&';		
		tb_show('Pick Store', url, false);
	$('#TB_window').addClass('shopMyStoreIFrameWrapper');
	}
}
