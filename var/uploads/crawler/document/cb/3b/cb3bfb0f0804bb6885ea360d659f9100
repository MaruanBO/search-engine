            <h3>Police Fire</h3>
            <div id="topitem" class="newsitem nopic">
                
                    
                        <div id="photocontainer" style="background:#000000;background-image:url(http://assets.mediaspanonline.com/prod/11525101/5128030_h340.jpg);background-repeat:no-repeat;background-position:center;">
                        </div>
                        <script>
                           var topimage = "http://assets.mediaspanonline.com/prod/11525101/5128030_h340.jpg";
                        </script>
                    
                
                <div id="photodata">
                    <h2 id="topheadline" class="textheadline"><a id="toplink" href="/news/local/police-fire/Syracuse-double-shooting-suspect-in-custody-5127725">Syracuse double-shooting suspect in custody</a> </h2>
                    <div class="summaryp">
                        <p id="topgraph">A man wanted in an early-morning double-shooting in Syracuse is now in custody.</p>
                    </div>
                </div>
                <div class="clearfloats"></div>
            </div>
        </div>
<script>
    if (topimage != null){
	var actimage = new Image();
	actimage.src = topimage;
	actimage.onload = function(){
		width = this.width;
		height = this.height;
		if (width > height){
			$("#topitem").removeClass("nopic");
			$("#topheadline").removeClass("textheadline");
			$("#topheadline").addClass("white");
			$("#toplink").addClass("white");
			$("#photocontainer").addClass("picL");
			$("#photodata").addClass("haspicL");
                        $("#topgraph").addClass("white");
                        $("#topgraph").addClass("indent");
		}
		else{
			$("#topitem").removeClass("nopic");
			$("#topheadline").removeClass("textheadline");
			$("#topheadline").addClass("Pwhite");
			$("#toplink").addClass("Pwhite");
			$("#photocontainer").addClass("picP");
			$("#photodata").addClass("haspicP");
			$("#topitem").addClass("vpic");
		}
	}
    }
</script>
    

</div>
