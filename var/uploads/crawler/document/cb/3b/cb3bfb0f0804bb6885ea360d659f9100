             if (thumbimage != ""){
                 var thimage = new Image();
                 thimage.src = thumbimage;
                 thimage.onload = function(){
                     width = this.width;
                     height = this.height;
                     $("#picdiv2").addClass("thumb");
                     $("#pichead2").removeClass("nothumb");
                     $("#picgraph2").addClass("hasthumb");
                     if (height > width){
                         $("#picgraph2").addClass("verticalthumb");
                     }
                 }
             }
