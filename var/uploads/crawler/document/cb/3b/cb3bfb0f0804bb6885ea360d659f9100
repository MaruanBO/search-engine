<script>
var deferredArr = $.map($(".dynvim"), function(el, i) {
	return $.getJSON($(el).data("loadUrl"), null,function (data){
			$.each( data, function( key, val ) {
				$(el).attr("src",val.thumbnail_medium);
                                var elemID = "#vimhead" + $(el).attr("id").substr(8);
				$(elemID).html(val.title);
			});
		});
});
$.when.apply(this, deferredArr).then(function() {
    $(".carousel").jCarouselLite({
        btnNext: ".next",
        btnPrev: ".prev",
        visible: 5
    });
});
</script>
