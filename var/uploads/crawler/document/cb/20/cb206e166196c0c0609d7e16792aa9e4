//hack for directory thumb display
if (lteIE8 || null !== navigator.appVersion.match(/\(iPad.+?Mac OS .*/) || jQuery(window).height()<600 || jQuery(window).width()<1000 || directoryThumbHover==false) {
	console.log("disabling hack for directory thumb display")
} else {
jQuery(function(){
	if (jQuery('.directoryThumbWrapper').size()==0) return;
	console.log("hack for directory thumb display")
	// restyle QVE button
	jQuery('.pqve_btn_thumb, .pqve_btn_thumb_off').css({
		zIndex: 1000,
		bottom: 0,
		height: 18,
		margin: 0,
		width: "100%",
		padding: "6px 0px 0px 0px",
		lineHeight: "inherit"
	});

	/* add dropshadow on hover */
	jQuery('.directoryThumbWrapper').hover(
		function(){ jQuery(this).addClass("thumbnailshadow"); },
		function(){ jQuery(this).removeClass("thumbnailshadow"); }
	)

	// add extra height for relocated quickshop button
	jQuery('.thumbInfo').css({height:20+jQuery('.thumbInfo:eq(0)').height()});

	/* show alt on hover */
		jQuery('div.directoryThumbWrapper').each(
			function(){
				var im = new Image();
				var me = jQuery('.thumbdiv a img', this);
				if (me.size()==0) return;
				var qs = jQuery('.pqve_btn_thumb, .pqve_btn_thumb_off', this);
				jQuery(im).bind('load', function(){
					me.attr('data-src', me.attr('src'));
					me.attr('data-alt', im.src);
				})
				var src = me.attr('src');
				im.src=src.replace(/_v1/,'_v2');

				jQuery('.qveThumbnail').unbind("mouseleave mouseenter"); // turn off the mouseover events on qveThumb

				jQuery(this) // swap out image if alt1 available and attache QVE button
				.hover(
					function(){
						if ( me.attr('data-alt')!="" ) me.attr('src', me.attr('data-alt'));
						var obj = jQuery('.qveThumbnail', this);
						obj.qve("attachButton", obj, obj.attr("dialogTitle"), obj.attr("catPK"), obj.attr("buttonOn"), obj.attr("buttonOff"), obj.attr("url"), obj.attr("windowWidth"), obj.attr("windowHeight"),(obj.attr("showInCenter")!=null));
					},
					function(){
						if ( me.attr('data-src')!="" ) me.attr('src', me.attr('data-src'));
						var obj = jQuery('.qveThumbnail', this);
						obj.qve("detachButton", obj);
					}
				);
			}
		)
	})
} // end hack for directory thumb display

jQuery(function(){
	console.log("remove quickshop hovers from family thumbnails")
	jQuery('div.directoryThumbWrapper:regex(catpk,^family:.*)').unbind('mouseover');

	console.log("remove quickshop hovers from KIT thumbnails")
	jQuery('div.directoryThumbWrapper:has("img:regex(src,/KIT[0-9]+)")').unbind('mouseover');

	console.log("remove quickshop hovers from CRAFT thumbnails")
	jQuery('div.directoryThumbWrapper:has("div:regex(url,/craft/)")').unbind('mouseover');

})


