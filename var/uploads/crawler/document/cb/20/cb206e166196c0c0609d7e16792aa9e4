<script type="text/javascript">

var facetToDropdowns = function (order) {
// curtains add horiz facet dropdowns
	var facets_to_duplicate = ["price","colorFamily","sizeFamily"];

	if (order == "colorLast") facets_to_duplicate = ["price","sizeFamily","colorFamily"];

	var allFacets = [];
	jQuery('[data-facet]').each(function(){
		allFacets.push(jQuery(this).attr('data-facet'))
	})

	var facets_data = {}
	jQuery('li:regex(class, filterGroup_('+facets_to_duplicate.join("|")+'))').each(function(){
		var facetcontainer = jQuery('[data-facet]',this);
		var facets = [];

		jQuery('.facetValue', facetcontainer).each(function(){
			facets.push(jQuery(this).attr('data-facet-value').replace(/\+/g," "));
		})
		facets_data[facetcontainer.attr('data-facet')] = {
			facets: facets,
			header: jQuery('.header', this).text().replace(/Reset x/,"")
			}
	})

	var facets_bar = jQuery('<div id="cpwm_facetbar_top" style="height: 20px; padding: 10px;background:url(/images/misc/gradient.jpg) repeat-x;"></div>');

	while (facet = facets_to_duplicate.shift()) {

		if (!facets_data[facet]) continue;

		var currSelection = cpwm_page.getQueryValue(facet) || false;

		var dropdown = jQuery('<select id="'+facet+'_top" style="float: left; margin-right: 20px; width: 140px"></select>');
		if (!currSelection) {
			dropdown.append('<option value="">'+facets_data[facet]["header"]+'</option>');
		} else {
			dropdown.append('<option value="">See all '+facets_data[facet]["header"]+'s</option>');
			currSelection = currSelection.replace(/\+/g," ");
		}

		for (var i=0; i<facets_data[facet]["facets"].length; i++) {
			var f = unescape(facets_data[facet]["facets"][i]);
			if (unescape(currSelection)==f) {
				// this value is currently selected
				dropdown.append('<option value="'+f.replace(/\+/g," ")+'"'+((unescape(currSelection)==f)?" selected":"")+'>'+facets_data[facet]["header"]+": "+f.replace(/\+/g," ")+'</option>');
			} else {
				dropdown.append('<option>'+f.replace(/\+/g," ")+'</option>');
			}
		}

		var dropdown_container = jQuery('<div id="'+facet+'_container_top"></div>').appendTo(facets_bar);

		dropdown_container.append(dropdown)

		dropdown.change(function(){
	//this is default
	//otherwise use curr URL
			var facetName = jQuery(this).attr('id').replace(/_top$/,"");
			var newargs = [];
			if ( location.search != '' ) {
				var args = location.search.replace(/\?/,'').split('&');
				for (var i=0; i<args.length;i++) {
					// only look for facets on page
					var pattern = '^('+allFacets.join("|")+')=';
	//				console.log("does "+args[i]+' match '+pattern)

	//				console.log("valid facet: "+(args[i].match(pattern)))
	//				console.log(args[i].split("=")[0]+' != '+facetName)
					if ( args[i].match(pattern) && args[i].split("=")[0] != facetName) {
						// if the curr URL has this facet in it, we override it
						newargs.push(args[i]);
					}
				}
			}
			newargs.push( facetName+'='+jQuery(this).val() );

			var newURL = MarketLive.FacetedNav.sNAV_ACTION +
					 newargs.join("&") +
					 MarketLive.FacetedNav.sNAV_ACTION_SUFFIX;
	//console.log(newURL)
			location.href = newURL;
		})
	}
	facets_bar.prepend('<div class="formlabel" id="formlabelfilterby" style="float: left; text-transform: uppercase;padding: 6px 15px 0 0;">Filter By</div>')
	facets_bar.appendTo(jQuery('.pgTablem')).hide().fadeIn()
	// END curtains add horiz facet dropdowns
}


/* Misha - code to display a confirmation message on URLs with code= in query string that match list of specific codes */

jQuery(function(){
	var promoCodes = ['EBAY10100', 'EBAYFS100', 'MB10', 'WM150', 'WM510', 'SHIP150', 'WMSAVE104', 'WMSAVE102', 'WM00S1Y20L1958', 'WORLD10', 'SAVEBIG10', 'SHIP175', 'SHIPFREE150', 'WMFS1502', 'WME100', 'PVTSALE', 'BIGWIG', 'PRESDAY10', '10PRESDAY', 'WM4750', 'BIG50', '150A4', 'SAVE4930', 'SAVE4931', '10FEB30', 'WMFS1052', 'BIGWIG', 'MB10100', 'MB10100', 'SAVE1030', 'LUCKY', 'SAVEMORE', 'JACKPOT', 'SUNSHINE', '10OFFUSIA', '10OFFUSIB', 'BLOOM', 'FNFSAVE', 'FNFWME', 'HUGS', 'MOBILE10', 'HONOR', 'MAYPASS', 'WMSAVE15', 'WMSHIP100', 'JULY4TH', 'LOOKLINGERLOVE', 'HOUSEOFHARPER', 'HWTH10', 'GROUPON', 'LIVING', 'SAVE1411', 'SPLURGE', 'SAVE1741', 'NICEDEAL', 'SAVEALOT', 'SHOPATHOME', 'WMLOCAL50', 'THEHSSFEED', 'HONEY', 'SAVE2613', 'SAVE2615', 'SAVE2616', 'SAVE2617', 'SAVINGS', 'LUCKYBUY', 'LUCKYDEAL', 'SOLUCKY', 'SHIPITFREE', 'FAFWEB', 'FAFSAVE', 'FAFSHIP', 'WOWFS100D', 'WM100', 'FURNTIME', 'WM3425', 'FURNSHIP', 'DOUBLEWOW', 'WMEHOLIDAY', 'YULELOVEIT', 'WMEPASS', 'WMEPERKS', 'CHOCOBOGO', 'SUPERCYBER', 'FRUITCAKE', 'JOLLYWME', 'READYSETNEW', 'MAKEITNEW', 'MYSTERYSOLVED', 'MYSTERYUNLOCKED', 'SECRETSOLVED', 'SECRETUNLOCKED', 'MEMBERPASS', 'PEACELOVE', 'SCOREMORE', 'BIGFIFTY', 'SOMUCHNEW', 'WM10010', 'FBWOW', 'WM150FS', 'WM100FS', 'WM149FS', 'WM99FS', 'THISISHUGE', 'SUNNYDAYS'];
       
	var c = cpwm_page.getQueryValue("code");

	if ( !c || typeof(c)==="object") return;
	if ( promoCodes.indexOf(c.toUpperCase())==-1 ) return;
	
	if (c.toUpperCase() == 'WMFS1502') {
		var promoMessage = 'Free Shipping on orders $150+ has been added to your cart.';
	}
	else {
		var promoMessage = 'This promotion code has been applied to your cart.'
	}
	jQuery('.wrapper').prepend('<div class="promoCodesWM">'+promoMessage+'<div class="promoCodesWMClose">&times;</div></div>');
	jQuery('.promoCodesWM').delay(1100).animate({'top':'0px'});
	jQuery('.promoCodesWMClose').click(function(){
		jQuery('.promoCodesWM').fadeOut('normal', function(){jQuery('.promoCodesWM').remove()})
	});
});

