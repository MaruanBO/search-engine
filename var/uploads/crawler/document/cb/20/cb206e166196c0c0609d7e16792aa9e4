<script type="text/javascript">
jQuery(document).ready(function () {
	// Change wk number ab codes
	var newWkNumber = 'wk50';
	jQuery('#mostPinned-list li a').each(function(){
    		var currentHref = jQuery(this).attr('href');
    		jQuery(this).attr('href', currentHref.replace('wkNumber',newWkNumber));
	});

	jQuery('.topTrandingProducts').each(function(){
		var currentHref = jQuery(this).attr('href');
		jQuery(this).attr('href', currentHref.replace('wkNumber',newWkNumber));
	});
	// function move slides

	var moveSlide = function(indexSlide){
		var stepMargin = jQuery('#homeSlides li').height();
		jQuery('#homeSlides ul').animate({"margin-top":+-stepMargin*indexSlide+"px"},{duration:300, queue:false});
		CE_SNAPSHOT_NAME = jQuery('#homeSlides li:eq('+indexSlide+')').attr('CEname');
	};

	// Update Active Nav and show slide
	// jQuery('#homeNav ul li:first').addClass('active');
	jQuery('#homeNav ul li').hover(function(){
		if (jQuery(this).hasClass('active')) {}
		else {
			jQuery('#homeNav ul li').removeClass('active');
			jQuery(this).addClass('active');
			var indexSlide = jQuery(this).index();
			moveSlide(indexSlide);
			autoR = false;
		}
	});
	// AutoRotation
	var slidesNumber = jQuery('#homeSlides li').size();
	var indexSlide = 1;
	var autoR = true;
	window.autoRotation_cpwmWk14test = function() {
		setTimeout(function() {
			if (indexSlide < slidesNumber && autoR == true){
				moveSlide(indexSlide);
				indexSlide++;
				jQuery('#homeNav li').removeClass('active');
				jQuery('#homeNav ul li:eq('+(indexSlide-1)+')').addClass('active');
				autoRotation_cpwmWk14test();
				}
			else if (autoR == false) {}
			else {
				moveSlide(0);
				jQuery('#homeNav li').removeClass('active');
				jQuery('#homeNav ul li:first').addClass('active');
				}
		}, 3000)
	}
	// autoRotation_cpwmWk14test();

	jQuery('#shop-category-slider').swSliderVmLight({
			'sliderImages': '#shop-category-list',
                        'slidePrev': '#shop-category-left',
                        'slideNext': '#shop-category-right',
                        'sliderEffect': 'easeInOutExpo',
			'sliderStep' : '5'
		});
	jQuery('#hot-sale-slider').swSliderVmLight({
			'sliderImages': '#hot-sale-list',
                        'slidePrev': '#hot-sale-left',
                        'slideNext': '#hot-sale-right',
                        'sliderEffect': 'easeInOutExpo',
			'sliderStep' : '5'
		});
});
</script>
