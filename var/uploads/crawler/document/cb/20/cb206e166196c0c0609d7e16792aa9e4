	jQuery(".promoClickSlot").click(function(){promoClickSlot24();})
};
window.promo15off = function(promo15off){
	var imagePath = "/text/content-slot-html/articles/2013/wk23/wk23_leftnav_test/";
	jQuery('<div style="background:url(/images/worldmarket/en_us/global/globalgraphics/fabric.png) repeat; padding-bottom:10px"><img src="'+imagePath+'wk25_promo_gatewayEndsToday.png" width="223" height="125" alt="15% Off" class="hoverpopup" style="cursor:pointer"/></div>').prependTo('td.navleftbg');

	jQuery('.hoverpopup').click(function(){
			jQuery('#overlayPopup').show();
			var windowHeight = jQuery(window).height();
			var popupHeight = jQuery('.megaPopup').height();
			var marginT = parseInt(windowHeight/2)-parseInt(popupHeight/2);
			jQuery('.megaPopup').css('marginTop',marginT);
			return false;
		});
		jQuery('.megaPopup').click(function(){
			jQuery('#overlayPopup').hide();
			return false;
		});
		jQuery(window).resize(function(){
			window.popupPlace();
		});

}
});

