		$(window).load(function() {
			$('#slideshow').flexslider({
				"pauseOnHover":true,
				"animationSpeed":850,
				"slideshowSpeed":6000,
				"animation":"slide",
				"directionNav":false
			});
		});
		$(document).ready(function() {
			$("#slideshow").on("click","ol li",function() {
				if($(this).attr('class') == "next") $("#slideshow").flexslider("next");
				else if($(this).attr('class') == "previous") $("#slideshow").flexslider("prev");
				else {
					var slidenum = parseInt($(this).text()-1);
					$("#slideshow").flexslider(slidenum);
				}
			});
/*
