		function loadVideo(ytID) {
			$.fn.ceebox.popup('<a href="http://www.youtube.com/v/'+ytID+'?fs=1&hl=en_US&rel=0&autoplay=1">Video</a>',{videoWidth:640,videoHeight:480,titles:false});
			return false;
		}
