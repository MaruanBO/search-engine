				(function($) {
					$('#mychive-button').on('click',function(e) {
						e.preventDefault();
						var popupOptions = "location=yes,height=530,width=440";
						var loginUrl = "https://my.theberry.com/login?source=thechive";
						var popup = window.open(loginUrl, "", popupOptions);
						var intervalId = setInterval(function() {
							if (popup.closed) {
								clearInterval(intervalId);
								this.location.reload();
							}
						}, 500);
						return false;
					});
