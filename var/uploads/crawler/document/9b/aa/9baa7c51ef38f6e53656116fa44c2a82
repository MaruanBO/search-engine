			var contentad_c1=cadElement.innerHTML;
		}
		cadElement.innerHTML = contentad_c1;
	}
}
function refreshAds() {
	adsc_refresh_count++;
	if (adsc_refresh_count % adsc_refresh_freq == 0) ads_get_cad();
}
