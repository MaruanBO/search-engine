<script type="text/javascript">
    var startTime = new Date();

    var checkLogin = function(){
        if(CN.login){ 
            CN.login.init();
        }
        else if( new Date()- startTime < 30000){
            setTimeout(checkLogin, 100);
        }
    }

    var pageAdsCall = function() {
        var promotheus = function(key) {
            var pageAds;
            if (typeof window.pageAds !== 'undefined') {
                pageAds = window.pageAds;
                if ((typeof pageAds[key] !== 'undefined') && pageAds[key].indexOf('No creatives found') === -1 && (CN.url.params('nojoy') !== 1)) {
                    jQuery("#" + key).html("");
                    jQuery("#" + key).html(pageAds[key].replace(/document.write\(.*\)/gi, "/*filtered by amg-magnet:document.write(...)*/"));
                }
            }
            jQuery("#" + key).css("visibility", "visible");
        };
        promotheus("AMS_ARD_GLOBAL_HEADERCALLOUT");
        promotheus("AMS_ARD_GLOBAL_HEADER");
        promotheus("AMS_ARD_GLOBAL_NAVBAR");

    }

    checkLogin();
    CN.labjsEcom.wait(pageAdsCall);
</script></h2></div>
