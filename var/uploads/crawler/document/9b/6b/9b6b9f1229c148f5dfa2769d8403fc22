                <div class="cn_parsys parsys cn_features_container cn_componentContainer"><div class="title cn_title"><h2 class="headline ">
    MOST POPULAR</h2>

</div>
<div class="parbase cn_subheader"><h3 class="subheader "><div id="parsely_most_popular_body"></div>
<script type="text/javascript">
(function() {

  function date_string(rawDate, isStartDate){
    var date = rawDate.getDate();
    if (JSON.stringify(date).length ===1){
      date = '0'+date;
    }

    var month =isStartDate?rawDate.getMonth():rawDate.getMonth()+1;
    if (JSON.stringify(month)==='0'){
      month='12';
      rawDate.setFullYear(rawDate.getFullYear()-1);
    }

    if (JSON.stringify(month).length ===1){
      month = '0'+month;
    }

    return JSON.stringify(rawDate.getFullYear())+'-'+month+'-'+date;
  }

  function array_contains(arr, elem) {
    for (var i in arr){
      if (arr[i] == elem) return true;
    }
    return false;
  };

  var start_date = date_string(new Date(), true);
  var end_date = date_string(new Date(), false);

  var request_url = 'http://api.parsely.com/v2/analytics/posts?apikey=architecturaldigest.com&secret=XtQS15Iqm80BOOQz3KhVs9ehvOBKYdcJlKMFXqxBTiU&page=1&limit=15&days=3&sort=_hits&pub_date_start='+start_date+'&pub_date_end='+end_date;

  jQuery.ajax({
    type:'GET',
    url: request_url,
    jsonp: "callback",
    dataType: 'jsonp',
    success: function( resp ) {
      var items  = [];
      var uniques =[];
      var count=0;

      jQuery.each(resp.data, function(index, item) {
        if (count<10 && !array_contains(uniques, item.title)){
          var clean_title=item.title;
          uniques.push(clean_title);
          if (clean_title.indexOf(":")>=0){
            clean_title = clean_title.substring((clean_title.indexOf(":") + 1), (clean_title.length+1));
          }

          var clean_url=item.url;
          if (clean_url.indexOf('content/ad')>1) {
            clean_url = clean_url.replace(/^http:\/\/.+\/content\/ad\//g,'http://www.architecturaldigest.com/').replace(/.html/g,'');
          }

            items.push('<li id="item_' + index  + '"><a href="' + clean_url+'?intcid=most_popular">'+ clean_title + '</a></li>');
            count++;
        }
      });

      jQuery( "<ol/>", {
        "class": "my-new-list",
        html: items.join( "" )
      }).appendTo( "#parsely_most_popular_body" );

    },
    error : function(xhr, textStatus) {
      if (typeof console === 'undefined') {
        return;
      }
      console.log('error occurred in ajax call');
    }
  });

})();
</script>
</h3></div>
