    var _ghearst_vars = (function() {
        var metaKeyWords = '', metaCollection;
        metaCollection = document.getElementsByTagName('meta');
        for (i = 0; i < metaCollection.length; i++) {
            nameAttribute = metaCollection[i].name.search(/keywords/);
            if (nameAttribute != -1) {
                metaKeyWords = metaCollection[i].content;
                metaKeyWords = metaKeyWords.replace(/,/g, '%2C'); // to replace ,
                metaKeyWords = metaKeyWords.replace(/ /g, ''); // to replace space
            }
        }
        if ( (navigator.userAgent.match(/iPad/i) !== null) && (CN.cookie.get('ipadTakeover') !== 'true') && 'AMS_ARD_GLOBAL_IPAD_TAKEOVER' !== 'null') {
            positionList = 'AMS_ARD_GLOBAL_HEADER%2CAMS_ARD_GLOBAL_HEADERCALLOUT%2CAMS_ARD_GLOBAL_NAVBAR%2CAMS_ARD_GLOBAL_NAVBAR_COLLAPSE%2CAMS_ARD_GLOBAL_NAVBAR_ROLLOVER%2CAMS_ARD_GLOBAL_RIGHTRAIL_A%2CAMS_ARD_GLOBAL_PROMOTOP%2CAMS_ARD_POPUP_FLOATINGAD%2CAMS_ARD_POPUP_EXIT%2CAMS_ARD_GLOBAL_EMBEDDEDFORM%2CAMS_ARD_SITE_FOOTER_SUBSCRIBE' + '%2CAMS_ARD_GLOBAL_IPAD_TAKEOVER';
