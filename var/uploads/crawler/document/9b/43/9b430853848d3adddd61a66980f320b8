    <script src="http://nhs-static.bdxcdn.com/GlobalResources14/Default/js/lib/jquery/jquery.ui.touch-punch.js"></script>
    <script type="text/javascript">
        var browserWidth;
        var browserHeight;
        var minWidth = 961;

        function jqUpdateBrowserSize() {
            // Get the dimensions of the viewport
            browserWidth = jQuery(window).width();
            browserHeight = jQuery(window).height();
            //alert("width: " + browserWidth);
        }

        if (!Modernizr.mq('only all')) { //check for mediaquiries and add listener
            jQuery(window).resize(function () {
                jqUpdateBrowserSize();
                if (browserWidth < minWidth) {
                    jQuery('html').addClass('width-under-960');
                }
                else {
                    jQuery('html').removeClass('width-under-960');
                }
            });
        }

        jQuery(document).ready(function () {
            function setLogoPush() {
                var isTvPage = 'False';
                if (isTvPage == 'True') {
                    jQuery.googlepush("NHS TV Page", "Main", "NHS Logo");
                }
                else {
                    jQuery.googlepush("Site Links", "Navigation v2", "NHS Logo");
                }
            }

            jQuery('#nhs_HeaderLogo').on('click', setLogoPush);

            if (!Modernizr.mq('only all')) { //check for mediaquiries and add class
                jqUpdateBrowserSize();
                if (browserWidth < minWidth) {
                    jQuery('html').addClass('width-under-960');
                }
            }
        });
    </script>

