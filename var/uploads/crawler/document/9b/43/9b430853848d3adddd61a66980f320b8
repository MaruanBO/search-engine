        };

        var facebook = new NHS.Scripts.Facebook(params);
        facebook.initialize();

        var marketId = 0;
        if (marketId == 0 && 0 != 0)
            marketId = 0;

        var parameters = {
            partnerID: 1,
            brandPartnerId: 1,
            siteRoot: '/',
            marketId: 0,
            communityId: 0,
            builderId: 0,
            planId: 0,
            specId: 0
        };


        logger = new NHS.Scripts.Globals.EventLogger(parameters);
        logger.attachEventsToAddThis();       
    });
    
    var adsAlreadyRender = false;   
    var isPro = false;
    var isNotMovePartner = true;
    var isNotNhsPartner = false;
    var isNotNhlPartner = true;
    var isNotYahooPartner = true;

    var isNotSearchResultPage = true;

    window.onload = function() {
        var parameters = {
            NoPhotoBig: 'http://nhs-static.bdxcdn.com/GlobalResources14/Default/images/no_photo/no_photos_325x216.png',
            NoPhotoSmall: 'http://nhs-static.bdxcdn.com/GlobalResources14/Default/images/no_photo/no_photos_120x80.png'
        };

        var lazyLoad = new NHS.Scripts.LazyLoad(parameters);
        lazyLoad.LazyLoadImages();

        if (adsAlreadyRender == false) {
            adsAlreadyRender = true;

            if (isPro || (isNotMovePartner && isNotNhsPartner && isNotNhlPartner && isNotYahooPartner) || isNotSearchResultPage) {
                adUpdater.RenderAllAds();
            } else {
                if (typeof commResults !== "undefined")
                    commResults.LoadAds();
            }
        }
        
        if (!isPro) {
            var livechat = new NHS.Scripts.ChatLoader();
            livechat.init('');
        }
    };
