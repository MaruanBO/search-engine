                        jQuery(document).ready(function() {
                            // get widths heights to resize
                            jQuery('.app-business .logo img').each(function(index){
                                var logo = $(this);
                                var logoWidth = logo.width();
                                var logoHeight = logo.height();
                                
                                if(logoWidth < logoHeight || (logo.height()/(logo.width()/150)) > 75){
                                    // vertical image
                                    logo.height(75);
                        
                                    if(logo.width() > 150){
                                        logo.width(150);
                                        logo.css("height","auto")
                                    }else{
                                        logo.height(75);
                                        logo.css("width","auto")
                                    }
                                } else {
                                    // horizontal image or equal
                                    logo.width(150);
                        
                                    if(logo.height() > 75){
                                        logo.height(75);
                                        logo.css("width","auto")
                                    } else {
                                        logo.width(150);
                                        logo.css("height","auto")
                                    }
                                }
                            })
                        });
                    </script>



