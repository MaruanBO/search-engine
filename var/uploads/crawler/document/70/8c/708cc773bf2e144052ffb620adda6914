   <div class="content">
   
      <!-- START: TSA modifications -->
      
      <style type="text/css">   
         /*
          * Drop down style
          */
         select.dropdown {border:1px solid #cbcec3; background-color:#F2F6F9; font-size:1.1em; height:2em;}
         select.dropdown-width {width:50%}
         @media only screen and (max-width: 1024px) { 
            select.dropdown-width {width:100%;}
         }
         span.cat-title {height: 58px; display: table; font-size:1.3em; font-weight:bold;}
         span.vert-align {display: table-cell; vertical-align: middle;}
         img.cat-icon {width:82px; height:58px; float:left; padding-right:15px;}
         
         /*
          * Show/Hide paragraph style
          */
         .show-more {cursor:pointer; color: #3452C9; display:block; text-align:left;background-color: #f8f8f8; border: 1px solid #999999; border-radius: 2px; width: 100px; padding: 0 0 0 4px;}
         .show-review {cursor:pointer; color: #3452C9; display:block; text-align:left;background-color: #f8f8f8; border: 1px solid #999999; border-radius: 2px; width: 100px; padding: 0 0 0 4px;}
      </style>
      <script language="javascript" type="text/javascript">
         /* 
          * Drop down script
          */
         jQuery(function() {
            jQuery('#page_select').bind('change', function () { 
               var pageurl = $(this).val(); 
               if (pageurl) {window.location.replace(pageurl)} 
               return false;
            }); 
            jQuery('#category_select').bind('change', function () { 
               var url = $(this).val(); 
               if (url) {window.location = url;} 
               return false;
            }); 
         });

         /*
          * Show/Hide paragraph script
          */
         jQuery(document).ready(function() {
            jQuery(".hidden").hide();
            jQuery(".show-more").html("Show more...");
            jQuery(".show-review").html("Show review...");

            jQuery(".show-more").click(function() {
               if (this.className.indexOf('clicked') != -1 ) {
                  jQuery(this).prev().slideUp();
                  jQuery(this).removeClass('clicked')
                  jQuery(this).html("Show more...");
               }
               else {
                  jQuery(this).addClass('clicked')
                  jQuery(this).prev().slideDown();
                  jQuery(this).html("...Show less");
               }
            });

            jQuery(".show-review").click(function() {
               if (this.className.indexOf('clicked') != -1 ) {
                  jQuery(this).prev().slideUp();
                  jQuery(this).removeClass('clicked')
                  jQuery(this).html("Show review...");
               }
               else {
                  jQuery(this).addClass('clicked')
                  jQuery(this).prev().slideDown();
                  jQuery(this).html("...Hide review");
               }
            });
            
         });
      </script>
      
