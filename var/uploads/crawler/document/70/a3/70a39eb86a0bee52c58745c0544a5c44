        brightTag.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 's.btstatic.com/tag.js';

        var brightTagData = '{site: "8QAs0PZ" }';
        try {
            // doesn't work on ie...
            brightTag.appendChild(document.createTextNode(brightTagData));
        } catch (e) {
            // IE has funky script nodes
            brightTag.text = brightTagData;
        }

        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(brightTag, s);
    })();
</script>

