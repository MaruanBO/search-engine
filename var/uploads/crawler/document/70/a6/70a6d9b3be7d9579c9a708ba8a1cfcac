    <script type='text/javascript'>

        ScriptLoader.addOnReadyEventListener(onReady);

        function onReady() {
            initRotator(jQuery);
            onClickToSponsor(jQuery);
            initNewsDataview(jQuery);
            initNewsletterModalLauncher(jQuery);
        };

        function initRotator($) {
            $('.marquee-tabs div:first-of-type').removeClass('tab-hidden');
            $('.marquee-tabs').featuredContentTabs({});
        }

        function onClickToSponsor($) {
            $('.sponsored-container').on('click', function () {
                var url = $(this).attr('data-url');
                window.location = url;
            });
        }

        function directorySearch() {
            var frm = document.getElementById('directoryForm');
            var category = document.getElementById('directoryCategory');
            var state = document.getElementById('directoryState');

            category = category.options[category.selectedIndex].value;
            state = state.options[state.selectedIndex].value;

            window.location.href = '/find/' + category + '/' + state;
        };

        function initNewsDataview($) {
            $('.recent-news').recentNews({
                endpoint: '/api/documents/findbytypeandstatus',
                pageAnchorSelector: '.recent-news ul.pagination-container > li:not(.pagination-next) > a',
                nextAnchorSelector: '.recent-news ul.pagination-container > li.pagination-next > a',
                maxTitleLength: 200,
                descriptionLength: 128,
                ellipsis: "...",
                pageSize: 7
            });
        };

        function initNewsletterModalLauncher($) {
            $('.newsletter-modal-launcher').newsletterModal();
        };

    </script>
  

        <script type="text/javascript">
            function _onReady() {
                initMyAccountMenu(jQuery);
                initUserWelcomeMessage(jQuery);
                initUserLogout(jQuery);
                initPrintables(jQuery);
                initIframeBreakOut(jQuery);

                if (!$('[data-no-newsletter]').length) {
                    $.fn.newsletterAuto({ delay: false });
                }
            };

            function initIframeBreakOut($) {
                if (window != top)
                    top.location.href = location.href
            };
            
            function initMyAccountMenu($) {
                $('.my-account-menu').renderMyAccountMenu({
                    callback: function(user){
                        if (user.isAuthenticated())
                            $('.my-account-li').css('display', 'block');
                        }
                });
            };

            function initUserWelcomeMessage($){
                $('.user-name-container').renderUserWelcomeMessage();
            };

            function initUserLogout($) {
                $('.logout').renderUserLogout();
            };

            function initPrintables($) {
                $('.printable').click(function () {
                    $(this).print();
                    return false;
                });
            };

            ScriptLoader.addOnReadyEventListener(_onReady);
        </script>
        
        
