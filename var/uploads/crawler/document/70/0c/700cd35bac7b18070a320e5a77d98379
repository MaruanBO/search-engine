function search(){
	var myTextField = document.getElementById('myText');

		if(myTextField.value == "")
		{
   			alert('Please enter a search term to begin');
			return false; 
		}
		else
		{
			window.location = "http://www.apus.edu/search/index.htm?dest="+myTextField.value
		}

}

function setFocus(event)
        {
            if (event.keyCode == 13)
            {
                 document.getElementById("searchsubmit").focus();

            }
        }
