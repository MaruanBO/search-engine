<!-- End Google Tag Manager --><div id="TemplateBackground"><div id="header"><h1 id="headerLogo">American Public University System</h1><script language="JavaScript" type="text/javascript" src="http://www.apus.edu/sebin/j/d/custom_functions.js"></script>
<script language="JavaScript" type="text/javascript">
var searchHighlight = {"background-color" : "#BEDCEF", "border" : "1px solid #0066A8"};
var searchDefault = {"background-image" : ""};
var searchToggle = {"background-image" : "url('/sebin/z/h/chk12px.png')", "background-repeat" : "no-repeat", "background-position" : "7px center"};
var curDomain = "apus.edu";
var toggleInfo = new Array();
var sites;
$(function() {
	$("#searchAMU").click(function() { searchSetAMU(); searchHide(); });
	$("#searchAPU").click(function() { searchSetAPU(); searchHide(); });
	$("#searchAPUS").click(function() { searchSetAPUS(); searchHide(); });
	$("#searchALL").click(function() { searchSetALL(); searchHide(); });
	$("#searchChange").click(function() { if($("#searchInclude").css("display")=="none") searchShow(); else searchHide(); });
	var cSite = Get_Cookie("searchSite");
	var fp = false;
	//cSite = cSite.replace("%7C", "|");
	if(cSite) {
		sites = cSite.split("|");
		if(!sites.length) sites[0] = cSite;
		for(var i = 0; i < sites.length; i++) {
			//searchSetAPUS();
			if(sites[i] == "apu") { searchSetAPU(); fp = true; }
			if(sites[i] == "amu") { searchSetAMU(); fp = true; }
			if(sites[i] == "apus") { searchSetAPUS(); fp = true; }
		}
		if(!fp) {searchSetAPU();searchSetAMU();searchSetAPUS();}
	} else {
		searchSetALL();
	}
});
function setSite() {
	var cSite = "";
	for(var i = 0; i < toggleInfo.length; i++) {
		cSite += (cSite ? "|" : "") + toggleInfo[i];
	}
	Set_Cookie("searchSite", cSite, null, "/", curDomain, null);
	$("#sSite").val(cSite);
}
function tgchk(what) {
	var my = $("#search" + what.toUpperCase());
	var tinfo = $.inArray(what.toLowerCase(), toggleInfo);
	if(tinfo < 0) {
		my.css(searchToggle);
		$("#searchALL").css(searchDefault);
		toggleInfo.push(what.toLowerCase());
	} else {
		my.css(searchDefault);
		toggleInfo.splice(tinfo, (tinfo == 0 ? tinfo + 1 : tinfo));
	}
	$("#myText").focus();
	if($.inArray("amu", toggleInfo) == -1 && $.inArray("apu", toggleInfo) == -1 && $.inArray("apus", toggleInfo) == -1) {
		searchSetALL();
	}
	setSite();
}
function searchSetAMU() { tgchk("AMU"); }
function searchSetAPU() { tgchk("APU"); }
function searchSetAPUS() { tgchk("APUS"); }
function searchSetALL() { toggleInfo = new Array(); searchSetAMU(); searchSetAPU(); searchSetAPUS(); }
function searchHide() { $("#searchInclude").fadeOut(); }
function searchShow() { $("#searchInclude").fadeIn(); }
$(function() {
	$(document).bind('click', function(e) {
		var $clicked = $(e.target);
		if(!$clicked.hasClass("filterException"))
			searchHide();
	});
});
</script>
