         <script type="text/javascript" charset="utf-8">
      $(function(){
        	  	$('.nav-content').each(function(){
        	  	
        	  	var leftColumn = $(this).children('.nav-content-left');
        	  	var rightColumn = $(this).children('.nav-content-right');
           	
            	if(leftColumn.children('nav').children('ul').children('li').length <= 0){
            		leftColumn.remove();
            		$(this).removeClass();
            		$(this).addClass('nav-content-singleCol');
            		rightColumn.removeClass();          		
            		rightColumn.addClass('nav-content-left-singleCol');
            	}

        });
      });
    </script>

