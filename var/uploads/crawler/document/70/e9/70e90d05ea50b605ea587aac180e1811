	InternationalShippingJS.setArrayValues(fxArray,flcArray);
		
		function selCountry(countryCode,selCountry,errMsg,from)
		{
		
		
		var previousContCookie = InternationalShippingJS.getCookie("iShippingCookie");
		
		var numOfItems = InternationalShippingJS.getCookie("numOfItems");
		if("" == from && null!=previousContCookie){
		
			selCountry= previousContCookie.split("|")[0];
		
		
		if(selCountry == 'US' && countryCode.value == 'CA' && numOfItems >0 ){

			$("#errMsg").css('display', 'inline-block');
			$("#errMsg").css('color', '#FF0000');
			$("#errMsg").text(errMsg);
		}else if(selCountry!= 'CA' && countryCode.value == 'CA' && numOfItems >0 ){
			$("#errMsg").css('display', 'inline-block');
			$("#errMsg").css('color', '#FF0000');
			$("#errMsg").text(errMsg);
		}
		else if(selCountry == 'CA' && countryCode.value != 'CA' && numOfItems >0){
		
			$("#errMsg").css('display', 'inline-block');
			$("#errMsg").css('color', '#FF0000');
			$("#errMsg").text(errMsg);
		}else{
		$("#errMsg").css('display', 'none');
		$("#errMsg").hide();
		}
		}
			
					var selectedCurrency = document.getElementById("currencyCode");
			
		if(countryCode.value == "US"){
			document.getElementById('USACURRENCY').style.visibility = "visible";
			document.getElementById('USACURRENCY').style.display = "inline";
			document.getElementById('INTCURRENCY').style.visibility = "hidden";
			document.getElementById('INTCURRENCY').style.display = "none";
			document.getElementById('CACURRENCY').style.visibility = "hidden";
			document.getElementById('CACURRENCY').style.display = "none";
			return true;
		 }else if(countryCode.value == "CA"){
		 
		 	document.getElementById('CACURRENCY').style.visibility = "visible";
			document.getElementById('CACURRENCY').style.display = "inline";
			document.getElementById('INTCURRENCY').style.visibility = "hidden";
			document.getElementById('INTCURRENCY').style.display = "none";
			document.getElementById('USACURRENCY').style.visibility = "hidden";
			document.getElementById('USACURRENCY').style.display = "none";
			return true;
		 } 
		 
		 else {
			document.getElementById('USACURRENCY').style.visibility = "hidden";
			document.getElementById('USACURRENCY').style.display = "none";
			document.getElementById('INTCURRENCY').style.visibility = "visible";
			document.getElementById('INTCURRENCY').style.display = "inline";
			document.getElementById('CACURRENCY').style.visibility = "hidden";
			document.getElementById('CACURRENCY').style.display = "none";
			
	
			
			var newOpt = cArray[countryCode.value];
			var ind= newOpt.indexOf("D");
			 
		//	 alert(newOpt.substring(4,ind));
			for (i = 0;i < selectedCurrency.length; i++)
			{
				var ind = selectedCurrency.options[i].text.indexOf(" (");
				if (selectedCurrency.options[i].text.substring(0,ind) == newOpt)
				{
					
					selectedCurrency.selectedIndex = i;
				}
			}
		 }
		}
		//InternationalShippingJS.languageToggle($('#countryCode').val());
</script>




