function PayPalCheckout()
{

	try {
		var newURL = "https://www.childrensplace.com/webapp/wcs/stores/servlet/TCPPayPalCCLookUpCmd?storeId=10151&catalogId=10551&langId=-1&orderId=&callingPage=AjaxOrderItemDisplayView";
		document.location.href = newURL;
	} catch(error) {
		alert("error detected while redirecting to PayPal : " + error.message);
	}
}

	
	
</script>
	

