		<script type="text/javascript">
			dojo.addOnLoad(function() { 
				parseWidget("MiniShoppingCart"); 
				var miniShopCartURL = 'http://www.childrensplace.com/webapp/wcs/stores/servlet/TCPMiniShopCartDisplayView?catalogId=10551&langId=-1&storeId=10151';
				
				var iShippingUrl = $('#WC_TCPCachedCommonToolbar_FormLink_IShipping1').attr('href');
				var INTcurrencyUrl = $('#INTcurrency').attr('href');
				if('https:' == document.location.protocol ){
					miniShopCartURL = 'https://www.childrensplace.com/webapp/wcs/stores/servlet/TCPMiniShopCartDisplayView1?catalogId=10551&langId=-1&storeId=10151';
					
					
					if(iShippingUrl){
						
						INTcurrencyUrl=INTcurrencyUrl.replace('http:','https:');
						$('a[id^=INTcurrency]').attr('href',INTcurrencyUrl);
						iShippingUrl=iShippingUrl.replace('http:','https:');
						$('a[id^=WC_TCPCachedCommonToolbar_FormLink_IShipping]').attr('href',iShippingUrl);
					}
				
				}else{
					
					if(iShippingUrl){
						
						INTcurrencyUrl=INTcurrencyUrl.replace('https:','http:');
						$('a[id^=INTcurrency]').attr('href',INTcurrencyUrl);
						iShippingUrl=iShippingUrl.replace('https:','http:');
						$('a[id^=WC_TCPCachedCommonToolbar_FormLink_IShipping]').attr('href',iShippingUrl);
					}
				
				}
				CommonControllersDeclarationJS.setControllerURL('MiniShoppingCartController',miniShopCartURL);
			});
		</script> 
  
    </div>
    </div>
    
    </div><!--  end div id="toolbar" -->
