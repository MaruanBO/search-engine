								<script type="text/javascript">
								
								  $(document).ready(function(){
								  var countryCookie = InternationalShippingJS.getCookie("iShippingCookie");
									var selectedCountry;		
										var from ='';
										if("" == from && null!=countryCookie){
										
											selectedCountry= countryCookie.split("|")[0];
											
											if(selectedCountry == "US" || selectedCountry == "us"){
											 $("#USACURRENCY").show();
								   			 $("#INTCURRENCY").hide();
								   			 	$("#CACURRENCY").hide();
								   			 
											}else if(selectedCountry == "CA" || selectedCountry == "ca"){
											
												$("#USACURRENCY").hide();
								   			 	$("#INTCURRENCY").hide();
								   			 	$("#CACURRENCY").show();
								   			 	
											}else{
												$("#USACURRENCY").hide();
								   			 	$("#INTCURRENCY").show();
								   			 	$("#CACURRENCY").hide();
											}
											}
								
								   
								    
								    
								    });
								    InternationalShippingJS.languageToggle($('#countryCode').val());
							    </script>
							
