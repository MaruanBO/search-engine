    <script>
    
    var usURL = 'http://www.childrensplace.com/webapp/wcs/stores/servlet/en/usstore/home';
    var canadaURL = 'http://www.childrensplace.com/webapp/wcs/stores/servlet/en/canadastore/home';
    
	InternationalShippingJS.setUSURL(usURL);
	InternationalShippingJS.setCAURL(canadaURL);	


	var x =window.location.href;
	if(-1 != x.indexOf('advid=')){
	var y = x.substring(x.indexOf('advid')+6);
	if(y.indexOf('&') !=-1)
		var advId = y.substring(0,y.indexOf('&'));
	else
		var advId = y;

  	document.cookie = "advId="+advId+";domain=.childrensplace.com; path=/";
	}

    function  ReadCookie(name)
		{
		  	var a_all_cookies = document.cookie.split( ';' );
			var a_temp_cookie = '';
			var cookie_name = '';
			var cookie_value = '';
			var b_cookie_found = false; // set boolean t/f default f
			
			for ( i = 0; i < a_all_cookies.length; i++ ) {
				a_temp_cookie = a_all_cookies[i].split( '=' );
				cookie_name = a_temp_cookie[0].replace(/^\s+|\s+$/g, '');
				if ( cookie_name == name ) {
					b_cookie_found = true;
					if ( a_temp_cookie.length > 1 ) {
						cookie_value = unescape( a_temp_cookie[1].replace(/^\s+|\s+$/g, '') );
					}
					return cookie_value;
					break;
				}
				a_temp_cookie = null;
				cookie_name = '';
			}
			if ( !b_cookie_found ) {
				return null;
			}
		}
		
function Delete_Cookie( name, path, domain ) {
if ( ReadCookie(name) ) {
document.cookie = name + "=" + ( ( path ) ? ";path=" + path : "") + ( ( domain ) ? ";domain=" + domain : "" ) + ";expires=Thu, 01-Jan-1970 00:00:01 GMT";
}
}

function setTCPSessionCookie(c_name,value,domain)
{
//console.debug("Creating session cookie as following : " + c_name+"="+value+"; path=/"+"; domain="+domain);
document.cookie = c_name+"="+value+"; path=/"+"; domain="+domain;
}





	function resetLogoutCookie(url) {
		var domain = '.childrensplace.com';
		//delete minicart ..
		Delete_Cookie('minicartcookie', '/', domain);
		Delete_Cookie('numOfItems', '/', domain);
		
		var iShippingCookie = ReadCookie("iShippingCookie");
		if(undefined != iShippingCookie || null != iShippingCookie){
		
		var currCode = iShippingCookie.split("|")[2];
		
		if(currCode == undefined){
		if(iShippingCookie == 'us' || iShippingCookie =='US'){
		currCode = '$';
		}else if(iShippingCookie == 'ca' || iShippingCookie =='CA'){
		currCode = 'CAD';
		}
		}
		}else {
		var pathName = window.location.pathname;
		if(window.location.search.indexOf('10151') != -1 || pathName.indexOf('usstore') != -1){
									currCode = 'USD';
								}else{
									currCode = 'CAD';
								}
		}
		var minicartcookie = "0" + "|" + currCode +"0" + "|" + "false" ;
		setTCPSessionCookie("minicartcookie",minicartcookie,domain);
			setTCPSessionCookie("numOfItems","0",domain);
		Delete_Cookie('tcp_firstname', '/', domain);
		Delete_Cookie('tcp_isregistered', '/', domain);
		Delete_Cookie('myPlaceAcctNbr', '/', domain);
		Delete_Cookie('memberSince', '/', domain);
		
		if(url !="" && url != undefined){
		 window.location.href = url;
		 }
		} 
		
 </script>
