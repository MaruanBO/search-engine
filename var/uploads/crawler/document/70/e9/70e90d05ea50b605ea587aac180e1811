<script type="text/javascript">
    function displayEmailSignupModal_emailSubscribe() {
        var email = document.getElementById('email-signup-modal-text-field').value;

        if (!MessageHelper.isValidEmail(email)) {
            alert("Please enter a valid email address.");
            return false;
        } else {
            document.signupFormPage.submit();
        }
    }

    function displayEmailSignupModal_onFocusAction(thisObject){                                                                             
        if(thisObject.value == 'Enter Email Address') { 
            thisObject.value = '';                                                      
        }                                                              
    }

    function displayEmailSignupModal_onKeyPressAction(eventObject, thisObject){                                                                                

        if(eventObject.keyCode!=13 || thisObject.value.length > 0) {
            return true;
        } else if(eventObject.keyCode==13) {
            return emailSubscribePage();

        } else {
            return true;
        }

    } 
  
</script><!-- END TCPHomePageContentAreaESpot.jsp -->

