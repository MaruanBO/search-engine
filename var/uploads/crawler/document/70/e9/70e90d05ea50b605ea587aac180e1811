<script type="text/javascript">

  $(document).ready(function(){
$(function(){
    $("#primary-nav ul li a").hover(function(){
    
        $(this).addClass("hover");
        $('#primary-nav div',this).css('visibility', 'visible');
    
    }, function(){
    
        $(this).removeClass("hover");
        $('#primary-nav div',this).css('visibility', 'hidden');
    
    });
  });  
});


  $(document).ready(function(){
    $("#primary-nav ul li").hover(function(){
     
        $(this).children(".nav-content, .nav-content-singleCol, .nav-content-placeShops").each(function(index){
            $(this).css("display","block");
        });
    },
    function(){
        $(this).children(".nav-content, .nav-content-singleCol, .nav-content-placeShops").each(function(index){
            $(this).css("display","none");
        });
    });
});
</script>


