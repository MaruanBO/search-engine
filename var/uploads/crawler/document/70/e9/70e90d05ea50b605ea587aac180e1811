<script type="text/javascript">
	if(window.location.search.indexOf("ref=cj") > 0) {

		var today = new Date();
		today.setTime(today.getTime());
		var expires_date = new Date(today.getTime() + (14*24*60*60*1000));
		document.cookie = "SOURCE" + "=CJ" 
					+ (";expires=" + expires_date.toGMTString())
					+ (";path=" + "/")
					+ (";domain=" + window.location.hostname);
	}				
