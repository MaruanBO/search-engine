<script>
var loaded_js=new Array()
var current_mf_idx = 0;
var max_width = 840;
var max_height = 684;

$j(window).resize(onResize);

$j(document).ready(function(){
    onResize();
    global['fb_like'] = new FB_Like();
    
    $j.ajax({
        url: 'http://api.facebook.com/restserver.php',
        type: 'GET',
        dataType: 'xml',
        data: 'method=links.getStats&urls=http%3A%2F%2Fwww.streetdirectory.com',
        success: function(xmlDoc){
            if (xmlDoc == null || xmlDoc == undefined) {
                return;
            }
            var link_stat = $j('link_stat', xmlDoc);
            link_stat.each(function() {
                var share_count = $j('share_count', this).text() != '' ? $j('share_count', this).text() : '0';
                $j('.share_count').html(share_count);
            });
        }
    });

    $j('#ov_img, #ov_img_play').click( function(){
		google_analytic('/stats/sg/mainpage/mid_ony_video');
        show_video('http://www.youtube.com/embed/Sl2IyHqXRUw?rel=0&autoplay=1', "$j('#video_preview').remove(); $j('#offers_vid_center').hide();");
    });

    $j('#offline_map_img').hover(
        function() {
            $j('#play_small_hover').show();
        }, function() {
            $j('#play_small_hover').hide();
        }
    );

    $j('#offline_map_img.offline_map').click( function(){
		google_analytic('/stats/sg/mainpage/rhs_offline_map');
        clear_preview();
        var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('offline_map_img')) + 'px; left:' + findPosX(E('offline_map_img')) + 'px; background-color:#000000; opacity:0.1; width:225px; height:190px; z-index:1;"></div>';
        $j('body').append(html);
        $j('#center_overlay').animate({
            opacity: 1,
            top: findPosY(E('content_wrapper')),
            left: findPosX(E('content_wrapper')),
            width: max_width + 'px',
            height: max_height + 'px'
            }, 500, function() {
                after_preview();
                global['active_preview'] = 'video_preview';
                //$j('#map_wrapper').hide();
                show_video('http://www.youtube.com/embed/ISdufwU0SOk?rel=0&autoplay=1', 'offline_map_animate_back()');
            }
        );
    });
    $j('#offline_map_img.offer_near_you').click( function(){
		google_analytic('/stats/sg/mainpage/rhs_ony');
        clear_preview();
        var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('offline_map_img')) + 'px; left:' + findPosX(E('offline_map_img')) + 'px; background-color:#000000; opacity:0.1; width:225px; height:190px; z-index:1;"></div>';
        $j('body').append(html);
        $j('#center_overlay').animate({
            opacity: 1,
            top: findPosY(E('content_wrapper')),
            left: findPosX(E('content_wrapper')),
            width: max_width + 'px',
            height: max_height + 'px'
            }, 500, function() {
                after_preview();
                global['active_preview'] = 'video_preview';
                $j('#map_wrapper').hide();
                show_video('http://www.youtube.com/embed/Sl2IyHqXRUw?rel=0&autoplay=1', 'offline_map_animate_back()');
            }
        );
    });

    $j('#mobile_feature_link').click( function(){
		google_analytic('/stats/sg/mainpage/rhs_app_features');
        clear_preview();
        var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('mobile_feature_link')) + 'px; left:' + findPosX(E('mobile_feature_link')) + 'px; background-color:#EBEBEB; opacity:0.1; width:225px; height:125px; z-index:1;"></div>';
        $j('body').append(html);
        $j('#center_overlay').animate({
            opacity: 1,
            top: findPosY(E('content_wrapper')),
            left: findPosX(E('content_wrapper')),
            width: max_width + 'px',
            height: max_height + 'px'
            }, 500, function() {
                after_preview();
                add_mfi_image(0);
                $j('#mobile_features').show();
                $j('#offers_vid_center, #offline_map_vid_center').hide();
                //$j('#map_wrapper').hide();
            }
        );
        
        return false;
    });

        $j("#traffic_cont").appendTo("#drag_map");
    $j("#traffic_cont").show();
    load_map();
    
    offers_splash_screen();
});
function show_video(url, close) {
    var html = '<div id="video_preview" style="position:absolute; top:0; left:0; background-color:#ffffff; width:' + max_width + 'px; height:' + max_height + 'px; z-index:2;">'+
                    '<iframe src="'+url+'" style="border:none; width:' + max_width + 'px; height:' + max_height + 'px;"></iframe>'+
                    '<a href="javascript:;" class="close_overlay" title="Close" onclick="'+close+'"></a>'+
               '</div>';
    $j('#content_wrapper').append(html);
}

function clear_preview(clicked) {
    if(clicked == undefined) clicked = '';
    $j('#center_overlay').remove();
    if($j('#map_link:visible').length == 0 && clicked != 'map_wrapper')
        $j('#map_wrapper').show();
    //$j('#' + global['active_preview']).css('z-index', '0');
}

function after_preview(clicked) {
    if(clicked == undefined) clicked = '';
    $j('#center_overlay').remove();
    $j('#video_preview').remove();
    //$j('#offers_vid_center, #offline_map_vid_center').show();
    $j('#mobile_features').hide();
    //if($j('#map_link:visible').length == 0 && clicked != 'map_wrapper')
        $j('#map_wrapper').show();
    global['last_preview'] = global['active_preview'];
}

function offline_map_animate_back() {
    clear_preview();
    var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('content_wrapper')) + 'px; left:' + findPosX(E('content_wrapper')) + 'px; background-color:#000000; opacity:1; width:' + max_width + 'px; height:' + max_height + 'px;"></div>';
    $j('body').append(html);
    $j('#center_overlay').animate({
        opacity: 0.1,
        top: findPosY(E('offline_map_img')),
        left: findPosX(E('offline_map_img')),
        width: '225px',
        height: '190px'
        }, 500, function() {
            after_preview();
        }
    );
}

function mf_animate_back() {
    //$j('#offers_vid_center, #offline_map_vid_center').show();
    $j('#mobile_features').hide();
    clear_preview();
    var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('content_wrapper')) + 'px; left:' + findPosX(E('content_wrapper')) + 'px; background-color:#EBEBEB; opacity:1; width:' + max_width + 'px; height:' + max_height + 'px;"></div>';
    $j('body').append(html);
    $j('#center_overlay').animate({
        opacity: 0.1,
        top: findPosY(E('mobile_feature_link')),
        left: findPosX(E('mobile_feature_link')),
        width: '225px',
        height: '125px'
        }, 500, function() {
            after_preview();
        }
    );
}

function sg_map_animate() {
	google_analytic('/stats/sg/mainpage/lhs_singaporemap');
    clear_preview('map_wrapper');
    var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('map_link')) + 'px; left:' + findPosX(E('map_link')) + 'px; background-color:#ffffff; opacity:0.1; width:222px; height:180px;"></div>';
    $j('body').append(html);
    $j('#center_overlay').animate({
        opacity: 1,
        top: findPosY(E('content_wrapper')),
        left: findPosX(E('content_wrapper')),
        width: max_width + 'px',
        height: max_height + 'px'
        }, 500, function() {
            after_preview();
            global['active_preview'] = 'map_wrapper';
            if($j('#map_wrapper').length == 0) {
                html = '<div id="map_wrapper" style="position:absolute; top:0; left:0; width:' + max_width + 'px; height:' + max_height + 'px;">'+
                            '<div id="drag_map"></div>'+
                            '<a href="javascript:;" class="close_overlay" title="Close" onclick="sg_map_animate_back()"></a>'+
                       '</div>';
                $j('#content_wrapper').append(html);
                $j("#traffic_cont").appendTo("#drag_map");
                $j("#traffic_cont").show();
                load_map();
            } else {
                $j('#map_wrapper').show();
            }
            $j('#map_link').hide();
            $j('#ov_thumb').show();
        }
    );

    return false;
}

function sg_map_animate_back() {
    clear_preview();
    $j('#map_wrapper').hide();
    var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('content_wrapper')) + 'px; left:' + findPosX(E('content_wrapper')) + 'px; background-color:#ffffff; opacity:1; width:' + max_width + 'px; height:' + max_height + 'px;"></div>';
    $j('body').append(html);
    $j('#center_overlay').animate({
        opacity: 0.1,
        top: findPosY(E('ov_thumb')),
        left: findPosX(E('ov_thumb')),
        width: '222px',
        height: '180px'
        }, 500, function() {
            $j('#map_link').show();
            after_preview('map_wrapper');
            global['active_preview'] = global['last_preview'];
            $j('#ov_thumb').hide();
        }
    );
}

function ov_animate() {
	google_analytic('/stats/sg/mainpage/lhs_ony_video');
    clear_preview();
    var html = '<div id="center_overlay" style="position:absolute; top:' + findPosY(E('ov_thumb')) + 'px; left:' + findPosX(E('ov_thumb')) + 'px; background-color:#ffffff; opacity:0.1; width:222px; height:180px; z-index:1;"></div>';
    $j('body').append(html);
    $j('#center_overlay').animate({
        opacity: 1,
        top: findPosY(E('content_wrapper')),
        left: findPosX(E('content_wrapper')),
        width: max_width + 'px',
        height: max_height + 'px'
        }, 500, function() {
            after_preview();
            if(E('offers_vid_center'))
                global['active_preview'] = 'offers_vid_center';
            else 
                global['active_preview'] = 'offline_map_vid_center';
            $j('#offers_vid_center').show();
            //$j('#map_wrapper').hide();
            //$j('#ov_thumb').hide();
            //$j('#map_link').show();
        }
    );

    return false;
}

function mf_prev() {
    var prev_idx = parseInt($j('.mfi:visible').attr('id').replace('mf',''));
    current_mf_idx = prev_idx - 1;
    if(current_mf_idx < 0)
        current_mf_idx = $j('.mf_dot').length - 1;
    
    mf_animate('left');
}

function mf_next() {
    var prev_idx = parseInt($j('.mfi:visible').attr('id').replace('mf',''));
    current_mf_idx = prev_idx + 1;
    if(current_mf_idx >= $j('.mf_dot').length)
        current_mf_idx = 0;
    
    mf_animate('right');
}

function mf_nav(idx) {
    var prev_idx = $j('.mfi:visible').attr('id').replace('mf','');
    current_mf_idx = idx;
    if(current_mf_idx > prev_idx)
        mf_animate('right');
    else if(current_mf_idx < prev_idx)
        mf_animate('left');
}

function mf_animate(direction) {
    var prev_idx = parseInt($j('.mfi:visible').attr('id').replace('mf',''));
    add_mfi_image(current_mf_idx);
    
    if(direction == 'right') {
        var cur_ori_left = max_width;
        var prev_left = -max_width;
    }
    else if(direction == 'left') {
        var cur_ori_left = -max_width;
        var prev_left = max_width;
    }
    
    $j('#mf' + current_mf_idx).show();
    $j('#mf' + prev_idx).css({'position':'absolute'});
    $j('#mf' + current_mf_idx).css({'position':'absolute', 'left':cur_ori_left+'px', 'opacity':'0.1'});
    $j('#mf' + prev_idx).animate({
        opacity: 0.1,
        left: prev_left
        }, 250, function() {
            $j(this).hide();
        }
    );
    $j('#mf'+current_mf_idx).animate({
        opacity: 1,
        left: 0
        }, 250, function() {}
    );
    $j('.mf_dot').removeClass('active');
    $j('#mfn' + current_mf_idx).addClass('active');

    if(current_mf_idx == 0)
        $j('#mf_prev').hide();
    else
        $j('#mf_prev').show();
    if(current_mf_idx == ($j('.mf_dot').length - 1))
        $j('#mf_next').hide();
    else
        $j('#mf_next').show();
}

function add_mfi_image(page){
    if(page > ($j('.mf_dot').length - 1))
        return false;
    
    var mfi_ver = 4, img;
    if(page == 0){
        img =   '<div id="mf0" class="mfi" style="display: block;">' + 
                    '<img src="/img/mainpage/v2/mobile/0.jpg?v='+mfi_ver+'" style="width:100%; height:95%;">' +
                    '<a href="javascript:;" onclick="mf_nav(1)" style="position:absolute; top:520px; left:389px; width:222px; height:46px;"></a>' + 
                '</div>';
    } else if(page == 11){
        img =   '<div id="mf11" class="mfi" style="display:none; position:absolute; width:840px; height:650px;">'+
                    '<div style="margin: 70px auto 0;">'+
                        '<div align="center">'+
                            '<div style="color:#008131; font-size:60px; line-height:50px; font-weight:bold;">Download</div>'+
                            '<div style="color:#008131; font-size:42px;">Streetdirectory app now</div>'+
                        '</div>'+
                        '<div style="position: relative; width: 350px; height: 150px; margin: 70px auto 0px;">'+
                            '<img style="position:absolute; top:0px; left:0px;" src="/img/mainpage/v2/mobile/mobile-logo.png?v=1">'+
                            '<a href="https://itunes.apple.com/sg/app/singapore-maps/id388128906" target="_blank">'+
                                '<img style="position:absolute; top:20px; left:170px;" src="/img/mainpage/v2/mobile/app-store.png?v=1">'+
                            '</a>'+
                            '<a href="https://play.google.com/store/apps/details?id=streetdirectory.mobile" target="_blank">'+
                                '<img style="position:absolute; top:80px; left:170px;" src="/img/mainpage/v2/mobile/playstore.png?v=1">'+
                            '</a>'+
                        '</div>'+
                        '<div id="edl_wrapper" style="margin:100px auto 0; width:370px;" align="center">'+
                            '<div id="edl_form" style="position:relative; background-color:#FFFFFF; width:370px; height:20px; border-radius:10px; padding:10px; text-align:left;">'+
                                '<input type="text" id="input_edl" placeholder="Email me the download link" style="border:none; background-color:transparent; width:290px; font-size:20px; margin-top:-3px;">'+
                                '<a id="submit_edl" href="javascript:;" onclick="send_download_link()">Submit</a>'+
                            '</div>'+
                            '<div id="edl_sent" style="font-size: 30px; color: #008131; display:none;">'+
                                '<img src="/img/mainpage/v2/mobile/sent.png">Email Sent!'+
                            '</div>'+
                        '</div>'+
                    '</div>'+
                '</div>';
    } else {
        img = '<img id="mf'+page+'" class="mfi" src="/img/mainpage/v2/mobile/'+page+'.jpg?v='+mfi_ver+'" style="display:block;">';
    }
    if(E('mf'+page) == undefined){
        $j('#mf_nav').before(img);
    }
}

function send_download_link() {
    $j('#edl_form').fadeOut('slow', function() {
        $j('#edl_wrapper').append('<div id="edl_progress" style="font-size: 30px; color: #008131;">Please wait ...</div>');

        var post = {};
        post.email = $j('#input_edl').val();

        $j.ajax({
            url: 'http://www.streetdirectory.com/ajax/jx_send_mail/mobile_link/',
            type: 'POST',
            data: post,
            success: function(result){
                $j('#edl_progress').remove();
                $j('#edl_sent').fadeIn('slow');
            }
        });
    });
}

function load_map() {
    SD.MAP_VERSION = '1.0.1.401';
    var latlng = new GeoPoint(103.83050972046, 1.304787132947);
    SD.MAP_VERSION = '1.0.1.401';
var myOptions = {
	zoom: 7,
	center: latlng,
	enableDefaultLogo: false,
	 
	bingCredentials: "AiaG2uLA098vYYqDrOThGAqf72utFIoDNGmRIW_v93UCM_xCmJdJrwwewPIJUDTr",
	 
	thumb : true
};
MapAPI = new SD.genmap.Map(E("drag_map"), myOptions);
if(typeof(overideCopyRight)=='function'){
	MapAPI.divCopyright.innerHTML='<!-- -->';
	MapAPI.divCopyright.appendChild(overideCopyRight());	
	MapAPI.divCopyright.style.right = '';
	MapAPI.divCopyright.style.left = '12px';
	MapAPI.divCopyright.style.bottom = '0px';
	MapAPI.divCopyright.style.backgroundColor = '';
}
// set_sg_14_layer();
var navControl = new CompleteMapControl();
MapAPI.addControl(navControl);
navControl.setDisplay(0, false);

SD_api = new sd_api();
layers = new SD_Layers();
thumb = new Thumbnail(MapAPI);


lines = new SD_Line();
	routing = new RoutingManager({ map: MapAPI, api_key : '' });
global['traffic_camera_causeway'] = [
	{"id":"2702", "desc":"Woodlands Checkpoint (Towards BKE)", "x":"103.7683487", "y":"1.445432098", "t":"20 February 2015 @ 5.35 am", "c":"cc"},
	{"id":"4713", "desc":"Tuas Checkpoint", "x":"103.6366821", "y":"1.347918007", "t":"20 February 2015 @ 5.35 am", "c":"cc"}
];
traffic = new Traffic('drag_map',latlng.lon,latlng.lat,'');
resetPhotoController();
//create3dLayer();

if(!SD.isIE6) setAutowidthSuperSearch();
global['fb_like'] = new FB_Like();
global['mapRegion'] = new MapRegion();
global['glayers'] = SDGlobalLayer.getInstance(MapAPI);

MapAPI.viewport.OnEndDrag.register(function(info) {
	if(MapAPI.isMapSd())
		onEndDragMap();
	else 
		onEndDragBingMap();
});
MapAPI.viewport.OnDraw.register(function(info) {
	onDrawMap();
});
MapAPI.viewport.OnLevelChanged.register(function(info) {
	if(!MapAPI.isMapSd()) onChangeLevelInBing();
});


if(window.location.hash == "#park_connector") {
	pcUrl = SD.API_URL + "assets/park_connector_1.0.2.694.js";
	SD.util.addScript(pcUrl);
}    var myOptions = {
        zoom: 7,
        center: latlng,
        enableDefaultLogo: false,
        thumb : true
    };

    MapAPI = new SD.genmap.Map(E('drag_map'), myOptions);

if(typeof(overideCopyRight)=='function'){
	MapAPI.divCopyright.innerHTML='<!-- -->';
	MapAPI.divCopyright.appendChild(overideCopyRight());
	MapAPI.divCopyright.style.right = '';
	MapAPI.divCopyright.style.left = '12px';
	MapAPI.divCopyright.style.bottom = '0px';
	MapAPI.divCopyright.style.backgroundColor = '';
}

    var navControl = new CompleteMapControl();
    MapAPI.addControl(navControl);
    navControl.setDisplay(0, false);

    SD_api = new sd_api();
    thumb = new Thumbnail(MapAPI);

        global['traffic_camera_causeway'] = [
        {"id":"2702", "desc":"Woodlands Checkpoint (Towards BKE)", "x":"103.7683487", "y":"1.445432098", "t":"20 February 2015 @ 5.35 am", "c":"cc"},
        {"id":"4713", "desc":"Tuas Checkpoint", "x":"103.6366821", "y":"1.347918007", "t":"20 February 2015 @ 5.35 am", "c":"cc"}
    ];
    traffic = new Traffic('drag_map',latlng.lon,latlng.lat,'');
    
    global['mapRegion'] = new MapRegion();
    global['glayers'] = SDGlobalLayer.getInstance(MapAPI);
}

function onResize() {
    if($j(window).width() > 1000) {
        var content_width = $j(window).width() - 255;
        if($j(window).width() > 1100)
            content_width = content_width - 20 - 225 - 20;
        var content_height = $j(window).height() - 10 - 50 - 15;
        max_width = content_width;
        max_height = content_height;
        $j('#offers_vid_center , #offline_map_vid_center, #mf_wrapper, #mf11, #map_wrapper, #drag_map, #video_preview, #video_preview iframe')
            .width(content_width)
            .height(content_height);
        
        if(typeof(MapAPI) != 'undefined') {
            MapAPI.resizeViewport(content_width, content_height);
        }
    }
    if($j(window).height() < 740){  
        $j('#lhs #lhs_content').css('height','500px');
        $j('#content_wrapper').css('height','592px');
    } else {
        $j('#lhs #lhs_content').css('height','600px');
        $j('#content_wrapper').css('height','684px');
    }
}
