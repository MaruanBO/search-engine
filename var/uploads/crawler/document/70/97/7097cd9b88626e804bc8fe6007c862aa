<script type="text/javascript">
       $(document).ready(function() {
           var waterMark = "Zipcode";

           $("#txtZip").keydown(function(evt) {
               if (evt.keyCode == "13") {
                   var anchor = $("#txtZip1Anchor");
                   WizardFinalSearchClick1(anchor);
                   location.href = anchor.attr("href");
                   return false;
               }
           });

           $("#txtZip2").keydown(function(evt) {
               if (evt.keyCode == "13") {
                   var anchor = $("#txtZip2Anchor");
                   WizardFinalSearchClick2(anchor);
                   location.href = anchor.attr("href");
                   return false;
               }
           });

           $("#txtZip").focus(function() {
               $(this).filter(function() {
                   return $(this).val() === "" || $(this).val() === waterMark;
               }).removeClass("watermarkedTxtZip").val("");
           });

           $("#txtZip").blur(function() {
               $(this).filter(function() {
                   return $(this).val() === "";
               }).addClass("watermarkedTxtZip").val(waterMark);
           });

           $("#txtZip2").focus(function() {
               $(this).filter(function() {
                   return $(this).val() === "" || $(this).val() === waterMark;
               }).removeClass("watermarkedTxtZip").val("");
           });

           $("#txtZip2").blur(function() {
               $(this).filter(function() {
                   return $(this).val() === "";
               }).addClass("watermarkedTxtZip").val(waterMark);
           });
       });
</script> 

