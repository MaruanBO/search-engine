    $(document).ready(function() {
        var bookmarkedSection = YAHOO.util.History.getBookmarkedState("w");
        var querySection = YAHOO.util.History.getQueryStringParameter("divs");
        var initialSection = bookmarkedSection || querySection || "c1";
        YAHOO.util.History.register("w", initialSection, WizardHistoryStateChangeHandler);

        bookmarkedSection = YAHOO.util.History.getBookmarkedState("s");
        querySection = YAHOO.util.History.getQueryStringParameter("l");
        initialSection = bookmarkedSection || querySection || "none";
        YAHOO.util.History.register("s", initialSection, SearchResultsHistoryStateChangeHandler);

        try { YAHOO.util.History.initialize("yui-history-field", "yui-history-iframe"); } catch (e) { }
        YAHOO.util.History.onReady(function() { }); 
    });
