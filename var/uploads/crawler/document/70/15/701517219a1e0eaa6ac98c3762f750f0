<script type="text/javascript">
	var breakingAjax = {
		url: base_url+"/section/breaking_ajax",
		cache: false,
		async: false,
		callback: function(html){
			if (trim(html).length > 0)
			{
				var m = html.match(/data-breaking-enhanced=\"(.*?)\"/);
				var enhanced = m[1];
				if (enhanced==1)
				{
					renderBreakingNewsEnhanced(html);
				}
				else
				{
					renderBreakingNews(html);
					breakingNewsPager();
				}
			}
		}
	}
	fetchAjax(breakingAjax);
	function renderBreakingNewsEnhanced(html)
	{
		$("#ps").hide();
		$("#above_hpcarousel_story").hide();
		$("#breaking-news-enhanced-wrapper").html(html);
	}
	function renderBreakingNews(html)
	{
		$("#scrollerpromobar2").attr("style", "display: none !important");
		$( "#breaking-news-wrapper" ).html(html);
	}
	function breakingNewsPager()
	{
	    itemPager({
			indvitem: $('.brnews_content_carousel_indvitem-1'),
			navcont: $('.brnews_content_nav_cont'),
			nextbtn: $('#brnews_content_nextbtn-1'),
			prevbtn: $('#brnews_content_prevbtn-1')
	    });
	}
</script>

