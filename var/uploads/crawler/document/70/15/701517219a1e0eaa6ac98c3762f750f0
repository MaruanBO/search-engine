<script type="text/javascript">var $jq = jQuery.noConflict( true );</script>
<script type="text/javascript" src="/Global/js/ugc/celljournalist.standard.uploadwidget.min.js"></script>
<script type="text/javascript">
var popupcontainer = $('#popup_innercont');
var popupclosebtncont = $('#popupclosebtn');
function embedCJUploader(){
	$("#cjuploader").uploadWidget({
	    partitionId: 342,
	    baseUrl: 'rockfordparent.celljournalist.com',
	    styleSheet: 'http://www.pjstar.com/Global/CSS/ugc/upload.css',
	    dateFormat: 'MMMM dd, yyyy',
	    imageFrameColor: '#dddddd',
	    creditCaption: 'Credit',
	    emailCaption: 'Email Address',
	    thumbStyle: 'crop',
	    tosText: 'I agree to the <b>Terms of Service </b><a href="#" target="_blank">Click Here</a> for details.',
        titleRequired: 1,
        descriptionRequired: 1,
        dateRequired: 1,
        assignmentRequired: 1,
        emailRequired: 1,
	    error: function (e) {
	        if (e.type == "imagecount") {
	            alert('too many images selected');
	        } else {
	            alert('wrong extension');
	        }
	    }
	});
}
function handlePopup(innercont,controller,ovrlycont,ovrlybg,vidsrc,imgsrc,imgthumb,imgtit,imgsum){
	$(innercont).html('');
	var controllerDomWidth = $(controller).width();
	var controllerMarginLeft = controllerDomWidth / 2;
	$(controller).html('<div id="cjuploader"></div>');
	embedCJUploader();
	$('#ovrly_cont').css({width: (controllerDomWidth + 20)+'px', 'margin-left': '-'+controllerMarginLeft+'px'});
	$(innercont).css({width: (controllerDomWidth + 15)+'px'});
	var controllerHTML = $(controller).html();
	$(innercont).html(controllerHTML);
	$('#ovrlybg').fadeIn(200,'linear');
	$(controller).html('');
}
</script>
