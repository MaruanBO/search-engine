			<script type="text/javascript">
	function refresh_page(){
	   var cURL = window.location.href;
	   var sectionNoGet = /\?refresh=true$/g;
	   var sectionNoGet = cURL.match(sectionNoGet)
	   var sectionGet = /\?refresh=true&/g;
	   var sectionGet = cURL.match(sectionGet)
	   var pageNoGet = /\&refresh=true$/g;
	   var pageNoGet = cURL.match(pageNoGet)
	   var pageGet = /\&refresh=true&/g;
	   var pageGet = cURL.match(pageGet)
	   if(pageNoGet){
	      var cURL = window.location.href.replace('&refresh=true','')
	      window.location = cURL+'&refresh=true'
	   }else if(pageGet){
	      var cURL = window.location.href.replace('&refresh=true&','?')
	      window.location = cURL+'&refresh=true'
	   }else if(sectionNoGet){
	      var cURL = window.location.href.replace('?refresh=true','')
	      window.location = cURL+'?refresh=true'
	   }else if(sectionGet){
	      var cURL = window.location.href.replace('?refresh=true&','?')
	      window.location = cURL+'&refresh=true'
	   }else if(!sectionGet || !sectionNoGet || !pageGet || !pageNoGet){
	      var newSectionNoGet = /\?/g;
	      var newSectionNoGet = cURL.match(newSectionNoGet)
	      if(newSectionNoGet){
	          window.location = cURL+'&refresh=true'
	      }else{
	          window.location = cURL+'?refresh=true'
	      }
	   }
	}
	$(document).ready(function() {setInterval(function(){if(vidplayed == false){refresh_page();}},240000);});  
	</script>

		
		
