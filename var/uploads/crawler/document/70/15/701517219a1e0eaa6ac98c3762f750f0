<script type="text/javascript">
function intitwebextras(){
	var wedata = webextras.data;
	var ccnt=0;
	var divcnt=1;
    mc.cnt=(Math.ceil(wedata.length/4)-1);
	for(d=0;(mc.cnt+1)>=d;d++){
	    var cls = "mcbkt";
	    if(d!=0 && d!=1 && d!=2){cls="none"}
		$('#'+mc.movecontainer).append('<div id="a'+d+'" class="'+cls+'"></div>')
	}
	for(d=0;(wedata.length)>d;d++){
	    var content = '<div class="webc">';
	        content += '<div class="mcimg click"><a href="'+wedata[d]['link']+'"><img src="'+wedata[d]['image']+'"/></a></div>';
	        content += '<div class="mctxt black"><a href="'+wedata[d]['link']+'">'+wedata[d]['title']+'</a></div>';
	        content += '</div>';
	    $('#a'+divcnt).append(content)
	    ccnt++;
	    if(ccnt==4){
		    ccnt=0;
		    divcnt++;
	    }
	}
	$("#a0").html($("#a"+(mc.cnt+1)).html())
	$('#'+mc.movecontainer).css({"left":"-"+mc.display_width+"px"});
	if(mc.cnt==0){$('#webxbuttons').hide()}
}  
intitwebextras();
mc.run(5000);
</script> 
