var _adSlots = new Array();
//array to support refreshes
var yb_slots = [];

$(document).ready(function() {
  
  <!-- Yieldbot.com Intent Tag Activation -->
  ybotq.push(function () {

    var yieldbot_window_width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    if (yieldbot_window_width <= 480)
      yieldbot.pub('83f4');
    else
      yieldbot.pub('92ce');

    // Only apply to standard above-the-fold placements
    var ads = $("body div.auto-ad#flexleader,#leaderboard,#leaderboard-mobile,#mobile-leader,#sidebar-atf").filter(":visible");
    for (i = 0; i < ads.length; i++)
    {
      var ad = $(ads[i]);
      var adUnit = ad.data("ad-unit");
      var adSize = ad.data("ad-size");

      if (!adUnit.length || !adSize.length)
        continue;

      yieldbot.defineSlot(adUnit, {sizes: adSize});
    }

    yieldbot.enableAsync();
    yieldbot.go();
  });

  var ads = $("body div.auto-ad").filter(":visible");
  var dfpKeyCount = 0;
  var maxKeys = 10;
  var index = 0;
  
  if (ads.length)
  {
    ybotq.push(function() {
      googletag.cmd.push(function() {
      




            
      for (i = 0; i < ads.length; i++)
      {
        var ad = ads[i];
        var unit = $(ad).data("ad-unit");
        var size = $(ad).data("ad-size");
        var id = ad.id;
        
        if (!unit.length || !size.length || !id.length)
          continue;
        
        ad["no_display"] = false;
        try {
          if ($(ad).data("no-display") == true)
            ad["no_display"] = true;
        } catch (err) {}
          
        _adSlots[index] = googletag.defineSlot("/11927475/" + unit, size, id).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);;

        // Only apply to standard above-the-fold placements
        if ($.inArray(ad.id, ["flexleader","leaderboard","leaderboard-mobile","mobile-leader","sidebar-atf"]) >= 0)
        {
          yieldbot.setSlotTargeting(unit, _adSlots[index]);
          yb_slots.push({'slot_name': unit, 'slot_var':_adSlots[index]});
        }  

        if ($(ad).data("ad-tag") && dfpKeyCount < maxKeys)
        {
          if (_useValidTags == true)
          {
            var validTags = [];
            var tags = $(ad).data("ad-tag").replace(/\s/g, '').toLowerCase().split(",");
            for (var j = 0; j < tags.length; ++j)
            {
              if (_validTags.indexOf(tags[j]) >= 0)
                validTags.push(tags[j]);
            }

            if (validTags.length > 0)
            {
              _adSlots[index].setTargeting("Tag", validTags.join(",").substring(0,40));
              dfpKeyCount++;
            }
          }
          else
          {
            _adSlots[index].setTargeting("Tag", $(ad).data("ad-tag").replace(/\s/g, '').substring(0,40));
            dfpKeyCount++;
          }            
        }
        index++;
      }

      googletag.pubads().enableSingleRequest();
    });

    googletag.enableServices();    
    
    for (var k = 0; k < ads.length; ++k)
    {
      var ad = ads[k];
      if (ad["no_display"] == true)
        continue;
      googletag.display(ad.id);
    }
  });
  }
});

</script>



