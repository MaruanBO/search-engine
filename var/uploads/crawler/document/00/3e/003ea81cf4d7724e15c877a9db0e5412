$('a.video-lightbox2').click(function(e) {
        e.preventDefault();
        $.fancybox({
            'type'          :   'iframe',
            'overlayShow'   :   true,
            'overlayOpacity':   0.7,
            'overlayColor'  :   '#000000',
            'transitionIn'  :   'fade',
            'transitionOut' :   'fade',
            'href'          :   $(this).attr('href') + '?autohide=1&fs=1&showinfo=0&autoplay=1&controls=2&rel=0&jsonapi=1',
            'width'         :   640,
            'height'        :   360,
            'padding'       :   0,
            afterLoad: function() {_demandforceGA.trackYouTubeVideo();}
        });
    });
</script>

<script type="text/javascript">
// For original homepage hero
var parallax = $('.hero-full-width');
var herobox = $('.hero-box.translucent');

// For seasonal homepage hero
            //var herobox = $('.hero-box');
            var back = $('#home-main-hero');
            var front = $('.front');
            var middle = $('.middle');

var speed = -1;

window.onscroll = function()
{
var yOffset = window.pageYOffset;
var minOffset = 100;

parallax.css("background-position-y", (((yOffset / speed ) / 5 )));
back.css("background-position-y", (-((yOffset / speed ) / 2 )));
middle.css("background-position-y", (-((yOffset / speed ) / 4 )));

if(yOffset > minOffset) {
herobox.css("opacity", ((1 - (yOffset / 500) + (minOffset / 500) ))  );
}
else { herobox.css("opacity", "1"); }
}
</script>

