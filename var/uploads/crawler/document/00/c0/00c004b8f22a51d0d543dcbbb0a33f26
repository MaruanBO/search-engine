			function getCookie(cname) {
				var name = cname + "=";
				var ca = document.cookie.split(';');
				for(var i=0; i<ca.length; i++) {
					var c = ca[i].trim();
					if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
				}
				return "";
			}
			
			function displayAlert(state)
			{
				var fullD = "fullAlert";
				var shortD = "shortAlert"

				if(document.getElementById(fullD) !=null && document.getElementById(shortD) != null)
				{
					var msgCookie = getCookie("viewSiteMessage");
					
					if(state == "click")
					{
						if(msgCookie == "" || msgCookie == null)
						{
							msgCookie = "hideMessage";
						}
					
						if(msgCookie == "showMessage")
						{
							document.getElementById(fullD).style.display = "block";
							document.getElementById(shortD).style.display = "none";
							document.cookie = "viewSiteMessage=hideMessage;path=/;domain=.fullcompass.com;";
						}
						else
						{
							document.getElementById(fullD).style.display = "none";
							document.getElementById(shortD).style.display = "block";
							document.cookie = "viewSiteMessage=showMessage;path=/;domain=.fullcompass.com;";
						}
					}

					if(state == "load")
					{
						if(msgCookie == "hideMessage" || (msgCookie == "" || msgCookie == null))
						{
							document.getElementById(fullD).style.display = "block";
							document.getElementById(shortD).style.display = "none";
						}
						else
						{
							document.getElementById(fullD).style.display = "none";
							document.getElementById(shortD).style.display = "block";
						}
					}
				}
			}
			
			function Blank_TextField_Validator()
			{
				var searchtext = document.getElementById("input-search").value
				if (searchtext == '')
				{
				   alert("Please enter keyword(s) or part number. ");
				   return (false);
				}
			}
