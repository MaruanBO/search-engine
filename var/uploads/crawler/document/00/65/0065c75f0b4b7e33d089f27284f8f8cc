<script type="text/javascript" language="javascript" async>
$(window).scroll(function(){
				if($( window ).width() > 760) {
					var aTop = 220;
					if($(this).scrollTop() > aTop){
						if( $('.sticky-nav').is(':hidden') )
							$('.sticky-nav').show();
					} else {
						if( !$('.sticky-nav').is(':hidden') )
							$('.sticky-nav').hide();
					}
				}
			});
</script>

