function displayWall(theState,signedIn,maxViews,viewsLeft,referringURL) {

var siteName = "Law.com";
var placMaxViews = maxViews;
var placViewsLeft = viewsLeft;
var userState = theState;
var loggedIn = signedIn;
var paywall_type_variable = "";
var promoCode = "LC";
