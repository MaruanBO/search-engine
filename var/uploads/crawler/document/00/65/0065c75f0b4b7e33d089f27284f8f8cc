         $('#accordion').on('show.bs.collapse', function (e) {
            var id = e.target.id;
            var btn = $('.advanced_search a[href="#'+id+'"]')
                    btn.toggleClass('active');
            });

            $('#accordion').on('hide.bs.collapse', function (e) {
            var id = e.target.id;
            var btn = $('.advanced_search a[href="#'+id+'"]')
                    btn.toggleClass('active');
        });
        $('[data-toggle=offcanvas]').click(function(e) {
            e.preventDefault();
