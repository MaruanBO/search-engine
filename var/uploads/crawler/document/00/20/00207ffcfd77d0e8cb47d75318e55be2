			"302": { // city id
				customId: "milwaukee"
			},
			"422": { // city id
				customId: "nepa"
			},
			"342": { // city id
				customId: "neworleans"
			},
			"312": { // city id
				customId: "ocala"
			},
			"222": { // city id
				customId: "triad"
			},
			"282": { // city id
				customId: "portland"
			},
			"352": { // city id
				customId: "providence"
			},
			"412": { // city id
				customId: "rochester"
			},
			"272": { // city id
				customId: "sacramento"
			},
			"292": { // city id
				customId: "seattle"
			},
			"322": { // city id
				customId: "springfield"
			},
			"252": { // city id
				customId: "upstate" 		
			},
			"242": { // city id
				customId: "wichita"
			},
			"332": { // city id
				customId: "worcester"
			}
		};
		this.init = function() {
			city_dropdown();
			headerCityDropDown();
			if(IS_DDP && IS_LAYOUT_SPLIT_40_60) {
				ddpFunc();
			}
			if (IS_RDP){
				rdpFunc();
			}
			if (IS_MY_ACCOUNT) {
				my_account();
			}
			if (IS_MY_PURCHASES) {
				my_purchases();
			}
			if (IS_SPLASH) {
				splash();
			}
			if (IS_M_PORTAL){
				mPortal();
			}

			if (IS_BILLING_CONFIRM) {
				$(".buyConfirmUserEmail").after('. <br><span class="gm-confirmation-message">The voucher will be attached to the email.  You can also view and print your voucher directly from your account. </span>');
			}

			// show city based radio logos
			city_based_radio_logos();

			//updateTermsUrls();
		};

		var city_based_radio_logos = function(){
			if(cities[cityId]) {
				$(".gmFooterRadio-" + cities[cityId].customId).fadeIn("fast");
			}
		};
		
		var my_account = function() {
			var $email_field = $body.find("#email");
			$email_field.attr("disabled", "disabled");
		};
		
		var my_purchases = function () {
			var $contact_info = $body.find(".helpTextForCustomer");
			$contact_info.html("Questions, or need help? <a href='/pages/contact'>Contact us</a>");
		};

		// handle promo widgets content on RDP
		var processPromoWidget = function(){
			if(!cities[cityId]) return;
			
			var $widget = $(".gmRdPromoWidget_js"),
				$message = $(".gmRdPromoMessage_js"),
				$number = $(".gmRdPromoNumber_js"),
				$message_number = $(".gmRdPromoMessageNumber_js"),
				isWidgetAvailable = false,
				message = "Perks",
				number = "",
				message_number = "7";
			switch (cities[cityId].customId){
				case "nepa": 
					isWidgetAvailable = true;
					number = "99404";
					break;
				case "boston":
					isWidgetAvailable = true;
					number = "37937";
					break;
				case "denver":
					isWidgetAvailable = true;
					number = "36266";
					break;
				case "gainesville":
					isWidgetAvailable = true;
					number = "72881";
					break;
				case "hamptonroads":
					isWidgetAvailable = true;
					number = "72881";
					break;
				case "indianapolis":
					isWidgetAvailable = true;
					number = "43688";
					break;
				case "kansascity":
					isWidgetAvailable = true;
					number = "23909";
					break;
				case "memphis":
					isWidgetAvailable = true;
					number = "47183";
					break;
				case "neworleans":
					isWidgetAvailable = true;
					number = "20357";
					break;
				case "milwaukee":
					isWidgetAvailable = true;
					number = "32961";
					break;
				case "ocala":
					isWidgetAvailable = true;
					number = "72881";
					break;
				case "sacramento":
					isWidgetAvailable = true;
					number = "45797";
					break;
				case "wichita":
					isWidgetAvailable = true;
					number = "21272";
					message_number = "4";
					break;
				case "triad":
					isWidgetAvailable = true;
					message = "GetMyPerks";
					number = "99404";
					break;
				case "greenville":
					isWidgetAvailable = true;
					number = "73807";
					break;
			};
			
			if (isWidgetAvailable){
				$message.text(message);
				$number.text(number);
				$message_number.text(message_number);
				$widget.removeClass("hide");
			}
			return; 
		};
		
		// update Terms ur's in application to custom
		var updateTermsUrls = function(){
			var privacyUrl = "https://getmyperks.s3.amazonaws.com/Privacy_Policy.pdf";
			// privacy
			$("a[href$='GroupBuyPrivacyPolicy']").prop("href", privacyUrl);
		};
		
		// works for footer drop down
		var city_dropdown = function() {
			var $city_dd = $body.find(".gmTopSelect_js");
			$city_dd.on("change", function() {
				var city_id = $city_dd.val();
				var $city_link = $("#categoryItemWrapper"+ city_id).find(".categoryItemV2");
				$city_link.click();
			});
		};

		var headerCityDropDown = function(){
			$("#merchantCategoriesDropDownBox").appendTo(".gmHeaderCityBox_js");
			
			$(".gmHeaderCurrentCity_js").on("click", function(){
				$(".twitterButtonSection").toggle(); // hack for IE, due to iframe overlay issue
				MerchantCategoryMenu.toggleMerchantCategoriesMenu();
			});
		};
		
		var rdpFunc = function(){
			var $highlightedDeal = $(".gb-item-highlighted-deal");
			var isHighlightedDeal = $highlightedDeal.length;
			if (isHighlightedDeal){
				var $shipping = $highlightedDeal.find(".shippingPriceCaption");
				$shipping.insertAfter($highlightedDeal.find(".viewDealBtn"))
			}
			processPromoWidget();
		};
		
		var ddpFunc = function() {
			var $basic_section = $body.find(".ddpBasicSection");

			// move share section
			var $section_wrapper = $body.find(".ddpSectionWrapper");
			var $share_box = $body.find(".dealInfoBar");
			$share_box.prependTo($section_wrapper);

			// move title
			var $offer_title = $basic_section.find(".ddpOfferTitle");
			$offer_title.prependTo($basic_section);

			// move status label
			var $statusLabelWrapper = $body.find(".statusLabelWrapper");
			$statusLabelWrapper.insertAfter($offer_title);

			// create new content for tab
			var $tab_content_block = $body.find(".ddpContentText");
			var $merchant_info_elems = $body.find(".merchantDescTitle, .ddpMerchantWebsiteUrl, .companyInfo, .merchantDescText, .googleMapSection");
			$merchant_info_elems.wrapAll('<div id="ddpMerchantInfoBoxWrapper" class="hide"/>');
			var $merchant_info_wrapper = $body.find("#ddpMerchantInfoBoxWrapper");
			$merchant_info_wrapper.appendTo($tab_content_block);
		
			if ($merchant_info_wrapper.length > 0) {
				// create naw tab
				$.aop.after({target: window, method: "initTabsForSplitTemplate"}, function() { 
					var $splitTabs = $body.find(".ddpSplitTabs");
					var $newTab = $('<li id="ddpMerchantInfoBoxWrapperTab" class="ddpSplitTab">Location</li>');
					$newTab.appendTo($splitTabs);
					$newTab.on("click", function() {
						splitTemplateTabsClick($(this), "ddpMerchantInfoBoxWrapper");
					});
				});
				$.aop.before({target: window, method: "splitTemplateTabsClick"}, function() {
					$merchant_info_wrapper.hide();
				});			
			}

			// move highlights for single offer deals
			if(!IS_MULTI_PRICE_OFFER) {
				$("#ddpHighlightstInnerBox").prependTo(".ddpBasicMainInfoWrapper");
				$("#ddpHighlightsBoxWrapper").remove();
				// move original price to price with discount
				var $offer_price = $body.find(".detailsPageDealInfoPrice");
				var $offer_orig_price = $body.find(".ddpOrigPriceBox");
				$offer_orig_price.insertAfter($offer_price);
			} else {
				$(".multi-offer-item-box").each( function(index, element) {
				    // move original price to price with discount
					var $offer_price = $(this).find(".detailsPageDealInfoPrice");
					var $offer_orig_price = $(this).find(".origPriceValue");
					$offer_orig_price.addClass("discountSectionValue ddpOrigPriceBox");
					$offer_orig_price.css({'color': 'black', 'font-size': '13px', 'font-weight': 'bold', 'line-height': '32px', 'text-decoration': 'line-through'});
		 	        $offer_orig_price.insertAfter($offer_price);
				 });
			}

			// move FB community box
			$(".gbFbLikeBox").insertAfter("#sideDealsWidgetSection");
			travelZooHacks();
			$body.find("#content").css("visibility", "visible");
		};
	
			//TZ Hacks
			function travelZooHacks(){
			var $tzWhen = $(".customTZWhen_js"),
				$prodDesc = $(".prodDescriptionText"),
				$highlights = $("#ddpHighlightsBoxWrapper"),
				$byLine = $(".customTZByLine_js"),
				$lead = $(".customTZLead_js"),
				$tzTerms = $(".customTZTnC_js"),
				$tzTips = $(".customTZTip_js"),
				$tzPhotoCredits = $(".customTZPhotoCredits_js"),
				$tzWhatsIn = $(".customTZWhatsIn_js");
			
				
			if ($lead.length){
				$lead = $("<div/>", { 
					"class": "tz-lead-box",
					html: $lead.html()
				});
				
				$prodDesc.prepend($lead);
			}
				
				if ($byLine.length){
				$byLine = $("<div/>", { 
					"class": "tz-by-line-box tz-credits-box",
					html: "By " + $byLine.html()
				});
				$byLine.prependTo($prodDesc);
			}	
				
			//photo credits box
			if ($tzPhotoCredits.length){
				$tzPhotoCredits = $("<div/>", { 
					"class": "tz-photo-credits-box tz-credits-box",
					html: " | " + "Photos: " + $tzPhotoCredits.html()
				});
				
				if ($byLine.length){
					$tzPhotoCredits.insertAfter($byLine);
				} else {
					$prodDesc.prepend($tzPhotoCredits);
				}
			}
			
				
			if ($tzWhen.length) {
				var tzHTML = "<div class='clearfix tz-when-wrapper'>" +
								"<h2 class='highlight tz-when-title'>When</h2>" + 
								"<div class='clearfix tz-when-content text'>" +
									$tzWhen.html() +
								"</div>" +
							"</div>";
				$(tzHTML).insertAfter($lead);
			}
							
				if ($tzWhatsIn.length){
				var tzWhatsInHTML = "<div class='clearfix tz-whats-in-wrap'>" + 
					"<h2 class='highlight tz-when-title'>What's included</h2>" + 
						"<div class='clearfix tz-whats-in-content text'><ul>" +
							$tzWhatsIn.html() +
					"</ul></div>" +
					"</div>";
					
					if ($tzWhen.length){
					$(tzWhatsInHTML).insertAfter($tzWhen);
				} else {
					$(tzWhatsInHTML).insertAfter($lead);
				}
			}
			
				if ($tzTips.length) {
					var tzHtmlCode = "<div class='clearfix tz-tips-wrap'>" + 
						"<span class='highlight tz-tip-title'>Travelzoo Tip:</span>" + 
						"<span class='clearfix tz-tip-content text'>" +
						$tzTips.html() +
						"</span>" +
						"</div>";
					$(tzHtmlCode).insertAfter(".tz-whats-in-wrap");
				}
				
		};
	
		var splash = function() {

			$('.select-category-input option').removeAttr('selected');
			
			var $splashOtpGroup = $splashPopup.find(".gm-spalsh-form-first-option");
			$splashOtpGroup.attr('selected','true');
			
			
			$(".splashSubmitButton").click(function(){
				if($('.select-category-input option:selected').val() == 0) {
					$('#miniCityError').html('Please choose your location');
				} else {
					Splash.changeCategory(null, false, false);
				}
			});
		};
	
	
		var mPortal = function(){
			// reorder Account Summary fields. MaximNC. 03/21/2014 (#35647)
			$(".sm-summary-earnings-row, .sm-summary-paid-row").insertBefore(".sm-summary-remaining-row");
			$(".sm-summary-total-sold-row, .summaryRedeemRow").insertAfter(".sm-summary-sales-row");
			return false;
		};
	}

	// city dd redirect only to rdp
	var selectCategory = function(cityId, cityName, url) {
		window.location.href = url + "/deal/" + cityName;
	}
		
