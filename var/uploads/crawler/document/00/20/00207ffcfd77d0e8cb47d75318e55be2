	<script>
	
		function showSplashPopup(){
			var params = {
					bgiframe: true,
					resizable: false,
					draggable: false,
					dialogClass: "splashPopup ui-dialog-no-titlebar",
					modal: true,
					shadow: true,
					minHeight: 0,
					width: "auto",
					closeOnEscape: false,
					overlay:{opacity: 0.5,background:"black"},
					//for splash to aperar when it is propper located
					create: function() {
						splashPopup.removeClass("splashIpadPositionFix")
					}
				},
				splashPopup = $("#splashPopup");
			splashPopup.on("dialogcreate", function() {
				Splash.init();
			});
			Dialogs.destroyDialog(splashPopup);
			splashPopup.dialog(params).dialog('open');

			// center splash on window resize
			$(window).resize(function() {
				splashPopup.dialog("option", "position", "center");
			});
		}
		$script.ready(NC.scriptsBundle.getKeysAsArray(), function() {
			$.support.minHeight = isSupportFeature("minHeight"); // fallback to jQuery minHeight support feature, because it's activated only on dicument ready event
			showSplashPopup();
			// Fix for Backround overlay on Splash popup
			$(".ui-widget-overlay").addClass("fixSplashOverlay");
			$(window).load(function () {
				$(".ui-widget-overlay").css("height", $(document).height() + "px");
			});
			// end. Fix for Backround overlay on Splash popup
		});
	</script>



	

	
		
		
		
	
	<div id="background" class="defaultAutoscale">
		<img src="https://s3.amazonaws.com/product-images.imshopping.com/nimblebuy/background_image_min_6.jpg" alt="" id="autoScaleImage"/>
	</div>
	<script>
		$(window).load(function(){
			swapBackground("https://s3.amazonaws.com/product-images.imshopping.com/nimblebuy/background_image_6.jpg");
		});
		function swapBackground(src) {
			if(jQuery.browser.msie){
				$("<img/>").attr('src', src).onImagesLoaded(function(_this) {
					loadBgr($(_this), src);
				});
			} else {
				$("<img/>").attr('src', src).load(function() {
					loadBgr($(this), src);
				});
			}
		};
		function loadBgr(img, src) {
			var old = $('#background img:first');
			$('#background').append( img );
			old.remove();
		};
	</script>

	










