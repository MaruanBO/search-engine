function dart_tracking(callback) {
  var cityName = jQuery(".selectCategoryInput option:selected").html();
  _gaq.push(['_trackEvent', 'Event', 'Subscribe', cityName]);

  if (typeof callback != 'undefined') {
     setTimeout(callback, 1500);
  } 

}
