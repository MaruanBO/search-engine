<script type="text/javascript">
function signup_tracking(callback) {
    if('' == '') {
        _gaq.push(['_trackEvent', 'Event', 'Registration', 'WEB']);     
    } else {
        _gaq.push(['_trackEvent', 'Event', 'Buy Page Registration', 'WEB']);     
    }
    setTimeout(function(){callback()}, 1500);
}
</script>
				
