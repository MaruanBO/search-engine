<script>
	$(function() {
		GmCustomFunc.init();
		var CustomSubscribeApi = new CustomSubscribeApiClass({
			enable_city_menu: false
		});
		CustomSubscribeApi.init();
	});

	var CustomSubscribeApiClass = function(new_ops) {
		var self = this,
		ops = {
			form_CSS: ".subscribe-form_js",
			enable_email: true,
			enable_zip: true,
			enable_city_menu: true,
			enable_confirm_popup: false,

			zip_code_type: "zipCode", // zipCode, postalCode, postCode
			custom_city_menu: false
		},

		$form = $((new_ops.form_CSS)? new_ops.form_CSS : ops.form_CSS),
		$fields 		 = $form.find(".subscribe-field-email_js, .subscribe-field-zip_js"),
		$field_email	 = $fields.filter(".subscribe-field-email_js"),
		$field_zip		 = $fields.filter(".subscribe-field-zip_js"),
		$field_city_menu = $form.find(".subscribe-field-city_js"),
		$submit_btn		 = $form.find(".subscribe-submit-btn_js"),
		$error_spans	 = $form.find(".subscribe-error-span_js"),
		$error_email	 = $error_spans.filter(".subscribe-error-email_js"),
		$error_zip		 = $error_spans.filter(".subscribe-error-zip_js"),
		$error_city_menu = $error_spans.filter(".subscribe-error-city_js"),

		field_error_class = "dataWrongBackground",
		confirm_message_class = "subscribe-confirm-message",
		confirm_popup_content_class = "subscribe-confirm-popup-content",

		EMAIL_ERROR_MESSAGE = Utils.getProperty("wrongEmailErorText"),
		ZIP_ERROR_MESSAGE = "",
		CITY_ERROR_MESSAGE = "Please choose your location.",
		SUBMIT_ERROR_MESSAGE = "Fail, please try again later.",
		CONFIRM_MESSAGE = "Thank you for subscribing!",

		// need for system
		email_value = "",
		zip_value = "",
		city_id = "212",

		email_valid_status = true,
		zip_valid_status = true,
		city_valid_status = true,
		common_valid_status = true,
		submit_status = true,

		URL = "/v2/subscribeAlerts.action";

		this.init = function() {
			var $subscribe_fields = $field_email.add($field_zip);
			var suscribe_controller = function() {
				$subscribe_fields.off("keypress");
				$submit_btn.off("click");

				validation();
				if(!common_valid_status) {
					show_message();
					return;
				}
				submit();
				if(!submit_status) {
					show_message();
					return;
				}
				if(ops.enable_confirm_popup) {
					confirm_popup();
					return;
				}
				// show_message("confirm");
				showConfirm();
			};

			var showConfirm = function(){
				$(".gmSubscribeBox_js").slideUp("fast",function(){
					$(".gmSubscribeConfirm_js").slideDown();				
				});
			};
			
			var subscribe_event = function() {
				$subscribe_fields.on("keypress", function() {
					if(event.which != 13) return;
					suscribe_controller();
					subscribe_event();
				});
	
				$submit_btn.on("click", function() {
					suscribe_controller();
					subscribe_event();
				});
			};

			$.extend(ops, new_ops);
			subscribe_event();
		};


		var confirm_popup = function() {
			$('<div class="'+ confirm_popup_content_class +'">'+ CONFIRM_MESSAGE +'</div>').dialog();
		};


		var submit = function() {
			var params = {
				subscribedCategoriesIds: city_id,
				email: email_value,
				zipCode: zip_value,
				dt: (new Date()).getTime()
			};

			$.get(URL, params, function(data) {
				submit_status = data.success;
			}, "json");
		};


		var show_message = function(confirm) {
			var error_status = true;
			var $current_error_field = $();
			var $current_error_span = null;
			var ERROR_MESSAGE = null;

			// clear error from fields and errorSpans
			$error_spans.html("");
			$fields.removeClass(field_error_class);

			// confirm message
			if(confirm) {
				$error_spans.addClass(confirm_message_class);
				$current_error_span = $error_spans.eq(0);
				ERROR_MESSAGE = CONFIRM_MESSAGE;
				error_status = false;
			}
			// submit request error
			if(error_status && !submit_status) {
				$current_error_span = $error_spans.eq(0);
				ERROR_MESSAGE = SUBMIT_ERROR_MESSAGE;
				error_status = false;
			}

			// email valid error
			if(error_status && !email_valid_status) {
				$current_error_field = $field_email;
				$current_error_span = $error_email;
				ERROR_MESSAGE = EMAIL_ERROR_MESSAGE;
				error_status = false;
			}
			// zip code valid error
			if(error_status && !zip_valid_status) {
				$current_error_field = $field_zip;
				$current_error_span = $error_zip;
				ERROR_MESSAGE = ZIP_ERROR_MESSAGE;
				error_status = false;
			}
			// city valid error
			if(error_status && !city_valid_status) {
				$current_error_span = $error_city_menu;
				ERROR_MESSAGE = CITY_ERROR_MESSAGE;
				error_status = false;
			}

			$current_error_span.html(ERROR_MESSAGE);
			$current_error_field.addClass(field_error_class);
		};


		var validation = function() {
			// reset validation status
			email_valid_status = true;
			zip_valid_status = true;
			city_valid_status = true;
			common_valid_status = true;
			submit_status = true;

			// validate email field
			if(ops.enable_email) {
				email_value = $.trim($field_email.val());
				email_valid_status = Utils.validateEmail(email_value);
			}

			// validate zip field
			if(ops.enable_zip) {
				zip_value = $.trim($field_zip.val());

				var zip_valid_result = null;
				switch(ops.zip_code_type) {
					case "postCode":
						zip_valid_result = Utils.validatePostCode(zip_value);
						break;
					case "postalCode":
						zip_valid_result = Utils.validatePostalCode(zip_value);
						break;
					default:
						zip_valid_result = Utils.validateZIP(zip_value);
				}

				if(zip_valid_result != "") {
					zip_valid_status = false;
					ZIP_ERROR_MESSAGE = zip_valid_result;
				}
			}

			// validate city field
			if(ops.enable_city_menu) {
				city_id = $field_city_menu.val();
				city_valid_status = (city_id == 0)? false : true;
			}
			common_valid_status = (email_valid_status && zip_valid_status && city_valid_status);
		};
	};

	var GmCustomFunc = new GmCustomClass();
	function GmCustomClass() {
		var self = this;
		var $body = $("body"),
			$splashPopup = $body.find("#splashPopup"),
			IS_DDP = $body.is(".dealDetailsPage"),
			IS_LAYOUT_SPLIT_40_60 = $body.is(".ol_split_40_60"),
			IS_RDP = $body.is(".groupBuysListPage"),	
			IS_MULTI_PRICE_OFFER = $body.is(".multiOffer"),
			IS_MY_ACCOUNT = $body.is(".mySettingsPage"),
			IS_MY_PURCHASES = $body.is(".myGroupBuyRequestsPage"),
			IS_M_PORTAL = $(".sm-summary-info-row").length,
			IS_BILLING_CONFIRM = $body.is(".billingConfirmPage"),
			IS_SPLASH = $splashPopup.length;
		
		// City static info for city specific widgets
		var cityId = "212";
		var	cities = {
			"192": { // city id
				// not reall custom ID from settings, 
				// just for rendering radio logos in the footer 
				// without additional updates on s3
				customId: "austin"
			},
			"212": { // city id
				customId: "bayarea"
			},
			"202": { // city id
				customId: "denver"
			},
			"172": { // city id
				customId: "boston"
			},
			"182": { // city id
				customId: "buffalo"
			},
			"492": { // city id
				customId: "" // Cinco De Mayo
