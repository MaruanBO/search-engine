    $(document).ready(function(){
        $('.top-login-form-input').focus(function(){
            $(this).prev().text('');
        });
        
        $('.top-login-form-input').blur(function(){
            if ($(this).val() == '') {
            	$(this).prev().text($(this).attr('title'));
            }
        });
        
