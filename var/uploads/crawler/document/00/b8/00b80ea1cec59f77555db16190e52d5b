<script>
$('.navWrapper .menu ul li').hover(
    function() 
    {
        if ($(this).text() == $('.navWrapper .menu ul').find('li').eq(0).text()) {
            $(this).css("background-image", "url(http://static.dailystrength.org/templates/images/ep/epNavBar/epNavBarBgSelected.gif)");
        } else {
            $(this).css("background-image", "url(http://static.dailystrength.org/templates/images/ep/epNavBar/epNavBarBgSelected.gif)");
            $(this).css("background-position", "-7px");
        }
        $(this).children('a').css("color", "#FFF");
    },
    function()
    {
        if (!$(this).is('.selected')) {
            $(this).css("background-image", "url()");
            $(this).children('a').css("color", "#243E92");
        }
    }
);
</script></div>		</div>
		
