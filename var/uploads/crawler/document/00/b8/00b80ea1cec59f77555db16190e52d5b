		<!-- new FB Connect based on updated API [DS-176]-->
		<div id="fb-root" style="padding: 0px;"></div>
		<script>
						/*
				FB USER:
				int(0)

				FB DATA:
				bool(false)

			 */
			window.fbAsyncInit = function() {
				FB.init({
					appId		: '247083736991',
					status		: true,
					logging		: true,
					channelUrl	: '//'+window.location.hostname+'/channel.php',
					cookie		: true,
					xfbml		: true,
					oauth		: true
				});

				FB.Event.subscribe('auth.login', function(response)
				{
//					window.location.href	= '/index.php?option=com_comprofiler&task=login&fb_login=1&cachebust='+Math.random();
					window.location.href	= '/component/option,com_comprofiler/task,ie_login/fb_login,1';
				});

				FB.Event.subscribe('auth.logout', function(response) {
					window.location.reload();
				});

				checkLoggedInFB();

			};
			(function(d){
				var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
				js = d.createElement('script'); js.id = id; js.async = true;
				js.src = "//connect.facebook.net/en_US/all.js";
				d.getElementsByTagName('head')[0].appendChild(js);
			}(document));

			function fbLogin()
			{
				FB.login(
					function(response)
					{
						if(response.authResponse)
						{
//							window.location.href	= '/index.php?option=com_comprofiler&task=login&fb_login=1&cachebust='+Math.random();
							window.location.href	= '/component/option,com_comprofiler/task,ie_login/fb_login,1';
						}
					}
				);
			}

			function checkLoggedInFB()
			{
				try
				{
					FB.getLoginStatus(
						function(response)
						{
							if(response.status == 'connected')
							{
//								window.location.href	= '/index.php?option=com_comprofiler&task=login&fb_login=1&cachebust='+Math.random();
								window.location.href	= '/component/option,com_comprofiler/task,ie_login/fb_login,1';
							}
						}
					);
				}
				catch(e)
				{
					alert('Error checking if logged into FB: '+ e.message);
					return false;
				}
				return true;
			}

		</script>
