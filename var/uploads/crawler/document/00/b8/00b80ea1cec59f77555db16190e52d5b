<script type="text/javascript">

	$(document).ready(function() {
		pageInit();
	});

	function showDsLogin() {
		$("#fbLoginTabButton").removeClass("active");
		$("#dsLoginTabButton").removeClass("inactive");
		$("#dsLoginTabButton").addClass("active");
		$("#fbLoginTabButton").addClass("inactive");
		$("#login_tab_fb").hide();
		$("#login_tab_ds").show();
	}
	function showFbLogin() {
		$("#fbLoginTabButton").removeClass("inactive");
		$("#dsLoginTabButton").removeClass("active");
		$("#dsLoginTabButton").addClass("inactive");
		$("#fbLoginTabButton").addClass("active");
		$("#login_tab_ds").hide();
		$("#login_tab_fb").show();
	}
</script>
					
	    <div class="ad_1" >
	<script type="text/javascript">
	if (typeof(s) != 'undefined'){
		s.prop23 = 1;
	}
