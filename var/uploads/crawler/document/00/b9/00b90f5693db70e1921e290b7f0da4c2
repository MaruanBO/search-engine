function checkSearchSubmit()
{
	if(document.getElementById('srh_field').value=='enter keyword or file ID')
	{
		alert('Please enter keyword or file ID');
		return false;
	}
	else
	{
	searchgg=$.trim(document.searchfrm.srh_field.value.replace(/[`~!@#$%^&*()_|+\-=?;:'",<>\{\}\[\]\\\/]/gi, ' ').replace(/\s{2,}/g, ' ')).toLowerCase();
	searchgg=searchgg.split(" ").join("-");
	window.location="/photos-images/"+searchgg+".html";
	return false;
	}
}
function checkLogin(){
	if(document.getElementById('inp_user').value==''){
		alert('Please insert your username');
		return false;
	}
	if(document.getElementById('inp_pass').value==''){
		alert('Please insert your password');
		return false;
	}
	return true;
}

bf=parent.document.getElementById('bottomfs1');
if(bf) top.location='killframes.php?url=http://www.dreamstime.com/index.php';

</script>
