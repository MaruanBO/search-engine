<script type = "text/javascript">

var imgArray =

['images/homepage/102411/hp_041514_01.jpg','images/homepage/102411/hp_041514_01.jpg','images/homepage/102411/hp_041514_01.jpg','images/homepage/102411/hp_041514_01.jpg'];
function randOrd(){return (Math.round(Math.random())-0.5)}
imgArray.sort(randOrd);
var len = imgArray.length;
var count = 0;

rotate1();

function rotate1() {
document.getElementById("im1").src = imgArray[count];
count++;
if (count>=len) {count = 0}
var tim1 = window.setTimeout("rotate1()", 5000); // 10 seconds
}



