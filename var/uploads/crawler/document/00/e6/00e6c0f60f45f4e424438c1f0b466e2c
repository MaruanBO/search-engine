    ga_custom_attrs['dimension12'] = CustomGA.dimensions.contentCC;
  if (CustomGA.dimensions.contentSR !== undefined)
    ga_custom_attrs['dimension13'] = CustomGA.dimensions.contentSR;

  for (var key in ga_custom_attrs) {
    if (ga_custom_attrs.hasOwnProperty(key) && ga_custom_attrs[key].length > 0) {
      setGAAttr(key, ga_custom_attrs[key]);
    }
  }

  // the important one!
  ga('send', 'pageview');

</script>

