/**
 * @namespace window
 */
 ;(window);

/**
 * @namespace window.AUTOBLOG
 */
window.AUTOBLOG = window.AUTOBLOG || {};

/**
 * @namespace window.AUTOBLOG.settings
 */
AUTOBLOG.settings = {
	apiUrl: '',
	debug: false,
	devId: 'ao1atoBLg0975R',
	devmode: false,
	protocol: 'http',
	proxy: '',
	originBase: 'http://o.aolcdn.com'
};
AUTOBLOG.settings.isLogging = !!('boolean' === typeof AUTOBLOG.settings.devmode && AUTOBLOG.settings.devmode);

AUTOBLOG.user = {
	authenticated: false,
	authPref: ''
};

/**
 * @namespace window.AUTOBLOG.pageData
 */
AUTOBLOG.pageData = Boot.extend(AUTOBLOG.pageData || {}, {
	ampId: '',
	displayName: '',
	galleryId: '',
	pageId: 'home',
	postId: '',
	topicId: ''
});

/**
 * @namespace window.UNB
 */
window.UNB = {
	config: {
		referrer: document.referrer,
		theme:'dark',
		devId: AUTOBLOG.settings.devId
	}
};

/**
 * Contains configurations for site-wide ads.
 * @namespace window.AUTOBLOG.ads
 * @property {object} mobile Mobile ad hash object.
 * @property {object} tablet Tablet ad hash object.
 * @property {object} desktop Desktop ad hash object.
 */
AUTOBLOG.ads = {"tablet":{"ad-btf-native-global":{"mn":"93440679","adType":"text","width":"320","height":"115","adSetInView":false},"ad-banner":{"mn":"93446775","adType":"F","width":"LB","height":"LB","adSetInView":false},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280","adSetInView":true},"ad-tile":{"mn":"93445673","adType":"F","width":"RR","height":"RR","adSetInView":false},"ad-banner-btf":{"mn":"93444082","adType":"F","width":"970","height":"250","adSetInView":true}},"desktop":{"ad-btf-native-global":{"mn":"93440679","adType":"text","width":"320","height":"115","adSetInView":false},"ad-banner":{"mn":"93373101","adType":"F","width":"LB","height":"LB","adSetInView":false},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280","adSetInView":true},"ad-tile":{"mn":"93310537","adType":"F","width":"RR","height":"RR","adSetInView":false},"ad-banner-btf":{"mn":"93444082","adType":"F","width":"970","height":"250","adSetInView":true}},"mobile":{"ad-btf-native-global":{"mn":"348-14-15-1908","adType":"text","width":"320","height":"115","adSetInView":false},"ad-banner":{"mn":"348-14-15-28dc","adType":"F","width":"320","height":"50","adSetInView":false},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280","adSetInView":true},"ad-tile":{"mn":"93442506","adType":"F","width":"300","height":"250","adSetInView":false},"ad-banner-btf":{"mn":"348-14-15-28de","adType":"F","width":"320","height":"50","adSetInView":false}}};

/**
 * Calls {@link window.htmlAdWH} method with arguments defined by the members of a ads hash object.
 * @alias window.AUTOBLOG.ads.adCall
 * @param {object} ads Ads hash object.
 * @param {string} layout Sets the ad type. `desktop|mobile|tablet`
 * @example
 * AUTOBLOG.ads.adCall(AUTOBLOG.ads.desktop, 'desktop');
 */
AUTOBLOG.ads.adCall = function(ads, layout){
	if(typeof(adSetInView) !== "function"){
		return;
	}
	adSetOthAT('kvugc=0;');

	// TODO: temp
	var devices = Boot.respond();

	

	if(typeof ads == 'string'){
		ads = AUTOBLOG.ads[ads];
	}else if(ads == null){
		ads = AUTOBLOG.ads.desktop;
	}

	AUTOBLOG.ads.adCleanup();

	if(devices == 'tn-devices' || devices == 'xs-devices'){
		if(AUTOBLOG.ads['mobile']['ad-banner']){
			adsOverlayAd('93456330', 'mm', 'ad-interstitial-overlay');
		}
	}

	for (var key in ads) {
		var obj = ads['mn'] && ads || ads[key] || {};

		if(obj.hasOwnProperty('mn')) {
			if(document.getElementById(key)){
				adVis(key, 1);

				
				if((key === 'ad-banner-btf' && devices === 'sm-devices')){
					return;
				}
				

				htmlAdWH(obj['mn'],obj['width'],obj['height'],obj['adType'],key);

				adVis(key, 0);

				if(layout){
					document.getElementById(key).setAttribute('data-adtype', layout);
				}

				if(AUTOBLOG.settings.debug){
					var elem = document.getElementById(key);
					var val = key + ' ' + JSON.stringify(obj);

					elem.setAttribute('data-toggle', 'popover');
					elem.setAttribute('data-content', val);
					elem.setAttribute('data-placement', 'bottom');
				}
			}
		}
	}

	function adVis(key, value) {
		if(value === 1){
			if(ads[key]['adSetInView']){
				if(key === 'ad-tile' || key === 'ad-tile-refresh'){
					if(layout === 'mobile'){
						adSetInView('1');
					}
				} else{
					adSetInView('1');
				}
			}
		}else if(value === 0){
			if(ads[key]['adSetInView']){
				if(key === 'ad-tile' || key === 'ad-tile-refresh'){
					if(layout === 'mobile'){
						adSetInView('0');
					}
				}else{
					adSetInView('0');
				}
			}
		}
	}
}

/*TODO: This should not be necessary, as there was a bad Ad injected into the page.*/
AUTOBLOG.ads.adCleanup = function(){
	var a = document.getElementById('ad-banner');

	if(a != null && a.hasAttribute('style')){
		var els = [];
		while (a) {
			a.removeAttribute('style');
			els.unshift(a);
			a = a.parentNode;
		}
	}
};

if (!('htmlAdWH' in window)) {
	/**
	 * Writes an element into the document containing an ad.
	 * @alias window.htmlAdWH
	 * @param {string} m Ad identifier.
	 * @param {string} w Ad width.
	 * @param {string} h Ad height.
	 * @param {string} t Ad type `ajax|iframe|text`.
	 * @param {object} dv Ad will become the content of this DOM element.
	 * @example
	 * &lt;div id=&quot;ad_container&quot;&gt;&lt;/div&gt;
	 * @example
	 * htmlAdWH( '2830714', '300', '250', 'ajax', document.getElementById('ad_container') );
	 */
	htmlAdWH = function () {
		AUTOBLOG.main.log('htmlAdWH: not defined', arguments);
	};
}

	//Global libs
	Boot.getJS({
		'src': 'http://o.aolcdn.com/os_merge/?file=/aol/jquery.openwindow-1.0.min.js&file=/aol/jquery.multiauth-2.1.min.js&file=/aol/beacon.min.js'
	});

	
			Boot.getJS({
				'src': 'http://www.blogsmithmedia.com/www.autoblog.com/v/0da3b19bbfec0ac41b6d6983482cef3061252278/js/merge/home.min.js'
			});
			
			Boot.getJS({
				'src': 'http://www.blogsmithmedia.com/www.autoblog.com/v/0da3b19bbfec0ac41b6d6983482cef3061252278/js/merge/global.min.js',
				callback: function(){
					
				}
			});
		

	Boot.getJS({
		'src': 'http://o.aolcdn.com/os_merge/?file=/aol/unb.min.js',
		'defer': 'load'
	});

	(function(d, t) {
		var a = d.createElement(t),
			s = d.getElementsByTagName(t)[0];
		a.src = 'http://o.aolcdn.com/ads/adsWrapper.js';
		s.parentNode.insertBefore(a, s);
	}(document, 'script'));

</script></body>
