<p></p><style>
	.nx_nxTimer_container { width: 300px; height:250px; background-image:url(http://cdn.idstatic.com/cms/live/202/Page-Badge-300x250.png?1423845265); background-repeat:no-repeat;}
	.timeframeWrap{width:300px; margin-left:55px; margin-top:-20px;}
	.timeFrame{ width:33px;float:left; margin-top:188px; margin-right:22px; font-size:11px; color:#FFF; text-align:center;}
	.nxCountdownTimer {font-size: 28px;}
	.nx_nxTimer_sponsor { width: 300px; }
</style>

<script language="JavaScript">
	function nxCountdownTimer()
	{
		var timerID = 'nxCountdown';
		var countdownDaysTxt = timerID+'Days';
		var countdownDaysEl = document.getElementById(countdownDaysTxt);
		var countdownHoursTxt = timerID+'Hours';
		var countdownHoursEl = document.getElementById(countdownHoursTxt);
		var countdownMinutesTxt = timerID+'Minutes';
		var countdownMinutesEl = document.getElementById(countdownMinutesTxt);
		var countdownSecondsTxt = timerID+'Seconds';
		var countdownSecondsEl = document.getElementById(countdownSecondsTxt);
		var countdownDate = new Date("February 22, 2015 1:00:00 PM EST");//input desired date here
		var currentDate = new Date();
		var dateDifference = new Date(countdownDate-currentDate);
		var seconds = dateDifference.valueOf()/1000;
		var days = (seconds > 0)? (Math.floor(seconds/86400))%100000 : 0;
		countdownDaysEl.innerHTML = days;
		var hours = (seconds > 0)? nxLeadingZeros((Math.floor(seconds/3600))%24) : 0;
		countdownHoursEl.innerHTML = hours;
		var minutes = (seconds > 0)? nxLeadingZeros((Math.floor(seconds/60))%60) : 0;
		countdownMinutesEl.innerHTML = minutes;
		var seconds = (seconds > 0)? nxLeadingZeros((Math.floor(seconds))%60) : 0;
		countdownSecondsEl.innerHTML = seconds;
		setTimeout("nxCountdownTimer()", 1000);
	}
	function nxLeadingZeros(num, length)
	{
		if (length == null){ length = 2; }
        num = String(num);
        while (num.length < length)
        {
          num = "0" + num;
        }
        return num;
    };
</script>

<a href="/shared/big-race-daytona">
<div class="nx_nxTimer_container">
	<div class="timeframeWrap">
		<div class="timeFrame"><span id="nxCountdownDays" class="nxCountdownTimer">9</span><br>Days</div>
    	<div class="timeFrame"><span id="nxCountdownHours" class="nxCountdownTimer">02</span><br>Hours</div>
    	<div class="timeFrame"><span id="nxCountdownMinutes" class="nxCountdownTimer">29</span><br>Minutes</div>
    	<div class="timeFrame"><span id="nxCountdownSeconds" class="nxCountdownTimer">00</span><br>Seconds</div>
  </div>
<script type="text/javascript">nxCountdownTimer();</script>
</div>
