								"name": $(this).attr("data-promotion-name"),
								"id": $(this).attr("data-promotion-id"),
								"creative": $(this).attr("data-promotion-creative"),
								"position": $(this).attr("data-promotion-position")		        	
					        });	
					        attrFound = true;
				    	}
			    	});					    		
		    	}			

		    	if(attrFound){
					dataLayer.push({
						"event": "promotionClick",
						"ecommerce": {
							"promoClick": {
								"promotions" : promotions
							}
						}
					});	
				}	
				consoleLog(promotions); debug(e);	 

		    });

		    $("#atg_store_checkout, #miniCartCheckoutButton").on("click", function(e){ // click checkout button

				dataLayer.push(GTMstep2);

				debug(e);
		    });

			dataLayer.push({
