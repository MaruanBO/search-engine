		$(function(){ 

			var products = [], add = [], remove = [],checkout = [], impressions = [], detail = [], promoClick = [], 
				promoView = [], promotions = [];		    

		    function debug(e) { 
		    	//alert("debug"); 
		    	//e.preventDefault(); // enable for testing
		    }

		    function consoleLog(e) { 
		    	if(e.isArray){
					x = [];
					for( var i in window.dataLayer ) {
					    x[i] = window.dataLayer[i];
					    if(x[i].ecommerce != undefined) {
					    	var z = x[i].ecommerce.e;
					    }
					}			    		
		    	}
		    	//console.log(e,z); // enable for testing
		    }

	    	if($(".atg_store_pageProductDetail.mattressSet").length){
				if($("#gtmProductName").length){
					var chosenMattressProduct = $(".chosenMattressProduct:checked");
					if(chosenMattressProduct){
						detail.push({ 
							"products": [{
								"name": $(chosenMattressProduct).parent().find("#gtmProductName").val().split("\n")[0],
								"id": $(chosenMattressProduct).parent().find("#gtmProductId").val(),
								"price": $(chosenMattressProduct).parent().find("#gtmProductPrice").val().split("$")[1],
								"brand": $(chosenMattressProduct).parent().find("#gtmProductBrand").val(),
								"category": $(chosenMattressProduct).parent().find("#gtmProductCategory").val(),
							}]
						});	
						var detail = detail[0];		
					}	
				}

	    	}else if($(".atg_store_pageProductDetail").length){
				if($("#gtmProductName").length){
					detail.push({ 
						"products": [{
							"name": $("#gtmProductName").val().split("\n")[0],
