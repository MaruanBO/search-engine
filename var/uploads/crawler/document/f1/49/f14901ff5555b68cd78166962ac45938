				        });	
				        attrFound = true;
			    	}		        
			    });	

				if($("#promoSlider").length){	

					function imageChanged(i){ 

						var slideCount = $("#promoSlider a").length,
							active = $("#promoSlider a:visible")
							attrFound = false;

						if(i <= slideCount) {
							//console.log(i,slideCount,active);

							if(active.filterAttr("data-promotion")){
								consoleLog(active[0].attributes);
						         promotions.push({
									"name": $(active).attr("data-promotion-name"),
									"id": $(active).attr("data-promotion-id"),
									"creative": $(active).attr("data-promotion-creative"),
									"position": $(active).attr("data-promotion-position")		  	        	
						         });
						         attrFound = true;	
							}
					    	if($(active).find("area")) {
		 						$(active).find("area").each(function(){ // if the slide has image maps
		 							consoleLog(this);
						    		if($(this).filterAttr("data-promotion")){
								        promotions.push({ 
											"name": $(this).attr("data-promotion-name"),
											"id": $(this).attr("data-promotion-id"),
											"creative": $(this).attr("data-promotion-creative"),
											"position": $(this).attr("data-promotion-position")		        	
								        });	
								        attrFound = true;
							    	}
						    	});					    		
					    	}

							setTimeout(function(){ i++; imageChanged(i); }, 4000); // wait 0.5 second for next active slide 
						}	
					}
					setTimeout(function(){ imageChanged(1); }, 500); // start index at 1, wait 0.5 second for active 
				}
				if(attrFound){
					dataLayer.push({
						"event": "promotionImpression",
						"ecommerce": {
							"promoView": {
								"promotions" : promotions
							}		
						}
					});	
				}						
		    }

		    $(".promoStatic a, .promoStatic area, #promoSlider a").on("click", function(e){  /// promotion static click

