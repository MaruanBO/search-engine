<script>
// Add referrer tracking as custom event
var dref = document.referrer;
ga('send', 'event', 'Custom Tracking', 'Referrer', dref);

// Set height and width of viewport as custom events
var w=Math.max(document.documentElement.clientWidth,window.innerWidth||0);
var h=Math.max(document.documentElement.clientHeight,window.innerHeight||0);
ga('send','event','Custom Tracking','viewPortDimensions',w+'x'+h,'1');

// Set custom GA event to detect frame state
function inIframe(){try{return window.self!==window.top;}catch(e){return true;}}
var frameState=inIframe();if(frameState===false){ga('send','event','Custom Tracking','frameState','noFrame','1');}
else{ga('send','event','Custom Tracking','frameState','inFrame','1');}

// Set custom GA variable to store user agent
var jsuagnt = navigator.userAgent;
ga('send','event','Custom Tracking','userAgent',jsuagnt,'1');

// Custom VP + UA event tracker
ga('send','event','Custom Tracking','viewPort+userAgent',w+'x'+h+' ::: '+jsuagnt,'1');
</script>


