				<script>
				/* LOAD AUTOCOMPLETE DATA FOR TRAVEL HEADER SEARCH */
				var searchHeaderTravelLocations;
				function initAutocompleteHeaderTravelLocations() {
					jQuery('[id^=travel_header_search_where]').autocomplete({
						source: searchHeaderTravelLocations,
						minLength: 2,
						focus: function( event, ui ) {
							jQuery('[id^=travel_header_search_where]').val(ui.item.label);
							return false;
						},
						select: function( event, ui ) {
							jQuery('[id^=travel_header_search_where]').val(ui.item.label);
							jQuery('[id^=travel_header_search_where_id]').val(ui.item.value);
							return false;
						}
					});
					jQuery('[id^=travel_header_search_where]').removeClass("ajax-loading");
					jQuery('[id^=travel_header_search_where]').removeAttr("disabled");
				}		
								/* AJAX calls to populate the autocomplete search fields */
				jQuery.ajax({
					url: "http://www.yankeemagazine.com/wp-content/uploads/cached-travelsearchcriteria-locations.json",
					cache: true,
					dataType: "json",
					error: function(request, status, error) {
						console.log("AJAX error loading locations for travel search criteria autocomplete dropdown: " + error);
					},
					success: function(response) {
						searchHeaderTravelLocations = response;
						initAutocompleteHeaderTravelLocations();
					}
				});
				
				// for mobile (and tablets in portrait Refs #8429): move the advertisement in the sidebar (first widget) -> beginning of main content block
				
				if (jQuery(window).width() <= 480) {
					jQuery('#sidebar > .widget.emailsignup').after(jQuery('#sidebar > .widget:eq(4)'));
					jQuery('#sidebar > .widget:first').prependTo('#main');					
				}
				else if (jQuery(window).width() <= 768) {
																		
							jQuery('#main > #FooterAds:first').remove().prependTo('#main');
										}
				
				jQuery(window).on( "orientationchange", function( event ) {
					if(window.orientation & 2) {
						console.log("Changed orientation to landscape");
						console.log(window.orientation);						
						jQuery('#main > #FooterAds:first').remove().insertAfter('.Last');
					}
					else {
						console.log("Changed orientation to portrait");
						console.log(window.orientation);						
						jQuery('#main > #FooterAds:first').remove().prependTo('#main');
					}
				});
				
				
				
				</script>
				
	
	
<!-- WP Audio player plugin v1.9.3 - https://www.tipsandtricks-hq.com/wordpress-audio-music-player-plugin-4556/ -->
    <script type="text/javascript">
        soundManager.useFlashBlock = true; // optional - if used, required flashblock.css
        soundManager.url = 'http://www.yankeemagazine.com/wp-content/plugins/compact-wp-audio-player/swf/soundmanager2.swf';
        function play_mp3(flg, ids, mp3url, volume, loops)
        {
            //Check the file URL parameter value
            var pieces = mp3url.split("|");
            if (pieces.length > 1) {//We have got an .ogg file too
                mp3file = pieces[0];
                oggfile = pieces[1];
                //set the file URL to be an array with the mp3 and ogg file
                mp3url = new Array(mp3file, oggfile);
            }

            soundManager.createSound({
                id: 'btnplay_' + ids,
                volume: volume,
                url: mp3url
            });

            if (flg == 'play') {
                    soundManager.play('btnplay_' + ids, {
                    onfinish: function() {
                        if (loops == 'true') {
                            loopSound('btnplay_' + ids);
                        }
                        else {
                            document.getElementById('btnplay_' + ids).style.display = 'inline';
                            document.getElementById('btnstop_' + ids).style.display = 'none';
                        }
                    }
                });
            }
            else if (flg == 'stop') {
    //soundManager.stop('btnplay_'+ids);
                soundManager.pause('btnplay_' + ids);
            }
        }
        function show_hide(flag, ids)
        {
            if (flag == 'play') {
                document.getElementById('btnplay_' + ids).style.display = 'none';
                document.getElementById('btnstop_' + ids).style.display = 'inline';
            }
            else if (flag == 'stop') {
                document.getElementById('btnplay_' + ids).style.display = 'inline';
                document.getElementById('btnstop_' + ids).style.display = 'none';
            }
        }
        function loopSound(soundID)
        {
            window.setTimeout(function() {
                soundManager.play(soundID, {onfinish: function() {
                        loopSound(soundID);
                    }});
            }, 1);
        }
        function stop_all_tracks()
        {
            soundManager.stopAll();
            var inputs = document.getElementsByTagName("input");
            for (var i = 0; i < inputs.length; i++) {
                if (inputs[i].id.indexOf("btnplay_") == 0) {
                    inputs[i].style.display = 'inline';//Toggle the play button
                }
                if (inputs[i].id.indexOf("btnstop_") == 0) {
                    inputs[i].style.display = 'none';//Hide the stop button
                }
            }
        }
    </script>
