<!-- Yieldbot.com Intent Tag ACTIVATION -->
<script type="text/javascript">
	function yieldbotWidth () {
	   var w = window,
	   d = document,
	   e = d.documentElement,
	   x = Math.max(
	       d.body.scrollWidth, e.scrollWidth,
	       d.body.offsetWidth, e.offsetWidth,
	       e.clientWidth
	     );
	   return x;
	 }

	var ybotq = ybotq || [];
	var isMobile = yieldbotWidth() < 767;	
	ybotq.push(function () {		
		if(!isMobile) {						
			yieldbot.pub('747b');
			yieldbot.defineSlot('YKE_728x90_A');
			yieldbot.defineSlot('YKE_300x250_A');			
		}		
		else{	
			yieldbot.pub('cb15');
			yieldbot.defineSlot('YKE_300x250_A');
		}
		yieldbot.enableAsync();
		yieldbot.go();
	});
</script>

