    <script language="javascript" type="text/javascript">
        var GeneralMaster_search_shouldDefault = true;
        var GeneralMaster_search_hasTerm = false;
        var GeneralMaster_search_hasFocus = false;
        var GeneralMaster_search_hasMouseover = false;
        function dosearch(text, gsayt) {
	   
            if (text.length > 0 && text != searchText) {
                GeneralMaster_search_hasTerm = true;
            }
	    
            //var searchElementIsSelected = (GeneralMaster_search_hasMouseover || (GeneralMaster_search_hasFocus && document.activeElement.id != "ctl00_ctlHeader_SearchImageButton"));
            var searchElementIsSelected = gsayt || (GeneralMaster_search_hasMouseover || (GeneralMaster_search_hasFocus));
	    
	    
            if (GeneralMaster_search_shouldDefault && !GeneralMaster_search_hasTerm && searchElementIsSelected) {
                alert("Please enter a search term and try your search again.\nYou can search by size, style, brand, item # and more.");
            }
	    
	    
            var appendurl='&action=true&ft=1';
            if (gsayt) {
                appendurl += '&saytini=1';
            }
	    
            if (GeneralMaster_search_shouldDefault && GeneralMaster_search_hasTerm && searchElementIsSelected) {
                window.location.href='http://www.barenecessities.com/search.aspx' + "?search=" + encodeURIComponent(text) + appendurl ;
                return false;
            }
	    
            return true;
        }

        var cookiesEnabled = BareNecessities.Cookie.AreCookiesEnabled('q', '1');

        if (!cookiesEnabled) {
            var cookiesDisabledUrl = "http://www.barenecessities.com/cookiesdisabled.aspx";
            var currentLocationWithoutParameters = location.protocol + '//' + location.host + location.pathname;
            if (currentLocationWithoutParameters != cookiesDisabledUrl) {
                window.location = cookiesDisabledUrl;
            }
        }
        
        window.onbeforeprint = function() {
            if (document.forms && document.forms.length > 0) {
                var children = document.getElementById("aspnetForm") != null 
                    ? document.getElementById("aspnetForm").childNodes
                    : document.forms[0].childNodes;
                if (children && children.length > 0 && children[0].style)
                    children[0].style.display = "none";
            }
        };
        window.onafterprint = function() {
            if (document.forms && document.forms.length > 0) {
                var children = document.getElementById("aspnetForm") != null 
                    ? document.getElementById("aspnetForm").childNodes
                    : document.forms[0].childNodes;
                if (children && children.length > 0 && children[0].style)
                    children[0].style.display = "";
            }
        };
    </script>
   
