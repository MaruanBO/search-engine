<script type="text/javascript">
    function validateEmail(emailvalue, returnval) {

        // a false returnval will stop the postback.
        // Depending on our context, we may want to continue.

        if (!(emailvalue == '')) {
            if (checkEmailFormat(emailvalue)) {
                return true;

            }
            else {
                return false;
            }
        }

        return returnval;
    }

    var emailText = 'Enter email address';

    // Clears the email text field when it is given the focus (if user has not entered anything)
    function clearEmailText(formfield) {
        if (formfield.value == emailText)
            formfield.value = '';
    }

    // Refills the email text field on blur if it is empty
    function fillEmailText(formfield) {
        if (formfield.value == '') formfield.value = emailText;
    }
    //-->
</script>
    





