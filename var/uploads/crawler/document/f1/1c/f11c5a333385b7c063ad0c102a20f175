    <div id="ctl00_DCMProBodyBottomSection_StandardSimplePanel">
	
    <script type="text/javascript">  
	BareNecessities.BillboardPopup.hide = function (refreshPageWithParams,Params)   
	{      
		checkToRefresh = true;
		if (BN_PageData)    
		{    
			if (BN_PageData.GlobalContext)     
			{        
				if (BareNecessities.GlobalContext.BillboardSecondPopupEnabled)        
				{      
					if (BareNecessities.GlobalContext.BillboardSecondPopupEnabled.toLowerCase() == "true")       
					{       
						BareNecessities.BillboardPopup.SecondBillboard.startCountDownForSecondBillboard(refreshPageWithParams,Params);
						checkToRefresh = false;
					}     
				}    
			}   
		}   
		$("#BillboardContent").text("");           
		BareNecessities.lightbox.hide({id:"Billboard_lightbox", fadein: true, DoNotRemoveContent: true });
		if (checkToRefresh) {
			if (refreshPageWithParams) {
				BareNecessities.refreshPageWithParams(Params);
			}
		}
	};  
	BareNecessities.BillboardPopup.GetDCMContent = function (DCM_Obj)   
	{   
        var DCM_Url = "http://www.barenecessities.com/util/GetBillboardContent.aspx";
		var milliseconds = new Date().getTime().toString();
		var hasContent = false;
		var isTiered = (typeof(DCM_Obj.IsTiered) != "undefined" &&  DCM_Obj.IsTiered === true) ? "true" : "false";
		var prependPageNameOnTier = (typeof(DCM_Obj.PrependPageNameOnTier) != "undefined" &&  DCM_Obj.IsTiered === true) ? "true" : "false";
	
		$.ajax({
			url: DCM_Url,
			cache: false,
			async: false,
			type: "GET",
			data: "PageType=" + DCM_Obj.PageType + "&PageName=" + DCM_Obj.PageName + "&PageSection=" + DCM_Obj.PageSection + "&ms=" + milliseconds + "&IsTiered=" + isTiered + "&PrependPageNameOnTier=" + prependPageNameOnTier,
			error: function (xhr, status)           
			{              
				$("#BillboardContent").hide();              
				$("#technical_issues").show();              
				//alert("xhr.status = " + xhr.status + "\nxhr.responseText = " + xhr.responseText);
				hasContent = true;
			},          
			success: function (responseText)           
			{              
				if (BareNecessities.isEmpty(responseText) || responseText == null) 
				{
					hasContent = false;
				} else {
					hasContent = true;
				}
				$("#BillboardContent").html(responseText);
			}
		});
		return hasContent;
	};
	BareNecessities.BillboardPopup.SecondBillboard.startCountDownForSecondBillboard = function (refreshPageWithParams,Params)
	{
		var Countdown_Url = "http://www.barenecessities.com/ajaxservice.aspx";
		$.ajax({
			url: Countdown_Url,
			cache: false,
			async: true,
			type: "GET",
			data: "req=bb2beg",
			error: function (xhr, status)
			{
				if (refreshPageWithParams) {
					BareNecessities.refreshPageWithParams(Params);
				}
			},
			success: function (responseText)
			{
				if (refreshPageWithParams) {
					BareNecessities.refreshPageWithParams(Params);
				}
			}
		});
		BareNecessities.GlobalContext.BillboardSecondPopupCountdown = BareNecessities.GlobalContext.BillboardSecondPopupDelay;
		BareNecessities.BillboardPopup.SecondBillboard.Setup();
	};
</script>
	
<script type="text/javascript">  
/**   BILLBOARD LIGHTBOX  **/
if (!BareNecessities.BillboardPopup){
	BareNecessities.BillboardPopup={};
	BareNecessities.BillboardPopup.SecondBillboard={};
	BareNecessities.BillboardPopup.SecondBillboard.timer_is_on=false;  
	BareNecessities.BillboardPopup.SecondBillboard.t="";  
	BareNecessities.BillboardPopup.Executing=false;
}
BareNecessities.BillboardPopup.ExecutePopUpLightbox=function(){
	BareNecessities.BillboardPopup.Executing=true;
	var expiration_days=1;
	if (BN_PageData.GlobalContext.ItemsInCartPopUpCookieExpirationDays) expiration_days=parseInt(BN_PageData.GlobalContext.ItemsInCartPopUpCookieExpirationDays);
	BareNecessities.Cookie.Set("BNItemsInCart", "Supress popup", expiration_days, true);
	var delay=0;
	if (BN_PageData.GlobalContext.SessionDurationMilliseconds){
		var SessionDurationMilliseconds=BN_PageData.GlobalContext.SessionDurationMilliseconds;
		if (parseFloat(SessionDurationMilliseconds) > parseFloat(delay)) delay=0;
		else delay=parseFloat(delay) - parseFloat(SessionDurationMilliseconds);
	}
	BareNecessities.BillboardPopup.timer_is_on=true;
	BareNecessities.BillboardPopup.t=setTimeout("BareNecessities.display_BillboardPopup_lightbox()", delay);
};
BareNecessities.BillboardPopup.CookieExists = function(){
	var BNBillboardPopup = BareNecessities.Cookie.Get("BNBillboardPopup");
	if (isEmpty(BNBillboardPopup)) return false;
	return true;
};
$(document).ready(function(){
	if (!BareNecessities.BillboardPopup.CookieExists()) BareNecessities.BillboardPopup.ExecutePopUpLightbox();
	else
		if (BareNecessities.GlobalContext.BillboardSecondPopupEnabled)
			if (BareNecessities.GlobalContext.BillboardSecondPopupEnabled.toLowerCase() == "true" && !BareNecessities.BillboardPopup.SecondBillboard.CookieExists())
				if (BareNecessities.GlobalContext.BillboardSecondPopupCountdown)
					BareNecessities.BillboardPopup.SecondBillboard.Setup();
});
BareNecessities.display_BillboardPopup_lightbox = function(){
	if (BareNecessities.BillboardPopup.timer_is_on){
		BareNecessities.BillboardPopup.timer_is_on=false;
		BareNecessities.Cookie.Delete("BNBillboardSecondPopup");
		var getOrganicBillboard=false;
		var showBillboard=false;
		if (document.referrer != "" && document.referrer.indexOf(".barenecessities.") == -1)
			if (document.location.href.indexOf("?cm_") == -1 && document.location.href.indexOf("&cm_") == -1) 
				getOrganicBillboard=true;
		if (getOrganicBillboard){
		showBillboard=BareNecessities.BillboardPopup.GetDCMContent({"PageType":"BillboardPopup_organic","PageName":"promo","PageSection":"body","IsTiered":false});
			$("#closeContainer1").hide();
			if (!showBillboard) getOrganicBillboard=false;
		}
		if (!getOrganicBillboard){
			$("#closeContainer1").show();
			$("#closeContainer2").hide();
			showBillboard=BareNecessities.BillboardPopup.GetDCMContent({"PageType":"BillboardPopup","PageName":"promo","PageSection":"body","IsTiered":true,"PrependPageNameOnTier":true});
		}
		if (showBillboard){
			var expiration_days = 10;
			if (BN_PageData.GlobalContext.BillboardPopupCookieExpirationDays)
				expiration_days = parseInt(BN_PageData.GlobalContext.BillboardPopupCookieExpirationDays);
			BareNecessities.Cookie.Set("BNBillboardPopup","BNBillboardPopup Lightbox displayed",expiration_days);
			BareNecessities.lightbox.init({ id: "Billboard_lightbox",fadein: true,DoNotRemoveContent:true,DoNotCenterOnScroll:true });
			BareNecessities.BillboardPopup.initEmailSignUp();
		} else {
			BareNecessities.Cookie.SetMinutesToExpire("BNBillboardPopup","BNBillboardPopup lightbox has no content",720);
			if (BareNecessities.GlobalContext.BillboardSecondPopupEnabled.toLowerCase() == "true"){
				BareNecessities.GlobalContext.BillboardSecondPopupCountdown=0;
				BareNecessities.BillboardPopup.SecondBillboard.Setup();
			}
		}
	}
	clearTimeout(BareNecessities.BillboardPopup.t);
};
</script>
<script type="text/javascript">  
	BareNecessities.BillboardPopup.SecondBillboard.CookieExists = function ()   
	{   
		var BNBillboardSecondPopup = BareNecessities.Cookie.Get("BNBillboardSecondPopup");   
		if (isEmpty(BNBillboardSecondPopup))    
		{    
			return false;   
		}   
		return true;  
	};  
	BareNecessities.BillboardPopup.SecondBillboard.Setup = function ()   
	{   
		var delay = parseFloat(BareNecessities.GlobalContext.BillboardSecondPopupCountdown);
		BareNecessities.BillboardPopup.SecondBillboard.timer_is_on = true;   
		BareNecessities.BillboardPopup.SecondBillboard.t = setTimeout("BareNecessities.BillboardPopup.SecondBillboard.Display()", delay); // second delay before showing  
	};  
	BareNecessities.BillboardPopup.SecondBillboard.Display = function ()   
	{   
		if (BareNecessities.BillboardPopup.SecondBillboard.timer_is_on)    
		{    
			BareNecessities.BillboardPopup.SecondBillboard.timer_is_on = false;
			var showBillboard = BareNecessities.BillboardPopup.GetDCMContent({"PageType":"BillboardPopup", "PageName":"promo2", "PageSection":"body", "IsTiered":true, "PrependPageNameOnTier": true});
			if (showBillboard)
			{
				BareNecessities.Cookie.Set("BNBillboardSecondPopup", "BNBillboardSecondPopup Lightbox displayed", 10);    
				BareNecessities.lightbox.init({ id: "Billboard_lightbox", fadein: true, DoNotRemoveContent: true, DoNotCenterOnScroll: true });    
				$("#billboard_anchor").hide();    
				$("#second_billboard_anchor").show();    
				BareNecessities.BillboardPopup.resetCountDownForSecondBillboard();
			} 
			else 
			{
				BareNecessities.Cookie.SetMinutesToExpire("BNBillboardSecondPopup", "BNBillboardSecondPopup lightbox has no content", 720); // expire in 12 hours
				//BareNecessities.BillboardPopup.resetCountDownForSecondBillboard();		
			}
		}   
		clearTimeout(BareNecessities.BillboardPopup.SecondBillboard.t);   
	};  
	BareNecessities.BillboardPopup.SecondBillboard.hide = function ()   
	{      
		BareNecessities.lightbox.hide({id:"Billboard_lightbox", fadein: true, DoNotRemoveContent: true });          
	};  
	BareNecessities.BillboardPopup.resetCountDownForSecondBillboard = function ()
	{
		var Reset_Countdown_Url = "http://www.barenecessities.com/ajaxservice.aspx";
		$.ajax({
			url: Reset_Countdown_Url,
			cache: false,
			async: true,
			type: "GET",
			data: "req=bb2end",
			error: function (xhr, status)
			{
				alert("Our apologies...We are experiencing a technical issue with this page\n\n\n" + xhr.status + "\nxhr.responseText = " + xhr.responseText)
			},
			success: function (responseText)
			{
			}
		});
	};
</script><script type="text/javascript">
$(document).ready(function ()
{
	if (!BareNecessities.BillboardPopup.Executing) {
		var delay = 0;
		if (BareNecessities.items_in_basket.ExecutePopUpLightbox()) {
			if (BN_PageData.GlobalContext.ItemsInCartPopUpDelay) {
				delay = parseFloat(BN_PageData.GlobalContext.ItemsInCartPopUpDelay);
			} 
			if (BN_PageData.GlobalContext.SessionDurationMilliseconds) {
				var SessionDurationMilliseconds = BN_PageData.GlobalContext.SessionDurationMilliseconds;
				if (parseFloat(SessionDurationMilliseconds) > parseFloat(delay)) {
					delay = 0;
				}  else {       
					delay = parseFloat(delay) - parseFloat(SessionDurationMilliseconds);
				}
			}
			BareNecessities.items_in_basket.timer_is_on = true;
			BareNecessities.items_in_basket.t = setTimeout("BareNecessities.items_in_basket.display_lightbox()", delay); // second delay before showing
		}
	}
});     
BareNecessities.items_in_basket.display_lightbox = function () {
	if (BareNecessities.items_in_basket.timer_is_on) {     
		var expiration_days = 1;
		BareNecessities.items_in_basket.timer_is_on = false;
		if (BN_PageData.GlobalContext.ItemsInCartPopUpCookieExpirationDays) {
			expiration_days = parseInt(BN_PageData.GlobalContext.ItemsInCartPopUpCookieExpirationDays);
		}
		BareNecessities.Cookie.Set("BNItemsInCart", "Supress popup", expiration_days, true);
		//alert("Display items in basket popup lightbox");
		BareNecessities.lightbox.init({ id: "items_in_cart_popup_lightbox", fadein: true, DoNotRemoveContent: true, DoNotCenterOnScroll: true });
	}
	clearTimeout(BareNecessities.items_in_basket.t);
};
BareNecessities.items_in_basket.hide_lightbox = function () {
	cmCreateConversionEventTag("CartReminder","1","Close");
	BareNecessities.lightbox.hide({ id: "items_in_cart_popup_lightbox", fadein: true });
};
</script><script type="text/javascript">
BareNecessities.BillboardPopup.initEmailSignUp = function (){
    if ($("#billboard_email_signup_txt").length){
		$("#billboard_email_signup_txt").keyup(function(event){
			if(event.keyCode == 13){
				$("#EmailHidden").focus();
				$("#billboard_email_signup_btn").click();
				$("#billboard_email_signup_txt").focus();
			}
		});
	}
};
BareNecessities.BillboardPopup.email_signup = function (source, cmEventID, cmActionType, cmCategoryId){
	if (!source || isEmpty(source)){
		source = "EmailCaptureLightbox";
	}
    var email_signup_txt = $("#billboard_email_signup_txt").val();
    var err = false;
    BareNecessities.email_signup.ErrorMessagesInit();
    var error_msg;
    if (isEmpty(email_signup_txt)){
        err = true;
        error_msg = "Please provide your email address.";
    } else if (!checkEmailFormat(email_signup_txt, false)) {
        err = true;
        error_msg = "Please make sure the \"@\" and \".\" are in the right locations.";
    }
    if (err){
        $("#errMsgText").html(error_msg);
        $("#errMsgTextContainer").show();
    } else {
        var loginUrl = "/secure/MembershipService.aspx";
        $.ajax({
            url: loginUrl,
            cache: false,
            async: false,
            type: "POST",
            data: "cmd=captureemail&target=barenecessities&output=json&source="+source+"&email=" + email_signup_txt,
            error: function (xhr, status) {
                $("#BillboardContent").hide();
                $("#technical_issues").show();
            },
            success: function (responseText) {
                var registration = responseText;
                if (registration.status == "failed"){
					$("#BillboardContent").hide();
					$("#technical_issues").show();
                } else {
					if (!isEmpty(cmEventID) && !isEmpty(cmActionType) && !isEmpty(cmCategoryId)){
						cmCreateConversionEventTag(cmEventID,cmActionType,cmCategoryId);
					}
					$("#BillboardContent").html($("#emailBillboardSignupThankYou_container").fadeIn());
					$("#billboard_anchor").html($("#emailBillboardSignupClose_container").show());
				}
            }
        });
    }
};
BareNecessities.email_signup.ErrorMessagesInit = function (){
	$("#errMsgText").text("");
	$("#errMsgTextContainer").hide();
};
BareNecessities.BillboardPopup.show_privacy_policy = function (){
	cmCreateConversionEventTag("RegPopUp","1","PrivPol");
	openCenteredWindow('http://www.barenecessities.com/PrivacyViewPopup.aspx', 'popup', 600, 600, '','yes');        
};
</script>
