    <script type="text/javascript">
        if (!BareNecessities.items_in_basket) {
            BareNecessities.items_in_basket = {};
            BareNecessities.items_in_basket.number_of_items = 0;
        }
        callServer("?req=iib", true, function (itemsInBasket) {
            if (itemsInBasket > "") {
                var itemsInCart = document.getElementById("ItemsInCartLabel");
                if (itemsInCart) {
                    itemsInCart.innerText = itemsInBasket;
                    itemsInBasket = parseInt(itemsInBasket);
                    BareNecessities.items_in_basket.number_of_items = parseInt(itemsInBasket);                    
                    if (itemsInBasket != 1) {
                        $("#PluralTextSpan").text("s");
                    }
                    else {
                        $("#PluralTextSpan").text("");
                    }
                }
            }
        });
        $(document).ready(function ()
        {
            if (!BareNecessities.touchEventsExist)
            {
                $("#SignInOutContainer").unbind("mouseover").unbind('mouseout')
                .mouseover(function ()
                {
                    displayDropdown($(this));
                })
                .mouseout(function ()
                {
                    hideDropdown($(this));
                });
            }
        });
        function displayDropdown(obj)
        {
            if (!BareNecessities.touchEventsExist)
            {
                var dropDown = obj.data("dropdown");
                var height = obj.outerHeight(true);
                var offset = obj.offset();
                var newLeft = offset.left - ((parseInt($("#" + dropDown).outerWidth(true)) / 2) - (parseInt($("#SignInOutLink").outerWidth(true)) / 2));
                $("#" + dropDown).css("top", height);
                $("#" + dropDown).css("left", newLeft);
                $("#" + dropDown).show();
                $("#" + dropDown).unbind("mouseover").unbind('mouseout')
                .mouseover(function ()
                {
                    $("#" + dropDown).show();
                })
                .mouseout(function ()
                {
                    $("#" + dropDown).hide();
                });
            }
        }
        function hideDropdown(obj)
        {
            if (!BareNecessities.touchEventsExist)
            {
                var dropDown = obj.data("dropdown");
                $("#" + dropDown).hide();
            }
        }
    </script>   

</div>



   


        
        <div id="global2_1" onclick="hideDropdown($('#SignInOutContainer'));">
            
        </div>

