	BareNecessities.ShopRunner.UpdateBannerContent = function() {
       var html = $("#sr_placeholderSource").html();
        for (var i = 0; i < BannerContent.length; i++) {
            var content = BannerContent[i];
            if (content.indexOf("sr_placeholderTarget") > -1) {
                BannerContent[i] = html;
            }
        }
    }
    setTimeout(BareNecessities.ShopRunner.UpdateBannerContent, 5000); 
</script>
