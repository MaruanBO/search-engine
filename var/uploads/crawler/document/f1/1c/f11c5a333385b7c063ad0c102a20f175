	<script language="javascript" type="text/javascript">
		function handleSignupEmail(textboxId) {
			var elem = document.getElementById(textboxId);
			if (elem != null) {
				if (elem.value.length == 0)
					document.location.href = "http://www.barenecessities.com/signup.aspx?email=" + elem.value;
				else if (checkEmailFormat(elem.value)) {
					document.location.href = "http://www.barenecessities.com/signup.aspx?email=" + elem.value;
				}
			}
		}
	</script>

    
