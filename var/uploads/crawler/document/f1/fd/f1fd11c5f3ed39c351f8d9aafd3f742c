(function($){$(function(){
    var num = '0x81d1ff', 
        forms = document.getElementsByTagName("form");

    function markAsHuman(e) {
        $this = $(this);
        $this.find("#imahuman").attr('value', parseInt(num, 16));
    }

    for (var i=0; i < forms.length; i++) {
        var form = $(forms[i]);
        if(form.hasClass('enable-captcha')) {
            form.append('<input type="hidden" name="imahuman" id="imahuman" value="0" />');
            form.on('focus', markAsHuman);
            form.on('click', markAsHuman);
        }
    }
});}(jQuery));
</script>

