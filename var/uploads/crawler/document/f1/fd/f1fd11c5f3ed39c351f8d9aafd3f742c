<script type='text/javascript'>
	jQuery(document).ready(function($){
		$('#sliderWrapper').addClass('js-on');
		var slider = $('#slider').bxSlider({
			mode: 'horizontal',
			infiniteLoop: true,
			prevText: '',
			nextText: '',
			displaySlideQty: 5,
			pager: true,
			speed: 600,
			auto: true,
			pause: 6000,
			onBeforeSlide: function(currentSlideNumber, totalSlideQty, currentSlideHtmlObject){
				$('#slider li').removeClass('active').fadeTo(300, 0.3);
				currentSlideHtmlObject.addClass('active');
				$('#slider li.active').fadeTo(500, 1);
				$('.slideContent').fadeTo(200, 0);
				$('#slider li.active .slideContent').fadeTo(900, 1);
			$('h2.captionText').each(function() {
					$(this).fadeOut(200);
				});
		},
			onAfterSlide: function(currentSlideNumber, totalSlideQty, currentSlideHtmlObject){
			$('li.active h2.captionText').each(function() {
					$(this).fadeIn(400);
				});
		}
		});
		window.bxSlider = slider;
		$('#slider li').not('.active').fadeTo(0, 0.3);
		$('#slider li.active').fadeTo(500, 1);
		$('.slideContent').fadeTo(0, 0);
		$('#slider li.active .slideContent').fadeTo(0, 1);
		$('#slider').swipe({
			swipeLeft: function() { slider.goToNextSlide(); },
			swipeRight: function() { slider.goToPreviousSlide(); },
			allowPageScroll: 'auto'
		});
	  });
  </script>
