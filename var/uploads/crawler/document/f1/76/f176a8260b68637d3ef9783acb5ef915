	var maintop = $('div#main-top div'); var mainseries = $('div#main-top ul li'); var red_arrow = $('div#main-top ul li.current');
	mainseries.hover(
		function(){ var o = $(this); var idx=o.index();mainseries.removeClass('on');
			switch(idx){
				case 1: red_arrow.css({'left':'40px','top':'-11px'}); o.addClass('on'); break;
				case 2: red_arrow.css({'left':'149px','top':'-11px'}); o.addClass('on'); break;
