// menu display control
$(window).scroll(function() {
	if($(document).scrollTop()>$('.static_menu_position').position().top) {
		$('.static_menu').css({'position':'fixed','zIndex':100,'top':'0','width':'100%','left':'0px'});
		$('.static_menu').removeClass('nav').addClass('nav2');
	} else {
		$('.static_menu').css({'position':'relative'});
		$('.static_menu').removeClass('nav2').addClass('nav');
	}
});
</script>
