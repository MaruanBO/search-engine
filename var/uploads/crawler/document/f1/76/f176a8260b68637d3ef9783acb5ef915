				case 4: red_arrow.css({'left':'367px','top':'-11px'}); o.addClass('on'); break;
			}
		},function(){}
	); mainseries.click(function(){var o = $(this); var idx=o.index();	maintop.addClass('doff').eq(idx-1).removeClass('doff');});
