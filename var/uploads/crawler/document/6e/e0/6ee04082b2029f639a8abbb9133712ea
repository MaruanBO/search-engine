function layer(in_ID, On,scroll)
{
 if(document.layers)
 {
    document.layers[in_ID].visibility = On ? "show" : "hide";
 }
 else if(document.getElementById)
 {
     var obj = document.getElementById(in_ID);
     obj.style.visibility = On ? "visible" : "hidden";
 }
 else if(document.all)
 {
     document.all[in_ID].style.visibility = On ? "visible" : "hidden";
 }

 if(scroll){
  window.scrollTo(0, 0);
 }
}
