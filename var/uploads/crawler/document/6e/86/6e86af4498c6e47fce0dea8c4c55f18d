function preCheckThecentredailytimes101198() {
  // check that we haven't just come from welcome page
  if ( window.location.href.indexOf('rh=1') > -1 ) {
    return 0;
  }
  // check if this is free student access
  if ( document.cookie.indexOf('mi_student_access') > -1 ) {
    return 0;
  }
  return 1;
}
</script>
