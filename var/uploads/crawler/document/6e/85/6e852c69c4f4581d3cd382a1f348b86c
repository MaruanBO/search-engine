<script>
var loggedInUser = '';
var userAlreadySet = 'false';
var holdBannerDetails = "";

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
    }
    return "";
}
	loggedInUser = getCookie("LOGGEDIN_USER_NAME");
	var networkVal;
hello.on('auth.login', function(r){
	// Get Profile
	networkVal = r.network;
	hello.api(r.network+':/me', function(p){
	var showLogin = 'true';
	loggedInUser = p.name;	
	if(r.network!=null || p.name!=null){
		showLogin = 'false';
	}
	if(showLogin == 'false'){
		if(document.getElementById('userLoginForComments')!= null){
			document.getElementById('userLoginForComments').style.display = "none";
		}	
		if(document.getElementById('leaveUrComment')!= null){
			document.getElementById('leaveUrComment').style.display = "block";
		}
		if(document.getElementById('blogNavWithLoginDetails')!= null){
			userAlreadySet = 'true';
			document.getElementById('blogNavWithLoginDetails').innerHTML = document.getElementById('blogNavWithLoginDetails').innerHTML + '<a href="#" style="color:#981d97;text-decoration:underline" onclick="externalUserLogout(networkVal);">'+p.name+' (click to log out)</a>';
		}		
	}else{
		if(document.getElementById('userLoginForComments')!= null){
			document.getElementById('userLoginForComments').style.display = "block";
		}
	}
		});
});

var redirect = window.location.href;
hello.init({
	//'linkedin' : LINKEDIN_CLIENT_ID,
	'facebook' : FACEBOOK_CLIENT_ID,
	//'twitter' : TWITTER_CLIENT_ID,		
	'google' : GOOGLE_CLIENT_ID,
},{
	scope : ['friends','email'],
	redirect_uri: redirect,
	oauth_proxy: OAUTH_PROXY_URL
	
});

$(document).ready(function ()
{	
	linkedInLoginResponse = getCookie("LINKEDIN_USER_NAME");
	
	if(linkedInLoginResponse != null && linkedInLoginResponse != "") {
		loggedInUser = getCookie("LINKEDIN_USER_NAME");
		var networkVal = "linkedin";
		if(document.getElementById('userLoginForComments')!= null){
			document.getElementById('userLoginForComments').style.display = "none";
		}	
		if(document.getElementById('leaveUrComment')!= null){
			document.getElementById('leaveUrComment').style.display = "block";
		}
		if(document.getElementById('blogNavWithLoginDetails')!= null){
			holdBannerDetails = document.getElementById('blogNavWithLoginDetails').innerHTML;
			userAlreadySet = 'true';
			document.getElementById('blogNavWithLoginDetails').innerHTML = document.getElementById('blogNavWithLoginDetails').innerHTML + '<a href="#" style="color:#981d97;text-decoration:underline" onclick="deleteLinkedInCookies();">'+linkedInLoginResponse+' (click to log out)</a>';
		}
	}			
});

function deleteLinkedInCookies() 
{	
	loggedInUser = "";
	var cookies = document.cookie.split(";");
	for (var i = 0; i < cookies.length; i++) 
	{
		var cookie = cookies[i];
		var eqPos = cookie.indexOf("=");
		var name = eqPos > -1 ? cookie.substr(0, eqPos) : cookie;
		document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 GMT";
	}
	setEyeMedCookie("LINKEDIN_USER_NAME","");
	if(document.getElementById("userLoginForComments") != null){
		document.getElementById('userLoginForComments').style.display = "block";
	}	
	if(document.getElementById("leaveUrComment") != null){
		document.getElementById('leaveUrComment').style.display = "none";
	}
	
	if(document.getElementById('blogNavWithLoginDetails')!= null){
		//document.getElementById('blogNavWithLoginDetails').style.display = "none";
		document.getElementById('blogNavWithLoginDetails').innerHTML = holdBannerDetails; //document.getElementById('blogNavWithLoginDetails').innerHTML + '<a href="#" style="color:#981d97;text-decoration:underline" onclick="internalUserLogout()">'+loggedInUser+' (click to log out)</a>';
	}
	location.reload();
}

$(document).ready(function ()
{	
	twitterLoginResponse = getCookie("TWITTER_USER_NAME");
	
	if(twitterLoginResponse != null && twitterLoginResponse != "") {
		loggedInUser = getCookie("TWITTER_USER_NAME");
		var networkVal = "linkedin";
		if(document.getElementById('userLoginForComments')!= null){
			document.getElementById('userLoginForComments').style.display = "none";
		}	
		if(document.getElementById('leaveUrComment')!= null){
			document.getElementById('leaveUrComment').style.display = "block";
		}
		if(document.getElementById('blogNavWithLoginDetails')!= null){
			holdBannerDetails = document.getElementById('blogNavWithLoginDetails').innerHTML;
			userAlreadySet = 'true';
			document.getElementById('blogNavWithLoginDetails').innerHTML = document.getElementById('blogNavWithLoginDetails').innerHTML + '<a href="#" style="color:#981d97;text-decoration:underline" onclick="deleteTwitterCookies();">'+twitterLoginResponse+' (click to log out)</a>';
		}
	}			
});

function deleteTwitterCookies() 
{
	loggedInUser = "";
	var cookies = document.cookie.split(";");
	for (var i = 0; i < cookies.length; i++) 
	{
		var cookie = cookies[i];
		var eqPos = cookie.indexOf("=");
		var name = eqPos > -1 ? cookie.substr(0, eqPos) : cookie;
		document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 GMT";
	}
	setEyeMedCookie("TWITTER_USER_NAME","");
	if(document.getElementById("userLoginForComments") != null){
		document.getElementById('userLoginForComments').style.display = "block";
	}	
	if(document.getElementById("leaveUrComment") != null){
		document.getElementById('leaveUrComment').style.display = "none";
	}
	
	if(document.getElementById('blogNavWithLoginDetails')!= null){
		//document.getElementById('blogNavWithLoginDetails').style.display = "none";
		document.getElementById('blogNavWithLoginDetails').innerHTML = holdBannerDetails; //document.getElementById('blogNavWithLoginDetails').innerHTML + '<a href="#" style="color:#981d97;text-decoration:underline" onclick="internalUserLogout()">'+loggedInUser+' (click to log out)</a>';
	}
	
	window.open("/sign-in-with-twitter/signout.jsp","",500,500);
	
	//location.reload();
}

