        var index = 0;
        var rotate = true;
        function rotateFeatures() {
            if (!rotate)
                return;

            switch (index) {
                case 0:
                    showFeature1();
                    index++;
                    break;
                case 1:
                    showFeature2();
                    index++;
                    break;
                case 2:
                    showFeature3();
                    index++;
                    break;
                case 3:
                    showFeature4();
                    index++;
                    break;
            }

            if (index == 4)
                index = 0;

            if (rotate)
                setTimeout(rotateFeatures, 8000);
        }  
    </script>


    
