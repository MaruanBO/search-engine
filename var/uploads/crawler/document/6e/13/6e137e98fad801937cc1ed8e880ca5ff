    <script type="text/javascript">
        var validationFlag = true;

        function scrollUp() {
            loc = new String(document.location);
            if (loc.endsWith("#login"))
                document.location = loc;
            else
                document.location = loc + "#login";

            var el = document.getElementById("ctl00_ctl00_LoginForm1_ctl01_txtUsername");
            if (el == null)
                el = document.getElementById("ctl00_ctl00_ctl00_LoginForm1_ctl01_txtUsername");

            el.focus();
        }

        function checkSearchString(searchString)
        {
                      
            return true;
        }

        function searchBtnPressed() {
            validationFlag = false;
            var el = document.getElementById('ctl00_ctl00_txtSearch')
            if (el == null)
                el = document.getElementById('ctl00_ctl00_ctl00_txtSearch')

            return checkSearchString(el.value);
        }
