<!--  Build placeholder attribute for IE. Before IE11 the placeholder attribute did not work in IE11-->
 <!--[if IE]>
<script type="text/javascript">

function add_placeholder (id, placeholder)
{
	var el = document.getElementById(id);
	el.placeholder = placeholder;
 
    el.onfocus = function ()
    {
		if(this.value == this.placeholder)
		{
			this.value = '';
			el.style.cssText  = '';
		}
    };
 
    el.onblur = function ()
    {
		if(this.value.length == 0)
		{
			this.value = this.placeholder;
			el.style.cssText = 'color:#A9A9A9;';
		}
    };
 
	el.onblur();
}
 
// Add right before </body> or inside a DOMReady wrapper
add_placeholder('first_name', 'First Name');
add_placeholder('last_name', 'Last Name');
add_placeholder('company', 'Company');
add_placeholder('phone', 'Phone'); 
add_placeholder('email', 'Email'); ; 
</script>
<![endif]-->

<!-- Validation Script - Checks to see if required fields are filled out, if not gives user an ale-->
        <script type='text/javascript'>
            function ValidateForm() {
                var helperMsg = '';
        
                 if (document.getElementById('first_name').value.length == 0 || document.getElementById('first_name').value == 'First Name') {
                    helperMsg += "Please enter First Name \n\r";
                }
                if (document.getElementById('last_name').value.length == 0 || document.getElementById('last_name').value == 'Last Name') {
                    helperMsg += "Please enter Last Name \n\r";
                }
                if (document.getElementById('email').value.length == 0 || document.getElementById('email').value == 'Email') {
                    helperMsg += "Please enter valid Email Address \n\r";
                }     
                if (document.getElementById('phone').value.length == 0 || document.getElementById('phone').value == 'Phone') {
                    helperMsg += "Please enter Phone \n\r";
                }      
                 if (document.getElementById('company').value.length == 0  || document.getElementById('company').value == 'Company') {
                    helperMsg += "Please enter Company name \n\r";
                }
              
		if(document.getElementById('00N60000002aIou').value == "") {
   		    helperMsg += "Please enter how many emails you plan to send \n\r";
		}
               
                 if (helperMsg.length > 0) {
                    alert(helperMsg);
                    return false;
                }
                return true;
            }
            </script> 
