<script type="text/javascript">

	function getParameterByName(name, str)
	{
		name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
		var regexS = "[\\?&]" + name + "=([^&#]*)";
		var regex = new RegExp(regexS);
		var results = regex.exec(str);
		if(results == null){
			return "";
		} else {
			return decodeURIComponent(results[1].replace(/\+/g, " "));
		}
	}


	jQuery('ul#lang_sel_list').find('a').each(function(){
		var q = getParameterByName("lang",jQuery(this).attr('href') );
		//if(q == "") {q = "en";}
		jQuery(this).attr('href',"http://www.easton.com/"+ q);

				jQuery(this).click(function() {
			var expires = new Date();
			var time = expires.getTime();
			time += (3600 * 12) * 1000;
			expires.setTime(time);

			document.cookie = "easton_lang=" + q + "; expires=" + expires.toGMTString() + "; path=/";
		});
	});

   jQuery('.custom.radio').on('click',function(){
      jQuery('.zoom').attr('src', jQuery(this).attr('data-href'));
      jQuery('.zoom').attr('data-zoom', jQuery(this).attr('data-zoom'));
      jQuery('.magnifyingFrame').css('background-image', 'url(' + jQuery(this).attr('data-href') + ')');
   });
   
jQuery(function(){
		jQuery('.date').datepicker({
		  dateFormat: 'mm/dd/yy',
		  showOn: "button",
		  buttonImage: "http://www.easton.com/skin/frontend/base/default/images/calendar-icon.gif",
		  yearRange:"1916:2014",
		  defaultDate:"-1y",
		  changeMonth: true,
		  changeYear: true,
		  buttonImageOnly: true
		});
  });
function addEmailNewsletter() {
  var email = jQuery('#newsletter').val().trim();
  if (email) {
    if (email.match(/\S+@\S+/)) {
      //alert(loc);
      jQuery(".bronto-newsletter #newsletter-email").val(email);
      jQuery("#bronto-newsletter-modal").reveal();
    }
    else {
       alert("Please enter a valid e-mail address to sign up!");
    }
  }
  else {
    alert("Please enter an e-mail address to sign up!");
  }
}
</script>

