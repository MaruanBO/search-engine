<script type="text/javascript">
//CJ pixel variables
	var cj_category    = "baseball";
	var cj_subcategory = "";
	var cj_userid = "";

	//if sku is undefined, set a blank value
	if (typeof cj_SKU == 'undefined'){
		var cj_SKU = '';
	}

	//if we're on the shopping cart and there is no sku, also clear the category and subcategory
        if ( (typeof cj_cart != 'undefined') && (cj_cart) && (cj_SKU=='') ){
                cj_category = '';
                cj_subcategory = '';
	}
</script>

