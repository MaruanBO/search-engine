        <section class="ib-container-item first item-0">
                        <!-- all times in 24-hour – so 1530 is 3:30, 900 is 9 a.m. -->
<script type="text/javascript">
var d = new Date();
var m = d.getMinutes();
var h = d.getHours() * 100;
var day = d.getDay(); //0 = Sun, 1 = Mon, 2 = Tues, etc.
var currtime = h + m;
window.onload = function() { //display banner on load if time checks out
 
//change all instances of ‘link’ to direct to your livestream page
/* if (currtime >= 900 && currtime <= 2300)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Testing the banner</a>"; } uncomment and change the time to test the banner*/
 
if (day >= 1 && day <= 5) { // if Monday through Friday
if (currtime >= 500 && currtime <= 700) //’if between 5am and 7am Monday through Friday’ – change for your show times
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 This Morning</a>"; } //change show title
else if (currtime >= 1159 && currtime <= 1229) //noon
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at Noon</a>"; }
else if (currtime >= 1700 && currtime <= 1730)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 5 p.m.</a>"; }
else if (currtime >= 1730 && currtime <= 1759)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 5:30 p.m.</a>"; }
else if (currtime >= 1759 && currtime <= 1830)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 6 p.m.</a>"; }
else if (currtime >= 2300 && currtime <= 2335)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 11 p.m.</a>"; }
}

if (day == 6 || day == 0) { //if Saturday & Sunday
if (currtime >= 700 && currtime <= 800)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 This Morning</a>"; }
else if (currtime >= 1800 && currtime <= 1830)
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 6 p.m.</a>"; }
}
else if (currtime >= 2300 && currtime <= 2330) 
{ bannerAdd.innerHTML = "<a href='/live-stream'><span class='liveNow'>Live Now</span> Click here to watch NewsChannel 12 at 11 p.m.</a>"; }
}
</script>
