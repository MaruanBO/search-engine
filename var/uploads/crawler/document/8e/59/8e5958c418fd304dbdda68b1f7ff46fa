<script type="text/javascript">
remove_nav_banner = true;
jQuery(document).ready(function(){
    //setTimeout ( "slideshow_startup()", 500 );
    what_gets_clicked();
});

function what_gets_clicked() {
   areas = [
      'dontmiss',
      'slideshow',
      'best',
      'rau',
      'signed-editions',
      'new-arrivals',
      'indiespensable',
      'google-ebooks',
      'bestys',
      'authorpod',
      'contest',
      'nav'
   ];
   jQuery.each(areas, function(index, value) {
      jQuery("#" + value + " a").bind('click', function(){
         area = value;
         link_type = (jQuery(this).attr('rel')) ? jQuery(this).attr('rel') : null ;
         TrackEvent("Home-Page-What-Gets-Clicked",area,link_type,0,true);
      });
   });
}
</script>

