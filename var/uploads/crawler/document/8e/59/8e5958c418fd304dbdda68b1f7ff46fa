        <tr>
          <td colspan='2' id='slideshow-cell'>       
            <div id='slideshow-wrapper'>
              <div id='slideshow' style='width:100%; height:215px;'>
                <!--<pre>-->
<!-- FILE: created Tue Feb 17 15:50:51 2015 -->
<script type='text/javascript'>

  // Slideshow-Specific Vars
  var slide_i = 0;
  var reset_var = 0;
  var slide_time = 8000;      // how long between slides (1000 = 1sec)
  var slide_pos = 0;          // slideshow starter position
  var fade_speed = 200;       // how quickly to fade in/out
  var next_slide = 1;         // starting out, slideshow goes up (i.e. 1,2,3,n)
  var periodical = null;      // initialize
  var slideshow = {};
  var gmt_diff = 7;

  function time_calc() {
    var currentTime  = new Date();
    current_time = Math.round(currentTime.getTime()/1000) - (60*60*gmt_diff);
  }

  // Pulls The list of slideshow images from the var 'list.'
  function get_slideshow_image_data() {
  time_calc();
  tmp = list.slides;
  slideshow.slides = new Array();
  for(var i in tmp) {
    exp_time = tmp[i].exp;
    if (current_time < exp_time) {
      slideshow.slides.push(tmp[i]);
      jQuery(document).append("<img src='" + tmp[i].img + "' />");
    }
  }
  //console.log(slideshow);
  return false;
  }

  // Flips Our Display Settings
  function hide_show(hide_el, show_el){
    jQuery(hide_el).css({ display: "none" });
    jQuery(show_el).css({ display: "" });
    return false;
  }

  function new_slide(n) {

    // Set Our New Slide Position
    slide_pos = slide_pos + n;  // p='position'   n='now'

    // Check For Edge Cases
    if (slide_pos >= slideshow.slides.length) {
	slide_pos = 0;
    } else if (slide_pos < 0) {
	slide_pos = slideshow.slides.length - 1;
    }

    // Grab Our New Values
    var link = slideshow.slides[slide_pos].link;
    var rel = slideshow.slides[slide_pos].rel;
    var image = slideshow.slides[slide_pos].img;

    // And Display
    if (periodical !== null) {
    //jQuery("#slideshow img").attr("src","/images/clearpixel.gif");
	jQuery('#slideshow img').fadeOut(1, function(){
	    jQuery("#slideshow img").fadeIn(fade_speed);
		jQuery("#slideshow a").attr("href", link);
		jQuery("#slideshow a").attr("rel", rel);
		jQuery("#slideshow img").attr("src", image);
		
	    }) ;
    }
    jQuery('#position').html((slide_pos + 1) + "/" + slideshow.slides.length);
  }

  function slideshow_setup() {
    // Read In The Array Of Slideshow Images
    get_slideshow_image_data();
    // Start With The First Slide
    new_slide(0);
    // Event Handlers
    jQuery('#play').click ( function() {
	    // $clear(periodical);     // keep the function from calling itself multiple times  (JMS: WTF is this???)
	    hide_show(jQuery('#play'),jQuery('#stop'));
	    next_slide = 1;
	    periodical = setInterval(
	       function() { 
	          new_slide(next_slide); 
	          if (slide_i == slideshow.slides.length) {
	             slide_i = 0;
	             reset_var = 1;
	             slideshow_setup();
	          };
	          //console.log('L: %s S:%s', slideshow.slides.length, slide_i);
	          slide_i++;
	       }, slide_time
	    );
	});
    jQuery('#prev').click ( function() {
	    next_slide = -1;
	    new_slide(next_slide);
	    clearInterval(periodical); 
	    hide_show(jQuery('#stop'),jQuery('#play'));
	});
    jQuery('#next').click ( function() {
	    next_slide = 1;
	    new_slide(next_slide);
	    clearInterval(periodical); 
	    hide_show(jQuery('#stop'),jQuery('#play'));
	});
    jQuery('#stop').click ( function() {
	    clearInterval(periodical); 
	    hide_show(jQuery('#stop'),jQuery('#play'));
	});
    return false;
  }

  function slideshow_startup_2() {
    // NOTE FROM JMS: There used to be a minor delay in here for Safari.
    // Not sure if that's still needed.  Keep it in mind.
    //setTimeout ( "slideshow_setup()", 200 );
    slideshow_setup();
    jQuery('#menu').css({ display: 'block' });
    jQuery('#play').trigger('click'); // starts the slideshow...
    return false;
  }

  var list = {
