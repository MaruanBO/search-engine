</div><div id="bCal" class="widget w-calendar">
   <h2 class="w-name"><a href="javascript:void(0);">
         USF Calendar
         </a></h2>
   <p id="calLoading" style="margin:100px auto; width:180px;"><img src="//www.usf.edu/_resources/images/global/elements/loading.jpg" alt="Loading"></p>
</div><script type="text/javascript">
				
					
					  $(function () {
      
      buildBedeWorksCal('USF Calendar','http://webcal.forest.usf.edu/cal/main/showMain.rdo?utm_source=calendar-widget-headline&utm_medium=widget&utm_campaign=usfhomepage','4'); 
    });

			 	</script><script type="text/javascript">
			   function buildBedeWorksCal(calTitle,calHeadURL,numItems) {
      //var feedURL = servicesURL + "Menus.asmx/sportsMenu";
      var feedURL = "/_resources/csharp/BedeworksJSON.aspx";
      var strURLFunction = "";
      var ajaxRequest = $.ajax({
        url: feedURL,
        dataType: "json",
			timeout:3500,
        data: {}
      });

      ajaxRequest.done(function (dates) {
        //var arrResults = $.parseJSON(msg.d);
        var arrEvents = dates.bwEventList.events;
        var events = document.getElementById("bCal");

        if (events.childNodes.length > 0) {
          events.removeChild(events.childNodes[0]);
        }

        // create a new unordered list
        var ul = document.createElement('ul');
        ul.className = "cal-events";
        for (dateItem in arrEvents) {
          test = 0;
          if (dateItem > (numItems - 1)) {
            break;
          }
          var calURL = "http://webcal.forest.usf.edu/cal/event/eventView.do?b%3Dde%26calPath%3D" + arrEvents[dateItem].calPath + "%26guid%3D" + arrEvents[dateItem].guid + "%26recurrenceId%3D" + arrEvents[dateItem].recurrenceId;
          calURL = decodeURIComponent(calURL);
          //Take link data and build li
          li = usfDateLi(arrEvents[dateItem].summary, calURL, formatGCalTime(arrEvents[dateItem].start.utcdate), formatGCalTime(arrEvents[dateItem].end.utcdate));
          // append the list item onto the unordered list
          ul.appendChild(li);
          //arrEvents[dateItem].summary
        }
        var calHeader = document.createElement('h2');
        var calHeaderLink = document.createElement('a');
        var loadingElem = document.getElementById('calLoading');
        calHeader.className = "w-name";
        var linkText = document.createTextNode(calTitle);
        calHeaderLink.href = calHeadURL;
        calHeaderLink.appendChild(linkText);
        calHeader.appendChild(calHeaderLink);
        //loadingElem.parentNode.removeChild(loadingElem);
        events.innerHTML = '';
        events.appendChild(calHeader);
        events.appendChild(ul);
      });

      ajaxRequest.fail(function (jqXHR, textStatus) {
                balternateContent("bCal", "/widget/usf-widgets/alternate-content.html");
      });

            function balternateContent(widgetId, alternateContent) {
                $('#' + widgetId).html("");
                $.get(alternateContent, function (data) {
                    $('#' + widgetId).replaceWith(data);
                });
            }
    }
	</script>
