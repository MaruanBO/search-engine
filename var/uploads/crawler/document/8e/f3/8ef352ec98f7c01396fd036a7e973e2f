	// Count Down Initialization
		var tCountdownLabelText = "";
		var endDate = new Date();

  $('.countdown-label span').html(tCountdownLabelText);

  // MONTH IS 0 BASED!!
	$('.countdown').countdown({
		date: endDate,
		render: function(data) {
			$(this.el).html("<div>" + this.leadingZeros((data.days * 24) + data.hours, 1) + " <span>hrs</span></div><div>" + this.leadingZeros(data.min, 2) + " <span>min</span></div><div>" + this.leadingZeros(data.sec, 2) + " <span>sec</span></div>");
		}
	});
</script>
