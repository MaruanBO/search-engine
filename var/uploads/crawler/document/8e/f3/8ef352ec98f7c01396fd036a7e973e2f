	function createCookie(name,value,days) 
	{
		if ( days) 
		{
		  var date = new Date( );
		  date.setTime( date.getTime( )+( days*24*60*60*1000));
		  var expires = "; expires="+date.toGMTString( );
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}

	function readCookie(name) 
	{
		var nameEQ = name + "=";
		var ca = document.cookie.split( ';');
		for( var i=0;i < ca.length;i++) 
		{
		  var c = ca[i];
		  while ( c.charAt( 0)==' ') c = c.substring( 1,c.length);
		  if ( c.indexOf( nameEQ) == 0) return c.substring( nameEQ.length,c.length);
		}
		return null;
	}
  
	 
    function ABTester(testName, splits, bucketNum) {
      if (bucketNum === undefined || bucketNum === 'undefined') bucketNum = 1;
      var cookieName = "";
      for (var i = 0; i < testName.length; i++) { cookieName += "" + testName.charCodeAt(i); }
      cookieName = "abt" + cookieName;      
      var fromEmailA = (document.location.href.indexOf("s=email") > -1 && document.location.href.indexOf("c=re2013-13-A") > -1);
      var fromEmailB = (document.location.href.indexOf("s=email") > -1 && document.location.href.indexOf("c=re2013-13-B") > -1);
      var fromEmailBF = (document.location.href.indexOf("s=email") > -1 && document.location.href.indexOf("c=re2013-12") > -1);
      
      var cookieData = readCookie(cookieName);
      var targetSplit;
      if (cookieData == null) {
        targetSplit = Math.floor(Math.random() * splits);
        if (fromEmailA) targetSplit = 2;
        if (fromEmailB) targetSplit = 3;
        if (fromEmailBF) targetSplit = 2;
        createCookie(cookieName, targetSplit, 1);
      } else {
        targetSplit = cookieData;
        
        if (fromEmailA && targetSplit != 2)
        {
		  targetSplit = 2;
          createCookie(cookieName, targetSplit, 1);	
		}
		else if (fromEmailB && targetSplit != 3)
		{
          targetSplit = 3;
          createCookie(cookieName, targetSplit, 1);			
		}
		else if (fromEmailBF && targetSplit != 5)
		{
          targetSplit = 2;
          createCookie(cookieName, targetSplit, 1);			
		}		
        /*
        if (fromEmail && targetSplit != 3) {
          targetSplit = 3;
          createCookie(cookieName, targetSplit, 1);
        }*/
        
      }
      var targetSplitLetter = String.fromCharCode(65 + parseInt(targetSplit));
      _gaq.push(['_setCustomVar',
        bucketNum,
        testName,
        targetSplitLetter,
        2
      ]);

      abTestObjs.push(testName + "_" + targetSplitLetter);
    }

