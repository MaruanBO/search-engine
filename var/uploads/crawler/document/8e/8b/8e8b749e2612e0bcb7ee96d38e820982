Data[slideCounter][2] = '';
slideCounter++;
	var slider296 = new MySlider(Data, 'slider296','imageTitle296','imageSummary296','7', '296');
	/* Start the slider */
	slider296.interval = setInterval("slider296.nextSlide(1)", 5000);
	/* Lazy loading of Images, after 1 second */
	setTimeout("slider296.lazyLoading()", 1000);
</script>


