<script language="javascript">
	var start384 = 0;
	var totalPredictions384 = 29	
	function next384() {
		
		start384 += 6;
		if(start384 >= totalPredictions384) {
			start384 -= 6;
			return false;
		}
		
		
		document.getElementById('loading_container_384').style.visibility="visible";
		$.ajax({
			method:'GET',
			url: "/contents/newHPWidgetAjax/2/"+ start384,
			cache: false,
			success: function(data){
				document.getElementById('loading_container_384').style.visibility="hidden";
				$('#middle_summary_container_384').html(data);
			}
		});
	}
	
	function previous384() {
		if(start384<=0) return false;
		start384 -= 6;
		document.getElementById('loading_container_384').style.visibility="visible";
		$.ajax({
			method:'GET',
			url: "/contents/newHPWidgetAjax/2/"+ start384,
			cache: false,
			success: function(data){
				document.getElementById('loading_container_384').style.visibility="hidden";
				$('#middle_summary_container_384').html(data);
			}
		});
	}
	
	function showExperts384() {
		$('#hpWidgetExpertBlock384').show();
		$('#hpWidgetPredictionBlock384').hide();
		
		
		document.getElementById('showExperts2384').style.display="none";
		document.getElementById('showExperts1384').style.display="block";
	}
	
	function showSummary384() {
		$('#hpWidgetExpertBlock384').hide();
		$('#hpWidgetPredictionBlock384').show();
		
		document.getElementById('showExperts2384').style.display="block";
		document.getElementById('showExperts1384').style.display="none";
	}
</script>


