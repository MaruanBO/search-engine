    <script language="javascript">
        /* Enable News1 Widget */
        function subscribe() {
            if(emailCheck(document.getElementById('UsersSubsEmailId2').value)) {
                var email = document.getElementById('UsersSubsEmailId2').value;
                fcn("http://www.goldderby.com/newsletters/saveFooterNewsLetter/"+email);
                $('#background-window').fadeOut('fast');
                $("#content-window").dialog('destroy');
                return false ; 
            }else{
                alert('Please enter email address ..');
                return false;
            }
        }
            	
        function validateNewsletter() {
            var errors=0;
            if(!errorCheck("UsersSubsEmailId2","Please enter a valid email address")) return false;
            if(document.getElementById('UsersSubsEmailId2').value) {
                if(!echeck(document.getElementById('UsersSubsEmailId2').value,document.getElementById('UsersSubsEmailIdError2'))) {
                    errors=1;
                } else {
                    //document.getElementById('UserSubsEmailIdError').innerHTML="";
                }
            }

            if (errors) {
                document.getElementById('UsersSubsEmailIdSuccess2').innerHTML = "";
                //document.getElementById('UserSubsEmailIdError').innerHTML = "Please enter a valid email address";
                return false;
            } else {
                return true;
            }
        }
            	
        function updateFooterNewsLetter(HTML,errors) {
            if(errors) {
                return false;
            }
            document.getElementById('UsersSubsEmailIdError2').innerHTML = "";
            document.getElementById('UsersSubsEmailIdSuccess2').innerHTML = HTML;
        }
    </script>


