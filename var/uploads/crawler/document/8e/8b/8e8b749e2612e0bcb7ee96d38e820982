<script language="javascript" type="text/javascript">
					
	function clearinput(e){
	if(e.value=='Enter your e-mail address')e.value="";
	}
	
	
	function subscribe1() {
		document.getElementById('EmailError').innerHTML = "";
		document.getElementById('EmailSuccess').innerHTML = "";
		
		if(validateNewsletter1()) {
			document.getElementById('EmailError').innerHTML = "<img src='http://www.goldderby.com/img/myloader.gif'>";
			
			var email = document.getElementById('Email').value;
			fcn("http://www.goldderby.com/newsletters/saveNewsLetter/"+email);
		}
		return false;
	}
	
	function validateNewsletter1() {
		var errors=0;
		if(!errorCheck("Email","Please enter a valid email address")) return false;
		
		if(document.getElementById('Email').value) {
			if(!echeck(document.getElementById('Email').value,document.getElementById('EmailError'))) {
				errors=1;
			} else {
				document.getElementById('EmailError').innerHTML="";
			}
		}

		if (errors) {
			document.getElementById('EmailSuccess').innerHTML = "";
			document.getElementById('EmailError').innerHTML = "Please enter a valid email address";
			return false;
		} else {
			return true;
		}
	}
function updateNewsLetter(HTML,errors) {
	if(errors) {
		return false;
	}
	document.getElementById('EmailError').innerHTML = "";
	document.getElementById('EmailSuccess').innerHTML = HTML;
}
</script>
