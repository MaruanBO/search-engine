<script>var loginStatus="no"</script><script>var partial_path="/";</script><script>var full_path="http://www.goldderby.com//";</script><script>var popups_array_final="";</script><script>var popups_array_denied_url="";</script>    <script language="javascript">
		//Call popup function in popup.js
		$(document).ready(function(){
			var popupsArr	=	Array();
			popupsArr	=	popups_array_final.split(',');
			
			var popupsDeniedUrlArr	=	Array();
			popupsDeniedUrlArr	=	popups_array_denied_url.split(',');
			
			var filteredPopupsArr	=	Array();
			var x=0;
			if((popupsArr.indexOf("likebutton")!=-1) && (popupsArr.indexOf("fblike")!=-1)) {
				if((getTempCookie('likebutton')=='no') && (getTempCookie('fblike')=='no')) {
					filteredPopupsArr[0]	=	'likebutton';
					filteredPopupsArr[1]	=	'fblike';
					x=2;
				} else if(getTempCookie('likebutton')=='no') {
					filteredPopupsArr[0]	=	'likebutton';
					x=1;
				} else if(getTempCookie('fblike')=='no') {
					filteredPopupsArr[0]	=	'fblike';
					x=1;
				}
			} else if(popupsArr.indexOf("likebutton")!=-1) {
				if(getTempCookie('likebutton')=='no') {
					filteredPopupsArr[0]	=	'likebutton';
					x=1;
				}
			} else if(popupsArr.indexOf("fblike")!=-1) {
				if(getTempCookie('fblike')=='no') {
					filteredPopupsArr[0]	=	'fblike';
					x=1;
				}
			}
			
			if(x==0) {
				for(var i=0; i<(popupsArr.length); i++) {
					if(getTempCookie(popupsArr[i])=='no') {
						filteredPopupsArr[x]	=	popupsArr[i];
						x++;
					}
				}
			}
			
			var randomnumber=Math.floor(Math.random()*x);
			
			if(loginStatus=='no' && (filteredPopupsArr.length!=0)) {
				var checkCookie	=	getPoupupCookie(filteredPopupsArr[randomnumber]);
				if(checkCookie=='no') {
					current_url=window.location;
					setTimeout( function() {
						if(info_popup==0){
							if(filteredPopupsArr[randomnumber]=='never_miss_a_post') {
								document.getElementById('floatdivRight').style.display	=	'';
								document.getElementById('floatdivRight').innerHTML	=	document.getElementById('never_miss_a_post').innerHTML;
							} else {
								popup_call(filteredPopupsArr[randomnumber], 'true');
							}
						}
					}, 10000 );
				}
			}
		});
		
		function setCookie(c_name,value,exdays)
		{
			var exdate=new Date();
			exdate.setDate(exdate.getDate() + exdays);
			var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
			document.cookie=c_name + "=" + c_value;
		}
		
		function getPoupupCookie(c_name)
		{
			var i,x,y,ARRcookies=document.cookie.split(";");
			var totalequal	=	0;
			
			for (i=0;i<ARRcookies.length;i++)
			{
				x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			  	y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			  	x=x.replace(/^\s+|\s+$/g,"");
				if (x==c_name)
				{
					totalequal++;
				}
				if(x=='today_popup') {
					totalequal++;
				}
			}
			if (totalequal!=0)
			{
				//return unescape(y);
				return 'yes';
			} else {
				setCookie(c_name,'completed',3);
				setCookie('today_popup','completed',2);
				return 'no';
			}
		}
		
		
		function getTempCookie(c_name)
		{
			var i,x,y,ARRcookies=document.cookie.split(";");
			var totalequal	=	0;
			
			for (i=0;i<ARRcookies.length;i++)
			{
				x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
				y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			  	x=x.replace(/^\s+|\s+$/g,"");
				if (x==c_name)
				{
					totalequal++;
				}
			}
			if (totalequal!=0)
			{
				//return unescape(y);
				return 'yes';
			} else {
				return 'no';
			}
		}
		
		
		
		
        /* Enable News1 Widget */
        try { 
            document.getElementById('banner_area').style.display="block";
        } catch(e) {
		
        };
	
        function newsfielddata(type)
        {
            var fieldValue	=	document.getElementById('UsersSubsEmailId').value;
            if(type=='1')
                if(fieldValue=='Enter your e-mail address') document.getElementById('UsersSubsEmailId').value='';
            if(type=='2')
                if(fieldValue=='') document.getElementById('UsersSubsEmailId').value='Enter your e-mail address';
        }
        function subscribe() {
              $('#UsersSubsEmailIdError').html(null);
              $('#UsersSubsEmailIdSuccess').html(null);
		
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            email=$('#UsersSubsEmailId').val();
            if(email=='Enter your e-mail address' || email==''){
                $('#UsersSubsEmailIdError').show().html('Enter valid email address');
                return false;
            }else if(reg.test(email) == false) {
                $('#UsersSubsEmailIdError').show().html('Enter valid email address');
               
            }
            else{ 
                document.getElementById('UsersSubsEmailIdError').innerHTML = "<img src='http://www.goldderby.com/img/myloader.gif'>";
			
                var email = document.getElementById('UsersSubsEmailId').value;
                $.ajax({
                    type:'POST',
                    url: '/newsletters/save_newsletter_email/',
                    data: 'email='+email,
                    dataType: 'json',
                    success: function (data) {
                        if(data==1){
                            $('#UsersSubsEmailIdError').show().html(null);
                            $('#UsersSubsEmailIdSuccess').show().html('<b>You have successfull subscribed.</b>');
                        } else if(data==2){
                            $('#UsersSubsEmailIdError').show().html('<b>This email address already subscribed.</b>');
                        } else {
                            $('#UsersSubsEmailIdError').show().html('<b>Enter valid email address.</b>');
                        }
                    }
                });  
                
            }
            return false;
        }
	
       
	
        function updateFooterNewsLetter(HTML,errors) {
            if(errors) {
                return false;
            }
            document.getElementById('UsersSubsEmailIdError').innerHTML = "";
            document.getElementById('UsersSubsEmailIdSuccess').innerHTML = HTML;
        }

        function getAllUsers(search_username, pg) {
            document.getElementById('finduserID').innerHTML = "<img src='http://www.goldderby.com/img/myloader.gif'>";
            $.ajax({
                method:'GET',
                url: "/users/findUsersAjax/"+search_username+"/page:"+pg,
                cache: false,
                success: function(HTML){
                    document.getElementById('finduserID').innerHTML = HTML;
                }
            });
            return false;
        }
       
        /* Socal floating Bar */
        if ($("#socialFloatingBar").size() > 0){
            $('#fixed_icons').show();
        }
        
        if ($(".subscribe_login").size() > 0){
            $('.subscribe_login form').each(function(index) {
                form_id='subscribe_login_'+index;
                input_text_id='email_text_'+index;
                $(this).attr('id',form_id);
                $('#'+form_id+' input.sb_input_box[type=text]').attr('id',input_text_id);
                submit_text="<input type='submit' value='Subscribe' onclick='javascript:newsletter_subs("+index+");' class='subscribe_btns'>";
                $('#'+form_id+' input.subscribe_btns[type=submit]').remove();
                $('#'+form_id+' .subcribe_btnsarea').append(submit_text);
                error_div_id='EmailError'+index;
                success_div_id='EmailSuccess'+index;
                next=$('#'+form_id).next();
                next.attr('id',error_div_id);

                next_e=$('#'+error_div_id).next();
                next_e.attr('id',success_div_id);
                $('#EmailError'+index).css("clear","both");
                $('#EmailSuccess'+index).css("clear","both");
            });
        }
        function newsletter_subs(index) {
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
         
            $('#EmailError'+index).html('&nbsp;');
            $('#EmailSuccess'+index).html('&nbsp;');
            email_address=$('#email_text_'+index).val();
            if(reg.test(email_address) == false) {
                $('#EmailError'+index).show().html('Enter valid email address');
               
            }else{
                $.ajax({
                    type:'POST',
                    url: '/newsletters/save_newsletter_email/',
                    data: 'email='+email_address,
                    dataType: 'json',
                    success: function (data) {
                        if(data==1){
                            $('#EmailSuccess'+index).show().html('<b>&nbsp;You have successfull subscribed.</b>');
                        } else if(data==2){
                            $('#EmailError'+index).show().html('<b>&nbsp;This email address already subscribed.</b>');
                        } else {
                            $('#EmailError'+index).show().html('<b>&nbsp;Enter valid email address.</b>');
                        }
                    }
                });    
            }
		
     
            return false;
        }
    </script>
