    function show_user_facebook_info(){    
        if($('#check_user_login').text()==1){
            $('#user_facebook_information').show();
            data=$('#user_facebook_information_hidden').html();
            $('#user_facebook_information').html(data);
            $('#user_facebook_information_hidden').remove();
        }else{
            $('#user_facebook_information').hide();
        }
        show_fb_info=1;
    }

    function set_facebook_options(type){
        if(type==0){
            $.fancybox.close();
        }else{
            $.ajax({
                url: "/opengraph/load_facebook_options/",
                cache: false,
                dataType: 'json',
                success: function(data){
                    $.fancybox.close();
                    if(data==1){
                        $('#facebook_opengraph_text').html('Are you sure want to turn off the facebook options?');
                        $('#opengraph_menu_text').text('Social Off');
                    }else{
                        $('#facebook_opengraph_text').html('Are you sure want to turn on the facebook options?');
                        $('#opengraph_menu_text').text('Social On');
                    }
                }
            });
        }
    }
    function checkCookie()
    {
         $.ajax({
            url: "/opengraph/set_facebookpopup_cookie/",
            cache: false,
            dataType: 'json',
            success: function(data){
                $.fancybox.close();
            }
        });
    }
	
