<script language="javascript">
			$(function() {
			$( "#tabs416" ).tabs();
			document.getElementById('tabs416').style.display="block";
			
			rotateTabs416(0);
		});
		
	
	var totalTabs416 	= '5';
	
	function rotateTabs416(currentTab) {
		if(totalTabs416 <=1 ) return false;
		
		$( "#tabi-416" + (currentTab+1) ).click();
		
		showBottomElement416(currentTab+1);
		
		currentTab ++;
		if(currentTab >= totalTabs416) {
			currentTab = 0;
		}
		
		setTimeout("rotateTabs416("+currentTab+")", 5000);
	}
	
	function showBottomElement416(tab) {
		/* show bottom element for selected tab only */
		for(var i=1; i<=totalTabs416; i++) {
			if(i==tab)document.getElementById('BE416'+i).style.display= "block";
			else document.getElementById('BE416'+i).style.display= "none";
		}
	}
	
</script>
