    function set_facebook_options(type){
        if(type==0){
            $.fancybox.close();
        }else{
            $.ajax({
                url: "/opengraph/load_facebook_options/",
                cache: false,
                dataType: 'json',
                success: function(data){
                    $.fancybox.close();
                    if(data==1){
                        $('#facebook_opengraph_text').html('Are you sure want to turn off the facebook options?');
                        $('#opengraph_menu_text').text('Social Off');
                    }else{
                        $('#facebook_opengraph_text').html('Are you sure want to turn on the facebook options?');
                        $('#opengraph_menu_text').text('Social On');
                    }
                }
            });
        }
    }
   
