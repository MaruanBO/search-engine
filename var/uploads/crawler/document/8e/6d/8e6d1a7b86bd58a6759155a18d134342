<!-- Start: Fundraising Acct -  Universal Analytics / RED -->
<script type="text/javascript">
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-2200333-66', 'diabetes.org');

 var url = document.location.href;
 url = url.toLowerCase();


//Filter URLs
 if ((
(url.indexOf("/site/donation2")  >=0) 
|| (url.indexOf("donations.diabetes.org/site/spageserver?") >=0)
|| (url.indexOf("www.diabetes.org") >=0) 
|| (url.indexOf("/goh/")  >=0) 
)
&&  (!(url.indexOf("fr_id=")>=0 )) 
&&  (!(url.indexOf("NEXTURL=")>=0 ))
)
{
//First clean up and break the main URL 
    var trackingURL;
    var baseURL=[];
    if (url.indexOf(";jsessionid") >= 0) //take out the session id
	{ 
	       url = url.substring(0,url.indexOf(";jsessionid")) + url.substring(url.indexOf("?"));
		
	} 

    if (url.indexOf("#sthash") >= 0) //take out the sthash
	{ 
	       url = url.substring(0,url.indexOf("#sthash"));
		
	} 	 


	if (url.indexOf("?") >= 0)
		{ 
 
        	baseURL = url.split("?");
		} 
	else 
		{	
            baseURL[0] = url;		
		}


		if (url.indexOf("www.diabetes.org") >=0) 
		{
			trackingURL= baseURL[0];
		}
		else 
		{
			//Start the formatting process 
			trackingURL= baseURL[0]+"?"; 
			
	
		var paramsURL =  baseURL[1].split("&");	
		//this is only for dForms -- move inside IF
		for (var i=0;i<paramsURL.length;i++)
		{ 
			if  (paramsURL[i].indexOf("df_id")!=-1){
			trackingURL = trackingURL  +  paramsURL[i]+"&";
                break;
			}
		}
    
		for (var i=0;i<paramsURL.length;i++)
		{ 
			if  (paramsURL[i].indexOf(".donation=")!=-1) { 
			trackingURL = trackingURL  +  paramsURL[i];
                break;
			}
		}	

		for (var i=0;i<paramsURL.length;i++)
		{ 
			if (paramsURL[i].indexOf("pagename")!=-1) {
			trackingURL = trackingURL  +  paramsURL[i];
                break;
			}
		}	
	
		} //End of formatting process for dForms

		if (trackingURL.indexOf("https") >= 0) {
			trackingURL = trackingURL.replace(/^https?:\/\//,'');
		} else { 
			trackingURL = trackingURL.replace(/^http?:\/\//,'');
		}
		
	//end else if there is URL parameter

//Track the reformatted URL as a pageview 
 ga('send','pageview', trackingURL);


 


} //End Tracking condition.
</script>

<!-- End: Fundraising Acct - Universal Tracking/RED -->






