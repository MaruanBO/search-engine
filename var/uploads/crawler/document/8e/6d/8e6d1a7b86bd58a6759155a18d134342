jQuery(document).ready(function($){
	$('#banner-bar .left').click(function() {
		$('#banner-bar').removeClass().addClass('active active-left');
	});
	$('#banner-bar .right').click(function() {
		$('#banner-bar').removeClass().addClass('active active-right');
	});
  /* Enable mobile nav toggle */
  $("#banner-bar").on("click", function() {
    $(this).toggleClass("mobile-active");
  });
});
