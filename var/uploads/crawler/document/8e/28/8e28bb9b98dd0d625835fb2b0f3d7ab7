<script type="text/javascript">

    function Locate(d) {
        if ($('.open').length > 0) {
            var Open = $('.open').parent().parent().parent();
            $('.open').removeClass('open');
            Open.find('.outer').hide();

        }


        //    var name = '\"a[name =' + d + ']\"';
        var name = 'a[name =' + d + ']';
        var Anchor = $(name).next('.new').find('.h').find('.trigger');
        Anchor.addClass("trigger open")
        var Outer = $(name).next('.new').find('.outer').show();

    }
</script>
<script type="text/javascript">

    function Search() {
        if ($('#pgHeader_txtsearch').val() != '') {
            $('#pgHeader_txtsearch').closest("div").attr("id", "searchwrapper_white");
        }
        else {
            $('#pgHeader_txtsearch').closest("div").attr("id", "searchwrapper");
        }
    }

    function Search_focus() {

        $('#pgHeader_txtsearch').closest("div").attr("id", "searchwrapper_white");

    }



    $(document).ready(function () {
        $('#pgHeader_txtsearch').val('search this site');
        $("#ComboBox").change(function () { /* do something here */
            var url = $("#ComboBox").val();
            window.open().location = url;
        });


        $("#pgHeader_txtsearch").focus(function () {
            if ($('#pgHeader_txtsearch').val() == "search this site") {
                $('#pgHeader_txtsearch').val('')
            }
        });


        $("#pgHeader_txtsearch").blur(function () {
            if ($('#pgHeader_txtsearch').val() == '') {
                $('#pgHeader_txtsearch').val('search this site')
            }
        });

        $('#pgHeader_ImgBtnSearch').click(function () {
            if ($('#pgHeader_txtsearch').val() == '' || $('#pgHeader_txtsearch').val() == 'search this site') {
                alert("Please enter search criteria");
                $('#pgHeader_txtsearch').focus();
                return false;
            }
            return true;
        });


    });
  
  
  
</script>
<script type="text/javascript">
     jQuery(function () {
         var SelfLocation = window.location.href.split('?');
         switch (SelfLocation[1]) {
             case "justify_right":
                 jQuery(".megamenu").megamenu({ 'justify': 'right' });
                 break;
             case "justify_left":
             default:
                 jQuery(".megamenu").megamenu();
         }
     });
    </script>
 

