<script type="text/javascript" > 
//<![CDATA[
 var popStatus9 = 0;
function loadPop9(){
if(popStatus9==0){
$(".backgroundPop").css({
opacity: "0.20" 
});
$(".backgroundPop").fadeIn("slow");
$("#popContact9").fadeIn("slow");
popStatus9 = 1;
}
}
function disablePop9(){
if(popStatus9==1){
$(".backgroundPop").fadeOut("slow");
$("#popContact9").fadeOut("slow");
popStatus9= 0;
}
}
function centerPop9(){
 var windowWidth = document.documentElement.clientWidth;
var windowHeight = document.documentElement.clientHeight;
var popHeight = $("#popContact9").height();
var popWidth = $("#popContact9").width();
$("#popContact9").css({
	"position": "relative",
});
$(".backgroundPop").css({
 "height": windowHeight
});
}
$(document).ready(function () {
  $("#button9").click(function () {
  centerPop9();
 loadPop9();
 $("#popContact9").find("h2 a.skpnav").focus();
 });
 $("#popContactClose9").click(function () {
disablePop9();
});
$(".backgroundPop").click(function () {
disablePop();
 });
 $(document).keyup(function (e) {
   if (e.keyCode == 27 && popStatus9 == 1) {
  disablePop9();
 }
 });
});

//]]>
