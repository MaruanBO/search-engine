  <script>
    function pow(){
      $el = $('.jumbotron.masthead').pow({
        rays:          21, // positive integer > 1
        bgType:        'vertical',
        bgColorStart:  '#05305b', // any css color
        bgColorEnd:    '#158da8', // any css color
        rayColorStart: 'rgba(255,255,255,0.3)', // any css color
        rayColorEnd:   'rgba(255,255,255,0)', // any css color
        rayRadius:      '500px',
        originEl:      '.cloudsponge-form-image'  // any jQuery selector
      });
      if ($el) {
        $el.css('filter', 'none');
      }
    }
    $(window).on('load resize', pow);
  </script>

    

    

