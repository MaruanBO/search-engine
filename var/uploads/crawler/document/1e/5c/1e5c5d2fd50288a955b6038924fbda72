<script language="javascript">
//*****************************************************************
//     This script is used and customized to measure the page scroll / interaction with Google Universal Analytics.
//     Author: E-Nor Inc.
//     Created By: Mohamed Adel
//     Last Update: 04/25/2014

//*****************************************************************
/** Predefined variables **/
EventNONInteraction = false; // This variable determines the event will be a noninertact event or not
Frequency = 20; // This variable determines the Frequency the event will be fired, MAKE SURE THE NUMBER ENTERED CAN BE DIVIDED BY 100 (10 means each 10 precent the event will fire)

GA_EventCategory = 'Page Interaction'; // Google Analytics event category
GA_EventAction = 'Scroll Down'; // Google Analytics event action.

/************ DON'T EDIT BELOW THIS PART ************/
_frequency = Frequency;
_iterations = 100 / Frequency;
var _scrollMatrix = new Array();
for (ix = 0; ix < _iterations; ix++) {
    _scrollMatrix[ix] = [_frequency, 'false'];
    _frequency = Frequency + _frequency;
}
$(document).scroll(function (e) {
    for (iz = 0; iz < _scrollMatrix.length; iz++) {
        if (($(window).scrollTop() + $(window).height() >= $(document).height() * _scrollMatrix[iz][0] / 100)  && (_scrollMatrix[iz][1]== 'false')) {
            _scrollMatrix[iz][1] = 'true';
            ga('send', 'event', GA_EventCategory, GA_EventAction, _scrollMatrix[iz][0]+'%', {'nonInteraction': EventNONInteraction});  
        }
    }
});
</script>

<script>
/* TRACK ARTICLE MENU CLICKS *******************************************************************/
$(document).ready(function() {
    var hashtag = /#\S+/;
    $('#page-nav a').click(function() {
        var match = $(this).attr('href').match(hashtag);
        ga('send', 'event', 'Article Menu Desktop', 'Click', match[0]);
    });
    $('#links-panel li.article-link a').click(function() {
        var match = $(this).attr('href').match(hashtag);
        ga('send', 'event', 'Article Menu Mobile', 'Click', match[0]);
    });
});
</script>

<script>
/* TRACK SIDEBAR IMAGE CLICKS *******************************************************************/
$(document).ready(function() {
    $('aside a.image-click').click(function() {
        var link = $(this).attr('href');
        ga('send', 'event', 'Page Interaction', 'Image Click', link);
    });
});
/* TRACK DONATION CLICKS ************************************************************************/
$('#donate-slideout').bind('click', function() {
    var link = window.location.pathname;
    ga('send', 'event', 'Page Interaction', 'Donation', link);
});
</script>

<script>
/* RENDER PAGE FOR MOBILE DEVICES *******************************************************************/
if ($(window).width() <= 600) {
    $(document).bind("mobileinit", function() {
        $.mobile.ajaxEnabled = false;
        $.mobile.pushStateEnabled = false;
    });
    loadCss('http://code.jquery.com/mobile/1.4.4/jquery.mobile-1.4.4.min.css', trackTimingCallback('jQuery Mobile CSS', 'Load CSS', 'jQuery CDN'));

    $("#main-content").wrap("<div id='main-content-wrapper' data-role='page'></div>");
    var fixedNav = '<div id="fixed-nav" data-role="header" data-position="fixed">' + '<a href="/search/mysearch.php" id="search-icon" data-icon="search" data-iconpos="notext">Search</a>' + '<a id="bars-icon" href="#nav-panel" data-rel="popup" data-icon="bars" data-position-to="window" data-iconpos="notext">Menu</a><a id="banner-link" href="/"></a></div>';
    if ($("#sidebar1").length) {
        $("#sidebar1").before(fixedNav);
    } else if ($("#hc").length) {
        $("#hc").before(fixedNav);
    } else if ($("#home").length) {
        $("#home").before(fixedNav);
    }

    loadJsAsync('https://code.jquery.com/mobile/1.4.4/jquery.mobile-1.4.4.min.js', trackTimingCallback('jQuery Mobile', 'Load Library', 'jQuery CDN'));

    $(".mobile-player").attr({
        width: '290',
        height: '163'
    });
}
</script>

