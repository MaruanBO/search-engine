<script language="javascript" type="text/javascript">
    
    function searchKeyTextboxClick() {
        if (window.jQuery) {
            var input = $("#searchText");
            if (input && input.length > 0) {
                if (input[0].value == 'Enter Keyword or Item #')
                    input[0].value = "";
            }
        }
    }
        
</script>

