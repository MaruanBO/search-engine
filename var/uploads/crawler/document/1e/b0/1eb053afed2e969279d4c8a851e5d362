  <script src='https://edge.fscdn.org/assets/js/clientManifest-634db5d540a4cf88dc8e47bc9bedfd6d.js' ></script>


<script type="text/javascript">

  /**
   * Clear out any inject cached assets from local storage.
   * Apparently, the Inject.clearCache call doesn't always remove old cached assets from localStorage.  This code
   * will loop through everything in localStorage and remove it if it's an inject cached asset.
   */
  for(var k in localStorage) {
    if(k.indexOf("lscache-INJECT") === 0) {
      localStorage.removeItem(k);
    }
  }

  
    Inject.setModuleRoot("https://edge.fscdn.org/assets/js/modules");

    function loadCSS(path) {
      return FS.File.loadCSS(path);
    }

    
      Inject.setCrossDomain({
        relayFile: "https://edge.fscdn.org/assets/inject_relay_0.5.2.2013_11_26.html",
        relaySwf: "https://edge.fscdn.org/assets/inject_relay_0.5.2.2013_11_26.swf"
      });
    
    Inject.setExpires(0);//Don't store files in localStorage
    Inject.clearCache();//Have to clear because inject will cache no matter what the time is set to.  Since we name our files based on a hash, the expire time will only apply if the file hasn't changed names.

  

  
    if(window.manifest) {

      if(window.debugOn) console.log("Need to change manifest over to debug versions");

      //add all the rules for the cdn files with hash suffixes
      for(var key in manifest.js){
        if(window.debugOn) { //for debug, use _raw (unminified)
          manifest.js[key] = manifest.js[key].replace(".js", "_raw.js");
        }
        //replace fileName with absolute url ref to CDN url
        Inject.addFileRule(key, manifest.js[key]); //tell inject to use hashed filename from client manifest
      } //for()
    } //if window(manifest)

  console.log("Controls have been configured");

  $(document).ready(function() {
    // this used to be document.body....changed to allow <body> to be a control
    FS.Controls.init(document);
  });
</script>


    
    
      
