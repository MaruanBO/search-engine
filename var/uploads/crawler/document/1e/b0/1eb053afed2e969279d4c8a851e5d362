<script type="text/javascript">
$( document ).ready(function() {
  if(FS.Cookie.getCookie('ip_cc') == null){
    campaignIpAjaxCall();
  }
  else{
    campaignMboxAjaxCall(FS.Cookie.getCookie('ip_cc'));
  }
});

function campaignIpAjaxCall(){
  $.ajax({
    url: '/ip/',
    headers: {"Accept": "application/json"},
    type: "GET",
    success: function(data,status){
      try{
        var country = data.country.iso_code;
        campaignMboxAjaxCall(country);
      }
      catch(e){
        campaignMboxAjaxCall(null);
      }
    },
    error: function(xhr,status,error){
      campaignMboxAjaxCall(null);
    }
  });
}

function campaignMboxAjaxCall(country){  
  
  var userTyper = 'unauthenticated';
  if(userTyper != 'unauthenticated' && userTyper != 'nonmember' && userTyper != 'member'){
    userTyper = "nonmember";
  }
  var mBoxLang = 'en';
  var env = 'www';
  var userQueryVars = '';
  var testing = 'false';
  if(userTyper == "member" || userTyper == "nonmember"){
    
    userQueryVars = '';
  }
  
    var campaignMboxUrl = "/campaign/mbox-home/"+userTyper+"/"+mBoxLang+"/?env="+env+"&testing="+testing+"&country="+country+userQueryVars;
  

  $.ajax({
    url: campaignMboxUrl,
    headers: {"Accept": "application/json"},
    type: "GET",
    success: function(data,status){
      content = JSON.parse(data.replace(/&quot;/g,'"'));
      $(".mbox-large-left").html(((content.large.left).replace(/&lt;/gi,"<")).replace(/&gt;/gi,">").replace(/&#39;/gi,"'"));
      $(".mbox-large-right").html(((content.large.right).replace(/&lt;/gi,"<")).replace(/&gt;/gi,">").replace(/&#39;/gi,"'"));
      $(".mbox-small-left").html(((content.small.left).replace(/&lt;/gi,"<")).replace(/&gt;/gi,">").replace(/&#39;/gi,"'"));
      $(".mbox-small-center").html(((content.small.center).replace(/&lt;/gi,"<")).replace(/&gt;/gi,">").replace(/&#39;/gi,"'"));
      $(".mbox-small-right").html(((content.small.right).replace(/&lt;/gi,"<")).replace(/&gt;/gi,">").replace(/&#39;/gi,"'"));
    },
    error: function(xhr,status,error){
      $( '.mbox-banner' ).css({'height':0});
    }
  });
}
</script>



<section class="secondary row-fluid mbox-banner">
  <div class="span6">
    <div class="mbox-large-left"></div>
  </div>
  <div class="span6">
    <div class="mbox-large-right"></div>
  </div>
</section>

