</div><script type="text/javascript">
//Start BrightTag implementation
window.btSiteMap = {'10151':'IctsMUI', '10153':'PCNPFl9','10165':'PCNPFl9','10153_kiosk':'QDsdc7e','10151_kiosk':'fvqvh1D','10155':'BciA6ZX','10154':'mrIBbYB'}; 
	window.bt = {"levelIds":"",
	"levels":"",
	"verticalId":"",
	"verticalName":"",
	"categoryID":"",
	"categoryName":"",
	"subCategoryID":"",
	"subCategoryName":"",
	"itemNumber":"",
	"beanType":"",
	"itemPrice":"N/A",
	"itemDiscount":"N/A",
	"productDescription":"",
	"oasSitePage":"sears.com/index/homepage"};
	(function() {
		var catArray = shc.catArray;
		var catIdArray =  shc.catIdArray;
		if (catArray.length > 0) {
			var i = 0, l = Math.min(3, catArray.length);
			for (i = 0; i < l; i++) {
				switch (i) {
					case 0:
						bt.verticalName = catArray[i];
						bt.levels = bt.verticalName;
						bt.verticalId = catIdArray[i];
						bt.levelIds = bt.verticalId;
						break;
					case 1:
						bt.categoryName = catArray[i];
						bt.levels += '_'+bt.categoryName;
						bt.categoryID = catIdArray[i];
						if(bt.categoryID !== ""){
						bt.levelIds += '_'+bt.categoryID;
						}
						break;
					case 2:
						bt.subCategoryName = catArray[i];
						bt.levels += '_'+bt.subCategoryName;
						bt.subCategoryID = catIdArray[i];
						if(bt.subCategoryID !== ""){
						bt.levelIds += '_'+bt.subCategoryID;
						}
				}
			}
		}
	})();
</script>

