<script type="text/javascript">
    var btn_blink_count       = 0,
        btn_blink_state       = 0,
        btn_blink_in_interval = 0,
        btn_blink_interval_id = 0;

    var blink_on = function(){
        handleOver();
    };
    var blink_off = function(){
        handleOut();
    };
    var btn_blink_func = function () {
        if (btn_blink_state == 0) {
            blink_on();
            btn_blink_state = 1;
        }
        else if (btn_blink_state == 1) {
            blink_off();
            btn_blink_state = 0;
        }
        if (!btn_blink_in_interval) {
            btn_blink_in_interval = 1;
            btn_blink_interval_id = setInterval(btn_blink_func,500);
        }
        btn_blink_count++;
        if (btn_blink_count>5) {
            clearInterval(btn_blink_interval_id);
            btn_blink_in_interval = 0;
            btn_blink_count = 0;
            btn_blink_state = 0;
        }
    }
    var begin_blink=function() {
        if (btn_blink_interval_id) clearInterval(btn_blink_interval_id);
        btn_blink_in_interval = 0;
        btn_blink_count = 0;
        btn_blink_state = 0;
        blink_off();
        btn_blink_func();
    };
    $(function(){
        $('.blinker').click(begin_blink);
        $('img').each(function() {
            if ($(this).parent()[0].tagName.toLowerCase()!='a' || !$(this).parent().hasClass('downloadlink')) {
                $(this).click(begin_blink);
            }
        });
    });
</script>
