    var phoneAnimated = false;

    phoneEle.on('click', function() {
        if(!phoneAnimated) {
            phoneAnimated = true;
						phoneEle.addClass('active');
						phoneEle.next().slideToggle();
        } else {
            phoneAnimated = false;
						phoneEle.next().slideToggle();
						phoneEle.removeClass('active');
        }
    });
});
