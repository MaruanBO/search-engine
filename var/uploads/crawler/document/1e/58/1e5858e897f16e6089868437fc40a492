  createCookie('4chan_disclaimer', 1, 365);
  location.href = disclaimerHref;
}
function disclaimerCancel() {
  dialog.hide();
}
function disclaimerLinkClick(e) {
  var top, cnt, s;
  
  disclaimerHref = this.href;
  dialog.show();
  if(e.preventDefault) e.preventDefault();
  
  cnt = document.getElementById('disclaimer-dialog_c');
  top = (window.pageYOffset + (window.innerHeight / 2) - cnt.offsetHeight / 2);
  s = cnt.style;
  
  if (top > 0) {
    s.top = top + 'px';
  }
  
  s.marginLeft = '50%';
  s.left = '-' + (cnt.offsetWidth / 2) + 'px';
  
  return false;
}

