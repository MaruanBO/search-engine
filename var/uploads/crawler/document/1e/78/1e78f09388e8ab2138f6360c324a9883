        //use 'wait' cursor for clicked links except: links with '#', '_blank' attribute, and have HTTP in the URL
        $('a').click(function() {
            if ((this.href.indexOf('#') == -1) && (this.href.indexOf('http') != -1) && (this.target != "_blank") && !$(this).hasClass('ajax')) {
                $('*').css('cursor','wait');
            } else {
            }
        });

