			<script type="text/javascript">
				function setCookie(c_name,value,exdays) {
					var exdate=new Date();
					exdate.setDate(exdate.getDate() + exdays);
					var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
					document.cookie=c_name + "=" + c_value;
				}

				Event.observe(window, 'load', function(e) {
					var showPopup = function() {
						var viewport = document.viewport.getDimensions(); // Gets the viewport as an object literal
						var vpHeight = viewport.height; // Usable window height
						var emailPopup = $('emailTeasePopup').getDimensions(); // Gets the viewport as an object literal
						var puHeight = emailPopup.height; // Usable window height
						$('emailTeasePopup').setStyle({
							top: ( (vpHeight + 30 - puHeight) /2 ) + "px"
						});
						$('emailTeaseOverlay').style.display = "block";
						$('emailTeasePopup').style.display = "block";
					}

					var hidePopupHandler = function(e) {
						$('emailTeaseOverlay').style.display = "none";
						$('emailTeasePopup').style.display = "none";
						e.stop();
					}

					$('closeTeasePopup').observe('click', hidePopupHandler);
					$('emailTeaseOverlay').observe('click', hidePopupHandler);
					setCookie('aListPopUp_cookieAero', 'aListPopUp_cookieAero', 20*24*60*60);
					showPopup.delay(1);
				});
			</script>
		
	</div>



