    <script type="text/javascript" language="javascript">
        $(document).ready(function() {
            $("[id*='ddlgqfaacombo']").change(SetProductList);
            InitializeList();
        });


       function InitializeList() {

            var zipQs = (getParameterByName('zipCode')) + (getParameterByName('zipcode'));
            if (zipQs == null) {
                $("[id*='ZipCode']").attr('value', 'Zip Code');
                $("[id*='ddlgqfaacombo'] option").eq(0).attr('selected', 'selected');
                $("[id*='ProductDDL'] option").eq(0).attr('selected', 'selected');
            }            
        }

        function getParameterByName(name) {
            var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
            return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
        }


        function SetProductList() {
            var action = $("[id*='ddlgqfaacombo']").val();
            if (action == "faa") {
                $('<option value="Snapshot">' + "Snapshot - Auto Insurance" + '</option>').insertAfter($("[id*='ProductDDL'] option[value='Auto']"));
            } else {
            $("[id*='ProductDDL'] option[value='Snapshot']").remove();
            }
       }

    </script>

