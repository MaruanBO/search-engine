        var webPropertyId = "UA-2108837-10";
        var gaUrl = "http://www.google-analytics.com/ga.js";
        var gaSiteSpeedSampleRate = "10";
        var paCategory = "Agent-Static";
        
        var _gaObj = _gaObj || {};
        _gaObj.getDomainForGA = function () {
            var url = window.location.hostname;
            var urlparts = url.split('.');
            var i = urlparts.length;
            var domainName = "";
            if (i >= 2)
                domainName = "." + urlparts[i - 2] + "." + urlparts[i - 1];
            else
                if (url.toLowerCase().indexOf('localhost')) {
                    domainName = url;
                }
                else {
                    domainName = ".progressiveAgent.com";
                }
            return domainName;
        };

        _gaObj.GATrackEvent = function (category, action, label, value, noninteraction) {
            noninteraction = !!noninteraction;
            if (!category) category = paCategory;
            if (!action) action = document.domain + window.location.pathname;
            if (typeof value != 'number') value = undefined;

            _gaq.push(['_trackEvent', category, action, label, value, noninteraction]);
        };

        _gaObj.GASetCustomVar = function (index, name, value, scope) {
            if (typeof scope != 'number') value = 3;
            _gaq.push(['_setCustomVar', index, name, value, scope]);
        };

        _gaObj.GATrackPageview = function (pageUrl) {
            if (typeof pageUrl != undefined) _gaq.push(['_trackPageview', pageUrl]);
        };

        var _gaq = _gaq || [];
        _gaq.push(
            ['_setAccount', webPropertyId],
            ['_setAllowHash', false],
            ['_setAllowLinker', true],
            ['_setDomainName', _gaObj.getDomainForGA()],
            ['_setCampSourceKey','code'],
            ['_setSiteSpeedSampleRate', gaSiteSpeedSampleRate],
            ['_trackPageview']);
 
        (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = gaUrl;
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>

