    $(document).ready(function() {
        $(".faaTab").click(function() {
        $("#quotestart").hide();
        $(".retrieveQuote").hide(); 
            $("#faa").show();
            $(".faaTab").css("background-position", "-186px -41px");
            $(".quoteTab").css("background-position", "0px -41px");
        });
        $(".quoteTab").click(function() {
            $("#faa").hide();
            $("#quotestart").show();
            $(".retrieveQuote").show(); 
            $(".faaTab").css("background-position", "-186px 0px");
            $(".quoteTab").css("background-position", "0px 0px");
        });
    });
</script>


