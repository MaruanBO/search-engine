<script type="text/javascript">
		$(function() {
			var ucartTimer;

			$("#widget-but-ucart").hover(function() {
				ucartTimer = setTimeout(function() {
					showBasket('show', '');
				}, 500);
			}, function(ev) {
				if (typeof ucartTimer != undefined) {
					clearTimeout(ucartTimer);
				}
			});
		});

		// When the Universal Cart layer opens// it will close if the user clicks on anything in the main browser window
		$(document).click(function(event) {
			var posX = event.clientX;
			var posY = event.clientY;
			var miniCart = $('#widget-ucart');
			var miniCartPos = miniCart.offset();
			if (isShowingBasket()) {
				// If user click to background, close the mini cart
				if (!(posX >= miniCartPos.left && posX <= (miniCartPos.left + miniCart.width()) && posY >= miniCartPos.top && posY <= (miniCartPos.top + miniCart.height()))) {
					hideBasket();
				}
			}
		});
	</script>
