                          // Polls 10x at 200ms after loaded, and 10x at 500ms after
                          $jq171(document).ready(function(){

                              var count = 0;
                              var button = $jq171('.liveagent-button');


                              var polling = function(){
                                  if(button.css('display') != 'none'){ // if liveagent shows, fire shown event
                                      $st.core.tracking.TrackingManager.getInstance().handleEvent(new $st.core.event.Event({
                                          group: 'Header',
                                          track:true,
                                          data:{
                                              "name": "Chat button shown",
                                              "url": button.data('st-track-url'),
                                              "type": button.data('st-track-buttontype')
                                          }
                                      }));

                                      return;
                                  }

                                  if (count < 10) {
                                      setTimeout(polling, 200);
                                  } else if (count < 20) {
                                      setTimeout(polling, 500);
                                  }
                                  count++;
                              };

                              setTimeout(polling, 200);

                          });
                      </script>
