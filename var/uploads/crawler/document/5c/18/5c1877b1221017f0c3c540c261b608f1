<!-- node--marketing_content.tpl.php -->
<!-- contextual links -->
<div  class="st-section node node-marketing-content " about="/home-page-video" typeof="sioc:Item foaf:Document">
        
    <!-- body -->
            <div class="st-line">
            <!-- add st-line-inner based on field_full_width value -->
                            <div class="st-box size16of16">
                    <link rel="stylesheet" href="//resource.squaretrade.com/sites/all/themes/stmothership/css/hero-home.css">
<script type="text/javascript" src="//resource.squaretrade.com/javascript/common/thirdparty/jquery/jq171_easydropdown.js"></script>
<script type="text/javascript">
        var phoneSelection = {
      '' : {          
          '' : '/smartphone-warranty',
          'appleStore' : '/apple-store-iphone-warranty',
          'att' : '/smartphone-warranty',
          'bestBuy' : '/smartphone-warranty',
          'sprint' : '/smartphone-warranty',
          'verizon' : '/smartphone-warranty',
          'otherCarrier' : 'smartphone-warranty'                      
      },
      'iphone' : {
        
          '' : '/iphone-warranty',
          'appleStore' : '/apple-store-iphone-warranty', 
          'att' : '/att-iphone-warranty',
          'bestBuy' : '/best-buy-iphone-warranty', 
          'sprint' : '/sprint-iphone-warranty', 
          'verizon' : '/verizon-iphone-warranty',
          'otherCarrier' : '/iphone-warranty'                      
      },
      'galaxy' : {
        
          '' : '/samsung-galaxy-warranty', 
          'appleStore' : '/samsung-galaxy-warranty',
          'att' : '/att-samsung-galaxy-warranty',
          'bestBuy' : '/best-buy-samsung-galaxy-warranty',  
          'sprint' : '/sprint-samsung-galaxy-warranty', 
          'verizon' : '/verizon-samsung-galaxy-warranty',
          'otherCarrier' : '/samsung-galaxy-warranty'          
      },
      'otherPhone' : {
          '' : '/smartphone-warranty',
          'appleStore' : '/smartphone-warranty',
          'att' : '/att-smartphone-warranty',
          'bestBuy' : '/best-buy-smartphone-warranty',  
          'sprint' : '/sprint-smartphone-warranty',
          'verizon' : '/verizon-smartphone-warranty',
          'otherCarrier' : '/smartphone-warranty'          
      }
    };
    var permissions = "i";
    var sendEmailDataToResponsys = function(userData) {
      var emailData = userData;
      $jq171.ajax({
        data: emailData,
        type: 'GET', 
        url: "https://Protect.SquareTrade.com/pub/rf?_ri_=X0Gzc2X%3DWQpglLjHJlYQGjuBtzezbYFSSty6G7zaDPITzfw8IsAzcBza5VwjpnpgHlpgneHmgJoXX0Gzc2X%3DWQpglLjHJlYQGucrtGPzacMREI5PTA1L2LeE8IsAzcBza5",
        dataType: 'jsonp'
      });
    }
    var submitEmailData = function(fullCarrier, fullPhone, phone, carrier){
      if (permissions) {
        var user_data = $st.core.storage.get("st_user_data");
        var email_is_hidden = $jq171("#emailAddressInputContainer").hasClass("is-hidden");
        var email_address = "";
        var new_email_address = $jq171("#edit").val();
        if (new_email_address) {
          email_address = new_email_address;
        } else if (user_data && email_is_hidden) {
           user_data = $st.utils.string.toObject(user_data, "&", "=");
           email_address = decodeURIComponent(user_data.email);
        } else {
          email_address = $jq171("#emailAddressInput").val();
        }
        if (fullCarrier == "Other Retailer") {
          fullCarrier = "Other Retailers";
        }
        var emailBody = {
          "EMAIL_ADDRESS_"  : email_address,
          "EMAIL_PERMISSION_STATUS_" : permissions,
          "LEAD_SOURCE" : "Quote_Tool", 
          "CARRIER": fullCarrier,
          "PHONE"  : fullPhone,
          "QUOTE_URL" : window.location.host + phoneSelection[phone][carrier]
        } 
        sendEmailDataToResponsys(emailBody);
      }
    }

    var submitLinkForm = function(){
        var carrier = document.getElementById("carrier").value;
        var phone = document.getElementById("phone").value;
        var fullCarrier = $jq171("#carrier").find(":selected").not(".label").text() || "";
        var fullPhone = $jq171("#phone").find(":selected").not(".label").text() || "";

        $st.core.tracking.TrackingManager.getInstance().handleEvent(new $st.core.event.Event({
                  group:'Navigation Widget',
                  track:true,
                  data:{
                      "name": 'Quote Button Clicked',
                      "Quote Phone Type": phone,
                      "Quote Retailer": carrier
                  }
              }));

         submitEmailData(fullCarrier, fullPhone, phone, carrier);
         setTimeout(function(){
              window.location = phoneSelection[phone][carrier];
         }, 300);
         
    };

    var toggleEmailInputs = function(){
      var user_data = $st.core.storage.get("st_user_data");
      user_data = $st.utils.string.toObject(user_data, "&", "=");
      if (user_data.hasLoggedIn) {
        $jq171("#emailAddressCheckboxContainer").removeClass("is-hidden");
        $jq171("#emailAddressInput").val(decodeURIComponent(user_data.email));
      } else {
        $jq171("#emailAddressInputContainer").removeClass("is-hidden");
      }
    };

    var editEmailAddress = function(e) {
      var user_data = $st.core.storage.get("st_user_data");
      user_data = $st.utils.string.toObject(user_data, "&", "=");
      $jq171("#emailAddressCheckboxContainer").addClass("is-hidden");
      $jq171("#editEmailAddressContainer").removeClass("is-hidden");
      $jq171("#edit").val(decodeURIComponent(user_data.email)).focus();
    }

    $jq171(document).ready(function(){

      var submitButton = document.getElementById("submit");
      $jq171('#submit').on('click', submitLinkForm);

      $jq171("#jumpMenu").change(function(e) {
        window.location.href = this.options[this.selectedIndex].value;
      });

      $jq171("#emailAddressInput").on("blur", function(e){
        if (!$jq171(this).val() == "") {
          $jq171(this).siblings("label").addClass("is-hidden");
        } else {
          $jq171(this).siblings("label").removeClass("is-hidden")
        }
      });

      toggleEmailInputs();

      $jq171("#editEmailAddress").on("click", function(e){
        editEmailAddress(e);
        permissions = "i";
      });

      $jq171("#emailAddressCheckbox").on("click", function(){
        if (!$jq171(this).attr("checked")) {
          permissions = null;
        }
      })
    
      $jq171("#home").removeClass("hideAll");    

    });
</script>


