<script type="text/javascript">
jQuery(document).ready(function($) {

		$('.get_notified').click(function() {
			var id = $(this).parent().attr('rel');
			popupHtml('#'+id+'-nmo-modal', $('#'+id+'-nmo-modal .modal-close-button'));
		});


		$('#nmo-email').keydown(function() {
			$('.errors').html('');
		});

		/* $('.nmo-get_notified').click(function(e) {
			e.preventDefault();
			var container = $(this).parent();
			var email = container.find('#nmo-email').val();
			var product = container.find('#nmo-product').val();
			var id = container.attr('rel');

			if (email != '') {
				window.BISPopover.create(email, product, id);
              	alert('We\'ve got your back! You\'ll be notified when your product comes back in stock!');
			} else {
				container.find('.errors').html('Wait up, what\'s your email?');
			}

		}); */ 

		$('.btn-cart').click(function() {
			var form = $(this).closest('form');
			var button = $(this);
			if ($.sdg.validateCartSumbit()) {
              
				$.ajax({
					type: 'POST',
					url: '/cart/add.js',
					dataType: 'json',
					data: form.serialize(),
                  	cache: false,
					complete: function() {
						var formFields = form.serializeArray();
						var variantId = '';
						$.each(formFields, function(idx, field) {
							if (field.name == 'id') {
								variantId = field.value;
							}
						});

						$.sdg.triggerReloadCart(variantId);
                      	if(typeof(upsell_offer) === "function"){ upsell_offer(); }
                      	
                      	if ($.inArray(button.attr('data-handle'), surchargeItems) !== -1) {
                              $.sdg.addSurcharge();
                        }
					}
				});
              
			} else {
                if ($.sdg.$underLimit) {
					var options = []; 
					$.each($.sdg.$options, function(idx, option) {
					  options.push(option.name);
					});
					alert('You probably forgot to select one of the following: <br/> '+options.join(', ') + ', or perhaps all of them...');
				}
			}

		});

		$('.modal-mobile-close-button').click(function() {
			$('.modal-container').fadeOut();
		});

});
</script>

