<script type="text/javascript">

	var submitSailthruAlertForm = function(button) {

		var form = $('#sailthru-alert-form');
		var email = form.find('#nmo-email').val();
		var product = form.find('#nmo-product').val();
		var id = form.attr('rel');

		if (email != '') {
			form.append('<input type="hidden" name="vars[sku_'+product+']" value="'+product+'" />');
			form.submit();
		} else {
			form.find('.errors').html('Wait up, what\'s your email?');
		}

	}

	jQuery(document).ready(function($) {

	    if (window.location.hash == '#thank-you-alert') {
	        alert('We\'ve got your back! You\'ll be notified when your product comes back in stock!');
	        window.location.hash = '';
	    }
	    
	});

</script>
</div>


<script type="text/javascript">
var loadCollectionUrls = function(urls, targetSelector, clearFirst, skipInit) {
    if (urls.length) {
      	$(targetSelector).append('<div class="ostrich-loading clear cf"><img src="//cdn.shopify.com/s/files/1/0065/0022/t/33/assets/ostrich-animation-light.gif?14371779826380298875" alt="" width="60" style="margin-top: 30px;" /></div>');
      	$.ajax({
	        url: urls[0],
	        success: function(response) {
	          	if (clearFirst) $(targetSelector).html('');
	          	$(targetSelector).append($(response).find(targetSelector).html());

	          	if (typeof skipInit == 'undefined' || skipInit != true) {
	              	initProductTiles();
	              	$.chive.addTileClicks();
	          	} else {
	          	  	$.chive.addSimpleTileClicks() 
	          	}

	          	$(targetSelector).find('.ostrich-loading').remove();
	          	urls.shift();

	          	if (typeof sdgGetCollectionProducts != 'undefined') {
	            	addSizeClassesToTiles(sdgGetCollectionProducts);
	          	}
	          	loadCollectionUrls(urls, targetSelector, false, skipInit);
	          
	          	$.sdg.replaceGifWithFullSize('.category_product', '_medium','img','src');
	        }
      	});
    }
};

var addSizeClassesToTiles = function(sdgGetCollectionProducts) {
    var showFilterSizesBlock = false;
	$.each(sdgGetCollectionProducts, function(id, data) {
		var productId = id; 
		var sizeIndex = null;
		var html = '';
        
		$.each(data.options, function(idx, type) {
			if (type == 'Size') {
				sizeIndex = idx;
			}
		});

		$.each(data.variants, function(k, variant) {
			if (variant.available == 'true') {
				if (variant.options[sizeIndex]) {
					var size = variant.options[sizeIndex].toLowerCase();
					if (size == '3xl') size = 'xxxl';
					html += ' size_'+size;
					showFilterSizesBlock = true;
				}
			}
		});

		$('.product-tile[rel="'+productId+'"]').addClass(html);
	});
  
  	return showFilterSizesBlock;
};
  
var initProductTiles = function() {

  	var $tile = null;
  
  	$('.category_product').not('.promo-tile').not('.event-added').addClass('event-added').hover(
		function(){
	        if (!$.chive.toolTipOpen) {
				if ($('#desktop').css("display") == "block") {
					if($(this).children('div.category_product_hover').hasClass('disabled')) {
						$(this).children('div.category_product_hover').show().children('div.out_of_stock_links').slideDown(300);
					} else {
						$(this).children('div.category_product_hover').show().children('div.category_product_links').slideDown(300);
					}
				}
	        }
		},
		function(){
			if (!$.chive.toolTipOpen) {
				$('div.category_product_hover').each(function(){
					if(!$(this).hasClass('disabled')) {
						$(this).hide()
					}
					if($(this).hasClass('disabled')) {
						$(this).children('div.out_of_stock_links').hide();
					} else {
						$(this).children('div.category_product_links').hide();
					}
				});
			}
		}
	);


	$('.promo-tile').not('.event-added').addClass('event-added').click(function() {
		window.location = $(this).attr('url');
	});
 
    $('div.buy_now').not('.event-added').addClass('event-added').click(function() {
  
		var prep = $('handle-product-essential-template');
		prep.find('.gender-selector-block').html('');
		prep.find('.gender-selector-block').html('');
		prep.find('.price-box .old-price').remove();
		prep.find('.price-box .regular-price').removeClass('special-price');
  
  
		$('div#modalProductContent').html('');

		/*
		* Position, offset, and arrow centering
		*/
		$tile = $(this).closest('.product-tile');
		var top = $tile.offset().top + $tile.outerHeight();
		var left = $tile.offset().left;
		var leftLimit = $('div.page.inner').offset().left;
		var rightLimit = leftLimit + $('div.page.inner').outerWidth();
		var height = 309;
		var width = 576;
		var topMargin = top - height - 50;
		var leftMargin = left;
		var shiftLeft = leftMargin + width - rightLimit;
		if (shiftLeft > 0) {
		  	leftMargin -= shiftLeft;
		} else {
		  	shiftLeft = 0;
		}

		var isConfigurable = $(this).hasClass('configurable') ? true : false;
		var height = $(document).height();
		$('div.modalWrapper').height(height);
		var id = $(this).parent().attr('rel');

		$('div.modalInner').css({
		  	'top': topMargin,
		  	'left': leftMargin
		});

      	$.chive.showTileLoading($tile);

      	var handle = $(this).attr('data-target');

      	jQuery.getJSON('/products/' + handle + '.js', function(product, textStatus) {
          	if ((typeof callback) === 'function') {
              	console.log(product);
          	} else {
        
              	if (product.id) {
                    $('#modalProductContent').html($('.handle-product-essential-template').html());	  
                    var container = $('#modalProductContent');

                    container.find('.gender-selector-block').attr('id', 'gender-block-'+product.id).html('');
                    container.find('.config-options').html('');	  
                    container.find('.main_image_zoom').attr('src', product.images[1]);
                    container.find('.product-name h1').html(product.title);
                    container.find('.out_of_stock_wrapper').attr('rel', product.id);

                    var money = product.price/100;	  
                    container.find('.price-box .price').html('$'+money.toFixed(2));

                    var description = (product.description.replace(/(<([^>]+)>)/ig, "")).split(/\s+/).slice(0, 20).join(" ");
                    container.find('.short-description').html(description+'...'); 	  
                    container.find('#product-variants select').attr('id', 'product-select-'+product.id);

                    var showSelect = 0;
                    $.each(product.variants, function(key, variant) {
                        if (variant.available) {
                            container.find('#product-variants select').append('<option value="'+variant.id+'">'+variant.title+'</option>');
                        }
                        showSelect++;
                    });
                    
                    // this product has surcharge tag
                   	container.find('#purchase').attr('data-handle',product.handle); 

                  	container.find('.bis-modal-container').attr('id', product.id+'-nmo-modal');
                  	container.find('.bis-modal-container .product-name').html(product.title);
                  	container.find('.bis-modal-container .nmo-product-fields').attr('rel', product.id);

                  	$.each(product.variants, function(key, variant) {
                      	if (!variant.available) {
                         	container.find('.bis-modal-container #nmo-product').append('<option value="'+variant.id+'">'+variant.title+'</option>');
                      	}
                  	});

                  	var data = container.html();

                  	$.chive.toolTipOpen = true;
                  	$('html,body').animate({scrollTop: $("div#modalProductContent").offset().top}, 400);
                  	$.chive.hideTileLoading();
                  	$('div#modalProductContent').html(data).find('.quickview-arrow').css('left', (shiftLeft + $tile.outerWidth() / 2 - 20));
                  	$('div#modalProduct').show();


                    sdgProductOptions = product.options;
                    sdgProductVariants = product.variants;
                    $.sdg.$selectedVariants = [];

                  	$.sdg.configurableOptions('product-select-'+product.id, sdgProductOptions, sdgProductVariants, chiverySettings, product.handle, product.title);

              	} else {
                  	alert('Error');
              	}
          	}
      	});
  
    });
}
  
jQuery(document).ready(function($) {

	$('.bg-follow-me').click(function() {
		$(this).parent('#modalProduct').fadeOut(300);
		$.chive.toolTipOpen = false; 
		$('#modalProductContent').html('');
		$('.category_product_hover').hide();
	});

	var height = $('html').height();
	$('div.modalWrapper').height(height);

	$.chive.toolTipOpen = false;

	
    initProductTiles();
	
	
});
</script>
