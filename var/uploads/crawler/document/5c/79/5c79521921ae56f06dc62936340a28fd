  jQuery(window).load(function() {
    jQuery('#header-banner').slideDown();    
    var padding = jQuery('.inner-wrapper').css('padding-top').replace('px','');
    var banner_height = 30;
    
    if ($(window).width() > 768) {       
     	jQuery('.inner-wrapper').animate({'padding-top' : (parseInt(padding)+banner_height)+'px'},300);
    }
  });
  jQuery(window).resize( function() {
    var header_height = jQuery('.header-container').height();
    if ($(window).width() < 768) {
      	jQuery('.inner-wrapper').css('padding-top','initial');
    } else {
      	if (jQuery('.inner-wrapper').hasClass('promo-on')) {
          jQuery('.inner-wrapper').css('padding-top', (header_height+7)+'px');
