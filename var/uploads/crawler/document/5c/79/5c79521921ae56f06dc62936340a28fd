    <script type="text/javascript">
    var flashsaleItem = false; 

    
    

        
        flashsaleItem = {"body_html":"\u003cp\u003e\u003cimg src=\"\/\/cdn.shopify.com\/s\/files\/1\/0065\/0022\/files\/St.Pattys_15_Header_Desktop.jpg?14102156131689269955\" class=\"desktop-display\"\u003e \u003cimg src=\"\/\/cdn.shopify.com\/s\/files\/1\/0065\/0022\/files\/St.Pattys_15_Header_Tablet.jpg?8035587516177835942\" class=\"tablet-display\"\u003e \u003cimg src=\"\/\/cdn.shopify.com\/s\/files\/1\/0065\/0022\/files\/St.Pattys_15_Header_Mobile.jpg?8052482738721974969\" class=\"phone-display\"\u003e\u003c\/p\u003e","handle":"st-paddys-day","id":32435015,"published_at":"2015-02-18T15:13:00-06:00","published_scope":"global","sort_order":"manual","template_suffix":"","title":"St. Paddy's Day Series | #LUCKYSOB","updated_at":"2015-02-19T11:45:29-06:00"};

    

    var settingTime = new Date(2015, 1, 24, 14, 00, 00, 00);


    settingTime.setTime(settingTime.getTime() + (6*60*60*1000));
    var nowInUTC = (new Date).getTime() + ((new Date()).getTimezoneOffset()/60)*60*60*1000;


    var url = '/collections/'+flashsaleItem.handle;
    if (chiverySettings.flash_sale_item_1_type == '1') {
        url = '/products/'+flashsaleItem.handle;
    }

    
        flashsaleItem.title = "St Paddy's Day Series";
    

    var sale = new flashSale(flashsaleItem.title, url, settingTime.getTime()/1000, nowInUTC/1000);



    var day = 24; 
    var monthNames = [ "Jan", "Feb", "March", "April", "May", "June", "July", "Aug", "Sept", "Oct", "Nov", "Dec"];
    var suffix = ["th", "st", "nd", "rd", "th", "th", "th", "th", "th", "th"];
    if (day % 100 >= 11 && day % 100 <= 13) {
        suffixDay = day + "th";
    } else {
        suffixDay = day + suffix[day % 10];
    }

    var launchDate = monthNames[1] + ' ' + suffixDay; 



    jQuery(document).ready(function($) {

       $('div.flashsale-wrapper').on('click',function(){
           window.location = sale.productUrl;
       });

       if ($(".flashsale-wrapper").length && typeof sale != "undefined") {
           $daysElem = $(".flashsale-wrapper .countdown_days");
           $hoursElem = $(".flashsale-wrapper .countdown_hours");
           $minutesElem = $(".flashsale-wrapper .countdown_minutes");
           $secondsElem = $(".flashsale-wrapper .countdown_seconds");
           $(".flashsale-wrapper .name").html(sale.saleName);
           sale.now += (Date.now() - timerStart)/1000;
           sale.startTimer();
           window.saleInterval = window.setInterval(
               function() {
                   if (sale.now >= sale.saleTimestamp) {
                        window.clearInterval(window.saleInterval);
                        $(".flashsale-wrapper .countdowns_wrapper").html("On sale now!");
                        $(".flashsale-wrapper .header_date_wrapper").html("");
                    }
                    $daysElem.html(sale.padZero(sale.daysUntil));
                    $hoursElem.html(sale.padZero(sale.hoursUntil));
                    $minutesElem.html(sale.padZero(sale.minutesUntil));
                    $secondsElem.html(sale.padZero(sale.secondsUntil));
                    $(".flashsale-wrapper .header_date_wrapper").html("Launching "+launchDate);
                    if (sale.now >= sale.saleTimestamp) {
                        $(".flashsale-wrapper .header_date_wrapper").html("On sale now!");
                    }  
                },
                1000
            );
        }

        setTimeout(function() {
            $('.header-container .flashsale .name').css('display', 'inline');
            $('.header-container .flashsale').animate({opacity: 1}, 300);
        }, 1000);

    });
    </script> 


