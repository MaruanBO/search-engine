      <script type="text/javascript">
        $('.cart-items').on('click','.add', function(e) {            
            var id = $(this).attr('data-id');
            var qty_input = $(this).siblings().find('input');
            var qty = parseInt(qty_input.val());
          	var max = parseInt(qty_input.attr('max'), 10) || 10000;
            if (qty == max) {
               alert('We only have ' + max + ' of this item in stock');
               //qty_input.val(max);
               $.sdg.updateCart($('.cart-items'),max, id);
            } else {
              if (typeof Shopify.Cart == 'undefined') {
                qty += 1;     
                  $.sdg.updateCart($('.cart-items'),qty, id);
              } else {
                if (qty < Shopify.Cart.limitQuantity) {
                  qty += 1;     
                  $.sdg.updateCart($('.cart-items'),qty, id);
                } else {
                  alert('Quantity Limit has been reached. Cannot add your product to cart.'); 
                } 
              }
            }
          
        });
        $('.cart-items').on('click','.subtract', function(e) {         
            var id = $(this).attr('data-id');
          	var qty_input = $(this).siblings().find('input');
            var qty = parseInt(qty_input.val());
            if (qty > 1 ) {
              qty -= 1;
            }         
            $.sdg.updateCart($('.cart-items'),qty, id);
        });        
        
        // hide shipping charge from the line items
        $(document).ready(function() {
          var cartcount = $('.cart-count').attr('data-cart-count');
          if ($('.shipping-charge').length != 0) {
            
           	$('.shipping-charge').hide();
            $('.cart-count').html(parseInt(cartcount)-1);
          }
          if (cartcount > 0) {
            $('.cart-count').removeClass('hide');
          }
        });
        
        var surchargeItems = [];
      	var surchargeItemSettingCount = 30;
        // get surcharge item from theme settings
        for (var i=1; i<surchargeItemSettingCount; i++) {
          if ( chiverySettings['surcharge_handle_'+i] != '') {
              surchargeItems.push(  chiverySettings['surcharge_handle_'+i] );
          }
        } 
        //$('#cartScroll').jScrollPane({autoReinitialise: true});
      </script>
