<script type="text/javascript">
jQuery(document).ready(function($) {    

  if ($(window).width() < 768) {
    $('.cart-button-container .rakit-wrapper').prependTo('.cart-button-wrap');
  } else {
    $('.cart-button-container .rakit-wrapper').appendTo('.cart-button-wrap')
  }
  $(window).on('resize', function() {
    var win = $(this); //this = window
    if (win.width() < 768) {
      $('.cart-button-container .rakit-wrapper').prependTo('.cart-button-wrap');
    } else {
      $('.cart-button-container .rakit-wrapper').appendTo('.cart-button-wrap')
    }
  });
  
});
$('#cart-menu .tooltip-toggle').click(function(e) {
  e.preventDefault();
  
  if ($('.rakit-tooltip-cart-menu').length < 1) {

    var tooltipHtml = 'This is your chance to put a smile on someone\'s face without ever meeting them or even knowing them, and it\'s that blind generosity that makes theCHIVE community so unique. <a href="http://www.thechivery.com/blogs/the-chivery-herald/18314559-secret-santa-rakit-style" target="_blank">Learn more about these options.</a>';

    $('.drop-cart-rak .select-container').before('<div class="rakit-tooltip-cart-menu" style="display: none;"><div class="rakit-horz-line-top"></div><h4>RAK = Random Act of Kindness.</h4><p>'+tooltipHtml+'</p><div class="rakit-horz-line-bottom"></div><div class="close"></div></div>');
    $('.rakit-tooltip-cart-menu').addClass('open').slideDown(300);

  } else {

    if ($('.rakit-tooltip-cart-menu').hasClass('open')) {
      $('.rakit-tooltip-cart-menu').removeClass('open').slideUp(300);
    } else {
      $('.rakit-tooltip-cart-menu').addClass('open').slideDown(300);
    }
    
  }
});
$(document).on('click','.rakit-tooltip-cart-menu .close', function() {
  $('.rakit-tooltip-cart-menu').slideUp(300);
});
</script>

