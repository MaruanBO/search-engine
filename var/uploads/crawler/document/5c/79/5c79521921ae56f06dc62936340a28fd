		<script type="text/javascript">
		var recording;
		var intentTo; 
		$('.live-cam').mouseenter(function(){
	        $('.live-cam .light').show();
			clearTimeout(intentTo);
			intentTo = setTimeout(function() {
				cameraLight();
			}, 100);
		});
		$('.live-cam').mouseleave(function() {
			$('.live-cam .light').fadeOut(100);
			clearInterval(recording);
			clearTimeout(intentTo);
		});

		var cameraLight = function() {
            recording = setInterval(function() {
               $('.live-cam .light').fadeOut(100).show();
            }, 400);
		}
		</script>
