<script type="text/javascript">
jQuery(document).ready(function($) {

    var slides = $('#slider1 .slide');
    slides.sort(function(a, b) {
        var contentA = parseInt($(a).attr('data-sort'));
        var contentB = parseInt($(b).attr('data-sort'));
        return (contentA < contentB) ? -1 : (contentA > contentB) ? 1 : 0;
    });

  	

    $('#slider1_container').find('.slide').remove();
    $('#slider1_container #slider1').html(slides);
	$('#slider1_container').css( "visibility", "visible" );

  
    var _SlideshowTransitions = [
        {$Duration:1200,$Opacity:2}
    ];    

    //$(window).on('resize', function(){
        var win = $(this); //this = window
        if (win.width() < 1024) {

            $("#slider1_container img").each(function() {
                $(this).attr("src", $(this).attr("src").replace("desktop_slide", "tablet_slide"));
            });  

            if (win.width() > 767) {
                $("#slider1_container img").each(function() {
                    $(this).attr("src", $(this).attr("src").replace("desktop_slide", "tablet_slide"));
                    $(this).attr("src", $(this).attr("src").replace("mobile_slide", "tablet_slide"));
                });
            } else {
                $("#slider1_container img").each(function() {
                    $(this).attr("src", $(this).attr("src").replace("tablet_slide", "mobile_slide"));
                });                
            }

        } else {

            $("#slider1_container img").each(function() {
                $(this).attr("src", $(this).attr("src").replace("tablet_slide", "desktop_slide"));
                $(this).attr("src", $(this).attr("src").replace("mobile_slide", "desktop_slide"));
            }); 

        }    
    //});

    var mobileOptions = {
        $AutoPlay:true,
        $SlideDuration: 100,
        $AutoPlayInterval: 4000,
        $DragOrientation: 1,
        $BulletNavigatorOptions: {
            $Class: $JssorBulletNavigator$,
            $ChanceToShow: 2
        },
        $SlideshowOptions: {                                
            $Class: $JssorSlideshowRunner$,                
            $Transitions: _SlideshowTransitions,          
            $ShowLink: true                               
        }
    };   

    var jssor_slider1 = new $JssorSlider$('slider1_container', mobileOptions);
    $('.marquee-block').find('.jssora05l').click(function() {
        jssor_slider1.$Pause();
        jssor_slider1.$Prev();
    });
    $('.marquee-block').find('.jssora05r').click(function() {
        jssor_slider1.$Pause();
        jssor_slider1.$Next();
    });

    $('.marquee-block').find('.bullet').click(function() {
        var index = $(this).index();
        jssor_slider1.$PlayTo(index);
    });

    //responsive code begin
    function ScaleSlider() {
        var parentWidth = $('#slider1_container').parent().width();
        if (parentWidth) {
            jssor_slider1.$SetScaleWidth(parentWidth);
        }
        else
            window.setTimeout(ScaleSlider, 30);
    }
    //Scale slider after document ready
    ScaleSlider();
    if (!navigator.userAgent.match(/(iPhone|iPod|iPad|BlackBerry|IEMobile)/)) {
        //Capture window resize event
        $(window).bind('resize', ScaleSlider);
    }
    if (navigator.userAgent.match(/(iPhone|iPod|iPad)/)) {
        $(window).bind("orientationchange", ScaleSlider);
    }

});
</script>




