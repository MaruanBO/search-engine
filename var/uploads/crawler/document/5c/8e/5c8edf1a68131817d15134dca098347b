<script src="/scripts/common/js/jquery.bxslider.min.js" type="text/javascript"></script>
<script type="text/javascript">
// SCRIPT FOR FRONT PAGE SLIDER --------------------------------------------------------------------- //
$(document).ready(function() {
    document.getElementById("slide01").style.display="block";
	document.getElementById("slide02").style.display="block";
    document.getElementById("slide03").style.display="block";
    document.getElementById("slide04").style.display="block";
    document.getElementById("slide05").style.display="block";
    $("div.sliderBackground#sliderBkgrnd01").css("background-image", "url(/images/common/frontpage/sliderfull_ind_awards_002.jpg)");
});
$(document).ready(function(){
	$('.bxslider').bxSlider({
	  auto: true, // Slides will automatically transition
	  autoDelay: 0, // Time (in ms) auto show should wait before starting
	  autoHover: false, // Will pause when mouse hovers over slider
	  pause: 5000, // The amount of time in ms between each auto transition
	  speed: 800,
	  mode: 'fade', // 'horizontal', 'vertical', 'fade'
	  adaptiveHeight: true, // Dynamically adjust slider height based on each slide's height
	  infiniteLoop: true, // If true, clicking "Next" while on the last slide will transition to the first slide and vice-versa
	  hideControlOnEnd: false, // If true, "Next" control will be hidden on last slide and vice-versa (Only used when infiniteLoop: false)
	  touchEnabled: false, // If true, slider will allow touch swipe transitions
	  swipeThreshold: 50, // Amount of pixels a touch swipe needs to exceed in order to execute a slide transition (Only used if touchEnabled: true)
	  controls: false, // If true, "Next" / "Prev" controls will be added
	  pager: true, // If true, a pager will be added
	  pagerType: 'full', // If 'full', a pager link will be generated for each slide. If 'short', a (x/y) pager will be used
	  pagerShortSeparator: '/', // If pagerType: 'short', pager will use this value as the separating character
	  pagerCustom: '#bx-pager', // Parent element to be used as the pager. Parent element must contain a <a data-slide-index="x"> element for each slide. Not for use with dynamic carousels.
	  //autoControls: true, // Displays play and pause icons
	  startText: '', // Shows text in place of play icon
	  stopText: '', // Shows text instead of pause icon
	  onSlideBefore: function($slideElement, oldIndex, newIndex) {
	  	var slideElementClass = $($slideElement.find("div.slidercontent > div")).attr("class");
		var sliderBkgrndNew = newIndex % 2 != 0 ? "sliderBkgrnd02" : "sliderBkgrnd01";
		var sliderBkgrndOld = sliderBkgrndNew == "sliderBkgrnd02" ? "sliderBkgrnd01" : "sliderBkgrnd02";
		if (slideElementClass == "slide01")
			$("div.sliderBackground#" + sliderBkgrndNew).css("background-image", "url(/images/common/frontpage/sliderfull_ind_awards_002.jpg)");
		else if (slideElementClass == "slide02")
			$("div.sliderBackground#" + sliderBkgrndNew).css("background-image", "url(/images/common/frontpage/sliderfull_ind_lowcost_002.jpg)");
		else if (slideElementClass == "slide03")
			$("div.sliderBackground#" + sliderBkgrndNew).css("background-image", "url(/images/common/frontpage/sliderfull_ind_globalproducts_002.jpg)");
		else if (slideElementClass == "slide04")
			$("div.sliderBackground#" + sliderBkgrndNew).css("background-image", "url(/images/common/frontpage/sliderfull_ind_tradingdemos_002.jpg)");
		else if (slideElementClass == "slide05")
			$("div.sliderBackground#" + sliderBkgrndNew).css("background-image", "url(/images/common/frontpage/sliderfull_ind_education_002.jpg)");
		$("#" + sliderBkgrndOld).fadeOut(800);
		$("#" + sliderBkgrndNew).fadeIn(800);
	  }
	});
});
</script>
