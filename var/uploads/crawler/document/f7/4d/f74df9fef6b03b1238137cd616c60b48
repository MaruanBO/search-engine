<script type="text/javascript">
PLATFORM.EventMan.registerToEvent(function() {
  $wn('#DisplaySizeId80, #DisplaySizeId82').find('.wnGroup').each(function() {  
    $wn(this).find('.wnItem.feature').each(function(i) {
      if (i === 0) {
        // First story
        $wn(this).prepend($wn(this).find('.summaryImage'));
        this.className = this.className.replace(/displaySizeId[^\s]+/g, '');
        $wn(this).addClass('displaySizeId-7');
        
        var storyId = (function(href) {
          var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
          return matches[1] || '';
        })($wn(this).find('a:first').attr('href'));
        $wn(this).find('.summary.abridged').append('<span class="csMobdubTarget" data-storyid="' + storyId + '">...</span>');
      
      } else {
        // Story 2 and above
        this.className = this.className.replace(/displaySizeId[^\s]+/g, '');
        $wn(this).addClass('displaySizeId-10'); 
      }
    });
  });
  $wn('.csHideParent').parent().hide();
}, 'WNCol23done');
</script>

