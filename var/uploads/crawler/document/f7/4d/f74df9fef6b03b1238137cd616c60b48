<script type="text/javascript">
PLATFORM.EventMan.registerToEvent(function() {
  var defaultImg = '<div class="wnContent summaryImage left"><a href="{$href}"><img border="0" src="http://WCAX.images.worldnow.com/images/900761_G.jpg"></a></div>';
  var $target = $wn('#DisplaySizeId-7');
  var clearEvery;

  if ($wn('#WNCol23').hasClass('wnColConfig2')) {
    // Wide layout (4-up)
    clearEvery = 4;

  } else if ($wn('#WNCol23').hasClass('wnColConfig23')) {
    // Narrow layout (2-up)
    clearEvery = 2;

  } else {
    // ::shrug::
    return false;
  }

  $target.find('.wnItem.header').append(
    '<div class="csGridList">' +
      '<a class="csGrid" href="#"></a>' +
      '<a class="csList" href="#"></a>' +
    '</div>'
  );
  
  $target.find('.wnItem.feature').each(function() {
    if ($wn(this).find('.summaryImage:not(".abridged")').length === 0) {
      var storyHref = $wn(this).find('a:first').attr('href');
      $wn(this).csFormatterPrepend(defaultImg, {href: storyHref});
    }
  });

  var preReset = function() {
    $target[0].className = $target[0].className.replace(/displaySizeId[^\s]+/g, '');
    $target.find('.wnItem.wnClear').remove();
    $target.find('.csGridList a').removeClass('csActive');
  };

  $target.find('.csGridList a.csGrid').click(function() {
    // GRID MODE
    preReset();
    $wn(this).addClass('csActive');
    $target.addClass('displaySizeIdCSGrid');
    $target.find('.wnItem.feature').each(function(i) {
      $wn(this).find('.summaryImage:not(.abridged)').after($wn(this).find('.headline:not(.abridged)'));
      if (i % clearEvery === 0) {
        $wn(this).before('<li class="wnItem wnClear"></li>');
      }
    });
    return false;
  });

  $target.find('.csGridList a.csList').click(function() {
    // LIST MODE
    preReset();
    $wn(this).addClass('csActive');
    $target.addClass('displaySizeIdCSList');
    $target.find('.wnItem.feature').each(function() {
      $wn(this).find('.headline:not(.abridged)').after($wn(this).find('.summaryImage:not(.abridged)'));
    });
    return false;
  });

  $target.find('.csGridList a:first').click();
  $wn('.csHideParent').parent().hide();
}, 'WNCol2done');
</script>

