<script type="text/javascript">
PLATFORM.EventMan.registerToEvent(function() {
  $wn('#WNCol2 #DisplaySizeId7').addClass('csDontMiss');
  $wn('#WNCol23Top').append($wn('#DisplaySizeId7.csDontMiss'));
  $wn('#DisplaySizeId7.csDontMiss .wnGroup:gt(0)').hide();
  $wn('#DisplaySizeId7.csDontMiss .wnItem.feature:gt(3)').hide();
  $wn('#DisplaySizeId7.csDontMiss .wnItem.feature').each(function() {
    $wn(this).prepend($wn(this).find('.summaryImage'));
    /* -- videos can't have comments
    var storyId = (function(href) {
      var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
      return matches[1] || '';
    })($wn(this).find('a:first').attr('href'));
    $wn(this).find('.summary.abridged').append('<span class="csMobdubTarget" data-storyid="' + storyId + '">...</span>');
    */
  });
  $wn('.csHideParent').parent().hide();
}, 'WNCol23done');
</script>

