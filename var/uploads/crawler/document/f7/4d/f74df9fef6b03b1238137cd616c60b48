<script type="text/javascript">
$wn(document).bind('csCbMostPop', function() {
  var titleCutoff = ' - WCAX.COM';
  var url = "http://api.chartbeat.com/live/toppages/?jsonp=?&host=wcax.com&apikey=ee504f50276e9fc7f1261df2b0278f57&limit=15";
  $wn.getJSON(url, function(data) {
    if (data.length > 0) {
      var rowTemplate = '' +
        '<div class="csCbRow">' +
          '<div class="csCbCellLeft"><a href="//{$path}">{$pageTitle}</a></div>' +
          '<div class="csCbCellRight">currently {$visitors} {$peopleWord}</div>' +
        '</div>';
    
      var index = 0;
      $wn.each(data, function() {
        // Check if we've done enough
        if (index >= 7) {
          return false;  //break
        }
        
        // Skip this result if it doesn't seem like a story
        if (false === /\/story\//i.test(this.path)) {
          return true;  //continue
        }
        
        this.peopleWord = (this.visitors === 1 ? 'person' : 'people');
        this.pageTitle = this.i;
        
        var len = this.pageTitle.indexOf(titleCutoff);
        if (len > 0) {
          this.pageTitle = this.pageTitle.substring(0, len);
        }
        
        $wn('#csChartbeatPopData').csFormatterAppend(rowTemplate, this);
        index++;
      });
      $wn('#csChartbeatPopular').show();
    }
  });
});
</script>

<div id="csChartbeatPopular">
  <h3>Most popular pages on wcax.com</h3>
  <div id="csChartbeatPopData"></div>
