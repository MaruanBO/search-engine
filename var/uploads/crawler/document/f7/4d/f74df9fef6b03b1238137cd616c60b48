<script type="text/javascript" src="http://ftpcontent.worldnow.com/wncustom/js/jquery.csutils.js"></script>

<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		// Load Story Data
		console.log('first');

		var storyData = [];
		$wn.merge(storyData, $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract());
		$wn.merge(storyData, $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract());
		console.log('story');
		console.log("Story Data: " + storyData);

		if (storyData.length < 1) {
			return;
		}

		// Build and pre-populate the TSR
		var $stage = $wn('#csTsr .csTsrStage');
		var $links = $wn('#csTsr .csTsrLinks ul');
		$wn.each(storyData, function(i) {
			if (i >= csTsrMaxStories) {
				return false;
				//break
			}

			// Doing some date modification as per Browser
			if ((navigator.userAgent.toLowerCase().indexOf('chrome') > -1 || navigator.userAgent.toLowerCase().indexOf('safari') > -1 || navigator.userAgent.toLowerCase().indexOf('trident') > -1) && /DT/.test(this.dateText)) {
				var mDate = new Date(this.dateText);
				mDate = (mDate.setHours(mDate.getHours() + 1));
				this.dateText = mDate;
			}
			
			// Pre-render some stuff for the template
			this.index = i;
			this.custDate = (new Date(this.dateText)).customFormat('%MMM% %D%, %YYYY% %h%:%mm% %AMPM%');
			this.hasVideo = (this.isClip || this.isVideo);
			this.hasNew = (function(dtext) {
				var now = (new Date()).getTime();
				var then = (new Date(dtext)).getTime();
				var diff = Math.abs(now - then) / 1000;
				return (diff < csTsrNewCutoff);
				//newer than [whatever]
			})(this.dateText);
			this.storyId = (function(href) {
				var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
				return matches[1] || '';
			})(this.headlineHref);
			if (!this.graphicSrc) {
				this.graphicSrc = csTsrDefaultImg;
			}

			// Add this story's slide to the stage
			$stage.csFormatterAppend('<div class="csSlide" id="csSlideId{$index}">' + '<a href="{$headlineHref}"><img src="{$graphicSrc}"></a>' + '<div class="csTsrBottom">' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}<img class="csHasVideo" src="http://ftpcontent.worldnow.com/wcax/custom/2012/icon_play.png">{/if}' + '</h4>' + '{if $dateText}<div class="csDate">Updated: {$custDate}</div>{/if}' + '<div class="csSummary">' + '{$summaryHtml}' + '</div>' + '</div>' + '</div>', this);

			// Add the nav link
			$links.csFormatterAppend('<li class="csLink" id="csLinkId{$index}">' + '{if $hasNew}<img class="csHasNew" src="http://ftpcontent.worldnow.com/wcax/custom/2012/icon_new.png">{/if}' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}<img class="csHasVideo" src="http://ftpcontent.worldnow.com/wcax/custom/2012/icon_play.png">{/if}' + '</li>', this);

			// Make the links actually do something
			(function(index) {//closure
				$wn('#csTsr #csLinkId' + index).hover(function() {
					$stage.find('.csSlide').not('#csSlideId' + index).hide();
					$stage.find('#csSlideId' + index).show();
					$links.find('.csLink').removeClass('csActive');
					$wn(this).addClass('csActive');
				});
			})(i);
		});

		// Trigger slide 1 (so the stage isn't empty)
		$wn('#csTsr .csTsrLinks .csLink').first().mouseover().mouseout();

		// Move the TSR into place, hide original sources
		$wn('#csTsr').parent().hide();
		$wn('#WNTSWContent').before($wn('#csTsr'));
		$wn('#WNTSWContent, #WNCol2 .wnDS-20').hide();
	}, 'WNCol23done'); 
</script>

