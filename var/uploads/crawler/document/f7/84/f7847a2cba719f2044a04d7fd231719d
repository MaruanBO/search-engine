        <script type="text/javascript">
            var giftUrl = function() {
                var form =  $('#gifts');
                var select = $(form).find('select');
                var action = 'http://www.giftbasket.ru/catalog';
                $(select).change(function () {
                    var option = $(select).find(':selected').attr('value');
                    var url = action + '/' + option;
                    window.open(url);
                    $(form).submit(function () {
                        window.location = url;
                        return false;
                    });
                });
            };
            giftUrl();
        </script>

