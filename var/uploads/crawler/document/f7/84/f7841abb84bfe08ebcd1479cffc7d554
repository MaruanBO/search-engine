    <script type="text/javascript">
    /* protection from extensions/toolbars/addons breaking functionality */
    var hasMutationObserver = "MutationObserver" in window;
    if(hasMutationObserver){
        try {
            var MutationObserver = window.MutationObserver || window.WebKitMutationObserver || window.MozMutationObserver;
            var page = document.querySelector('body');
            var observer = new MutationObserver(function(mutations) {  
                mutations.forEach(function(mutation) {
                    if (mutation.type === 'attributes') {
                        if(window.getComputedStyle(document.querySelector('body')).getPropertyValue("position") != "static") {
                            document.querySelector('body').style.position = "static";
                        }
                    }
                });
            });
            observer.observe(page, {
                attributes: true, 
                childList: true, 
                characterData: true 
            });
        } catch(error){}
    }
    </script>
