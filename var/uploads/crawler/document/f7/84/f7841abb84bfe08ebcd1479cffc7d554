	<script type="text/javascript" src="//content.delta.com/dlhome/jawr_loader.js?build=20140619"></script>
	<script type="text/javascript">
	JAWR.loader.mapping =JAWR.loader.mapping;
	JAWR.loader.cssmapping =  JAWR.loader.cssmapping;
	JAWR.deltaLoader = {
		script: function (path) {
			this.insert(JAWR.loader.jsbundles, 'insertScript', JAWR.loader.mapping, path);
		},
		style: function (path, media) {
			this.insert(JAWR.loader.cssbundles, 'insertCSS', JAWR.loader.cssmapping, path, media);
		},
		appendScript : function(path) {
			this.append(JAWR.loader.jsbundles, 'addScriptTag', JAWR.loader.mapping, path);
		},
		insert: function (bundles, func, mappingToUse, path, media) {
			for (var x = 0; x < bundles.length; x++) {
				var bundle = bundles[x];
				if (bundle.belongsToBundle(path) && !JAWR.loader.usedBundles[bundle.name]) {
					JAWR.loader.usedBundles[bundle.name] = true;
					var pathtoRender = bundle.alternateProductionURL ? bundle.alternateProductionURL : this.normalizePath(mappingToUse + '/' + bundle.prefix + bundle.name);
					if (bundle.ieExpression) {
						JAWR.loader.insertCondComment(bundle.ieExpression, func, pathtoRender, media);
					} else { 
						JAWR.loader[func](pathtoRender, media);
					}
				}
			}
		},
		append: function (bundles, func, mappingToUse, path, media) {
			for (var x = 0; x < bundles.length; x++) {
				var bundle = bundles[x];
				if (bundle.belongsToBundle(path) && !JAWR.loader.usedBundles[bundle.name]) {
					JAWR.loader.usedBundles[bundle.name] = true;
					var pathtoRender = bundle.alternateProductionURL ? bundle.alternateProductionURL : this.normalizePath(mappingToUse + '/' + bundle.prefix + bundle.name);
					this[func](pathtoRender, media);
				}
			}
		},
		addScriptTag: function (path) {
			DeltaUtils.addScriptTag(path);
		},
		normalizePath : function (path) {
			var isRelativePathing = path.indexOf('//') === 0;
			var subPath = isRelativePathing ? path.substring(path.indexOf('//') + 1) : path;
			while (subPath.indexOf('//') != -1) {
				subPath = subPath.replace('//', '/');
			}
			path = isRelativePathing ? path = path.substring(0, path.indexOf('//') + 1) + subPath : subPath
			if (path.indexOf("http") == 0) {
				path = path.replace(/http:\/(\w)/g, "http://$1").replace(/https:\/(\w)/g, "https://$1");
			}
			return path;
		}
	};
	</script>
