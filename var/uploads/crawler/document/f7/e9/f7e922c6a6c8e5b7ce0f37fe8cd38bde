<script type="text/javascript">
    function WaitForjQuery() {
        if (typeof $ === 'undefined') {
            setTimeout(WaitForjQuery, 100);
        }
        else {
            $('#widget-tabs li').removeClass('active');
            $('#widgetContent .tab-pane').removeClass('active in');
            var lastSearch = localStorage.getItem("lastSearch");
            var widgetId = null;
            switch (lastSearch) {
                case "car":
                    $('#carWidget').addClass('active in');
                    $('#showCars').addClass('active');
                    widgetId = '#carWidget';
                    break;
                case "hotel":
                    $('#hotelWidget').addClass('active in');
                    $('#showHotels').addClass('active');
                    widgetId = '#hotelWidget';
                    break;
                case "vacation":
                    $('#packageWidget').addClass('active in');
                    $('#showPkg').addClass('active');
                    widgetId = '#packageWidget';
                    break;
                case "flight": default:
                    $('#flightWidget').addClass('active in');
                    $('#showFlights').addClass('active');
                    widgetId = '#flightWidget';
                    break;
            };
            HandleWidgetTabLoad(widgetId);

            $(document).on('shown.bs.tab', '#widget-tabs a[data-toggle="tab"]', function (e) {
                var newObj = e.target.getAttribute('href');
                $(newObj).find(":input:not(input[type=button],input[type=radio],input[type=submit],button):visible:first").focus();
                HandleWidgetTabLoad(newObj);
            });
            if (navigator.userAgent.match(/msie/i) || navigator.userAgent.match(/trident/i) || navigator.userAgent.indexOf("Chrome") != -1 || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i)) {
                window.onload = function () {
                    var e = document.getElementById("refreshed");
                    if (e.value == "no") e.value = "yes";
                    else { e.value = "no"; location.reload(); }
                }
            }
        }

        function HandleWidgetTabLoad(tabId) {
            switch (tabId) {
                case "#flightWidget":
                    if (!window.config.flightLoaded) {
                        $("#flightWidget").flightSearch({
                            flightHolder: "flightWidget",
                            travelersTemplateID: "num-travelers-template",
                            adultsdropdownID: "adult-travelers",
                            seniorsdropdownID: "senior-travelers",
                            minorsdropdownID: "minor-travelers",
                            minorAgeSelectorID: "childrenPanel",
                            serviceClassDropdownID: "ddClass",
                            errMsgHolderID: "errMsg",
                            prefAirlineID: "preferred-airline",
                            tripTemplateID: "single-trip-template",
                            formID: "flight-search-form",
                            tripTypeGroupID: "trip-type-group",
                            submitButtonID: "search-btn",
                            flexiDateID: "chkFlexiDts",
                            directFlightID: "chkDirectFlt",
                            openJawID: "open-jaw-switch",
                            serviceClassDropdownHolderID: "select_class",
                            isPrimary: true
                        });
                        window.config.flightLoaded = true;
                    }
                    //LoadTabContent('F');
                    break;
                case "#hotelWidget":
                    LoadTabContent('H');
                    break;
                case "#carWidget":
                    LoadTabContent('C');
                    break;
                case "#packageWidget":
                    LoadTabContent('P');
                    break;
                case "#sampleFaresWidget":
                    LoadTabContent('DF');
                    break;
            }
        }
    }
    WaitForjQuery();
</script>




                    </article>
