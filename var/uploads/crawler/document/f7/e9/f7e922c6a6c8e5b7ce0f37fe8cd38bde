<!--NL SignUp Slider Start--->

<script type="text/javascript">
    var timeOut,
    today = new Date(),
    inActivityTime = 2 * 60 * 1000, //five minutes inactivity
    appPopup = $('#freeApp').is(':hidden');

    $(document).ready(function () {
        if ($('#freeApp').length) {
            if (!$.cookie("modalsliderbox") && appPopup) {
                document.onmousemove = resetTimer;
                document.onkeypress = resetTimer;
                timeOut = setTimeout(OpenSliderBox, inActivityTime);
            }
        } else {
            if (!$.cookie("modalsliderbox")) {
                document.onmousemove = resetTimer;
                document.onkeypress = resetTimer;
                timeOut = setTimeout(OpenSliderBox, inActivityTime);
            }
        }
        $(window).scroll(function () {
            resetTimer();
            var topPos = $(document).scrollTop() + 120;
            $('#divSliderBox').css({ top: topPos + 'px' })
        });
        $('#freeApp .close_btn').on('click', function () {
            if (!$.cookie("modalsliderbox")) {
                document.onmousemove = resetTimer;
                document.onkeypress = resetTimer;
                timeOut = setTimeout(OpenSliderBox, inActivityTime);
            }
        });
        $('#closesignupbtn').on('click', function () {
            btnCloseSBX_Click();
        });
    });

    $.cookie = function (j, n, k) {
        if (typeof n != "undefined") {
            k = k || {};
            if (n == null) {
                n = "";
                k.expires = -1
            }
            var g = "";
            if (k.expires && (typeof k.expires == "number" || k.expires.toUTCString)) {
                var e;
                if (typeof k.expires == "number") {
                    e = new Date();
                    e.setTime(e.getTime() + (k.expires * 86400000))
                } else {
                    e = k.expires
                }
                g = "; expires=" + e.toUTCString()
            }
            var l = k.path ? "; path=" + (k.path) : "";
            var f = k.domain ? "; domain=" + (k.domain) : "";
            var m = k.secure ? "; secure" : "";
            document.cookie = [j, "=", encodeURIComponent(n), g, l, f, m].join("")
        } else {
            var c = null;
            if (document.cookie && document.cookie != "") {
                var b = document.cookie.split(";");
                for (var h = 0; h < b.length; h++) {
                    var a = jQuery.trim(b[h]);
                    if (a.substring(0, j.length + 1) == (j + "=")) {
                        c = decodeURIComponent(a.substring(j.length + 1));
                        break
                    }
                }
            }
            return c
        }
    };

    function resetTimer() {
        if (timeOut != null) {
           
            clearTimeout(timeOut);
            timeOut = setTimeout(OpenSliderBox, inActivityTime); //five minutes inactivity
        }
    }

    function OpenSliderBox() {

        timeOut = null;
	//	var expirySliderCookie = new Date(today.getTime() + 86400000 * 7); // 7 days cookie age
	//	$.cookie("modalsliderbox", "modalsliderbox", { expires: expirySliderCookie });
        var topPos = $(document).scrollTop() + 125,
         leftPos = $('.banner-txt').offset().left;

        $('#divSliderBox').css({top: topPos + 'px'});

       
       
        $('#divSliderBox')
            .stop(true, true)
            .show(1000)
            .animate({ "left": "=400px" }, { duration: 500, queue: true });
       
		_gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Shown", 1, true]);
        
    }

    var signupServiceSBX = null;
    var responseAffiliateSBX = 'NoAffiliate';

    $(document).ready(function () {
        try {

            if ($('#tbEmailSBX').val() == '')
                $('#tbEmailSBX').val($('#hidEmailWatermarkSBX').val());

            $('#tbEmailSBX').focus(function () {
                if ($('#tbEmailSBX').val() == $('#hidEmailWatermarkSBX').val() || $.trim($('#tbEmailSBX').val()).indexOf(' ') > -1) {
                    $('#tbEmailSBX').val('');
                    $('#lblMessageSBX').hide();
                    if (typeof (signupServiceSBX) != 'undefined' && signupServiceSBX != null)
                        signupServiceSBX.CleanDefault();
                }
            });

            $('#tbEmailSBX').blur(function (event) {
                if ($('#tbEmailSBX').val() == "")
                    $('#tbEmailSBX').val($('#hidEmailWatermarkSBX').val());
            });

            

            $('#tbEmailSBX').keydown(function (event) {
                var keycode = (event.keyCode ? event.keyCode : (event.which ? event.which : event.charCode));
                if (keycode == 13) {
                   // SaveAlertSBX();
                    event.preventDefault();
                    return false;
                }
            });

            $('#lblFromSBX').text(GetQueryStringParamsSBX('oa'));
            $('#lblToSBX').text(GetQueryStringParamsSBX('da'));
        }
        catch (err) {
        }
    });    

    function btnCloseSBX_Click() {
        _gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Click_Close", 1, true]);
		var expirySliderCookie = new Date(today.getTime() + 86400000 * 30); // 30 days cookie age
		$.cookie("modalsliderbox", "modalsliderbox", { expires: expirySliderCookie });
        $('#divSliderBox').hide(1000);
    }

    function SaveAlertSBX() {
        try {

            var errorMessageSBX = "";
            var emailaddressValSBX = $('[id*=tbEmailSBX]').val();

            var emailRegSBX = /^([\w-\.]+@([\w-]+\.)+[a-z]{2,4})?$/;

            if (emailaddressValSBX == "" || emailaddressValSBX == $('#hidEmailWatermarkSBX').val()) {
                errorMessageSBX = "Please enter an email address.";
                $('#lblMessageSBX').text(errorMessageSBX).show();
                return false;
            }
             else if (!emailRegSBX.test(emailaddressValSBX) || emailaddressValSBX.split(/[A-Z]/gi).length - 1 < 1) {
                errorMessageSBX = "Please enter valid email address.";
                $('#lblMessageSBX').text(errorMessageSBX).show();
                return false;
            }


            signupService = new SignupService();
            signupService.TxtEmail = $('#tbEmailSBX');
            var baseAppPathSBX = '';
            if (location.href.indexOf('https://') > -1) {
                baseAppPathSBX = baseAppPathSBX.replace('http://', 'https://');
            }
            signupService.BaseAppPath = baseAppPathSBX;
            signupService.IsValidateEmail = true;
            signupService.ErrorInfo = '';
            signupService.DefaultEmail = $('#hidEmailWatermarkSBX').val();
            signupService.DefaultErrorInfo = '';
            signupService.Email = $.trim(emailaddressValSBX);
            signupService.FlowType = 'f';
            signupService.CampaignId = '5';
            signupService.FPAffiliate = responseAffiliateSBX;
            signupService.OriginCity = GetQueryStringParamsSBX('oa');
            signupService.DestinationCity = GetQueryStringParamsSBX('da');
            signupService.PageComponent = '2min-LightBox-S';
            signupService.PageCategory = 'SM-Flight-HP';
            signupService.SignUpV3();

            if (signupService.IsSignupSuccessful) {
               
                errorMessageSBX = "Thank You for Signing Up.";
                $('#lblMessageSBX').text(errorMessageSBX).removeClass("emlErrMsg").addClass("emlSccssMsg").show();
                _gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Click_Success", 1, true]);
            }
            else {
                if ($.inArray("Email id already exists.", signupService.ErrorInfo.split("~")) >= 0) {
                    _gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Click_UserAlreadyExists", 1, true]);
                    errorMessageSBX = "Email id already exists.";
                    $('#lblMessageSBX').text(errorMessageSBX).removeClass("emlSccssMsg").addClass("emlErrMsg").show();
                }
                else
                    _gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Click_Failure", signupService.ErrorInfo, true]);
            }
            setTimeout(callBack_Success, 1000);
			var expirySliderCookie = new Date(today.getTime() + 86400000 * 365); // 365 days cookie age
			$.cookie("modalsliderbox", "modalsliderbox", { expires: expirySliderCookie });
        }
        catch (err) {
            _gaq.push(['_trackEvent', "HomePage", "2min-LightBox-S", "Click_FailureCatch", err, true]);
        }
    }

	function callBack_Success()	{
	    $('#divSliderBox').hide(1000);
	}

	function GetQueryStringParamsSBX(sParam) {
	    var sPageURL = window.location.search.substring(1);
	    var sURLVariables = sPageURL.split('&');
	    for (var i = 0; i < sURLVariables.length; i++) {
	        var sParameterName = sURLVariables[i].split('=');
	        if (sParameterName[0] == sParam) {
	            return sParameterName[1];
	        }
	    }
	    return '';

	}

    function OpenNLTC_SBX() {
        window.open('/travel/privacy-policy.asp', 'taxfee', 'width=900,height=600,resizable=no,scrollbars=yes,toolbar=no,status=no,titlebar=no');
    }


    
</script>

