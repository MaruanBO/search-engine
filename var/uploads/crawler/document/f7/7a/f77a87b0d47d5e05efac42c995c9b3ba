<script type="text/javascript">
 	$(document).ready(function() {
				//this is where the menu toggling happens
				$(".mainitem").click(function(e){
					//this is to avoid the default action on click
					e.preventDefault();
					//get the rel tag of the element
					var get_element_rel = $(this).attr('rel');
					var get_element_id = $(this).attr('id');
					//append a hash to the rel tag to reference the element for toggle
					var id_to_toggle = "#"+get_element_rel;
					//loop through each ul that has a class of subitem
					$( ".subitem" ).each(function( index ) {
						//if the id of the item that you clicked on is equal to the rel attribute
						//then toggle the submenu within that
						if($( this ).attr('id') == get_element_rel) {
							//if item is visible, show it; if not, hide it.
							if ($(id_to_toggle).is(':visible')) {
								$( id_to_toggle).slideUp( "slow", function() {
									// Animation complete.
									console.log(get_element_rel+" side");
									$("#"+get_element_id+" img").attr('src', '/img/corp/bg_right_arrow.png?v=1');
								});
							//$(id_to_toggle).hide();
							} else {
								$( id_to_toggle).slideDown( "slow", function() {
				  			  	// Animation complete.
				  			  		console.log(get_element_rel+" side 1");
				  			  	    $(".mainitem img").attr('src', '/img/corp/bg_dropdown_arrow.png?v=2');
								});
								//$(id_to_toggle).show();
							}
					//close all others
				 	} else {
					 	$(this).slideUp( "slow", function() {
	  						  // Animation complete.
	  						  console.log(get_element_rel+" side 2");
	  						  $(".mainitem img").attr('src', '/img/corp/bg_right_arrow.png?v=1');
						});
						//$(this).hide();
				 	}
				});
			});
				
