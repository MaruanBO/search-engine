		    var URL = document.goToUrl.mobileSelect.options[document.goToUrl.mobileSelect.selectedIndex].value;
		    alert(URL);
		    document.location.href = URL;
		 }
		 
		 $(".showmobileNav").click(function(){
			$('.navLinks_mobile').show();
			$('.showmobileNav').hide();
		 });
		 $(".hidemobileNav").click(function(){
			$('.navLinks_mobile').hide();
			$('.showmobileNav').show();
		 });
		 
	});

 	function redirec_to_url(){
	 	//alert(1);
	    var URL = document.goToUrl.mobileSelect.options[document.goToUrl.mobileSelect.selectedIndex].value;
	    //alert(URL);
	    document.location.href = URL;
	 }
	 
    var timeout    = 500;
    var closetimer = 0;
    var ddmenuitem = 0;
    var prevddmenuitemId = 0;
    var isOpen     = false;
    
    function nav_open() {	
    	nav_canceltimer();
    	nav_close();
    	ddmenuitem = $(this).find('ul').eq(0).css('visibility', 'visible');
	}
	
    function nav_close() {	
    	if(ddmenuitem) {
    		ddmenuitem.css('visibility', 'hidden');
    	}
    }
    
    function nav_timer() {	
    	closetimer = window.setTimeout(nav_close, timeout);
    }
    
    function nav_canceltimer() {	
    	if(closetimer) {
			window.clearTimeout(closetimer);
    		closetimer = null;
    	}
	}
	
	function nav_toggle() {	
		try {
			if($(this).find('ul').eq(0).context.id) {
		    	if(isOpen && prevddmenuitemId == $(this).find('ul').eq(0).context.id) {
		    		isOpen = false;
		    		closetimer = window.setTimeout(nav_close, timeout);
		    	} else {
		    		isOpen = true;
		    		if(closetimer) {
						window.clearTimeout(closetimer);
			    		closetimer = null;
			    	}
			    	if(ddmenuitem) {
			    		ddmenuitem.css('visibility', 'hidden');
			    	}
			    	ddmenuitem = $(this).find('ul').eq(0).css('visibility', 'visible');
			    	prevddmenuitemId = $(this).find('ul').eq(0).context.id;
		    	}
	    	} else {
	    		isOpen = false;
		    	closetimer = window.setTimeout(nav_close, timeout);
	    	}
    	} catch (e) {
    		isOpen = false;
	    	closetimer = window.setTimeout(nav_close, timeout);
    	}
	}
	
    $(document).ready(function() {
        $('#nav .nav-name').bind('mouseover', nav_open);
    	$('#nav .nav-name').bind('mouseout', nav_timer);
    	$('#nav .nav-name').bind('touchstart', nav_toggle);
    });

    //document.onclick = nav_toggle;
</script>
