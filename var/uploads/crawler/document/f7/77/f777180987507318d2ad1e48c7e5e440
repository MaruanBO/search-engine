<script>//ajax login info so not cached
$(function() {
var login = $.ajax({ type: 'GET', cache: false, url: '/includes/floating-nav-login.php', data: {id: 'login-nav'}, 
    success: function(data) {
       $('#floating-nav-login').html(data);
       //alert(data);
		var $nav = $('#nav-shortcut'),
			navLeft = $nav.offset().left;
			
			if ($('.tools').length > 0) {
				navRight = $('.tools').offset().left;
			
			}
			else {
				$floaty_nav_container_width = $(".shortcut-bar .container").innerWidth();
				$logo_width = $(".shortcut-bar" .logo).outerWidth();
				navRight = $floaty_nav_container_width - $logo_width;
			}
			$nav.width((navRight - navLeft) - 15);
    }
})
.error(function() {
    console.log('There was an error making the AJAX request');
});
});
</script>



