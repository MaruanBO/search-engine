            	                     </div><!-- fin_headlines_fullsize_items -->                                    

    <script>        
    // Assign id numbers to the items in the main image frame
    $("#fin_top_promos .fin_headlines_item")
      .each(function(index){
        $(this).data('linkid',index);
        if(index==0) {
          $(this).addClass('fin_top_promos_current');
        }
      });

    // Create the thumbnails
    $('#fin_top_promos').append('<div id="fin_top_promos_thumbs">' + $("#fin_top_promos .fin_headlines_fullsize_items").html() + '</div>');

    // Enable the thumbnails
    $("#fin_top_promos_thumbs .fin_headlines_item")
      .each(function(index){
        $(this).data('tnlinkid', index);
        if(index==0) {
          $(this).addClass("fin_top_promos_current"); 
        }
      })
      .hover(function(){
        var tnhovered = $(this).data('tnlinkid');
        if(tnhovered != $('#fin_top_promos').data('current_id')) {
          $("#fin_top_promos_thumbs .fin_headlines_item").removeClass("fin_top_promos_current");
          $(this).addClass("fin_top_promos_current");
          
          $('#fin_top_promos .fin_headlines_fullsize_items .fin_headlines_item').removeClass("fin_top_promos_current");
          $('#fin_top_promos .fin_headlines_fullsize_items .fin_headlines_item').each(function(){
            if($(this).data('linkid') == tnhovered){
              $(this).addClass('fin_top_promos_current');
              var this_top = $(this).find('.fin_headlines_title').outerHeight() - $(this).find('.fin_headlines_summary').outerHeight() - 1;
              $(this).find('.fin_headlines_summary').css({top: this_top + 'px'});
            }
          });
          $('#fin_top_promos').data('current_id', tnhovered);
        }
      },function(){
        // blur
      });

      // Enable the description
      $("#fin_top_promos .fin_headlines_fullsize_items .fin_headlines_item").hover(function(){
        var this_top = $(this).find('.fin_headlines_title').outerHeight();
        $(this).find('.fin_headlines_summary').stop().animate({top:this_top+'px'},{queue:false,duration:160});
      }, function() {
        var this_top = $(this).find('.fin_headlines_title').outerHeight() - $(this).find('.fin_headlines_summary').outerHeight() - 1;
        $(this).find('.fin_headlines_summary').stop().animate({top: this_top + 'px'},{queue:false,duration:160});
      });
  </script>
</div>

