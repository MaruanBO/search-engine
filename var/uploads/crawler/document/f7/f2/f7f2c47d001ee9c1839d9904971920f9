<script type="text/javascript">
if(col4wxNews.feedName && col4wxNews.feedName.length > 0){
	CScol4wxNews();
}

function CScol4wxNews(){
	var iconBase = 'http://ftpcontent.worldnow.com/wncustom/wx_icons/wsi55/';
	CSWxManager.ready(function(wx) {
		 if (wx.length === 0) {
			 return;
		 }
	 
		 function col4Rotate(){
			 // Specific to the MyWeather XML format
			 //var hi = wx.fetch('forecastdays').first().find('forecast').attr('high');
			 //var lo = wx.fetch('forecastdays').first().find('forecast').attr('low');
			 var hi = wx.fetch('forecastdays').attr('HiTempF');
			 var lo = wx.fetch('forecastdays').attr('LoTempF');
		 
			 $wn('#CScol4WxNews .wxLoc').text(wx.fetch('name'));
			 $wn('#CScol4WxNews .wxIcon').css('background-image', 'url("' + iconBase + wx.fetch('iconcode') + '.png")');
			 $wn('#CScol4WxNews .wxTemp').html('Hi ' + hi + '&deg; / Lo ' + lo);
			 $wn('#CScol4WxNews .wxCond').text(wx.fetch('sky'));
		   
			 if (wx.length > 1) {
				 wx.next();
				 setTimeout(col4Rotate, 5000);
			 }
		 }
		 col4Rotate();
	});
    
	var status = 0;
	var totalParse = col4wxNews.feedCategory.length;
	var storage = new Array(totalParse);

	for(var ii=0;ii<totalParse;ii++){
		WNHttpRequestManager.makeRequest(col4wxNews.feedCategoryPath[0] + col4wxNews.feedCategory[ii] + col4wxNews.feedCategoryPath[1], {
			onSuccess: {callback : initFeed, args : ii},
			onError: function() {}
		});	
	}
	
	function initFeed(num){
		status++;
		col4newsParseFeed(this,num);
		if(status === totalParse){
			organizeItem();
		}
	}
	
	function col4newsParseFeed(xml,arg){
		var fullFeed = xml.response.responseXML;
    	var $target = $wn(fullFeed).find('story').not('[displaysize="19"]').first();
		if($target.length > 0){
			var id = $target.find("id:first").text();
			var headline = $target.find("headline:first").text();
			var pageURL = $target.find("seo:first pageurl").text();
			var fullURL = "/story/" + id + "/" + pageURL;
			if(!pageURL){
				pageURL = "article";
			}
			storage[arg] = "<li class='wnItem feature'><h4 class='wnContent headline'><div class='section'><a href='" + col4wxNews.feedSectionURL[arg] + "'>" + col4wxNews.feedName[arg] + "</a></div><div class='text'><a href='" + fullURL + "'>" + headline + "</a></div></a></h4><div class='wnClear'></div></li>";
		}
	}
	
	function organizeItem(){
		$wn.each(storage,function(index, value){ 
			if(value === undefined){
				storage.splice(index,1);
			}
			$wn("#CScol4WxNews .contentGroup").append(storage[index]);
		});
		$wn("#CScol4WxNews .feature:first").addClass("first");
		$wn("#CScol4WxNews .feature:last").addClass("last");
	}
	if(col4wxNews.blockTitle.length){
		$wn("#CScol4WxNews .header .text").html(col4wxNews.blockTitle);
	} else {
		$wn("#CScol4WxNews .header").hide();
	}
	if(col4wxNews.wxImage.length){
		$wn("#CScol4WxNews .wxFeature .wxInfo .wxImage").html('<a href="' + col4wxNews.wxImage[0] + '"><img src="' + col4wxNews.wxImage[1] + '" /></a>');
	}
	/*
	if(col4wxNews.wxLinksText.length > 1  && col4wxNews.wxLinkSchoolClosing === false){
		$wn("#CScol4WxNews .wxFeature .wxInfo").append('<a class="wxLinks one" href="' + col4wxNews.wxLinksURL[0] + '">' + col4wxNews.wxLinksText[0] + '</a><a class="wxLinks two" href="' + col4wxNews.wxLinksURL[1] + '">' + col4wxNews.wxLinksText[1] + '</a>');
	}
	if(col4wxNews.wxLinksText.length > 0 && col4wxNews.wxLinkSchoolClosing === true){
		$wn("#CScol4WxNews .wxFeature .wxInfo").append('<a class="wxLinks one" href="' + col4wxNews.wxLinksURL[0] + '">' + col4wxNews.wxLinksText[0] + '</a>');
	}
	if(col4wxNews.wxLinkSchoolClosing === true){
		$wn("#CScol4WxNews .wxFeature .wxInfo").append('<a class="wxLinks two closings" href="' + col4wxNews.wxLinkSchoolClosingInfo[0] + '">' + col4wxNews.wxLinkSchoolClosingInfo[1] + '</a>');
	}
	*/	
}
</script></div><script>Worldnow.Helpers.debug('UB NO HEADLINES-HOME PAGE TODAY on MYFOXDC.COM for top column 4(285500)', 'UB_285500');</script></div>
