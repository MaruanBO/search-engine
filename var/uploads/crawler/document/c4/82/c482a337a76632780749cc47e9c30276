<script type="text/javascript">
  $(document).ready(function() {
    TweenMax.set($('.ftSocialItem'), {autoAlpha:.3});
	TweenMax.set($('img', '.ftSocialItem'), {alpha:0});
	
	$('.ftSocialItem').bind('mouseover', function(){
		var $sibs = $(this).siblings();
		var $sibBlur = $(this).siblings().find('img:first');
		TweenMax.to($(this), .3, {autoAlpha:1});
		TweenMax.to($sibBlur, .3, {alpha:1});
	}).bind('mouseleave', function(){
		var $sibs = $(this).siblings();
		var $sibBlur = $(this).siblings().find('img:first');
		TweenMax.to($(this), .3, {autoAlpha:.3});
		TweenMax.to($sibBlur, .3, {alpha:0});
	});
  });
</script>
