	    		'/ann/common/home.jsp'                    : 'Home Page',
	    		'/ann/siteMap.jsp'                        : 'Site Map Page',
	    		'pageName=ATCard'                         : 'Customer Service Page:Credit Card',
	    		'pageName=Privacy'                        : 'Customer Service Page:Privacy & Security',
	    		'/ann/giftCard.jsp'                       : 'Gift Cards Page',
	    		'/ann/customer-service'             : 'Customer Service Page:Contact us',
	    		'http://www.anntaylorstorescorp.com/'     : 'Corporate Page',
	    		'http://careers.anntaylor.com/'           : 'Careers Page', 
				'http://www.annthinks.com'                : 'Blog Page',
				'/ann/department/AT-Apparel/cata00002'    : 'Apparel Landing Page',
				'/ann/department/AT-Shoes-%26-Accessories/cata00003' : 'Shoes & Accessories Landing Page',
				'/ann/department/AT-Petites/cata00004'    : 'Petites Landing Page',
				'/ann/department/AT-Tall/cat140002'       : 'Tall Landing Page',
				'/ann/department/AT-Lookbook/cata00006'   : 'Lookbook Landing Page',
				'/ann/department/AT-Weddings-%26-Events/cata00005' : 'Weddings & Events Landing Page',
				'/ann/department/AT-Sale/cata00007'       : 'Sale Landing Page',
				'/ann/findinstore/findInStore.jsp'		  : 'Style Finder Page',
				'/findinstore/findInStoreResults.jsp'		  : 'Style Finder Results Page',
                '/findinstore/inputDirections.jsp'		  : 'Style Finder Directions',
                '/international/internationalIndex.jsp'	  : 'Checkouts',
            	'/international/envoyOrderSuccess.jsp'	  : 'Thank you',
				'/cwm/instantcredit/cbcc/instantCreditCBCCRegistration.jsp':'CBCC Application Page',
				'/cwm/instantcredit/cbcc/instantCreditCBCCApproval.jsp':'CBCC Approval Page ',
				'/cwm/instantcredit/cbcc/instantCreditCBCCApprovalNeedMoreInfo.jsp':'CBCC Not Approved Page- Under Review',
				'/cwm/instantcredit/instantCreditRegistration.jsp':'PLCC Application Page',
				'/cwm/instantcredit/instantCreditApproval.jsp':'PLCC Approval Page ',
				'/cwm/instantcredit/instantCreditApprovalNeedMoreInfo.jsp':'PLCC Not Approved Page- Under Review'
            	}
	    	
	    	var dest = linksMap[href];
	    	if (dest == null) {
		    	var n = href.indexOf('/ann/custserv/custserv.jsp');
		    	if (n == 0) {
			    	var s = href.split('?');
			    	if (s[1] != null) {
				    	var t = s[1].split('&');
				    	dest = linksMap[t[0]];
				    	if (dest == null) {
					    	dest = href;
				    	}
			    	}
		    	} else {
		    		var arr = href.split('/');
		        	if (arr[2] != null && arr[2] == 'sale' && arr[3] != null) {
		        		dest = unescape(arr[3]);
		        	} else if (arr[2] != null && 
		                	(arr[2] == 'cat' || arr[2] == 'outfit' || arr[2] == 'editorial' || arr[2] == 'product') &&
		                	arr[3] != null && arr[4] != null) {
		        		dest = unescape(arr[3]) + ":" + unescape(arr[4]);
		        	} else {
		        		dest=href;
		        	}
		    	}
	    	}
	    	return dest;
		}
