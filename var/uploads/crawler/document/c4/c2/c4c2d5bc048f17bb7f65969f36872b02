</div>
<!--ls:end[component-1423816401588]--></div><div class="col_4 last tertiaryFeature" id="ls-row-5-col-1-row-4-col-1-row-1-col-1-row-3-col-2-row-1"><!--ls:begin[component-1351198571201]--><div class="newsNearMeBS">
<div class="linkList">
<div class="header">
<h2>NEWS NEAR ME</h2>
<ul>
<li class="enterZip popup">
<a class="isIcon myLocation" href="#settings"><span id="nnmZipCode">Change ZipCode</span></a>
<ul class="enterZip">
<li>
<form action="/tampabay/index.page?submit=true&componentID=1351198571201" method="POST" id="newsNearMeForm">
<input placeholder="Location" id="newsNearMeLoc" name="location" type="text"><button type="submit" id="newsNearMeBtn">Save</button>
</form>
</li>
</ul>
</li>
</ul>
</div>
<div class="content">
<ul id="newsNearMeList"></ul>
</div>
<script type="text/javascript">
                
                    $(document).ready(function() 
                        {
                            runNewsNearMe();
                        }
                    );
                    
                    function populateNewsNearMe() {
                        $.ajax(
                            {
                                type : 'GET',
                                url : '/services/newsNearMeData.page',
                                data: "location=" + encodeURIComponent($('#newsNearMeLoc').val()),
                                dataType: 'xml',
                                success: function(xml) {
                                    var zipLoc = $('#newsNearMeLoc').val();
                                    var locationParam =  $(xml).find("locationParam").text();
                                    //Clear existing news near me list items
                                    if (locationParam  && locationParam.length > 2) {
                                        $("#nnmZipCode").empty();
                                        $("#nnmZipCode").append(locationParam);
                                        $('#newsNearMeLoc').attr("value",locationParam);
                                    } else if (zipLoc  && zipLoc.length > 4) {
                                        $("#nnmZipCode").empty();
                                        $("#nnmZipCode").append(zipLoc);
                                        $('#newsNearMeLoc').attr("value",locationParam);
                                    }
                                    $("#newsNearMeList").empty();
                                    if ($(xml).find("staffArticle").length == 0 ) {
                                        $("#newsNearMeList").append("<li>No results were found.</li>");
                                    } else {
                                        $(xml).find("staffArticle").each(function() {
                                                $("#newsNearMeList").append("<li><a href=\"/" + $(this).find("uri").text() + "\">" + $(this).find("webHeadline").text() + "</a></li>");
                                            }
                                        );		
                                    }
                                }
                            }
                        );
                    }
                    
                    function runNewsNearMe() {
                        //Check if jQuery is loaded
                        if(window.$) {
                            $("#newsNearMeForm").submit(
                                function(e) {
                                    e.preventDefault();
                                    populateNewsNearMe();
                                    return false;
                                }
                            );
                            populateNewsNearMe();
                        } 
                        else {
                            //Wait 50 milliseconds and try again
                            window.setTimeout(runNewsNearMe, 50);
                        }
                    }
                
                </script>
</div>
</div>
