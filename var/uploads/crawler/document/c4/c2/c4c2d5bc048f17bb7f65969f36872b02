			var slideShowElements = $("#thumbnailSlideShow li").size();
			if( slideShowElements > 0 ) {
				var slideId = 1;
				intervalHandle = window.setInterval(function() {
					scrollSlideShow(slideId);
					slideId++;
					if(slideId>slideShowElements) {slideId = 1;}
				}, 7000);
			}

			function scrollSlideShow(slideId) {
				//display main photo
				$("#mainPhoto"+slideId).show();
				$("#mainPhoto"+slideId).siblings().hide();

				// remove active tag from the current and move it to the next
				var parent = $("#slide"+slideId).parent();
				parent.find('li.active').removeClass('active');
				$("#slide" + slideId).addClass('active');
			}
		</script>
