<!--
    OAS_url = 'http://ads.bhmedianetwork.com/RealMedia/ads/';
    OAS_sitepage = 'hickoryrecord.com/townnews';
    OAS_listpos = 'Top,Top1,TopRight,Middle,Left3,Right1,Bottom,BottomLeft,BottomRight';
    OAS_query = '';
    OAS_target = '_top';
    OAS_version = 10;
    OAS_rn = new String (Math.random()); OAS_rns = OAS_rn.substring (2, 11);

    _version=11;
    if (navigator.userAgent.indexOf('Mozilla/3') != -1 || navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1)
    {
        _version=10;
