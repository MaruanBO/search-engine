	<!-- SCRIPT SECTION 2:  Eloqua Web Data Lookups -->
    <script type="text/javascript">
        var timerId = null, timeout = 5;
		// SetElqContent is triggered by the Data Lookup Scripts automatically.  The "FirstLookup" modification allows for the Visitor Lookup to trigger the second Lookup (Contact/Datacard/Prospect/Company)
        var FirstLookup = true;
        var strTimeout = "";
        function SetElqContent() {
            if (!!(timerId)) {
                if (timeout == 0) {
                    strTimeout = "timeout"
                    return;
                }
				if (FirstLookup) {
                    if (this.GetElqContentPersonalizationValue) {
                        _elqQ.push(['elqDataLookup', escape(eloquaConfig.lookup.lookupIdPrimary), '<' + eloquaConfig.lookup.primaryUniqueField + '>' + GetElqContentPersonalizationValue(eloquaConfig.lookup.visitorUniqueField) + '</' + eloquaConfig.lookup.primaryUniqueField + '>']);
                        FirstLookup = false;
                        return;
                    }
                }
                else {
                	//This is triggered the after Lookup B fires.  LoadFields starts the field prepoulation process.
					if (this.GetElqContentPersonalizationValue) {
                        TrackAnalytic();
                        return;
                    }

           		}
                timeout -= 1;
            }
            timerId = setTimeout("SetElqContent()", 1000);
            return;
        }
        // LOOKUP A:  Eloqua Visitor Data Lookup from Cookie  -  Ensure VisitorUniqueField is pulled in the Profile View
        _elqQ.push(['elqDataLookup', escape(eloquaConfig.lookup.lookupIdVisitor), '']);

    </script>	

