    <script type="text/javascript">
    jQuery(function(){
        var SelfLocation = window.location.href.split('?');
        switch (SelfLocation[1]) {
            case "justify_right":
                jQuery(".megamiddlemenu").megamenu({ 'justify':'right' });
                break;
            case "justify_left":
            default:
                if($(document).width() >1024)
                	jQuery(".megamiddlemenu").megamenu({ 'activate_action':'mouseover', 'enable_js_shadow':false });
                else
                    jQuery(".megamiddlemenu").megamenu({ 'activate_action':'click', 'enable_js_shadow':false });

        }
    });

    function clickfunction(item,noSubnav)
    {
		if($(document).width() <1025)
        {
            if(noSubnav)
            {
                 window.location.href = $(item).attr("data-href");
                 return false;
            }

            var count = parseInt($(item).attr("data-clickCount")); 
            count=count+1;

			if(count==1)
            {
                //$(item).attr("href",$(item).attr("data-href"));
				$(item).attr("data-clickCount",count);
            }
            else
				$(item).attr("href",$(item).attr("data-href"));
        }
        else
        {
            var linkName=$(item).attr("data-href");
            if(linkName.indexOf("contact-us")>0)
            {
            	window.location.href = $(item).attr("data-href");
            }
            else
            {
            	window.location.href = $(item).attr("data-href");
            }
        }

        return false;
    }

    $(document).ready(function() {
        $("#header-middle-menu ul.megamiddlemenu li.mm-item a.mm-item-link").each( function(){
			$(this).attr("data-href",$(this).attr("href"));
            $(this).attr("href","javascript:void(0)");

        });
    });
</script></div>

	
