<script type="text/javascript">
    jQuery(function($){
        var SelfLocation = window.location.href.split('?');
        switch (SelfLocation[1]) {
            case "justify_right":
                jQuery(".ulCountry").simplemegamenu({ 'justify':'right' });
                break;
            case "justify_left":
            default:
                if($(document).width() > 1024)
                	jQuery(".ulCountry").simplemegamenu({ 'activate_action':'mouseover', 'enable_js_shadow':false });
                else
                    jQuery(".ulCountry").simplemegamenu({ 'activate_action':'click', 'enable_js_shadow':false });
        }
        
        $(document).ready(function() {
        	$(".ulpopupcountries li a").click(function(event) {
        		var $link = $(event.target);
        		var site = $link.data("site");
        		//on the author servers the mapping will come back the same as the path (no domain name)
        		//for that case just use /, otherwise double check trailing /
        		var wwwDomain = "https://www.optum.com/";
        		if (wwwDomain == "" || wwwDomain.indexOf("/content/") > -1) {
        			wwwDomain = "/";
        		} else if (wwwDomain.length > 0 && !wwwDomain.match("/$")) {
        			wwwDomain += "/";
        		}
        		location.href= wwwDomain + "bin/optum/countryResolver?site=" + encodeURIComponent(site) + "&path=" + encodeURIComponent(location.pathname);
        	});
        });
    }); 
</script></div>

