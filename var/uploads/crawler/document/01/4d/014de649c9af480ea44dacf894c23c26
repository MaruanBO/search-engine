<script type="text/javascript">
    //global search function, needs to be here so the path can be mapped
    function performSearch(query) {
        if (query != null && query != "") {
            //trigger search page here
            location.href = "https://www.optum.com/search-results.html?q=" + encodeURIComponent(query);
        }
    }
</script>
