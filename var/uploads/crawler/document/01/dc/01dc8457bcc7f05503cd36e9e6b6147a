<!-- Pageview tracker -->
<script type="text/javascript">
function create_cookie(name, value, days2expire, path) {
  var date = new Date();
  date.setTime(date.getTime() + (days2expire * 24 * 60 * 60 * 1000));
  var expires = date.toUTCString();
  document.cookie = name + '=' + value + ';' +
                   'expires=' + expires + ';' +
                   'path=' + path + ';';
}

function retrieve_cookie(name) {
  var cookie_value = "",
    current_cookie = "",
    name_expr = name + "=",
    all_cookies = document.cookie.split(';'),
    n = all_cookies.length;
 
  for(var i = 0; i < n; i++) {
    current_cookie = all_cookies[i].trim();
    if(current_cookie.indexOf(name_expr) == 0) {
      cookie_value = current_cookie.substring(name_expr.length, current_cookie.length);
      break;
    }
  }
  return cookie_value;
}

// Define cookie
var cookie_name = 'looper301';
// var cookie_value = '1';
// Set current page URL
var url = window.location.href;

// Check for cookie
var res = retrieve_cookie(cookie_name);

// Fire pop-under tag if cookie = 1, then increment
if(res == 1) {
	create_cookie(cookie_name, 2, 1, "/");
}

// If the cookie is set with a value >1, increment by 1
else if (res > 1) {
	var newres = parseInt(res) + 1;
	create_cookie(cookie_name, newres, 1, "/");
}

// If the cookie is set to 'nill', do nothing
else if (res == 'null') {
	//
}

// Otherwise, create the cookie with 1 day shelf-life when the utm_source = 'bl'
else if (url.indexOf('utm_source=bl')) {  
	create_cookie(cookie_name, 1, 1, "/");
}

// Otherwise, set cookie value to 'null'
else { 
	create_cookie(cookie_name, 'null', 1, "/");
}

// Set variable name for use in tags
var looper301 = res;
</script>
<!-- /Pageview tracker -->

<!-- Bounce if viewport width or height is unknown -->
<script>
var w=Math.max(document.documentElement.clientWidth,window.innerWidth||0);
var h=Math.max(document.documentElement.clientHeight,window.innerHeight||0);

if ((cvalue.indexOf('traop') === -1) && (cvalue.indexOf('traunkwn') === -1)) {
	if ((w == 0) || (h == 0)) { 
		// Attempt to resize window to 800x600
		window.resizeBy(800, 600);
		
		// Now, check again to see if we can determine the width and height
		var w2=Math.max(document.documentElement.clientWidth,window.innerWidth||0);
		var h2=Math.max(document.documentElement.clientHeight,window.innerHeight||0);
		if ((w2 == 0) || (h2 == 0)) {
			// If not, bounce the user from the site
			window.location = "http://www.yahoo.com"; 
		}	
	}
}

//console.log('document.documentElement.clientWidth: '+document.documentElement.clientWidth);
//console.log('window.innerWidth: '+window.innerWidth);
//console.log('document.documentElement.clientHeight: '+document.documentElement.clientHeight);
//console.log('window.innerHeight: '+window.innerHeight);
</script>

