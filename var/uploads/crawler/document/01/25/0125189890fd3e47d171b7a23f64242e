<script type="text/javascript" >
// Load the SDK Asynchronously
(function(d){
   var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
   if (d.getElementById(id)) {return;}
   js = d.createElement('script'); js.id = id; js.async = true;
   js.src = "//connect.facebook.com/en_US/sdk.js";
   ref.parentNode.insertBefore(js, ref);
 }(document));


window.fbAsyncInit = function() {
  FB.init({
    appId      : '118686281521793', // App ID
    version    : 'v2.1',
    status     : true, // check login status
    cookie     : true, // enable cookies to allow the server to access the session
    channelUrl : '//'+window.location.hostname+'/jhtml/fbchannel.htm',
    xfbml      : true  // parse XFBML
  });
  
  
  
 // listen for and handle auth.statusChange events
    FB.Event.subscribe('auth.statusChange', authenticateWithFB );
    
    
};

var blnLoggingOut = false;
var blnLoggingIn = false;

function loginwithredirect(response)
{
    blnLoggingOut = false;
    blnLoggingIn = true;

}
    
function authenticateWithFB(response) 
{
  if (blnLoggingOut)
  {
    FB.logout();
    return;
  }

  if (response.authResponse) 
  {  
    FB.api('/me?fields=picture,name,first_name,last_name,id,email,gender,locale', function(me){
    
        
      integrateFBLoginWithNR(me.email, me.first_name, me.last_name, response.authResponse.userID, response.authResponse.accessToken, me.picture);
    });

        
  } 
}


function integrateFBLoginWithNR(emailaddress, firstname, lastname, fbid, token, purl )
{
    $.ajax({
        url:"/ajax/LoginViaFacebookV2.ashx", 
        async: true,
        timeout: 20000,
        error:function(jq, textStatus, err)
        {
            $('.fbloginresponse').text(textStatus);
        }, // end error
        data:{ email: emailaddress, fn: firstname, ln: lastname, fid: fbid,  tk: token, pt:purl }, 
        success:function (r, status) 
        { 
            if (r.blnError)
            {   
                $('.fbloginresponse').text(r.strErrMsg);
            }
            else
            {
              
                if ("" != r.strUser)
                {
                    var strWelcome = r.strUser;
                    if (strWelcome.length > 8) strWelcome = strWelcome.substring(0, 8) + "...";
                    $('.clnkCreateAcc').replaceWith('<span style="color:#88A957;" title=' + r.strUser + ' >' + strWelcome + '</span>');
                    $('.clnkLogin').text('Logout');
                    $('.clnkLogin').attr('href','/login.aspx?action=logout');
                     
                }
            }
            
        } // end success
    });
}
</script>



	

