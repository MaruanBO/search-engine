 <!-- Slider -->
<script type="text/javascript">
      $(document).ready(function () {
        var cur = 1, count = $(".linkbtns").children().length;
        var interval = 7000; // 7 secs
        var T0;
        var w = 984;
        
        var $first, $cur, $next, $last, $slider, go;
        $first = $cur = $('.selected');
        $last = $('.last');
        $slider = $("#slider");
        go = true;
            
        $(".slink").click(function () { Stop(this); return false; });
        
        $("#slider").mouseenter(function () { Pause(); });                     // pause
        $("#slider").mouseleave(function () { Continue(); });  // resume
        var $slinks = $(".linkbtns");
            
        function Stop(obj) { 
            Pause();
            if($(obj).index() == 0)
                $cur = $last;
            else
                $cur = $(obj).prev(); 
            
            Next(); 
        }    
        
        function Pause() { 
            clearTimeout(T0); 
            go = false; 
        }
        
        function Continue() {
            go = true;
            T0 = setTimeout(Next,interval);
        }
                 
        T0 = setTimeout(Next,interval+000);   // on page load, take 0sec longer.

        function Next() {
            if($cur.hasClass('last'))
                $next = $first;
            else 
                $next = $cur.next();
            
            $slider.animate({ opacity: 0 }).animate({ left: -$next.index() * w }).animate({ opacity: 1 });
            $slinks.children().removeClass("selected");
            $next.toggleClass('selected');
            $cur = $next;
            
            SetTestimonial($cur.index());
            
            if(go)
               T0 = setTimeout(Next, interval);
        }
        
        function SetTestimonial(i){
            $('#testimonial').fadeOut(550, function() {
                $(this).html($('#testimonials-hidden div').eq(i).html());
                $(this).fadeIn(1800);
            });//fadeOut
        }
    }); 
</script>

<script type="text/javascript">
function FetchLatestBlogPosts() {
    var bposts_svc = "/websvcs/RenderBlogPosts.ashx";
    var a = bposts_svc,
        d = $("ul#home-news");
    $.ajax({
        type: "GET",
        url: a,
        data: {},
        dataType: "html",
        cache: 1,
        context: this,
        async: !0,
        beforeSend: function () {},
        success: function (a) {
            var c = 1,
                b, dt, imgtmp;
            $(a).find('div[id^="post-"]').each(function () {
                b = $(this).find(".entry-title>a");
                var arrDt2 = $(this).find(".entry-meta").text();
                var arrDt = $(this).find(".post-date");
                dt = arrDt.text();
                $description = $(this).find(".entry-content");
                imgtmp = $(this).find(".wp-post-image").attr("src");
                if (imgtmp == null)
                {
					switch (Math.floor((Math.random() * 6) + 1))
					{
						case 1:
							imgtmp = "/images/strategies/dollar_house_small.jpg";
							break;
						case 2:
							imgtmp = "/images/strategies/dollar_key.jpg";
							break;
						case 3:
							imgtmp = "/images/strategies/growing_money_plant.jpg";
							break;
						case 4:
							imgtmp = "/images/strategies/house_on_money.jpg";
							break;
						case 5:
							imgtmp = "/images/strategies/house_umbrella.jpg";
							break;
						case 6:
							imgtmp = "/images/strategies/money_plant.jpg";
							break;
						default:
							imgtmp = "/images/strategies/nest_egg.jpg";
					}
                }
                post = {
                    title: b.text(),
                    description: $description.text(),
                    link: b.attr("href"),
                    imgsrc: imgtmp
                };
                d.append("<li>" + (null == post.imgsrc ? "" : '<img src="' + post.imgsrc + '" align="left" />') + '<div><div id="dvphtitle' + c + '" class="title"><a href="' + post.link + '" class="title">' + post.title + "</a></div><p id='phnews" + c + "' class='pbdesc'>" + post.description.substring(0, 140) + '...</p><div>' + dt +'</div></div></li>');
                
                //alert( $('#dvphtitle' + c).height() + ":" + $('#dvphtitle' + c).text() );

                if (4 <= c) return !1;
                c++
            })
        },
        error: function () {},
        complete: function () {}
    })
};

FetchLatestBlogPosts();

</script>

