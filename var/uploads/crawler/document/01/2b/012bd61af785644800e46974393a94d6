<!-- Google Code for All Visitors Remarketing List -->
<script type="text/javascript">
	var pageTypeChannel, pageProductId, pageTotalValue;
	// Switch statement to give Google their specific page type
	switch(s.channel){
		case "search":
	        pageTypeChannel = "searchresults";
	        break;
	    case "browse":
	        pageTypeChannel = "category";
	        break;
        case "home":
	        pageTypeChannel = "home";
	        break;
        case "pdp":
	        pageTypeChannel = "product";
	        break;
        case "cart":
	        pageTypeChannel = "cart";
	        break;
        case "checkout":
	        pageTypeChannel = "purchase";
	        break;
	    default:
	        pageTypeChannel = "other";
	}

	// Parse product ID's and pass them to Google
	if (s.products != null) {
		pageProductId = s.products.split(",;");
		pageProductId[0] = pageProductId[0].substring(1);
		if (pageProductId.length == 1) {
			pageProductId = pageProductId[0];
		}
	} else {
		pageProductId = "";
	}

	// Price switch between product and cart price
	if (pageTypeChannel == "product") {
		pageTotalValue = parseFloat("");
	} else if (pageTypeChannel == "cart" || pageTypeChannel == "purchase"){
		pageTotalValue = parseFloat("");
	} else {
		pageTotalValue = "";
	}

	var google_tag_params = {
		ecomm_prodid: pageProductId,
		ecomm_pagetype: pageTypeChannel,
		ecomm_totalvalue: pageTotalValue
	};
</script>
<script type="text/javascript">
	/* <![CDATA[ */
	var google_conversion_id = 1070957613;
	var google_custom_params = window.google_tag_params;
	var google_remarketing_only = true;
	/* ]]> */
</script>
