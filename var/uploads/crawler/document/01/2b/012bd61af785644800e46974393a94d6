<script type="text/javascript">
$(document).ready( function() {
	//Hide (Collapse) the toggle containers on load
	$(".toggle_container").hide(); 
	//Switch the "Open" and "Close" state per click then slide up/down (depending on open/close state)
	$("#dimensions .trigger").click( function() {
		$(this).toggleClass("active").prev().slideToggle("slow",function(){
			if($('#compareForm').height() < $('#refinements').height()){
				$('#sr-products').css('height',$('#refinements').height());
			}
			else{
				$('#sr-products').css('height',$('#compareForm').height());	
			}
		 });
		$('.leftColumn .trigger').text(($('.leftColumn .trigger').text() == 'show more') ? 'show less' : 'show more');
		$('.leftColumn .trigger.active').text(($('.leftColumn trigger.active').text() == 'show less') ? 'show more' : 'show less');
	});

    $('.echoOfferSignup').live('click', function(){
        $.fancybox(
                {
                    'autoDimensions'	: false,
                    'width'         		: 530,
                    'height'        		: 'auto',
                    'transitionIn'		: 'fade',
                    'transitionOut'		: 'none',
                    'href' : '#email-signup-form',
                    'showCloseButton' : false
                }
        )

    });
});
</script>


