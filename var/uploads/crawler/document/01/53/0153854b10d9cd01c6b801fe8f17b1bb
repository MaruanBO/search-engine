<script type="text/javascript">
jQuery(function(){
    if(/(american-fashion-menswear-designers-style-trends|america-fashion-inventions|classic-american-fashion-brands|quintessentially-american-fashion-archetypes|made-in-america-grooming-products|john-varvatos-thom-browne-and-michael-bastian-talk-american-style|public-school-woolmark-american-menswear-finalists-2014|fashion-style\/rules-of-style|blogs\/.*rules-of-style)/.test(location.pathname)){
        jQuery('#AMS_DET_GLOBAL_RIGHTRAIL_A,#rolloverAd').addClass('hideComponent');
        jQuery(window).on('hashchange',function(){
            jQuery('#AMS_DET_GLOBAL_SLIDESHOW_RIGHTRAIL').addClass('hideComponent');
        });
        jQuery(function(){
            setTimeout(function(){jQuery('#AMS_DET_GLOBAL_SLIDESHOW_RIGHTRAIL').addClass('hideComponent')},200);
        });
    }else {
        // Commenting the following to enable floating Ads on home page
        /*if(!(/^\/$/.test(location.pathname)) && !jQuery('body').hasClass('listC') && !jQuery('body').hasClass('listE')){
            CN.popups.setLayer(true).setExiting(false).init();
        }*/
       //Enabling the floating Ads on home page
if(!jQuery('body').hasClass('listC') && !jQuery('body').hasClass('listE')){
            CN.popups.setLayer(true).setExiting(false).init();
        }
    }
});



if(/^\/(fashion-style|body-health|grooming|culture-trends)?\/?$/.test(location.pathname)) {
    jQuery(window).on('CN.customEvents.GPTAdsRendered',function(){
        if(jQuery('#yrail_ads_top').length && jQuery('#yrail_ads_top .yrail_ad_wrap').outerHeight() > 1000) {
           jQuery('#yrail_ads_top .yrail_ad_wrap').addClass('noscroll');
        }
    });
    
    jQuery(window).scroll(function(){
        if(jQuery('#yrail_ads_top').length && jQuery('#yrail_ads_top .yrail_ad_wrap').outerHeight() > 1000) {
           jQuery('#yrail_ads_top .yrail_ad_wrap').addClass('noscroll');
        }
    });
}else if (/^\/(fashion-style)\/(rules-of-style|perfect-wardrobe)\/201006\//.test(location.pathname)){
    /*CN.popups.setLayer(false).setExiting(false).setArriving(false).init();
    jQuery(function(){
        jQuery('.home #rolloverAd').addClass('hide');
    });*/
}
else {
 //CN.popups.setLayer(true).setExiting(false).init();
}
    CN.ecom.rolloverAd.setOffset(1, -29).timeout(3);

</script>
        
    


    






    
    
    
    
    
    
    

    
    

    
    

    
    

    
    
        
    

    

    

    
