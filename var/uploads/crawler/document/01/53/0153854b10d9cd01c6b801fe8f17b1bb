<script type="text/javascript">
    /** Encode special characters in a string*/
    var htmlEncode = (function(str) {
        var map = {"&":"amp", "<":"lt", ">":"gt", '"':"quot", "'":"#39"};
        return str.replace(/[&<>"']/g, function(match) { return "&" + map[match] + ";"; });
    });
    var mboxDesc = jQuery("meta[name='description']").attr('content') || '';
    mboxCreate(
    'det_ed_global',
    'entity.id=2000000003053432',
    'entity.name=' + htmlEncode(mboxDesc),
    'entity.categoryId=IndexV2',
    'entity.pageUrl=http://www.details.com/',
    'entity.thumbnailUrl=http://subscribe.condenet.com/images_covers/cover_details_80.jpg?fbrefresh=20150219',
