<!-- MAKES THE MENU STICKY WHEN YOU SCROLL -->
<script>
$(document).ready(function() {
	var s = $("#menu_container");
	var t = $("#menu_bg");
	var u = $("#filler");
	var pos = s.position();
	$(window).scroll(function() {
		var windowpos = $(window).scrollTop();
		if (windowpos >= pos.top) {
			s.addClass("stick");
			t.addClass("stick");
			u.addClass("moveit");
		} else {
			s.removeClass("stick");
			t.removeClass("stick");
			u.removeClass("moveit");
		}
	});
});
</script>

