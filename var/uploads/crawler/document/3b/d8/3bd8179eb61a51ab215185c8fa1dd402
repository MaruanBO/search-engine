<script>
jQuery(function($) {
    $('.comment-more').click(function() {
        $(this).hide();
        $(this).siblings('.comment-excerpt').hide();
        $(this).siblings('.comment-full').show();
        $(this).siblings('.comment-less').show();
        return false;
    });
    $('.comment-less').click(function() {
        $(this).hide();
        $(this).siblings('.comment-full').hide();
        $(this).siblings('.comment-excerpt').show();
        $(this).siblings('.comment-more').show();
        return false;
    });
	$('#ask_a_question').click(function() {
		if(CS.user.id != 0) {
			window.location = '/community/?internallink=qa-widget-home-ask#qa-ask';
		} else {
			PF.qaLogin(this);
		}
	});
});
</script>
