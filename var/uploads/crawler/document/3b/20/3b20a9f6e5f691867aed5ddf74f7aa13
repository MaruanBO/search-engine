<script type="text/javascript">

window.addEvent('domready', function() {
	blucommerce.buildPage();

	Locale.use('en-GB');		if (typeof $$('select').chosen != 'undefined') {
			$$('select').chosen();
		}

	// Handle desktop and iPad, uf[#432]
	$('currency-select').addEvent('click', function(e) {
		this.toggleClass('show');
	});

	$('currency-select').addEvent('outerClick', function(e) {
		this.removeClass('show');
	});

	
			new FixedSidebar({
				margin: 0,
				marginPosition: 'top'
			});
		
	$$('#currency-select .pop-over .country a').addEvent('click', function(event) {
		event.stop();

		bluStats.trackEvent({
			category: "Site Switch",
			action: "ON:" + event.target.get('data-destination').toUpperCase(),
			nonInteraction: true
		});

		setTimeout(function() {
			document.location.href = event.target.get('href')
		}, 100);
	});
	$$('#currency-select .pop-over .currency a').addEvent('click', function(event) {
		event.stop();

		bluStats.trackEvent({
			category: "Currency Switch",
			action: "GBP:" + event.target.get('id').toUpperCase(),
			nonInteraction: true
		});

		setTimeout(function() {
			document.location.href = event.target.get('href')
		}, 100);
	});


			$('panel-frame').setStyle('height', window.getSize().y - 44);

			var homePanel;
			var resize = function() {
				if (homePanel && homePanel.current) {
					homePanel.pause();
					homePanel._jumpTo(homePanel.current);
					homePanel.start();
				}
			};
			resize();
			window.addEvent('resize', resize);

			window.addEvent('load', function() { 

				$('control-images-nav').getFirst('li.panel-1').addClass('active');

				// Create the slideshow
				homePanel = new Panels('panel-frame', 'panels-nav', {
					defaultPanel: 'panel-1',
					controls: 'index-banner',					mode: 'horizontal',
					transition: 'expo:out',
					rotate: 10000,				
					tabElement: 'li',
					direction: 'backwards',
					duration: 0,
					fade: true,
					fadeDuration: 0,
					onPanelChanged: function(link, id) {
						$('control-images-nav').getFirst('li.active').removeClass('active');
						$('control-images-nav').getFirst('li.'+id).addClass('active');
					}
				});
			});

			$$('.index-slideshow-change').each(function(item) { 
				item.removeEvents('click').addEvent('click', function(event) {
					event.preventDefault();
					homePanel._transitionTo(item.get('data-panel'));
				});
			});

		

		var setPanelHeight = function() {
			var size = $$('#panel-frame-index_love .panels-inner .panel')[0].getScrollSize(),
				height = size.y;

			$('panel-frame-index_love').setStyle('height', height);
		}
		window.addEvent('resize', setPanelHeight);

		window.addEvent('load', function() {

			var indexBrandPanels = $('panel-frame-index_love').getElements(".panels-inner div").length;
			var indexBrandPrev = $('panel-scroller-index_love').getElement('.prev');
			var indexBrandNext = $('panel-scroller-index_love').getElement('.next');
			var loveCurrent = $('panel-scroller-index_love').getElement('.love_current');
			var indexPanels = new Panels('panel-frame-index_love', 'panels-nav-brands', {
				defaultPanel: 'panel-1',
				controls: 'panel-scroller-index_love',
				mode: 'horizontal',
				transition: 'expo:out',
				scrollToOnStart: false,
				tabElement: 'li',
				onPanelChanged: function(link, id) {
					currentPanel = id.split('-')[1];
					var index = id.slice(-1).toInt();
					indexBrandPrev.setStyle('display', 'block');
					indexBrandNext.setStyle('display', 'block');



					if (index == 1) {
						indexBrandPrev.setStyle('display','none');
					}

					if (index == indexBrandPanels) {
						indexBrandNext.setStyle('display','none');

					}

					loveCurrent.set('text', index);
				}
			});

			setPanelHeight();

		});


	

			window.bindSidebarClick = function() {

				$$('.side-tab').each(function(el) {

					// Tab title handler
					el.getElement('.tab-title').removeEvents('click');
					el.getElement('.tab-title').addEvent('click', function() {

						// Remove previous tabs
						$$('.side-tab').each(function(element) {
							element.removeClass('active');
						});
						el.addClass('active');

					});

					// Close buttons
					var closeEl = el.getElement('.side-close');
					closeEl.removeEvents('click');
					closeEl.addEvent('click', function() {
						el.removeClass('active');
					});

				});

				if ($('side-tabs')) {
					$('side-tabs').addEvent('outerClick', function(e) {
						var parent = e.target.getParents('div#side-tabs');
						if (!parent.length) {
							$$('.side-tab').each(function(el) {
								el.removeClass('active');
							});
						}
					});
				};

			}

						window.bindSidebarClick();
			
			new NavDelay();

			// Add cart tracking
			if (blucommerce.cart) {
				blucommerce.cart.addEvent('addedItem', function(form, response) {
					if (!response || !response.length) {
						return;
					}

					response.each(function (item) {
						bluStats.trackEvent({
							category: "Add to Bag",
							action: item.name + ":" + item.sku.substring(0, 5),
							label: item.sku,
							value: item.price,
							nonInteraction: true
						});
					});
				});
			}

			
			function isTouch() {

				if ( navigator.userAgent.match(/(iPhone|iPod|iPad|Blackberry|Android)/) ) {
					return true;
				}

				try {
					document.createEvent("TouchEvent");
					return true;
				} catch (e) {
					return false;
				}
			}

			var removeScroll = function(e){
				e.preventDefault();
			}

		
			var newsletter = window.location.href.match(/#newsletter$/);
			if (newsletter) {

			// Show tell a friend popup for relevent urls
					(function() {
						$('footer-newsletter').fireEvent('submit');
					}).delay(1000);
			}

			var Beggining = true;

				// Handle back to top
				var scroller = new Fx.Scroll(window);
				$$('.back-to-top').each(function(el) {
					el.addEvent('click', function(event) {
						event.stop();
						scroller.toTop();
					});
				});

				// Only show back to top when the scroll bar moved
				window.addEvent('scroll', function(event) {
					var BackToTop = false;
					var scrollDown = true;

					if ($(window).scrollY > 100) {
						BackToTop = true;
						scrollDown = false;

					}

					// Change all back to top link's visibility
					$$('.back-to-top').each(function(el) {
						if (BackToTop) {
							el.fade('in');
						} else {
							el.fade('out');
						}
					});

					// Change all back to top link's visibility
					$$('.scroll-down').each(function(el) {
						if (scrollDown) {
							el.fade('in');
						} else {
							el.fade('out');
						}
					});
				});

				var checkPosition = function() {
					var windowSizeX = window.getSize().y;
					$$('.scroll-down').each(function(el) {
						el.removeClass('fixed-position');
						if ((windowSizeX - el.getPosition().y) < 100) {
							el.addClass('fixed-position');
						}
					});
				}

				checkPosition();

				window.addEvent('resize', function() {
					checkPosition();
				});

				$$('.scroll-down').each(function(el) {
					el.addEvent('click', function(e) {
						var myFx = new Fx.Scroll(window, {
			    			offset: {
			       				x: 0,
			        			y: 400
			    			}
						}).toTop();
					});
				});

			
		

	blucommerce.destruct();
});

window.addEvent('load', function() {
	});

</script>


    
