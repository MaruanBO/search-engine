	campuses_arr = [
		{'manoa':'manoa.gif', 'hilo':'hilo.gif', 'westoahu':'westoahu.gif', 'hawaii':'hawaii.gif', 'honolulu':'honolulu.gif', 'kapiolani':'kapiolani.gif', 'kauai':'kauai.gif', 'leeward':'leeward.gif', 'maui':'maui.gif', 'windward':'windward.gif', 'random':'honolulu.gif'}
		,{'manoa':'University of Hawai&#145;i at Manoa (O&#145;ahu)', 'hilo':'University of Hawai&#145;i at Hilo (Hawai&#145;i)', 'westoahu':'University of Hawai&#145;i at West O&#145;ahu (O&#145;ahu)', 'hawaii':'Hawai&#145;i Community College (Hawai&#145;i)', 'honolulu':'Honolulu Community College (O&#145;ahu)', 'kapiolani':'Kapi&#145;olani Community College (O&#145;ahu)', 'kauai':'Kaua&#145;i Community College (Kaua&#145;i)', 'leeward':'Leeward Community College (O&#145;ahu)', 'maui':'Maui Community College (Maui)', 'windward':'Windward Community College (O&#145;ahu)', 'random':'Honolulu Community College (O&#145;ahu)'}
		,{'manoa':'manoa.hawaii.edu', 'hilo':'hilo.hawaii.edu', 'westoahu':'westoahu.hawaii.edu', 'hawaii':'hawaii.hawaii.edu', 'honolulu':'honolulu.hawaii.edu', 'kapiolani':'kapiolani.hawaii.edu', 'kauai':'kauai.hawaii.edu', 'leeward':'www.leeward.hawaii.edu', 'maui':'maui.hawaii.edu', 'windward':'windward.hawaii.edu', 'random':'honolulu.hawaii.edu'}
		];

	function campuses_swap (campus) {
	    img = document.getElementById('campuses_img');
	    img.src='site/images/' + campuses_arr[0][campus];
	    alt_title = campuses_arr[1][campus];
	    var okina = "&#145;";
	    // Pageserver strips out the codes if diacritical is set to none.
	    if (okina.length > 0) {
		var regexp = new RegExp(okina, "gi");
		alt_title = alt_title.replace(regexp, "\u02bb");
	    }
	    var kahako = "a";
	    if (kahako.length > 1) {
		var regexp = new RegExp(kahako, "gi");
		alt_title = alt_title.replace(kahako, "a\u0304");
	    }
	    
	    img.alt=alt_title;
	    img.title=alt_title;

	    // Remove link from campus image per Jeela on 2007-09-07.
	    //img_a = document.getElementById('campuses_img_a');
	    //img_a.href='http://' + campuses_arr[2][campus];
	}



	function banner_motto_swap (lang) {
	    img = document.getElementById('banner1_img');
	    if (lang == 'eng') {
		img.src='site/images/banner_motto_english.gif';
		alt_title = "Above all nations is humanity.";
	    } else {
		img.src='site/images/banner_motto_hawaiian.gif';
		alt_title = "Ma luna a&#145;e o na lahui a pau ke ola o ke kanaka";
	    }
	    var okina = "&#145;";
	    // Pageserver strips out the codes if diacritical is set to none.
	    if (okina.length > 0) {
		var regexp = new RegExp(okina, "gi");
		alt_title = alt_title.replace(regexp, "\u02bb");
	    }
	    var kahako = "a";
	    if (kahako.length > 1) {
		var regexp = new RegExp(kahako, "gi");
		alt_title = alt_title.replace(kahako, "a\u0304");
	    }
	    
	    img.alt=alt_title;
	    img.title=alt_title;
	}
    /* ]]> */
