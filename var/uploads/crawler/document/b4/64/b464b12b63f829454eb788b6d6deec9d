<script type="text/javascript">
    $(document).ready(function() {
        $('#sliderHP').coinslider({ width: 762, height: 300, navigation: false, numericalnav: true, delay: 4800, hoverPause: true, links: true });
    });

	$(window).load(function() {
		var GetUrlParameter = function (name,href) {
			name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
			var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
			results = regex.exec(href);
			return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
		}	

		$('a.cs-sliderHP').click(function(){
			var href = $(this).attr("href");
			href = href.toLowerCase();
			var campaign = GetUrlParameter('campaign',href);			
			if (campaign.length > 0) {
				_gaq.push(['_trackEvent', 'HomeSlider', 'Click', campaign, 2, 'false']);
			}
		});
	});
</script>

