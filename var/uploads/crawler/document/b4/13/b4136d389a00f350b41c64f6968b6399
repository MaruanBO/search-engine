$j(document).ready(function(){
     
      $j('li#tab-id-149 a span').html('Media');
      //$j('li#tab-id-391 a').attr('href', 'http://www.brighton.com/collections/1466/1/ytc-sk-radiant.html');

       $j('#meu a.shop_tab').click(function(e) {
		if ($j(this).parent().find('ul').length && !$j(this).parent('li.touched').length && $j('html').hasClass('toucher')) {
			e.preventDefault();
			$j(this).parent('li.shop_tab').addClass('sfhover touched');
		}
	});
	//
	$j(document).on('touchstart', function(e){
		$j('.sfhover').each(function() {
			if ($j(this).has(e.target).length === 0) {
				$j(this).removeClass('sfhover touched');
			}
		});
	});
	//
});

if (jQuery.cookie('GeoSpot') || userData.zip) {
	var anchor = "/stores?zip=10110&zipcode_distance=50&state=&canadaonly=0";
	if (jQuery.cookie('GeoSpot')) {
		anchor = anchor.replace("10110", jQuery.cookie('GeoSpot'));
	} else {
		anchor = anchor.replace("10110", userData.zip);
	}
	if (userData.zipcode_distance != 50) {
		anchor = anchor.replace("distance=50", "distance="+userData.zipcode_distance);
	}
	if (userData.state) {
		anchor = anchor.replace("state=", "state="+userData.state);
	}
	if (userData.canadaonly) {
		anchor = anchor.replace("canadaonly=0", "canadaonly="+userData.canadaonly);
	}
	$j('.b-c-store a').attr('href',anchor);
	$j('.eng-sl a').attr('href',anchor);
}
