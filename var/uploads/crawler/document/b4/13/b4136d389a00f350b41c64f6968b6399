<script>
function updateBlanket(){
	$j('.certona .blanket').css('height','auto');
	var biggest = 0;
	$j('.certona .blanket').each(function() {
		if ($j(this).height() > biggest) {
			biggest = $j(this).height();
		}
	});
	$j('.certona .blanket').css('height',biggest);
}
//
function getRecProdId(rp) {
	var url = rp;
	var m;
	if (m = url.match(/\/product\/[^\/]*\/([^\/]+)/)){
		return m[1];
	} else if (m = url.match(/\/show_product\/([^\/]+)/)) {
		return m[1];
	} else {
		return "";
	}
}
//
var findCertona = setInterval(function(){
	if ($j('.certona td').length) {
		$j('.certona .rec-items').append('<div id="rec-carousel" class="owl-carousel br-theme"></div>');
		$j('.certona td').each(function() {
			//
			var prp = $j(this).find('.certona_img a').attr('href');
			var urp = getRecProdId(prp);
			var nrp = $j(this).find('.certona_name a').text();
			$j(this).find('.certona_img').append('<div class="certona_qv"><a href="#" title=""><img src="//www.brighton.com/store_image/site/category_quick_view.png"></a></div>');
			$j(this).find('.certona_qv a').attr('href','javascript:QuickView("'+urp+'");').attr('title','View '+nrp);
			//
			if (!$j(this).find('.rating_select a img').length) {
				$j(this).find('.rating_select a').append('<img src="//www.brighton.com/store_image/site/stars-love-it.png" border="0">');
			}
			//
			$j('#rec-carousel').append('<div class="feedme item"></div>');
			var reccell = $j(this).html();
			$j('#rec-carousel .feedme').html(reccell);
			$j('.feedme').removeClass('feedme');
		});
		//
		var owl = $j('#rec-carousel');
		//
		if (owl.parents('#search_rr').length || owl.parents('#subcat_rr').length || owl.parents('#category_rr').length) {
			owl.owlCarousel({
				responsive:{
					0:{items:2},
					600:{items:3},
					768:{items:4}
				},
				slideBy: 'page',
				nav:true,
				navText:["<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-left-inv.png' width='256' height='550' border='0'>","<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-right-inv.png' width='256' height='550' border='0'>"],
				afterUpdate:function(){updateBlanket()},
				afterInit:function(){updateBlanket()}
			});
		} else if (owl.parents('#product_rr').length) {
			owl.owlCarousel({
				responsive:{
					0:{items:2},
					959:{items:3}
				},
				slideBy: 'page',
				nav:true,
				navText:["<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-left-inv.png' width='256' height='550' border='0'>","<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-right-inv.png' width='256' height='550' border='0'>"],
				afterUpdate:function(){updateBlanket()},
				afterInit:function(){updateBlanket()}
			});
		} else {
			owl.owlCarousel({
				responsive:{
					0:{items:2},
					600:{items:3},
					768:{items:4},
					1000:{items:5}
				},
				slideBy: 'page',
				nav:true,
				navText:["<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-left-inv.png' width='256' height='550' border='0'>","<img class='tc_rsp' src='/store_image/site/owl/carou-arrow-right-inv.png' width='256' height='550' border='0'>"],
				afterUpdate:function(){updateBlanket()},
				afterInit:function(){updateBlanket()}
			});
		}
		//
		if (!$j('quick_view_area').length) {
			$j('.certona').append('<div id="quick_view_area" style="display:none;position:absolute;margin:auto;"><div id="quick_view_stage"></div></div>');
			$j = jQuery.noConflict();
			$ = jQuery;	
		}
		//
		clearInterval(findCertona);
	}
},500);
//
setInterval(function(){
	updateBlanket();
},500);
