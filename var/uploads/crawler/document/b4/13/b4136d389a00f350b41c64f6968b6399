if ($j('#header_li27').length) {
	$j('#header_li27').html('<span class=\"mock-a nm_tab\">Help<\/span><ul><li><div id=\"help_popup\"><table cellspacing=\"0\" cellpadding=\"0\"><tbody><tr><td><\/td><td>We are here to help<\/td><\/tr><tr><td COLSPAN=\"2\" class=\"htline\"><div class=\"liner\"><\/div><\/td><tr><td class=\"helptitle htcall\">Call:<\/td><td class=\"helpinfo hicall\">1-800-628-7687<BR>Monday - Friday 7am - 5pm PST<\/td><\/tr><tr><td class=\"helptitle htemail\">Email:<\/td><td class=\"helpinfo hiemail\"><a href=\"\/contact\">Anytime<\/a><\/td><\/tr><tr><td COLSPAN=\"2\" class=\"htline\"><div class=\"liner\"><\/div><\/td><tr><tr><td><\/td><td><a href=\"\/content\/34\/faqs\">Check out our FAQ page<\/a><\/td><\/tr><\/tbody><\/table><\/div><\/li><\/ul>');
}

if ($j('#customer_message').length) {
	$j('.menu_header1').append($j('#customer_message'));
}

var findCustomer = setInterval(function(){
	if ($j('#customer_message').text().length > 5) {
		var cstmsg = $j('#customer_message').html();
		var cstmsgH = cstmsg.replace('<a href="/Shop/Customer/Logout">Logout</a>','');
		var cstmsgP = cstmsg.replace('Hello ','').replace('.','?');
		//
		$j('#header_li23 .popover').append('<div class="pop-customer">Not '+cstmsgP+'</div>');
		//
		$j('#mobileMenu .b-c-ma-a').prepend('<span class="hello">'+cstmsgH+'</span>');
		clearInterval(findCustomer);
	}
},500);
