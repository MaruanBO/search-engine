<script>
function mockSearch() {
	var ms_attempt = $j('#searchsite_keyword').val(); 
	if ( (ms_attempt != undefined) && (ms_attempt != "") && (ms_attempt != " ") && (ms_attempt != "  ") && (ms_attempt != "Search") && (ms_attempt != "SEARCH") ) {
		$j('input.search_btn').click();
	}
}
</script>

<script>
function refreshCategory() {
	$j('.pages .number').each(function() {
		var str = $j(this).text();
		if(($j.trim( str )).length==0) {
			$j(this).remove();
		}
	});
	//
	//
	$j('#product_content .back .text').html('<img class="tc_rsp" src="/store_image/site/pagination-prev.gif" alt="">');
	$j('#product_content .next .text').html('<img class="tc_rsp" src="/store_image/site/pagination-next.gif" alt="">');
	$j('.back').addClass('ready');
	$j('.next').addClass('ready');
	//
	if ($j('.pages').length && !$j('.pages .gost').length && !$j('.pages .desired').length) {
		var pagii = $j('.pages span').length;
		for (var i = 0; i < pagii; i++) {
			var imod = i+1;
			$j('.pages span:eq( '+i+' )').addClass('gost numby numby'+imod);
		}
		//
		var selA = 2;
		var selZ = pagii-1;
		var selT = $j('.number_selected').attr('class');
		var selN = selT.split(" ").pop().replace('numby','');
		var selNl = parseInt(selN)-1;
		var selNl2 = parseInt(selN)-2;
		var selNl3 = parseInt(selN)-3;
		var selNh = parseInt(selN)+1;
		var selNh2 = parseInt(selN)+2;
		var selNh3 = parseInt(selN)+3;
		$j('.pages span').each(function() {
			if ($j(this).is('.pages span:first') || $j(this).is('.pages span:last') || $j(this).is('.number_selected') || $j(this).is('.numby'+selNl) || $j(this).is('.numby'+selNh)) {
				$j(this).addClass('desired').removeClass('gost');
			}
		});
		if ($j('.number_selected').is('.pages span:first')) {
			$j('.numby'+selNh2+', .numby'+selNh3).addClass('desired').removeClass('gost');
		}
		if ($j('.number_selected').is('.numby'+selA)) {
			$j('.numby'+selNh2).addClass('desired').removeClass('gost');
		}
		if ($j('.number_selected').is('.pages span:last')) {
			$j('.numby'+selNl2+', .numby'+selNl3).addClass('desired').removeClass('gost');
		}
		if ($j('.number_selected').is('.numby'+selZ)) {
			$j('.numby'+selNl2).addClass('desired').removeClass('gost');
		}
		$j('.pages .numby').each(function() {
			if ($j(this).is('.desired') && !$j(this).is('.pages span:last')) {
				if ($j(this).next().is('.desired')) {
					$j(this).after('<span class="divider desired">|</span>');
					//console.log($j(this).next().attr('class'));
				} else if ($j(this).next().is('.gost')) {
					$j(this).after('<span class="divider desired">...</span>');
				}
			}
		});
		//
		$j('#pagination_top .pages').clone().appendTo('#pagination_bottom');
	}
	//
	if (!$j('.amt-of-items').length && $j('#item_results').length) {
		var lastS = $j('#item_results').text();
		var lastW = lastS.split(" ").pop();
		$j('#pagination_top .pages').after('<span class="amt-of-items">'+lastW+' Items</span>');
		//
		$j('#pagination_top .amt-of-items').clone().appendTo('#pagination_bottom');
	}
	//
	//
	if	($j('#products .product').length) {
		//
		$j('#products .product').each(function() {
			if ($j(this).find('.category_mini_video_icon').length) {
				$j(this).find('.category_mini_video_icon').appendTo($j(this).find('.tc_spacer'));
			}
			if ($j(this).find('.product-tag-name-new').length) {
				$j(this).find('.product_thumb_container a').append('<div class="tag-diag"><img src="//www.brighton.com/store_image/mfil/new2.png" alt="New Item!"></div>');
			}
			if ($j(this).find('.product-tag-name-power-of-pink').length && !$j(this).find('.tag-diag2').length) {
				$j(this).find('.product_thumb_container a:first').append('<div class="tag-diag2"><img src="//www.brighton.com/store_image/p-o-p/power-of-pink.png" alt="Brighton Power of Pink!"></div>');
			}
			if ($j(this).find('.colors').length && !$j(this).find('.colors-mobi').length && !$j(this).find('.colors-mock').length) {
				$j(this).find('.pricing').after('<div class="colors-mobi"><a href="'+$j(this).find('.product_thumb_a').attr('href')+'">MORE COLORS</a></div>');
			} else if (!$j(this).find('.colors').length) {
				$j(this).find('.product_thumb_container').after('<div class="colors colors-mock"></div>');
			}
			if ($j(this).find('.item_zoom').length) {
				if ($j(this).find('.item_zoom a').attr('title').indexOf("YTC Sk") >= 0) {
					$j(this).find('.item_zoom').addClass('tc_ghost');
					$j(this).find('.addcategorytocartdiv').addClass('tc_ghost');
				} else {
					$j(this).find('.product_thumb_container').append($j(this).find('.item_zoom'));
					if (($j(".category314").length > 0) || ($j(".page-my-flat-in-london").length > 0) || ($j(".brand787").length > 0)) {
						$j('.product.item_zoom img').attr('src', '//www.brighton.com/store_image/mfil/category_quick_view.gif');
					}
				}
			}
			if (!$j(this).find('.rating_set').length) {
				$j(this).find('.tc_spacer').append('<div class="rating_set r-love"><a href="'+$j(this).find('.product_thumb_container a:first').attr('href')+'"><img src="//www.brighton.com/store_image/site/stars-love-it.png" alt="Rate this Item"></a></div>');
			}
		});
	}
}
</script>

<script>
$j(document).ready(function(){
	$j('.banmo').prependTo('.header_spacer');
	$j('.engagement').appendTo('.header_spacer');
	$j('.engagement').addClass('ready');
	//
	$j('#searchsite_keyword').click(function() {
		if ($j(this).val() == $j('#SearchField').val()) {
			$j(this).val('');
		}
	});
	//
	$j("#searchsite").appendTo('.header_spacer');
	//
	$j('.content_pwrap').addClass('clearfix').attr('id','products');
	//
	refreshCategory();
	
	if (($j(".category314").length > 0) || ($j(".page-my-flat-in-london").length > 0) || ($j(".brand787").length > 0)) {
		$j('#logoMFIL').appendTo('.header_spacer');
$j('#goHOME').appendTo('.tab_area');
		
		/* prod */
		if	($j('#sn-email').length > 0) {
			$j('#sn-email img').attr('src', '//www.brighton.com/store_image/mfil/email.gif');
			$j('#sn-email').addClass('mfil');
		}
		if	($j('#product_question span').length > 0) {
			$j('#product_question span').html('Questions?');
		}
		if	($j('#add_to_cart').length > 0) {
			$j('#add_to_cart').attr('src', '//www.brighton.com/store_image/mfil/addtocart.gif');
			$j('#add_to_cart').addClass('mfil');
		}
		if	($j('#add_to_wishlist').length > 0) {
			$j('#add_to_wishlist').attr('src', '//www.brighton.com/store_image/mfil/add_to_wishlist.gif');
			$j('#add_to_wishlist').addClass('mfil');
		}
	}
});
</script>


<script>
var productSelectionTimer = false;
var showHelpTimer = false;
var showHelpHideTimer = false;
var showCartTimer = false;
var showCartHideTimer = false;
var checkcartloadedtimer = false;
var stock_product_id;


function addLoadEvent(func)
{    
    var oldonload = window.onload;
    if (typeof window.onload != 'function')
    {
        window.onload = func;
    } 
    else 
    {
        window.onload = function()
        {
            oldonload();
            func();
        }
    }
}

addLoadEvent(onloadVersionTwo);

function onloadVersionTwo() {
    if($j("#product_page").attr('id')) {
	   /* if(jQuery.cookie('product_stores') != "viewed") {
	        $j("#product_stores_message").show();
	        jQuery.cookie('product_stores',"viewed", { path: '/', expires: 3650 });
	    }*/
		if($j("#vwitem").attr('id')) {
			if($j("#vwitem").val()) {
				checkProductStockSelection();
			} else {
				productSelectionTimer = setTimeout("checkProductStockSelection()",200);
			}
		}
	    if($j("#product_store_stock_window").attr('id')) {
	    	$j("#product_store_stock_tabs").children('h3').each(function(index){
    			$j(this).click(function() {
    				$j("#product_store_stock_tabs").children('h3').each(function(index){
    						$j(this).removeClass("selected");
		    				var div = $j(this).attr("id");
		    				div = div.substring(1);
		    				$j("#"+div).hide();
    					});
    				var click_div = $j(this).attr("id");
    				click_div = click_div.substring(1);
    				if(click_div == "product_store_stock_area") {
    					$j("#store_locator").show();
    				} else {
    					$j("#store_locator").hide();
    				}
    				$j(this).addClass("selected");
    				$j("#"+click_div).show();
    			});
	    	});
	    }
	    
    	tobj=$j(".product_description").offset();
    	//$j("#product_store_stock_window").offset({top:tobj.top+$j(".product_description").height()+40});
    }
	if($j("#shopping_cart_area").attr('id')) {
		checkcartloadedtimer = setTimeout("checkCartLoaded()",200);
	}
}

cartItemsOnload = function() {
	$j("#cartrequest_addeditems").hide();
	$j("#cartrequest_subnumber").hover(function() {
		showCartTimer = setTimeout("showQuickCart()",300);
	});
	$j("#cartrequest_subnumber").mouseleave(function() {
		if(showCartTimer) {
			clearTimeout(showCartTimer);
			showCartTimer = false;
		}
	});
	$j("#cartrequest_addeditems,#cartrequest_subnumber").mouseenter(function() {
		clearTimeout(showCartHideTimer);
	});
	$j("#cartrequest_addeditems,#cartrequest_subnumber").mouseleave(function() {
		showCartHideTimer = setTimeout("hideQuickCart()",300);
	});	
}

cartItemsJsonpResponse = function(response) {
	$j("#cart_request_status").html(response['items']);
	cartItemsOnload();
}

checkCartLoaded = function() {
	if($j("#cart_request_area").attr('id')) {
		if (QuickCart_jsonp_enabled) {
			QuickCart_JsonpRequest("http://www.brighton.com/Shop/Cart/Request/Items?callback=cartItemsJsonpResponse");
		} else {
			$j("#cart_request_status").load("http://www.brighton.com/Shop/Cart/Request/Items",cartItemsOnload);
		}
		if(checkcartloadedtimer) {
			clearTimeout(checkcartloadedtimer);
			checkcartloadedtimer = false;
		}
	} else {
		checkcartloadedtimer = setTimeout("checkCartLoaded()",200);
	}
}

showQuickCart = function()
{
	if(showCartTimer) {
		$j("#cartrequest_addeditems").fadeIn('slow'); 
		$j("#cartrequest_subnumber").addClass("selected");
                $j("#cartrequest_action_close").hide();
	}
	showCartTimer = false;
}

hideQuickCart = function()
{
	$j("#cartrequest_addeditems").fadeOut('slow');
  //$j("#cartrequest_action_close").show();
	$j("#cartrequest_subnumber").removeClass("selected");
}

checkProductStockSelection = function() {
	if($j("#product_page").attr('id')) {
		var product_id = $j("#vwitem").val();
		if (!product_id) {
			product_id = 0;
		}
		if(stock_product_id !== product_id) {
			stock_product_id = product_id;
			$j("#product_store_stock_window").show();
			$j("#store_locator").hide();
			$j("#store_locator_content").show();
			var extraParams = {};
			if (typeof userData != "undefined") {
				if(userData.zip) {
					extraParams.zip = userData.zip;
				}
				if (userData.zipcode_distance) {
					extraParams.zipcode_distance = userData.zipcode_distance;
				}
			}
			if (typeof productFinderParams != "undefined") {
				for (var i in productFinderParams) {
					if (productFinderParams.hasOwnProperty(i)) {
						extraParams[i] = productFinderParams[i];
					}
				}
			}
			var loadUrl = 'http://www.brighton.com/product-finder/'+product_id+"?nomap=1&nofilters=1&nolegend=1&embedded=1&limit=3&"+jQuery.param(extraParams);
			$j("#store_locator_content").load(loadUrl,function(){
				//$j('div').remove('.loading');
				$j("#store_locator").show();
				});
		} else {
			productSelectionTimer = setTimeout("checkProductStockSelection()",200);
		}
	}
}

CheckProductFinderSubmitExtra = function() {
	if ($j("#vwitem").length && !$j("#vwitem").val()) {
		alert("Please select a style.");
		return false;
	}
	return true;
};
</script>


