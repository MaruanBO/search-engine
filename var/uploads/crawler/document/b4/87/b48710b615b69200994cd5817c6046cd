        var containerWidth = jQuery( ".top-adblock" ).width();
        var adID           = containerWidth > 727 ? "div-gpt-ad-1401302428641-5" : "div-gpt-ad-1401302428641-2";
        var adWidth        = containerWidth > 727 ? "728" : "320";
        var adHeight       = containerWidth > 727 ? "90" : "50";

        jQuery( ".header-right-banner-wrapper" ).attr( 'id', adID ).css({ "width": adWidth, "height": adHeight });

        googletag.cmd.push(function(){googletag.display( adID );});
    </script>
</div>
</div>

