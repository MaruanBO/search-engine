<script>
    var sourceSwap = function () {
        var $this = $(this);
        var newSource = $this.data('other-src');
        $this.data('other-src', $this.attr('src'));
        $this.attr('src', newSource);
    }

    $(function () {
        $('img.catnav[data-other-src]').each(function () {
            new Image().src = $(this).data('other-src');
        }).hover(sourceSwap, sourceSwap);
    });
</script>

