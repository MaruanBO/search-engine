<script type="text/javascript">
function refreshPinterestButton(url, media, description,divId) {
        var js, href, html, onclick;
        url = escape(url);
        media = escape(media);
        //description = escape(description);
        href = 'http://pinterest.com/pin/create/button/?url=' + url + '&media=' + media + "&description=" + description;
        onclick = "'" + href + "', 'popup','width=600,height=600'";
        html = '<a href="#" onclick="javascript:var s = window.open(' + onclick + ' ); return false;" class="pin-it-button" count-layout="none" id="pinterestBtn"><img border="0" src="http://assets.pinterest.com/images/PinExt.png" title="Pin It" /></a>';
        
        
        document.getElementById(divId).innerHTML = html;
    }
</script>


