                    boundingBox: billboardTileSrc,
                    contentBox: billboardTileSrc.one('.content'),
                    id : 'cds-static-content-2',
                    endlessScroll: true
                });

                scrollerInterval = window.setInterval(function() { billboardTileCarousel.nextPage(); }, 7000);
                Y.on('scroller-user-change:cds-static-content-2', function() {
                    window.clearInterval(scrollerInterval);
                });
            } else {
                billboardTileSrc.one('.breadcrumb-nav').addClass('hide');
            }        
        });
