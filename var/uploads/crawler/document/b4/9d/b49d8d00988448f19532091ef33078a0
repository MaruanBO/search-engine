    BN_YUI.use('node', 'scroller-carousel-large', function(Y) {
        Y.on('domready', function () {
            var billboardTileSrc = Y.one('#cds-static-content-2 .scroller'),
                billboardTileCarousel = null,
                scrollerInterval = null,
                items = billboardTileSrc.all('.content ol li'),
                size = items ? items.size() : 0;

