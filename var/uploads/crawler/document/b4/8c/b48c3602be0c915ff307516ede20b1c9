			<script>
			  
			function checkEmail() {
			  
				var email=$('#txtEmailID').val();
				var filter = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/
			  
				if (!filter.test(email)) {
				alert('Please enter a valid email address');
				return false;
			 }
			  else{
			  alert('Nice!..you have entered valid email address');
			  }
			}
			</script>
			<script>
			 
				$(document).ready(function(){
				
				$('#message').hide();
				$('#submit').click(function(){
			
				$.post("/tools/newsletter", $("#mycontactform").serialize(),  function(response) {   
				var resp=$.parseJSON(response);				
				if(resp.status=="success"){
				 $('#message').html(resp.message);
				 $('#mycontactform').hide('slow');
				 $('#message').show();
				 setTimeout(function() { $('#popup_box').fadeOut("slow");
						$("#container").css({ // this is just for style		
							"opacity": "1"  
						});  }, 5000)  
						setCookie("ibtnewsletter","ibtnewsletter",30);
				
				}else{
				$('#success').html(resp.message);
				
				//setTimeout("$('#popup_box').dialog()",5000);
				}
				});
				return false;


				});

				});
				
				</script>
