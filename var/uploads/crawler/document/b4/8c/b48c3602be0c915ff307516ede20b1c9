					var size = new Array();
					size = getSizes();
					var counter = size.length - 1;
					var acceptVal="0";
					var accept= "";
					var adv= "";
					var imp= "";
					for( var i = 0; i<counter; i++ ) {
						accept = size[i];
						adv = "adv"+size[i];
						imp = "imp"+size[i];
						acceptVal=isVizInterested(size[i]);
						if(acceptVal){ acceptVal = "true"; }
						pubads.setTargeting(accept,acceptVal);
						pubads.setTargeting(adv,getAdv(size[i]));
						pubads.setTargeting(imp,getImpr(size[i]));
					}
	});
