<script type="text/javascript">
	(function($) {
		var $menu = $('nav.main-menu');
		var $navItem = $('nav.main-menu div > ul > li');
		var timer;

		$navItem.has('ul').on('mouseover', function() {
			$(this).addClass('on').siblings().removeClass('on');
			clearTimeout(timer);
		});

		$navItem.on('mouseleave', function () {
			timer = setTimeout(function() {
				$navItem.removeClass('on');
			}, 200);
		});

		$menu.children('ul').not(':first-child').on('mouseenter', function(){
			$navItem.removeClass('on');
		});
	})(jQuery);
</script>
