<script>
function setCurrentNavMenu() {
		
		loc = location.href;
		loc = loc.replace('http://','').replace('https://','').split(/[/?#&]/, 4)[3];
			if (typeof(loc) != 'undefined') {
				
				if (loc.match(('/avery/en_us/Products').split('/',4)[3])) {
					menuActive("Products");
				} else if (loc.match(('/avery/en_us/Templates-%26-Software').split('/',4)[3])) {
					menuActive("Templates");
				} else if (loc.match(('/avery/en_us/Templates-&-Software').split('/',4)[3])) {
					menuActive("Templates");
				} else if (loc.match(('/weprint/'))) {
					menuActive("WePrint");
				} else if (loc.match(('/avery/en_us/Projects-&-Ideas').split('/',4)[3])) {
					menuActive("Ideas");
				} else if (loc.match(('/avery/en_us/Projects-%26-Ideas').split('/',4)[3])) {
					menuActive("Ideas");	
				} else if (loc.match(('/avery/en_us/Help-Center').split('/',4)[3])) {
					menuActive("Help");
				} else if (loc.match(('/avery/en_us/Help').split('/',4)[3])) {
					menuActive("Help");
				} 
			} 
	}
	
$(function() {
	setCurrentNavMenu();
});	
</script>


	
