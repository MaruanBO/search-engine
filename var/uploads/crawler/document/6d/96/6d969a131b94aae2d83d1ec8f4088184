jQuery(document).ready(function($){

var $setoffavs = $('ul.favorites-set');
var $favsetmiddle = $('ul.favorites-set:eq(1)');

 $('#favs-first').click(function(){
            $('ul.favorites-set').first().css('display', 'block').addClass('bounceIn').removeClass('bounceOut');
            $('ul.favorites-set:eq(1)').css('display', 'none').addClass('bounceOut').removeClass('bounceIn');
            $('ul.favorites-set').last().css('display', 'none').removeClass('bounceIn').addClass('bounceOut');
            });
});

jQuery(document).ready(function($){
        $('#favs-second').click(function(){
            $('ul.favorites-set').last().css('display', 'none').addClass('bounceOut').removeClass('bounceIn');
            $('ul.favorites-set:eq(1)').css('display', 'block').addClass('bounceIn').removeClass('bounceOut');
            $('ul.favorites-set').first().css('display', 'none').addClass('bounceOut').removeClass('bounceIn');
        });
});

jQuery(document).ready(function($){

    $('#favs-third').click(function(){
        $('ul.favorites-set').last().css('display', 'block').addClass('bounceIn').removeClass('bounceOut');
        $('ul.favorites-set:eq(1)').css('display', 'none').addClass('bounceOut').removeClass('bounceIn');
        $('ul.favorites-set').first().css('display', 'none').addClass('bounceOut').removeClass('bounceIn');
    });


    $(".nav_icon").on("click", "#favs-first", function(){
      $("#favs-second, #favs-third").removeClass("activenav").addClass("inactivenav");
      $("#favs-first").removeClass("inactivenav").addClass("activenav");
    });

    $(".nav_icon").on("click", "#favs-second", function(){
      $("#favs-first, #favs-third").removeClass("activenav").addClass("inactivenav");
      $("#favs-second").removeClass("inactivenav").addClass("activenav");

    });

    $(".nav_icon").on("click", "#favs-third", function(){
      $("#favs-first, #favs-second").removeClass("activenav").addClass("inactivenav");
      $("#favs-third").removeClass("inactivenav").addClass("activenav");
    });
});
</script>

