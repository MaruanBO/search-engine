<script>
LoaderUtil.addLoadEvent(function()
{		
	var onClick = function(e){
		var target = jQ(e.target);
		if (target.is('a')) {
			e.preventDefault();
			var targetHref = target.attr('href');
			var targetTitle = target.attr('title');
			var parentId = e.currentTarget.id;
			if (parentId && parentId == 'nav') {
				parentId = 'topNav';
			}
			_gaq.push(['_trackEvent',category,parentId,targetTitle]);
			location.href = targetHref;
			
		}
	};
	var category = 'homepage-nav-click';
	jQ('#nav').bind('click',onClick);
	jQ('#sidebar').bind('click',onClick);
	
	// TPTECH-2032: Load tracking pixels after page has loaded

	// Mediaplex Universal
