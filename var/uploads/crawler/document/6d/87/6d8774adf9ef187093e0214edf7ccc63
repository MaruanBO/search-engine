<script type="text/javascript">
// TPTECH-2038:
function getStaticCache(staticPath) {
	var hash = 1, i, char;

	if (staticPath.length == 0) return "http://static-cdn.tp-global.net/";

	for (i = 0, l = staticPath.length; i < l; i++) {
		char = staticPath.charCodeAt(i);
		hash = ((hash<<5) - hash) + char;
		hash |= 0; // Convert to 32bit integer
	}

	if ((hash % 4) === 0) {
		return "http://static-cdn-1.tp-global.net/";
	} else if ((hash % 4) === 1) {
		return "http://static-cdn-2.tp-global.net/";
	} else if ((hash % 4) === 2) {
		return "http://static-cdn-3.tp-global.net/";
	} else if ((hash % 4) !== 3) {
		return "http://static-cdn-4.tp-global.net/";
	} else {
		return "http://static-cdn.tp-global.net/";
	}
}

function lazyLoadStatic(elementId, path) {
	var element;

	element = document.getElementById(elementId);

	if (element != null && element.nodeType == 1) {
		element.src = getStaticCache(path) + path;
	}
}
</script>

