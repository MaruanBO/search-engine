	var isiPad = navigator.userAgent.match(/iPad/i) != null;

	if (!isiPad) {
		// remove CSS fallback
		jQ('.dropdown .hover').removeClass('hover');
		
		jQ(document).ready(function(){
			// on hover, fade in and delay menu
			jQ('ul.nav li.dropdown').hover(function() {
			  jQ(this).find('.dropdown-menu').stop(true, true).delay(300).fadeIn(300);
			}, function() {	
			  jQ(this).find('.dropdown-menu').stop(true, true).delay(300).fadeOut(300);
			});
		});
	} 	
</script>


