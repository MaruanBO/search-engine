		//should probably move this to end of body
		function purdifier (longString) {
			//does not capture &nbsp; as space
			var cleanString = longString.replace(/ /g,"-");
			cleanString = cleanString.replace(/&-/g,'');
			return cleanString;
		}
		jQ(document).ready(function() {
    		jQ('#header-nav li.dropdown').each(function(index, value){
    			//find dropdown label
				var dropdowntitle = jQ(this).find('>a').attr('title');
				jQ(this).find('>a').each(function(index, value){
					var oldAnc = this;
					oldAnc.setAttribute('name', purdifier('&lid=GlobalHeader_'+dropdowntitle));
				});
				var sectionheader = '';
				jQ(this).find('>ul span').each(function(index, value){
					//find the header for the section
					//add check if no anchor, then grab text value of li	
					jQ(this).find('li').each(function(index, value){
						if (jQ(this).hasClass('nav-header')) {
							if(jQ('>a').length > 0) {
								sectionheader = jQ(this).find('>a').attr('title');	
							} else {
								sectionheader = jQ(this).text();
							}
						}
						jQ(this).find('a').each(function(index, value){
						//should check for image and add image identifier
						var oldAnc = this;
			        	oldAnc.setAttribute('name', purdifier('&lid=GlobalHeader_'+dropdowntitle+'_Sub_'+sectionheader+'_'+jQ(this).attr('title')));
						});
					});

