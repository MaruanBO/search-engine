	<script type="text/javascript">
		function showSection(sectionName){
			var div;
			var expand;
			var collapse;
			
			div = document.getElementById("div" + sectionName);
			expand = document.getElementById("imgExpand" + sectionName);
			collapse = document.getElementById("imgCollapse" + sectionName);
			
			div.style.display = "";
			expand.style.display = "none";
			collapse.style.display = "";
		}
		
		function hideSection(sectionName){
			var div;
			var expand;
			var collapse;
			
			div = document.getElementById("div" + sectionName);
			expand = document.getElementById("imgExpand" + sectionName);
			collapse = document.getElementById("imgCollapse" + sectionName);
			
			div.style.display = "none";
			expand.style.display = "";
			collapse.style.display = "none";
		}
		
	</script>
