 function StartScrolling() { 
 var isShowSearchBox;
 var startHeight = 30;
 var endHeight = 100;
 var mapObj = $o('MapField');
 var masker = $o('MapFieldMasker');
 var homeSearch = $o('Homesearchbox');
 var headerSearch = $o('headerSearchBox');
 if (System.Browser.isChromeOrSafari) {
 scrollElement = document.body;
 }
 else {
 scrollElement = document.documentElement;
 }
 isShowSearchBox = scrollElement.scrollTop > (homeSearch.offsetHeight - 96);
 function onScroll() { 
 //Show SearchBox
 if (scrollElement.scrollTop <= homeSearch.offsetHeight - 96) {
 if (!isShowSearchBox) {
 Perform.Alpha.Natural(headerSearch, 0);
 sectionArrowBorder.style.display = '';
 Perform.Alpha.Natural(sectionArrowBorder, 100);
 isShowSearchBox = true;
 }
 }
 else {
 if (isShowSearchBox) {
 Perform.Alpha.Natural(headerSearch, 100);
 Perform.Alpha.Natural(sectionArrowBorder, 0, null, function () { sectionArrowBorder.style.display = 'none'; });
 isShowSearchBox = false;
 }
 }
 
 //Show map
 var screenBottom = scrollElement.scrollTop + clientHeight;
 var mapTop = Perform.Position.Top(mapObj), mapBottom = mapTop + mapObj.offsetHeight;
 if (screenBottom <= mapTop + startHeight) {
 masker.style.height = mapObj.offsetHeight + "px";
 }
 else if (screenBottom >= mapBottom + endHeight) {
 masker.style.height = 75 + "px";
 }
 else{
 var maskerHeight = (mapBottom + endHeight - screenBottom) * (mapObj.offsetHeight - 75) / (mapObj.offsetHeight - startHeight + endHeight) + 75;
 masker.style.height = maskerHeight + "px";
 }
 }
 System.Event.Add(window, "scroll", onScroll);
 onScroll();
 }
 function OnListingSlide(index) {
 var lastItem = window.lastListingCity;
 var o = $o('ListingCity' + index);
 if (lastItem)
 lastItem.className = 'listingCityButton extraLargeButton';
 o.className = 'newBlueButton1 extraLargeButton';
 window.lastListingCity = o; 
 }
 function ItemOn(o,groupItem) {
 var itemList = $n("GroupItem" + groupItem);
 if (itemList) {
 for (var i = 0; i < itemList.length; i++) {
 if (o == itemList[i]) {
 itemList[i].className = "item on";
 }
 else {
 itemList[i].className = "item off";
 }
 }
 }
 }
 function OnArrowDownClick() {
 var section = $o('WhyMyMovotoSection');
 var sectionTop = Perform.Position.Top(section);
 Perform.Scroll.Natural(scrollElement, null, sectionTop - headerHeight);
 }
