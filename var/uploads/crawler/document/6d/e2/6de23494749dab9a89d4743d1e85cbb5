  $('.category-selection-indicator').css({transform: "translateX(" + (e.position().left + 8) + "px)", width: (e.innerWidth() + 16) + "px"});
  $('#seo-footer .seo-links').hide();
  $('#' + $(e).data('link-table')).show();
}

$('.category-selection-indicator').show();

$('.footer-browse-categories a').each(function(){
  $(this).click(function () {
    var e = $(this);
    $('.footer-browse-categories a').removeClass('active');
    e.addClass('active');
    slideIndicatorUnderElement(e);
    return false;
  });
});
