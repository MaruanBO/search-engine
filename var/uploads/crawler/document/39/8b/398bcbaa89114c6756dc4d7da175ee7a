$("#q").autocomplete("/search/autocomplete?infix=1",
{
scrollHeight: 500,
width: 420,
formatItem: function (row)
{
switch (row[0])
{
case 'Realm':
return "<span class='informal'>"+row[0]+" | </span> <a class='realm' href='"+row[1]+"'>"+row[2]+"</a>";
case 'Guild':
return "<span class='informal'>"+row[0]+" | </span> <span class='realm'>"+row[3]+"</span> <a class='guild "+row[4]+"' href='"+row[1]+"'>"+row[2]+"</a>";
case 'Character':
return "<span class='informal'>"+row[0]+" | </span> <span class='realm'>"+row[3]+"</span> <a class='character "+row[4]+"' href='"+row[1]+"'>"+row[2]+"</a>";
}
},
formatResult: function (row)
{
if ("undefined" == typeof row[3])
  return row[2];
else
  return row[2]+" "+row[3];
}
});
$("#q").result(
function(event,row)
{
location.href = row[1];
}
);

    })();
  }
  else
    window.setTimeout(_init, 100);
}
_init();
}
