<script type="text/javascript">
$(document).ready(function(){	

	$("#yes-key").click(function() {
		$("#yes-key-form").submit();
	});
	
	$("#mainSignInBtn").click(function(){	
		var e = $("#loginUserName").val();
		var p = $("#loginPassword").val();
		
		if(!isEmpty(e) && !isEmpty(p)) {
			$(this).hide();
			$("#mainSignInBtnLoading").show();
			$("#mainLoginForm").submit();
		} else {
			$("#emptyLoginError").show();
		}
	});
	
	function isEmpty(input) {		
		if (input == '' || input == undefined) {
			return true;
		}		
	   	return false;
	}
	
	$("#loginPassword").keypress(function(e) {
        if(e.which == 13) {
        	e.preventDefault();
            $(this).blur();
			$("#mainSignInBtn").focus().click(); 
			return false;
        }        
    });

	checkLoginPasswordPlaceHolder();
	
    $("#loginPassword").focus(function(){
    	checkLoginPasswordPlaceHolder();	
    });
    
    $("#loginPassword").keyup(function(e) {
  		checkLoginPasswordPlaceHolder();
    });  	
	
    function checkLoginPasswordPlaceHolder(){
    	if($("#loginPassword").val().length > 0) {
    		$("#loginPassword").removeClass("SE_password_button_placeholder");
    		$("#loginPassword").addClass("SE_password_button_no_placeholder");
    	} else {
    		$("#loginPassword").removeClass("SE_password_button_no_placeholder");
    		$("#loginPassword").addClass("SE_password_button_placeholder");
    	}  
	}  

	checkUserNamePlaceHolder();

    $("#loginUserName").focus(function(){
    	checkUserNamePlaceHolder();	
    });
    
    $("#loginUserName").change(function() {
    	checkUserNamePlaceHolder();	
    });    
    
    $("#loginUserName").click(function() {
    	checkUserNamePlaceHolder();	
    });      
    
    $("#loginUserName").keyup(function(e) {
  		checkUserNamePlaceHolder();
    });    
    
    function checkUserNamePlaceHolder(){
    	if($("#loginUserName").val().length > 0) {
    		$("#loginUserName").removeClass("SE_email_button_placeholder");
    		$("#loginUserName").addClass("SE_email_button_no_placeholder");
    	} else {
    		$("#loginUserName").removeClass("SE_email_button_no_placeholder");
    		$("#loginUserName").addClass("SE_email_button_placeholder");
    	}  
	}    
    
});
</script>





