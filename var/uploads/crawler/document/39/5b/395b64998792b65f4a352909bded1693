<script type="text/javascript">
$(document).ready(function(){	

	$(".forgot-password-link").click(function() {
		$("#modalForgotPassword").hide();		
		$("#forgot-password-error").text("");
		$("#forgot-password-msg").text("");
		$("#forgot-password-form").show();
		$("#forgotPassword").show();
	});
	
    $("#forgotPassword").click(function(){    
		var email = $("#forgot_pw_email").val();
		$(this).hide();
		
		if (email.length>0) {
			$("#modalForgotPassword").show();
			forgotPasswordRequest(email);				
		} else {
			$("#forgot-password-error").text("Email is required.");
		}

    });


	// AJAX Forgot Password
	
	function forgotPasswordRequest(email) {
		var protocol = document.location.protocol;
		var currentrootValue = $("#webroot-value").text();
		
		if(protocol == 'https:') {	
			currentrootValue = $("#secureroot-value").text();	
		}

		getForgotPasswordRequestURL(currentrootValue+'/send_password_reset_link','email',encodeURIComponent(email));
	}

	function getForgotPasswordRequestURL(url,key1,val1) 	
	{
	        if (window.XMLHttpRequest) 
	        { // Non-IE browsers                
	                var req = new XMLHttpRequest();   
	                //$("#req").append(id + " ");    
	                req.onreadystatechange = function() { 
	        									if (req.readyState==4) {
	            									if (req.status==200) { 
	                									processForgotPasswordRequestResponse(req,val1);
	                								}
	                							}
	    										};
	    			
	                try {
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                } catch (e) {
	                         alert(e);
	                }
	                req.send(null);
	        } else if (window.ActiveXObject) { // IE                        
	                var req = new ActiveXObject("Microsoft.XMLHTTP");
	                if (req) {
	                         req.onreadystatechange = function() { 
	        									if (req.readyState==4) {
	            									if (req.status==200) {
	                									processForgotPasswordRequestResponse(req,val1);
	                								}
												}	                								
	    										
	    										};
	    										
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                        req.send();
	                 }
	        }
	} 
	
	function processForgotPasswordRequestResponse(req,email) {		
	    var returned_html = req.responseText;
	    var obj = $(returned_html);
	
		var successMsg = $(obj).find('#successMsg').html();
		var errorMsg = $(obj).find('#errorMsg').html();
					
		if(errorMsg != null) {
			$("#forgot-password-msg").text('');		
			$("#forgot-password-error").text(errorMsg);
			$("#modalForgotPassword").hide();
			$("#forgotPassword").show();
		}
		
		if(successMsg != null) {
			$("#forgot-password-form").hide();
			$("#forgot-password-error").text('');
			$("#forgot-password-msg").text(successMsg);		
		}								
	}
	
});
</script>


 




