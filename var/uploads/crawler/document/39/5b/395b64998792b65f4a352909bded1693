<script type="text/javascript">
$(document).ready(function(){	

	$(".order-card").click(function() {
		$("#order-card-button-div").hide();	
		
		$("#session-card-error").text('');
		$("#order-card-header").text('');
		$("#order-card-text").text('');
		$("#order-card-error-msg").text('');
		$("#order-card-msg").text('');
		
		var method = "init";
		orderCardModalRequest(method);
	});
	
	$("#order-card-submit").click(function() {
		$("#order-card-error-msg").text('');
		$("#order-card-msg").text('');
		var method = "order";
		
		$(this).hide();
		$("#orderCardLoading").show();						
		orderCardModalRequest(method);
	});	

	// AJAX text block
	
	function orderCardModalRequest(method) {
		var protocol = document.location.protocol;
		var currentrootValue = $("#webroot-value").text();
		
		if(protocol == 'https:') {	
			currentrootValue = $("#secureroot-value").text();	
		}
		
		getOrderCardModalRequestURL(currentrootValue+'/order_card_modal','method',method);
	}

	function getOrderCardModalRequestURL(url,key1,val1) 	
	{
	        if (window.XMLHttpRequest) 
	        { // Non-IE browsers                
	                var req = new XMLHttpRequest();   
	                //$("#req").append(id + " ");    
	                req.onreadystatechange = function() { 
	                							//alert(url+" "+req.status);
	        									if (req.readyState==4) {
	            									if (req.status==200) { 
	                									processOrderCardModalResponse(req);
	                								}
	                							}
	    										};
	    			
	                try {
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                } catch (e) {
	                         alert(e);
	                }
	                req.send(null);
	        } else if (window.ActiveXObject) { // IE                        
	                var req = new ActiveXObject("Microsoft.XMLHTTP");
	                if (req) {
	                         req.onreadystatechange = function() { 
	        									if (req.readyState==4) {
	            									if (req.status==200) {
	                									processOrderCardModalResponse(req);
	                								}
												}	                								
	    										
	    										};
	    										
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                        req.send();
	                 }
	        }
	} 
	
	function processOrderCardModalResponse(req) {		
	    var returned_html = req.responseText;
	    var obj = $(returned_html);
	
		var textMsg = $(obj).find('#textMsg').html();
		var textHeader = $(obj).find('#textMsgTitle').html();
		var sessionErrorMsg = $(obj).find('#sessionErrorMsg').html();
		var processingErrorMsg = $(obj).find('#processingErrorMsg').html();
		var errorMsg = $(obj).find('#errorMsg').html();		
		var successMsg = $(obj).find('#successMsg').html();
						
		$("#orderCardLoading").hide();	
		$("#order-card-submit").show();	
		
				
		if(sessionErrorMsg != null) {	
			$("#session-card-error").show();
			$("#order-card-header").hide();
			$("#order-card-text").hide();
			document.getElementById('session-card-error').innerHTML = sessionErrorMsg;  	
		} else {	
			
			$("#session-card-error").hide();
			$("#order-card-header").show();
			$("#order-card-text").show();		
			
			if(textHeader != null) {	
				document.getElementById('order-card-header').innerHTML = textHeader;
			}	
			
			if(textMsg != null) {	
				document.getElementById('order-card-text').innerHTML = textMsg;
			}		
			
			if(errorMsg != null) {
				$("#order-card-error-msg").show();
				document.getElementById('order-card-error-msg').innerHTML = errorMsg;			
			}
	
			if(processingErrorMsg != null) {				
				$("#order-card-button-div").hide();	
				$("#order-card-msg").hide();		
				$("#order-card-error-msg").show();	
				document.getElementById('order-card-error-msg').innerHTML = processingErrorMsg;
			} else {
				$("#order-card-button-div").show();	
			}			
			
			if(successMsg != null) {	
				$("#order-card-button-div").hide();	
				$("#order-card-error-msg").hide();	
				$("#order-card-msg").show();	
				document.getElementById('order-card-msg').innerHTML = successMsg;
			}				
		}
		
	}
	
});
</script>


 












