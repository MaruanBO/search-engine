<script type="text/javascript">
$(document).ready(function(){	
	
	$(window).unload( function () { $("#mask").fadeOut(); } );
	
    var window_width = $(window).width();
    var window_height = $(window).height();
    var docHeight = $(document).height()+50;

    $('.modal_window, .modal_window_small').each(function(){
        var modal_height = $(this).height();
        var modal_width = $(this).width();
        var top = 10;
        var left = (window_width-modal_width)/2;
        $(this).css({'top' : top , 'left' : left});
    });

    $('.activate_modal').click(function(){
        var modal_id = $(this).attr('name');
        show_modal(modal_id);
		//$(".focus-panel-1-new").focus();
    });

    $('.close_modal').click(function(){
    	var flag = $(this).hasClass("close_modal");
    	
    	if (flag) {
    		//alert("here " + flag);
        	$('body').css('overflow','scroll');    	
            close_modal();    		
    	}
    });	
    
    function close_modal(){
    	$('#mask').fadeOut(500);
    	$('.modal_window, .modal_window_small').fadeOut(500);
	}

	function show_modal(modal_id){
    	$('#mask').css({ 'display' : 'block', opacity : 0, 'height' : docHeight});
    	$('#mask').fadeTo(500,0.9);
    	$('#'+modal_id).fadeIn(500);
	}
	
	$("#se-login-close-modal").live('click',function() {	
		history.pushState(null, null, "/www/home.jsp");
	});	

	function close_modal(){
    	
    	$('#mask').fadeOut(500);
    	$('.modal_window, .modal_window_small').fadeOut(500);
	}
	
	$(".offer-detail").live('click',function() {	
		var id = $(this).attr("offerid");
		$("#offer-detail-modal-header").text("Offer Details");
		$("#offer-detail-modal-body").html($("#detail_"+id).html());
		show_modal("offer-detail-modal");

	});

	$("#email-accepted-offers").live('click',function() {	
		$("#send-accepted-offers-btn").show();
		$("#sending-accepted-offers-email").hide();
		$("#close-accepted-offers-email").hide();
		$("#cancel-accepted-offers-email").show();
		
		$("#email-accepted-offers-body").html("Send accepted offer list to my account's email address.");
		//$("#email-accepted-offers-msg").show();
		
		show_modal("email-accepted-offers-modal");
	});	
	
// 	$(".activate-personalized-offers-modal").live('click',function() {	
// 		alert("here");
//		var id = $(this).attr("offerid");
//		$("#offer-detail-modal-header").text("Offer Details");
//		$("#offer-detail-modal-body").html($("#detail_"+id).html());
// 		show_modal("offer-text-block-modal");
// 	});	
		


});
</script> 
 







