<script type="text/javascript">
$(document).ready(function() {	
	var timeoutHandle;
	
	initSlides();
	runSlides();
	
	function initSlides() {	
		$('#slides .slide').last().addClass("lastSlide");
	}
	
	function runSlides() {
		timeoutHandle = window.setTimeout(
  			function() 
  			{
				switchSlide();    			    			
    			runSlides();	
  			}, 7000);  	
	}  		
	
	function switchSlide() {
			var $first = $("#slides .slide").first();
			var $this = $("#slides .slide:visible");
			var $nav = $(".slider-nav span");
	   		
	   		$this.hide();
	   		$nav.removeClass("active");
	   		
	   		if($this.hasClass("lastSlide")) {  
	   		
	   			$("#"+$first.attr("id") + "-nav").addClass("active");
	   			$first.fadeIn();
	   			
	   		} else { 
	   		  		
	   			$("#"+$this.next().attr("id") + "-nav").addClass("active");
	   			$this.next().fadeIn(); 
	   			  			
	   		}   								
	}
  
	$(".slider-nav span").click(function() {			
		window.clearTimeout(timeoutHandle);
		
		var target = $(this).attr("target");
			
		$(".slider-nav span").removeClass("active");
		$(this).addClass("active");
			
		$(".slide").hide();
		$("#"+target).stop(true, true).fadeIn();
		
		timeoutHandle = window.setTimeout(
  			function() 
  			{
				switchSlide();    			    			
    			runSlides();	
  			}, 7000);
	});	

});
</script> 

