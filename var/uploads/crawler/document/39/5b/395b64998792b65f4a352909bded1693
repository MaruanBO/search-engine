<script type="text/javascript">
$(document).ready(function(){	

	$(".text-block").click(function() {
		var id = $(this).attr('id');

		textBlockRequest(id);
	});

	// AJAX text block
	
	function textBlockRequest(id) {		
		var protocol = document.location.protocol;
		var currentrootValue = $("#webroot-value").text();
		
		if(protocol == 'https:') {	
			currentrootValue = $("#secureroot-value").text();	
		}

		getTextBlockRequestURL(currentrootValue+'/text_block','id',id);
	}

	function getTextBlockRequestURL(url,key1,val1) 	
	{
	        if (window.XMLHttpRequest) 
	        { // Non-IE browsers                
	                var req = new XMLHttpRequest();   
	                //$("#req").append(id + " ");    
	                req.onreadystatechange = function() { 
	        									if (req.readyState==4) {
	            									if (req.status==200) { 
	                									processTextBlockResponse(req);
	                								}
	                							}
	    										};
	    			
	                try {
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                } catch (e) {
	                         alert(e);
	                }
	                req.send(null);
	        } else if (window.ActiveXObject) { // IE                        
	                var req = new ActiveXObject("Microsoft.XMLHTTP");
	                if (req) {
	                         req.onreadystatechange = function() { 
	        									if (req.readyState==4) {
	            									if (req.status==200) {
	                									processTextBlockResponse(req);
	                								}
												}	                								
	    										
	    										};
	    										
	                        req.open("POST",url+'?'+key1+'='+val1, true);
	                        req.send();
	                 }
	        }
	} 
	
	function processTextBlockResponse(req) {		
	    var returned_html = req.responseText;
	    var obj = $(returned_html);
	
		var textMsg = $(obj).find('#textMsg').html();
		var textHeader = $(obj).find('#textMsgTitle').html();
		var errorMsg = $(obj).find('#errorMsg').html();
					
		if(errorMsg != null) {	
			document.getElementById('text-block-msg').innerHTML = errorMsg;  	
		}
		
		if(textMsg != null) {	
			document.getElementById('text-block-msg').innerHTML = textMsg;
		}		
		
		if(textHeader != null) {	
			document.getElementById('text-block-header').innerHTML = textHeader;
		}									
	}
	
});
</script>


 












