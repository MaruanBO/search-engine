<script type="text/javascript">
$(document).ready(function(){	


	$("#modalSignIn").click(function(){	
		var e = $("#login-modal-email").val();
		var p = $("#login-modal-password").val();
		
		if(!isEmpty(e) && !isEmpty(p)) {
			$(this).hide();
			$("#modalSigningInLoading").show();
			$("#modalLoginForm").submit();
		} else {
			$("#login-msg").text("Email address and password are required.");
		}
	});

	$("#login-modal-password").keypress(function(e) {
        if(e.which == 13) {
        	e.preventDefault();
            $(this).blur();
			$("#modalSignIn").focus().click(); 
			return false;
        }        
    });

	function isEmpty(input) {		
		if (input == '' || input == undefined) {
			return true;
		}		
	   	return false;
	}
});
</script>


 












