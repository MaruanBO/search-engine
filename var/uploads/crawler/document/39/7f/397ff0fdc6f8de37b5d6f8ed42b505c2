function checkFBStatus(){		
		FB.login(function(response) {
			  if (response.session) {
				if (response.perms) {
				  // user is logged in and granted some permissions.
				  // perms is a comma separated list of granted permissions				 
				} else {
				  // user is logged in, but did not grant any permissions
				}
				 window.location.href="http://www.careers.org/careers-jobs/jobseekers/face_login";
				
			  } else {
				// user is not logged in
			  }
			}, {perms:'read_stream,user_interests,user_education_history,user_location,user_about_me,user_work_history,email,user_hometown,user_photos'});
		}

