<script>
	function dropDownMainMenu(item_id) {
		changeMainMenuClass( item_id, "top-nav-item-menu-active" );
	}
	
	function hideDropDownMainMenu( item_id ) {
		changeMainMenuClass( item_id, "top-nav-item-menu" );
	}
	
	function changeMainMenuClass( item_id, new_class) {
		the_div = $("top-nav-item"+item_id)
		if (the_div) {
			the_div.className = new_class
		}
	}
	
</script>


      	
