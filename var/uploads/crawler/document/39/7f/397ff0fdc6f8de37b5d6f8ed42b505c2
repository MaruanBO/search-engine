		<script type="text/javascript">
jQuery(document).ready(function() {
var homelogged = 1;
homeloggedres = jQuery.ajax({
   type: "GET",
   async: false,
   url: "/careers-jobs/jobseekers/home_loggedincheck"
 }).responseText;
 if(jQuery.trim(homeloggedres)=="success")
 {
 homelogged = 0;
 }
 	if(homelogged)
	{
	//When page loads...
	jQuery(".main_tab_container").show(); //Hide main content div
	jQuery(".tabs").show(); //Show first tab content
	jQuery(".tab_content").hide(); //Hide all content
	jQuery("ul.tabs li:first").addClass("active").show(); //Activate first tab
	jQuery(".tab_content:first").show(); //Show first tab content

	//On Click Event
	jQuery("ul.tabs li").click(function() {

		jQuery("ul.tabs li").removeClass("active"); //Remove any "active" class
		jQuery(this).addClass("active"); //Add "active" class to selected tab
		jQuery(".tab_content").hide(); //Hide all tab content

		var activeTab = jQuery(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
		jQuery(activeTab).fadeIn(); //Fade in the active ID content
		return false;
	});
jobseekers_usernames = readCookie("CakeCookie[Jobseeker][username]");
jobseekers_passwords = readCookie("CakeCookie[Jobseeker][password]");
jobseekers_remembers = readCookie("CakeCookie[Jobseeker][remember]");
if(jobseekers_usernames!=null && jobseekers_usernames!='' )
		{
			jQuery("#JobseekerUsername").val(jobseekers_usernames);			
		}
if(jobseekers_passwords!=null && jobseekers_passwords!='' )
		{
			jQuery("#JobseekerPassword").val(jobseekers_passwords);
		}
if(jobseekers_remembers!=null && jobseekers_remembers!='' )
		{
			jQuery("#JobseekerRemember").attr('checked', true);
		}

recruiters_usernames = readCookie("CakeCookie[Recruiter][userlogin]");
recruiters_passwords = readCookie("CakeCookie[Recruiter][password]");
recruiters_remembers = readCookie("CakeCookie[Recruiter][remember]");
if(recruiters_usernames!=null && recruiters_usernames!='' )
		{
			jQuery("#RecruiterUserlogin").val(recruiters_usernames);			
		}
if(recruiters_passwords!=null && recruiters_passwords!='' )
		{
			jQuery("#RecruiterPassword").val(recruiters_passwords);
		}
if(recruiters_remembers!=null && recruiters_remembers!='' )
		{
			jQuery("#RecruiterRemember").attr('checked', true);
		}
	}
	else
	{
	jQuery(".main_tab_container").hide(); //Hide main content div
	jQuery(".tabs").hide(); //Hide all tabs
	jQuery(".tab_container").hide(); //Hide all content
	}

});
            /* <![CDATA[ */
            jQuery(function(){
