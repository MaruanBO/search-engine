<script language="Javascript" type="text/javascript">
function submitLogin(msg) {
	if (checkSystemTime(1424382249,msg)) document.login_form.submit();
}

function ent(event,msg) {
    var keynum;

    if (window.event)       // IE
        keynum = event.keyCode;
    else if (event.which)   // FF, Opera, etc
        keynum = event.which;

    if (keynum == 13)
        submitLogin(msg);
    else
        return true;
}

function CleanUsername()
{
    var filter = /[^a-zA-Z0-9_]/i;
    if (filter.test(document.sform.f_username.value)) document.sform.f_username.value = document.sform.f_username.value.replace(/[^a-zA-Z0-9_]/i,'');
}

function winOpen(theUrl,w,h)
{
	window.open(theUrl,'i','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width='+w+',height='+h+',top='+((screen.height-h)/2)+',left='+((screen.width-w)/2));
}

