			<script>
	include.ready(include.jquery, function() {
		$(document).ready(function(){
			if( $('body').hasClass("pagetype-countryselector") ){
				//don't show this on the Country Selector page.
				return;
			}

			var cookieName = "loyaltyModalHits"
			//check for cookie
			var c = $.cookie(cookieName);
			//if not set...
			if(c == null) c = 0;
			if(c < 1){
				//...display modal...
				$('body').append($('#ERSignupModal'));
				$('#ERSignupModal').show();
				//find the top
				topOfModal = ($(window).height() - $('#ERSignupModal .modalbox').outerHeight()) / 2;
				if(topOfModal < 0) topOfModal = 0;
				$('#ERSignupModal .modalbox').css('top', topOfModal);
			}
			//...set cookie
			$.cookie(cookieName, parseInt(c,10)+1, { expires: 30, path: '/' });

			$('#ERSignupModal .closebtn').click(function(){
				$('#ERSignupModal').remove();
			});
			$('#ERSignupModal .overlay').click(function(e){
				$('#ERSignupModal').remove();	//close the modal upon click of the overlay...
			}).children().click(function(e) {
				e.stopPropagation();				//... but not when clicking children. (no bubble up)
			});
		});
	});
</script>
