<script language="javascript">

function displaySelected(field)
{


	var DetailObj_Latest = document.getElementById("USDANewsDetail_Latest");
	var DetailObj_Blog = document.getElementById("USDANewsDetail_Blog");

	var TopObj_Latest = document.getElementById("USDANewsTop_Latest");
	var TopObj_Blog = document.getElementById("USDANewsTop_Blog");

	DetailObj_Latest.style.visibility = "hidden";
	DetailObj_Blog.style.visibility = "hidden";

	TopObj_Latest.className = "toplink";
	TopObj_Blog.className = "toplink";

	if (field == "")
	{
		field = "Latest";
	}
		var selectedObj_Top = document.getElementById("USDANewsTop_" + field);
		var selectedObj_Detail = document.getElementById("USDANewsDetail_" + field);


		selectedObj_Top.className = "selectedTab";
		selectedObj_Detail.style.visibility = "visible";

}
</script>

