	<script language="javascript">
		// Brand Site Disruptor [Creates modal for marketing promotion]
		$(function() {
			'use strict';

			var countdown_time, current_time, qnow, start_time, delay_time, open_delay, is_offer_polite,
				openOffer, openOfferPolitely;
			
			// timeout for popup, in milliseconds
			countdown_time = 60000;

			// gives us a simple flag to help scope this (NA only for now)
			is_offer_polite = true;
			
			// set the current time to create the to calculate the amount of time for the countdown and QNOW cookie if it is not present from a previous page load.
			current_time = new Date();
	
			// this checks to see if the cookie is there
			qnow = $.cookie('QNOW');
			if (qnow === null) {
				// if the cookie isn't there set the cookie
				$.cookie('QNOW', current_time);

				// set the start time in to the cookie set
				start_time = new Date(current_time);
			} else {
				// if the cookie exists set the start time based on existing cookie
				start_time = new Date(qnow);
			}
	
			// this figures out how much time is left base on the current time minus the start time minus test time
			delay_time = (countdown_time - (current_time - start_time));
	
			// if the delay time is greater then 0 reset the delay to the time remaining
			open_delay = (delay_time > 0) ? delay_time : 0;

			// our unified open call
			openOffer = function () {
				$.colorbox({
					fixed:  true,
					inline: true,
					href:   '#brandsiteInterruptMessageBox'
				});
			};

			// this allows us to queue up the existing open on the colorbox close event if the colorbox is already showing some content
			openOfferPolitely = function () {
				if ($.colorbox.isOpen()) {
					// .one binds to the first occurance of an event and then removes itself
					$(document).one('cbox_closed', openOffer);
				} else {
					// otherwise just fire the open command
					openOffer.apply(this, arguments);
				}
			};

			// set the time out to the current delay left
			if (open_delay > 0) {
				// if we have specified that the offer should be polite, pass that handler
				// update: attaching this to the window object, so we can modify it, if need be (currently mens landing)
				window.openBrandsiteOfferId = setTimeout((is_offer_polite) ? openOfferPolitely : openOffer, open_delay);
			}
		});
	</script>


