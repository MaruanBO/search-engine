			{
			//Adslot 6 declaration
			gptadslots[6]= googletag.defineSlot('/55210017/hp_skin', [[1,1]],'skin').addService(googletag.pubads());
			
						
			//Adslot 7 declaration
			gptadslots[7]= googletag.defineSlot('/55210017/hp_overlay', [[1,1]],'overlay').addService(googletag.pubads());
			
						
			}

			var userViews = decodeURI(jQuery.cookie('userViews')).split("|");
		    userViews = userViews[0];
			// user action for targeting popup closed/action completed 
		    var userAction = 'n';
		    if (jQuery.cookie('popUpClosed')) {
			    userAction = 'x';
		    }
		    if ( jQuery.cookie('hg_fbLike')) {
			    userAction = 'c';
		    }
			googletag.pubads().enableSingleRequest();
			googletag.pubads().setTargeting('s',['hp']).setTargeting('pid',['']).setTargeting('d',[browserSize]).setTargeting('sl',['n']).setTargeting('t',['n']).setTargeting('uv', [userViews] ).setTargeting('ua', [userAction] );
