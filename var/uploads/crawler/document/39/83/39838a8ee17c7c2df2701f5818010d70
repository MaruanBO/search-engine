		<script type="text/javascript">	
				
			// Width Breakpoints 1024px 728px 480px 320px
			var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0)
				
			//We don't need height but here it is incase we ever do
			//var h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0)
		
			var browserSize;
				
				if (w >1024)
				{
					browserSize = 'dt';	
				}
				else if (w <= 1024 && w > 480 ) 
				{
					browserSize = 'tl';	
				}
				else if ( w <= 480 )
				{
					browserSize = 'sp';	
				}
				else {
					browserSize = 'dt';
				}
		
			// GPT slots
		 	var gptAdSlots = [];
		
			var mappingLeader_a = googletag.sizeMapping().
		        addSize([1024, 0], [[728,90]]). // Desktop
		        addSize([0, 0], [[728,90],[320,50]]). // Tablet
		        build();
		
			var mappingLeader_b = googletag.sizeMapping().
		        addSize([1024, 0], [[728,90]]). // Desktop
		        addSize([480, 0], [[728,90],[1,1]]). // Tablet
		        addSize([0, 0], [[300,50],[1,1]]). // Smartphone
		        build();
		
		    var mappingBox_a = googletag.sizeMapping().
		        addSize([1024, 0], [[300,1050],[300,600],[300,250]]). // Desktop
		        addSize([480, 0], [[300,250]]). // Tablet
		        addSize([0, 0], [[300,250],[200,200],[1,1]]). // Smartphone
		        build();


		    // Cookie for first view impressions
			var cookieSplit = "";
		    if ( jQuery.cookie('userViews') == undefined ) {
	    		var date = new Date();
			    var minutes = 1440;
			    date.setTime(date.getTime() + (minutes * 60 * 1000));
			    var cookieVal = '1|' + date; 
		    	jQuery.cookie('userViews', cookieVal, { expires: date, path: '/' });
		    } else {
			    cookieSplit = decodeURI(jQuery.cookie('userViews')).split("|");
			    var newCookieVal = parseInt ( cookieSplit[0] ) + 1;
			    var cookieDate = new Date (cookieSplit[1]);
		    	jQuery.cookie('userViews', newCookieVal + '|' + cookieDate, { expires: cookieDate, path: '/'});

		    }
			
			if (w >1024)
			{
			//Adslot 1 declaration
			gptadslots[1]= googletag.defineSlot('/55210017/hp_pushdown_a', [[970,90]],'pushdown').setTargeting('ps',['a']).addService(googletag.pubads());
			}
			
			//Adslot 2 declaration
			gptadslots[2]= googletag.defineSlot('/55210017/hp_leader_b', [[728,90],[300,50],[1,1]],'leader_b').setTargeting('ps',['b']).defineSizeMapping(mappingLeader_b).addService(googletag.pubads());

						
			if (w > 480 ) {
			//Adslot 3 declaration
			gptadslots[3]= googletag.defineSlot('/55210017/hp_leader_a', [[728,90],[320,50]],'leader_a').setTargeting('ps',['a']).defineSizeMapping(mappingLeader_a).addService(googletag.pubads());			
			}

						
			if ( w > 1024 )
			{
			//Adslot 4 declaration
			gptadslots[4]= googletag.defineSlot('/55210017/hp_box_b', [[300,250]],'box_b').setTargeting('ps',['b']).addService(googletag.pubads());
			}
	
			//Adslot 5 declaration
			var abSwitch = w <= 1024 ? 'b' : 'a';
			gptadslots[5]= googletag.defineSlot('/55210017/hp_box_a', [[300,1050],[300,600],[300,250],[200,200],[1,1]],'box_a').setTargeting('ps',[abSwitch]).defineSizeMapping(mappingBox_a).addService(googletag.pubads());
						
