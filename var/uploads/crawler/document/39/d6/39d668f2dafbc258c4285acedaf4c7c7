<script type="text/javascript">
//<![CDATA[ 
/* ABLib Class 
 * @Param String cookieVarName = Key in Key=Value pair stored in cookie
 * @Param String cookieUsedName = name of cookie to use
 * @Param String cookieValueBase = Prefix of Value in Key=Value pair stored in cookie
 * @Param String forcedModeBaseName = Key in Key=Value pair for forced modes
 * @Param String segmentBreakdown = "10-40-50" determines how many segments too!
 * @Param String theEvar (Optional) = the evar number to use (i.e. 42). This Paramater is optional
 * @Author Jeff Held
 * @Modified by Jermaine Jackson 7/21/2011
 * - Implemented MVC format
 * - Improved Processing Time 
 * - Ensured all variables and arguments follow naming convention described in the GID IT/WCD Coding Standards
 * - Documented Class according to coding standards
 */

var ABLib = Class.create({

	initialize: function(cookieVarName,cookieUsedName,cookieValueBase,forcedModeBaseName,segmentBreakdown,theEvar){
		var main = this.controller.init.main.bind(this); 
		var context = this;
		main(cookieVarName,cookieUsedName,cookieValueBase,forcedModeBaseName,segmentBreakdown,theEvar, context);
	},
	
	model : {
		
		evarToUse :null,
		cookieVariableName : null,
		cookieUsed : null,
		cookieValueBase : null,
		segBreakdown : null,
		retrievedCookieValue : null,
		quickSegment : "",
		forcedModeBase : null,
		modeArrayIndex : null, 
		modeArray : new Array("a","b","c","d","e","f","g","h","i","j","k"),
		userSegment : ""
		
	},
	
	controller : {
		init : {
			main : function(cookieVarName,cookieUsedName,cookieValueBase,forcedModeBaseName,segmentBreakdown,theEvar, context) {
				var model = context.model;
				model.evarToUse = theEvar != "" ? theEvar : false;
				model.cookieVariableName = cookieVarName;
				model.cookieUsed = cookieUsedName;
				model.cookieValueBase = cookieValueBase;
				model.segBreakdown = segmentBreakdown.split("-");
				model.retrievedCookieValue = gidLib.getCookieVar(model.cookieUsed, model.cookieVariableName);
				model.quickSegment = '';
				model.forcedModeBase = forcedModeBaseName;
				model.modeArrayIndex = model.segBreakdown.length;
		
				if (location.href.indexOf(model.forcedModeBase) >= 1) {
					this.controller.forcedMode(context);
				}else{
					this.controller.cookieScanner(context);
				}
			}
		},
		
		cookieScanner:function(context) {
			var model = context.model;
			//If the cookie isn't available, bake one!
			if (model.retrievedCookieValue) {
	
				if (model.evarToUse) {
					context.controller.setABTestVariable(model.retrievedCookieValue, context);
				}

				for (i = 0; i < model.modeArrayIndex; i++) {
					if (model.retrievedCookieValue == (model.cookieValueBase + model.modeArray[i].toUpperCase())) {
						model.userSegment = model.cookieValueBase + model.modeArray[i].toUpperCase();
						model.quickSegment = model.modeArray[i].toUpperCase();
						break;
					}
				}
	
			} else {
				context.controller.tagManager(context);
			}
	
		},
	
		tagManager:function(context) {
	
			//Breaking out the segmentation into specified percentages
			var model = context.model;
			var segmentation = Math.floor((Math.random()*100)+1); // chooses 1 to 100
			//alert("Seg: " + this.Segmentation);	
			var floorThreshold = 0;
			var topThreshold = 0;
			
			for (i = 0; i < model.modeArrayIndex; i++){
	
				topThreshold = floorThreshold + parseInt(model.segBreakdown[i]);
	
				if ((segmentation > floorThreshold) && (segmentation <= topThreshold)){
					// put me in this segment
					model.userSegment = model.cookieValueBase + model.modeArray[i].toUpperCase();
					model.quickSegment = model.modeArray[i].toUpperCase();
					break;
				} else {
					// add top to floor and try next segment
					floorThreshold = topThreshold;
				}
			}
			
			gidLib.setCookieVar(model.cookieUsed,model.cookieVariableName,model.userSegment);
			context.controller.setABTestVariable(model.userSegment, context);
			
		},
		
		forcedMode:function(context) {
		
			var model = context.model;
			for (i = 0; i < model.modeArrayIndex; i++) {
				if (location.href.indexOf(model.forcedModeBase + "=" + model.modeArray[i]) >= 1) {
					model.userSegment = model.cookieValueBase + model.modeArray[i].toUpperCase();
					model.quickSegment = model.modeArray[i].toUpperCase();
					break;
				}
			}
			
			gidLib.setCookieVar(model.cookieUsed,model.cookieVariableName,model.userSegment);
			
			context.controller.setABTestVariable(model.userSegment, context);
			
		},
		
		setABTestVariable: function(setValue, context) {
			var model = context.model;
			if (model.evarToUse) {
				thisEvar = 'eVar' + model.evarToUse;
				ABTestVariables[thisEvar] = setValue;
			}
		}
	}
	
});

// ]]> 
</script>


