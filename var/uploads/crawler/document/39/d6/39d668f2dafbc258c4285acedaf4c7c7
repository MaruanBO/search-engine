<script type="text/javascript">
  function enableResponsiveImages () {
  	var htmlElement = jQuery('html');
  	var imagesToEnable = [];
  	if (htmlElement.hasClass('breakpoint-small')) {
  		imagesToEnable = jQuery('img.showS');
  	} else if (htmlElement.hasClass('breakpoint-medium')) {
  		imagesToEnable = jQuery('img.showM');
  	} else if (htmlElement.hasClass('breakpoint-large')) {
  		imagesToEnable = jQuery('img.showL');
  	} else if (htmlElement.hasClass('breakpoint-xlarge')){
  		imagesToEnable = jQuery('img.showXL');
  	} else {
  		imagesToEnable = jQuery('img.non-responsive'); //this must be a non-responsive page, so load these
  	}
  	jQuery.each(imagesToEnable, function (index, img) {
  		var image = jQuery(img);
  		var imgSrc = image.attr('data-src');
  		if (imgSrc) {
  			image.attr('src', imgSrc);
  		}
  	});
  }

  jQuery(window).load(function () {
  	enableResponsiveImages();
  });

  jQuery(window).resize(function () {
  	setTimeout(enableResponsiveImages, 500);
  });
</script>
