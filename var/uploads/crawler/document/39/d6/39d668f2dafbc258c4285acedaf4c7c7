<script type="text/javascript">
var disruptorManager = {

	CIID 			: '9395386',
	PPC_ID 			: '106878',
	PROMO_TRACKING_ID 	: 'Promo_Disruptor_Legal',
	COOKIE_KEY 		: "ONOL_PROMO_HDLN_DSRPT",
	DISRUPT_TIME 		: 8000,
	closePopupTimerID 	: 0,
	segmentSuffix 		: "_segA",
	mainContent 		: [],
	
	init : function(seg){
		//segmentSuffix is only used when this promo is a volume control. otherwise we set it to default 'segA' so we can leave it in for future use.
		
		if( location.pathname == '/buy/shopping_bag.do' || location.pathname == '/buy/shopping_bag_delete_item.do' || location.pathname == 'buy/shopping_bag_save_item.do' || location.pathname == '/buy/edit_shipping_method_bag.do' ) {
			disruptorManager.responsiveBagAbTestOn = true;
		}
		else {
			disruptorManager.responsiveBagAbTestOn = false;
		}
		

		disruptorManager.segmentSuffix = typeof seg !== "undefined" ? "_" + seg : "_segA";

		/*********** OVERRIDE TEMPLATE TO FIX SPACING BELOW PROMO **********/
		disruptorManager.mainContent = jQuery("#mainContentContainer").length > 0 ? jQuery("#mainContentContainer") : jQuery("#mainContent");
		disruptorManager.mainContent.css({"margin":"0px auto 0px auto"});
		jQuery("#universalTopNavLayout").css({"margin":"0px 0px 0px 0px","height":"140px"});
		jQuery("#contentItemContainer10000328").css("float","left");
		/********** END OVERRIDES **********/

		//May need to adjust this minHeight if promo headline is taller than 46px
		jQuery("#topNav").css({"minHeight":"110px"});

		//set max height on promo container (disruptor height + headline's top margin)
		var promoContainerMaxHeight = jQuery("#headline_popup" + disruptorManager.segmentSuffix).height() + parseInt(jQuery("#headline" + disruptorManager.segmentSuffix).css("marginTop").replace("px",""));
		jQuery("#GID_MarketingContainer_10000394").css("max-height",promoContainerMaxHeight);

		jQuery("#headline" + disruptorManager.segmentSuffix).show();
		if( ! disruptorManager.responsiveBagAbTestOn ) {
			disruptorManager.initializeDisruptor();
		}
	},
	showPopup : function(){
		//fix IE7 #topNav height :(  - have to check height of popup before it animates
    	if( ! disruptorManager.responsiveBagAbTestOn ) {
    		if(clientBrowser.isIE7){
    			var newHeight = jQuery("#topNav").height() + (jQuery("#headline_popup_segA").height() - jQuery("#headline_segA").height());
    			jQuery("#topNav").height( newHeight );
    		}

			jQuery("#headline_popup" + disruptorManager.segmentSuffix).slideDown();
			jQuery("#headline" + disruptorManager.segmentSuffix).css("visibility","hidden");
        
			disruptorManager.mainContent.css({"margin":"10px auto 0px auto"});

			disruptorManager.closePopupTimerID = setTimeout(function(){
				disruptorManager.closePopup(true);
			},disruptorManager.DISRUPT_TIME);
			return false;
		}
		else {
			disruptorManager.showPromoLegalPopup();
			}

	},
	closePopup : function(closeImmediately){
		
		if(closeImmediately === true){
			jQuery('#headline_popup' + disruptorManager.segmentSuffix).css('display','none');
			jQuery("#headline" + disruptorManager.segmentSuffix).css("visibility","visible");
			//undo IE7 height fix
	        if(clientBrowser.isIE7){jQuery("#topNav").height("");}
		}else{
			//delay the close
			disruptorManager.closePopupTimerID = setTimeout(function(){
				jQuery("#headline_popup" + disruptorManager.segmentSuffix).slideUp(400,function(){				
					//undo IE7 height fix
		        	if(clientBrowser.isIE7){jQuery("#topNav").height("");}
				});
				jQuery("#headline" + disruptorManager.segmentSuffix).css("visibility","visible");
			},1000);		
		}

		disruptorManager.mainContent.css({"margin":"0px auto 0px auto"});

		return false;
	},
	initializeDisruptor : function(){
		var hasSeenDisruptor = gidLib.getCookieVar("mktUniversalPersist",disruptorManager.COOKIE_KEY);
		if(hasSeenDisruptor != disruptorManager.CIID){
			disruptorManager.showPopup();
			wcdBrandDisrupterDidFire = true;
			gidLib.setCookieVar("mktUniversalPersist",disruptorManager.COOKIE_KEY, disruptorManager.CIID);
			var temp = setTimeout(function(){
				disruptorManager.closePopup(true);
			},disruptorManager.DISRUPT_TIME);
		}		
	},
	stayOpen : function(){
		clearTimeout(disruptorManager.closePopupTimerID);
		jQuery('#headline_popup' + disruptorManager.segmentSuffix).css("display", "block");
	},
	showPromoLegalPopup : function(){
		disruptorManager.closePopup(true);
		wcdLib.trackValue(this,'promo_legal','promo_legal','eVar7');

		if( location.pathname.indexOf('buy') < 0 && location.pathname.indexOf('checkout') < 0)  {
		//jbox.boxIt({width: 460, height: 330, url: '/buy/promo_legal_details.do?promoId=' + disruptorManager.PPC_ID });
		jbox.boxIt({width: 480, height: 330, url: '/Asset_Archive/ONWeb/content/0009/395/386/assets/popup_legal_static.html'});
		}
		else {
		//jbox.boxIt({width: 320, height: 330, url: '/buy/promo_legal_details.do?promoId=' + disruptorManager.PPC_ID });
		jbox.boxIt({width: 330, height: 330, url: '/Asset_Archive/ONWeb/content/0009/395/386/assets/popup_legal_static.html?mobile=true'});
		}
		/*
		//Do not use below code w/ static popup (for use w/ promo_legal_details.do)
		jQuery('div#jbox iframe').load(function() { 
			var popupBody = jQuery('div#jbox iframe').contents().find('div#mainContent');
			var popupHtml = jQuery('div#jbox iframe').contents().find('html');
			popupBody.attr('style','padding:10px');
			popupHtml.attr('style','overflow-x:hidden;');
		});*/
		return false;
	},
	trackExternalLink : function(trackingId, url) {
		wcdLib.trackValue(this,trackingId,trackingId,'eVar7');
		window.location = url;
	}
};

function setPPCCookieValue(cookieName,cookieKey) {
	if(location.protocol == "http:"){
		var volControlSegment = gidLib.getCookieVar(cookieName, cookieKey);
		var ppcKey = cookieKey + "_PPC";
		var ppcValues = {
			"segA" : "0411volcontrolA",
			"segB" : "0411volcontrolB"
		};
		gidLib.setCookieVar("mktUniversalPersist", ppcKey, ppcValues[volControlSegment]);
	}
}

jQuery(document).ready(function(){
	//SUPPRESS FROM HP
	// var loc = location.pathname;
	// if(loc != "/browse/home.do" && loc != "/" && loc != "/browse/division.do"){

		// var userSegment = myPromo_VC_ABLib.model.userSegment;
		// userSegment = userSegment == "" ? "segA" : userSegment;
		userSegment = "segA";
		disruptorManager.init(userSegment);

		// setPPCCookieValue("mktUniversalPersist",myPromo_VC_ABLib.model.cookieVariableName);
	//}
});


</script>


