<!-- Begin subCategory selected fix Override43-->
<script type="text/javascript">
document.observe("dom:loaded",function() {
    var cid = gidLib.getQuerystringParam("cid");
    var categoryNode = $('categoryNav'+cid);
    if (categoryNode) {
        var thisClass = categoryNode.className;
        var append = "";
        if (thisClass.include("Selected") == false) {
            append += "Selected";
            if (thisClass.include("subCat")) {
                append += " categoryActive";
            }
            categoryNode.className = thisClass + append;
        }
    }
});

</script>
<!-- Begin subCategory selected fix Override43-->



