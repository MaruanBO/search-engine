<script type="text/javascript">
	jQuery(document).ready( function() {
		var edfsLink = jQuery('.edfsPostageStamp a');
		if( edfsLink.length > 0 ) {
			edfsLink.click( function(e) {
				if( window["jbox"] != null ) {
					var edfsPopup = '/Asset_Archive/ONWeb/content/0009/187/706/assets/edfsLegal.html?v=1';
					var edfsPopupWidth = '475';
					var edfsPopupHeight = '450';
					if( jQuery(window).width() < 569 ) {
						edfsPopupWidth = '300';
						edfsPopupHeight = '350';
					}
					jbox.boxIt({ width: edfsPopupWidth, height: edfsPopupHeight, url: edfsPopup });
					jQuery('#jbox #iframeContainer').css('overflow','hidden');
				}
			});
		}
	});
</script>


