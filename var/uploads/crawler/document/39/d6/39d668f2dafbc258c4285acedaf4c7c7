<script type="text/javascript">

function openEDFSPopup(){

	if(location.pathname.indexOf('buy') < 0 && location.pathname.indexOf('checkout') < 0){
		gidLib.layeredPopup.openLayeredPopup({str:'/browse/info.do?cid=42021&mlink=5151,8980858,hp_free&clink=8980858',id:'FSpopup',width:471,height:477,style:{name:'universalPopup'}});
	}else{
		jbox.boxIt({width: 471, height: 477, url: '/buy/info.do?cid=42021&mlink=5151,8980858,hp_free&clink=8980858'});
	}
	
	return false;

}

jQuery(document).ready(function(){
	jQuery("#Region_1_Layout_10000515").prepend(jQuery("#edfsFooter-shoppingBag"));
});

</script>


