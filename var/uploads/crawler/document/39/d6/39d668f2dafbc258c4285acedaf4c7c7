	var privacyPolicyLink = $("privacyPolicyLink");
	if(privacyPolicyLink){
	    var htmlStr = privacyPolicyLink.innerHTML;
	    htmlStr = htmlStr.replace("/customerService/info.do?cid=","/customerService/info.do?cid=3318");
	    privacyPolicyLink.innerHTML = htmlStr;
	}	
});

