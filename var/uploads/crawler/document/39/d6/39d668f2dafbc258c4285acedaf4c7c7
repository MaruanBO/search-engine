<script type="text/javascript">

	/* checks for the "launchStoreLocator=parent|child" querystring parameters
	 * if present, load the storeLocator.do in a lightbox popup
	 * Separately handles parent and child iframe windows
	 * Also changes the layout store.do pages in the iframe (i.e. remove topnav etc.)
	 */
	 storeLocatorPopupManager = {

	 	model : {},
	 	
	 	controller : {
	 		
	 		init : function(){
	 			storeLocatorPopupManager.controller.launchStoreLocator();
	 		},

	 		launchStoreLocator : function(forcePopup){
	 			var launchStoreLocatorType = gidLib.getQuerystringParam('launchStoreLocator');

				if(launchStoreLocatorType == "parent" || forcePopup == true){
					location.hash = location.hash == "" || location.hash == "storeLocator"? "storelocator" : location.hash + "&storelocator";
					var winHeight = window.innerHeight < 600 ? window.innerHeight - 100 : 600;
					jbox.boxIt({width: 991, height: winHeight, url: "/customerService/storeLocator.do?kwid=1&sem=false&launchStoreLocator=child"});

					//close on escape key press
					jQuery(document).keyup(function(e) {
						if (e.keyCode == 27) { 
							jbox.close();
						}   
					});

				}else if(launchStoreLocatorType == "child"){
					
					//wait for jbox to load url
					parent.jQuery(parent.document).bind("jbox:loaded", function(e,url) {
						storeLocatorPopupManager.view.fixStoreLocatorChildLayout();
					});

					jQuery(document).ready(storeLocatorPopupManager.view.fixStoreLocatorChildLayout);
				}

				return false;
	 		}
	 	},

	 	view : {

	 		fixStoreLocatorChildLayout : function(){
	 			var temp = setTimeout(function(){
					//close on escape key press
					// jQuery(document).keyup(function(e) {
					// 	if (e.keyCode == 27) { 
					// 		window.parent.jbox.close();
					// 	}   
					// });

					//resize store locator elements
					jQuery("#listsConstainer").css({"height":"489px"});
					jQuery("#storeLocatorList").css({"height":"489px"});
					jQuery("#storeLocatorMap").css({"height":"489px"});

					//hide stuff
					jQuery("body").css("background-image","none");
					jQuery("#stamp").hide();
					jQuery("#universalBar").hide();
					jQuery("#topNav").hide();
					jQuery("#backgroundColorBar").hide();
					jQuery("#footer").hide();
					jQuery("#storeLocatorMainContent").css({"margin":"0px"});
					jQuery("#storeLocatorWrapper").css({'position':'absolute','top':'0px'});

					if(clientBrowser.isIE7){
						jQuery("#storeLocatorMap").css("width","660px");
					}

				},750);
	 		},

	 		fixStorePageLayout : function(){
	 			//hide top nav, footer, etc if store.do page is loaded in the lightbox
				if(typeof window.parent.jbox != "undefined"){
					if(window.parent.jbox.isOpen){
						jQuery("#universalBar").hide();
						jQuery("#topNav").hide();
						jQuery("#backgroundColorBar").hide();
						jQuery("#footer").hide();
						jQuery("#storeLocatorMainContent").css({"margin":"0px"});
						jQuery("#storeLocatorWrapper").css({'position':'absolute','top':'0px'});

						//fix storeLocator breadcrumb link to have our custom querystring
						var href = jQuery("#breadcrumbRoot a").attr("href");
						href = href == "storeLocator.do" ? href + "?launchStoreLocator=child" : href;
						jQuery("#breadcrumbRoot a").attr("href",href);
					}
				}
	 		}
	 	}
	};

	if((gidLib.getQuerystringParam('launchStoreLocator') == "parent" || gidLib.getQuerystringParam('launchStoreLocator') == "child") && location.hash.indexOf('storelocator') < 0){

		jQuery(document).ready(storeLocatorPopupManager.controller.init);

	}else if(location.pathname == "/customerService/store.do"){

		jQuery(document).ready(storeLocatorPopupManager.view.fixStorePageLayout);
	}

</script>


