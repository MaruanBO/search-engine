<script type="text/javascript">
  /** SET TRIGGERS HERE **/
jQuery(window).load(function(){
  //variable used for AB Tests
  var flyoutAction = "click";
  /////////////////////////////////////////////////////////
  //set width and height on trigger div and image
  jQuery(".flyout_trigger").each(function(index,element){
    jTarget = jQuery(element);
    jTargetImage = jTarget.children(":first");
    var w = jTargetImage.width();
    var h = jTargetImage.height();
    jTarget.css({"width":w,"height":h,"visibility":"visible"});
    jTargetImage.css({"width":w,"height":h});
  });
  /////////////////////////////////////////////////////////
  jQuery(".flyout_trigger").on(flyoutAction, function(){
    //jQuery(".carousel_hp_wrapper").trigger('pauseCarousel.jc');
    var jTrigger = jQuery(this);
    flyoutHeight = typeof jTrigger.data("flyoutheight")=="undefined" ? "275px" : jTrigger.data("flyoutheight");
    flyoutColor = typeof jTrigger.data("flyoutliacolor")=="undefined" ? "#006699" : jTrigger.data("flyoutliacolor"); 
    //flyout width keeps changing... so hardcode this width
    // var flyoutWidth = Math.floor(jTrigger.children(".flyout_links").outerWidth(true));
    
    jTrigger.find(".flyout_links a").css("color",flyoutColor);
    
    flyoutWidth = 300;
    var triggerWidth = jTrigger.width();
    if(flyoutWidth < triggerWidth){
      var marginLeft = (triggerWidth - flyoutWidth) / 2;
      jTrigger.children(".flyout_links").css({"margin":"0px 0px 6px " + marginLeft + "px"});
     
    }
    jTrigger.children(".flyout_links").stop(true).show();
    jTrigger.children(".flyout_links").stop(true).animate({
        height:flyoutHeight
    });
  });
    
  jQuery(".flyout_trigger, .flyout_links").mouseleave(function(evt){
    jTarget = jQuery(evt.currentTarget);
    jTarget.find( ".flyout_links" ).stop(true).delay(1000).animate({
      height:"0px"
    },250,function(){
      jTarget.find(".flyout_links").hide();
    });
  });

  // jQuery(".flyout_links").mouseover(function () {
  //   jQuery(".flyout_links").stop(true).animate({
  //       height: flyoutHeight
  //   });
  // });
});
/** END TRIGGERS **/

</script>




