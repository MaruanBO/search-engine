<script type="text/javascript" src="/Asset_Archive/ONWeb/content/0009/420/672/assets/jCarouselLite.js"></script>
<script type="text/javascript">

  var hpMinorBottomCarouselManager = {
    currentIndex : 0,
    numSlides : 0,
    init : function(){
      hpMinorBottomCarouselManager.numSlides = jQuery("#carousel_bottom li").length;
      hpMinorBottomCarouselManager.setIndicators();
      jQuery(".carousel_sneakPeak_wrapper").jCarouselLite({
        btnNext: ".carousel_sneakPeak_next",
        btnPrev: ".carousel_sneakPeak_previous",
        visible: 3,
        start  : 0,
        auto   : true,
        btnGo : [".hpMinorBottomCarouselGo_0",".hpMinorBottomCarouselGo_1",".hpMinorBottomCarouselGo_2",".hpMinorBottomCarouselGo_3",".hpMinorBottomCarouselGo_4",".hpMinorBottomCarouselGo_5",".hpMinorBottomCarouselGo_6",".hpMinorBottomCarouselGo_7",".hpMinorBottomCarouselGo_8",".hpMinorBottomCarouselGo_9",".hpMinorBottomCarouselGo_10"],
        beforeStart: function(a) {
        },
        afterEnd: function(a) {
          hpMinorBottomCarouselManager.currentIndex = parseInt(jQuery(a[0]).children("a").attr("class"));
          hpMinorBottomCarouselManager.setIndicators();
        }
      });
    },
    setIndicators : function(){
      var jIndicatorContainer = jQuery("#sneakPeakCarouselContainer #carousel_indicator_container ul");
      //check if indicators were insterted first
      if(jIndicatorContainer.children().length > 0){
        jIndicatorContainer.children('li').each(function(index,element){
          indicatorClass = index === hpMinorBottomCarouselManager.currentIndex ? "on" : "off"; 
          jQuery(element).attr('class',indicatorClass);
        });
      }else{
        for(var i = 0;i < hpMinorBottomCarouselManager.numSlides; i++){
          indicatorClass = i === hpMinorBottomCarouselManager.currentIndex ? "on" : "off"; 
          jIndicatorContainer.append("<li class='" + indicatorClass + "'><a onclick=\"jQuery('.carousel_sneakPeak_wrapper').trigger('pauseCarousel.jc');\" class='hpMinorBottomCarouselGo_" + i + "' href='#'></a></li>");
        }
      }
    }
  };

  function openJZVideo(u,trackId){
    wcdLib.trackValue(jQuery("#joeZee"),trackId,trackId,'eVar7');
    jbox.boxIt({width: 850, height: 478, url: u});
    return false;
  }

  if(location.search.indexOf('showvideo')>=0){
    jQuery(document).ready(openJZVideo);
  }

  jQuery(document).ready(hpMinorBottomCarouselManager.init);

</script>


