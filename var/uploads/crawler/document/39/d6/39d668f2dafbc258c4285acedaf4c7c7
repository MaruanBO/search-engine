		var gcT = $('categoryNav56017');
		if (gcT) {
			gcT.firstChild.href = "/browse/info.do?cid=35433";
		}
		/* END GIFT CARD OVERRIDES */ 
		  
		/* START fix long bargain sorts */
		var sideNavCategoryContainer = $('sideNavCategories');
		if(sideNavCategoryContainer){
			sideNavCategoryContainer.select("li[class=sale category]").each(function(categoryNode) {
				if (categoryNode && categoryNode.firstChild.href.indexOf('&pageID=0') < 0) {
					categoryNode.firstChild.href += "&pageID=0";
	     		}
			});
			  
			//fix long selected bargain sorts
			sideNavCategoryContainer.select("li[class=sale categorySelected categoryActive]").each(function(categoryNode) {
				if (categoryNode) {
					categoryNode.firstChild.href += "&pageID=0";
		  		}
			});
			  
			//fix long selected bargain sorts again
			sideNavCategoryContainer.select("li[class=sale category previewHighlighter3A]").each(function(categoryNode) {
				if (categoryNode) {
					categoryNode.firstChild.href += "&pageID=0";
			    }
			});
		}
		/* END fix long bargain sorts */
		
		//Bold left nav - "Shop Baby Girl..."
		var cat51468 = $('categoryNav51468');
		if (cat51468 != null) {
			if (cat51468.down().innerHTML.indexOf('Sale') < 0) {
				cat51468.style.fontWeight = 'bold';
			}
		}
		
		//Bold left nav - "Shop Baby Boy..."
		var cat51469 = $('categoryNav51469');
		if (cat51469 != null) {
			if (cat51469.down().innerHTML.indexOf('Sale') < 0) {
				cat51469.style.fontWeight = 'bold';
		    }
		}
		
		var cat51477 = $('categoryNav51477');
		if (cat51477 != null) {
			cat51477.style.fontWeight = 'bold';
		}
		
		var cat51474 = $('categoryNav51474');
		if (cat51474 != null) {
			if (cat51474.down().innerHTML.indexOf('Sale') < 0) {
				cat51474.style.fontWeight = 'bold';
		  	}
		}		
		
		
		/* Highlight KBM Sale Parent Category Links in Left Nav */
		if($('sideNavCategories')){
			var kbmIDs = [77880, 77886, 77883, 71869, 71881, 65364];
			for(var i=0; i<kbmIDs.length;i++){
				catNav = $('categoryNav' + kbmIDs[i]);
				if(catNav){
					catNav.style.fontWeight = "bold";
				}
			}
		}
		/* END Highlight KBM */
		
		
		/* Override Petite and Tall Fit Guide Links to open in layered popup */
		//have to use attr() because someone is modifying the sidenav after page load and i keep losing my on('click') event handlers :(
		jQuery("#categoryNav54213").children(":first").attr("onclick","return gidLib.layeredPopup.openLayeredPopup({str:'/browse/info.do?cid=64160&mlink=5151,6912202&clink=6912202',id:'tallFitGuide',width:730,height:748,title:'Tall Fit Guide'});");
		    
        jQuery("#categoryNav54211").children(":first").attr("onclick","return gidLib.layeredPopup.openLayeredPopup({str:'/browse/info.do?cid=64159&mlink=5151,6912202&clink=6912202',id:'petiteFitGuide',width:730,height:748,title:'Petite Fit Guide'});");
		/* END Override Petite and Tall Fit Guide Links to open in layered popup */
		
		/* Override Mens Big and Tall Fit Guide Links to open in layered popup */
		jQuery("#categoryNav85362 a").attr({
	 		"href":"#fitGuide",
	 		"onclick":"return gidLib.layeredPopup.openLayeredPopup({str:'/browse/info.do?cid=64693&mlink=5151,6140611&clink=6140611',id:'SizeGuide',width:730,height:748,title:'Big and Tall Fit Guide'})"
		});
		/* END Override Mens Big and Tall Fit Guide Links to open in layered popup */
		
		/* Clearance  - LeftNav OVERRIDES */
		if($('sideNavCategories')){
			var ClearIDs = [36302,36276,36307,36309,53679,53699]; //enter numbers of the LI IDs
			for(var i=0; i<ClearIDs.length;i++){
				catNav = $('categoryNav' + ClearIDs[i]);
				if(catNav && catNav.className.indexOf("Selected") < 0 ){
					catNav.down().style.color = "#CC0000";
				}
			}
		}
		/* END -- Clearance - LeftNav OVERRIDES */


		/* OVERRIDE Link for "Can't Make A UPS Return" - Added 8/13/13
		 * This appears in the top right corner after youâve entered in the information for a label at the following URL:
		 * https://secure-oldnavy.gap.com/profile/returns_address_entry.do 
		 */
		if(location.pathname == '/profile/returns_label.do'){
			jQuery('#cantUPSReturn').children(':first').attr('href','/customerService/info.do?cid=82724&cs=items_bought_online');
		}


		/////////////////////////////////////////////////
		// Store Locator Outlet Preselector
		if( location.pathname == '/customerService/storeLocator.do' ) {
			
			var wcdStoreLocatorUpdater = {
				preselectOutlet : function() {
					if( $('filter_wps') ) {
						$('filter_wps').checked = true;
					}
				},
				presetOutletCookie : function() {
					if( storeLocatorManager.controller.setBrandCookieVar ) {
						storeLocatorManager.controller.setBrandCookieVar(storeLocatorManager.constants.cookies.FILTERS, "Outlet");
					}
				}
			}

			// Observe building of tools/filter area of map
			Event.observe(document, 'filterTools:ready', function() {
				var isOutletPreselect = gidLib.getQuerystringParam('showOutlets');
				if( isOutletPreselect == 'true' ) {
					wcdStoreLocatorUpdater.preselectOutlet();
				}
			});

			if( storeLocatorManager && storeLocatorManager.controller && storeLocatorManager.controller.eventHandlers && storeLocatorManager.controller.eventHandlers.personalizationDataReadyHandler ) {
				storeLocatorManager.controller.eventHandlers.personalizationDataReadyHandler = storeLocatorManager.controller.eventHandlers.personalizationDataReadyHandler.wrap( function( om ) {
					var isOutletPreselect = gidLib.getQuerystringParam('showOutlets');
					if( isOutletPreselect == 'true' ) {
						wcdStoreLocatorUpdater.presetOutletCookie();
					}
					om();
				});
			}

		}
		//END Store Locator Outlet Preselector
		/////////////////////////////////////////////////

		

		/* VI BANNER DEFAULT COLOR OVERRIDE */
		jQuery("#categoryNameHeader").css("background-color","#134881");
		/* END VI BANNER DEFAULT COLOR OVERRIDE */

		/* WOMEN'S PLUS LEFT NAV OVERRIDE */
		jQuery("#categoryNav1016054 a").attr("href","/browse/division.do?cid=5585");
		/* WOMEN'S PLUS LEFT NAV OVERRIDE */

		
		
	}
};
jQuery(document).ready(wcdSiteOverrides.init);

