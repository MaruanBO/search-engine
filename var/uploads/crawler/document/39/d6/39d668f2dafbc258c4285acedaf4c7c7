/* HIDE SUB CATS FROM PRODUCT GRID */
var thisCID = gidLib.getQuerystringParam("cid");
if(thisCID == "71881" || thisCID == "71869" || thisCID == "65364" || thisCID == "65365" || thisCID == "65366" || thisCID == "77886" || thisCID == "77883"){
	Event.observe(document, 'categoryProductGrid:ready', function(){
		categoryGrid = $('categoryProductGrid');
		if(categoryGrid){
			
			//hide subcat headers - G & B
			var headerPrefix = "ShopBySize:";
			var headerPrefix2 = "PlusStyles(Hidden)"
			$('categoryProductGrid').select("h2[class=header4]").each(function(thisHeader){
				htmlHeader = wcdLib.getCleanString(thisHeader.innerHTML);
				if(htmlHeader.indexOf(headerPrefix) >= 0 || htmlHeader.indexOf(headerPrefix2) >= 0){
					thisHeader.style.display = "none";
					//now remove the next divider
					var nextEl = thisHeader.next();
					if(nextEl.className == "divider1"){
						nextEl.remove();
					}
				}
			});
			
			//hide subcat headers - TG & TB
			var headerPrefix = "BabySale:Upto";
			$('categoryProductGrid').select("h2[class=header4]").each(function(thisHeader){
				htmlHeader = wcdLib.getCleanString(thisHeader.innerHTML);
				if(htmlHeader.indexOf(headerPrefix) >= 0){
					thisHeader.style.display = "none";
					//now remove the next divider
					var nextEl = thisHeader.next();
					if(nextEl.className == "divider1"){
						nextEl.remove();
					}
				}
			});
			
			//hide subcat headers - Baby
			var headerPrefix2 = "BabySale:ShopbySize";
			$('categoryProductGrid').select("h2[class=header4]").each(function(thisHeader){
				htmlHeader = wcdLib.getCleanString(thisHeader.innerHTML);
				if(htmlHeader.indexOf(headerPrefix2) >= 0){
					thisHeader.style.display = "none";
					//now remove the next divider
					var nextEl = thisHeader.next();
					if(nextEl.className == "divider1"){
						nextEl.remove();
					}
				}
			});
			
			//hide these subcats' products
			var subcats = [78739,78735,78716,78732,78731,78729,78728,78727,78725,78723,78722,78721,78717,78475,78412,78413,78414,78416,78417,78418,78419,78420,78430,78112,78113,78114,78115,78116,78117,78118,78119,78261,78262,78263,78264,78265,78266,78267,78238,78240,78242,78244,78269,78271,78275,78512,78523,78522,78515,78739,78735,78716,78732,78731,78729,78728,78727,78725,78723,78722,78721,78717,78739,78735,78716,78732,78731,78729,78728,78727,78725,78723,78722,78721,78717];
			$('categoryProductGrid').select("div[class=brand3 productCatItem]").each(function(thisProductContainer){
				thisID = thisProductContainer.id;
			    //cast as number to compare to items in the array
			    thisCID = parseInt(thisID.substr(thisID.lastIndexOf("_")+1,5));
			    if(subcats.indexOf(thisCID) >= 0){
			    	thisProductContainer.style.display = "none";
			    }
			});
		}
	});
}


