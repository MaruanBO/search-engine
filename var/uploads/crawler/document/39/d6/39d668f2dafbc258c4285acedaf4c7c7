	var mainContent = $('mainContent');
	if (mainContent && mainContent.select('.lineItemDisplay')) {
		var liDisplay = mainContent.select('.lineItemDisplay')[0];
		if (liDisplay && liDisplay.childElements()) {
			liDisplay.childElements().each(function(lineItem) {
				var badge = lineItem.select('img')[0];
				if (badge && badge.alt.include("oldnavy")) {
					var invMsg = lineItem.select('.inventoryMessage4')[0];
					if (invMsg) invMsg.hide();
					var invWindow = $('inventoryStatusWindow');
					if (invWindow) invWindow.hide();
				}
			})
		}
	}
}
