<script type="text/javascript">
//<![CDATA[
function validate()
{
    // validate the user input on client side using Regular Expressions
    var pattern = /^[a-zA-Z]?[a-zA-Z'\-\s\.]{0,28}[a-zA-Z]$/;
    var regex = new RegExp(pattern);
    var  el = document.getElementById('FirstNameTextBox').value;
    el = $.trim(el);
    
    if(!regex.test(el))
    {
        alert('Invalid first name.');
        return false;
    }

    el = document.getElementById('LastNameTextBox').value;
    el = $.trim(el);
    var pattern = /^[a-zA-Z]?[a-zA-Z'\-\s\.]{0,28}[a-zA-Z]$/;
    var regex = new RegExp(pattern);
    if(!regex.test(el))
    {
        alert('Invalid last name.');
        return false;
    }
    
    var emailRegEx = /^[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?[a-zA-Z]$/;
    el = document.getElementById('EmailTextBox').value;
    el = $.trim(el);
    if(!el.match(emailRegEx))
    {
        alert('Invalid email.');
        return false;
    }    
    regex = new RegExp(/^[0-9]{5}$/);
    el = $.trim(document.getElementById('ZipTextBox').value);
    if(!el.match(regex))
    {
        alert('Invalid zip.');
        return false;
    }
    return true;
}
//]]>
</script>

