      <script type="text/javascript">
         jQuery.noConflict();
         
         
         
         
         jQuery(function() {
            
            jQuery('body').scrollspy({ target: '#primaryNavigation', offset: 160});
            
            preload([
                '/images/bg1.jpg',
                '/images/bg2.jpg',
                '/images/bg3.jpg',
                '/images/bg4.jpg'
            ]);
            
            jQuery('#primaryNavigation a').click(function(){
                autoScroll(jQuery(this).attr("href"));
            });
            
            jQuery('.carousel').carousel({
                interval: 10000
               }).on('slide', function (e) {
                  var xx = jQuery(this);
                  var img = xx.find('.active').attr("alt");
                   
                  // Change Background Image for Slide
                  jQuery(".pitchbackground img").fadeOut(300).delay(200).fadeIn(300);
               
            });
            
            jQuery('.carousel').swipe( {
                swipeLeft: function() {
                    jQuery(this).carousel('next');
                },
                swipeRight: function() {
                    jQuery(this).carousel('prev');
                },
                allowPageScroll: 'vertical'
            });
         });
         
         function autoScroll(anch){
            // Check for mobile / desktop offset
            var offset = 160;
            if (!jQuery(".navdesktop").is(":visible")){
               offset = 0;
            }
            jQuery('html, body').animate({
                 scrollTop: jQuery(anch).offset().top - offset
                  
             }, 500);
             if(anch != "#top"){
                  jQuery(".goTopContainer .goTop").fadeIn(300);
               }else
               {
                  jQuery(".goTopContainer .goTop").fadeOut(300);
               }
             return false;
         }
         
         
         
         
         
          

   
         function preload(arrayOfImages) {
            jQuery(arrayOfImages).each(function(){
                 jQuery('<img/>')[0].src = this;
             });
         }
  
           
      </script>
      
