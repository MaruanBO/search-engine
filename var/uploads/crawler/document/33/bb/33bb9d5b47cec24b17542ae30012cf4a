<script type="text/javascript">

	function loadHeaderCartItemCount() {
		$.ajax({
			type: "POST",
			url: '/includes/ajax/header_cart_item_count.jsp',
			data: { time: (new Date().getTime()) },
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
					$(".js-headerItemCount").html(data);
				}
			}
		});
	}
	function loadHeaderWishlistItemCount() {
        $.ajax({
                type: "POST",
                url: '/includes/ajax/header_wishlist_item_count.jsp',
                data: { time: (new Date().getTime()) },
                dataType: "html",
                success: function(data) {
                        if(data != null && data != "") {
                                $(".js-wishlistItemCount").html(data);
                        }
                }
        });
    }
	function openWindowEmailRegistry(){
	   	 site.func.openModalLayer({
			url: "/registry/email_registry.jsp", 
			modalId: "contextChooserModal", 
			styleClass: "changecountry"
	     }); 
	     //var win = window.open("/user/email_registry.jsp",'','width=500,height=410,left=380px,top=50px');
	    }
	function loadHeaderUserMessage() {
		$.ajax({
			type: "POST",
			url: '/includes/ajax/header_user_message.jsp',
			data: { time: (new Date().getTime()) },
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
				    $(".accountutilswrap ul").append(data);
				}
			}
		});
	}

	function loadHeaderUserShiptoCountry() {
		$.ajax({
			type: "POST",
			url: '/includes/ajax/header_user_country_currency.jsp',
			data: { time: (new Date().getTime()) },
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
					$(".js-headerUserCountryCurrency").html(data);
				}
			}
		});
	}
	
	$(function() {
		loadHeaderCartItemCount();
		loadHeaderWishlistItemCount();
		loadHeaderUserMessage();
		
		
			loadHeaderUserShiptoCountry();
		
		
	});
    function openWindowChangeCountry(){
		site.func.openModalLayer({
			url: "/user/context_chooser_body.jsp?baseUrl=/" , 
			modalId: "contextChooserModal", 
			styleClass: "changecountry"
		});
    }

    $(document).ready(function() {
        $('a.ShopButton').click(function() {
          if($(this).parents('.rightContent').siblings('nav.siteNav').hasClass('open')){
            $(this).parents('.rightContent').siblings('nav.siteNav').removeClass('open');
          } else {
            $(this).parents('.rightContent').siblings('nav.siteNav').addClass('open');
          }
          return false;

        });
          $('a.ShopButton').click(function() {
          if($(this).parents('#mobileNav').siblings('.rightContent').hasClass('right')){
            $(this).parents('#mobileNav').siblings('.rightContent').removeClass('right');
          } else {
            $(this).parents('.rightContent').siblings('.rightContent').addClass('right');
          }
          return false;

        });

        $('a.close').click(function() {
          if($(this).parent('nav.siteNav').hasClass('open')){
            $(this).parent('nav.siteNav').removeClass('open');
          }
          return false;
        });
      
          $('a.close').click(function() {
          if($(this).parents('nav.siteNav').siblings('.rightContent').hasClass('right')){
            $(this).parents('nav.siteNav').siblings('.rightContent').removeClass('right');
          }
          return false;
        });
          
          $( ".js-searchKeyword" ).autocomplete({
              source: "/search/autocomplete.cmd",
              minLength: 2,
              select: function( event, ui ) {
                  this.value = ui.item.label;
                  $("#searchForm").submit();
              }
          });          
      });

</script>

