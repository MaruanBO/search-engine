   jQuery( document ).ready(function( $ ) {
       $( '.nav-level-1 > li a.dc-mega' ).on( 'click', function( e ) {
           var width = $( window ).width();
           if( width <= 760 && $( this ).parent( 'li' ).find( '.nav-level-2' ).is(':hidden')){
               e.preventDefault();
               $( this ).parent( 'li' ).find( '.nav-level-2' ).show();
           }else if (width > 760 && (/iPhone|iPod|iPad|Android|BlackBerry/).test(navigator.userAgent)){
               if(!$(this).hasClass("dc-menuopen")){
                   e.preventDefault();
                   $('.nav-level-1 > li .nav-level-2').hide(); 
                   $('.nav-level-1 > li a.dc-mega').removeClass("dc-menuopen");
                   $( this ).parent( 'li' ).find( '.nav-level-2' ).show();
                   $(this).addClass("dc-menuopen");
               }
           }
       });
   });
</script> 

