function InOut( elem )
{
elem.delay()
     .fadeIn(1000)
     .delay(4000)
     .fadeOut(
               function(){
                   if(elem.next().length > 0)
                   {InOut( elem.next() );}
                   else
                   {InOut( elem.siblings(':first'));}
                        
                 }
             );
}
 
$(function(){
$('#promo_fade li').hide();
InOut( $('#promo_fade li:first') );
$('#promo_fade').show();
