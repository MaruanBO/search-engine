		Y.one('#hot-topics-wrapper').plug(Y.NFL.RankedWidgetPlugin, {
			tmplName: 'hpHtTmpl',
			/*tmpl: Y.one('#tmpl-hot-topics').getHTML(),
			printCompiledTmpl: true,*/
