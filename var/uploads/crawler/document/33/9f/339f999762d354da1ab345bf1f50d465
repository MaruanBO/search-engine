      <script>
        nfl.Y.use('browser-panels',function(Y){
          // load background ad
          // browserPanel.adClickable: false
          Y.NFLAds.BrowserPanels.set('clickable',false);

          
          
                  Y.NFLAds.BrowserPanels.set('bgPosFromConsole','center top');
          

