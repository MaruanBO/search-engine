	// HIDING THE MAP/LOCATIONS FROM APPEARING ON SAKS DEAL PAGES	 
	var pathname = window.location.pathname;
     if(pathname.indexOf('saks') > -1){
        $('.sjGoogleMap').addClass('hide');
     } 	 
		 
	// SETTING ACTIVE STATE FOR SUBNAV	 
    var urlPath = [], hash;
    var q = document.URL.split('?')[1];
    if(q != undefined){
        q = q.split('&');
        for(var i = 0; i < q.length; i++){
            hash = q[i].split('=');
            urlPath.push(hash[1]);
            urlPath[hash[0]] = hash[1];
        }
	}
	var tagID = urlPath['tag'];
	  if(tagID == "19,20,9,22") {
	 	jQuery('.dealAll').addClass("active");
	  } else if (tagID == "19") {
	 	jQuery('.dealActivities').addClass("active");
	  } else if (tagID == "20") {
	 	jQuery('.dealFood').addClass("active");
	  } else if (tagID == "9") {
	 	jQuery('.dealHealth').addClass("active");
	  } else if (tagID == "22") {
	 	jQuery('.dealRetail').addClass("active");
	  } else if (tagID == "r-s") {
	 	jQuery('.rsDealsTab').addClass("active");
	  } else if (tagID == "For-Her") {
	 	jQuery('.dealHer').addClass("active");
	  } else if (tagID == "For-Him") {
	 	jQuery('.dealHim').addClass("active");
	  } else if (tagID == "For-Kids") {
	 	jQuery('.dealKids').addClass("active");
	  } else if (tagID == "For-Home") {
	 	jQuery('.dealHome').addClass("active");
	  } else if (tagID == "For-Techies") {
	 	jQuery('.dealTech').addClass("active");
	  } else if (tagID == "city") {
	 	jQuery('.dealCity').addClass("active");
	  } else if (tagID == "beach") {
	 	jQuery('.dealBeach').addClass("active");
	  } else if (tagID == "MTN") {
	 	jQuery('.dealMountains').addClass("active");
	  } else if($(".groupBuysListPage").length > 0) {
		jQuery('.dealAll').addClass("active");
	  }
	}	
);
</script>


<!-- Merchant Categories Menu v1.5 { -->
<script id="wlSelectedStateWrapperTemplate" type="wl/template">
	<div class="wlMerchantStates">
		<div class="wlSelectedState"> 
			<div class="selectDownArrowIcon">&gt;</div>
			<span class="wlDropDownStateName">
				<span class="wlSelectedStateTitle"></span>
			</span>	
		</div>
	</div>
	<div class="wlStatesMenuWrapper" id="wlStatesMenuContainer"></div>
</script>
<script id="sjCategoriesAdditionalInfoTpl" type="text/tpl">
	<div class="ui-helper-clearfix l-sjCatMenuAdditionalInfo sjCatMenuAdditionalInfo">
		<div class="sjCatMenuAdditionalInfoTitle">
			Don't see your city?
		</div>
		<a class="link sjCatMenuAddInfoLink" href="/local/deal/national/">
			View our national deals &raquo;
		</a>
	</div>	
</script>
<script id="sjStatesAdditionalInfoTpl" type="text/tpl">
	<div class="ui-helper-clearfix l-sjStatesMenuAdditionalInfo sjStatesMenuAdditionalInfo">
		<div class="sjStatesMenuAdditionalInfoTitle">
			Don't see your state?
		</div>
		<a class="link sjStatesMenuAddInfoLink" href="/local/deal/national/">
			View our national deals &raquo;
		</a>
	</div>	
</script>
<div id="categoryListBoxHolderWrapper"></div>

<script>
	var ExtendedMerchantCategoryMenu = new ExtendedMerchantCategoryMenuClass();
	function ExtendedMerchantCategoryMenuClass() {
		var options = {},
			merchantStatesWrapper;

		var self = this,
			categoryListBoxElem = {},
			categoryItemWrapperClass = "categoryItemWrapperV2",
			categoryItemWrapperId = "categoryItemWrapper";

		this.activeStateAbbrAlias = false;
		this.stateAbbreviationsObj = {};

		this.init = function(customOptions) {
			//detect SSL pages
			if(window.location.protocol == 'https:') {
				$("html").addClass("wlSSLSecurePage");
			}
			else {
				$("html").addClass("wlSSLInSecurePage");
			}
			
			options = $.extend({
				statesDropDownAppendToObj : $("#merchantStates"),
				selectedCategoryId : "25241",
				realignCategoryListBoxPosition : true,
				numberOfStatesPerColumn : 20
			}, customOptions || {});
			merchantStatesWrapper = options.statesDropDownAppendToObj,
			categoryListBoxElem = $("div.categoryListBoxV2");

			if(!merchantStatesWrapper.length) {
				return false;
			}

			jQuery.ajax({
				url : "/local/loadOtherCategories.action",
				dataType : "json",
				async : false,
				success : function(categoriesData) {
					var categories = categoriesData.categories;
					if(categories.length) {
						self.stateAbbreviationsObj = processAbbreviations(categories);

						//category items UNWRAP
						categoryListBoxElem.find(".categoryItemWrapperV2").unwrap();

						// clone categoryListBox HTML to SCRIPT element to reuse later (odd code as a workaround for IE)
						$("#categoryListBoxHolderWrapper").html("<scr"+"ipt id='categoryListBoxHolder' type='wl/template'>" + $.trim(categoryListBoxElem.html()) + "</scr"+"ipt>");
						
						addStatesDropDown(categories, self.stateAbbreviationsObj);
					}
				}
			});
		};

		this.updateCategoryListBox = function(stateAlias) {
			var html = $("#categoryListBoxHolder").html();

			//update category items box
			categoryListBoxElem.html(html).find(".categoryItemWrapperV2[data-stateabbr!='" + stateAlias + "']").remove();
			
			//update curent state value 
			merchantStatesWrapper.find(".wlSelectedStateTitle").text(self.stateAbbreviationsObj[stateAlias]);

			//update curent category value (take first from list only if current category is not visible)
			//var cityName = categoryListBoxElem.find("#categoryItemWrapper" + options.selectedCategoryId).find(".categoryNameV2").text() || categoryListBoxElem.find(".categoryItemWrapperV2").eq(0).find(".categoryNameV2").text();
			var cityName = categoryListBoxElem.find("#categoryItemWrapper" + options.selectedCategoryId).find(".categoryNameV2").text() || "- pick a city -";

			$("#categoryHeaderArea").find(".selectedCategoryTitle").text(cityName);
			
			// wrap categories
			wrapMerchantCategories();
				//MerchantCategoryMenu.wrapMerchantCategories(categoryListBoxElem.find(".categoryItemWrapperV2"), categoryListBoxElem, false);
			
			// append additional info to Cities drop down menu
			addAdditionalInfo2CitiesMenu();

			// rebind select category function
			bindCategoryLinks();
		};

		function processAbbreviations(categories) {
			var stateAbbreviationsObj = {};

			$.each(categories, function(i, val){
				if(val.stateAbbreviation) {
					var abbr = val.stateAbbreviation,
						abbrAlias = cleanUpAbbreviation(abbr);

					// add abbreviation data to items
					$("#" + categoryItemWrapperId + val.id)
						.attr("data-stateabbr", abbrAlias)
						.attr("data-categoryid", val.id);

					// fill states abbrs array
					if(!stateAbbreviationsObj.hasOwnProperty(abbrAlias)) {
						stateAbbreviationsObj[abbrAlias] = abbr;
					}
				}
			});

			return stateAbbreviationsObj;
		}

		function addStatesDropDown(categories, stateAbbreviationsObj) {
			var html = $("#wlSelectedStateWrapperTemplate").html(),
				ulHtml = '',
				stateArrObjSize = objSize(stateAbbreviationsObj),
				count = 1;
			
			// get HTML from template			
			merchantStatesWrapper.html(html);

			//sort array keys
			var keys = [],
				k, i, len;

			for (k in stateAbbreviationsObj) {
				if (stateAbbreviationsObj.hasOwnProperty(k)) {
					keys.push(k);
				}
			}

			keys.sort();
			len = keys.length;

			for (i = 0; i < len; i++) {
				key = keys[i]
				val = stateAbbreviationsObj[key];
				
				if(count % options.numberOfStatesPerColumn == 1) {
					ulHtml += "<ul>";
				}

				ulHtml += "<li data-menustateabbr='" + key + "'>" + val + "</li>";

				if(count % options.numberOfStatesPerColumn == 0 || count == stateArrObjSize) {
					ulHtml += "</ul>";
				}

				count++;
			}

			// fill UL list
			// $.each(stateAbbreviationsObj, function(key, val) {
			// });

			$("#wlStatesMenuContainer").html("" + ulHtml + "");

			// calculate menu offset
			calculateMenuOffset();

			self.activeStateAbbrAlias = getSelectedStateAbbr(categories);
			if(self.activeStateAbbrAlias) {
				self.updateCategoryListBox(self.activeStateAbbrAlias);
				// append additional info to states drop down
				addAdditionalInfo2StatesMenu();
			}

			bindStateLinks();
		}

		function calculateMenuOffset() {
			var wlSelectedState = merchantStatesWrapper.find(".wlSelectedState"),
				wlStatesMenuContainer = $("#wlStatesMenuContainer"),
				top = wlSelectedState.offset().top,
				left = wlSelectedState.offset().left,
				height = wlSelectedState.outerHeight(true),
				width = wlSelectedState.outerWidth(true),
				menuWidth = wlStatesMenuContainer.outerWidth(true),
				windowWidth = $(window).width();

				wlStatesMenuContainer.css("top", "140px");

				if(menuWidth + left > windowWidth) {
					wlStatesMenuContainer.css("right", 5);
					wlStatesMenuContainer.css("left", "auto");
				}
				else {
					wlStatesMenuContainer.css("left", "auto");
				}		
		}
		
		function getSelectedStateAbbr(categories) {
			var abbr, abbrAlias;

			$.each(categories, function(i, val){
				if(val.id == options.selectedCategoryId && val.stateAbbreviation) {
					abbr = $("#" + categoryItemWrapperId + val.id).data("stateabbr"),
					abbrAlias = cleanUpAbbreviation(abbr);

					merchantStatesWrapper.find(".wlSelectedStateTitle")
						.text(abbr)
						.attr("data-selectedstate", abbrAlias);

					return false;
				}
			});

			return abbrAlias;
		}

		function bindStateLinks() {
			var wlStatesMenuContainer = $("#wlStatesMenuContainer");

			merchantStatesWrapper.find(".wlSelectedState").click(function() {
				calculateMenuOffset();				
				wlStatesMenuContainer.toggle();
			});
			wlStatesMenuContainer.on("click", "li", function() {
				self.updateCategoryListBox($(this).data("menustateabbr"));
				wlStatesMenuContainer.toggle();
			});

			//hide on outter click
			$("body").on("click.wlStatesMenuContainerToggle", function(e){
				if(wlStatesMenuContainer.is(":visible") && !$(e.target).parents(".wlMerchantStates").length) {
					wlStatesMenuContainer.hide();
				}
			});
		}

		function wrapMerchantCategories(categoryListBoxElem) {
			var merchantCategoriesDropDownBox = $("#merchantCategoriesDropDownBox"),
				categoryListBoxV2 = merchantCategoriesDropDownBox.find(".categoryListBoxV2"),
				categoryItemWrapperV2 = merchantCategoriesDropDownBox.find(".categoryItemWrapperV2"),
				itemsPerCol = Math.ceil(categoryItemWrapperV2.length / 4),
				i=0,
				catItemWidth = categoryItemWrapperV2.eq(0).outerWidth(true);
			
			if(!categoryItemWrapperV2.parent().hasClass("categoryListBoxV2")) {
				categoryItemWrapperV2.unwrap();
			}

			while(i<=categoryItemWrapperV2.length) {
				categoryItemWrapperV2.slice(i, itemsPerCol + i).wrapAll($("<div />", {
					"css" : {
						"width" : "155px"
					},
					"class" : "merchantCategoriesCol"
				}));
				
				i = i + itemsPerCol;
			}

			realignCategoryListBox();

			return;
		}
		
		/* Additional Info box for Cities drop down menu. Maxim - 10/17/2012 refs #24811 */
		function addAdditionalInfo2CitiesMenu(){
			var additionalInfoHTML = $("#sjCategoriesAdditionalInfoTpl").html();
			// append additinal info box to cities drop down menu			
			categoryListBoxElem.append(additionalInfoHTML);
		};
		
		/* Additional Info box for States drop down menu. Maxim - 10/17/2012 refs #24811 */
		function addAdditionalInfo2StatesMenu(){
			var additionalInfoHTML = $("#sjStatesAdditionalInfoTpl").html();
			// append additinal info box to states drop down menu			
			$("#wlStatesMenuContainer").append(additionalInfoHTML);
		};

		/* Redirect ot RDP on Category click. Andreyk - 11/21/2012 refs #25595 */
		function sjSelectCategory(offerCategoryId, MICROSITE_URL_PREFIX) {
			$.get(MICROSITE_URL_PREFIX + "/selectCategory.action", {offerCategoryId: offerCategoryId, now: (new Date()).getTime()}, function(data) {
				var catName = data;
				if(CommonVariables.isDealStore || CommonVariables.isRedemptionStore) {
					var endPoint = "/store";
					if (CommonVariables.isRedemptionStore) {
						endPoint = "/membershipstore";
					}
					var url = MICROSITE_URL_PREFIX + "/deal/" + catName + endPoint; 
					if (null != additionalParams && (null != additionalParams.tag && "" != additionalParams.tag) ) {
						url += "?tag=" + additionalParams.tag;
					}
					window.location.href = url;
					return true;
				}
				console.log(MICROSITE_URL_PREFIX + "/deal/" + catName);
				window.location.href = MICROSITE_URL_PREFIX + "/deal/" + catName; // redirecting to RDP
				return true;
			}, "html");						
		}

		function bindCategoryLinks() {
			$(".categoryItemV2").removeAttr("onclick");
			$(".categoryItemWrapperV2").click(function() {
				sjSelectCategory($(this).data("categoryid"), "/local");
				return false;
			});
		}

		function realignCategoryListBox() {
			var	selectedCategory = $("#categoryHeaderArea").find(".selectedCategory"),
				selectedCategoryOffsetRight = $(window).width() - ( selectedCategory.offset().left + selectedCategory.outerWidth() );

			if(!options.realignCategoryListBoxPosition) {
				return false;
			}
			$("#merchantCategoriesDropDownBox").css("left", "155px");

		}


		/* Helpers */
		function cleanUpAbbreviation(abbr) {
			return abbr.replace(/[^a-z]+/gi, '').toLowerCase();
		}

		function objSize(obj) {
			var size = 0, key;
			for (key in obj) {
				if (obj.hasOwnProperty(key)) size++;
			}
			return size;
		}
	}

	// run on doc ready
	$(function(){
		// checkHeaderMenu(20);
	});

	//wait for header categories menu to load
	function checkHeaderMenu(tryLimit) {
		if(--tryLimit < 0) {
			console.warn("checkHeaderMenu limit tries");
			return false;
		}

		if(!$("#categoryHeaderArea").find(".selectedCategory").length) {
		  setTimeout(function(){
			checkHeaderMenu(tryLimit);
		  }, 750);
		}
		else {
			// init menu
			ExtendedMerchantCategoryMenu.init({
				realignCategoryListBoxPosition:true,
				numberOfStatesPerColumn:10
			});
		}
	}
	
</script>
<!-- Merchant Categories Menu v1.5 } -->

