function dart_tracking(callback) {
     var img = new Image(); 
     img.src="http://www.googleadservices.com/pagead/conversion/1003769976/?value=0&label=qyiOCPjJmgMQ-KDR3gM&guid=ON&script=0";
    _gaq.push(['_trackEvent', 'Subscribe', 'GDA pulldown', '',decodeURIComponent("")]);
    if(typeof callback === 'undefined')  return;
    if(typeof callback === 'function') {
          setTimeout(function(){ callback()}, 1500);
    }
    if(typeof callback === 'string') {
          setTimeout(callback, 1500);
    }
}

