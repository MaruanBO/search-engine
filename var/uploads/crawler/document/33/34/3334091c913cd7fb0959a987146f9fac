<script type="text/javascript">
	jQuery(function() {
		sjUpdateEmailSettingsUrl();
		sjAddDealsNumber();

		if(jQuery(".detailsContent").size() > 0) {
			jQuery(".merchantDescTitle .highlight").html(jQuery(".ddpMerchantInfoAboutText").html());
			jQuery(".ddpOfferTitle, .ddp-nimble-network-sourcer-partner, .dealInnerContent").wrapAll('<div class="sjDdpMainInfoWrap"></div>');
			jQuery(".prodDescriptionTitle, .prodDescriptionText, .productAttrSection").wrapAll('<div class="sjDescription"></div>');
			jQuery(".merchantDescTitle, .ddpMerchantWebsiteUrl, .merchantDescText, .googleMapSection, .companyInfo").wrapAll('<div class="sjGoogleMap"></div>');
			jQuery("#ddpHighlightsBoxWrapper, #ddpFinePrintBoxWrapper").prependTo(".nimbleExtraInfoCol").wrapAll('<div class="sjInfoSection"></div>');
			jQuery(".gbReviews").appendTo(jQuery(".dealFullInfo"));
			jQuery("#dealTalkSection").appendTo(".dealFullInfo");
			jQuery(".productAttrSection").insertBefore("#ddpFinePrintInnerBox");
			sjMoveGiftLink();
			//travelZooHacks();
		}

		if(jQuery(".gbListPage").size() > 0) {
		    sjAddClickableTitle();
			sjAddCategoriesMenu();
		}

		if(jQuery(".sjNoDeals").size() > 0) {
			sjAddCategoriesMenu();
		}

		if(jQuery(".purchasesPageContent").size() > 0) {
			jQuery(".myPurchasesListTitle.purchaseCouponItem").html(jQuery(".myPurchasesListTitle.gbItemPhoto").text());
			jQuery(".myPurchasesListTitle.gbItemPhoto").html("");
		}
		
		if(jQuery(".buyPageContentV2").size() > 0) {
			jQuery('#buyFlowSubscriptionChkBok').attr('checked', true);
			jQuery(".addressZipField").attr("maxlength","5");
		}
		
		if(jQuery(".myEmailSettingsPage").size() > 0) {
			jQuery(".userSubscribedCategoriesBlock").text($(".userSubscribedCategoriesBlock").text().replace(/,/g, ', '));
		}
		
		if(jQuery(".sm-portal-content").size() > 0) {
			jQuery(".sm-contact-email").text("help@sweetjack.com");
			jQuery(".sm-contact-email").attr('href','mailto:help@sweetjack.com')
		}

		jQuery("body").click(function(e){
		    //hide menu
		    if ($("#headerMenu").height() > 0) $("#headerMenu").hide();
		    
		    //hide other menus
		    var clickedEl = $(e.target);    
			var sjMainCitiesDropList = $("#merchantCategoriesDropDownBox");    
			var sjHeaderDealCategoriesDropList = $(".sjHeaderMenuItem").find(".sjHeaderMenuItemDropBox");
			var sjRDPDealCategoriesDropList = $(".gbListPage").find(".sjHeaderMenuItemDropBox");
			var sjRDPRecentDealsButton = $(".gbListTitle");

			if(!sjMainCitiesDropList.hasClass("showHideDropdown") && !clickedEl.parents("#categoryHeaderArea").length) {
				sjMainCitiesDropList.addClass("showHideDropdown");
			}

			if(sjHeaderDealCategoriesDropList.is(":visible") && !clickedEl.hasClass("sjHeaderMenuItemLink")) {
				sjHeaderDealCategoriesDropList.hide();
			}

			if(sjRDPDealCategoriesDropList.is(":visible") && !clickedEl.hasClass("gbListTitle")) {
				sjRDPDealCategoriesDropList.hide();
				sjRDPRecentDealsButton.removeClass("sjGbListTitleHighlighted");
			}
		});
	});

function travelZooHacks(){
	var $tzWhen = $(".customTZWhen_js"),
		$sjInfoSection = $(".sjInfoSection"),
		$byLine = $(".customTZByLine_js"),
		$lead = $(".customTZLead_js"),
		$tzTerms = $(".customTZTnC_js"),
		$tzTips = $(".customTZTip_js"),
		$tzPhotoCredits = $(".customTZPhotoCredits_js"),
		$tzWhatsIn = $(".customTZWhatsIn_js"),
		tzPhotoCreditsHTML = "",
		tzTipHTML = "",
		tzWhenHTML = "",
		tzLeadHTML = "",
		tzByLineHTML = "",
		tzWhatsInHTML = "",
		isTz = false;
	if ($lead.length){
		isTz = true;
		tzLeadHTML = "<div class='tz-lead-box text tz-box'>" + $lead.html() + "</div>";
	}
	//photo credits box
	if ($tzPhotoCredits.length){
		isTz = true;
		tzPhotoCreditsHTML = "<span class='tz-photo-credits-box tz-credits-box'> | Photos: " 
			+ $tzPhotoCredits.html() + "</span>";
	}
	if ($byLine.length){
		isTz = true;
		tzByLineHTML = "<div class='tz-by-line-box tz-credits-box tz-box'>By " + $byLine.html() + tzPhotoCreditsHTML + "</div>";
	}
	// TZ when section
	if ($tzWhen.length){
		isTz = true;
		tzWhenHTML = "<div class='ui-helper-clearfix tz-when-wrapper tz-box'>" +
			"<h2 class='highlight tz-when-title'>When</h2>" + 
			"<div class='ui-helper-clearfix tz-when-content text'>" +
			$tzWhen.html() +
			"</div>" +
			"</div>";
	}
	
	if ($tzWhatsIn.length){
		isTz = true;
		tzWhatsInHTML = "<div class='ui-helper-clearfix tz-whats-in-wrap tz-box'>" + 
			"<h2 class='highlight tz-when-title'>What's included</h2>" + 
				"<div class='ui-helper-clearfix tz-whats-in-content text'><ul>" +
					$tzWhatsIn.html() +
			"</ul></div>" +
			"</div>";
	}
	
	// TZ Tip section
	if ($tzTips.length) {
		isTz = true;
		tzTipHTML = "<div class='ui-helper-clearfix tz-tips-wrap tz-box'>" + 
			"<h2 class='highlight tz-tip-title'>Travelzoo Tip:</h2>" + 
			"<div class='ui-helper-clearfix tz-tip-content text'>" +
				$tzTips.html() +
			"</div>" +
			"</div>";
	}
	// exit, in case no TZ data found
	if (!isTz) return false;
	$("#ddpHighlightstInnerBox h2").text("Why we love it");
	
	var resultHTML = tzByLineHTML + tzLeadHTML + tzWhenHTML + tzWhatsInHTML;
	
	$(".dealFullInfo").prepend(resultHTML);
	var $highlits = $("#ddpHighlightsBoxWrapper").addClass("tz-box").unwrap();
	if (tzWhenHTML != "") {
		$highlits.insertBefore(".tz-when-wrapper");
	} else {
		$highlits.insertBefore(".tz-whats-in-wrap");
	}
	var $finePrint = $("#ddpFinePrintBoxWrapper").addClass("tz-box");
	$finePrint.insertAfter(".tz-whats-in-wrap");
	$(tzTipHTML).insertAfter($finePrint);
	
	// hide product description section, as it's empty. We moved already all TZ section
	$(".productDescription .sjDescription:first").hide();

	return;
};
	
function sjMoveGiftLink() {
	if(jQuery("#savingsBlock").is(":visible") && jQuery(".giftTheDealBlock").size() > 0) { 
		var giftSection = jQuery(".giftTheDealBlock").hide();
		jQuery("#savingsBlock").html(giftSection.html());
		jQuery(".ddpGiftLinkSuffix").text("Gift This Deal!");
		jQuery(".giftTheDealBtn").html("<img src='https://productimages.nimbledeals.com/nimblebuy/57e27121237181f456c17cbe41eab49' class='sjGiftIcon' />");
		giftSection.addClass("sjGiftTheDealBlock");
		jQuery(".giveGiftLink").addClass("sjGiveGiftLink");
		jQuery(".giftTheDealBtn").addClass("sjGiftTheDealBtn");
	}
}

function sjAddDealsNumber() {
	jQuery('#categoryItemWrapper25241 .liveDealsCountV2').clone().appendTo('.sjMoreDealsCount');
	$('.sjMoreDealsCount .liveDealsCountV2 span').remove();
	$('.sjMoreDealsCount').html($.trim($('.sjMoreDealsCount .liveDealsCountV2').text()));	
}

function sjAddClickableTitle() {
	$('.gbItemTitle').click(function(){
		window.location.href =  $(this).parents('.gbItem').find('.moreDealsOfferItemLink').prop('href');
		return false;
	});
}

function sjAddCategoriesMenu() {
	jQuery(".sjHeaderMenuItemDropBox").clone().insertAfter("h2.gbListTitle");
	jQuery("h2.gbListTitle").bind('click', function() {
	var box = jQuery("#contentWrapper .sjHeaderMenuItemDropBox");
		if(box.css("display") == "none") box.show();
			else box.hide();
	jQuery("h2.gbListTitle").toggleClass("sjGbListTitleHighlighted");
	});
}
	
	function sjUpdateEmailSettingsUrl(){
		if(!window.sjGetUserEmail) return false;
		var customerEmail = sjGetUserEmail();
		var subscriberId = sjGetSubscriberID();
		var targetUrl = "http://preferences.sweetjack.com/?email=" +
						customerEmail + "&sid=" + subscriberId;
		var $target = $(".myEmailSettingsTab a, #headerMenu a[href$='/myEmailSettings']");
		$target.prop("href", targetUrl);
	};
</script>

<script>
	$.urlParam = function(string, parameterName){
	    var results = new RegExp('[\\?&]' + parameterName + '=([^&#]*)').exec(string);
	    if (results != null) {
	        return results[1] || 0;
	    } else return '';
	}

	jQuery(document).ready(function() {
		if ($(".gbListTitle").length > 0) {
			$('.sjHeaderMenuItemDropLink').each(function() {
				var categoryId = $.urlParam($(this).attr('href'), 'tag');
				var tagUrlValue = decodeURIComponent($.urlParam(window.location.href, 'tag'));
				if (categoryId == tagUrlValue) {    
					$(".gbListTitle").text($(this).text());
				}
    		});
		}
	});
</script>


