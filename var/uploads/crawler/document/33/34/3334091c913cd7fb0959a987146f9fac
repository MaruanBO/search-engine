<script type="text/javascript">

	$script.ready(NC.scriptsBundle.getKeysAsArray(), function(){
		
		initPagination(1, '', 18, 7, '', '', '', '', '' );
		$(".navigation-controls-page-label").removeClass("hide");
		
		shareOnTwitter();
		shareOnFB();
		showFilterBox();
		
		$(".elementsPerPage_js").change(function() {
			window.location = makeRequestURL(1,'',$(this).val(),'');
		});

		$(".dealSortBy_js").change(function() {
			window.location = makeRequestURL(1,'','18',$(this).val());
		});

		$(".layoutLink_js").click(function() {
			window.location = makeRequestURL('1',$(this).data("readablename"),'18','');
		});
		var rdpHighlightsExpander = function(){
			var highlightsWrapperEl = $(".rdHighlightsListWrap_js");
			var highlightsDefaultHeight = highlightsWrapperEl.height();
			var highlightsMoreLinkEl = $("#rdHighlightsMoreLink");
			var highlightsLessLinkEl = $("#rdHighlightsLessLink");
			if ($(".rdHighlightsList_js").height() > highlightsDefaultHeight){
				highlightsMoreLinkEl.show();
				highlightsMoreLinkEl.click(function(){
					highlightsWrapperEl.css("height", "auto");
					$(this).hide();
					highlightsLessLinkEl.show();
				});
				highlightsLessLinkEl.click(function(){
					highlightsWrapperEl.css("height", highlightsDefaultHeight + "px");
					$(this).hide();
					highlightsMoreLinkEl.show();
				});
			}
		};
		rdpHighlightsExpander();
	});
	
	function initPagination(page, searchString, elementsPerPage, listSize, filter, offerType, aditionalFiltering, tags, template) {
		var filterStr = "";
		
		if (template != ''){
			filterStr += "&t=" + template;
		}
		if(searchString != ''){
			filterStr+="&search=" + searchString;
		}
		filterStr += aditionalFiltering;
		listSize = parseInt(listSize);
		if(listSize > elementsPerPage){
			
			$.fn.imsPagination = function(options){
				options = $.extend({
					items_per_page: elementsPerPage,
					num_display_entries: 4,
					link_to: "http://" + window.location.host + window.location.pathname + "?p=__id__"+filterStr,
					current_page: page - 1,
					callback: function(p){}
				}, options||{});
				return $(this).pagination(listSize, options);
			}

			$("#pagination").imsPagination();
			
			$("#pagination a").each(function(){
				$(this).attr('rel', 'nofollow');
			});
		}
		//clone pagination for bottom navigation controls
		$(".navigationControls_js").last().html($(".navigationControls_js").first().html());
	}

	function makeRequestURL(page, template, elementsPerPage, sortBy) {
		var url = "http://" + window.location.host + window.location.pathname;
		var aditionalFiltering = '';
		url +=  "?p=" + page;
		if (template != "") {
			url += "&t=" + encodeURIComponent(template);
		}
		url += aditionalFiltering;
		if (elementsPerPage != "") {
			url = updateQueryStringParameter(url,'elementsPerPage',elementsPerPage);
		}
		if (sortBy != "") {
			url = updateQueryStringParameter(url,'sort',sortBy);
		}
		if('' != ''){
			url += this.appendURL(url, "search=" + encodeURIComponent(''));
		}
		return url;
	}

	function appendURL(url, param) {
		url += ((url.indexOf("?") > 0) ? "&" : "?") + param;
		return url;
	};

	function updateQueryStringParameter(uri, key, value) {
		var re = new RegExp("([?|&])" + key + "=.*?(&|$)", "i");
		separator = uri.indexOf('?') !== -1 ? "&" : "?";
		if (uri.match(re)) {
			return uri.replace(re, '$1' + key + "=" + value + '$2');
		}
		else {
			return uri + separator + key + "=" + value;
		}
	}

	function showFilterBox(){
		$("#filterBox").selectbox().bind('change', function(){
			document.location = $(this).val();
		});
		$(".greenSelect").css("visibility", "visible");

	}

	function shareOnEmail(subject, body){
		body = unescape(Utils.htmlDenc(body));
		body = escape(body);
		window.location.href="mailto:?Subject=" + subject + "&body=" + body;
	}

	$script.ready(NC.scriptsBundle.getKeysAsArray(), function(){
		$(".gbItem").each(function(){
			var that = $(this),
					timeLeftItem = that.find(".timeLeftDigitValue_js");

			if ((that.data("timerenabled") == true) && timeLeftItem.length) {
				var dealEpirationDate = that.find(".timeLeftDigitValue_js").data("enddate");

				$(timeLeftItem).countdown({
					endDate: dealEpirationDate,
					daysItem: ".rdpTimeDays_js",
					hoursItem: ".rdpTimeHours_js",
					minutesItem: ".rdpTimeMins_js",
					secondsItem: ".rdpTimeSecs_js",
					format: "on" // on (03:07:52) | off (3:7:52) - two_digits set to ON maintains layout consistency
				}, function(){});
			}
		});
	});
</script>















	
		
























