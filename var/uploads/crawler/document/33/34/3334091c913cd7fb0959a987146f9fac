<!-- BriteVerify -->
<script src="https://api.briteverify.com/users/danielcaplin/api.js?version=01"></script>
<script>
jQuery(function(){
	sjInitSplashBriteVerify();
	sjInitSignUpBriteVerify();
	sjInitGDABriteVerify();
});

function sjInitSignUpBriteVerify(){
	if(jQuery(".signUpPage").length){
		var redirect_url = location.href.split('?redirectUrl=')[1];
		if(redirect_url == "" || redirect_url == "undefined"){
			redirect_url = "";
		}
		var jsBriteVerifySignUp = new sjBriteVerifyClass({
			submitBtn: $("#signUpBtn"),
			emailField: $(".emailAuthField"),
			errorField: $(".emailError"),
			submitFunc: function(){
				Auth.signUpUser(document.forms["signUpForm"], false, redirect_url);
			}
		});
		jsBriteVerifySignUp.init();
	}	
};

function sjInitSplashBriteVerify(){
	if (!$(".sjSplashPage_js").length) return;
	
	var sjBriteVerifySplash = new sjBriteVerifyClass({
		submitBtn: $(".sjSplashSubmitBtn_js"),
		emailField: $("#regEmailAddress"),
		errorField: $("#miniRegEmailError"),
		submitFunc: function(){
			sjSplashSubscribeWithTracking();
		}
	});
	sjBriteVerifySplash.init();
};

function sjInitGDABriteVerify(){
	jQuery.aop.after({target: window, method: "subheaderSubscribeEvents"}, function(){
		var sjBriteVerifyGDA = new sjBriteVerifyClass({
			submitBtn: $("#submitDalyAlerts"),
			emailField: $("#sbEmail"),
			errorField: $(".sbEmailError"),
			submitFunc: function(){
				subscribeAlerts();
			}
		});
		sjBriteVerifyGDA.init();
	});
};
	
function sjBriteVerifyClass(opts){
	var _self = this;
	var isEmailValid = false;
	var ncEmailValidation = false;
	// Extract options
	var submitBtn = opts.submitBtn;
	var emailField = opts.emailField;
	var errorField = opts.errorField;
	var submitFunc = opts.submitFunc;

	this.init = function(){
		emailField.blur(function(e){
			var emailValue = getEmailValue();
			if (Utils.validateEmail(emailValue)){
				briteVerifyEmailValidation(emailValue, e);	
			} else {
				bindSubmitEvent();
			}
		}).focus(function(){
			submitBtn.off("click").prop("onclick", "");
			isEmailValid = false;
		});	
	};
	
	function getEmailValue(){
		return $.trim(emailField.val());
	};
	
	function bindSubmitEvent(){
		submitBtn.on("click", function(){
			submitFunc();
		});
	};
	
	function briteVerifyEmailValidation(emailValue, event){
		if (isEmailValid) return;
		$.briteVerify.email(emailValue, function(email){
			if(email.errors){
				msg = email.errors[0][1]; 
				errorField.html(msg);
				submitBtn.off("click").prop("onclick", "");
				isEmailValid = false;
			} else {
				errorField.empty();
				bindSubmitEvent();
				isEmailValid = true;
			}
		});
	};
};
</script>
<!-- BriteVerify END -->

<!-- States Categories Popup { -->
<script id="stateCategoryTemplate" type="text/x-handlebars-template">
	<div class="popupRectang"></div>
	<div class="stateCategoryWrapper ui-helper-clearfix{{#if isNational}} isNational{{/if}}">
		<div class="stateCategorySection">
			<div class="statesSection">
				<h2 class="stateCategoryTitle">Select a state:</h2>
				<ul class="ui-helper-clearfix statesList" id="statesContainer">
					{{#each states}}
					<li>
						<span class="stateLink{{#if selected}} selectedState{{/if}}" data-abbr="{{name}}">{{name}}</span>
					</li>
					{{/each}}
				</ul>
			</div>
			<div class="categoriesSection">
				<h2 class="stateCategoryTitle">Select a city:</h2>
				<div class="ui-helper-clearfix" id="categriesContainer">

				</div>
			</div>
		</div>
		<div class="stateCategoryNationalSection">
			<div class="stateCategoryNational">
				<span class="stateCategoryNationalPrefix">or</span>
				<span data-id="{{nationalId}}" data-readablename="{{nationalReadableName}}" id="viewNationalLink" class="viewNationalLink">View National Deals</span>
			</div>
		</div>
	</div>
</script>

<script id="citiesListTemplate" type="text/x-handlebars-template">
	{{#each this}}
	<ul class="ui-helper-clearfix categoriesList">
		{{#each cities}}
		<li>
			<span data-readablename="{{readableName}}" data-id="{{id}}" class="categoryLink">{{name}}</span>
		</li>
		{{/each}}
	</ul>
	{{/each}}
</script>

<script type="text/javascript">
function StateCategoryListClass(userOptions) {
	var options = $.extend({
			$placeholder 			: $("#stateCategoryPlaceholder"),
			$selectedCity 			: $(".selectedCity"),
			$selectedCityWrapper	: $(".selectedCityWrapper"),
			selectedCityId 			: 25241,
			minCitiesPerCol 		: 5,
			maxCols 				: 3,
			defaultState			: "CA"
		}, userOptions);

	var self = this;

	this.statesObj 			= {};
	this.statesArr			= [];
	this.nationalCityData 	= {};
	this.selectedState;

	this.init = function() {
		// load Handlebars.js
		$.getScript("//staticcontentprod.nimbledeals.com/js/handlebars-1.0.rc.1.min.js", function() {
			getOtherCategoriesList();
		});
	};


	function getOtherCategoriesList() {
		jQuery.ajax({
			url : MICROSITE_URL_PREFIX + "/loadOtherCategories.action",
			dataType : "json",
			success : function(data) {
				var cities = data.categories.sort(sortByName),
					templateData;

				formStateObj(cities);

				createStateCategoryPopup();
				if(!self.nationalCityData.isNational) {
					if (!self.selectedState) self.selectedState = options.defaultState;
					updateCategoriesList(self.statesObj[self.selectedState]);
				}
				bindings();
				return;				
			}
		});
	}

	function formStateObj(cities) {
		var statesArray = [];
		if(!cities.length) {
			return false;
		}

		$.each(cities, function(i, cityObj) {
			self.statesObj 	= addToStatesObj(cityObj, self.statesObj);
			self.statesArr 	= addToStatesArr(cityObj, self.statesArr);
			if(!self.selectedState) {
				self.selectedState 	= getSelectedState(cityObj);
			}
			if(jQuery.isEmptyObject(self.nationalCityData)) {
				self.nationalCityData = getNationalCity(cityObj);
			}
		});
		return;
	}

	function addToStatesArr(cityObj, statesArr) {
		if(cityObj.stateAbbreviation) {
			var hasState = false;
			$.each(statesArr, function (key, val) {
				if(val.name == cityObj.stateAbbreviation) {
					hasState = true;
					return false; 
				}
			});

			if(hasState) {
				return statesArr;
			}

			statesArr.push({
				"name" 		: cityObj.stateAbbreviation,
				"selected" 	: false
			});
		}

		return statesArr;
	}

	function addToStatesObj(cityObj, statesObj) {
		if(cityObj.stateAbbreviation) {
			if(!statesObj.hasOwnProperty(cityObj.stateAbbreviation)) {
				statesObj[cityObj.stateAbbreviation] = {
					name 		: cityObj.stateAbbreviation,
					selected 	: false,
					cities 	: []
				};
			}

			cityObj.name = self.trimStateFromCityName(cityObj.name);
			statesObj[cityObj.stateAbbreviation].cities.push(cityObj);

			if(cityObj.id == options.selectedCityId) {
				statesObj[cityObj.stateAbbreviation].selected = true;
			}
		}
		return statesObj;
	}

	function getSelectedState(cityObj) {
		var selectedState;
		if(cityObj.id == options.selectedCityId) {
			selectedState = cityObj.stateAbbreviation;

			$.each(self.statesArr, function(key, val) {
				if(val.name == selectedState) {
					self.statesArr[key].selected = true;
				}
			});		
		}
		return selectedState;
	}

	function getNationalCity(cityObj) {
		var nationalCityData = {};
		if(cityObj.isNational) {
			if(cityObj.id == options.selectedCityId) {
				nationalCityData.isNational = true;
			}

			nationalCityData.nationalId = cityObj.id;
			nationalCityData.nationalReadableName = cityObj.readableName;
		}
		return nationalCityData;
	}

	function createStateCategoryPopup() {
		var source			= $("#stateCategoryTemplate").html(),
			template 		= Handlebars.compile(source),
			templateData 	= {
				states 					: self.statesArr.sort(sortByName),
				isNational 				: self.nationalCityData.isNational,
				nationalId 				: self.nationalCityData.nationalId,
				nationalReadableName 	: self.nationalCityData.nationalReadableName
			};

		options.$placeholder.html(template(templateData));		
	}

	function updateCategoriesList(templateData) {
		var source		= $("#citiesListTemplate").html(),
			template 	= Handlebars.compile(source);

		templateData = citiesToColumns(templateData);
		options.$placeholder.find("#categriesContainer").html(template(templateData));
	}

	function citiesToColumns(data) {
		var cities = data.cities,
			itemsPerCol,
			columns = [],
			column 	= [];

		if(cities.length / options.minCitiesPerCol <= options.maxCols) {
			itemsPerCol	= options.minCitiesPerCol;
		}
		else {
			itemsPerCol = Math.ceil(cities.length / options.maxCols);
		}

		for (var i = 0; i < cities.length; i++) {
			var category = cities[i];
			column.push(category);
			if((i+1) % itemsPerCol == 0 || i+1 == cities.length) {
				columns.push({cities : column});
				column = [];
			}
		};

		return columns;
	}

	function bindings() {
		// bind state links
		options.$placeholder.find("#statesContainer").on("click", ".stateLink", function() {
			selectStateFromList($(this));
			return false;
		});

		// bind categories links
		options.$placeholder.find("#categriesContainer").on("click", ".categoryLink", function() {
			selectCityFromList($(this));
			return false;
		});

		// bind national link
		options.$placeholder.find("#viewNationalLink").on("click", function() {
			selectCityFromList($(this));
			return false;
		});

		// menu toggle
		options.$selectedCityWrapper.bind("click", function(e){
			options.$placeholder.toggle();
			e.stopImmediatePropagation();
			return false;
		});

		$("body").on("click", function(e) {
			//click anywhere outside popup to hide it
            if(!$(e.target).parents(".stateCategoryPlaceholderWrapper").length) {
				options.$placeholder.hide();
            }        
		});
	}

	function selectStateFromList($el) {
		var	state 	= $el.data("abbr");

		options.$placeholder.find(".categoriesSection").show();
		$(".selectedState").removeClass("selectedState");
		$el.addClass("selectedState");

		updateCategoriesList(self.statesObj[state]);
	}

	function selectCityFromList($el) {
		var id 	= $el.data("id"),
			readableName 	= $el.data("readablename");

		/* Use JS function for redirecting to RDP. Maxim Nc. 03-29-2013 (re #28204) */
		sjSelectCategory(id, MICROSITE_URL_PREFIX);
		// selectCategory(id, readableName, MICROSITE_URL_PREFIX); // common function used on WL sites
	}

	/* Redirect to RDP on Category click. Andreyk - 11/21/2012 refs #25595 */
	function sjSelectCategory(offerCategoryId, MICROSITE_URL_PREFIX) {
		// Update Slect City function to use JSON, instead simple get method. Maxim NC (07/28/2013)
		$.getJSON(MICROSITE_URL_PREFIX + "/selectCategory.action", {offerCategoryId: offerCategoryId, now: (new Date()).getTime()}, function(data) {
			var catName = data.info.map.readableName;
			if(CommonVariables.isDealStore || CommonVariables.isRedemptionStore) {
				var endPoint = "/store";
				if (CommonVariables.isRedemptionStore) {
					endPoint = "/membershipstore";
				}
				var url = MICROSITE_URL_PREFIX + "/deal/" + catName + endPoint; 
				if (null != additionalParams && (null != additionalParams.tag && "" != additionalParams.tag) ) {
					url += "?tag=" + additionalParams.tag;
				}
				window.location.href = url;
				return true;
			}
			console.log(MICROSITE_URL_PREFIX + "/deal/" + catName);
			window.location.href = MICROSITE_URL_PREFIX + "/deal/" + catName; // redirecting to RDP
			return true;
		}, "html");						
	}

	self.trimStateFromCityName = function(cityName) {
		cityName = cityName.replace(/,[\s]?[A-Z]+$/i, '');
		return cityName;
	}

	// object sorting function for states array
	function sortByName(a,b) {
		if (a.name < b.name)
			return -1;
		if (a.name > b.name)
			return 1;
		return 0;
	}
}

//launch on window load to override document ready
$(document).ready(function(){
	var stateCategoryList = new StateCategoryListClass();
	stateCategoryList.init();

	// remove state from selected city name
	(function(){
		var $sjSelectedCityName = $("#sjSelectedCityName");
		$sjSelectedCityName.html(stateCategoryList.trimStateFromCityName($sjSelectedCityName.html()));
	})();
})
</script>
<!-- States Categories Popup } -->

