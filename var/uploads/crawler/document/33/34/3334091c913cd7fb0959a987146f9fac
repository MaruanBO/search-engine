			$container: $("#sjHeaderSubscription")
		});
		sjHeaderSubscription.init();
		$('#my-button').popover({
			header: '#my-popover > .header',
		    content: '#my-popover > .content'
		});
		
		/*$('#promocode-info').popover({
			header: '#promocode-info-content > .header',
		    content: '#promocode-info-content > .content'
		});*/
		
		// BIG DEALS
		var sjBigDealTopSubscription = new sjSubscriptionClass({
			$container: $("#sjBigDealsTopSubscription")
		});
		sjBigDealTopSubscription.init();
		var sjBigDealpSubscription = new sjSubscriptionClass({
			$container: $("#sjBigDealsSubscription")
		});
		sjBigDealpSubscription.init();
	});
	
	function sjSubscriptionClass(opts){
		var URL = MICROSITE_URL_PREFIX + "/v2/subscribeAlerts.action",
			form = ".sjSubscriptionForm_js",
			emailField = ".sjSubscriptionEmailField_js",
			zipField = ".sjSubscriptionZipField_js",
			confirm = ".sjSubscriptionConfirm_js",
			container = "body",
			serverErroMsg = "Something went wrong. Please, try again later!";
		
		var _opts = {
			$container: $(container),
			form: form,
			email: emailField,
			zip: zipField,
			confirm: confirm
		}
		
		this.init = function(){
			// merge default options with user
			$.extend(_opts, opts);
			
			var $form = _opts.$container.find(_opts.form);
			$form.on("submit", function(e){ e.preventDefault();});
			
			initValidation();
		};
		
		function initValidation(){
			var $email = _opts.$container.find(_opts.email);
			var $form = _opts.$container.find(_opts.form);
			$form.validationEngine('attach', {
				promptPosition : "topLeft", 
				scroll: false,
				onValidationComplete: function(form, status){
					var isValid = status;
					if (status){
						var emailValue = $.trim($email.val());
						$.briteVerify.email(emailValue, function(email){
							if(email.errors){
								msg = email.errors[0][1]; 
								$email.validationEngine('showPrompt', msg, 'error', "topLeft", true);
								isValid = false;
							} else {
								// $(form).validationEngine("detach");
								// $(form).find("#sjSubmit").trigger("click");
								ajaxSubscription();
								isValid = true;
							}
							return isValid;
						});				
					} else {
						return isValid
					}
				}
			});
		};
		
		function ajaxSubscription(){
			$.ajax({
				url: URL,
				type: "post",
				data: _opts.$container.find(_opts.form).serialize(),
				success: function(data){
					showConfirm();
				}, 
				error: function(){
					showError(serverErroMsg);
				}
			});
		};
		
		function showConfirm(){
			_opts.$container.find(_opts.form).addClass("hide");
			_opts.$container.find(_opts.confirm).removeClass("hide");
		};
	};	
</script>

