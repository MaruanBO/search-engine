        function _iFrameBrowserIsIE9() 
        {
            return !!( (/msie 9./i).test(navigator.appVersion) && window.ActiveXObject && XDomainRequest );
        }

        function _iFrameBrowserIsChrome()
        {
            return $j.browser.webkit && (navigator.userAgent.toLowerCase().indexOf('chrome') > -1);
        }
        
        function _iFrameBrowserIsSafari()
        {
            return $j.browser.webkit && (navigator.userAgent.toLowerCase().indexOf('safari') > -1);
        }

