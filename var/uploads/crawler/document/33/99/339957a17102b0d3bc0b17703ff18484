        var quotes = jQuery('.quote-block');
        quotes.hide().filter(':first').show();

        var period = 8000; // 8 seconds
        var fadeInDuration = 400;

        var cycle = setTimeout(function cycleQuotes() {
            if (cycle) {
                cycleQuote();
                setTimeout(cycleQuotes, period);
            }        
        }, period);

        jQuery('nav.quote-collection a.dot').on('click', function() {
            var quote_id = jQuery(this).attr('quote-id');

            jQuery('.quote-block').hide().filter('#quote-'+quote_id).fadeIn(fadeInDuration);

            jQuery('nav.quote-collection a.dot').removeClass('active');
            jQuery(this).addClass('active');
        });

        jQuery('.quote .quote-block').on('click', function() {
            cycleQuote();
            cycle = false; // stop cycling of carousels
            clearTimeout(cycle);
        });

        function cycleQuote() {
            // current quote displayed
            var current = jQuery('nav.quote-collection a.active').attr('quote-id');
            jQuery('nav.quote-collection a.dot').removeClass('active');

            // cycle to next quote
            if (current++ < quotes.length) {
                quotes.hide().filter('#quote-'+current).fadeIn(fadeInDuration);
                
                jQuery('nav.quote-collection a.dot').filter(function(index) {
                    return (index+1) == current;
                }).addClass('active');
            } else {
                // return to first quote
                jQuery('nav.quote-collection a.dot').filter(':first').addClass('active');
                quotes.hide().filter('#quote-1').fadeIn(fadeInDuration);
            }
        }

