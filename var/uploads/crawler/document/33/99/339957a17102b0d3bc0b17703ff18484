        jQuery(document).ready(function() {
            // datepickers
            jQuery('.search-date').datepicker({
                changeMonth: true,
                changeYear: true,
                showOn: 'both',
                buttonImage: 'http://static.clicktripz.com/images/calendar.png',
                buttonImageOnly: true,
                minDate: '0d',
                duration: '',
                beforeShow: function(input) {
                    if ((_iFrameBrowserIsIE9) || (_iFrameBrowserIsChrome) || (_iFrameBrowserIsSafari)) {
                        input.click();                  
                    }
                },
                onSelect: function(dateText) {
                    var startDate = new Date(jQuery(this).datepicker('getDate'));
                    var endDate = new Date(jQuery('.search-date-2').datepicker('getDate'));
                    var d = new Date(jQuery(this).datepicker('getDate'));
                    jQuery('.search-date-2').each(function(index) {
                        jQuery(this).datepicker('option', 'minDate', d);
                    });
                    if (endDate < startDate) {
                        // auto-set second date to 2 days after first date.
                        d.setHours(48);
                        jQuery('.search-date-2').datepicker('setDate', d);
                        jQuery('.search-date-2').datepicker('option', 'minDate', startDate);
                        // set to round trip if return date is specified
                        if (jQuery('.search-date-2').val().length > 0) {
                            jQuery('#search-flights-roundtrip').click();
                        }
                    }
                    jQuery('#search-flights-change').show();
                },
                onClose: function(dateText) {
                    jQuery('.search-date').each(function(index) {
                        jQuery(this).datepicker("disable"); // need to disable, set date, then re-enable for IE, otherwise datepicker dialog opens.
                        jQuery(this).datepicker('setDate', dateText);
                        jQuery(this).datepicker("enable");
                    });
                }
            });

            jQuery('.search-date-2').datepicker({
                changeMonth: true,
                changeYear: true,
