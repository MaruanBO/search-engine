var numberPages = 4;
$(function() {
    $('#hp-hero-in-todays-issue').after('<div id="hp-hero-in-todays-issue-nav"><div id="prevPage"><a class="active" id="prevLink" href="#">prev</a></div><ul id="nav" class="simplePagerNav"/><div id="nextPage"><a class="active" id="nextLink" href="#">next</a></div></div>').cycle({
        fx:     'fade',
        nowrap:  0,
        timeout: 8000,
        speed:  750,
        containerResize: 0,
        fit: 1,
        pager: '#nav',
        prev: '#prevLink',
        next: '#nextLink',
        pagerAnchorBuilder: function(idx, slide) {
           return '<li class="otherPage"><a rel="' + idx + '" href="#"></a></li>';
        }
    });
    if (numberPages == 1)
    {
       $("#hp-hero-in-todays-issue-nav").hide();
    }
    else
    {
       // Calculate the width of the navigation controls, based on images, 8 pixels need to be added for each page over 2
       var pageCounterOver2 = numberPages - 2;
       var marginLeft = 100 - (pageCounterOver2 * 8);

       $("#hp-hero-in-todays-issue-nav").attr("style", "left: " + marginLeft + "px;");
   }

   $('#hp-hero-in-todays-issue').show();
});
