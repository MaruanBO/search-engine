            cmg.cmgRotator.pushMetrics = function (obj) {
                var link = obj.attr('href'),
                    title = obj.data('title').toLowerCase(),
                    page_name = obj.data('pagename').toLowerCase(),
                    site_type = 'city-site',
                    index = obj.index + 1,
                    channel = (site_type == 'city-site') ? 'entertainment' : cmg.s_coxnews.channel;
                payload = ['47', this];
                payload.push(['pageName', page_name]);
                payload.push(['eVar40', 'interactive_rotator_slide' + index + '_np_' + channel]);
