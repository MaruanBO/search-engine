  <script type="text/javascript">
  $(".div_tab__more_cont_142438208431437").not(':first').addClass('disNone');
  
  var tabtotal = '16';
  var tabcount = '6';
  var firsttab = 1;
  var lasttab = 2;
  var key=1;
  var panelsetmapid = '1124';
  function movingtabs(action){
	  	  
	  if(action=='p' && tabcount > 1 && 1 <= firsttab ){		  
		  if(firsttab>1){
			  firsttab--;
		  	  lasttab--;
		  }
		  $("#tabpage_"+panelsetmapid+"_"+lasttab).hide();
		  $("#tabpage_"+panelsetmapid+"_"+firsttab).show();
		  if(key>3) key = key-3;	
		  var newTab = 'newtab_'+panelsetmapid+'_'+key;
		  var newCount = 'newCont_'+panelsetmapid+'_'+key;
		  showhideTab(tabtotal , panelsetmapid , newTab , newCount );
		  if( firsttab > 1 ){ $("#company_prev").show(); }else{ $("#company_prev").hide(); }
		  if( firsttab  < tabcount ){ $("#company_next").show(); }else{ $("#company_next").hide(); }    
	  }else if(action=='n' && tabcount > 1  && tabcount >= lasttab ){			  
		  $("#tabpage_"+panelsetmapid+"_"+firsttab).hide();
		  $("#tabpage_"+panelsetmapid+"_"+lasttab).show();
		  firsttab++;
		  lasttab++;
		  key = key+3;
		  var newTab = 'newtab_'+panelsetmapid+'_'+key;
		  var newCount = 'newCont_'+panelsetmapid+'_'+key;
		  showhideTab(tabtotal , panelsetmapid , newTab , newCount );	
		  if( firsttab  < tabcount ){ $("#company_next").show(); }else{ $("#company_next").hide(); }	
		  if( lasttab > 1 ){ $("#company_prev").show(); }else{ $("#company_prev").hide(); }	  
	  }	 
  }
  </script>
