<script type="text/javascript">
$(document).ready(function(){
	$(".accordion h3.th3").addClass("active");

	$(".accordion h3#h3_524").click(function(){
		$(this).next(".accordion .contContainer").slideToggle("slow")
		.siblings(".accordion .contContainer:visible").slideUp("slow");
		$(this).toggleClass("active");
		$(this).siblings("h3").removeClass("active").find('img').attr('src','/include/_mod/site/images/collapse_acc_arrow.png');
		if ($(this).hasClass('active')) {
			$(this).find('img').attr('src','/include/_mod/site/images/expand_acc_arrow.png');

			var symbol = $(this).attr('rel');
			var exchange = $(this).attr('exchange');
			var expiryDate = $(this).attr('expdate');
			
			createCommodityChart('chartDiv_'+symbol,exchange,symbol,expiryDate,'524');
					   
		}else{
			$(this).find('img').attr('src','/include/_mod/site/images/collapse_acc_arrow.png');
		}
	});

	     
});
function InptContFocus(id, value)
{
	if(document.getElementById(id).value == value)
	{
		document.getElementById(id).value = '';
	}
}
function InptContBlur(id, value)
{
	if(document.getElementById(id).value == '')
	{
		document.getElementById(id).value = value;
	}
} 
function searchCommodity(data){
	if(data == ""){
	  data = $('#search_commodity').val().toUpperCase();;	
	  var iChars = "!@#$%^&*()+=-[]\\\';,./{}|\":<>?~_";
	  for (var i = 0; i < data.length; i++) {
	  	if (iChars.indexOf(data.charAt(i)) != -1) {
	  	  alert("Please enter valid commodity.");
	  	  return false;
	  	}
	  }
    }
	window.location = "/live-market/commodity/"+"search/symbol/"+data+"/exchng/"+$("#exchng").val(); 
}
</script></div>
