<script type="text/javascript">
function setPollOption(obj){
	$("#poll_selected_option").val(obj.value);
}
function getPollResult(categoryBlockMapId){	
	$("#poll_msg_"+categoryBlockMapId).hide();
	var pollId = $("#poll_id_"+categoryBlockMapId).val();
	var optionId = '';
	var	groupId = '';
	var optionVal = $("#poll_selected_option").val();
	if(optionVal!='' && optionVal != undefined){
		var optionObj = eval('('+unescape(optionVal)+')');
		if(optionObj != undefined){
			optionId = optionObj.option_id;
			groupId = optionObj.group_id;
		}
	}
	if( categoryBlockMapId != '' && pollId != '' && groupId != ''  && optionId != '' ){	
		$.ajax({	    	
		    url: "/poll/manager/result",	
		    type: 'POST',
		    cache: false,
		    async: false,
		    data: { 'pollId': pollId , 'groupId':groupId , 'optionId':optionId },
		    success: function(data){
			    var dataArr = data.split('#*#*#');
				if(dataArr[0]=='success'){
					$("#main_display_"+categoryBlockMapId).hide();
					$("#result_display_"+categoryBlockMapId).show();
					if(dataArr[1]!=''){
						$("#poll_msg_"+categoryBlockMapId).show();
						$("#poll_msg_"+categoryBlockMapId).html(dataArr[1]);
					}	
					$("#poll_option_"+categoryBlockMapId).html(dataArr[2]);
				}else{
					$("#main_display_"+categoryBlockMapId).show();
					$("#result_display_"+categoryBlockMapId).hide();
					$("#poll_msg_"+categoryBlockMapId).show();
					$("#poll_msg_"+categoryBlockMapId).html(dataArr[1]);
				}
			}
		});	
	}else{
		$("#poll_msg_"+categoryBlockMapId).show();
		$("#poll_msg_"+categoryBlockMapId).html('Please select an option to submit poll.');
	}
}
</script><!--pubDate: Thu, 19 Feb 2015 13:33:45 +0530-->		
