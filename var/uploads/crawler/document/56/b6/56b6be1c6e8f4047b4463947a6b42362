<script type="text/javascript">
function foSearch(catBlockId)
{

			symbol = $('#fno-api-equity-form'+catBlockId+' #symbol').val();
			scriptType = $('#fno-api-equity-form'+catBlockId+' #script-type').val();
			expiryDate = $('#fno-api-equity-form'+catBlockId+' #expiry-date').val();
			instrumentType = $('#fno-api-equity-form'+catBlockId+' #instrument-type').val();
			optionType = $('#fno-api-equity-form'+catBlockId+' #option-type').val();
			categoryBlockMapId = $('#fno-api-equity-form'+catBlockId+' #category-block-map-id').val();
			
			postUrl ="script-type="+scriptType+"&symbol="+symbol+"&instrument-type="+instrumentType+"&expiry-date="+expiryDate+"&option-type="+optionType+"&categoryBlockMapId="+categoryBlockMapId;
			
		$.ajax({
	        type : "POST",
	        url: "/live-market/derivatives-api/search-equity",	
	        data: postUrl, 
	        dataType : "html",
	        success: function(resultData) {
				if(resultData!=undefined){
					try{
						$("#derivatives-fno-search"+catBlockId).parent().html(resultData);
					}catch(e){}
				}
	        },
	        error : function() {
	        	if( console && console.log ) {
	        	    console.log("Sample of data:", data.slice(0, 100));
	        	  }
	        }
	});
}

	$("#fno-api-equity-form2176").validationEngine();
</script></div>
