    <script type="text/javascript">
        $(document).ready(function(){
 $('#howitworks,#pricing').hide();
 $('#howitworks .backgroundColor>div,#pricing .backgroundColor>div').css({'min-height':'565px'});
 $("img.lazy").each(function(){$(this).attr('src',$(this).attr('data-src'))});
 $('#header-nav-bottom a').click(function(){
   if(!$(this).hasClass('cf_activenav')) {
     $($('#header-nav-bottom a.cf_activenav').attr('href')).hide();
     $('#header-nav-bottom a.cf_activenav').removeClass('cf_activenav');
     $(this).addClass('cf_activenav');
     $($(this).attr('href')).show();
     utag_data.template = $($(this).attr('href')).attr('data-analytics-template');
     utag_data.content_type = $($(this).attr('href')).attr('data-analytics-content-type');
     utag_data.sub_section = $($(this).attr('href')).attr('data-analytics-sub-section');
     utag.view(utag_data);
   }
   return false;
 });
});
    </script>

 

     
