			$(document).ready(function(){
				$('#HomeShow') 
				
				.cycle({ 
				    fx:     'fade', 
				    speed:  'slow', 
				    timeout: 7000, 
				    pager:  '#slidenav',
				    before:	onBefore,
				    after:	onAfter  
				});
				
				function onBefore() {
				    $('#output').html("");
				}

				function onAfter() {
				    $('#output').html('<p>' + this.title + '</p>');
				}
				
				$('#slidenav a').html("&nbsp;");
			});
	</script>


