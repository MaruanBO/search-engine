</script>  <script>
if(typeof window != 'undefined' && typeof window.location != 'undefined' && typeof window.location.pathname != 'undefined' &&
(window.location.pathname == '/' || window.location.pathname == '/en_GB/' || window.location.pathname == '/es_ES/' || window.location.pathname == '/fr_FR/' || window.location.pathname == '/it_IT/' || window.location.pathname == '/de_DE/' || window.location.pathname == '/ja_JP/')) {
  (function(){
    // redirect user based on subdomain
    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }
    if ( readCookie('box_marketing') && readCookie('box_active_user_subdomain') ) {
      var subdomain = encodeURIComponent(readCookie('box_active_user_subdomain'));
      var patt = /^[a-zA-Z0-9\.\-\_]+$/;
      if(subdomain && patt.test(subdomain)){
        window.location.href = 'https://'+subdomain+'.'+box_server_domain_tld+'/login';
        document.getElementsByTagName("html")[0].setAttribute("style","visibility:hidden;");
      }
    }
  })();
}
</script>  <script>
  var init_redirect_translation_urls = {"en":"https:\/\/www.box.com\/home\/","en-gb":"https:\/\/www.box.com\/en_GB\/home\/","fr":"https:\/\/www.box.com\/fr_FR\/home\/","de":"https:\/\/www.box.com\/de_DE\/home\/","it":"https:\/\/www.box.com\/it_IT\/home\/","ja":"https:\/\/www.box.com\/ja_JP\/home\/","es":"https:\/\/www.box.com\/es_ES\/home\/"};
var init_redirect_curr_url_lang = "en";
var init_redirect_language_mapping = {"en":"","en-gb":"en_GB","fr":"fr_FR","de":"de_DE","it":"it_IT","ja":"ja_JP","es":"es_ES"};

(function(){
  function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return encodeURIComponent(c.substring(nameEQ.length,c.length));
    }
    return null;
  }

  function createCookie(name,value,days,domain,path) {
    var date = new Date();
    date.setTime(date.getTime()+(days*24*60*60*1000));
    var expires = date.toGMTString();
    if(!domain) {
      var domain = document.domain;
      domain = domain.replace(/^[^.]+\./g, "");
    }
    if(!path){
      var path = "/";
    }
    document.cookie = name+"="+value+"; expires="+expires+"; domain="+domain+"; path="+path+";secure";
  }

  function redirectToLocale(langKey,createCookieBool){
    var langKeyArray = ['en','en-gb','es','fr','it','de','ja']; //ordered to match up with LocaleArray.
    var localeArray = ['en_US','en_GB','es_ES', 'fr_FR', 'it_IT', 'de_DE', 'ja_JP'];
    if(langKeyArray.indexOf(langKey) > -1){
      var locale = localeArray[langKeyArray.indexOf(langKey)];
      if(createCookieBool){
        var cookieDomain = document.domain.match(/box\.com$/) ? "box.com" : false;
        createCookie('box_locale',locale,61,cookieDomain);
        createCookie('lang',locale.replace('_','-'),61,cookieDomain);      
      }
      //do redirection if we aren't already on correct language:
      if(typeof init_redirect_translation_urls != 'undefined' && typeof init_redirect_translation_urls[langKey] != 'undefined' && init_redirect_curr_url_lang != langKey){
        window.location.href = init_redirect_translation_urls[langKey];
        document.getElementsByTagName("html")[0].setAttribute("style","visibility:hidden;");
      }
    }                            
  }

  //check for box_locale cookie and redirect to locale if necessary.
  var box_locale_cookie = readCookie('box_locale');
  if (box_locale_cookie){
    var language_key = null;
    for (var language_code in init_redirect_language_mapping) {
        if (init_redirect_language_mapping[language_code] === box_locale_cookie) {
            language_key = language_code;
            break;
        } else {
            language_key = 'en';
        }
    }
    redirectToLocale(language_key, false);
  }
  else{    
    //if the user was not hitting a language-url path, set box_locale cookie based on their accept_language headers using ajax request
    var xmlhttp;
    if (window.XMLHttpRequest){// code for IE7+, Firefox, Chrome, Opera, Safari
      xmlhttp=new XMLHttpRequest();
    }
    else{// code for IE6, IE5
      xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
    }
    xmlhttp.onreadystatechange=function(){
      if (xmlhttp.readyState==4 && xmlhttp.status==200 && xmlhttp.responseText){
        var headers = JSON.parse(xmlhttp.responseText);
        var primaryLang = headers.acceptLanguage.substring(0,5).toLowerCase();
        var primaryLangStart = primaryLang.substring(0,2);
        var langStarts = ['en','es','fr','it','de','ja'];
        var countryCode = headers.countryCode;
        //countryCode.match(/^[A-Z]{2}$/) ? createCookie('CTRY', countryCode, 5, document.domain) : createCookie('CTRY', 'US', 5, document.domain);
        var euroNonFIGSCountries = ['GB', 'AL','AD','AM','AT','AZ','BY','BE','BA','BG','HR','CY','CZ','DK','EE','FI','GE','GR','VA','HU','IS','IE','XK',
                                     'LV','LI','LT','LU','MK','MT','MD','MC','ME','NL','NO','PL','PT','RO','RU','SM','RS','SK','SI','SE','CH','TR','UA'];  
        if(primaryLang == 'en-gb' || (countryCode && euroNonFIGSCountries.indexOf(countryCode) > -1)){
          redirectToLocale('en-gb', true);
        }
        else if(langStarts.indexOf(primaryLangStart) > -1){
          redirectToLocale(primaryLangStart, true);
        }
      }
    }
    xmlhttp.open("GET","/headers.php",true); //true for asynchronous
    xmlhttp.send();
  }
})();
</script>
