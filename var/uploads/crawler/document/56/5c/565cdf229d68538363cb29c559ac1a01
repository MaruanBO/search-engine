			<div class="section-pick">
            <div class="home-new home-active">
                <a>New</a>
            </div>
            <div class="home-featured">
                <a>Featured</a>
            </div>
           <!--  <div class="home-popular">
                <a>Popular</a>
            </div> -->
        </div>
        <script type="text/javascript">
            jQuery(function(){
            	if( jQuery( window ).width() < 1000 )
            	{
            		function resizeimg1()
                	{
                		jQuery('.left-col-img').each(function()
                        {
                            var leftwidth = jQuery(this).width();
                            var leftheight = Math.round( Number(leftwidth ) / Number (1.92) );
                            jQuery(this).parent().css('height', leftheight);
                        });
                	}
                	
                    jQuery('.section-pick').parent().css('padding', '0');
                    jQuery('.loop-content').addClass('loop-padding');

                    jQuery('.loop-featured').append(jQuery("#aomsidebars_2-2").html());
                    jQuery('.loop-featured img').each(function()
                    {
                    	jQuery(this).attr('class', 'th wp-post-image left-col-img');
                        var src = jQuery(this).attr('data-original');
                    	jQuery(this).attr('src', src);
                    });
                    
                    jQuery('.section-pick div').click(function()
                    {
                    	jQuery('.home-active').removeClass('home-active');
                    	jQuery(this).addClass('home-active');
                        if (jQuery(this).hasClass('home-new'))
                        {
                        	jQuery('.loop-featured').hide();
                        	jQuery('.loop-content').show();
                        }
                        else if (jQuery(this).hasClass('home-featured'))
                        {
                        	jQuery('.loop-content').hide();
                        	jQuery('.loop-featured').show();
                        }
                        resizeimg1();
                    });

                    jQuery(window).on('resize', resizeimg1);
            	}
            });
        </script>
        <style>
            @media screen and (min-width: 1000px) {
                .section-pick {
                	display: none;
                }
                .loop-featured {
                	display: none !important;
                }
                .loop-content {
                	display: block !important;
                }
            }
            @media screen and (max-width: 999px) {
                .section-pick {
                	width: 100%;
                	text-transform: uppercase;
                	margin-bottom: 20px;
                	display: table;
                	background-color: #faf3eb;
                }
                .section-pick div {
                	width: 33.3%;
                    display: table-cell;
                	white-space:nowrap;
                }
                .section-pick div a {
                	color: #555;
                    padding: 9px 15px 3px;
                    display: block;
                    text-decoration: none;
                	color: #555;
                	font-size: 20px;
                	font-weight: 700;
                	line-height: 1.5em;
                	font-family: "Oswald",sans-serif;
                }
                .home-new {
                	text-align: center;
                }
                .home-featured, .home-popular {
                	text-align: center;
                }
                .home-active {
                	background-color: #ebddc4;
                }
                .loop-padding {
                	padding-left: 8px !important;
                	padding-right: 8px !important;
                }
                .section-title {
                	display: none;
                }
                .loop-featured {
                	display: none;
                	padding: 0 18px 20px 18px;
                }
                .loop-featured .home-ad, .loop-featured header strong {
                    display: none;
                }
            }
        </style>
