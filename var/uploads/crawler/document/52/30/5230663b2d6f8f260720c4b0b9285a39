        <script type="text/javascript">
        (function personalize() {
            var attributes = Profile.getAttributes();
            if ("login-bi" in attributes || "login-pi" in attributes) {
                document.getElementById("account-login").className += " active";
                document.getElementById("js-account-login").className += " active";
            } else {
                document.getElementById("get-a-quote").className += " active";
                document.getElementById("js-get-a-quote").className += " active";
            }
        })();
        </script>
