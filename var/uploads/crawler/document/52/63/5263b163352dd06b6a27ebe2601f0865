//Add to DB
function AddDB(videourl,AdId)
{
	getLastVast='Content Video';
	
	var gourl= videourl;	
	var goID= AdId;	
	gourl=gourl.replace(/\https/g,'http');
	var getvideoUrl='http://addvideo.ani-view.com/addVideo.aspx';
	var postvideoUrl='videoURL='+gourl+'&xchangePartner=7&ADID='+goID+'&vast='+getLastVast;		
	var Checkifvideoexists = new XMLHttpRequest();
	Checkifvideoexists.open('POST',getvideoUrl, true);
	Checkifvideoexists.setRequestHeader("Content-type","application/x-www-form-urlencoded");
	Checkifvideoexists.send(postvideoUrl);
	
	Checkifvideoexists.onreadystatechange = function() { 
				if (Checkifvideoexists.readyState == 4) {
					var textResult=document.getElementById("result");
					textResult.innerHTML='Your Video Content ID:'+AdId;
				}
				
			}
			
	
}	
var goButton=document.getElementById("go");
	goButton.onclick = function () {
		AdId=new Date().getTime();
		videoUrl=document.getElementById("VideoUrl").value
		AddDB(videoUrl,AdId);
	}		

}
}(jQuery));	

	 
