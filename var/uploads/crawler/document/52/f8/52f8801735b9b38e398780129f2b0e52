              return;
            }

            googletag.cmd.push(function() {
              var slot = googletag.defineSlot('/53396253/st-homepage-728x90btf', [[728,90],[728,91]],'gtp-ad-st-homepage-728x90btf');
              slot.setCollapseEmptyDiv(true);
