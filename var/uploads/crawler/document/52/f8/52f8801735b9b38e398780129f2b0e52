              refreshableAdSlots.push(slot);
            });
            adCalled = true;
          }

          
              if(typeof SHTApplication === 'undefined'){
                callAd();
              } else {
                if(!SHTApplication.isHandHeldDevice()) {
                  callAd();
                }else{
                  $('#gtp-ad-st-homepage-300x250atf').hide();
                }

                $(SHTApplication).on('type_change', function(ev, change) {
                  if (change.forMobile()) {
                    $('#gtp-ad-st-homepage-300x250atf').hide();
                  } else if (change.forDesktop()) {
                    callAd();
                  }
                });
              }

})(jQuery);</script><noscript><a href="//pubads.g.doubleclick.net/gampad/jump?iu=/53396253/st-homepage-300x250atf&amp;sz=300x250&amp;c=88925&amp;t=kw=homepage,shermans-travel" target="_blank"><img alt="St-homepage-300x250atf&amp;sz=300x250&amp;c=88925&amp;t=kw=homepage,shermans-travel" src="//pubads.g.doubleclick.net/gampad/ad?iu=/53396253/st-homepage-300x250atf&amp;sz=300x250&amp;c=88925&amp;t=kw=homepage,shermans-travel" /></a></noscript></div>
<!-- end of DoubleClick script -->
      </div>
    </div>
  </section>


