  <script type="text/javascript">
    (function($){
      function setMobileSource($) {
        $('input#person_source').val('st_widget');
      }

      function setDesktopSource($) {
        $('input#person_source').val('st_widget');
      }

      if(isHandHeldDevice()) {
        setMobileSource($);
      } else {
        setDesktopSource($);
      }

      $(SHTApplication).on('type_change', function(ev, change) {
        if (change.forMobile()) {
          setMobileSource($);
        } else if (change.forDesktop()) {
          setDesktopSource($);
        }
      });
    })(jQuery);
  </script>


