//Collapse fix
$(document).ready(function () {
	$(".accordion-toggle").click(function(){
		//alert("clicked");
		//alert($(this).attr("href"));
		var hrefValue = $(this).attr("href");
		if(hrefValue != "#contact")
		{
			$('#contact iframe').css( "display", "none" );
		}
		else
		{
			$('#contact iframe').css( "display", "inline" );	
		}
		//var frameFix = $('#contact iframe').css('display');
		//alert(frameFix);
	});
});
	


