    var gptPath = window.location.pathname.toLowerCase() + (window.location.pathname == '/' ? 'home/' : '');
    gptPath = gptPath.substring(0, gptPath.lastIndexOf('/'));
    function dfpMessageReceived(event) {
        var allowed = false;
        var domains = ['doubleclick.net', 'googlesyndication.com', 'googleadservices.com'];
        domains[domains.length] = window.location.hostname;

        for (var i=0; i < domains.length; i++) { if (event.origin.indexOf(domains[i]) >= 0) { allowed = true; break;}}

        if (allowed) {
            try { window.dfpMessageData = JSON.parse(event.data); } catch(err) { return; }

            window.removeEventListener('message', dfpMessageReceived, false); 
            var script = document.createElement('script');
            script.async = true;
            script.type = 'text/javascript';
            script.src = window.dfpMessageData.scriptUrl;
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(script, node);
        }
    }

    window.addEventListener('message', dfpMessageReceived, false);    
