<script>
	/*	Fixes search issue where clicking on form causes dropdown to toggle off. 
		Stops propagation of click event in form input elements. */
	$(function() {
	  // Setup drop down menu
	  $('.dropdown-toggle').dropdown();
	 
	  // Fix input element click problem
	  $('.dropdown-menu input, .dropdown-menu button').click(function(e) {
	    e.stopPropagation();
	  });
	});
</script>


