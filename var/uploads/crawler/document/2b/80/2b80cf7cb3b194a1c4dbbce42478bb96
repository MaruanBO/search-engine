<script>
$(function(){
 $("#languages").find("#in").prop("selected", "selected");
            $("#languages").change(function() {
                   var getLanguage = $(this).val();
 
                     if(getLanguage=="ar"){
                     document.location.href = 'http://web.vizury.com/website/'+getLanguage +"?v=ar";
                 }
                 else if(getLanguage=="pt"){
                    document.location.href = 'http://web.vizury.com/website/'+getLanguage +"?v=pt";
                 }
                 else if(getLanguage=="cn"){
                    document.location.href = 'http://web.vizury.com/website/'+getLanguage +"?v=cn";
                 }
                 else if(getLanguage=="ja"){
                    document.location.href = 'http://web.vizury.com/website/'+getLanguage +"?v=ja";
                 }
                 else{
                  document.location.href = 'http://web.vizury.com/website/'+getLanguage;   
                 }
              });

$(document).on("mouseenter","ul#menu-main-menu > li",function(){
if (!($(this).hasClass("current-menu-item")))  {
    $(this).animate({"background-color":"#f3f3f3"},500);
}
})
$(document).on("mouseleave","ul#menu-main-menu > li",function(){
if ((!($(this).hasClass("current-menu-item"))) && (!($(this).hasClass("current-menu-ancestor")))) {
    $(this).animate({"background-color":"transparent"},500);
}
})

});
</script>
<script>
    // Custom Slider with Minimal Functionality
    $(function(){
        $("article.testimonial:eq(0)").removeClass("hidden");
        var countlength = $("article.testimonial").length - 1;
        var _i = 0; _p = 0;
        $(".rightarrow").on("click",function(){
            _i++; _i = _i%(countlength+1); 
            // console.log(_i);
            $("article.testimonial").addClass("hidden");
            $("article.testimonial:eq("+_i+")").removeClass("hidden");
            _p = _i;
        });
        $(".leftarrow").on("click",function(){
            _p--; _p%=(countlength+1);
            if (_p < 0){
                _p = (countlength+1) +_p;
            }
            // console.log(_p);
            $("article.testimonial").addClass("hidden");
            $("article.testimonial:eq("+_p+")").removeClass("hidden");
            _i = _p;
        });
    });
</script>
<script>
    $(function(){
        var countlength = $("article.slide").length - 1;
        window._i = 0; window._p = 0;
        $("#next-slide").on("click",function(e){
            e.preventDefault();
            window._i = window._i + 1; window._i = window._i%(countlength+1); 
            // console.log(_i);
            $("article.slide").fadeOut();
            $("article.slide").addClass("hidden");
            $("article.slide:eq("+_i+")").removeClass("hidden").fadeIn();
                $("section#slider").find("a.dot").removeClass("selected");
                $("section#slider a.dot:eq("+_i+")").addClass("selected");

            window._p = window._i;
        });
        $("#prev-slide").on("click",function(e){
            e.preventDefault();
            window._p--; window._p%=(countlength+1);
            if (window._p < 0){
                window._p = (countlength+1) +window._p;
            }
            // console.log(_p);
            $("article.slide").fadeOut();
            $("article.slide").addClass("hidden");
            $("article.slide:eq("+window._p+")").removeClass("hidden").fadeIn();
                $("section#slider").find("a.dot").removeClass("selected");
                $("section#slider a.dot:eq("+window._p+")").addClass("selected");

            window._i = window._p;
        });
    });

</script>
<script>
    $(function(){
        var items = $("article.slide");
        var itemsLength = items.length;
        for(_q = 0;_q < itemsLength; _q++) {
        $("section#slider a.dot:eq("+_q+")").attr("enable-image",_q.toString());
        }
        $("section#slider a.dot:eq(0)").addClass("selected");

        $("section#slider").find("a.dot").each(function(){
            $(this).on("click",function(e){
                e.stopPropagation();
                $("section#slider").find("a.dot").removeClass("selected");
                $(this).addClass("selected");
            $("article.slide").addClass("hidden");
                            window._i = window._p = parseInt($(this).attr("enable-image"));

            $("article.slide:eq("+$(this).attr("enable-image")+")").removeClass("hidden").fadeIn();
            $("article.slide").each(function(index){
                if (!($(this).hasClass("hidden"))) {
                $(this).find(".tile").each($).wait(450,function(index){
                $(this).trigger("click");
                });
                // $(this).find(".tile-square").trigger("click");

                }
                });
            return false;
            })
        })
    })
</script>
