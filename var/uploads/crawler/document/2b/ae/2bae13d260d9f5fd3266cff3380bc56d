		wuFooter = new Clazz.com.wu.widget.module.WUFooter({"propertiesData":{"show_quicklink":"true","show_sendmoneylink":"true","show_menulink":"true","show_securityImage":"true"}});
	 }
	
	// clear cache, should only do this in edit mode... 
