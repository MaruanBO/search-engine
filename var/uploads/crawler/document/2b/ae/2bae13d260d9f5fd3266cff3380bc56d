	//feedback link..
	$("#wu-feedback-link").click(function(){
		var url = 'https://thewesternunion.custhelp.com/app/ask/';
		var emailField = 'contacts.email';
		var firstNameField = 'incidents.c$sender_first_name';
		var lastNameField = 'incidents.c$sender_last_name';
		var phoneField = 'incidents.c$sender_phone';
		
		var gatewayCustomer = JSON.parse(Clazz.WULocalStorage.getGatewayCustomer());
		if(gatewayCustomer){
			var email = gatewayCustomer.email;
			var firstName = gatewayCustomer.name.firstName;
			var lastName = gatewayCustomer.name.lastName;
			var senderPhone = gatewayCustomer.contactPhone;
			url = url+emailField+'/'+email+'/'+firstNameField+'/'+firstName+'/'+lastNameField+'/'+lastName+'/'+phoneField+'/'+senderPhone;
			window.open(url, '_blank', 'location=1,toolbar=0,scrollbars=1, width=500, height=900');
		}else{
			window.open(url, 'newwindow', 'scrollbars=1, width=500, height=900');
		}
	});
	 //fixing for RTHREE-4307
	// $(function () { $.smartbanner() });
});
</script>

