$(document).ready(function(){
	 var isForgotPassword = Clazz.WUNavigator.isInCurrentPage("password-recovery.html");
	 var wuFooter = "";
	 if(isForgotPassword){
		wuFooter = new Clazz.com.wu.widget.module.WUFooter({"propertiesData":{"show_quicklink":"true","show_sendmoneylink":"true","show_menulink":"true","show_securityImage":"true"}, "fromResetPassword":isForgotPassword});
