<script type="text/javascript">
    LS_App.SocialToolbar = {};
    LS_App.SocialToolbar.SetCount = function (objId, count) {
        if (count > 0) {
            $LS(objId).text(count);
            LS_Utility.Show(objId);
        }
    }

    LS_App.SocialToolbar.Facebook = {}; 
    LS_App.SocialToolbar.Facebook.SetCount = function (objId){
        $LS.getJSON('http://graph.facebook.com/?ids=' + window.location.href, function(data){;
            if (data[window.location.href] != null)
            {
                var count = data[window.location.href].shares;
                LS_App.SocialToolbar.SetCount(objId, count);
            }
        });
    }

    LS_App.SocialToolbar.Twitter = {}; 
    LS_App.SocialToolbar.Twitter.SetCount = function (objId){
        $LS.getJSON('http://urls.api.twitter.com/1/urls/count.json?url=' + window.location.href + '&callback=?', function(data){
            var count = data.count;
            LS_App.SocialToolbar.SetCount(objId, count);
        });
    }

    LS_App.SocialToolbar.Google = {}; 
    LS_App.SocialToolbar.Google.SetCount = function (objId){
        $LS.ajax({
            type: "POST",
            url: "/services/socialshareserver.asmx/GoogleShareCount",
            data: "{'url':'" + window.location.href + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function(data) {
                var count = data.d;
                LS_App.SocialToolbar.SetCount(objId, count);
            }
        });
    }
</script>

