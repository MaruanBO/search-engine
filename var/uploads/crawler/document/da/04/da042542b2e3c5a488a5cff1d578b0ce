<script type="text/javascript" src="//ajax.aspnetcdn.com/ajax/jquery.cycle/2.99/jquery.cycle.all.min.js"></script>
<script type="text/javascript">
$LS(function() {
    $LS('.slideExperts').cycle({
    fx: 'fade',
    speed: 'slow',
    timeout: 6000,
    height: '196px',
    width: '458px',
    fit: '1'
    });

}); 

$LS(function() {
    $LS('.fadeHomePage').before('<div class="HPTopModNav">').cycle({
    fx: 'fade',
    speed: 'slow',
    timeout: 6000,
    pagerEvent: 'mouseover',
    height: '434px',
    width: '464px',
    fit: '1',
    pager: '.HPTopModNav',
    before: function () { if (window.console) window.console.log(this.src); }
    });

}); 

function fnNextSlide(slide, total)
{
    if (slide == -1)
        slide = total;
    
    var divs = $LS('div.article-container');
    $LS('.slideExperts').cycle('stop');
    $LS('.slideExperts').cycle({
        fx: 'fade',
        startingSlide: slide,
        speed: 'slow',
        timeout: 6000,
        height: '196px',
        width: '458px',
        fit: '1'
    }); 
}

function fnchangeSlide(slide)
{
    var divs = $LS('div.article-container');
    $LS('.fadeHomePage').cycle('stop');
    $LS( '.HPTopModNav').find('.article-container').removeClass('activeSlide').filter('.article-container:eq('+slide+')').addClass('activeSlide');
    $LS('.fadeHomePage').cycle({
        fx: 'fade',
        startingSlide: slide,
        pager: '.HPTopModNav',
        speed: 'slow',
        timeout: 6000,
        height: '434px',
        width: '464px',
        fit: '1'
    }); 
}

function fncontinueSlide(slide) {
    $LS('.fadeHomePage').cycle({
        startingSlide: slide,
        fx: 'fade',
        pager: '.HPTopModNav',
        speed: 'slow',
        timeout: 6000,
        height: '434px',
        width: '464px',
        fit: '1'
    });
}
</script>
