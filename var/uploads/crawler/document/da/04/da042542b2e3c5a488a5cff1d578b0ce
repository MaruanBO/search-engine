<script type="text/javascript">
    function RedirectToSearch() {
        var query = $LS("#topSearchBox").val();
        window.location = "/search.aspx?query=" + encodeURI(query);
    }

    $LS(".topSearchBox").focus(function () {
        $LS('.leftTopSearch').css("background-image", "url(http://images.lifescript.com/media/header/search-open-left.png)");
        $LS('.centerTopSearch').css("background-image", "url(http://images.lifescript.com/media/header/search-open-middle.png)");
        $LS('#topRightSearchButton').attr("src", "http://images.lifescript.com/media/header/search-bar-open-right.jpg")
    });
    $LS(".topSearchBox").blur(function () {
        $LS('.leftTopSearch').css("background-image", "url(http://images.lifescript.com/media/header/search-closed-left.png)");
        $LS('.centerTopSearch').css("background-image", "url(http://images.lifescript.com/media/header/search-closed-middle.png)");
        $LS('#topRightSearchButton').attr("src", "http://images.lifescript.com/media/header/search-bar-closed-right.jpg")
    });
</script>
