	$(function() {
		$('#block-slideshow-playlist .slideshow img').each(function(){
			if($(this).attr('src')=='')
				$(this).hide();
		});
		
		$('#block-slideshow-playlist .loading').hide();
		$('#block-slideshow-playlist .slideshow').show();    
		$('#block-slideshow-playlist .slide-navigator').show();    
		var controls = '#block-slideshow-playlist .slide-navigator';
	
		$('#block-slideshow-playlist .slideshow').cycle({
			fx:      'fade',
			speed:1250,
			timeout:  7000,
			before:    onBefore,
			after:    onAfter,
			pager:    controls,
	
			pagerAnchorBuilder: function(idx, slide) {
				var slideLink = $(slide).find("img").attr('rel');
				var slideImage = $(slide).find("img").attr('src');
				var slideOrientation = $(slide).find("img").attr('class');
	
				var slideTitle = $(slide).find("p.title").html();
				var slideDate = $(slide).find("p.update").html();
				var slideDescription = $(slide).find(".description").html();
	
				// markup for thumb previews
				var item = '';
				item += '<li>';
	
				if(slideImage){
					item += '<a href="#" class="' + slideOrientation + ' preview' + '">';
					item += '<img src="' + slideImage + '" align="left" />';
					item += '</a>';
				}
	
				item += '<p class="title">' + slideTitle + '</p>';
				    item += '<p class="date">' + slideDate + '</p>';
				
				
				item += '<a href="' + slideLink + '" class="source-link">More</a>';
				item += '</li>';
				return item; 
			}
		});
	
		// stuff to do before the slide transition
		function onBefore(curr, next, opts) {
			// grab current slide object and do stuff to the title
			var currentSlide = $('#block-slideshow-playlist .slide').eq(opts.currSlide).find('.slidecaption');
			currentSlide.animate({ 
					bottom: "-65px"
				}, 250 );
		}
	
		// stuff to do after the slide transition
		function onAfter(curr, next, opts) {
			if($('#block-slideshow-playlist .slide-navigator li').eq(opts.currSlide).attr('class')!='advertisement'){
				// shows and hides previous and next buttons appropriately
				var index = opts.currSlide;
		
				// grab current slide object and do stuff to the title
				var currentSlide = $('#block-slideshow-playlist .slide').eq(opts.currSlide).find('.slidecaption');
				currentSlide.animate({bottom: "0px"}, 400 );
			}
			$('#block-slideshow-playlist .activated').removeClass('activated');
			$('#block-slideshow-playlist .slide-navigator li').eq(opts.currSlide).addClass('activated');
		}
	
		$('.source-link').click(function(){
			location.href = $(this).attr('href'); 
		})
		
	
		$('.slide-navigator li').hover(function(){
			$('#block-slideshow-playlist .slideshow').cycle('pause');
			
		});
		
		$('#block-slideshow-playlist .slide-navigator li:contains(Advertisement)').addClass('advertisement');
		$('#block-slideshow-playlist .slide-navigator li.advertisement').html('Advertisement');
	});
	//]]>
	</script>

