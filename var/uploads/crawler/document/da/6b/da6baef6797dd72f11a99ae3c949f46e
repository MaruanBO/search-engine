<script>
(function(window, document, $, undefined) {
    $(function() {
    
        var $selectDateStringD1 = $('#tncms-block-270865 .search-form-wrapper .select-date-string-d1'),
            $selectDateStringD2 = $('#tncms-block-270865 .search-form-wrapper .select-date-string-d2'),
            $inputDateIntegerD = $('#tncms-block-270865 .search-form-wrapper .input-date-integer-d'),
            $inputDateIntegerD1 = $('#tncms-block-270865 .search-form-wrapper .input-date-integer-d1'),
            $inputDateIntegerD2 = $('#tncms-block-270865 .search-form-wrapper .input-date-integer-d2'),
            $searchCustomDateWrapper = $('#tncms-block-270865 .search-form-wrapper .search-custom-date-wrapper'),
            
            $searchInput = $('#tncms-block-270865 .search-form-wrapper #search-input'),
            $searchSubmit = $('#tncms-block-270865 .search-form-wrapper #search-submit'),
            
            $searchForm = $('#tncms-block-270865 .search-form-wrapper #search-form'),
            $searchFormInputValue = $('#tncms-block-270865 .search-form-wrapper #search-form :input[value=""]');
        
        var hrBorder = $('#tncms-block-270865 .search-form-wrapper.ui-widget-content') ? $('#tncms-block-270865 .search-form-wrapper.ui-widget-content').css('border-top-color') : 'rgb(0, 0, 0)';
            
        $('.search-hr').each(function(i) {
            $(this).css('border-top-color', hrBorder);
        });
            
        /* Setup datepickers for all the date input boxes */
        $inputDateIntegerD.datepicker({
            maxDate: '+0d',
            showButtonPanel: true,
            numberOfMonths: 3
        });
        
        $inputDateIntegerD1.datepicker({
            maxDate: '-1d',
            showButtonPanel: true,
            changeMonth: true,
            changeYear: true
        });
        
        $inputDateIntegerD2.datepicker({
            maxDate: '+0d',
            showButtonPanel: true,
            changeMonth: true,
            changeYear: true
        });

        /* Disable single date and from and to date input boxes unless 'custom date' is selected */
        if ($selectDateStringD1.val() != 'custom') {
            $inputDateIntegerD1.val('').prop('disabled', true);
            $inputDateIntegerD2.val('').prop('disabled', true);
            $inputDateIntegerD.val('').prop('disabled', true);
        }
        
        /* If single date is selected, disable from and to date input boxes */
        $inputDateIntegerD.focusin(function() {
            $inputDateIntegerD1.val('').prop('disabled', true);
            $inputDateIntegerD2.val('').prop('disabled', true);
        }).focusout(function() {
            $inputDateIntegerD1.val('').prop('disabled', false);
            $inputDateIntegerD2.val('').prop('disabled', false);
        });
        
        /* If from and to date input boxes are selected, disable single date input box */
        $inputDateIntegerD1.focusin(function() {
            $inputDateIntegerD.val('').prop('disabled', true);
        }).focusout(function() {
            $inputDateIntegerD.val('').prop('disabled', false);
        });
        $inputDateIntegerD2.focusin(function() {
            $inputDateIntegerD.val('').prop('disabled', true);
        }).focusout(function() {
            $inputDateIntegerD.val('').prop('disabled', false);
        });

        /* jUI-fy the buttons, inputs and select menus */
        
        $searchInput.tooltip();
        $inputDateIntegerD.tooltip();
        $inputDateIntegerD1.tooltip();
        $inputDateIntegerD2.tooltip();
        $searchSubmit.button();
        
        $selectDateStringD1.selectmenu({
            width: 172,
            change: function() {
                if ($selectDateStringD1.val() == 'custom') {
                    $inputDateIntegerD1.val('').prop('disabled', false);
                    $inputDateIntegerD2.val('').prop('disabled', false);
                    $inputDateIntegerD.val('').prop('disabled', false);
                    $selectDateStringD1.prop('disabled', true);
                    $selectDateStringD2.prop('disabled', true);
                    $searchCustomDateWrapper.fadeIn();
                } else if ($selectDateStringD1.val() != 'custom') {
                    $searchCustomDateWrapper.fadeOut();
                }
            }
        });
        
        $searchForm.fadeIn();
        $searchSubmit.click(function() {
            //$searchFormInputValue.attr('disabled', true);
            $searchForm.submit();
        });
        
        
    });
})(window, document, jQuery);
</script></div><div id="tncms-block-109196" class="tncms-block calendar double-pad-bottom no-border"><script type="text/javascript" charset="utf-8">
var jEvents = [];
$(function(){
     $("#cal-loader").bind("ajaxStart", function(){
      $(this).parent().parent().find('#calendar-widget-block').addClass('transparent'); 
      $(this).show();
   }).bind("ajaxStop", function(){
      $(this).parent().parent().find('#calendar-widget-block').removeClass('transparent');   
      $(this).hide();
    $('#calendar-widget').datepicker(
        {
        	
            numberOfMonths: 1,
            inline: true,
            showSpeed: 'fast',
            beforeShow: function(input, inst) {(this).delay(1000);},
            beforeShowDay: editDays,
            onChangeMonthYear: fetchDays,                                        
            onSelect: function(dateText, inst) {
                $("#calWidgetSearchDate").val(dateText);
                $("#calWidgetSearchForm").submit();
            }
        }
    );
});
    
function fetchDays(year, month) {
    jEvents = [];
    i = null;
    if (year == undefined && month == undefined) {
    var d = new Date();
    var n = d.getMonth()+1;
    var y = d.getFullYear();
    month = n; year = y;
        }
    $.ajax({
        type: 'GET',
        dataType: 'json',
        url: '/calendar/search/?year='+year+'&month='+month+'&fmt=json',
        cache: true,
        success: function(data) {
        $.each(data, function(index, value) {
        jEvents.push(value.result.numEvents);
        //console.log(jEvents+'test');
                });
         $("#calendar-widget").datepicker("refresh");
          },
      error: function(response) {
        //console.log(response.status);
          }
    });
}

fetchDays();
    function editDays(date) {
    var i = date.getDate()-1;
        //console.log('date : '+i+' -> Array Value : '+jEvents[i]);
            if (jEvents[i] == 0 ) {   
                 return [false, , '(no events)'];
            }    
                    var tempEvents = 'Event';
                    if (jEvents[i] > 1){tempEvents = 'Events';} 
                    return [true,'events-'+i,jEvents[i]+' '+tempEvents];
                     return [true,''];
     } 

});
    </script>
