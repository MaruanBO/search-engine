<script language="javascript"> 
<!--
 
/* ************************************************************************************* */
/* ************************************************************************************* */
/* PRESET VALUE CHECKER/SWITCHER */
/* ************************************************************************************* */
/* ************************************************************************************* */

/* Presets default values */
var presetWhatDefault = "(e.g. Dentist, Bob's Market, Tattoo parlor)";
var presetWhereDefault = ", ";
/* Define other variables to store values */
var formWhatDefault = "";
var formWhereDefault = ""; 
var gotDefaultValues = false; 
var formWhat = "";
var formWhere = "";

function assignDefaults() /* Get the form defaults */
{
	formWhatDefault = document.getElementById('l-what').value; 
	formWhereDefault = document.getElementById('l-where').value; 
	gotDefaultValues = true;
} /* /END assignDefaults */

function formValueDefaults(checkStatus,currentElement) /* Show hide/defaults */
{

	/* Grab the defaults the first time */
	if (!gotDefaultValues) { assignDefaults(); } 

	/* Get object data */
	formWhat = document.getElementById('l-what'); 
	formWhere = document.getElementById('l-where'); 
	
	/* Check it, change it */
	if (checkStatus == "start")
	{
		switch(currentElement) 
		{
			case 'what':
				/* Page default is same as preset default */
				if (formWhat.value == presetWhatDefault) {  
					formWhat.value = ""; 
					formWhat.className='form-text-user';
				}	
				/* Is the value the loaded page default? */
				formWhat.focus();
		    	break;
		    case 'where':
				/* Page default is same as preset default */
				if (formWhere.value == presetWhereDefault) {  
					formWhere.value = ""; 
					formWhere.className='form-text-user';
				}	
				/* Is the value the loaded page default? */
				formWhere.focus();
			    break;
		}
	}
	else
	{
		switch(currentElement) 
		{
			case 'what':
				/* Is it blank? Fill it back in with the default */
				if (formWhat.value == "") {	
					formWhat.value = presetWhatDefault; 
					formWhat.className='form-text';
				}
    			break;
		    case 'where':
				/* Is it blank? Fill it back in with the default */
				if (formWhere.value == "") {	
					formWhere.value = presetWhereDefault; 
					formWhere.className='form-text';
				}
	    		break;
		}
	} /* END checkStatus */
} /* /END formValueDefaults */	
	
/* ************************************************************************************* */
/* ************************************************************************************* */

// --></script>



