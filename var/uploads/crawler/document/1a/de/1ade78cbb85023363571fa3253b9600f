				jQuery(function() {
					var tblz_ps_tab_social  = jQuery('#tblz_ps_tab_social');
					var tblz_ps_tab_popular = jQuery('#tblz_ps_tab_popular');
					var tblz_ps_tab_right   = jQuery('#tblz_ps_tab_right');
					var tblz_ps_tab_left    = jQuery('#tblz_ps_tab_left');
								
				     tblz_ps_tab_right.click(function() {
				     	tblz_ps_tab_social.css('display', 'block');
					    tblz_ps_tab_popular.css('display', 'none');
				     	tblz_ps_tab_right.addClass('tblz_ps_tab_active');
						tblz_ps_tab_left.removeClass('tblz_ps_tab_active');
						_gat._getTrackerByName()._trackEvent('Site Features', 'Social', 'PopSocial Widget');
				     	return false;
				     });
	
				     tblz_ps_tab_left.click(function() {
						tblz_ps_tab_popular.css('display', 'block');
						tblz_ps_tab_social.css('display', 'none');
						tblz_ps_tab_left.addClass('tblz_ps_tab_active');
						tblz_ps_tab_right.removeClass('tblz_ps_tab_active');
						_gat._getTrackerByName()._trackEvent('Site Features', 'Popular', 'PopSocial Widget');
						return false;
				     });
								
					jQuery('#tblz_widget_form_signup').submit(function() {
						jQuery.post(
							jQuery('#tblz_widget_form_signup').attr('action'),
							jQuery('#tblz_widget_form_signup').serialize(),
							function(data) { alert(data); }
						);
						_gat._getTrackerByName()._trackEvent('Site Features', 'Sign Up', 'PopSocial Widget');
						return false;
					});
				});
