<script>
function getCookie(sKey) {
if (!sKey) { return null; }
return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
}
//Visitor Types = 1 - New | 2 - Existing//
var PageType = 'CMS - UGG';
var FirstTimer = getCookie("FirstTimer");
var Visitor_Type = '2';
if (FirstTimer == null) {
document.cookie= "FirstTimer=yes";
Visitor_Type = '1';}
//Visitor Status 1:not logged in; 2-logged || Visitor Gender 1 - Male; 2- Female; 3 - Unknown
var Visitor_Status = '1';
var Visitor_Gender = '3';
jQuery(document).ready(function() {
dataLayer.push ({
	'PageType' : PageType,
	'Visitor_Status' : Visitor_Status,
    'Visitor_Type' : Visitor_Type,
    'Visitor_Gender' : Visitor_Gender
});
if (Visitor_Status == 2) {
    dataLayer.push ({ 'Visitor_ID' : Visitor_Id });
}
if (PageType=='Category') {
    dataLayer.push ({ 'Gender_Category': Gender_Category, 'Product_Category': Product_Category });
}
});
</script>    
