<script type="text/javascript" src="/floodsmart/js/jquery.cycle.all.js"> </script>

<script type="text/javascript">
$(document).ready(function() {
	//build out links for custom pager nav items
	function paginate(ind, el) {
		// link for spring flooding slide <li class="slide-link" id="slide-one"><a href="#">Springtime Flooding</a></li>
		if (ind == 0) return '<li class="slide-link" id="slide-one"><a href="#">Protect What Matters</a></li>';
        else if (ind == 1) return '<li class="slide-link" id="slide-two"><a href="#">Winter Rainy Season</a></li>';
        else if (ind == 2) return '<li class="slide-link" id="slide-three"><a href="#">Recent Changes</a></li>';
		else if (ind == 3) return '<li class="slide-link" id="slide-four"><a href="#">New Flood Maps</a></li>';
        else if (ind == 4) return '<li class="slide-link" id="slide-five"><a href="#">Flood After Fire</a></li>';
		//else if (ind == 3) return '<li class="slide-link" id="slide-four"><a href="#">Video Testimonials</a></li>';
	}
	
	$('#slide-container').css("z-index","0");
 	
	$('#slide-container').cycle({ 
		fx: 'scrollHorz', 
		prev: 'li#prev',
		next: 'li#next',
		activePagerClass: 'activeSlide',
		pager:  'ul#slide-pager',
		sync: 'true',
		after: checkSlide,
		pagerAnchorBuilder: paginate,
		pauseOnPagerHover: 1,
		pause: 1,
		timeout: 5000
	});

	$('.slide-link a').click(function(){
		hideFlashSlide();
	});
	$('li#prev').click(function(){
		hideFlashSlide();
	});
	$('li#next').click(function(){
		hideFlashSlide();
	});

	function checkSlide() {
		// these will change depending on the length of the words
		// 120, 265, 390, 530
		if ( $('li#slide-one').children('a').hasClass('activeSlide') ) {
			$('#marker-container img').animate({marginLeft: '121px'});
		} else if ( $('li#slide-two').children('a').hasClass('activeSlide') ) {
			$('#marker-container img').animate({marginLeft: '262px'});
		} else if ( $('li#slide-three').children('a').hasClass('activeSlide') ) {
			$('#marker-container img').animate({marginLeft: '387px'});
		} else if ( $('li#slide-four').children('a').hasClass('activeSlide') ) {
			$('#marker-container img').animate({marginLeft: '511px'});
        } else if ( $('li#slide-five').children('a').hasClass('activeSlide') ) {
            $('#marker-container img').animate({marginLeft: '622px'});
        }
	}
});
</script>

