	function querystring(key) 
	{
	   var re=new RegExp('(?:\\?|&)'+key+'=(.*?)(?=&|$)','gi');
	   var r=[], m;
	   while ((m=re.exec(document.location.search)) != null) r.push(m[1]);
	   return r;
	}
	
	/*
	*	The following snippet of code is used to center any dialogs on the screen
	*	from the JQuery UI library when the browser window is resized.
	*
	*/
	if (!$.browser.msie || ($.browser.msie  && parseInt($.browser.version, 10) !== 7)) 
	{
		//JQuery 1.7.1 and below
		$.ui.dialog.prototype.options.autoReposition = true;
	
		//JQuery 1.7.2 and higher
		//$.ui.dialog.defaults.autoReposition = true;
	
		$( window ).resize(function() {
			$( ".ui-dialog-content:visible" ).each(function() {
				var dialog = $( this ).data( "dialog" );
				if ( dialog.options.autoReposition ) {
					dialog.option( "position", dialog.options.position );
				}
			});
		});
	}
	
	// script to activate cycle slide show
	$(document).ready(function() {
		$(".tooltip-trigger").each(function()
		{
			$(this).tooltip({opacity: 0.8});
		});
		
		$(".tooltip-trigger-opaque").each(function()
				{
					$(this).tooltip();
				});
		
		//Hide the font toggle on the newer pages. It does not work for those pages.
		if(window.location.toString().indexOf("ffr_overview") > -1 || window.location.toString().indexOf("what_causes_flooding") > -1
				|| window.location.toString().indexOf("flood_after_fire") > -1 || window.location.toString().indexOf("heavy_rains") > -1
				|| window.location.toString().indexOf("ice_jams") > -1 || window.location.toString().indexOf("la_nina") > -1
				|| window.location.toString().indexOf("levees") > -1 || window.location.toString().indexOf("mud_flows") > -1
				|| window.location.toString().indexOf("snow_melt") > -1 || window.location.toString().indexOf("spring_thaw") > -1
				|| window.location.toString().indexOf("tropical_storms") > -1 || window.location.toString().indexOf("west_coast") > -1
				|| window.location.toString().indexOf("defining_flood_risks") > -1 || window.location.toString().indexOf("videos") > -1
				|| window.location.toString().indexOf("flood_scenarios") > -1 || window.location.toString().indexOf("cost_of_flooding") > -1
				|| window.location.toString().indexOf("levee_simulator") > -1 || window.location.toString().indexOf("cc_overview") > -1)
		{
			$("#font-toggle").hide();
		}
		
		$('INPUT.auto-hint, TEXTAREA.auto-hint').focus(function(){
		    if($(this).val() == $(this).attr('title')){
		        $(this).val('');
		        $(this).removeClass('auto-hint');
		    }
		});
		
		$('INPUT.auto-hint, TEXTAREA.auto-hint').blur(function(){
		    if($(this).val() == '' && $(this).attr('title') != ''){
		       $(this).val($(this).attr('title'));
		       $(this).addClass('auto-hint');
		    }
		});
		
		$('INPUT.auto-hint, TEXTAREA.auto-hint').each(function(){
		    if($(this).attr('title') == ''){ return; }
		    if($(this).val() == ''){ $(this).val($(this).attr('title')); }
		    else { $(this).removeClass('auto-hint'); }
		});

		// search form scripts
		$('input#gobutton').mouseover(function() {
			$(this).attr('src','/floodsmart/images/search-go-button-over.jpg');
		}).mouseout(function() {
			$(this).attr('src','/floodsmart/images/search-go-button.jpg');
		});
		
		$('#updateAddressLinkImg').mouseover(function() 
				{
					$(this).attr('src','/floodsmart/images/flood_risks/change-addess-button-roll.jpg');
				}).mouseout(function() 
				{
					$(this).attr('src','/floodsmart/images/flood_risks/change-addess-button.jpg');
				});
				
		$('#update-address').mouseover(function() 
				{
					$(this).attr('src','/floodsmart/images/flood_risks/update-button-roll.jpg');
				}).mouseout(function() 
				{
					$(this).attr('src','/floodsmart/images/flood_risks/update-button.jpg');
				});
		
		// home page risk profile scripts
		$('select#findagent_nav_state option:first-child').html('State/Territory');
		
		$('#flood-risk-container input#findagent_0').mouseover(function() {
			$(this).attr('src','/floodsmart/images/risk-form/go-button-over.jpg');
		}).mouseout(function() {
			$(this).attr('src','/floodsmart/images/risk-form/risk-go-button.jpg');
		});
		
		$('#errorDisplay').click(function() {
			$(this).fadeOut('slow');
		});
		
		$('input#q').blur(function() {
			if ( $(this).val() === '' ) {
				$(this).val('Search FloodSmart.gov');
			}
		});
		
		return false;
	});
	
	function showFlashSlide(movie, firstframe, tr, cc) {
		$('#slide-container').animate({'height': '412px'});
		$('#video-player').show();
		$('#video-player').flash({
			id: 'floodsmartVideoPlayer',
			name: 'floodsmartVideoPlayer',
			swf: "/floodsmart/flash/FloodsmartVideoPlayer.swf?c=" + new Date().getTime().toString(),
			width: 732,
			height: 412,
			menu: false,
			allowFullScreen: true,
			allowScriptAccess: "sameDomain",
			wmode: "opaque",
			expressInstaller: "/floodsmart/flash/expressInstall.swf",
			bgColor: "#ffffff",
			align: "middle",
			quality: "high",
			flashvars: {
				video: movie,
				autoPlay: "false",
				autoHide: "true",
			 	firstFrame: firstframe,
			 	transcript: tr,
			 	overlay: "true",
			 	cc: cc
			}
		});
	}

	function hideFlashSlide() {
		$('#slide-container').animate({'height': '303px'});
		$('#video-player').flash().remove();
		$('#video-player').hide();
	}
	
	function checkError() {

        
	}
	
	function clearField(what) {
		document.getElementById(what).value = '';
	}

    function validateSearch() {
        if ($('#search_form #query').val() == '') {
            alert('You must enter a search query');
            return false;
        }
    }
	</script>

