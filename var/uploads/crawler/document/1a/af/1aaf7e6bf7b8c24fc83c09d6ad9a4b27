</script><script type="text/javascript">

//load United States on page load
$(document).ready(function() {

$("select").each(function () {
  $(this).val($(this).find('option').val());
});

var loadedFeed = 'map/us.xml';

$('#countries').change(function (){

if ($("option:selected", this).attr("rel").substr(0,1) == '/') {
	
	location.href = $("option:selected", this).attr("rel");
	
} else if ($("option:selected", this).attr("rel").substr(0,1) == 'h') {

	location.href = $("option:selected", this).attr("rel");

} else {
	
	loadedFeed = $("option:selected", this).attr("rel");
	slider.goToSlide(this.selectedIndex);
	
}

});

$('area').bind('click', function (){	

$(".stateTitle").html('');	
$("#output").html('');	
$(".stateTitle").show();
$(".ctaDesc").show();
$("#output").fadeIn('slow');
$('#stateContent').fadeIn('fast');

var clickedState = $(this).attr('rel');

	$.ajax({
		
		type: "GET",
		url: loadedFeed,
		dataType: "xml",
		success: function(xml) {

	  $(xml).find("state").each(function() {
		
		if($(this).attr("name") == clickedState){
			
			$(".stateTitle").append($(this).attr("fullname"));
			var activeState = $(this).attr("active");
			
			if(activeState == "yes"){
				$(this).find("market").each(function(index, element) {
				$("#output").append('<li>' + '<a href="'+$(this).attr("url")+'">'+$(this).attr("name")+'</a></li>');
			});
			}else{
				$("#output").append('<li style="width:100%; color:#fff;  font-size:20px; line-height:20px;"><p> Unfortunately, we are not yet in this market. If you would like Golfnow.com to expand to your market, suggest your city for our next expansion.</p><a href="http://www.golfnow.com/support/feedback"><img src="http://www.golfnow.com/map/images/btn-submit-sug.png" /></a></li>');
			}
		}
	});
}
});
});

$('.closeBtn').click(function() {
  $('#stateContent').fadeOut('slow'); 
  $("#output").fadeOut('slow');
  $(".stateTitle").fadeOut('slow');
  $(".ctaDesc").fadeOut('slow');
});

var slider = $('#maps').bxSlider({
       controls: false,
	   mode: "fade"
});

});

$(document).ready(function(e) {
  $("#countries").msDropDown();
});

