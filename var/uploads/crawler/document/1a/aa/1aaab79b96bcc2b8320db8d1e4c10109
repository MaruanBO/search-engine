<script>

$(document).ready(function(){
  promoBanner.init('1');
});


var promoBanner = {
  init: function(variation, id){
    promoBanner.id = id;
    promoBanner.variation = variation;
    promoBanner.handlers();

    if(promoBanner.isEligible()) { promoBanner.setBanner(); }
  },
  handlers: function(){
    $('.btn-banner').click(function(){
      $('.banner-wrapper.promo-'+ promoBanner.variation).toggleClass('active');
      sessionStorage[promoBanner.id] = true;
      _gaq.push(['_trackEvent', 'Per_Top_Banner', 'Banner_Click', promoBanner.id]);
      dataLayer.push({
          'event': 'GAevent',
          'eventCategory': 'Per_Top_Banner',
          'eventAction': 'Banner_Click',
          'eventLabel': promoBanner.id
      }); 
    });

    $('.close').click(function(){
      $('.banner-wrapper.promo-'+ promoBanner.variation).toggleClass('active');
      sessionStorage[promoBanner.id] = false;
      _gaq.push(['_trackEvent', 'Per_Top_Banner', 'Banner_Close', promoBanner.id]);
      dataLayer.push({
          'event': 'GAevent',
          'eventCategory': 'Per_Top_Banner',
          'eventAction': 'Banner_Close',
          'eventLabel': promoBanner.id
      }); 
    });
  },
  isEligible: function(){
    var promoEligible = (sessionStorage[promoBanner.id] === 'true' || sessionStorage[promoBanner.id] === undefined);
    var pageEligible = promoBanner.pageFilter();
    var userEligible = !$('.atg-data').data('unlimited');
    return (promoEligible && pageEligible && userEligible) ? true : false;
  },
  pageFilter: function() {
    var url = location.href;
    var blocked = ['cm_template', 'checkout', 'compliance', 'ExpiryPage'];
    var bool = true;

    for(i=0; i < blocked.length; i++){
      if(url.indexOf(blocked[i]) > 0) {
        bool = false;
      } 
    }

    return bool;
  },
  setBanner: function(){
    $('.banner-wrapper.promo-'+ promoBanner.variation).addClass('active');
  },
  id: ""
}

