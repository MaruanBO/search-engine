<script type="text/javascript">
	$(document).ready(function () {
		$("a[href*='.pdf'],a[href*='.doc'],a[href*='.ppt'],a[href*='.xls'],a[href*='.wmv'],a[href*='.kmw']").click(function () {
			var fileType = '';
			if ($(this).attr('href').indexOf(".pdf") >= 0)
				fileType = 'PDF';
			if ($(this).attr('href').indexOf(".doc") >= 0)
				fileType = 'DOC';
			if ($(this).attr('href').indexOf(".ppt") >= 0)
				fileType = 'PPT';
			if ($(this).attr('href').indexOf(".xls") >= 0)
				fileType = 'XLS';
			if ($(this).attr('href').indexOf(".wmv") >= 0)
				fileType = 'WMV';
			if ($(this).attr('href').indexOf(".kmw") >= 0)
				fileType = 'KMW';

			if (fileType != '') {
				var filename = $(this).attr('href');
				_gaq.push(['_trackEvent', 'Downloads', fileType, filename])
			}
		});
	});
</script>
