    <script language="javascript" type="text/javascript">
    (function(global) {
        global.MAT = global.MAT || {};
        global.MAT.JsTools = {
            addDotNotationObject : function(keyValues, current) {
                for(var key in keyValues) {
                    var value = keyValues[key];
                    this.addKeyValue(key, value, current);
                }
            },
            addKeyValue : function(key, value, current) {
                var parts = key.split('.');
                this.addParts(parts, value, current);
            },
            addParts : function(parts, value, current) {
                if(parts.length === 1) {
                    current[parts[0]] = value;
                } else {
                    if(current[parts[0]] === undefined) {
                        current[parts[0]] = {};
                    }
                    this.addParts(parts.slice(1), value, current[parts[0]]);
                }
            }
        };
