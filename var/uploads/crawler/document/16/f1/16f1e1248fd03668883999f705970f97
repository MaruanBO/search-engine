<script type="text/javascript">
function trim(str){
if(str != undefined){
return str.replace(/^\s\s*/, '').replace(/\s\s*$/, '')
}else{
return str;
}
}
function getCountryHawkValue() {
var allCookies = document.cookie.split(';');
var countryHawkValue = '';
for (var i = 0; i < allCookies.length; i++) {
var cookies = allCookies[i].split('=');
var attrName = trim(cookies[0]);
if( attrName == 'LocaleInformation') {
var localeInformationValues = unescape(cookies[1]).split('|');
for(var n = 0; n < localeInformationValues.length; n++){
var valueNamePair = localeInformationValues[n].split('=');
if(trim(valueNamePair[0]) == 'COUNTRYHAWKCODE'){
countryHawkValue = trim(valueNamePair[1]);
return countryHawkValue;
}
}
}
}
return countryHawkValue;
}
</script>
