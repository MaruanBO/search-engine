// determine current URI without sid, hash-symbol and country/language selection and then encode it
var pathname = window.location.pathname.replace(/;sid=[^?]*/gi,'');
pathname = pathname.slice(10);
var url = window.location.protocol + '//' + window.location.hostname + pathname + window.location.search;
url = encodeURIComponent(url);
var title = encodeURIComponent(window.document.title);
var shareImage = "";

shareImage = jQuery('.article, .coverstory').find('img:first').attr('src');
if (((shareImage == "") || (shareImage === undefined)) && (typeof mySlide !== 'undefined')) {
shareImage = mySlide.images[0].src;
}

if ((shareImage == "") || (shareImage === undefined)) {
shareImage = "http://www.swarovski.com:80/is-bin/intershop.static/WFS/SCO-Web_AA-Site/-/en_US/img/basics/swarovski.png";
}
config['share.ajaxparams'] = {"recUrlIso":url,"recTitlePlain":title,"shareImage":shareImage};
