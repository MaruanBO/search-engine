function triggerWebtracking() {
_tag.DCSext.listoption_cat = undefined;
_tag.DCSext.listoption_val = undefined;
_tag.DCSext.productfilter_cat = undefined;
_tag.DCSext.productfilter_val = undefined;
var params = new Array();
for (var i = 0; i < triggerWebtracking.arguments.length; i++) {
var keyValues = triggerWebtracking.arguments[i].split("&");
for (var j = 0; j < keyValues.length; j++) {
var keyValue = keyValues[j].split("=");
if (keyValue.length == 2) {
params[decodeURI(keyValue[0])] = decodeURI(keyValue[1]);
}
}
}
var scriptText = "dcsMultiTrack('WT.ti', _tag.WT.ti, 'DCS.dcsuri', _tag.DCS.dcsuri, 'WT.es', _tag.WT.es";
for (var key in params) {
if (params.hasOwnProperty(key)) {
scriptText += ", '" + key + "', '" + params[key] + "'";
}
}
scriptText += ");";
