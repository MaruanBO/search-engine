    var bkProp2 = (typeof s.prop2 != "undefined" ? s.prop2 : "");
    var bkProp3 = (typeof s.prop3 != "undefined" ? s.prop3 : "");
    var bkProp4 = (typeof s.prop4 != "undefined" ? s.prop4 : "");

