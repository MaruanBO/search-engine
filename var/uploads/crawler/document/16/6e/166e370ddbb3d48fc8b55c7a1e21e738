            <script type='text/javascript'>
                $(function() {
                    var ua = window.navigator.userAgent;
                    var msie = ua.indexOf("MSIE ");
                    if(msie>0){
                        $(".ddarrow").hide();
                        $("select").css("background", "#fff url('http://media.carfax.com/img/cfx/tnt/dropdown.png') no-repeat 79% 50%");
                    }

                    $('#j-form-home').on('submit', function() {
                        if(!$('#location').hasClass('placeholder') && $('#location').val() ) {
                            var make = $('#make').val() || 'None';
                            var model = $('#model').val() || 'None';
                            var hasAccidents = $('#has-accidents').hasClass('true') ? 'Accident' : 'None';
                            var oneOwner = $('#is-one-owner').hasClass('true') ? 'Owners' : 'None';
                            var hasService = $('#has-service-records').hasClass('true') ? 'Service' : 'None';
                            var personal = $('#has-personal-use').hasClass('true') ? 'Use' : 'None';
                            s=s_gi('carfaxcom');
                            s.linkTrackVars='eVar46,events';
                            s.linkTrackEvents='event56';
                            s.eVar46= make + ' | ' + model + ' | ' + 'None' + ' | ' + hasAccidents + ' | ' + hasService + ' | ' + oneOwner + ' | ' + personal;
                            s.events='event56';
                            s.tl('true', 'o', 'Find Car Search - Homepage Nav');
                        }

                    });
                });

                $(".ucl-cpo").hide();
            </script>
        </form>
