    <script type="text/javascript">
        $(function () {
            // Set up the billboard.
            $('#billboard ul li a').click(function () {
                var previous = $('#billboard ul li.selected');
                var current = $(this);
                var imageLink = $(this).attr('data-link');
                var imageFile = $(this).attr('data-image');
                var imageAlt = $(this).attr('data-alt');
                var imageTarget = $(this).attr('data-target');
                var image = $('#billboard img');
                var link = $('#billboard > a');
                previous.removeClass('selected');
                current.parent().addClass('selected');
                image.attr('src', imageFile).attr('alt', imageAlt);
                link.attr('href', imageLink).attr('target', imageTarget);
            });
        });
    </script>

