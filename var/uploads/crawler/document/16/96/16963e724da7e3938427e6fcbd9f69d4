<!-- Code for the footer tabs. -->
<script type="text/javascript">
    // Set up dismiss buttons in tab panes.
    $(document).ready(function () {
        // Hide a tab when the dismiss button in its tab pane is clicked.
        $('button[data-dismiss="tab-content"]').on('click', function (e) {
            $(this).parents('div.tab-pane').removeClass('active');
            $("a[data-toggle]='pill'").parent('li').removeClass('active');
        })
    });

    // Extend the bootstrap tab plugin.
    (function ($) {
        var tabExtensions = {
            // Modify the show method to implement toggling (if the 'nav-toggle' class is present).
            oldShow: $.fn.tab.Constructor.prototype.show,
            show: function (event) {
                var $this = this.element;
                if ($this.parents('ul.nav').hasClass('nav-toggle') && $this.parent('li').hasClass('active')) {
                    this.hide(event);
                }
                else {
                    this.oldShow(event);
                }
            },

            // Add the hide method.
            hide: function (event) {
                var $this = this.element;
                $this.parent('li').removeClass('active');
                var id = $this.attr('href');
                $(id).removeClass('active');
                if (!this.blur) {
                    $this.blur();
                }
            }
        };
        $.extend($.fn.tab.Constructor.prototype, tabExtensions);
    })(jQuery);
</script>

