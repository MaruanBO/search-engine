<!-- Code for the header nav. -->
<input type="hidden" id="MenuSelection" data-menuid="" data-quicklinkid="" />
<script type="text/javascript">

    // Initialize the navigation display.
    $(document).ready(function () {
        var menuId = $("#MenuSelection").data().menuid;
        var quicklinkId = $('#MenuSelection').data().quicklinkid;

        // If a menu tab was specified, highlight it and show it's quick links menu.
        if (menuId != '') {

            // Highlight the menu link.
            var menu = $('#' + menuId).addClass('intcomm-nav_selected');

            // Now see if it has a quicklink menu associated with it.
            var dataMenuId = $('#' + menuId).attr('data-menu');
            if (dataMenuId != '' && $('#' + dataMenuId).length > 0) {

                // Hide the most popular quick link menu (necessary because it is shown by default).
                $('#intcommMostpopular').removeClass('intcomm-menu_selected').addClass('intcomm-menu_hidden');

                // Show the quick link menu associated with the menu link.
                $('#' + dataMenuId).removeClass('intcomm-menu_hidden').addClass('intcomm-menu_selected');

                // If a specific quick link was specified, highlight it.
                if (quicklinkId != '') {
                    $('#' + quicklinkId).addClass('intcomm-nav_selected');
                }
            }
        }
    });

    // Set up the menu activation.
    $(document).ready(function () {

        // If we are in mobile view, remove any initial menu item and quicklink activations.
        var isMobileView = ($('#intcommPrimarynav p.intcomm-nav_title:visible').length > 0);
        if (isMobileView) {
            $('#intcommPrimarynav div ul li a').removeClass('intcomm-nav_selected');
            $('#intcommQuicklinks div.intcomm-menu_selected').removeClass('intcomm-menu_selected').addClass('intcomm-menu_hidden');
        }

        // Set up the header nav menus.
        $('#intcommPrimarynav div ul li a').click(function () {

            // Restore the postion of any quicklinks moved due to mobile view.
            var movedQls = $('#intcommMenu .intcomm-menu_selected');
            $('#intcommQuicklinks').append(movedQls);

            // Determine if we are using mobile view.
            var isMobileView = ($('#intcommPrimarynav p.intcomm-nav_title:visible').length > 0);

            // Determine if this menu item is currently active.
            if ($(this).hasClass('intcomm-nav_selected')) {

                // Yes, deactivate it and its quick links.
                $(this).removeClass('intcomm-nav_selected');
                $('#intcommQuicklinks div.intcomm-menu_selected').removeClass('intcomm-menu_selected').addClass('intcomm-menu_hidden');

                // Activate the most popular quick links.
                $('#intcommMostpopular').removeClass('intcomm-menu_hidden').addClass('intcomm-menu_selected');
            }
            else {

                // Deactivate the currently active menu item and quicklinks (if any).
                $('#intcommPrimarynav div ul li a').removeClass('intcomm-nav_selected');
                $('#intcommQuicklinks div.intcomm-menu_selected').removeClass('intcomm-menu_selected').addClass('intcomm-menu_hidden');

                // Activate this menu item and its quicklinks.
                $(this).addClass('intcomm-nav_selected');
                var ql = $('#' + $(this).attr('data-menu'));
                ql.addClass('intcomm-menu_selected').removeClass('intcomm-menu_hidden');

                // For mobile view, move the newly activated quicklinks.
                if (isMobileView) {
                    var isInQuicklinks = (ql.parents('#intcommQuicklinks').length > 0);
                    if (isInQuicklinks) {
                        $(this).parent('li').append(ql);
                    }
                }
            }

            // Remove focus from this menu item.
            this.blur();
        });
    });
</script>





