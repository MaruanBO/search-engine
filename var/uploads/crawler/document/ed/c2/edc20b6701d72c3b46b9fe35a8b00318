<script type="text/javascript">
function jQH_MMM_MoveBlogPosts() {
    console.log('--- in jQH_MMM_MoveBlogPosts.');
    var blogSelector = '';
    var sectionSelector = '';

    // If on a page with at least 2 blog posts in a blog post list:
    if (x$('.page-home article.blogSection-entry').length  > 1) {
        blogSelector = '.page-home .section-blogBundle article.blogSection-entry';
        sectionSelector = '.section-blogBundle';
    }
    else if (x$('.page-profiles-blog-list article.entry').length > 1) {
        blogSelector = '.page-profiles-blog-list .section-primaryContent article.entry';
        sectionSelector = '.section-primaryContent';
    }
    else if (x$('.page-profiles-blogs-list article.entry').length > 1) {
        blogSelector = '.page-profiles-blogs-list .section-primaryContent article.entry';
        sectionSelector = '.section-primaryContent';
    }

    if (blogSelector != '') {
        console.log('blogSelector: ' + blogSelector);

        // First see if there are > 8 posts.  If so, move 9th and more to interBlog3_after:
        if (x$(blogSelector).length > 8) {
            console.log('About to slice 8.');
            x$(blogSelector).slice(8).appendTo('#interBlog3_after');
        }

        // Now see if there are > 5 posts.  If so, move 6th and more to interBlog2_after:
        if (x$(blogSelector).length > 5) {
            console.log('About to slice 5.');
            x$(blogSelector).slice(5).appendTo('#interBlog2_after');
        }

        // Now see if there are > 2 posts.  If so, move 3rd and more to interBlog1_after:
        if (x$(blogSelector).length > 2) {
            console.log('About to slice 2.');
            x$(blogSelector).slice(2).appendTo('#interBlog1_after');
        }

        // Last, move whatever is left into interBlog1_before, and add section-blogBundle class to the HTML textbox.
        console.log('About to do last move.');
        
        // Before we move what's left (post 1 and 2), we need to move the pagination and RSS feed icon to #interBlog3_after
        x$(sectionSelector).find('div.pagination').appendTo('#interBlog3_after');
        x$(sectionSelector).find('footer.module-footer').appendTo('#interBlog3_after'); 

        // Move what's left in the section:
        x$('#interBlog1_before').html(x$(sectionSelector).html());
        x$(sectionSelector).remove();
        x$('#interBlog1_before').parent().parent().parent().addClass(sectionSelector.substr(1));

        x$('div.blogListPage').removeClass('sheet').removeClass('grid-frame');  // For blog list pages! May not be needed at MMM?
    }
    else {
	x$('#blogAdsWrapper').remove();  // No blog list ads on pages where no blogSelector was found.
    }
}


function jQH_MMM_SetupBlogDetailPage() {
    // Reworking of article code, which was at the end of interface.js
    // if on a blog article detail page, or special ad campaigne page:
    if ((x$("body").hasClass("page-profiles") &&
  (x$("body").hasClass("page-profiles-blog-list") == false) &&
  (x$("body").hasClass("page-profiles-blogs-list") == false) &&
  (x$('div.entry-comments').length > 0)) 
  || x$("body").hasClass("page-honda")) {

        console.log('--- in jQH_MMM_SetupBlogDetailPage on a blog detail page.');
        var socialareawidth = x$(".entry-comments").width();

        x$(".entry-comments").remove();  // MMM does not use Ning Comment system.

	x$('#blogAdsWrapper').remove();  // No blog list ads on blog detail page.

        var url = x$(location).attr('href');

	var youmightlike = '<div id="taboola-below-article-thumbs-mix"></div><scr' + 'ipt type="text/javascript">window._taboola = window._taboola || [];_taboola.push({mode:"organic-thumbs-2r-2nd", container:"taboola-below-article-thumbs-mix", placement:"below-article-thumbs", target_type:"mix"}); </scr' + 'ipt>';


        x$('#facebook-ad').before('<div id="footer-social"></div>');

        x$('#facebook-ad').append('<div class="fb-like-box" data-href="https://www.facebook.com/mymodernmet" data-height="253" data-colorscheme="light" data-show-faces="true" data-header="true" data-stream="false" data-show-border="true"</div>');

        x$('#facebook-ad').after('<div id="youmightlike">' + youmightlike + '</div><div id="facebook-comments"><h3>Comments</h3><div class="fb-comments" data-href="' +
url + '" data-width="' + socialareawidth + '" data-numposts="5" data-colorscheme="light"></div>');


	x$(".pluginSkinLight").find("div").css("width","100%");

var FBshare = '<div id="jumbo_BOTT_BLOG" class="addthis_jumbo_share"></div><div style="margin-left:203px;margin-top:-50px" class="addthis_sharing_toolbox"></div>';



// Add TOP BLOG Facebook and Twitter buttons:
x$('.entry-byline').append('<div id="jumbo_TOP_BLOG_wrapper"><div id="jumbo_TOP_BLOG" class="addthis_jumbo_share"></div><div class="addthis_responsive_sharing"></div></div>');

// Remove extra white space between header and content section:
TopBlogSpacingPoll();


x$("#footer-social").append(FBshare).css({"display":"block","margin-bottom":"12px","margin-left":'0'});

	x$("header .google-plusone").remove();
    }

    else {
        x$('#facebook-ad').remove();
    }
}

var EntryHeadlineHeight = 0;
var TitleHeightOriginal = 0;

function TopBlogSpacingPoll() {
  var ht = x$('header.entry-headline').height();
  if (EntryHeadlineHeight > 0 && ht == EntryHeadlineHeight) {
    x$('div.blogDetailPage .entry-headline').css('height', EntryHeadlineHeight);
    TopBlogSpacingPoll2();
  }
  else {
    EntryHeadlineHeight = ht;
    console.log('===EntryHeadlineHeight: ' + EntryHeadlineHeight);
//    DisplayTestMsg('=== EntryHeadlineHeight: ' + EntryHeadlineHeight);
    TitleHeightOriginal = x$('h3.entry-title').height();
    console.log('===TitleHeightOriginal: ' + TitleHeightOriginal);
//    DisplayTestMsg('=== TitleHeightOriginal: ' + TitleHeightOriginal);
    setTimeout(TopBlogSpacingPoll, 250);
  }
}

function TopBlogSpacingPoll2() {
  var ht = x$('h3.entry-title').height();
  if (ht < TitleHeightOriginal) {
    console.log('=====ht: ' + ht);
//    DisplayTestMsg('===== ht: ' + ht);
    x$('div.blogDetailPage .entry-headline').css('height', ht + EntryHeadlineHeight - TitleHeightOriginal);
  }
  else {
    setTimeout(TopBlogSpacingPoll2, 500);
  }
}

function DisplayTestMsg(msg) {
  x$('section.entry-content').before('<div style="background-color:#EEEEEE;border-bottom:solid 1px #CCCCCC;padding:5px;">' + msg + '</div>');
}


