<script type="text/javascript">
  $().ready(function(){ 
    $(".validate-form").validate({
      submitHandler: function(form){
        $form = $(form);
        var $responseElement = $("#" + $form.attr("id") + "-response");
        if ($responseElement.length > 0) {
          $form.hide();
          $responseElement.show();
        }
        form.submit();
      }
    });
  });
</script>

