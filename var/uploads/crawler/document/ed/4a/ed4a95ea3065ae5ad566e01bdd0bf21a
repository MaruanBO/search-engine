<script type="text/javascript">
	$(function () {
		$('.fsl-inner-box').each(function () {
			var $box = $(this),
				timeoutId,
				expanded;
			if ('ontouchstart' in window) {
				var $mainLink = $box.find('.fsl-content>a');
				$mainLink.click(function (e) {
					if (!expanded) {
						$box.mouseenter();
					}
				});
			}
			$box.hover(function (e) {
				window.clearTimeout(timeoutId);
				$('.fsl-slider', this).stop(true, false).animate({ top: $('.fsl-slider', this).outerHeight() * -1 }, function () { expanded = true });
			}, function (e) {
				var $slider = $('.fsl-slider', this);
				timeoutId = window.setTimeout(function () {
					expanded = false;
					$slider.stop(true, false).animate({ top: -38 });
				}, 500);
			});
		});
	});
</script>

