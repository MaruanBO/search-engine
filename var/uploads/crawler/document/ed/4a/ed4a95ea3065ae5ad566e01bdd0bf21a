	}
};
BUY.Tracking.GetReferrerWebAddress = function() {
	var referrer = document.referrer;
	var re = new RegExp("http://|https://", "gi");
	referrer = referrer.replace(re, "");
	referrerParts = referrer.split("/");
	return referrerParts[0];															
};

$(function(){
	if(!BUY.Tracking.DeferLoad)
		BUY.Tracking.Load();
});
