	<script type="text/javascript">
		(function ($) {
			var $window = $(window), preLoadHeight = 200;
			$.fn.ScrollLoad = function (action) {
				return this.each(function () {
					var $this = $(this);
					//check if it's already in view
					if ($window.scrollTop() + $window.height() > $this.offset().top) {
						action.call($this[0]);
					}
					else {
						$window.scroll(function (e) {
							if ($window.scrollTop() + $window.height() + preLoadHeight > $this.offset().top) {
								$window.unbind(e);
								action.call($this[0]);
							}
						});
					}
				});
			};
		})(jQuery);
	</script>

    
	    <script type="text/javascript">
		    $(function () {
			    var $window = $(window),
				    $images = $('img.deferred'),
				    preLoadHeight = 100;
			    $window.scroll(function (e) {
				    var bottomEdge = $window.scrollTop() + $window.height();
				    for (var i = $images.length - 1; i >= 0; i--) {
					    var $image = $images.eq(i);
					    if ($image.offset().top < bottomEdge + preLoadHeight) {
						    $image.attr('src', $image.data('src'));
						    $images.splice(i, 1);
					    }
				    }
				    if ($images.length === 0) {
					    $window.unbind(e);
				    }
			    });
			    //trigger the function once to show visible items
			    $window.scroll();
		    });
	    </script>
	    

	<script type="text/javascript">
		(function ($) {
			var $window = $(window), preLoadHeight = 100;
			$.fn.ScrollLoad = function (action) {
				return this.each(function () {
					var $this = $(this);
					//check if it's already in view
					if ($window.scrollTop() + $window.height() > $this.offset().top) {
						action.call($this[0]);
					}
					else {
						$window.scroll(function (e) {
							if ($window.scrollTop() + $window.height() + preLoadHeight > $this.offset().top) {
								$window.unbind(e);
								action.call($this[0]);
							}
						});
					}
				});
			};
			$.fn.ScrollLoadAtEnd = function (action) {
				return this.each(function () {
					var $this = $(this);
					//check if it's already in view
					var $window = $(window);
					if ($window.scrollTop() + $window.height() + preLoadHeight > ($this.offset().top + $this.outerHeight())) {
						action.call($this[0]);
					}
					else {
						$window.scroll(function (e) {
							if ($window.scrollTop() + $window.height() + preLoadHeight > ($this.offset().top + $this.outerHeight())) {
								$window.unbind(e);
								action.call($this[0]);
							}
						});
					}
				});
			};
		})(jQuery);

	</script>

	<script type="text/javascript">

		
			var qsSiteId = qs["siteid"];
			if (qsSiteId != null && qsSiteId != "") {
				qsSiteId = qsSiteId.split('-');
				qsSiteId = qsSiteId[0];
			}
	            $(document).ready(function () {
	            	//check if popup should pop up, boo
	            	if ((GetCookie("hideEmailSignupPopup") == "null" || GetCookie("hideEmailSignupPopup") == "0")
						&& qsSiteId != "AysPbYF8vuM" && qsSiteId != ".7WaaTN6umc") {
	                    //animation is still choppy, add another half second after load
	                    var temp1 = window.setTimeout(function () { $("#divEmailSignupPopup").slideDown(); }, 500);
	                    //make it so popup doesn't EVER pop up again, yay
	                    var exdate = new Date();
	                    exdate.setYear(exdate.getFullYear() + 666);
	                    document.cookie = "hideEmailSignupPopup=1;expires=" + exdate.toGMTString() + ";domain=.rakuten.com;path=/";
	                }
	            });

	            var signupCloseTimer = null;
	            function closeEmailSignupPopup() {
	                if (signupCloseTimer)
	                    clearTimeout(signupCloseTimer);
	                $("#divEmailSignupPopup").slideUp();
	            }

	            function signupMakeEnterSubmit(e) {
	                if (!e) e = window.event;
	                if (e.keyCode == 13) {
	                    submitEmailSignup();
	                    return false;
	                }
	            }

	            function submitEmailSignup() {
	                var SignupEmailVal = $('#SignupEmail').val();
	                if (!/^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/.test(SignupEmailVal)) {
	                    alert('This doesn\'t appear to be a valid address.  Check for typos and try again.');
	                    return;
	                }

	                $.ajax({
	                    type: "GET",
	                    dataType: "jsonp",
	                    url: "https://secure.rakuten.com/AC/services.ashx?method=createemailprofile&emailaddress=" + encodeURIComponent(SignupEmailVal) + "&callback=?",
	                    success: function (data) {
	                        if (data.Success) {
	                            $("#divSignupRequest").fadeOut("fast", function () {
	                                $("#divSignupResponse").fadeIn("fast");
	                            });
	                            signupCloseTimer = window.setTimeout(function () { $("#divEmailSignupPopup").slideUp(); }, 3000);
	                        }
	                        else {
	                            $("#divSignupRequest").hide();
	                            $("#divSignupError").show();
	                        }

	                    },
	                    error: function () {
	                        $("#divSignupRequest").hide();
	                        $("#divSignupError").show();
	                    }
	                });
	            }
	    

	</script>

    

    <script type="text/javascript" src="/ct/Scripts/country-banner-map.js"></script>
	<script type="text/javascript">

		var om_globalbanner;

		(function () {

			var isGlobalVisitor = BUY.Cookies.IsGlobalVisitor;
			var countryCode = BUY.Cookies.GlobalVisitorCountry;

			/* RDCSUIUX-642 */
			function showGlobalBanner(cc) {

			    //set the global banner using ip mappings
			    $.each(globalBannerMap.Mappings, function (i, item) {

			        if (cc == item.CountryCode) {
			            var html = [];
			            html.push("<map id='gbmap' name='gbmap'><area shape='rect' coords='650,0,670,20' href='#' alt='' item='close' id='closeArea'/></map>");
			            html.push('<div style="text-align:center; margin-bottom:5px;">');
			            html.push("<a onclick='s.tl(this,\'o\',\'" + item.Image + "\');' href='" + item.Link + "'>");
			            html.push("<img id='globalBanner' usemap='gbmap' src='" + item.Image + "' /></a></div>");
			            $('#main').prepend(html.join(''));

			            $("map#gbmap").click(function (ev) {
			                $('#globalBanner').hide();
			            });
			        }
			    })

			    //if the banner hasn't been set, use language to set the global banner
			    if (!$("#globalBanner").length) {

			        var language = navigator.language || navigator.userLanguage; //doesn't work in chrome.

			        var html = [];
			        html.push("<map id='gbmap' name='gbmap'><area shape='rect' coords='650,0,670,20' href='#' alt='' item='close' id='closeArea'/></map>");
			        html.push('<div style="text-align:center; margin-bottom:5px;">');

			        if (language == "zh-CN") {
			            html.push("<a onclick='s.tl(this,\"o\",\"http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_sc.gif\");' ");
			            html.push("href='http://global.rakuten.com/zh-cn/'>");
			            html.push("<img usemap='gbmap' id='defaultGlobalBanner' src='http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_sc.gif' /></a>");
			        }
			        else if (language == "zh-TW") {
			            html.push("<a onclick='s.tl(this,\"o\",\"http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_tc.gif\");' ");
			            html.push("href='http://global.rakuten.com/zh-tw/'>");
			            html.push("<img usemap='gbmap' id='defaultGlobalBanner' src='http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_tc.gif' /></a>");
			        }
			        else if (language == "ko") {
			            html.push("<a onclick='s.tl(this,\"o\",\"http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_ko.gif\");' ");
			            html.push("href='http://global.rakuten.com/ko/'>");
			            html.push("<img usemap='gbmap' id='defaultGlobalBanner' src='http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket_ko.gif' /></a>");
			        }
			        else { //English
			            html.push("<a onclick='s.tl(this,\"o\",\"http://ast1.r10.io/buy_assets/v10/homepage/global_banners/globalrakuten_670x55.gif\");' ");
			            html.push("href='http://global.rakuten.com/en/'>");
			            html.push("<img usemap='gbmap' id='defaultGlobalBanner' src='http://ast1.r10.io/buy_assets/v10/homepage/global_banners/bnr_globalMarket.gif' /></a>");
			        }

			        html.push("</div>");
			        $('#main').prepend(html.join(''));

			        $("map#gbmap").click(function (ev) {
			            $('#defaultGlobalBanner').hide();
			        });
			    }
			}

			//show global banner using existing cookie
			if (typeof isGlobalVisitor === "string" && typeof countryCode === "string") {

			    if (isGlobalVisitor != "0") {

			        if (countryCode != "" && countryCode != "US" && countryCode != "CA" && countryCode != "MX") {
			            showGlobalBanner(countryCode);
			        }
			    }
			}
			    //get country code, set cookie, show banner
			else {

			    $.ajax("/ct/GlobalCheck.ashx?method=getglobal&more=1",
				{
					cache: false,
					timeout: 5000,
					success: function (data) {
						isGlobalVisitor = data.global;
						countryCode = data.countryCode;

						if (isGlobalVisitor) {
						    if (countryCode != "" && countryCode != "US" && countryCode != "CA" && countryCode != "MX") {
						        showGlobalBanner(countryCode);
						    }
						}

						var expires = new Date();
						expires.setTime(expires.getTime() + 1000 * 60 * 60 * 24 * 365);
						document.cookie = "IsGlobalVisitor=" + isGlobalVisitor + "; domain=.rakuten.com; path=/; expires=" + expires.toGMTString();
						document.cookie = "GlobalVisitorCountry=" + countryCode + "; domain=.rakuten.com; path=/; expires=" + expires.toGMTString();
					}
				});
			}

		})();
	</script>

    

	

