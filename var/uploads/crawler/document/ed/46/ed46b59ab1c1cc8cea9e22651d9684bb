				jQuery('#widgets #gardhomenewest ul li img').each(function(e){
					var ihi = jQuery(this).height();
					var lihi = jQuery(this).parents("li").height();
					var mhi = (ihi - lihi) * 0.5;
					if(ihi > lihi){jQuery(this).css({"marginTop": "-" + mhi + "px"});}
				});
				
				jQuery('.hparallax').each(function(){
					var pobj = jQuery(this);
					var top = pobj.offset().top;
					if (top < 0) top = 0;
					jQuery(window).scroll(function() {
						var sPos = jQuery(window).scrollTop();
						var yPos = -((sPos - top)/ 5) - 52;
						var bgpos = '0% '+ yPos + 'px';
						pobj.css('background-position', bgpos );
					});
				}); 
