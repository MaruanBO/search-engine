	<script type="text/javascript">
		(function() {
			var CACHE_COOKIE = 'LOADEDFROMBROWSERCACHE';
			lenox.loadedFromPageCache = $.cookie(CACHE_COOKIE) == 'true';
			$.cookie(CACHE_COOKIE, 'true', {path:'/'});
		})();

		$(function () {
			$(".nav-mobile").tinyNav({
    			header: 'Select a category...' // String: Specify text for "header" and show header instead of the active item
			});
		});
		$(function () {
			$(".mobile-account").tinyNav({
				header: 'My Account' // String: Specify text for "header" and show header instead of the active item
			});
		});
		function goCart(){
			var referer = getRefererJSON();
			window.location.assign("/cart-v2/index.cfm?fuseaction=dspCart&referer="+referer);
		};

		if (typeof JSON === 'undefined') {
			document.write('<script type="text\/javascript" src="\/includes\/js\/lenox\/json2-min.js?_dc=1202"><\/script>');
		}

		lenox.membership = new lenox.Membership();
		lenox.quickView = new lenox.QuickView();
		lenox.bogo = new lenox.Bogo();

		(function() {
			
				lenox.emailSignUp = new lenox.EmailSignUp({useCoremetrics: true});
			
			
			var miniCart = new lenox.cart.MiniCartPopup({enableButtonTxt: 'Add to cart'});
			var options = {
				maxItems: 5,
				domTotalElems: ['cart_totals'], // Pass in the elem id's only. The DOM elem do not exist yet.
				orderQty: 0
			}

			lenox.shoppingBag = new lenox.cart.Controller(options);
			lenox.shoppingBag.on({
					'load': miniCart.update,
					'beforeshow': lenox.membership.enroll,
					'show': miniCart.show
			});
			lenox.shoppingBag.on({
					'show': miniCart.disableBuyBtn
			});

			lenox.marketing = {
				coremetrics: {active: false},
				invodo: {active: false},
				googleAnalytics: {active: false, pageName: 'default'},
				commissionJunction: {active: false},
				acxiom: {active: false},
				acxiomEmail: {active: false},
				dynamicRemarketing: {active: false},
				facebookTracking: {active: false},
				optimizely: {active: false},
				liveEngage: {active: false},
				mercuryMedia: {active: false}
			};

			
			
			lenox.marketing.googleAnalytics.addToCart = function(prodName, qty) {
				var pageName = lenox.marketing.googleAnalytics.pageName || 'default';
				_gaq.push(['_trackEvent', prodName, 'add to cart', pageName, qty]);
			}
		
						lenox.marketing.mercuryMedia.active = true;
					
						lenox.marketing.googleAnalytics.active = true;
					
						lenox.marketing.commissionJunction.active = true;
					
						lenox.marketing.optimizely.active = true;
					
						lenox.marketing.facebookTracking.active = true;
					
						lenox.marketing.acxiomEmail.active = true;
					
						lenox.marketing.coremetrics.active = true;
					
						lenox.marketing.dynamicRemarketing.active = true;
					
						lenox.marketing.invodo.active = true;
					
						lenox.marketing.acxiom.active = true;
					
						lenox.marketing.steelHouseSmarterPixel.active = true;
					
						lenox.marketing.liveEngage.active = true;
					

			lenox.shoppingBag.on('afteradd', function(result) {
				var items =	result.data.lastItem;
				if (Ext.isArray(items) && items.length > 0){
					var item = items[0];
					if (lenox.marketing.invodo.active) {
						lenox.invodo.addedToCart(item.SKU,item.ORDER_QUANTITY,item.PRICE);
					}
					if (lenox.marketing.googleAnalytics.active) {
						lenox.marketing.googleAnalytics.addToCart(item.PRODUCT_DESC,item.ORDER_QUANTITY);
					}
				}
			});

			$(document).ready(function() {
				if (lenox.loadedFromPageCache) {
					lenox.shoppingBag.load();
				}
				lenox.emailSignUp.autoShow();
			});
		})();
	</script>
