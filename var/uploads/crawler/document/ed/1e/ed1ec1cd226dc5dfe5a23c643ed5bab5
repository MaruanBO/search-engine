<script type="text/javascript" language="JavaScript">
function initialFocus(){ 
   document.frmDefault.txtZip.value='Enter Your ZIP Code';
   document.frmDefault.txtZip.focus();
   document.frmDefault.txtZip.select();
}

function submitForm() {
		if ( !isNaN (document.frmDefault.txtZip.value ) && (document.frmDefault.txtZip.value != "")) {
		    sZip = new String(document.frmDefault.txtZip.value);
		        sZip = sZip.replace(/(\s*)/g, "");
				nlength = sZip.length;
				if ((nlength < 5) | (nlength > 5)){
				    errorMsg();
					return false;
	            } else {
	                document.frmDefault.txtZip.value = sZip;
					checkedvalue = getCheckedValue(document.frmDefault.HIType);
					if (checkedvalue == "over64") {
						document.frmDefault.action="demographics.aspx";
						document.frmDefault.submit();
					}
					if (checkedvalue=="under64"){return true;}	
				}
		}else{
		    errorMsg();	
			return false;
		}
}

function errorMsg() {
    alert('Please Enter a Valid Zip');
    document.frmDefault.txtZip.focus();
    document.frmDefault.txtZip.select();
}

function gotolink() { 
	checkedvalue = getCheckedValue(document.frmDefault.HIType1);
	if (checkedvalue=="pageMS"){document.frmDefault.action="demographics.aspx?lessThan64=true";}
	else {
	    if (checkedvalue == "pageIFP") {
	        checkLeadSrc();
	        document.frmDefault.action = "http://www.healthplanone.com/demographics.aspx";
	    }
	}
	document.frmDefault.submit();
}

function getCheckedValue(radioObj) {
	if(!radioObj)
		return "";
	var radioLength = radioObj.length;
	if(radioLength == undefined)
		if(radioObj.checked)
			return radioObj.value;
		else
			return "";
	for(var i = 0; i < radioLength; i++) {
		if(radioObj[i].checked) {
			return radioObj[i].value;
		}
	}
	return "";
}

function checkLeadSrc() {
    var eLead = document.getElementById("leadSrc").value;
    switch (eLead) {
        case "1":
            document.getElementById("leadSrc").value = "701"
            break
        case "2":
            document.getElementById("leadSrc").value = "702"
            break
        case "3":
            document.getElementById("leadSrc").value = "703"
            break
    }
}
</script>

