function showOffer(id) {
	$('.watches').not('#watch-'+id).each(function(){
         $(this).hide();
	});
	$('#watch-'+id).slideToggle();
	return false;
}

function hideOffer(id) {
	$('#watch-'+id).slideUp('slow');
	return false;
