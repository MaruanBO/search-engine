    <script src="/javascripts/jquery.ticker.js" type="text/javascript"></script>
<script src="/javascripts/date.format.js" type="text/javascript"></script>
<script src="/javascripts/jquery-ui.datepicker.js" type="text/javascript"></script>
<script src="/javascripts/jquery-validate/jquery.validate.js" type="text/javascript"></script>
    <script type="text/javascript">
        //<![CDATA[
        $(document).ready(function(){
            $('#tips ul').ticker({
                prependHTML: '<h4><span>Perx Tips</span>: ',
                appendHTML: '</h4>'
            });
        });
        
        $('#resort-search form').validate({});
        
            $(document).ready(function(){
                Date.prototype.addDays = function(days) {
                    this.setDate(this.getDate()+days);
                    return this;
                }

                var fromElem = $('.start-date');
                var toElem = $('.end-date');

                toElem.datepicker();

                fromElem.datepicker({
                    showOn: 'focus',
                    minDate: new Date(),
                    duration: 'fast',
                    defaultDate: new Date("02/19/2015"),
                    numberOfMonths: 2,
                    buttonImageOnly: true,
                    onSelect: function(date) {
                        var startDate = new Date($.trim(date));
                        var endDate = new Date($.trim(date));
                        var newMaxDate;
                        
                          endDate.addDays(14);
                          newMaxDate = (new Date(startDate)).addDays(60);

                        if ($('#resort-search-toggle') && $('#resort-search-toggle').attr('checked')){
                          endDate = null;
                          endDate = new Date($.trim(date)).addDays(3);
                          newMaxDate = null;
                          newMaxDate = (new Date(startDate)).addDays(30);
                        }

                        fromElem.val(startDate.format('mm/dd/yyyy'));
                        toElem.css('background-color', '#ffff88')
                              .val(endDate.format('mm/dd/yyyy'));

                        toElem.datepicker('destroy');

                        toElem.datepicker({
                            showOn: 'focus',
                            minDate: startDate,
                            maxDate: newMaxDate,
                            duration: 'fast',
                            defaultDate: new Date("03/05/2015"),
                            numberOfMonths: 2,
                            buttonImageOnly: true,
                            onSelect: function(date) {
                                toElem.css('background-color', '#fff');
                            }
                        });
                    }
                });
            });
        //]]>
    </script>

    <script type="text/javascript">
        //<![CDATA[
        $(document).ready(function(){
            $('#resort-search').hide();
            $('#cruise-search-toggle').click(function(){
              $('#resort-search').hide();
              $('#cruise-search').show();
            });
            $('#resort-search-toggle').click(function(){
              $('#cruise-search').hide();
              $('#resort-search').show();
            });
        });
        //]]>
    </script>

