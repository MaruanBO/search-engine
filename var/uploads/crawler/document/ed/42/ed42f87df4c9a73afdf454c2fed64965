    function LoadYTVideo(frame, id) {
        $("#" + id).html(frame);
    };
    function Opentab(id) {
        for (i = 1; i < 6; i++) {
            $("#tab" + i).removeAttr("class"); $("#tbl" + i).css("display", "none");
        }
        $("#tab" + id).attr("class", "active"); $("#tbl" + id).css("display", "block");
        return false;
    };
</script>
 
        
        
