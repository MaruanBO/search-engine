<script type="text/javascript">
function trackOutboundLink(link, category, action) {
	try {
		ga('send', 'event', 'category', 'action', 'opt_label', opt_value, {'nonInteraction': 1});
	} catch(err){}
	 
	setTimeout(function() {
		document.location.href = link.href;
	}, 100);
}
</script>
 
