	function parseUri(str) {
    	var	o   = {
    			strictMode: false,
    			key: ["source","protocol","authority","userInfo","user","password","host","port","relative","path","directory","file","query","anchor"],
    			q:   {
    				name:   "queryKey",
    				parser: /(?:^|&)([^&=]*)=?([^&]*)/g
    			},
    			parser: {
    				strict: /^(?:([^:\/?#]+):)?(?:\/\/((?:(([^:@]*)(?::([^:@]*))?)?@)?([^:\/?#]*)(?::(\d*))?))?((((?:[^?#\/]*\/)*)([^?#]*))(?:\?([^#]*))?(?:#(.*))?)/,
    				loose:  /^(?:(?![^:@]+:[^:@\/]*@)([^:\/?#.]+):)?(?:\/\/)?((?:(([^:@]*)(?::([^:@]*))?)?@)?([^:\/?#]*)(?::(\d*))?)(((\/(?:[^?#](?![^?#\/]*\.[^?#\/.]+(?:[?#]|$)))*\/?)?([^?#\/]*))(?:\?([^#]*))?(?:#(.*))?)/
    			}
    		};

    	var	m   = o.parser[o.strictMode ? "strict" : "loose"].exec(str),
    		uri = {},
    		i   = 14;

    	while (i--) uri[o.key[i]] = m[i] || "";

    	uri[o.q.name] = {};
    	uri[o.key[12]].replace(o.q.parser, function ($0, $1, $2) {
    		if ($1) uri[o.q.name][$1] = $2;
    	});

	
    	return uri;
    };
function cmProcesTagsFromUrl(url) {
	utag_data.page_id = 'AG:US:HOME';
	utag_data.category_id = 'AG:US';
	utag_data.site_section_attr8 = '';
	utag_data.page_type_attr10 = '';
	utag_data.page_subtype_attr11 = '';
	if (url.indexOf('?') > 0) {
		url = url.replace('#', '&');
	}else {
		url = url.replace('#', '?');
	}
	var win_location_parsed = parseUri(url);
	var dir_parsed = [];	
	if (win_location_parsed.directory != ''){
		dir_parsed = win_location_parsed.directory.toUpperCase().split('/');
		//alert(dir_parsed);
		query_parsed = win_location_parsed.query.toUpperCase().split('&');
		//cat & page id
		for (var i=0; i < dir_parsed.length; i++){
			if (dir_parsed[i] != '') {
				utag_data.category_id += ':' + dir_parsed[i];
				utag_data.page_id += ':' + dir_parsed[i];
				if(i == 1) {
					utag_data.page_type_attr10 = dir_parsed[i];
				}
				if(i == 2) {
					utag_data.page_subtype_attr11 = dir_parsed[i];
				}
			}
		}
		for (var i=0; i < query_parsed.length; i++){
			if (query_parsed[i] != '') {
			//	alert(query_parsed.length);
				query_parsed_value = query_parsed[i].split('=');
			//	alert(query_parsed_value);
				
				utag_data.category_id += ':' + query_parsed_value[1];
				

				if(query_parsed.length > 1){
				if(i == ((query_parsed.length)-1)){
//alert('value 1 adding now'+ query_parsed_value[1]);
				utag_data.page_id += ':' + query_parsed_value[1];
				

					}
//alert('value 1 adding asasanow'+ query_parsed_value[1]);

				}
				else if(query_parsed.length <= 1){
				if(i ==0){
//alert('value 2 adding now'+ query_parsed_value[1]);
				utag_data.page_id += ':' + query_parsed_value[1];
				
					}
//alert('value 1 addingdfdsdfd now'+ query_parsed_value[1]);

				}	
			}		
			
		}
		//site section
		if (dir_parsed[dir_parsed.length-1] != '') {
			utag_data.site_section_attr8 = dir_parsed[dir_parsed.length-1];
		}else if (dir_parsed[dir_parsed.length-2] != '') {
			utag_data.site_section_attr8 = dir_parsed[dir_parsed.length-2];
		}
		//if (utag_data.site_section_attr8 != '') {
			//utag_data.page_id += ':' + utag_data.site_section_attr8;
		//}
		//utag_data.site_section_attr8 = dir_parsed[dir_parsed.length-2];
		//page type
		//if (typeof win_location_parsed.queryKey.page != 'undefined') {
			//utag_data.page_type_attr10 = win_location_parsed.queryKey.page.toUpperCase();
		//}
		//if (utag_data.page_type_attr10 != '') {
			//utag_data.page_id += ':' + utag_data.page_type_attr10;
		//}
		//page sub type
		//if (typeof win_location_parsed.queryKey.popup != 'undefined') {
			//utag_data.page_subtype_attr11 = win_location_parsed.queryKey.popup.toUpperCase();
		//}
		//if (utag_data.page_subtype_attr11 != '') {
			//utag_data.page_id += ':' + utag_data.page_subtype_attr11;
		//}
		if(dir_parsed[2] != '') {
			utag_data.brand_attr20 = dir_parsed[2];
		}
		//page sub type
		if (utag_data.page_subtype_attr11 == '' && typeof win_location_parsed.file != 'undefined') {
			filename_parsed = win_location_parsed.file.toUpperCase().split('.');
			utag_data.page_subtype_attr11 = filename_parsed[0];
		}
	}
