	<script type="text/javascript" language="javascript">
//this is for the canadian site home	
	function canadaImg(){
		$('body.site img').each(function(){
			var img = $(this);
			var imgSrc = img.attr('src');
			if (imgSrc.indexOf("bottom-banner-left-can.png") >= 0){
				img.css({
					'position' : 'absolute',
  					'margin-left' : '0px'
					});
				//	$('#rules').css('display','none');
				//img.attr('alt', 'Canadian friends: WELCOME to the world of American Girl');
			}
			if (imgSrc.indexOf("bottom-banner-right-can.png") >= 0){
				img.css({
					'margin-left' : '466px'
					});
			}
			if (imgSrc.indexOf("slide1-can.jpg") >= 0){
				img.css({
					'margin-left' : '-90px'
					});
			}
		 });
		}
	$(document).ready(function(){
		setTimeout('canadaImg()',250)
	});
</script>
