<script>
	mouseY=0;
	mouseX=0;
	function displayIt(){
		$('#ageWarning').css({
			'display':'block',
			'top':mouseY,
			'left':mouseX
		});
	}	
	function toggleOff(){
	$('#ageWarning').css('display','none');
	}
		
	$(document).ready(function(){
		$(this).click(function(e){
			mouseX=e.pageX;
			mouseY=e.pageY;	
		});
		$('.banner a, .ageWarningTrigger').click(function(){
			$('#ageWarning').css('display','none');
		});
			
	});
</script>
