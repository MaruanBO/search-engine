    <script type="text/javascript">
	    // assumes ga.js has been loaded
	    function PrimaryActionTimer(category, variable, optlabel) {
		    // Max elasped time to send Google Analytics.
		    // 5 minutes in milliseconds.
		    this.MAX_TIME = 5 * 60 * 1000;
		    this.category = category;
		    this.variable = variable;
		    this.label = optlabel ? optlabel : undefined;
		    this.startTime;
		    this.elapsedTime;
		    this.environment = 'live';
		    this.isProduction = true;
		    return this;
	    }

	    PrimaryActionTimer.prototype.start = function () {
		    this.startTime = new Date().getTime();
		    return this;
	    };

	    PrimaryActionTimer.prototype.stop = function () {
		    this.elapsedTime = new Date().getTime() - this.startTime;
		    return this;
	    };

	    PrimaryActionTimer.prototype.send = function () {
		    var c = [];
		    c.push('_trackTiming');
		    c.push(this.category);
		    c.push(this.variable);
		    c.push(this.elapsedTime);
		    c.push(this.label);

		    // set sampling rate to 100% for test & stage environments
		    // _gaq processing does not function properly on a local dev machine because required tag-management tags don't fire locally
		    if (this.environment === 'test' || this.environment === 'stage') {
			    c.push(100);
		    }

		    window._gas = window._gas || [], window._gas.push.call(window._gas, c);
	    };

	    var VRBO = VRBO || {};
	    VRBO.PrimaryActionTimer = new PrimaryActionTimer('PrimaryAction', 'Home', 'SearchBarReady');
	    VRBO.PrimaryActionTimer.start();

    </script>


     
	 
	
