<script type="text/javascript">


    khJQuery(function() {
        var $ = khJQuery;
        var clientSize = function () {
            var size;
            if (window.innerHeight) { // all except Explorer
                size = {w:window.innerWidth, h:window.innerHeight};
            }
            else if (document.documentElement && document.documentElement.clientWidth) { // IE6 Strict Mode
                size = {w:document.documentElement.clientWidth,h:document.documentElement.clientHeight};
            } else if (document.body) {
                size = {w:document.body.clientWidth,h:document.body.clientHeight};
            }
            return size;
        };
        $("#cu_trending_wrap").hide();
        $(window).ready(function(){
            $(window).trigger("resize");
        });
        var $cufayt = $("#cufayt.faythp");
        var $cumagglass = $("#cumagglass");
        var $cusearch = $("#cusearch");
        $(window).resize(function() {
            var width = clientSize().w;
            var $cuwrapcenter = $("#cu_wrap_center");
            var $cutextwrapper = $("#cutextwrapper");
            var $jcarouselctrlnext= $(".jcarousel-control-next");
            var $jcarouselctrlprev= $(".jcarousel-control-prev");
            var $cuwrapimg =$("#cu_wrap_img");
            var $trendingTitle = $("#trendingTitle");
            var $trending = $(".trending");
            var $cumaintitle = $("#cumaintitle");
            var $cutaglines = $("#cutaglines");
            var $cubuttons = $("#cu_buttons");
            var $cubutdiv =  $("#cu_buttons").find("div");
            $cumaintitle.width($cumaintitle.find("span").width());
            $cutaglines.width($cutaglines.find("span").width());
            $cutaglines.css("font-size","17px")
            if(width>930){
                $cumagglass.css("margin-left","597px");
                $cusearch.width("656px");
                $cuwrapcenter.width($cusearch.width()+13);
                $cusearch.css("margin-left","0px");
                $jcarouselctrlnext.css("right","-64px");
                $jcarouselctrlprev.css("left","-64px");
                $cuwrapimg.height(530);
                $trendingTitle.css({"top":"-150px","color":"white"});
                $trending.css("top","-150px");
                $cumaintitle.css("font-size","49px");
                $cubutdiv.height("50px");
            }else {
                $cusearch.width(width-150);
                $cuwrapcenter.width($cusearch.width());
                $cumagglass.css("margin-left",($cusearch.width()+parseInt($cusearch.css("paddingLeft"))-$cumagglass.width())+"px");
                $jcarouselctrlnext.css("right","-30px");
                $jcarouselctrlprev.css("left","-30px");
                $cuwrapimg.height(370);
                $trendingTitle.css({"top":"0px","color":"#535353","width":"78%"});
                $trending.css("top","0px");
                $cumaintitle.css("font-size","40px");
                $cubutdiv.height("50px");
            }
            if (width<650){
                $cusearch.css("margin-left","20px");
                $cumagglass.css("margin-left",($cusearch.width()+parseInt($cusearch.css("paddingLeft"))-$cumagglass.width()+ 20)+"px");
                $cuwrapcenter.css({"left":"10px","top":"53px"});
                $cumaintitle.width("90%");
                $cutaglines.width("90%");
                $cubutdiv.height("50px");
            }else{
                $cuwrapcenter.css({"left":Math.floor((width - $cusearch.width())/2) +"px","top":"153px"});
            }
            $cubuttons.width($cuwrapcenter.width());
            $cubuttons.css({"left":$cuwrapcenter.css("left"),"top":$cuwrapcenter.css("top")+$cubutdiv.height()});
            $cufayt.width($cusearch.width()+parseInt($cusearch.css("paddingLeft")));
            $cufayt.css("top",$cutextwrapper.height()+$cusearch.height()+parseInt($cutextwrapper.css("paddingBottom"))+4);
        });

        var settings = {
            suggest:{
                pos     : -1,
                typed   : ""
            },
            tp:"true",
            hostname:"curiyo.com",
            initiator:"cucards"
        };

        $cufayt.cufayt(settings);
        var cufayt = $cufayt.data("cufayt");
        $cusearch.keydown(cufayt.faytKey);
        $cufayt.on("click","div a",cufayt.handleFayt);
        $cufayt.on("mouseenter","div",settings,cufayt.mouseHighlight);
        $cufayt.on("mouseleave","div",cufayt.unHighlight);
        $cusearch.blur(cufayt.hideSuggest);
        $cumagglass.on("click",{tp:"true"},function(e){
            e.data.query = $cusearch.val();
            cufayt.handleFayt(e);
        });

        $(function() {
            var jcarousel = $('.jcarousel').jcarousel();

            $('.jcarousel-control-prev')
                    .on('jcarouselcontrol:active', function() {
                        $(this).removeClass('inactive');
                    })
                    .on('jcarouselcontrol:inactive', function() {
                        $(this).addClass('inactive');
                    })
                    .jcarouselControl({
                        target: '-=1'
                    });

            $('.jcarousel-control-next')
                    .on('jcarouselcontrol:active', function() {
                        $(this).removeClass('inactive');
                    })
                    .on('jcarouselcontrol:inactive', function() {
                        $(this).addClass('inactive');
                    })
                    .jcarouselControl({
                        target: '+=1'
                    });


            function getTrending(lang,locale,count) {
                var url;
                var dataType="json";
                count = count?count:6;
                url = "//curiyo.com/trending3?lang="+lang+"&locale="+locale+"&count="+count+"&width=200&height=150&callback=?";
                $.ajax({
                    url: url,
                    dataType: dataType,
                    success: trendingResults,
                    error: err
                });
            }
            function trendingResults(data){
                var lang = (data._id && data._id.length==5)?data._id.substring(0,2):"en";
                var html = '<ul>';

                $("#trendingTitle").html(data.title?data.title:"");
                $.each(data.topics,function(index){
                    html += '<li style="background-image:url(\''+this.images[0].source+'\')"><a href="' +'//curiyo.com/'+lang+ '/topic/' +encodeURIComponent(this.topic)+
                            '"></a><div class="photo-title">'+this.topic+'</div><div class="cu_gradient"></div></li>';
                });
                html += '</ul>';
                // Append items for carousel
                jcarousel
                        .html(html);
                // Reload carousel
                jcarousel
                        .jcarousel('reload');
                $("#cu_trending_wrap").fadeIn(1000);
            }
            jcarousel
                    .on('jcarousel:reload jcarousel:create', function () {
                        var width = jcarousel.innerWidth();
                        if (width >= 800) {
                            width = 200;
                        } else if (width >= 600) {
                            width = width / 3;
                        }else if (width >= 400) {
                            width = width / 2;
                        }

                        jcarousel.jcarousel('items').css('width', width + 'px');
                    })
                    .jcarousel({
                        wrap: 'circular'
                    });

            function err(jqXHR, textStatus, errorThrown) {
                console.log("textStatus= "+textStatus+"\n"+"errorThrown= "+errorThrown);
            }
            getTrending("en","en-us",10);
            $(".jcarousel").on("click","li",function(){
                location.href = $(this).find("a").prop("href");
            });
        });
    });

</script>
