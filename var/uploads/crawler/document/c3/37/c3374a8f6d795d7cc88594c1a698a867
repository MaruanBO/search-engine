	function GetSiteProduct(urlHost) {
        try {
	        if (urlHost) {
	            urlHost = urlHost.toLowerCase();

	            if (urlHost.indexOf("order") != -1 || urlHost.indexOf("accounts") != -1 || window.location.pathname.indexOf("Cart.aspx") > -1) {
	                /* check for Cart.aspx until the subdomain change */
	                return "checkout";
	            } else if (urlHost.indexOf("gifts") != -1 || urlHost.indexOf("products") != -1) {
                    return "product";
                } else {
                    return "content";
                }
	        }
        } catch (e) { }
        return "";
    }
    function GetSubdomain() {
        try {
            if (window.location.host) {
                return window.location.host.split(".")[0];
            } else {
                return "";
            }
        } catch (e) { }
    }
