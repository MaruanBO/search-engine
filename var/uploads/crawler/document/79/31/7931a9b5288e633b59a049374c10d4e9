// Controls Top Ads

	var img = new Array()
	var lnk = new Array()
	var title = new Array()

	img[0] = '/img/ads2013/hmp_ad021714a.jpg'
	lnk[0]  = '/jewelry/?pagtp=1'
	title[0] = 'Jewelry Auctions'	
	
	img[1] = '/img/ads2013/hmp_ad030514b.jpg'
	lnk[1]  = '/category/listing.aspx?qs=kw=apple&catid=0'
	title[1] = ' Apple Auctions'
	
	img[2] = '/img/ads2013/hmp_ad032614rt2.jpg'
	lnk[2]  = 'http://redtag.com/'
	title[2] = ' Always Free Shipping'

	var q = img.length-1;
	
	var index_2 = Math.round(Math.random()*(q));
	
		function showMainPromo(index)
	{
		index = (index + index_2) % q
		document.write('<a href="' + lnk[index] + '"><div class="title">' + title[index] + '</div><img src="' + img[index] + '" /></a>');
	}
	             
				 
				 	function showMainPromoredtag(index)
	{
		index = 2;
		document.write('<a target="_blank" href="' + lnk[index] + '"><div class="title">' + title[index] + '</div><img src="' + img[index] + '" /></a>');
	}
</script>
