	<script type="text/javascript">
	var browser=navigator.appName;
	var b_version=navigator.appVersion;
	var version=parseFloat(b_version);

	function SearchDisplay(action)
	{
		if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)) //test for MSIE x.x;
		{
			var ieversion=new Number(RegExp.$1) // capture x.x portion and store as a number

			if (ieversion < 7)
			{
				if (action == "hide") {
					SearchDisplayHide(); }
				else if (action == "show") {
					SearchDisplayShow(); }
				else
				{}
			}
		}
	}

	function SearchDisplayHide()
	{
		document.getElementById("ubcatmenu").style.display="none";

		var x = document.getElementById("searchImgReplace");
		x.src="/img/replacementimg2.gif";
	}

	function SearchDisplayShow()
	{
		document.getElementById("ubcatmenu").style.display="inline";

		var x = document.getElementById("searchImgReplace");
		x.src="/img/dot_clear.gif";
	}
	
	
	function SearchProducts(link)
	{
	   	link.href = "http://www.ubid.com/category/listing.aspx?qs=" + document.getElementById("ubseartx").value + "&catid=" +  document.getElementById("ubcatmenu").value;
		return true;
	}

	function SearchProductsBottom(link)
	{
	   	link.href = "http://www.ubid.com/category/listing.aspx?qs=" + document.getElementById("ubseartx_bot").value + "&catid=" +  document.getElementById("ubcatmenu_bot").value;
		return true;
	}
	
    </script>
