<script>
     aim_replace_cycler_nav = function() {
        console.log("aim_replace_cycler_nav");
		var nav_array = [{"title":"Make It Tonight","link":"http:\/\/www.vegetariantimes.com\/recipe\/baked-tofu-salad-with-broccoli-and-pineapple\/"},{"title":"Build a Better Breakfast","link":"http:\/\/www.vegetariantimes.com\/recipe\/quinoa-eggs-florentine\/"},{"title":"Veg Daily Blog","link":"http:\/\/www.vegetariantimes.com\/blog\/how-to-eat-vegetarian-on-a-budget\/"},{"title":"2015 Reader Recipe Contest","link":"http:\/\/www.vegetariantimes.com\/2015readerrecipecontest\/"}];
		for (var i=0; i < nav_array.length; i++ ) {
			var elem = jQuery('#viewer_nav a:nth-child(' + (i + 1) + ')');
            if(i == 0){
                elem.addClass('activeSlide');
            }
    		elem.html(nav_array[i]['title']).attr('href', nav_array[i]['link']).unbind('click');
    		if (!elem.data('height_set')){
                var orig_height = elem.height();
                var target_height = 80;
	            var padding_top = ((target_height - orig_height ) / 2);
	            var set_height = target_height - (padding_top);
	            elem.css({'padding-top' : padding_top+"px", 'height' : set_height});   
	    		elem.data('height_set', true);
            }
		}
	}
	jQuery(aim_replace_cycler_nav);//ready(function)
</script>

