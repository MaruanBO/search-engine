		function toggle_menu_slider(toggle_state){
			var $ = jQuery;
			var slide_main = $("#slide-tab");
			var slide_container = $("#slide-container");
			var slide_transparent_hide_button = $("#slide-transparent-hide-button");
			var body = $("body");
			var header = $('#header');
			if(parseInt(body.css('left')) >= 0 || toggle_state == 'show'){
				body.css('left', '-84%');
				header.css('left', '-84%');
				slide_main.css('right', '84%');
				slide_container.css('right', '0');
				slide_transparent_hide_button.css('left', '0');
			} else {
				body.css('left', '0');
				header.css('left', '0');
				slide_main.css('right', '0');
				slide_container.css('right', '-85%');
				slide_transparent_hide_button.css('left', '-19%');
			}
		}
		</script>
		
