<script type="text/javascript">
	// Enable only if we're not on a login page
	if (window.location.pathname.split('/Security')[0] !== '') {
		(function(){
			var $Floater = $('#Floater'),
				footerTop = $('#footer'),
				$document = $(document);
			function checkOffset() {
				if ($document.scrollTop() + window.innerHeight <= footerTop.offset().top) {
					// restore when you scroll up
					$Floater.removeClass('stuck');
				} else  {
					$Floater.addClass('stuck');
				}
			}
			checkOffset();
			$(document).scroll(checkOffset);
			$(window).resize(checkOffset);
		})();
	} else {
		$('#Floater').addClass('stuck');
	}
</script>


