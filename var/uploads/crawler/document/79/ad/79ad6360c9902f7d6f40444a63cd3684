    <script type="text/javascript">
    //Main Google Analytics Tracking Code
        var googleId = 'UA-1005942-1';
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', googleId]);
        if (document.referrer == '')
            _gaq.push(['_setCustomVar', 5, 'Traffic', 'Direct', 2]);
        _gaq.push(['_trackPageview']);

        (function() {
            var ga = document.createElement('script');
            ga.type = 'text/javascript';
            ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ga, s);
        })();

        //Google Analytics Event Handling

        function PageTrackerClickEvent(category, action, label) {
            _gaq.push(['_trackEvent', category, action, label]);
        }

        //Google Analytics Fix to catch old event tracking
        pageTracker = {};
        pageTracker._trackEvent = PageTrackerClickEvent;
        pageTracker._trackPageview = function() {
            return true;
        };


        //Google Analytics Ajax Handling
        $(document).bind("ajax-trigger-analytics", function(e) {
            _gaq.push(['_trackPageview']);
        });
    </script>
  
         
