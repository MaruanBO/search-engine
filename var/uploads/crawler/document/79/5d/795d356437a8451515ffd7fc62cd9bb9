    <script>
      jQuery.noConflict();
      ;(function($) {
           // DOM Ready
          $(function() {
            if ($.cookie('clatlNewsletterModal') != 'shown')    {
                $.cookie('clatlNewsletterModal', 'shown', { expires: 10 });
                $(document).one('ready', function () {
                    $('#modalOuter').bPopup({
                        easing: 'easeOutBack', //uses jQuery easing plugin
                        speed: 450,
                        transition: 'slideDown',
                        modalColor: 'black'
                    });
                });
            }

              $('#closeModal').on('click', function(e) {
                $('#modalOuter').bPopup().close();
              });
          });

      })(jQuery);
    </script>

