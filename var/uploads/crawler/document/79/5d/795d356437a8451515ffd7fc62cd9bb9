  <!-- Google Analytics Social Interaction hooks -->
  <script type="text/javascript">
    jQuery(document).ready(function($){

      
      try {
        if (FB && FB.Event && FB.Event.subscribe) {
          FB.Event.subscribe('edge.create', function(targetUrl) {
            _gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
            _gaq.push(['test._trackSocial', 'facebook', 'like', targetUrl]);
            
          });
          FB.Event.subscribe('edge.remove', function(targetUrl) {
            _gaq.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
            _gaq.push(['test._trackSocial', 'facebook', 'unlike', targetUrl]);
            
          });
          FB.Event.subscribe('message.send', function(targetUrl) {
            _gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]);
            _gaq.push(['test._trackSocial', 'facebook', 'send', targetUrl]);
            
          });
        }
        else {
          console.error("Facebook events unavailable for social interaction hooks!");
        }
      }
      catch(e) {
        
      }

      
      function extractParamFromUri(uri, paramName) {
        if (!uri) {
          return;
        }
        var uri = uri.split('#')[0];  // Remove anchor.
        var parts = uri.split('?');  // Check for query params.
        if (parts.length == 1) {
          return;
        }
        var query = decodeURI(parts[1]);

        // Find url param.
        paramName += '=';
        var params = query.split('&');
        for (var i = 0, param; param = params[i]; ++i) {
          if (param.indexOf(paramName) === 0) {
            return unescape(param.split('=')[1]);
          }
        }
      }
      try {
        if (twttr && twttr.events && twttr.events.bind) {
          twttr.events.bind('tweet', function(event) {
            if (event) {
              var targetUrl;
              if (event.target && event.target.nodeName == 'IFRAME') {
                targetUrl = extractParamFromUri(event.target.src, 'url');
              }

              // Fallback to URL used by sharing tools.
              if (!targetUrl) {
                targetUrl = ""
              }

              _gaq.push(['_trackSocial', 'twitter', 'tweet', targetUrl]);
              _gaq.push(['test._trackSocial', 'twitter', 'tweet', targetUrl]);
              
            }
          });
        }
        else {
          console.error("Twitter events unavailable for social interaction hooks!");
        }
      }
      catch(e) {
        
      }
    });
  </script>
  <!-- End Google Analytics Social Interaction hooks -->


