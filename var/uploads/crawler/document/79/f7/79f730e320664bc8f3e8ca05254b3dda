<script type="text/javascript">
    $(document).ready(function () {
        //console.log('enter email and click');
        $('#sign-up-flyout').submit(function () {
            //console.log('clicked submit');
            if (signuphead_validate()) {
                var data = {
                    'email': $('#signupemail').val(),
                    'eid': $('#eid').val(),
                    'aid': $('#aid').val(),
                    'pid': $('#pid').val()
                };
                //console.log('valid email');
                $.post('/tools/cheetah_sign_up', data, function (response) {
                    if (response.message == 'OK') {
                        window.location.replace('http://ebm.e.taunton.com/r/regf2?a=0&aid=730585392&n=225&email=' + encodeURIComponent($('#email').val()));
                        //console.log(data.email);
                    } else {
                        alert("An error has occured. Sorry about that.");
                    }
                }, 'json');
            } else {
                alert("Please enter a valid email address");
            }

            // Create the forever (10 years) cookie
            createCookie('scram', 'y', 3650);
            return false;
        });
    });

    function signuphead_validate() {
        $('input.error').removeClass('error');
        var required = function (t) {
            if ($.trim(t.val()) == '' || $.trim(t.val()) == t.attr('placeholder')) {
                t.addClass('error');
                return 1;
            }
            return 0;
        };

        var errors = 0;
        //errors += required($('#signupemail'));
        var email = $('#signupemail');
        if (required(email)) {
            errors += 1;
        } else {
            if (!signuphead_echeck(email.val())) {
                email.addClass('error');
                errors += 1;
            }
        }
        return (errors == 0);

    }

    // DHTML email validation script. Courtesy of SmartWebby.com (http://www.smartwebby.com/dhtml/)
    function signuphead_echeck(str) {
        var at = "@";
        var dot = ".";
        var lat = str.indexOf(at);
        var lstr = str.length;
        var ldot = str.indexOf(dot);
        if (str.indexOf(at) == -1) {
            return 0;
        }
        if (str.indexOf(at) == -1 || str.indexOf(at) == 0 || str.indexOf(at) == lstr) {
            return 0;
        }
        if (str.indexOf(dot) == -1 || str.indexOf(dot) == 0 || str.indexOf(dot) == lstr) {
            return 0;
        }
        if (str.indexOf(at, (lat + 1)) != -1) {
            return 0;
        }
        if (str.substring(lat - 1, lat) == dot || str.substring(lat + 1, lat + 2) == dot) {
            return 0;
        }
        if (str.indexOf(dot, (lat + 2)) == -1) {
            return 0;
        }
        if (str.indexOf(" ") != -1) {
            return 0;
        }
        return 1;
    }
	</script>
