    $('.home-gal-tabs div').hide();
    $('.home-gal-tabs div:first').show();
    $('.home-gal-tabs .filters ul li a').click(function () {
        $('.home-gal-tabs ul li a').removeClass('active');
        $(this).addClass('active');
        var currentTab = $(this).attr('href');
        $('.home-gal-tabs div').hide();
        $(currentTab).show();
        return false;
    });
