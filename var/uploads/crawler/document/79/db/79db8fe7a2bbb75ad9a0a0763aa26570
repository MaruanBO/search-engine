<!-- Microsite Overlay -->
            <script type="text/javascript">
				var MICROSITE_BASEURL = '//stores.guitarcenter.com/';

				if (typeof getCookie === "undefined") {
					getCookie = function (name) {
						var arg = name + "=";
						var alen = arg.length;
						var clen = document.cookie.length;
						var i = 0;
						while (i < clen) {
							var j = i + alen;
							if (document.cookie.substring(i, j) == arg)
								return getCookieVal(j);
							i = document.cookie.indexOf(" ", i) + 1;
							if (i == 0) break;
						}
						return null;
					}
				}

                $(window).load(function () {
                    if (getCookie("profileId") != null) {
                        var pId = getCookie("profileId");
                        var wId = getCookie("widgetId");
                        var contType = getCookie("profileType");
