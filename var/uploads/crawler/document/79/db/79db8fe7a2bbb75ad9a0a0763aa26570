<script type="text/javascript">
    var WelcomeMat = {
        url: 'https://embassy.fiftyone.com/welcome/welcome.srv',
        merchantId: '2083',
        userCountryCode: 'US',
        enableLogging: false
    };

    /*** FiftyOne International Visitor Welcome Mat | July 2010***/
    function wlcme51func(url) {
        var wlcme51 = document.createElement('script');
        wlcme51.src = url;
        wlcme51.type = 'text/javascript';
        document.getElementsByTagName('head')[0].appendChild(wlcme51);
    }

    function isWelcome() {
        var c_name = 'wlcme';
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + '=');
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(';', c_start);
                if (c_end == -1)
                    c_end = document.cookie.length;
                return unescape(document.cookie.substring(c_start, c_end))
            }
        }
        return '';
    }

    try {
        if (!isWelcome()) {
            wlcme51func(WelcomeMat.url + '?merchId=' + WelcomeMat.merchantId + '&countryId=' + WelcomeMat.userCountryCode + '&setCookie=Y');
            // Only perform logging if it's enabled on the server
            if(WelcomeMat.enableLogging) {
                $.ajax({
                    type: 'POST',
                    async: true,
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    url: '/api/WelcomeMat/LogWelcomeMat',
                    data: '{ "currentLocation": "' + window.location.href + '" }',
                    success: function (response) {
                        performSuccessAction(response.d);
                    },
                    error: function (xhr, status, error) {
                    }
                });
            }
        }
    }
    catch (err) { }
    /*** End FiftOne Welcome Mat ***/
</script></html>
