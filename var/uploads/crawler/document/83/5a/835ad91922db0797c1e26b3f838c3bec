     sitecues.config = {
       site_id : 's-0000ee0c'
     };
     sitecues.showBadgeWhenReady = function (triesRemaining) {
         // This method is designed to force the sitecues badge to appear
         // only when the rest of our library is ready for use...
         var badge = document.getElementById('sitecues-badge');
         if (badge && sitecues.getAllModulesLoaded && sitecues.getAllModulesLoaded()) {
             // sitecues is ready
             // enable keyboard help...
             badge.setAttribute('role', 'link');
             badge.setAttribute('tabindex', '0');
             badge.addEventListener("keydown", function (e) {
                 if (e.keyCode == 13) {
                     window.open("/sitecueskeys.cfm", "_self");
                 }
             });
             // setup a fade-in effect
             badge.style.transition='opacity 0.6s linear';
             // make the badge appear
             badge.style.visibility='visible';
             badge.style.opacity='1';
         }
         else if (triesRemaining > 0) {
             // sitecues is not completely ready, so recurse this function after the wait period,
             // all arguments after that get passed to the function (this is non-blocking)...
             sitecues.showBadgeTimer = setTimeout(sitecues.showBadgeWhenReady, 150, triesRemaining - 1);
         }
     };
     (function(){
       var script = document.createElement('script'),
       first = document.getElementsByTagName('script')[0];
       sitecues.config.script_url=document.location.protocol+'//js.sitecues.com/l/s;id='+sitecues.config.site_id+'/js/sitecues.js';
       script.addEventListener("load", function() {
           var triesRemaining = 100;
           sitecues.showBadgeWhenReady(triesRemaining);
       });
       script.src=sitecues.config.script_url;
       script.type = 'text/javascript';
       script.async = true;
       first.parentNode.insertBefore(script, first);
     })();
