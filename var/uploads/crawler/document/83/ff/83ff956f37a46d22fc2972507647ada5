<script type="text/javascript">
    var emailBox = $('#footerEmail');    
    var Notification = $('#responseNotification');

    emailBox.keypress(function (event) {
        if (event.keyCode == 13) {
            $("#footerSubmit").click();
        }
    });

    $('#footerSubmit').click(function (e) {
        e.preventDefault();
        var emailValue = emailBox.val();
        //simple validation
        var validated = true;
        if (emailValue == null || emailValue == "") {
            validated = false;
        }

        var atpos=emailValue.indexOf("@");
        var dotpos=emailValue.lastIndexOf(".");
        if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= emailValue.length) {
            validated = false;
        }
            
        if (validated) {
            var request = $.ajax({
                type: "POST",
                dataType: "json",
                url: "/handlers/ListrakSubscribe.ashx",
                data: { email: emailBox.val(), subscribe: "true", sf: "false", ltk: "true" }
            });
            request.done(function (msg) {
                //alert("Response: " + msg);   
                emailBox.val('');
                Notification.html('Thank you for signing up!');
                Notification.fadeIn('slow');
                setTimeout("Notification.fadeOut('slow');", 5000);
                setTimeout("emailBox.val('Email Address');", 8000);
            });
            request.fail(function (jqXHR, textStatus) {
                //alert("Request failed: " + textStatus);
                Notification.html("Sorry, we've encountered a temporary error and could not complete your request. Please try again.<br />If the problem persists, please <a href='contact.aspx'>contact our support team</a>");
                Notification.fadeIn('slow');
            });
        }

        else {
            //alert("Response: " + msg);   
            emailBox.val('');
            Notification.html('Please enter a valid email address.');
            Notification.fadeIn('slow');
            setTimeout("Notification.fadeOut('slow');", 5000);
        }
    });
</script>


