		facebook.prevent_auto_login = false;
	}

	window.fbAsyncInit = function() {

		try {

			if (typeof FB === 'undefined') {
				return;
			}

			if (typeof redirect === 'undefined') {
				var redirect = null;
			}

			FB.init({
				appId: 241284008322,
				channelUrl: 'http://www.deezer.com/fbgateway.html',
				status: true,
				cookie: true,
				xfbml: false,
				oauth: true,
				music: true
			});

			FB.getLoginStatus(function(statusResponse) {
				facebook.logged = (statusResponse.status === 'connected' || statusResponse.status === 'not_authorized');
				Events.trigger(Events.facebook.user_status, {
					status: statusResponse.status
				});
				if (statusResponse.status === 'connected') {
					if (USER.USER_ID !== 0 || facebook.access_token || facebook.prevent_auto_login) {
						facebook.init(!facebook.prevent_auto_login);
						return;
					}
					facebook.access_token = statusResponse.authResponse.accessToken;
					facebook.connect(statusResponse.authResponse.userID, redirect);
					return;
				}
			}, true);

		} catch(e) {
			error.log(e);
		}
	};

