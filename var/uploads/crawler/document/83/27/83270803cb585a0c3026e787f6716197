<script type="text/javascript">

	googleplus.client_id = '630457285321-d5nmkpnjktjoeh426nvdu40rch1b1qe0.apps.googleusercontent.com';

	(function() {
		var po = document.createElement('script');
		po.type = 'text/javascript';
		po.async = true;
		po.src = 'https://apis.google.com/js/client:plusone.js?onload=renderggplus';
		po.text = "{ parsetags : 'explicit' }";
		//var param = document.createTextNode("{ parsetags : 'explicit' }");
		//po.appendChild(param);
		var s = document.getElementsByTagName('script')[0];
		s.parentNode.insertBefore(po, s);
	})();

	function renderggplus(id) {

		if (typeof(gapi) == 'undefined') {
			return false;
		}

		if (typeof(id) == 'undefined') {
			id = 'hp_create_account_gp';
		}

		var $gg_button = $('#' + id);

		if ($gg_button.is('[data-gg-init="true"]')) {
			return false;
		}

		$gg_button.attr('data-gg-init', true);
		$gg_button.off().on('click', function() {
			var redirect = $(this).data('redirect');
			if (typeof(redirect) !== 'undefined' && redirect !== false) {
				googleplus.redirect = redirect;
			}
			googleplus.gplus_clicked = true;
		});

		gapi.signin.render(id, {
			'clientid' : googleplus.client_id,
			'callback' : 'signInCallback',
			'cookiepolicy' : 'single_host_origin',
			'requestvisibleactions' : 'http://schemas.google.com/AddActivity http://schemas.google.com/ListenActivity http://schemas.google.com/CommentActivity',
			'scope' : 'https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/userinfo.email',
			'accesstype' : 'offline',
			'apppackagename' : 'deezer.android.app',
			'redirecturi' : 'postmessage'
		});

		Events.resolve(Events.googleplus.ready);
	}

	function signInCallback(authResult) {

		if (googleplus.gplus_clicked === false) {
			return false;
		}

		if (authResult['access_token']) {

			googleplus.code = authResult['code'];

			// Load the oauth2 libraries to enable the userinfo methods.
			gapi.client.load('oauth2', 'v2', function() {

				var request = gapi.client.oauth2.userinfo.get();
				request.execute(function(r) {

					if (typeof(r.id) != 'undefined') {
						googleplus.connect(r.id);
					}
				});
			});
		}

		googleplus.gplus_clicked = false;

		return true;
	}
</script>
