			var compLen = comparisons.length;
			for( var j=0; j < compLen; j++ ) {
				var comparison = comparisons[ j ];
				var columns = comparison.comparisonData;

					/* Determine if its an image row */
					var columnLen = columns.length;
					var hasImage = false;
					for( var x=0; x < columnLen; x++ ) {
						if(columns[x].url) {
							hasImage = true;
							break;
						}
					}
		%>
