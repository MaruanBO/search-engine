<script type="text/html" id="com_scion_views_comparator_CompareGrid_SUCCESS">


	<%

	/**
	 * Trim Data Structure
	 *		trimId:
	 *		name:
	 *		imageURL:
	 *		year:
	 *		makeName:
	 *		modelName:
	 */
	if(compareResults.compareIds) {

		var
				scionTrim =  compareResults.scionTrim
			,	compareTrims = compareResults.compareTrims
			, 	numberOfOtherColumns = (compareResults.numberOfOtherColumns >= 3) ? 3 : compareResults.numberOfOtherColumns;
		%>

		<table class="gridHeader">
			<!-- Scion Vehicle -->
			<td class="tableHeader">
				<div class="blankColumnContainer">
					&nbsp;
				</div>
			</td>
			<td class="scionHeader tableHeader">
				<div class="vehicleContainer">
					<div class="compareContent">
						<img src="<%=scionTrim.imageURL%>" />
						<div><%=scionTrim.year%> <%=scionTrim.makeName%> <%=scionTrim.modelName%></div>
						<div><%=scionTrim.name%></div>
					</div>
					<div class="selectScion">SELECT A SCION</div>
				</div>
			</td>

			<%
				var len = (compareTrims.length >= 3) ? 3 : compareTrims.length;
				for(var i=0;i<len;i++) {
					var compareTrim = compareTrims[i];
					var isLast = (i == numberOfOtherColumns - 1);
					var classes = "compareHeader tableHeader";

					if(isLast) {
						classes += " last";

					}
			%>
			<td class="<%=classes%>">
				<div class="vehicleContainer">
					<div class="compareContent">
						<img src="<%=compareTrim.imageURL%>" />
						<div><%=compareTrim.year%> <%=compareTrim.makeName%> <%=compareTrim.modelName%></div>
						<div><%=compareTrim.name%></div>
					</div>
					<div class="changeVehicle" id="<%=compareTrim.trimId%>ChangeVehicle">CHANGE VEHICLE</div>
					<image class="removeVehicle" id="<%=compareTrim.trimId%>RemoveVehicle" src="/images/compare/comparator_vehicle_remove.gif"/>
				</div>
			</td>
			<%
				}

					for(var i=len;i<numberOfOtherColumns;i++) {

						var classes = "";
						if(i == (numberOfOtherColumns - 1)) {
							classes = "last";
						}
			%>
						<td class="addHeader tableHeader <%=classes%>">
							<div class="vehicleContainer">
								<div class="changeVehicle">ADD VEHICLE</div>
							</div>
						</td>

			<%
					}
			%>
		</table>
		<!-- Header -->
		<div class="comparatorHeader">
			<div class="overview btn tab">OVERVIEW</div>
			<div class="advantages btn tab">ADVANTAGES</div>
			<div class="gallery btn tab">GALLERY</div>
			<div class="spacer">&nbsp;</div>
			<div class="btn action reset">RESET</div>
			<div class="btn action email">EMAIL</div>
			<div class="btn action print">PRINT</div>
			<div class="btn expandAll"><span>+</span> EXPAND ALL</div>
			<div class="clear"></div>
		</div>
		<div class="gridView"></div>
		<div class="logo"><a href="http://www.chromedata.com" class="interstitialLink"><img src="/images/compare/chrome.png" class="tooltip" text='Certain specifications, prices and equipment data have been provided under license from Chrome Data Solutions ("Chrome Data"). ©2012 Chrome Data Solutions, LP. All Rights Reserved. This information is supplied for personal use only and may not be used for any commercial purpose whatsoever without the express written consent of Chrome Data. Chrome Data makes no guarantee or warranty, either express or implied, including without limitation any warranty of merchantability or fitness for particular purpose, with respect to the data presented here. All specifications, prices and equipment are subject to change without notice.' /></a></div>
		<div class="trimListOverlay"></div>
		<div class="scionListOverlay"></div>
	<%
	}
