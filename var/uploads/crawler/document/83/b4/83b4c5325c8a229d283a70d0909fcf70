	<% var
			l = layout.length
		,	i
		;
		for( i=0; i<l; i++) {
			var column = layout[ i ];
			%>
		<!-- start column-->
		<div class="column">

			<% var
					cl = column.length
				,	ii
				;
				for( ii = 0 ; ii < cl ; ii++ ) {
					var entry = column[ii]; %>

				<!-- start header -->
				<h4>
					<a href="<%=entry.header.url%>" <%=entry.header.target%> <%=entry.header.addClass%> > <%=entry.header.text%></a>
				</h4>
				<!-- end header -->
				<!-- start nav links -->
					<% if( entry.children.length > 0 ) {
							var
								iii
							,	enL = entry.children.length
							;

							for( iii = 0 ; iii < enL; iii++ ) {
								var child = entry.children[ iii ]; %>

								<div class="nav-item">
									<a class="nav-link <%=child.classes%>" href="<%=child.url%>"><%=child.text%></a>

									<!-- subnav start -->
									<% if( typeof( child.subnav ) !== "undefined" && child.subnav.length > 0 ) {
										var
											iv
										,	subnavLen = child.subnav.length
										;

										for( iv = 0; iv < subnavLen; iv++) {
											var item = child.subnav[ iv ]; %>

											<div class="subnav-item">
												<a class="nav-link <%=item.classes%>" href="<%=item.url%>"><%=item.text%></a>
											</div>

										<% } %>
										<!-- subnav end -->
										<div class="row-spacer">&nbsp;</div>
									<% } %>

								</div>
							<% } %>


					<% } %>
				<!-- end nav links -->
				<div class="row-spacer">&nbsp;</div>
			<% } %>

		<!-- close column -->
		</div>
	<% } %>
