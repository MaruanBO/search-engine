</script><script type="text/html" id="com_scion_views_event_EventList">
	<%
	/* TODO: find out why IDs are used on these DOM nodes */
	var
		i
	,	event
	,	j
    ,   k
	,	eventLtr
	,	eventAsset
	,	types = com_scion.CONST.EVENT_TYPES
	,	eventImg = "flyerImg"
	,	hasFlyer
	, 	ii
	,	jj
	,	eventInst
	,	eventInstArr = []
	,	evtIstItem
	,	event_type
	,	monthArr = ["January","February","March","April","May","June","July","August","September","October","November","December"]
	,	month
	,	instMonth
	,	day
	,	year
	,	newDate
	,	date
	;
	%>
	
	<div class="sortLinks <%=showControls%>">
		Sort By: <a href="" class="dateSort">Date</a> | <a href="" class="eventSort">Event Type</a>
	</div>
	<table id="eventsListTable" class="tablesorter">
		<thead>
			<tr>
				<th></th>
				<th></th>
				<th></th>
				<th></th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% 
			for( i=0; i<events.length; i++){
				event = events[i];
				if (!event.cancelled) {
					for( jj=0; jj<event.eventInstances.length; jj++){ 
						evtIstItem = event.eventInstances[jj]; 
						if (!evtIstItem.cancelled) {
							evtIstItem.eventType = event.eventType;
							evtIstItem.code = event.code;
							if (!evtIstItem.name) {
								evtIstItem.name = event.name;
                                evtIstItem.venueName = event.venueName;
                                evtIstItem.streetAddress = event.streetAddress;
                                evtIstItem.city = event.city;
                                evtIstItem.state = event.state;
                                evtIstItem.zipCode = event.zipCode;
                                evtIstItem.code = event.code;
                                evtIstItem.minimumAge = event.minimumAge;
                                evtIstItem.externalRegistrationUrl = event.externalRegistrationUrl;
                                evtIstItem.waitlistMessage = event.waitlistMessage;
							}
							if (!evtIstItem.description) {
								evtIstItem.description =event.description;
							}
							if (event.assetMap) { 
								evtIstItem.assetMap = {};
								if (event.assetMap.flyerThumbnail) {
									evtIstItem.assetMap.flyerThumbnail = event.assetMap.flyerThumbnail;
								}
							}
                            if (!evtIstItem.registrationType) {
                                evtIstItem.registrationType = event.registrationType;
                            }
                            if (!evtIstItem.externalRegistrationUrl) {
                                evtIstItem.externalRegistrationUrl = event.externalRegistrationUrl;
                            }
                            
                            if (!evtIstItem.maxPrimaryCapacity) {
                                evtIstItem.maxPrimaryCapacity = event.maxPrimaryCapacity;
                                evtIstItem.maxSecondaryCapacity = event.maxSecondaryCapacity;
                                evtIstItem.maxGuests = event.maxGuests;
                            }
                            
							eventInstArr.push(evtIstItem);
						}	
					}
				}
			} 
			
			eventInstArr.sort(function(a, b){
				return a.utcStartMillis-b.utcStartMillis;
			});
			for( ii=0; ii<eventInstArr.length; ii++){ 						
						eventInst = eventInstArr[ii]; 
						if (!eventInst.cancelled) {
							for (var j = 0; j < types.length; j++) {
								if (eventInst.eventType == types[j].name) {
									event_type = types[j].code;
									eventLtr = types[j].letter;
								}
							}
							%>
							<tr class="<%=event_type %>">
								<td class="type backgroundColor"><%=eventLtr %></td>
								<%
								newDate = new Date(eventInst.utcStartMillis);
								instMonth = eventInst.localStartDateString.split(" ")[0];
                                for(var k=0;k<monthArr.length;k++) {
                                    if ( instMonth == monthArr[k] ) {
                                           month = (k+1).toString();
                                    }
                                }
								month = (month.length == 1) ? '0'+month : month;
								year = newDate.getFullYear().toString();
								day = newDate.getDate().toString();
								day = (day.length == 1) ? '0'+day : day;
								date = month + '/' + day;
								%>
								<td class="date">
									<div style="display: none;"><%=eventInst.utcStartMillis %></div>
									<span class="typeColor"><%=date %></span><br/>
									<%=event.city %>
								</td>
								<td class="img">
									<a href="/events/detail/#code=<%=eventInst.code %>&date=<%=year %>-<%=month %>-<%=day %>" class="eventImg" name="<%=eventInst.name %>">
										<% 
										
											if (eventInst.assetMap && eventInst.assetMap.flyerThumbnail) {
												hasFlyer = true;  %>
												<img class="listFlyer" src="<%=eventInst.assetMap.flyerThumbnail %>" />
											<% 
											} else {
												hasFlyer = false; %>
												<img class="fallbackImg" src="/images/common/spacer.gif" />
											<% 
											}
										%>
									</a>
								</td>
								<td class="desc">
									<span><a href="/events/detail/#code=<%=eventInst.code %>&date=<%=year %>-<%=month %>-<%=day %>">
										<%=eventInst.name %>
									</a></span><br/>
									<%=eventInst.description %>
								</td>
								<td class="rsvp">
									<% 
									if (eventInst.externalRegistrationUrl) { %>
										<a class="button" href="<%=eventInst.externalRegistrationUrl%>" target="_blank">
											RSVP
										</a>
									<% 
									} else { 
										if ((!eventInst.registrationClosed) && (eventInst.guestCount < eventInst.maxPrimaryCapacity)) { %>
											<a class="button rsvp" href="#" name="<%=eventInst.name %>" description="<%=eventInst.description %>" venue="<%=eventInst.venueName %>" address="<%=eventInst.streetAddress %>" city="<%=eventInst.city %>" state="<%=eventInst.state %>" zipCode="<%=eventInst.zipCode %>" code="<%=eventInst.code %>" startTime="<%=eventInst.utcStartMillis %>" endTime="<%=eventInst.utcEndMillis %>" guestCnt="<%=eventInst.maxGuests %>" minAge="<%=eventInst.minimumAge %>">
												RSVP
											</a>
