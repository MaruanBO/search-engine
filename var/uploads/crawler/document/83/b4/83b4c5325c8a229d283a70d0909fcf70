<script type="text/html" id="com_scion_views_nav_Primary">
	<%
		var
			items = items || []
		,	item
		,	link
		,	text
		,	liClass
		,	textClass
		,	i
		,	l
		;
	%>
	<div class='navContent'>
		<div class='logo'>
			<a href="/pub/"><img src='/assets/mast.png'/></a>
		</div>
		<ul>
			<% for( i = 0, l = items.length; i < l; i++ ){
				item = items[ i ];
				link = item.url || "";
				text = item.text || "";
				section = item.address || "";
				liClass =
					item.className
				+	( item.isActive ? " active" : "" )
				+	( i == ( l-1 ) ? " last" : "" )
				;
				textClass =
					( item.isActive ? "" : ( item.flyout ? "flyout" : "" ) ) + " "
				+	( item.inset ? "indent" : "" ) + " "
				+	( item.transformOverride ? "notransform" : "" )
				;
				target = item.newWindow ? "_blank" : "_self";
				%>
				<li class="<%=liClass%>"><%
						%><a href="<%=link%>" class="<%=textClass%>" section="<%=section%>" target="<%=target%>"><%=text%></a><%
				%></li>
				<%
			} %>
		</ul>
	</div>
</script>
<script type="text/html" id="com_scion_views_nav_Secondary">
<%
	var
		breadcrumbs = breadcrumbs || []
	,	modelClass
	,	subsections = subsections || []
	,	sectionClass = sectionClass || ""
	,	subsection = subsection || ""
	,	item
	,	i
	,	l
	,	hTag
	,	col
	,	isActive = ""
	,	isLast = ""
	,	newWin = ""
	;

	if( breadcrumbs.length > 0 ){ %>
	<ul class="breadcrumbs">
		<% for( i = 0, l = breadcrumbs.length; i < l; i ++ ){
			item = breadcrumbs[ i ];

			hTag = "h" + ( i + 1 );
			modelClass = ( isModelSection  && hTag === "h1" ) ? "model" : "";
			if( item.text && item.link ){ %>
				<li><span class="<%= hTag %> <%= modelClass %>"><a href="<%= item.link %>" data-address="<%= item.address %>" data-level="<%= hTag %>"><%= item.text %></a></span></li>
			<% } else if (item.text) { %>
				<li><span class="<%= hTag %> <%= modelClass %>" data-address="<%= item.address %>"><%= item.text %></span></li>
			<% } %>

			<%
				if(i+1 < l) {
			%>
				<li class="divider"><div>&nbsp;</div></li>
			<%
				}
			%>
		<% } %>
	</ul>
	<%
	}
	if( subsections.length > 0 && showSubsections ) { %>
	<ul class="subsections <%= sectionClass %>">
		<% for( i = 0, l = subsections.length; i < l; i ++ ){
			item = subsections[ i ];
			col = "col" + ( i + 1 );
			/*isActive = ( subsection == item.key ) ? "active" : ""; /* TODO: less fragile method for this */
			isActive = ( document.location.pathname.replace( "index.html", "" ) === item.link.replace( "index.html", "" ) ) ? "active" : "";


			/* if active, we dont show the link */
			if(isActive) {
				item.link = "";
			}
			isLast = ( i == ( l - 1 ) ) ? "last" : "";

			if( item.link.indexOf( "http://" ) === 0 ){
				newWin = " target='_blank' ";
			} else {
				newWin = "";
			}

			if( item.text && item.link ){ %>
				<li class="<%= col %> <%= isActive %> <%= isLast %>"><a href="<%= item.link %>" key="<%= item.key %>" <%= newWin %>><%= item.text %></a></li>
			<% }
			else if(item.text) { %>
				<li class="<%= col %> <%= isActive %> <%= isLast %>"><span><%= item.text %></span></li>
			<% } %>
		<% } %>
	</ul>
	<% } else if( returnLink ) { %>
		<div class="line"></div>
		<a 
			href="<%=returnLink.url%>"
			class="returnLink"
			data-address="<%= item.address %>"
			><img 
				class="arrowBack" 
				src="/images/common/spacer.gif" 
				width="5" height="10" alt="back"
			><%=returnLink.text%></a>
	<% } else { %>
		<div class="line"></div>
	<% } %>
	<!-- TODO: this should be elsewhere, right?  In sectional templates/pages?  a Ternary nav?
		<ul class="categories">
		<li></li>
	</ul> -->
</script><div id="fb-root"></div>
