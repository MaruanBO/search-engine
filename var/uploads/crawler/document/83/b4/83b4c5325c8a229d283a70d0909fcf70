</script><script type="text/html" id="com_scion_views_comparator_Advantages_SUCCESS">

	<table>
	<%
		var 
				categories = compareResults.category
			,	catLen = categories.length
			, 	scionTrim = compareResults.scionTrim
			;
	%>
	<% if(competitorTrim) { %>
		<tr class="categoryHeader">
				<td colspan="2"><div>
					ADVANTAGES OF Scion <%=scionTrim.modelName%> OVER <%=competitorTrim.makeName%> <%=competitorTrim.modelName%>
				</div></td>	
		</tr>	
		
		
		<tr class="compareFeature header">
				<th class="headerColumn"><div>FEATURE</div></th>
				<td class="headerColumn"><div>ADVANTAGE</div></td>
		</tr>
		<%
			var competitorIndex = $.inArray(competitorTrim.trimId, compareResults.compareIds);
			
			/**
			 * Loop over all categories in the compare reuslts
			 */ 
			for(var i=0;i<catLen; i++) {
				var 
						category = categories[i]
					,	comparisons = category.comparison
					;
				
				/*
			 	* Loop over each compare feature row and then build each column 
			 	*/
				var compLen = comparisons.length;
				for( var j=0; j < compLen; j++ ) {
					var comparison = comparisons[j];
					var columns = comparison.comparisonData;
							
						/* if scion has advantage */
						if(columns[0].hasAdvantage == "True") {
							var advantages = columns[0].advantageText.split("|");
							
							var advantage = advantages[competitorIndex];
							
							if(advantage != "") {
			%>
					<tr class="compareFeature">
						<th><div><%=comparison.name%></div></th>
						<td><div><%=advantage%></div></td>
						
						</tr>
			
			<%	
								
							}						
						}
				}
			}
		
		%>
		
	<% }
	/* Shown when no competitor is selected.*/
	else {
	%>
		<tr class="categoryHeader">
			<td colspan="2"><div>
					&nbsp;
				</div></td>	
		</tr>
		<tr>
			<td colspan="2" class="noResults"><div>
				Please select a competitive vehicle to see a complete list of Scion advantages compared to that competitor.
			</div></td>			
		</tr>
	<%}%>
	
	<tr class="categoryHeader">
		<td colspan="2"><div>
			&nbsp;
		</div></td>	
	</tr>	
	</table>
