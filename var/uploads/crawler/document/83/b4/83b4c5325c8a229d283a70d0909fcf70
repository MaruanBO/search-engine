<script type="text/html" id="com_scion_views_overlays_EventRsvp">
	<% 
	var monthNames = [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" ];
	var startDate;
	var month;
	var day;
	var year;
	var startTime;
	var endTime;
	
	%>
