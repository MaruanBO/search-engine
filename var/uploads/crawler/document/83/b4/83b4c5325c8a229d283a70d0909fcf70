		<h4>RSVP FOR EVENT</h4>
	</div>
	<div class="eventInfo">
		<span><%=event.name%></span><br/>		
		<%
			startDate = new Date(parseInt(event.startTime));
			month = monthNames[startDate.getMonth()];
			day = startDate.getDate().toString();
			year = startDate.getUTCFullYear().toString();
		%>		
		<%=month%> <%=day%>, <%=year%><br/>
		<%=event.venue%> <% if (event.venue) { %> | <% } %> <%=event.address%> <% if (event.address) { %> | <% } %> <%=event.city%>, <%=event.state%> <%=event.zipCode%>
	</div>
    <% if (event.waitlistMessage) { %>
        <div class="registrationMsg">
            <%=event.waitlistMessage %>
        </div>
    <% } %>
	<iframe src="/ssl/events/rsvp_slim.html?evtCode=<%=event.code%>&name=<%=event.name%>&venue=<%=event.venue%>&description=<%=event.description%>&city=<%=event.city%>&state=<%=event.state%>&zipCode=<%=event.zipCode%>&address=<%=event.address%>&starttime=<%=event.startTime%>&endtime=<%=event.endTime%>&guestCnt=<%=event.guestCnt%>&minAge=<%=event.minAge%>" name="rsvp" frameBorder="0"></iframe>
