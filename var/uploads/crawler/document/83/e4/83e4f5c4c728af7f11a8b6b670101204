  <!-- GPT: Configuration for all possible ad slots --><script type='text/javascript'>

  googletag.cmd.push(function() {
    
    var screwattackPageID = "interior";
    
    if(typeof Drupal.settings.screwattackGPTPage != 'undefined'){
      var screwattackPageID = Drupal.settings.screwattackGPTPage;
    }
        
    //-------------------------------------------------------------------------------------------|
    //  Size mappings
    //-------------------------------------------------------------------------------------------|
      /*
        Greater than 1124px aka "Desktop” 
        (2)*728x90 above and below the fold
        (1)*300x250/300x600 hybrid tag above the fold where possible.
        (1)*stand alone 300x250 below the fold where possible.
        
        Less than 1124px, greater than 730px aka “Tablet”
        (2)*728x90 above and below the fold
        (1)*160x600 hybrid tag above the fold where possible.
        (1)*160x600 hybrid tag below the fold where possible.
        
        Less than 730px aka “Mobile”
        (2)*320x50/300x50 hybrid above the fold and below the fold.
      */
      
      
      var mappingHorizontal = googletag.sizeMapping().
       addSize([730, 1 ], [728, 90]).
       addSize([0, 0 ], [320, 50]).
       addSize([0, 0 ], [300, 50]).
       build();
       
      var mappingSidebar = googletag.sizeMapping().
       addSize([1124, 1 ], [300, 250]).
       addSize([1124, 1 ], [300, 600]).
       addSize([586, 1 ], [160, 600]).
       addSize([0, 0], [300, 250]).
       addSize([0, 0], [300, 600]).
       build();
      
      
      
    //-------------------------------------------------------------------------------------------|
    //  End size mappings
    //-------------------------------------------------------------------------------------------|
    
    //-------------------------------------------------------------------------------------------|
    //  Slots
    //-------------------------------------------------------------------------------------------|
    
      //--Horizontal Above the Fold------------------------------------------------------------------------|
        googletag.defineSlot(
          '/212735230/fs.screwattack/'+screwattackPageID, 
          [[728, 90], [320, 50], [300, 50]], 
          'screwattack-gpt-banner'
        ).defineSizeMapping(mappingHorizontal).addService(googletag.pubads()).setTargeting("pos", ["atf"]);
      
      //--Horizontal Below the Fold------------------------------------------------------------------------|
        googletag.defineSlot(
          '/212735230/fs.screwattack/'+screwattackPageID, 
          [[728, 90], [320, 50], [300, 50]],
          'screwattack-gpt-banner2'
        ).defineSizeMapping(mappingHorizontal).addService(googletag.pubads()).setTargeting("pos", ["btf"]);
        
        
      //--Sidebar Ads, Slot1------------------------------------------------------------------------|
        googletag.defineSlot(
          '/212735230/fs.screwattack/'+screwattackPageID, 
          [[300, 250], [300, 600], [160, 600]],
          'screwattack-gpt-sidebar1'
        ).defineSizeMapping(mappingSidebar).addService(googletag.pubads()).setTargeting("pos", ["atf"]).setCollapseEmptyDiv(true,true);
        
      //--Sidebar Ads, Slot2------------------------------------------------------------------------|
        googletag.defineSlot(
          '/212735230/fs.screwattack/'+screwattackPageID, 
          [[300, 250], [300, 600], [160, 600]],
          'screwattack-gpt-sidebar2'
        ).defineSizeMapping(mappingSidebar).addService(googletag.pubads()).setTargeting("pos", ["btf"]).setCollapseEmptyDiv(true,true);

      
      //--Out-Of-Page Slots------------------------------------------------------------------------|
        googletag.defineOutOfPageSlot('/212735230/fs.screwattack/'+screwattackPageID, 'screwattack-gpt-oop').addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
    
    //-------------------------------------------------------------------------------------------|
    //  End slots
    //-------------------------------------------------------------------------------------------|
    
    googletag.pubads().enableSingleRequest();
    googletag.enableServices();
  });

</script>
