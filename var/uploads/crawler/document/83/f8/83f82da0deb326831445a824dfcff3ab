            <script type="text/javascript">

function searchableBySLI(srchForm){

   var prefixItemNumberPattern =/^([a-zA-Z0-9]{1}[a-zA-Z0-9]{1})?[0-9]{3,6}$/;

   if (srchForm.keyword.value.search(prefixItemNumberPattern)!=-1) {
		//Search by Lucene
    	return false;
   }else{
		//Search by SLI
        return true;
   }

}

function searchProductItem(srchForm){

	
		   if (searchableBySLI(srchForm)){
			   //SLI search
				var search = escape(srchForm.keyword.value);
				var asug = escape(srchForm.asug.value);
			    window.location = "http://bedding.wards.com/search#w="+search+"&asug="+asug;
			    return false;
		   }else{
			   //Lucene search
			   srchForm.action = "/catalog/search.cmd";
			   srchForm.submit();
			   return false;
		   }
		

}

</script>



<!-- /search -->
