<script type="text/javascript">
    if (typeof window.location.search !== "undefined") {
        var arr_queryString = window.location.search.substring(1).toLowerCase().split('&');
        var affiliate_comp;
        
        // Loop through each querystring key=value pair.
        for (x=0; x<arr_queryString.length; x++) {
            // If the key is utm_source check the value
            if (arr_queryString[x].match(/^utm_source/)) {
                affiliate_comp = arr_queryString[x].substring(arr_queryString[x].indexOf("=")+1);
            }
        }

        var affiliate_comp_element = document.getElementById('kwp_' + affiliate_comp + '_banner_wrap');

        if (affiliate_comp && affiliate_comp_element) {
            affiliate_comp_element.style.display = "block";
        }
    }
</script></div>
