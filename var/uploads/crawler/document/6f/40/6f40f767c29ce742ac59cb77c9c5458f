			<script type='text/javascript' language='javascript'>
				$(document).ready(function() {
					var test = false;
var popupName = null;
var KoboiPopup = (function() {
	var ua = navigator.userAgent,
	getZoom, getTop, getLeft,
	popupInterval, popupClassName;
	
	function init() {
		var activePopup = $('<div class="KoboiPopup hidden ' + popupClassName + '" />').html('<b class="arrow"><b><b></b></b></b><p class="close"></p><img class="logo" src="http://ecimages.kobobooks.com/Merchandising.ashx?Resource=ipopup_kobo_logo.png" /><p>For fast access to the Kobo Store, simply tap <img class="share" src="http://ecimages.kobobooks.com/Merchandising.ashx?Resource=ipopup_share_button.png" /> and select <strong>Add to Home Screen</strong></p>').appendTo('body'),
		$window = $(window),
		didScroll = true,
		previousOffsetX = pageXOffset,
		previousOffsetY = pageYOffset;

		bindScroll(activePopup, $window, didScroll, previousOffsetX, previousOffsetY);
		bindClose(activePopup);
	}

	function bindScroll(activePopup, $window, didScroll, previousOffsetX, previousOffsetY) {
		$window.scroll(function() {
			didScroll = true;
		});

		popupInterval = setInterval(function() {
			if(didScroll || previousOffsetX != pageXOffset || previousOffsetY != pageYOffset) {
				didScroll = false;
				previousOffsetX = pageXOffset;
				previousOffsetY = pageYOffset;

				activePopup.css({
					'-webkit-transform': 'scale(' + getZoom() + ')',
					'top': getTop($window, activePopup),
					'left': getLeft($window, activePopup)
				}).removeClass('hidden');
			}
		}, 250);
	}

	function bindClose(activePopup) {
		activePopup.find('.close').click(function() {
			activePopup.addClass('hidden');
			Kookie.set('KoboiPopup', true, 7300); // 20 years or so
			clearInterval(popupInterval);
		});
	}
	return function() {
		var cookieValue = Kookie.get('KoboRepeatVisit');
		if(cookieValue=='true') {
			Kookie.set('KoboRepeatVisit', 1, 7300);
		}
		if(!Kookie.isset('KoboiPopup')) {
			if(test==true || (ua.indexOf('iPhone') != -1 || ua.indexOf('iPad') != -1)) {
				if(ua.indexOf('iPhone') != -1) {
					popupClassName = 'KoboiPopupiPhone';

					getZoom = function() {
						if(window.orientation == 0 || window.orientation == 180) {
							return (window.innerWidth / document.documentElement.clientWidth) * 1.8;
						} else {
							return (window.innerWidth / document.documentElement.clientWidth) * 1.5;
						}
					};

					getTop = function($window, activePopup) {
						return (window.innerHeight - ((activePopup.outerHeight() + 20) * getZoom())) + $window.scrollTop() + 'px';
					};

					getLeft = function($window, activePopup) {
						return (window.innerWidth / 2) - ((activePopup.outerWidth() * getZoom()) / 2) + $window.scrollLeft() + 'px';
					};
					
					//
					init();
				} else if( (test==true || (ua.indexOf('iPad') != -1) ) && Kookie.isset('KoboRepeatVisit')) {
					popupClassName = 'KoboiPopupiPad';
					getZoom = function() {
						if(window.orientation == 0 || window.orientation == 180) {
							return (window.innerWidth / screen.width) * 0.75;
						} else {
							return (window.innerWidth / document.documentElement.clientWidth) * 0.75;
						}
					};

					getTop = function($window) {
						return $window.scrollTop() + (20 * getZoom()) + 'px';
					};

					getLeft = function($window) {
						var stupidIOSTabbedBrowsing = 0;

						if(ua.indexOf('OS 5_') != -1) {
							stupidIOSTabbedBrowsing = 60;
						}

						if(window.orientation == 0 || window.orientation == 180) {
							return $window.scrollLeft() + ((201 - stupidIOSTabbedBrowsing) * getZoom()) + 'px';
						} else {
							return $window.scrollLeft() + ((192 - stupidIOSTabbedBrowsing) * getZoom()) + 'px';
						}
					};
					//
					init();
				}
				/*iPad large pop-up window for first time users*/
				else if ( (test==true || (ua.indexOf('iPad') != -1) ) &&
								(!Kookie.isset('KoboRepeatVisit') || (Kookie.isset('KoboRepeatVisit') && parseInt(Kookie.get('KoboRepeatVisit'))>1) )
						){
					//versionRand = Math.random();
						$('#html5_app').show();
					//if(versionRand >= 0.5){
					//	$('#alert_box_1').show();
					//	popupName = $('#alert_box_1').find('.launch_link').attr('data-version');
					//}
					//else{
						$('#alert_box_2').show();
						popupName = $('#alert_box_2').find('.launch_link').attr('data-version');
					//}
					
					//push to Google Analytics
					var timeout = 40, iteration = 0,
					waitForGA = setInterval(function() {
						if(typeof(_gaq) != 'undefined' && popupName) {
							clearInterval(waitForGA);
							_gaq.push(['_trackEvent', 'HTML5test', 'Load', popupName + ' popup displayed']);
						}
						iteration++;
						if(iteration == timeout) { clearInterval(waitForGA); }
					}, 100);
					//
					
					var count = parseInt(Kookie.get('KoboRepeatVisit'));
					
					Kookie.set('KoboRepeatVisit', count++, 7300); // mark machine as repeat user: 20 years or so
				}
			}
		}
		//Kookie.set('KoboRepeatVisit', true, 7300); // mark machine as repeat user: 20 years or so
	}
})();

new KoboiPopup();

//popup close button
$('.exit_popup').click(function() {
	$('#html5_app').hide();
});

$('.launch_link').click(function(){
	_gaq.push(['_trackEvent', 'HTML5test', 'Click', $(this).attr('data-version') + ' popup clicked']);
});

	(function() {
		var currentPage = location.pathname;
		
		if(currentPage.indexOf('/browse/') != -1) {
			/* Dynamic Remarketing Ads with Google */
