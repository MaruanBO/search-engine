<script type="text/javascript">
$(document).ready(function() {
   createde_dropdown();
   
   $(".de_dropdown dt a").click(function() {
       $(".de_dropdown dd ul").toggle();
   });

   $(document).bind('click', function(e) {
       var $clicked = $(e.target);
       if (! $clicked.parents().hasClass("de_dropdown"))
           $(".de_dropdown dd ul").hide();
   });
               
   $(".de_dropdown dd ul li a").click(function() {
       var text = $(this).html();
       $(".de_dropdown dt a").html(text);
       $(".de_dropdown dd ul").hide();

       var source = $("#source");
       source.val($(this).find("span.value").html())
   });
});

function createde_dropdown(){
   var source = $("#source");
   var selected = source.find("option[selected]");
   var options = $("option", source);
   
   $(".add-sites").append('<dl id="target" class="de_dropdown"></dl>')
   $("#target").append('<dt><a href="#add-sites">' +  selected.text()  +
       '</span></a></dt>')
   $("#target").append('<dd><ul></ul></dd>')

   options.each(function(){
       $("#target dd ul").append('<li><a href="'+ $(this).val() +'" >' + 
           $(this).text() +  '</span></a></li>');
   });
}

$("#direct_shareDiv").before("<div class='add-sites'><h3>Narrow Your Search</h3><select id='source' ONCHANGE='location = this.options[this.selectedIndex].value;'><option selected='selected' value='#add-sites'>Xerox Career Sites</option><option value='http://xerox-accounting.jobs/'>Accounting Jobs</option><option value='http://xerox-analyst.jobs/'>Analyst Jobs</option><option value='http://xerox-california.jobs/'>California Jobs</option><option value='http://xerox-callcenter.jobs/'>Call Center Jobs</option><option value='http://xerox-customercare.jobs/'>Customer Care Jobs</option><option value='http://xerox-customerservice.jobs/'>Customer Service Jobs</option><option value='http://xerox-engineering.jobs/'>Engineering Jobs</option><option value='http://xerox-finance.jobs/'>Finance Jobs</option><option value='http://xerox-humanresources.jobs/'>Human Resources Jobs</option><option value='http://xerox-india.jobs/'>India Jobs</option><option value='http://xerox-it.jobs/'>Information Technology Jobs</option><option value='http://xerox-kentucky.jobs/'>Kentucky Jobs</option><option value='http://xerox-marketing.jobs/'>Marketing Jobs</option><option value='http://xerox-maryland.jobs/'>Maryland Jobs</option><option value='http://xerox-mexico.jobs/'>Mexico Jobs</option><option value='http://xerox-newjersey.jobs/'>New Jersey Jobs</option><option value='http://xerox-northcarolina.jobs/'>North Carolina Jobs</option><option value='http://xerox-philippines.jobs/'>Philippines Jobs</option><option value='http://xerox-sales.jobs/'>Sales Jobs</option><option value='http://xerox-texas.jobs/'>Texas Jobs</option><option value='http://xerox-training.jobs/'>Training Jobs</option><option value='http://xerox-usa.jobs/'>USA Jobs</option><option value='http://xerox-veterans.jobs/'>Veterans Jobs</option><option value='http://xerox-virtual.jobs/'>Virtual Jobs</option><option value='http://xerox.jobs/'>All Jobs/Careers</option><option value='/'></option></select></div>");

