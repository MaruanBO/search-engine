<script language="javascript">
var ecard_searching = true;
$(function () {
 $("#fli-btnsearch input").click(function (e) {
        if ($('#search').val() == 'Search eCards' || $('#search').val() == 'Search Printables' || $('#search').val().trim() == '') {
            e.preventDefault();
        }
    });
    $("#search").click(function () {
        if ($("#search").val() == 'Search eCards' || $("#search").val() == 'Search Printables' || $('#search').val().trim() == '') {
            return false;
        }
    });
    document.getElementById('search').onkeypress = function (e) {
        var event = e || window.event;
        var charCode = event.which || event.keyCode;
        if (charCode == '13') {
            if ($('#search').val().trim() == '') {
                return false;
            }
        }
    }
    $('#header-search-choose').click(function () {
        $('#header-search-drop').show();
        $('#search').click(function () {
            $('#header-search-drop').hide();
            if ($("#search").val() == 'Search Printables' || $("#search").val() == 'Search eCards') this.val('');
        });
        if (ecard_searching) {
            $('a.ecard-search-link').hide();
            $('a.cnp-search-link').show();
            $("a.cnp-search-link").unbind();
            $("a.cnp-search-link").click(function (e) {
                ecard_searching = false;
                $('#search').attr('value', 'Search Printables');
                $('#header-search-drop').hide();
                $("#fli-btnsearch input").unbind();
                $("#fli-btnsearch input").click(function (e) {
                    if ($('#search').val() == 'Search eCards' || $('#search').val() == 'Search Printables' || $('#search').val().trim() == '') {
                        e.preventDefault();
                    } else {
                        e.preventDefault();
                        document.location.href = 'http://www.bluemountain.com/printable-cards/search/' + $('#search').val();
                    }
                });
                return false;

            });

        } else {
            $('a.ecard-search-link').show();
            $('a.cnp-search-link').hide();
            $("a.cnp-search-link").unbind();
            $("a.ecard-search-link").click(function (e) {
                ecard_searching = true;
                $('#search').attr('value', 'Search eCards');
                $('#header-search-drop').hide();
                $("#fli-btnsearch input").unbind();
                $("#fli-btnsearch input").click(function (e) {
                    if ($('#search').val() == 'Search eCards' || $('#search').val() == 'Search Printables' || $('#search').val().trim() == '') {
                        e.preventDefault();
                    } else {
                        e.preventDefault();
                        document.location.href = 'http://www.bluemountain.com/ecards/search/' + $('#search').val();
                    }
                });
                return false;
            });

        }
    });

});
</script>
