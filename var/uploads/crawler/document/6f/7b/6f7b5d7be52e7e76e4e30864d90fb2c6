            <script type="text/javascript">
dojo.addOnLoad(function(){
var early = "Good Morning";
    var midday = "Good Afternoon";
    var late = "Good Evening";

    var msg = document.getElementById('greeting');

    var localTime = new Date().getHours();
 
    if (msg) {
        if (localTime <= 11) {
            msg.innerHTML = early;
        } else if (localTime  <= 17) {
            msg.innerHTML = midday;
        } else {
            msg.innerHTML = late;
        }
    }
});
</script>
