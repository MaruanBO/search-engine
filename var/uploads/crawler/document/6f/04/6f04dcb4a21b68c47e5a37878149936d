    jQuery(document).ready(function(){
        var sudoSlider = jQuery("#slider").sudoSlider({
			effect: function (obj) {
				var orgCallback = obj.callback;
				var fadeOutObjects = jQuery("#slider").children();
				obj.callback = function () {
					fadeOutObjects.stop().animate({opacity: 1}, 0);
					orgCallback();
				}
				jQuery.fn.sudoSlider.effects.fade(obj);
				fadeOutObjects.animate({opacity: 0.001}, obj.options.speed);
			},
			auto: true,
			pause: 8000,
			continuous:true
        });
    });
</script>

