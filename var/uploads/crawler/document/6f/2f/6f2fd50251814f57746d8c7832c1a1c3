<script>

$( "#bg-links" ).click(function() {
  var link = $('#bg-links').css('content');
  link  = link.substring(1, link.length-1);
	if(link > ""){
  		window.open(link, '_blank');
	}
});

function getRandomInt(min, max) {
  return Math.floor(Math.random() * (max - min)) + min;
}

var bg_path = '';
var bg_path = bg_path.trim();
if(bg_path == ''){
	bg_path = '/';
}
var bg_path_safe = (bg_path != '/')? bg_path: '';		
//console.log(bg_path_safe);	
	
ord = getRandomInt(1,1000)
document.write('<SCR'+'IPT LANGUAGE="JavaScript1.1" SRC="http://ad.doubleclick.net/adj/a.site152.tmus' + bg_path_safe +  ';tile=1;pos=1;dcopt=ist;sz=882x836;ord=' + ord + '?" ><\/SCRIPT>');
if ((!document.images && navigator.userAgent.indexOf("Mozilla/2.") >= 0)  || navigator.userAgent.indexOf("WebTV")>= 0) {
	document.write('<' + 'a href="http://ad.doubleclick.net/jump/a.site152.tmus' + bg_path_safe + ';tile=1;pos=1;dcopt=ist;sz=882x836;ord=' + ord + '?" target="_blank">');
	document.write('<' + 'img src="http://ad.doubleclick.net/ad/a.site152.tmus' + bg_path_safe + ';tile=1;pos=1;dcopt=ist;sz=882x836;ord=' + ord + '?" border="0" alt=""><' + '/a>');
}
	
</script>

	

	



 
	
	
	
	<script>
		var interstitialTimer
		interstitialTimer = setInterval(function(){RemoveInterstitial()},30000);

		var IconHTML
		function RemoveInterstitial() {
			$('#interstitial-holder').hide();
		}

	</script>

	<script type="text/javascript">

