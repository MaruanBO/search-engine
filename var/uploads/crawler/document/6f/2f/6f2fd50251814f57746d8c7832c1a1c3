<script>

var gwRankings_world = angular.module('gwRankings_world', []);


function RankingController_world($scope, $http) {
	$scope.rankList = "http://rankings.golfweek.com/rankings/json/pro_top20.json";
	$scope.tournamentList = "";
	$scope.currentPage = 1;
	$scope.pageSize = 5;	

	$scope.$watch('rankList', function() {
		

		if($scope.rankList == 'json/boys_top20.json' || $scope.rankList == 'json/girls_top20.json'){
			$scope.colspan = 7
		} else if ($scope.rankList == 'json/men_top20.json' || $scope.rankList == 'json/women_top20.json') {
			$scope.colspan = 6
		} else {
			$scope.colspan = 5
		}

		$scope.q = ""
		$scope.r = ""
		$scope.s = ""
		$scope.t = ""


		$http.get($scope.rankList).success(function(response) {$scope.rankings = response;});

	});
}

</script>

	
  <div id="gwRankings_world" ng-cloak>
	  
    <div class="row">
      <div class="col-lg-12">

        <div ng-controller="RankingController_world" class="rankings-controller">

			
		  <div class="row">
			  <div class="col-lg-12">
              <table class="table table-hover table-condensed table-responsive ranking-table">
				<thead>
					<tr>
						<th>Rank</th>
						<th>Name</th>
						<th ng-if="rankList == 'json/men_top20.json' || rankList == 'json/women_top20.json'">Team</th>
						<th ng-if="rankList == 'json/boys_top20.json' || rankList == 'json/girls_top20.json'">GradYear</th>
						<th ng-if="rankList == 'json/boys_top20.json' || rankList == 'json/girls_top20.json'">State</th>
						<th>Rating</th>
					</tr>
				</thead>
				<tbody>
					<tr ng-repeat="rank in rankings | filter: { Name: q, GradYear: r, State: s, Team: t } | limitTo: 5">
						<td>{{ rank["Rank"] }}</td>
						<td>{{ rank["Name"] }}</td>
						<td ng-if="rankList == 'json/men_top20.json' || rankList == 'json/women_top20.json'">{{ rank["Team"] }}</td>
						<td ng-if="rankList == 'json/boys_top20.json' || rankList == 'json/girls_top20.json'">{{ rank["GradYear"] }}</td>
						<td ng-if="rankList == 'json/boys_top20.json' || rankList == 'json/girls_top20.json'">{{ rank["State"] }}</td>
						<td>{{ rank["Rating"] }}</td>
					</tr>
				</tbody>
              </table>
			  </div>
			</div>
        </div>
			 			
      </div>
    </div>
  </div>	

	<a href="http://rankings.golfweek.com/rankings/default.asp?t=lpga"><span>Women's Pro Rankings</span></a>
	

	
	
<script>

var gwRankings_lpga = angular.module('gwRankings_lpga', []);


function RankingController_lpga($scope, $http) {
	$scope.rankList = "http://rankings.golfweek.com/rankings/json/lpga_top20.json";
	$scope.tournamentList = "";
	$scope.currentPage = 1;
	$scope.pageSize = 5;	

	$scope.$watch('rankList', function() {
		

		if($scope.rankList == 'json/boys_top20.json' || $scope.rankList == 'json/girls_top20.json'){
			$scope.colspan = 7
		} else if ($scope.rankList == 'json/men_top20.json' || $scope.rankList == 'json/women_top20.json') {
			$scope.colspan = 6
		} else {
			$scope.colspan = 5
		}

		$scope.q = ""
		$scope.r = ""
		$scope.s = ""
		$scope.t = ""


		$http.get($scope.rankList).success(function(response) {$scope.rankings = response;});

	});
}

</script>

	
  <div id="gwRankings_lpga" ng-cloak>
	  
