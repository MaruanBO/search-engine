      // Utility methods
      var parseQueryString = function( queryString ) {
        var params = {}, queries, temp, i, l;
        if (queryString.charAt(0) == '?') { queryString = queryString.substring(1); }
        queries = queryString.split("&");
        for ( i = 0, l = queries.length; i < l; i++ ) {
          temp = queries[i].split('=');
          params[temp[0]] = unescape(temp[1]);
        }
        // return object of unescaped parameters
        return params;
      };
      var cmSetDD = function() {
        if (typeof _CM_PROD_PATTERN == "undefined") { _CM_PROD_PATTERN = "^www\\."; } // define as a string (note the escaping of the escape char)
        if (typeof _CM_STG_PATTERN == "undefined") { _CM_STG_PATTERN = /-stg\./; } // or a regexp directly
