<script>
var C1; //width of ad
var adCount = 0;
var temp;
var scrollSpeed = 3000; //milliseconds

window.verticalScroller = function(elem) {
    var left = parseInt(elem.css("left"));
    temp = -1 * (164 + (adCount - 8) * C1); //spacing is dependent upon number of ads
    if(left < temp) {
        left = $('#bannerContainer').width();
        elem.css("left", left);
    }
    elem.animate({ left: (parseInt(left)-C1) }, scrollSpeed, 'linear', function () {
      window.verticalScroller($(this));
    });
};

$(document).ready(function() {
    var i = 0;
    $("#scroller > div").each(function () {
        adCount++;
        C1 = $(this).width() + 40;
    });
    $("#scroller > div").each(function () {
          $(this).css("left", i);
          i += C1;
          window.verticalScroller($(this));
    });
});
</script> 


</body>
</html>";i:2;s:4:"8250";}