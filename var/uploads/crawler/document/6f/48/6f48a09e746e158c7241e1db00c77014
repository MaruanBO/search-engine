        jQuery(document).ready(function () {
            jQuery('a').each(function () {
                var url = jQuery(this).attr('href');
                if (url.toLowerCase().indexOf('advocacytoaction.com/campaign.aspx?') > 0) {
                    if (url.toLowerCase().indexOf('&pr=null') > 0) {
                        jQuery(this).attr('href', url.replace('&pr=null', '&pr=' + encodeURIComponent(document.referrer)));
                    } else {
                        jQuery(this).attr('href', url + '&pr=' + encodeURIComponent(document.referrer));
                    }
                }
            });
        });
    </script>
    
