    <script type="text/javascript">

        var ortcNumberOfMessages = 0;
        var messageOrtcNumberOfMessages = false;

        var ortcNumberOfConnections = 0;
        var messageOrtcNumberOfConnections = false;

        var ortcNumberOfConnectionsPerPeriod = 0;
        var messageOrtcNumberOfConnectionsPerPeriod = false;

        var ortcNumberOfVisitores = 0;
        var messageOrtcNumberOfVisitores = false;
                

        /* REALTIME */

        xRTML.load(function () {

            xRTML.Config.debug = false;
            var conn = xRTML.ConnectionManager.create({
                id: 'ACME_connection',
                appKey: 'XRTMLO',
                authToken: 'mntb3c1c0exagxk3wqx44454',
                url: 'http://ortc-developers.realtime.co/server/2.1/'
            });

            var channelOrtcNumberOfConnections = conn.createChannel({ name: 'ortcNumberOfConnections' });
            channelOrtcNumberOfConnections.bind({
                message: function (e) {
                    if (!messageOrtcNumberOfConnections) {
                        $.get('http://app.realtime.co/ajax/setStatistics.ashx', { prop: 'ortcNumberOfConnections', val: e.message }, function (data) {

                        });
                        messageOrtcNumberOfConnections = true;
                    }

                    if (ortcNumberOfConnections == 0) 
                    {
                        ortcNumberOfConnections = (parseInt(e.message) - parseInt(parseInt(e.message) * 0.1));
                    }

                    $("#connec").animateNumber(ortcNumberOfConnections, parseInt(e.message), 5000, addCommas);
                    $("#connecSliderPM").animateNumber(ortcNumberOfConnections, parseInt(e.message), 5000, addCommas);
                    ortcNumberOfConnections = parseInt(e.message);                    
                }
            });

            var channelOrtcNumberOfMessages = conn.createChannel({ name: 'ortcNumberOfMessages' });
            channelOrtcNumberOfMessages.bind({
                message: function (e) {
                    if (!messageOrtcNumberOfMessages) {
                        $.get('http://app.realtime.co/ajax/setStatistics.ashx', { prop: 'ortcNumberOfMessages', val: e.message }, function (data) {

                        });
                        messageOrtcNumberOfMessages = true;
                    }

                    if (ortcNumberOfMessages == 0) {
                        ortcNumberOfMessages = (parseInt(e.message) - parseInt(parseInt(e.message) * 0.1));
                    }

                    $("#msgsModule").animateNumber(ortcNumberOfMessages, parseInt(e.message), 5000, addCommas);
                    $("#msgSliderPM").animateNumber(ortcNumberOfMessages, parseInt(e.message), 5000, addCommas);
                    ortcNumberOfMessages = parseInt(e.message);

                }
            });

            var channelOrtcNumberOfConnectionsPerPeriod = conn.createChannel({ name: 'ortcNumberOfConnectionsPerPeriod' });
            channelOrtcNumberOfConnectionsPerPeriod.bind({
                message: function (e) {
                    if (!messageOrtcNumberOfConnectionsPerPeriod) {
                        $.get('http://app.realtime.co/ajax/setStatistics.ashx', { prop: 'ortcNumberOfConnectionsPerPeriod', val: e.message }, function (data) {

                        });
                        messageOrtcNumberOfConnectionsPerPeriod = true;
                    }

                    if (ortcNumberOfConnectionsPerPeriod == 0) {
                        ortcNumberOfConnectionsPerPeriod = (parseInt(e.message) - parseInt(parseInt(e.message) * 0.1));
                    }

                    $("#connecPerPeriod").animateNumber(ortcNumberOfConnectionsPerPeriod, parseInt(e.message), 5000, addCommas);
                    $("#userConnecSliderPM").animateNumber(ortcNumberOfConnectionsPerPeriod, parseInt(e.message), 5000, addCommas);
                    ortcNumberOfConnectionsPerPeriod = parseInt(e.message);

                }
            });

         

            $(function () {
                $("#connecSliderPM").html('642,978');
                $("#msgSliderPM").html('686,223');
                $("#userConnecSliderPM").html('207,883,110');
            });


        });
    </script> 
    <!-- Broadcasting (END) -->

