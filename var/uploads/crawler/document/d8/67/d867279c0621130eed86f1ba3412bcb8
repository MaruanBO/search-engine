<script type="text/javascript">

    $(document).ready(function () {
        var name = getCookieValue("username");
        if (name != "") {
            $('#log').css('display', 'block');
            $('#auth').css('display', 'none');
            $('#youraccount').css('display', 'none');
            $('#username').html(name);
        }
        else {
            $('#log').css('display', 'none');
            $('#auth').css('display', 'block');
            $('#youraccount').css('display', 'none');
        }

        if (window.location.href.indexOf("/pt") == -1) {
            $('#linkEN').addClass("active");
            $('#linkPT').removeClass("active");
            if (window.location.host.indexOf("app.") == -1) {
                $('#linkPT').attr("href", "http://www.realtime.co/pt" + window.location.pathname);
            }
            else {
                $('#linkPT').attr("href", "http://app.realtime.co/pt" + window.location.pathname);
            }
            $('.info').removeClass("pt");
        }
        else {
            $('#linkEN').removeClass("active");
            $('#linkPT').addClass("active");
            if (window.location.host.indexOf("app.") == -1) {
                $('#linkEN').attr("href", "http://www.realtime.co" + window.location.pathname.replace("/pt", ""));
            }
            else {
                $('#linkEN').attr("href", "http://app.realtime.co" + window.location.pathname.replace("/pt", ""));
            }
            $('.info').addClass("pt");
        }
    });

    function getCookieValue(key) {
        currentcookie = document.cookie;
        if (currentcookie.length > 0) {
            firstidx = currentcookie.indexOf(key + "=");
            if (firstidx != -1) {
                firstidx = firstidx + key.length + 1;
                lastidx = currentcookie.indexOf(";", firstidx);
                if (lastidx == -1) {
                    lastidx = currentcookie.length;
                }
                return unescape(currentcookie.substring(firstidx, lastidx)).replace('semicol', ';');
            }
        }
        return "";
    }
        
    </script>

