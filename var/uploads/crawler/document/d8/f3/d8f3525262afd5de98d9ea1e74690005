<script>
         jQuery(document).ready(function(){
                 var numItems = jQuery('.slideshow_view').length;
                 checkCookie();
                 jQuery(".slideshow_next").click(function(event)
                 {
                     event.preventDefault();
                     nextItem();
                 });
                 jQuery(".slideshow_previous").click(function(event)
                 {
                     event.preventDefault();
                     prevItem();
                 });
                 //jQuery('.slideshow_view:nth-child(3)').addClass('slideshow_currentView');
              });
             
              function setCookie(cname, cvalue, exdays) {
                var d = new Date();
                d.setTime(d.getTime() + (exdays*24*60*60*1000));
                var expires = "expires="+d.toUTCString();
                document.cookie = cname + "=" + cvalue + "; " + expires;
               }   

               function getCookie(cname) {
                    var name = cname + "=";
                    var ca = document.cookie.split(';');
                    for(var i=0; i<ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') c = c.substring(1);
                        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
                      }
                      return "";
                  }
               function nextItem()
               {
                   var currentItem = getCookie("currentItem");
                   if (currentItem=='')
                   {
                       setCookie("currentItem",1,1);
                   }
                   else
                   {
                       var nextItem = parseInt(currentItem) + 1;
                       var numItems = jQuery('.slideshow_view').length;                 
                       if (numItems < nextItem)
                       {
                           nextItem = 1;
                       }

                       setCookie("currentItem",nextItem,1);
                        
                   }
                   location.reload();
               }
               function prevItem()
               {
                   var currentItem = getCookie("currentItem");
                   if (currentItem=='')
                   {
                       setCookie("currentItem",1,1);
                   }
                   else
                   {
                       var nextItem = parseInt(currentItem) - 1;
                       var numItems = jQuery('.slideshow_view').length;                 
                       if (nextItem < 1)
                       {
                           nextItem = numItems;
                       }
                       
                       setCookie("currentItem",nextItem,1);
                       
                   }
                   location.reload();
               }
               function checkCookie()
               {
                   var currentItem = getCookie("currentItem");
                   if (currentItem=='')
                   {
                       setCookie("currentItem",1,1);
                   }
                   else
                   {
                       currentItem = parseInt(currentItem);
                       jQuery('.slideshow_view:nth-child(' + currentItem + ')').addClass('slideshow_currentView');
                       jQuery('.slideshow_view:nth-child(' + currentItem + ')').attr('style',' width: 602px; height: 400px;');
                       
           
                       if (currentItem != 1)
                       {
                          jQuery('.slideshow_view:nth-child(1)').removeClass('slideshow_currentView');
                             jQuery('.slideshow_view:nth-child(1)').attr('style','top: 400px; width: 602px; height: 400px;');
                       }
                       
                   }
               }
        </script>
