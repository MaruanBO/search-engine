<script type="text/javascript">
    // It will randomly choose from these elements. First is the ID of the box itself and 2nd
    // is the class to add to #subscribe_box. Third is the ID to use for Google Analytics.
    var subscriber_elements = [
      ['id-subscriber1', 'subscribeBox1', 1],
      /*['id-subscriber3', 'subscribeBox2', 3],*/
    ];

    // The cookies need to be set at the town domain
    var domain = document.domain.split('.').reverse();
    if (domain.length > 1) {
      domain = '.' + domain[1] + '.' + domain[0];
    } else {
      domain = domain[0];
    }

    function randomIntFromInterval(min, max) {
      return Math.floor(Math.random() * (max-min + 1)) + min;
    }

    // Initialize the cookie to keep them viewing the correct subscriber box.
    if (!$.cookie('sub1')) {
      $.cookie(
        'sub1',
        randomIntFromInterval(0, subscriber_elements.length - 1),
        {domain: domain, path: '/', expires: 7}
      );
    }

    var subNbr = $.cookie('sub1');

    if ($.cookie('sub1hide')) {
      // Sub hidden.. Track this somehow?
    } else {
      $('#' + subscriber_elements[subNbr][0]).show();
      $('#subscribe_box').removeClass();
      $('#subscribe_box').addClass(subscriber_elements[subNbr][1]);
    }

    $(document).ready(function () {
      $("#subscribe_box").effect('slide', {direction: "down", mode: "show"}, 1500);
    });

    $('.loginForm').click(function() {
      _gaq.push(['_trackEvent', 'Subscribe', 'SubVersion' + subscriber_elements[subNbr][2], 'User clicked on subscribe version ' + subscriber_elements[subNbr][2]]);
    });

    $('.closeX').click(function (e) {
      $.ajax({
        url: "/counting/subscribe_X_close/national/counthitall/",
        type: "GET"
      });

      $("#subscribe_box").effect('slide', {direction: "down", mode: "hide"}, 1500);
      // Set a cookie that expires in 7 days to hide the box
      $.cookie('sub1hide', true, {domain: domain, path: '/', expires: 7});
      ////////////////////////////////////////////
      // NOTE: now you only can close with the X
      ////////////////////////////////////////////
      
      e.preventDefault();
    });

  </script>
