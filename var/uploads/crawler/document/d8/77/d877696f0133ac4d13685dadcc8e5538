   };

$(function() {

   function update_town() {
       var $town = $("#town_select");
       
       var $state = $("#state_select");
       var $state_val = $state.val();
       

       var options = "<option value=''>Town</option>";
       if ($state_val in towns) {
           $(towns[$state_val]).each(function() {
               options += "<option value='" + this.url + "'>" + this.town + "</option>";
           });
       }
       $town.html(options);
       $town.attr("disabled", $town[0].options.length == 1);

   }


   $("#state_select").change(update_town);

   update_town();
   $("#town_select").change(function() {
       window.location = $(this).val();
   });

});
</script>
