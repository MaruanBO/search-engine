    $(document).ready(function() {
      //story slider
      $('.stories-slider .stories').cycle({
        fx: 'fade',
        timeout: 6000,
        speed: 500,
        pager:  '#content-holder #column-1 .stories-slider ul.story-slider-nav',
        next:   '#slider_next',
        prev:   '#slider_prev',

        // callback fn that creates a thumbnail to use as pager anchor
        pagerAnchorBuilder: function(idx, slide) {
          return '<li><a href="#"></a></li>';
        }
      });

      // nationals slider
      $('.nationals').cycle({
        fx: 'fade',
        timeout: 2000,
        speed: 500
      });

      // static story slide up
      $('.story-static-details').hover(function() {
        $(this).stop().animate({
          "top": 0,
          "padding-top":"16px"
        }, 300),
        $(this).parent().children('.button-more').stop().animate({
          "bottom": '-62px'
        }, 300)
      }, function() {
        $(this).stop().animate({
          "top": '194px',
          "padding-top":"14px"
        }, 300),
        $(this).parent().children('.button-more').stop().animate({
          "bottom": '0px'
        }, 300)
      });

      // more button slide up
      $('.button-more').hover(function() {
        $(this).stop().animate({
          "bottom": '-62px'
        }, 300),
        $(this).parent().children('.story-static-details').stop().animate({
          "top": 0,
          "padding-top":"16px"
        }, 300)
      }, function() {
        $(this).stop().animate({
          "bottom": '0px'
        }, 300),
        $(this).parent().children('.story-static-details').stop().animate({
          "top": '194px',
          "padding-top":"14px"
        }, 300)
      });
    });
  </script>
 
