  jQuery(document).ready(function() {
    try {
      var refreshPollNumber = null;
      var intervalLength    = 150000;

      setInterval(function() {
        var jqxhr = $.get( "/page_reloader?v="+refreshPollNumber, function( resp ) {
          if (refreshPollNumber == null) {
            refreshPollNumber = resp;
            return;
          } else if (resp > refreshPollNumber) {
            window.location.reload();
          }
        })
        .done(function() { })
        .fail(function() { })
        .always(function() { });
      }, intervalLength );
    } catch (err ) {
    }
  });
</script>


