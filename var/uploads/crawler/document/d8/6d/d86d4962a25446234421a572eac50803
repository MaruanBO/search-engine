//<![CDATA[

  function resizeDarkBgThing() {
    var channel = $("#channel");
    var channel_offset = channel.offset();
    var dark_bg_thing_height = channel_offset.top + channel.height();
    $("#dark-bg-thing").height(dark_bg_thing_height);
  }
  
  resizeDarkBgThing();
  
  $(document).ready(function() {
    setInterval(function(){
      resizeDarkBgThing();
    }, 1000);
  });

