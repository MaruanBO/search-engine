	// popular ajax
	$('div.popular.tab_inactive').click(function() {
		$(this).removeClass('tab_inactive').addClass('tab_active');
		$('div.latest').removeClass('tab_active').addClass('tab_inactive')
		$.ajax({ url: pop_url, success: function(html) {
            $("div#popular_ajax").empty().append(html);
            size_li = $("#popular_ajax li").size();
    x=5;
    $('#popular_ajax li:lt('+x+')').fadeIn();
    $('#loadMore').click(function () {
        x= (x+5 <= size_li) ? x+5 : size_li;
        $('#popular_ajax li:lt('+x+')').fadeIn();
        if (x == size_li) {
        	$('#loadMore').fadeOut();
        }
    });
            }
		});
	});

	$('div.latest').click(function() {
		$("div#popular_ajax").children().fadeOut();
		
		$(this).removeClass('tab_inactive').addClass('tab_active');
		$('div.popular').removeClass('tab_active').addClass('tab_inactive');
	});

	// section top posts refresh
        size_li = $("#section_pop li").size();
    	x=3;
    	$('#section_pop li:lt('+x+')').fadeIn().addClass("popshowing");

    	$('#refresh').click(function () {
    	
        x= (x+3 <= size_li) ? x+3 : size_li;
        newselected = $('#section_pop li:lt('+x+')').not(".popshowing");
        $('li.popshowing').fadeOut('fast');
        $(newselected).fadeIn().addClass('popshowing');
        if (x == size_li) {
        	$('#refresh').fadeOut();
        }
    });
    

	// end popular stuff
	
		var imgurl = 'http://intothegloss.com/overlay_black.png';
	$(".newform").fancybox({
		maxWidth	: 673,
		maxHeight : 550,
		wrapCSS : 'formnew',
		height	: 484,
		type: 'iframe',
		beforeShow : function() {
		        $('.fancybox-overlay').css('background-image', 'url(' + imgurl + ')');
			
		    }
	
	});

	$(".enlarge").fancybox({
		
	});
	
});
</script>


