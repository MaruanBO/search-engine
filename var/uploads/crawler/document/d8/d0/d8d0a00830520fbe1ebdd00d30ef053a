              $('#promo_feed').show();
            }
          });
        });
    
       
   	// end gift guide

	// img load
	$("img").unveil(600);

	$('html.no-touch #hp_arrow_right, html.no-touch #hp_arrow_left').fadeIn(200).delay(1500).fadeOut(500);

	// ad stuff

	// newsletter signup submit
