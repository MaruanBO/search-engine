<script>
$(document).ready(function(){

	function effectiveDeviceWidth() {
  		var deviceWidth = window.screen.width;
  		console.log('in function the width is ' + deviceWidth);
  		if (navigator.userAgent.indexOf('Android') >= 0 && window.devicePixelRatio) {
    		deviceWidth = deviceWidth / window.devicePixelRatio;
  		}
  		return deviceWidth;
	}

	var the_width = effectiveDeviceWidth(); 
	// var the_width = $(document).width();
	console.log('this width is ' + the_width);
    if (the_width <= 376) {
        $("body").addClass("mobile");
   	} else {
   		$("body").addClass("desktop").css('width', '100%');
   		$('meta[name=viewport]').attr('content','width=980, maximum-scale=1.0');
   	}
   	var isMobile = $("body").hasClass("mobile");
   	if(isMobile) {
   		$.getScript('http://intothegloss.com/js/site_mobile_dev.js', function() {
		   	$('#m_cover').bind( "tap", function(e){
				$('#search_hidden').blur();
			});


			$('.mobile #search_hidden').blur(function(){
				$('#search_box').animate({right: "+=242px", leaveTransforms:true }, 360);
				$('#logo').fadeIn(1000);
				$('#m_search.exed').toggleClass("exed magnifying");
				$('#m_cover').fadeOut();
			});

			$( "#m_search.magnifying").bind( "tap", function(e){
				$('#logo').fadeOut(300);
				$('#m_cover').stop().delay(500).fadeIn();
				$('#m_search.magnifying').toggleClass("magnifying exed");
				$('#search_box').show().animate({right: "+=20px", leaveTransforms:true }, 360);
				window.scrollTo(0,0);
		  		$('#search_hidden').focus();
			});

			$("#m_share").bind("tap", function(e){
				$(this).toggleClass('exed');
				$('#top_social').slideToggle('fast');
			});

			$("#m_touch").delay(1500).queue(function(n) {
		    	$(this).fadeIn().animate({left: "54px", leaveTransforms:true, useTranslate3d:true }, 500);
		    	n();
			});

			$('#m_navigation').bind( "tap", function(e){
				$('#m_nav').animate({left: "320px", opacity: .99, leaveTransforms:true, useTranslate3d:true }, 300);
				$("body").bind('touchmove', function(e){e.preventDefault()});
			});

			$('#m_nav_close').bind( "tap", function(e){
				$('#m_nav').animate({left: "0px", opacity: .2, leaveTransforms:true, useTranslate3d:true }, 300);
				$("body").unbind('touchmove');
			});
   		});
   				var slideone = $('body.mobile #hp_slide_one').clone();
		$('#mb_header').html(slideone);
				$('.ad_unit.mobile').dfp({
    		dfpID:"2996658",
    		enableSingleRequest: true,
    		    		refreshExisting: false
		});

		// mobile reload function
		
		function mobileReload() {
		    setTimeout(function () {
		        $('.ad_unit.mobile').dfp({
		    		dfpID:"2996658",
		    		enableSingleRequest: true,
		    				    		refreshExisting: false
				});
		        mobileReload();
		    }, 30000);
		}

		mobileReload();


		// move single ad if needed
		   	} else {
   		$('.ad_unit.desktop').dfp({
    		dfpID:"2996658",
    		enableSingleRequest: true,
    				
    		refreshExisting: false
		});
   	}

   	// Gift Guide
   	            var JSONService = "http://intothegloss.com/gifts12.json";
            $.ajax({
                type: "GET",
                contentType: "application/json",
                dataType: "json",
                url: JSONService
            }).done(function (response) {
	            		            $.each(response, function (key, value) {
		            	var gift_day = this.day;
		            	var gift_month = this.month;
		            	var gift_title = this.gift_title;
		            	var gift_dek = this.dek;
		            	var gift_wrapped = this.wrapped;
		            	var gift_unwrapped = this.unwrapped;
		            	var gift_hyperlink = this.hyperlink;
		            	if ((gift_day == current_day) && (gift_month == current_month)) {
		            		$('#gift_feed .entry.two').html('<a href="' + gift_hyperlink +'" class="darken" style="display:inline-block; overflow:hidden;"><img src="' + gift_wrapped + '" class="post_image imgtwo "></a><a style="display:inline-block; overflow:hidden;" href="' + gift_hyperlink +'" class="darken"><img src="' + gift_unwrapped + '" class="post_image imgtwo picmargin"></a><div class="gift_cat">HOLIDAY GIFT GUIDE</div><h2 style="margin-top:8px;"><a href="' + gift_hyperlink +'">' + gift_title +'</a></h2><div class="promo"><p style="text-align:center; margin-bottom:5px; margin-top:2px;">' + gift_dek + '</p><span class="continue"><a href="' + gift_hyperlink +'">SEE THE GUIDE ></a></span></div>	')
		            		$('#gift_feed').show();
		            	} else {
		            	
		            	}
		            });
	            	                        }).fail(function (jqXHR, textStatus) { console.log("error" + textStatus); }).always(function () { console.log("complete"); });
    
            var PJSONService = "http://intothegloss.com/promo.json.php?v=2.1";
        $.ajax({
            type: "GET",
            contentType: "application/json",
          dataType: "json",
          url: PJSONService
        }).complete(function (response) {
          $.each(response, function (key, value) {
            if ($('.promo-header').size() > 0 && response.responseText != '') {
