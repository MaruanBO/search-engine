<script type="text/javascript">
    jQuery(document).ready(function(){
		
		jQuery('#cartHeader').click(function(){
			
			if(jQuery('#topCartContent').hasClass('cart-down')){	
				jQuery('#topCartContent').slideUp(500);
				jQuery('#topCartContent').removeClass('cart-down');
			}else{
				jQuery('#topCartContent').slideDown(500);
				jQuery('#topCartContent').addClass('cart-down');
			}
			
		});
		jQuery('.close-btn').click(function(){
			
			jQuery('#topCartContent').slideUp(500);
			jQuery('#topCartContent').removeClass('cart-down');
			
		});
		
	});
