	jQuery(document).ready(function(){
		jQuery('div.top-menu ul li.level0').hover(
			function(){
				jQuery('ul',this).stop(true,true).delay(450).slideDown();
			},
			function(){
				jQuery('ul',this).stop(true,true).slideUp();
			}
		);
		

		jQuery('.weekly-ad-emailed').click(function() {
			jQuery('#newsletter-validate-detail').show();
			jQuery('#mail-validate-detail').hide();
