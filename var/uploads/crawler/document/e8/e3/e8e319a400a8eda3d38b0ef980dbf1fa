	<script type="text/javascript">
		decorateDataList('narrow-by-list2');
		jQuery(document).ready(function(){
			jQuery('.show-all-categories').click(function(){
				if(jQuery(this).hasClass('open')){
					jQuery('.all-product-content').slideUp(500);
					jQuery(this).html('&bull; Show All Categories &bull;');
					jQuery(this).removeClass('open');
				}else{
					jQuery('.all-product-content').slideDown(500);
					jQuery(this).html('&bull; Hide All Categories &bull;');
					jQuery(this).addClass('open');
				}
			});
			jQuery('.block-category-nav .block-title strong').click(function(){
				if(jQuery('.block-category-nav .block-title strong').not('.active')){
					jQuery('.block-category-nav .block-title strong').removeClass('active');
					jQuery(this).addClass('active');
					jQuery('.block-category-nav .block-content dl dd').hide();
					var tabId = jQuery(this).attr('id');
					jQuery('.'+tabId).show();
				}
			});
			jQuery('.block-category-nav .block-title strong').hover(
				function(){
				
					jQuery(this).addClass('hover',200);
					
				},
				function(){
					
					jQuery(this).removeClass('hover',200);
				
				}
			);
			jQuery('.product-content ol li').hover(
				function(){
					jQuery(this).find('.products-sub-menu').show();
				},
				function(){
					jQuery(this).find('.products-sub-menu').hide();
				});
					
				jQuery('.all-product-content ol li').hover(
				function(){
					jQuery(this).find('.products-sub-menu').show();
				},
				function(){
					jQuery(this).find('.products-sub-menu').hide();
				});
		});
	</script>	
