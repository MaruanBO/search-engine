                    };
                if (r < Math.floor(Math.random() * 100)) {
                    d.cookie = "IPE_S_115556=0;Path=/;"
                } else if (!(/(^|;)\s*IPE(_S_)?115556=/.test(d.cookie))) {
                    if (w.addEventListener) {
                        w.addEventListener(l, go, false);
                    } else if (w.attachEvent) {
                        w.attachEvent("on" + l, go);
                    }
                }
            })(window, document, "script", "load");
        </script>
