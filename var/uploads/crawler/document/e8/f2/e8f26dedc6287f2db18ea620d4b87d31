  $(function(){
    $(".SubHeader h3").html($(".real").html());
    $(".real").hide();
    
    $('.modal').each(function(){
      url = $(this).attr("href");
      title = $(this).attr("title");
      if (!title) title = " ";
      
      $(this).acModal( {
        modalWidth: 1000,
        modalHeight: ($(window).height()*.98),
        contentType: "iframe",
        src : url,
        title : title
      });
    });
  });
