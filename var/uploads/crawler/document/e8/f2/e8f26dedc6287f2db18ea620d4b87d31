    var date = new Date();
    date.setTime((new Date()).getTime() + ( 30*24*60*60*1000 ));
    
    $.cookie("acproduct","americommerce", { 
      expires: date,
      domain : "americommerce.com",
      path : "/"
    });
    
    $("#ContactPopup").click(function(){
      slideUpForm();
      return false;
    }); 
    
    $("#ContactClose").click(function(){
      closePopup();
      return false;
    });
    
    showform = $.cookie("hidecontactform");
    
    if (!mobilecheck()){
      setTimeout("fadeInPopup()", 5000);
    }
    
    function fadeInPopup(){
      $("#ContactForm").fadeIn();
      
      if(!showform)
        slideUpForm();
    }
    
    function closePopup(){
      $("#ContactForm").animate({"bottom":"-440px"}, 500);
      $.cookie("hidecontactform", true, { "expires": date });
    }
    
    function slideUpForm(){
      $("#ContactForm").animate({"bottom":"0"}, 500);
    }
    
    
    
    function ShowTrialBackdrop(){
      $(".trial-backdrop").fadeTo("slow", 0.99);
      trialTimer = new Date();
    }
    function HideTrialBackdrop(url) {
      if (url){
        
        var timeElapsed = new Date();
        if (timeElapsed - trialTimer < 7000) {
          //console.log(timeElapsed - trialTimer);
          window.setTimeout(function() { HideTrialBackdrop(url); }, 7000-(timeElapsed - trialTimer));
          return;
        }
        
        $(".trial-loading-message").html("Congrats, your store is ready!<div>We've emailed your login details, now create your password.</div>");
        //$(".trial-loading-spinner").fadeOut();
        $(".trial-loading-crane").animate({"top":"1000%"}, 500);
        
        $(".trial-loading-button a").attr("href", url);
        //$(".trial-loading-button").fadeIn();
        $("#goToTrialButton").prop('disabled', false);
        $("#goToTrialButton").animate({backgroundColor: '#FF703E'}, 'slow');
        $("#goToTrialButton").removeClass('disabled-trial-button');
        
        $("#hdTrialUrl").val(url);
        
      }else {
        $(".trial-backdrop").fadeOut("slow"); 
        $("#goToTrialButton").addClass('disabled-trial-button');
      }
    }
    
    $(function(){      
      $(".trial-backdrop").appendTo("body");
      $("#goToTrialButton").prop('disabled', true);
      
      $("#trialPassword").on('keyup', function(event) {
        
        var password = $.trim($("#trialPassword").val());
        var passwordConfirm = $.trim($("#trialPasswordConfirm").val());
        
        var $passwordOk = $("#passwordOk");
        var $passwordNotOk = $("#passwordNotOk");
        var $confirmPasswordOk = $("#confirmPasswordOk");
        var $confirmPasswordNotOk = $("#confirmPasswordNotOk");
        
        $passwordOk.hide();
        $passwordNotOk.hide();
        $confirmPasswordOk.hide();
        $confirmPasswordNotOk.hide();        
        
        if (password.length > 0 && password.length < 5) {
          $passwordNotOk.css('display', 'inline');
        }
        else if (password.length >= 5) {
          $passwordOk.css('display', 'inline');
        }
          
          if (passwordConfirm.length > 0 && password != passwordConfirm) {
            $confirmPasswordNotOk.css('display', 'inline');
          }
        else if (passwordConfirm.length > 0) {
          $confirmPasswordOk.css('display', 'inline');
        }
            
            });
      
      $("#trialPasswordConfirm").on('keypress', function(event) {
        if ( event.which == 13 ) {
          event.preventDefault();
          if ($("#goToTrialButton").hasClass('disabled-trial-button'))
            return;
          
          goToTrial();
          
        }
      });
      
      $("#trialPasswordConfirm").on('keyup', function(event) {
        
        var password = $.trim($("#trialPassword").val());
        var passwordConfirm = $.trim($("#trialPasswordConfirm").val());
        
        var $passwordOk = $("#confirmPasswordOk");
        var $passwordNotOk = $("#confirmPasswordNotOk");
        
        $passwordOk.hide();
        $passwordNotOk.hide();
        
        if (passwordConfirm.length > 0 && password != passwordConfirm) {
          $passwordNotOk.css('display', 'inline');
        }
        else if (passwordConfirm.length > 0) {
          $passwordOk.css('display', 'inline');
        }
          
          });
      
      $("#goToTrialButton").on('click', function(event) {
        //verify passwords match, verify length
        //show error if not
        
        event.preventDefault();    
        
        if ($("#goToTrialButton").hasClass('disabled-trial-button')) return;
        
        goToTrial();
        
        
      });
    });
    
    function goToTrial() {
      var $password = $("#trialPassword");
      var $confirmPassword = $("#trialPasswordConfirm");
      var $dvPasswordErrors = $("#dvPasswordErrors");
      var $passwordErrorMessage = $("#passwordErrors");
      var $hdTrialUrl = $("#hdTrialUrl");
      
      $dvPasswordErrors.hide();
      $passwordErrorMessage.html('');
      
      if ($hdTrialUrl.val() == '') {
        $passwordErrorMessage.html('No trial url found');
        $dvPasswordErrors.show();
        return;
      }               
      
      if ($.trim($password.val()) != $.trim($confirmPassword.val())) {
        $passwordErrorMessage.html('Passwords do not match');
        $dvPasswordErrors.show();
        return;
      }
      else if ($.trim($password.val()).length < 5) {
        $passwordErrorMessage.html('Password must be at least 5 characters.');
        $dvPasswordErrors.show();
        return;
      }
        
        //now, passwords are ok, so ajax post to a server method that will call the trial to run the wizard.  not sure what to do about sso them into the trial yet.
        //the trial url will contain the change password url, all we want is the domain name
        var trialUrl = $('<a>').prop('href', $hdTrialUrl.val()).prop('hostname');
      var result, temp;
      
      //disable the gototrial button
      $("#goToTrialButton").prop('disabled', true);
      $("#goToTrialButton").addClass('disabled-trial-button');
      
      // always save the destination
      temp = $.ajax({
        type: "POST",
        url: "/store/customercode/acdotcom/trialajaxhandler.asmx/TrialAssignedHandler",
        data: JSON.stringify({
          password: $.trim($password.val()),
          storeUrl: trialUrl
        }),
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        async: false
      }).responseText;
      
      
      
      if (temp) {
        result = JSON.parse(temp).d;
        
        if (result) {
          
          var bErrors = false;
          
          if (!result.Success) {
            $passwordErrorMessage.val(result.Messsage);
            $dvPasswordErrors.show();
            return;
          }
          else{
            
            $("#goToTrialButton").prop('href', result.Message);
            location.href=result.Message;
            
            
          }
          
        }
      }
    }
    
    window.addEventListener("message", receiveMessage, false);
    
    function receiveMessage(event){
      if (event.data == "ShowTrialBackdrop"){
        ShowTrialBackdrop();
        //console.log("ShowTrialBackdrop()");
      } else if (event.data.indexOf("HideTrialBackdrop") > -1) {
        HideTrialBackdrop(event.data.split('|')[1]);
        //console.log(event.data);
      }
        }
    
