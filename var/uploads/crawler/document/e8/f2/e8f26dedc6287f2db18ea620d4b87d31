  <script>
    $(function(){
      $("#FormMiddle").click(function(){
        $("#formwrap").css("z-index",2);
        console.log("asdfas");
      });
      
      $("#submit").click(function(e){
        if($(this).attr("disabled")=="true"){
          return false;
        }
        
        errors = 0;
        pwerrors = 0;
        strError = "";
        $(".spinner").fadeIn();
        
        if (!$("#firstname").val()){
          errors++;
          strError += "<li>Your first name was left blank.</li>";
        }
        if (!$("#lastname").val()){
          errors++;
          strError += "<li>Your last name was left blank.</li>"
        }
        if (!$("#email").val()){
          errors++;
          strError += "<li>Your email was left blank.</li>";
        }
        
        if (!/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))$/i.test($("#email").val())) {
          errors++;
          strError += "<li>Your must enter a valid email address.</li>";
          
        }
        
        
        if ($("#pw").val().length < 8){
          errors++;
          pwerrors++;
          strError += "<li>Your password must be atleast 8 characters long.</li>";
        }
        if(!/\d/.test($("#pw").val())){
          errors++;
          pwerrors++;
          strError += "<li>Your password must include at least one number.</li>";
        }
        if(!/[a-zA-Z]/.test($("#pw").val())){
          errors++;
          pwerrors++;
          strError += "<li>Your password must include at least one letter.</li>";
        }
        if(pwerrors){
          _gaq.push(['_trackEvent', 'Trial Signups', 'Signup', 'Customer Had Password Errors.']);
        }
        if (errors){
          showMessage("<ul>"+strError+"</ul><a class='closeflash'>Try Again</a>", "flasherror");
        }else{
          $.ajax({
            type : "GET",
            url : "/freetrial.aspx?" + ["timestamp=", $("#timestamp").val(), "&firstname=", $("#firstname").val(), "&lastname=", $("#lastname").val(), "&email=", $("#email").val(), "&pw=", $("#pw").val(),"&ip=128.199.135.235","&phone=", $("#phone").val()].join(''),
            success : function(res){
              result = $.parseJSON(res);
              if (result.cartid != null){
                cartid = result.cartid;
                showMessage("<center><span style=\"font-size: 16px;color: green;\">Congratulations!</span></center><br/> Your trial shopping cart has been generated. Please write down and remember the following information to access your shopping cart. <br/><br/><span style=\"font-size: 13px;color: #000000;\"><b>Your cart id is: <strong>"+cartid+"</strong><br/>Your password is: <strong>"+$("#pw").val()+"</strong></b></span><br/><br><a style=\"font-size: 14px;\" href=\"/login.php?cartid="+cartid+"\">Click Here to login to your new store!</a>", "flashsuccess");
                _gaq.push(['_trackEvent', 'Trial Signups', 'Signup', 'Signup - Home Page Form']);
                $("#submit").attr("disabled","true");
                window.gwsoframe.location.href = "/success.html";
                $(".formbox").fadeOut();
              }else{
                showMessage("There was an issue generating your trial.", "flasherror");
              }
            }
          });
        }
      });
      
      $("#cartcode").on("keypress", function (e) {
        if (e.which == 13) {
          e.preventDefault();
          $("#submit").focus().click();
        }
      });
      
      $(".closeflash").on("click", function(){ $(".flash").slideUp("1000"); } );
      
      function showMessage(msg, cssclass){
        $('.spinner').fadeOut();
        $(".flash").slideUp().delay(500).html(msg).slideDown("1000").removeClass("flasherror flashsuccess").addClass(cssclass);
      }
    });
  </script>

