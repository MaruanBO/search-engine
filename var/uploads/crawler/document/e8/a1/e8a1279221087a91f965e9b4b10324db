		        var imageHeight = 0;

		        function homePageFullSlider() {
		            try {
		                Hammer($slides[0]).on("swipeleft", function (e) {
		                    $slides.data('superslides').animate('next');
		                });

		                Hammer($slides[0]).on("swiperight", function (e) {
		                    $slides.data('superslides').animate('prev');
		                });
		            } catch (ex) { }
		        }

		        if ($slides && $slides[0]) {
		            //lazyLoadImage();
		            $slides.superslides({
		                hashchange: false
						, play: 8000
		            });
		            slideHeight = $slides.height() - 30;
		            $slides.css('height', slideHeight);
		        }

		        if (Modernizr.touch) {
		            $("#citiesRegionsDropdownRegionSubmit").css("display", "block");
		            $(".citiesRegions .regionBlocks li a").click(function (e) {
		                setDropdownImage($(this).closest("li").attr("title").toLowerCase().replace(' ', ''));
		                e.stopPropagation();
		                return false;
		            });
		            $(".citiesRegionsDropdown map area").click(function (e) {
		                setDropdownImage($(this).attr("title").toLowerCase().replace(' ', ''));
		                e.stopPropagation();
		                return false;
		            });
		        } else {
		            $(".citiesRegionsDropdown").mousemove(function () {
		                setDropdownImage();
		            });

		            $(".citiesRegionsDropdown map, .citiesRegions .regionBlocks li").mousemove(function (event) {
		                event.stopPropagation();
		            });
		        }

		        wdot.County.loadCountyAutocompleteForPublicSite($(".citiesRegionsSearch"), ".countyAutoComplete", ".cityAutoComplete");
		        wdot.City.loadCityAutocompleteForPublicSite($(".citiesRegionsSearch"), ".cityAutoComplete");
		        wdot.City.loadCityAutocompleteForPublicSite($(".eventsDropdown"), ".cityFilterAutoComplete");

		        $(".menu-destination-search").submit(function () {
		            
		            var url = "";

		            if (($("input[name='CityCitiesAndRegions']").data("cityurl")) !== "") {
		                url = $("input[name='CityCitiesAndRegions']").data("cityurl");
		            } else if (($("input[name='County']").data("countyurl")) !== "") {
		                url = $("input[name='County']").data("countyurl");
		            }

		            if (url != undefined && url != "") {
		                window.location = url;
		            }

		            return false;
		        });

		        $("#citiesRegionsDropdownRegionSubmit").click(function () {
		            var currentSelectedRegion = $(".regionBlocks li.selected");
		            if (currentSelectedRegion.length > 0) {
		                window.location = currentSelectedRegion.find("a").first().attr("href");
		            } else {
		                window.location = "/cities-regions";
		            }
		        });
