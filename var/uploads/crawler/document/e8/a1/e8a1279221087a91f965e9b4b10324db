	<script>
		$(document).ready(function () {
			//        $('.collage__link--video').magnificPopup({
			//            disableOn: 700,
			//            type: 'iframe',
			//            mainClass: 'mfp-fade',
			//            removalDelay: 160,
			//            preloader: false,
			//            fixedContentPos: false
			//        });

			var analytics = false;

			$('.playlist-feature').waypoint(function (direction) {
				if (!analytics) {
					_gaq.push(['_trackPageview', '/home-playlist']);
					analytics = true;
				}
			});

			var items = $('.playlist-slider__item').length;
			var pageSize = parseInt($('#PlaylistPageSize').text(), "10");
			var currentPage = parseInt($('#PlaylistCurrentPage').text(), "10");

			for (var i = 0; i < pageSize; i++) {
				$('[data-number="' + (i + 1) + '"]').removeClass("hidden");
			}

			wdot.Playlist.GetNextAdsHomepage($('.playlist-slider__item').length, 10);

			if (items > pageSize) {
				$.each($(".events-nav-item"), function (index, value) {
					if ($(this).data("number") > pageSize) {
						$(this).addClass("hidden");
					} else {
						$(this).removeClass("hidden");
					}
				});
				$('.next').removeClass("hidden");
			} else {
				$(".events-nav-item").removeClass("hidden");
			}

			$('.PlaylistNext').click(function () {
				$('.PlaylistPrevious').removeClass("hidden");

				for (var i = (currentPage * pageSize + 1) ; i < ((currentPage * pageSize + 1) + pageSize) ; i++) {
					$('[data-number="' + (i - pageSize) + '"]').addClass("hidden");
					$('[data-number="' + i + '"]').removeClass("hidden");
				}

				currentPage += 1;

				if ((currentPage * pageSize) >= $(".playlist-slider__item").length) {
					$('.PlaylistNext').addClass("hidden");
				}
			});

			$('.PlaylistPrevious').click(function () {
				$('.PlaylistNext').removeClass("hidden");
				currentPage -= 1;

				for (var i = (currentPage * pageSize + 1) ; i < ((currentPage * pageSize + 1) + pageSize) ; i++) {
					$('[data-number="' + (i - pageSize) + '"]').removeClass("hidden");
					$('[data-number="' + i + '"]').addClass("hidden");
				}

				if (currentPage === 1) {
					$('.PlaylistPrevious').addClass("hidden");
				}
			});

			$.breakpoint((function () {
				return {
					condition: function () {
						// Tablet adjustments
						return window.matchMedia('only screen and (min-width:760px)').matches;
					},
					first_enter: function () {
					},
					enter: function () {
						// Initiate carousel
						$("#homepageCallouts").carouFredSel({
							responsive: true,
							auto: false,
							scroll: 4,
							prev: 'a.carousel-prev',
							next: 'a.carousel-next',
							width: 200,
							items: {
								visible: 4
							}
						});
					},
					exit: function () {
						// Destroy carousel
						$("#homepageCallouts").trigger("destroy", true);
						$('.html_carousel div.slide img').removeAttr("style");
					}
				};
			}()));

			$.breakpoint({
				condition: function () {
					// Things to do for Desktop
					return window.matchMedia('only screen and (min-width:1024px)').matches;
				},
				first_enter: function () {
					// Initate Carousel
					$("#homepageCallouts").carouFredSel({
						responsive: true,
						auto: false,
						onCreate: function (data) { removeBorder(data.items.visible); },
						scroll: {
							items: 5,
							onBefore: function (data) { addBorder(data.items.old); },
							onAfter: function (data) { removeBorder(data.items.visible); }
						},
						prev: {
							button: 'a.carousel-prev'
							//onAfter: function(data) {$('#homepageCallouts > li.first-ad').removeClass('first-ad'); data.items.visible.each(function() {$(this).addClass("first-ad");});}
						},
						next: {
							button: 'a.carousel-next'
							//onAfter: function(data) {$('#homepageCallouts > li.first-ad').removeClass('first-ad'); data.items.visible.each(function(i) {$(data.items.visible[i]).addClass("first-ad");});}
						},
						width: 200,
						items: {
							visible: 5
						}
					});
				},
				enter: function () {
					// Show more items
					$("#homepageCallouts").trigger("configuration", {
						items: {
							visible: 5
						},
						scroll: 5
					});
				},
				exit: function () {
					// Show fewer items
					$("#homepageCallouts").trigger("configuration", {
						items: {
							visible: 4
						},
						scroll: 4
					});
				}
			});

			function removeBorder(items) {
				//items.filter(":eq(0)").addClass('no-border');
				//return items;
			};
			function addBorder(items) {
				//items.removeClass('no-border');
				//return items;
			};

			

		});		// End Document Ready
	</script>

