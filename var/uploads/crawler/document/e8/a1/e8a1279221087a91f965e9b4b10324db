                    function blurCleanup(input) {
                        if (input.value == '') {
                            $(input).data("cityname", '');
                            $(input).data("countyname", '');
                            $(input).data("cityid", '');
                            $(input).data("countyid", '');
                            $(input).data("regionid", '');
                            $(input).data("cityurl", '');
                        }
                    }
                </script>
            </form>
