<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script src="js/bootstrap.min.js"></script>
<script src="js/bootstrapValidator.min.js"></script>
<script>
    $(function () {
        $('#compare').bootstrapValidator({
            message: 'A valid service address and zip code are required.',
            feedbackIcons: {
                valid: 'glyphicon glyphicon-ok',
                invalid: 'glyphicon glyphicon-remove',
                validating: 'glyphicon glyphicon-refresh'
            },
            live: 'enabled',
            trigger: 'focus blur',
            fields: {
                address: {
                    message: 'A valid street address is required.',
                    validators: {
                        notEmpty: {
                            message: 'A valid street address is required.'
                        },
                        stringLength: {
                            min: 3,
                            message: 'A valid street address is required.'
                        }
                    }
                },
                zip: {
                    message: 'A valid 5 digit zip is required.',
                    validators: {
                        notEmpty: {
                            message: 'A 5 digit zip code is required.'
                        },
                        zipCode: {
                            message: 'Not a valid zip code'
                        },
                        regexp: {
                            regexp: /^\d{5}$/,
                            message: 'Not a valid US 5 digit zip code'
                        }
                    }
                }
            }
        });
    });
    function getLocation(){
        if (navigator.geolocation){
            $("div.geomsg").html("<i class='fa fa-circle-o-notch fa-spin'></i> Getting Location...");
            navigator.geolocation.getCurrentPosition(showPosition, errPosition);
        } else {
            msg.innerHTML="Geolocation is not supported by this device.";
            $("div.geomsg").text("Geolocation is not supported by this device.");
            $("div.geomsg").addClass('alert-danger');
        }
    }
    function showPosition(position){
        if (position.coords.accuracy < 500){
            var lat = position.coords.latitude;
            var long = position.coords.longitude;
            var radius = Math.round(position.coords.accuracy*3.28);//meters to feet
            $("div.geomsg").text("Location accuracy is +-" + radius + " feet. Please confirm address and submit.");
            $("div.geomsg").removeClass('alert-danger');
            $("div.geomsg").removeClass('alert-warning');
            $("div.geomsg").addClass('alert-success');
            getAddress(lat,long,radius);
        } else {
            $("div.geomsg").text("Location accuracy from device is too low. Try again or manually enter an address.");
            $("div.geomsg").removeClass('alert-danger');
            $("div.geomsg").removeClass('alert-success');
            $("div.geomsg").addClass('alert alert-warning');
        }
    }
    function errPosition(msg) {
        $("div.geomsg").text("Geolocation failed. Try manually entering an address.");
        $("div.geomsg").addClass('alert alert-danger');
    }

    getAddress = function(lat,long,acc) {
        $.ajax({
            type: 'GET',
            url: '/json_address.php',
            data: { lat: lat, long: long, accuracy: acc },
            cache: false,
            success: function(data, textStatus, jqXHR) {
                console.log(data);
                var geodata = $.parseJSON(data);
                $("input[name='address']").val(geodata.streetaddress).blur();
                $("input[name='zip']").val(geodata.zipaddress).blur();
                $("button[type='submit']").focus;

            },
            error: function(jqXHR, textStatus, errorThrown) {
                console.log(jqXHR.responseText);
            }
        });
    };

</script>
