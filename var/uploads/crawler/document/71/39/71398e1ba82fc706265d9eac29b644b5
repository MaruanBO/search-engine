      if($(this).siblings('.regions').hasClass('active')) {
        $(this).siblings('.regions').removeClass('active').slideToggle();
      } else {
        $('.regions').each(function() {
          $(this).slideUp('fast').removeClass('active');
        });
        $(this).siblings('.regions').slideToggle('fast').addClass('active');
      }
      
      
      e.preventDefault();
    });
    $('.regions').each(function() {
      $(this).children('li:first').addClass('firstItem');
      $(this).children('li:last').addClass('lastItem');
    });
