<a href="/category/unique+gifts.do">
<div id="vDayBanner">
	<noscript><img src="/images/set_a/en_us/local/page_specific/content/globalBnr_2015_14.png" border="0" /></noscript>
</div>
</a>

<script type="text/javascript">

	var BANNER_ID = "vDayBanner";
	var EVENT_DATE = "Feb, 14, 2015";
	var IMAGE_EXT = ".png";
	var IMAGE_PREFIX = "globalBnr_2015_";
	var IMAGE_HEIGHT = 58;
	var IMAGE_WIDTH = 960;
	var IMAGE_PATH = "/images/set_a/en_us/local/page_specific/content/";
	var MAX_DAYS = 14;
	var MIN_DAYS = 1;
	
	// Calculate full days until...
	
	var DateDiff = {
		inDays: function(d1, d2) {
			var t2 = d2.getTime();
			var t1 = d1.getTime();
			var diff = parseInt((t2-t1)/(24*3600*1000));
			diff++; 	// remove zero-based count

			if ( diff < MIN_DAYS )  diff = MIN_DAYS;
			if ( diff > MAX_DAYS )  diff = MAX_DAYS;

			return diff;
		}
	}

	var d1 = new Date();
	var d2 = new Date(EVENT_DATE);
	var daysUntil = DateDiff.inDays(d1, d2);
	
	var banner = document.getElementById(BANNER_ID);
	var bannerImg = document.createElement("img");

	bannerImg.src = IMAGE_PATH + IMAGE_PREFIX + daysUntil + IMAGE_EXT;
	bannerImg.height = IMAGE_HEIGHT;
	bannerImg.width = IMAGE_WIDTH;
    bannerImg.border = 0;
	
	banner.appendChild(bannerImg);

</script>

</div> -->


