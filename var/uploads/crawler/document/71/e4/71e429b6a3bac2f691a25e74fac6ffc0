			jQuery(".tnavWrapper", this).each(function(){
				
				jQuery(this).css("top", jQuery(this).parent().position().top+jQuery(this).parent().outerHeight());
				var sp = (jQuery(this).find(".subCats .subCats").length > 0?sw:0); // does this nav have *any sub sub cats, if so, we need to pad by the sub-nav width (sw);
				var nl = l; // nav left
				var pl = jQuery(this).parent().position().left + jQuery(this).parent().width()/2; // parent left
				var ow = (jQuery(this).data("original-width")!=null?jQuery(this).data("original-width"):jQuery(this).width());
				var w = ow + sp; // nav width
				w = (w>r-l?r-l:w); // if the padding makes the width greater than the max of 870
				if ( jQuery(this).data("original-width") == null ){
					jQuery(this).data("original-width",jQuery(this).width());
					/*jQuery(this).width(w);*/
					//jQuery(this).css("min-width", w); // resize to leave a space for the sub-sub cats on rollover
				}
				var el = (w+ep)/2; // half width of element for centering
				if ( pl-el >= l && pl+el <= r){ // menu isn't full-width and can center properly under parent
					nl = pl-el;
				} else if ( pl-el < l ) { // menu is too far left, move right until centered again
					nl = l;
				} else if ( pl-el+w > r ) { // menu is too far right, move left until aligned right
					nl = r-w-2*ep;
				} else {
					console.log("um..." + nl);
				}
				jQuery(this).css("left",nl);
				jQuery(this).customFadeIn("fast");
			});
		}, function(){
			jQuery(this).find(".tnavSubSubCats").html("");
			jQuery(".tnavWrapper", this).customFadeOut("fast");
			jQuery(".tnavArrow", this).customFadeOut("fast");
			jQuery(this).removeClass("over");
		});
	});
	
	// bottom nav
	jQuery(function() {
		jQuery(".bottom .tnavCategory").on("mousedown", function() {
				jQuery(this).addClass("over");
				
				var np = 25; // nav padding
				var ep = 20; // element padding + border
				var l = jQuery(".tnavCategories.bottom").position().left + np; // leftmost point nav can exist
				var r = l + jQuery(".tnavCategories.bottom").width() - (2*np); // rightmost point nav can exist
				/*jQuery(".arrow-wrapper", this).css("left",jQuery(this).position().left+(jQuery(this).outerWidth()/2)-jQuery(".arrow-wrapper", this).width()/2);
				jQuery(".arrow-wrapper", this).css("top",jQuery(this).position().top+jQuery(this).outerHeight()-jQuery(".arrow-wrapper", this).height());	*/
				jQuery(".arrow-wrapper", this).customFadeIn("fast");
				
				jQuery(".tnavWrapper", this).each(function(){
					
					jQuery(this).css("top", jQuery(this).parent().position().top-jQuery(this).outerHeight()-15);
					var sp = 0; // does this nav have *any sub sub cats, if so, we need to pad by the sub-nav width (sw);
					var nl = l; // nav left
					var pl = jQuery(this).parent().position().left + (jQuery(this).parent().width()+20)/2; // parent left
					var ow = jQuery(this).width();//(jQuery(this).data("original-width")!=null?jQuery(this).data("original-width"):jQuery(this).width());
					var w = ow + sp; // nav width
					w = (w>r-l?r-l:w); // if the padding makes the width greater than the max of 870
					var el = (w+ep)/2; // half width of element for centering
					if ( pl-el >= l && pl+el <= r){ // menu isn't full-width and can center properly under parent
						nl = pl-el;
					} else if ( pl-el < l ) { // menu is too far left, move right until centered again
						nl = l;
					} else if ( pl-el+w > r ) { // menu is too far right, move left until aligned right
						nl = r-w-2*ep;
					} else {
						console.log("um..." + nl);
					}
					jQuery(this).css("left",nl);
					jQuery(this).customFadeIn("fast");
				});
			});
		jQuery(".bottom .tnavCategory").on("mouseleave", function(){
			jQuery(".tnavWrapper", this).customFadeOut("fast");
			jQuery(".arrow-wrapper", this).customFadeOut("fast");
			jQuery(this).removeClass("over");
		});
	});
