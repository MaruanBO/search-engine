			var h2s = h2s || {};
			h2s.ffos = {
				handleLinkClicks: function( e ) {
					// get the target, still FF only, so no cross browser needed.
					var target = e.target;
					// check if this click is an a tag
					if ( 'A' === target.nodeName ){
						var href = target.href;
						var test = /(?::\/\/hip2save.com|^\/|^#)/;

						// Test if this is an external link and if so, open a new window.
						if ( ! test.test( href ) ) {
							e.preventDefault();
							window.open( href );
						}
					}
				},
			}
			// only bind in Firefox OS when the location bar isn't visible (inside the app).
			if ( navigator.mozApps && window.locationbar.visible === false ) {
				// only for firefox, so we don't care about IE cross browser.
				document.addEventListener( 'click', h2s.ffos.handleLinkClicks );
			}
		</script>
