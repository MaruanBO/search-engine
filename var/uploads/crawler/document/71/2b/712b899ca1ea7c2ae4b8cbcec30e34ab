					$(document).ready(function(){
						$(".tab_ad ul li:eq(1)").hide();
						$(".tab_ad ul li:eq(2)").hide();
						$(".tab_ad ul span:eq(1)").hide();
						$(".tab_ad ul span:eq(2)").hide();
