					   //alert(i);
						$($(".tab_ad ul li")[i]).show().siblings("li").hide();
						$($(".tab_ad ul span")[i]).show().siblings("span").hide();
						$($(".tab_ad dl dt")[i]).addClass('hover').siblings("dt").removeClass();
