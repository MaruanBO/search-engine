<script type="text/javascript">
    jQuery(document).ready(function() {
        jQuery('#cart_top .addthis_32x32_style a').click(function() {
            _kmq.push(['record', 'Social buttons', {'link': jQuery(this).attr('href')}]);// _kmq.push(['trackClickOnOutboundLink', 'link1', 'Visited Other Site']);
            return true;
        });

        jQuery('#newsletter-checkbox').click(function() {
            _kmq.push(['record', 'Email unsubscribe']);
            return true;
        });

        _kmq.push(['trackSubmit', 'KM-register-account', 'Register account']);
        _kmq.push(['trackClick', '.cartRemoveItemDisplay a', 'Remove product from cart', {'product': jQuery(this).parent().find('#cartProdTitle').html()}]);
        _kmq.push(['trackSubmit', 'KM-get-offer', 'Get Offer', { 'Merchant_name': jQuery('#merchant_name').val(), 'value':jQuery('#card_amount').val(), 'quantity':jQuery('#card-quantity').val()}]);
        _kmq.push(['trackSubmit', 'KM-add-cart', 'Add product to cart', { 'product': jQuery('#product_'+jQuery('#KM-add-cart input[name="products_id"]').val() + ' .itemTitle').html(), 'quantity':jQuery('#KM-add-cart .cart_quantity').val()}]);
    });
</script>


