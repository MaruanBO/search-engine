<script>
jQuery(document).ready(function($){
	$('form#mgsubscribe').submit(function() {
		var form = this;
		$.ajax({
			type: "GET",
			dataType: "jsonp",
			contentType: "application/json; charset=utf-8",
			url: "http://faithhub.net/mgapi/mgapi_listSubscribe.php",
			data: $(form).serialize(),
			success: function(data) {
				$('#email-capture-thanks').show();
				$('.free-ebook').show();
				$('.email-capture').hide();
				$(form)[0].reset();
			}
		});
		return false;
	});
});
