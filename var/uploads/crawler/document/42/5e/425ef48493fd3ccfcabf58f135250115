<script type='text/javascript'>

    function createXMLHttp()
    {
        // If XMLHttpRequest is available then using it
        if (typeof XMLHttpRequest !== undefined)
        {
            return new XMLHttpRequest;
        }
        // if window.ActiveXObject is available than the user is using IE...so we have to create the newest version XMLHttp object
        else if (window.ActiveXObject)
        {
            var ieXMLHttpVersions = ['MSXML2.XMLHttp.5.0', 'MSXML2.XMLHttp.4.0', 'MSXML2.XMLHttp.3.0', 'MSXML2.XMLHttp', 'Microsoft.XMLHttp'], xmlHttp;
            // In this array we are starting from the first element (newest version) and trying to create it. If there is an
            // exception thrown we are handling it (and doing nothing ^^)
            for (var i = 0; i < ieXMLHttpVersions.length; i++)
            {
                try
                {
                    xmlHttp = new ActiveXObject(ieXMLHttpVersions[i]);
                    return xmlHttp;
                }
                catch (e)
                {
                }
            }
        }
    }

    var xmlhttp = createXMLHttp();

    if (xmlhttp && is_lt_ie10 != true)
    {
        xmlhttp.open("GET", "https://landing.molio.com/land.php?client=poopourri&url=" + encodeURIComponent(document.URL) + "&referrer=" + encodeURIComponent(document.referrer), true);

        xmlhttp.send();
    }
    else
    {
        if (window.XDomainRequest)
        {
            // Use Microsoft XDR
            var xdr = new XDomainRequest();

            xdr.open("GET", "https://landing.molio.com/land.php?client=poopourri&url=" + encodeURIComponent(document.URL) + "&referrer=" + encodeURIComponent(document.referrer));

            xdr.send();
        }
    }

</script>

