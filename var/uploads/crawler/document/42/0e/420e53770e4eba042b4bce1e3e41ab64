  (function($) {
    if (!$) return;
    $('header').one('shown.bs.collapse', loadSearchToCommunity);
    $('#user-search').one('mouseover', loadSearchToCommunity);
    function loadSearchToCommunity() {
      $.getScript('https://static.senioradvisor.com/assets/search-to-community-685c075ca73bd5393b17354a52980a8e.js');
    }
  })(window.$);
</script>


