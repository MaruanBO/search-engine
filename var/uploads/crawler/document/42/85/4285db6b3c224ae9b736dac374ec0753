            var _renderingFeaturedRotator = false;
            var etsApiOptionsFeaturedRotator = {
                top: 10
            };

        var _apiFeaturedRotator = new BellMedia.EtsApi(etsApiOptionsFeaturedRotator);
        var _widgetFeaturedRotator = BellMedia.videoWidgets.videoHub(
         {
             containerDiv: "widget-7a717224-5659-4f2b-948d-4a823940aa60",
             templateDiv: "widget-item-featuredRotator",
             widgetName: "vhWidget7a71722456594f2b948d4a823940aa60",
             qsParamName: "vid",
             videoRotatorId: "data-d33ad74e-c807-4f3d-9966-521f98ba557f",
             onSuccess: function (data) {
                 _renderingFeaturedRotator = false;
             },
             api: _apiFeaturedRotator
         });
        
        function renderFeaturedRotator() {

            if (_renderingFeaturedRotator == false) {
                _renderingFeaturedRotator = true;
                _widgetFeaturedRotator.renderByDataMethod(
                             _apiFeaturedRotator.Collections().collection(757).getData
                    );
         }
     }

        renderFeaturedRotator();

        axisAuthApi.events.addEvent('on_authentication_state_change', function (authenticationStatus) {
            renderFeaturedRotator();
        });

    });
    </script>

