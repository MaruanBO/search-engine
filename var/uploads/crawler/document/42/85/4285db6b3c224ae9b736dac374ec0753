<script  type="text/javascript">

    var globalDestination = "ctv_web";
    var globalPlatform = "desktop";
    var playerIntervalId = 0;
    var player = {};
    var readyCount = 0;

    $(document).ready(function () {
        player = BellMedia.videoHub.videoPlayer({
            destinationCode: globalDestination,
            platformCode: globalPlatform,
            autoplay: 0,
            width: 480,
            height: 270,
            playerName: 'vplayer',
            playerEl: 'mediaplayerdiv',
            omniturePlayerName: 'CTV Spotlight Player',
            playerZone: 'TheTonightShowStarringJimmyFallon',
            cmsId: 554514,
            liveId: '',
            envr: 'prod'
        });
        initPlayer();
    });

    function initPlayer() {
        readyCount++;
        if (readyCount >= 2) {
            player.embedPlayer();
        }
    }

    $(document).on('onready.etsapi', initPlayer);

</script>        


