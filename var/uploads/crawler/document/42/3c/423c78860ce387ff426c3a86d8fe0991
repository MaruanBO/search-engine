		function loadRR_Placements(jsonData){
			var divname = '';
			var findDot = 0;
			var lotTitle = '';
			var thisLotRef = ''
			bidInfo = '';
			bidCount = 0;
			var bidData = '';
			var bidDataJSON = ''
			var currencySymbol = '';
			if (jsonData.length == 0){
				J('#lotsUpcoming').show();
				}
			else {
				//   ga ecommerce tracking variables
				var cfPageID = 'homepage';
				var thisCategoryVal = '';
				var thisListVal = '';
				if (cfPageID == 'viewLot'){
					var isPastLot = 'false';
					if (isPastLot == 'YES'){
						thisListVal = 'past lot';
						}
					else {
						thisListVal = 'upcoming lot';
						}
					}
				else if (cfPageID == 'category'){
					thisListVal = 'category';
					}
				else if (cfPageID == 'superCategory'){
					thisListVal = 'category';
					}
				else if (cfPageID == 'homepage'){
					thisListVal = 'homepage';
					}
				else if (cfPageID == 'browseUpcoming'){
					thisListVal = 'browse upcoming';
					}
				else if (cfPageID.substring(0,2) == 'my'){
					thisListVal = 'my invaluable';
					}
				var currentLotRef = '';
				var thisBidInDollars = 0;
				var thisHouseName = '';
				var rrImpressions = new Array();
				//  get bids and bidder counts in one object referenced by lot ref
				var lotList = '';
				for (var a = 0; a < jsonData.length; a++){
					//  catch for hero data in RR
					if (typeof jsonData[a].items != 'undefined'){
						for (var b = 0; b < jsonData[a].items.length; b++){
							if (lotList.length > 0){
								lotList = lotList + ','
								}
							lotList = lotList +  jsonData[a].items[b].id;
							}
						}
					}
				J.ajax({
					dataType: 'json',
					url: '/lib/jQueryObj/jFineArtObj.cfc?wsdl',
					data: {
						method : 'getBidAndBidderCount',
						'lotRefList' : lotList
						},
					success: function(data){
						bidDataJSON = data;
							}
						}).done(function(){
						for (var a = 0; a < jsonData.length; a++){
							divname = jsonData[a].placement_name;
							findDot = divname.indexOf('.');
							divname = divname.substr(findDot + 1);
						// hide vertical placement from iPad
							if (divname != 'lotright'  || window.screen.width >= 1024){
								J('#' + divname + ' #rrTitle').html(jsonData[a].message);
							//  catch for hero data in RR
							if (typeof jsonData[a].items != 'undefined'){
									for (var b = 0; b < jsonData[a].items.length; b++){
											J('#' + divname + ' #rrImageBox_' + b + ' img').attr('src',jsonData[a].items[b].imageURL);
										//set title
										lotTitle = jsonData[a].items[b].name;
										J('#' + divname + ' #rrLotText_' + b).html('<a href="' + jsonData[a].items[b].linkURL + '">' +  lotTitle + '</a>')
										J('#' + divname + ' #rrImageBoxLink_' + b).attr('href',jsonData[a].items[b].linkURL);

										thisLotRef = jsonData[a].items[b].id;
										if (bidDataJSON[thisLotRef] === undefined){
											bidInfo = '$0';
											bidCount = 0;
											}
										else {
											bidInfo = bidDataJSON[thisLotRef].currencySymbol  + bidDataJSON[thisLotRef].currentBid;
											bidCount = parseInt(bidDataJSON[thisLotRef].totalOrderBids);
											}
										if (bidCount == 1){
											bidInfo = bidInfo + ' (1 bid)';
											}
										else if (bidCount > 1){
											bidInfo = bidInfo + ' (' + bidCount + ' bids)';
											}
										else {
											bidInfo = bidInfo + ' (starting bid)';
											}
										J('#' + divname + ' #rrLotText2_' + b).html(bidInfo);
										J('#' + divname + ' #rrLotBox_' + b).show();
										//build array for GA eCommerce
										if (thisCategoryVal == ''){
											thisCategoryVal = jsonData[a].message;
											}
										 if (bidDataJSON[thisLotRef] === undefined){
											thisBidInDollars = 0;
											thisHouseName = '';
										 	}
										 else {
										 	thisBidInDollars = bidDataJSON[thisLotRef].bidInDollars;
											thisHouseName = bidDataJSON[thisLotRef].houseName;
										 	}
										rrImpressions[rrImpressions.length]= {
									       'name': jsonData[a].items[b].name,
									       'id': jsonData[a].items[b].id,
									      'price': thisBidInDollars,
									      'brand': thisHouseName,
									       'category': thisCategoryVal,
									       'list': thisListVal,
									       'position': 1,
									       'variant' :'Active lot'
									     	} ;
										}
									J('#' + divname).show();
									J('.rr_rec_lotText').dotdotdot({height:36});
									}
								}
							}

						if (lotImpressions.length > 0){
							var multiImpressions = lotImpressions.concat(rrImpressions);
							}
						else {
							var multiImpressions = rrImpressions;
							}

					// push GA eCommerce
						dataLayer.push({
							 'ecommerce': {
						    	'impressions': multiImpressions
						  		}
							});
						});
					}
				}

	// end rich relevance function

		function slideUkHeader(direction){
			if (direction == 'down'){
				J('#topNavbar').addClass('ukHeaderSlide');
				}
			else {
				J('#topNavbar').removeClass('ukHeaderSlide');
				}
			}

		function logoutAndRedirect(newURL){
			var isSecurePage = 0;
			if (isSecurePage == 1){
				var logoutURL =  'https://secure.invaluable.com/signin/signout.cfm';
				}
			else {
				var logoutURL =  'http://www.invaluable.com/logout.cfm';
				}
			jQ.ajax({
				url: logoutURL,
				type: 'get',
				dataType: 'text',
				async: false,
	           	success: function(data, textStatus, jqXHR){
	           		window.location = newURL;
					},
				error: function(){
					window.location = logoutURL;
					window.location = newURL;
					}
				});
			}

		function noEnter(e){
        	var key;
        	// firefox vs. ie
        	if (window.event){
        		key = window.event.keyCode;
        		}
        	else {
        		key = e.which;
        		}
        	if (key == 13 &&  (J('#sliderSearchText').val() == '' || J('#sliderSearchText').val() == 'What do you collect?')){
        		J('#sliderSearchText').val('What do you collect?');
        		return false;
        		e.stopPropagation();
        		}
        	else {
        		return true;
        		}
    		}
		var 	isLoggedIn_slider = false;
		_udn = ".invaluable.com";
	</script>


