<script type="text/javascript">
mi__dart_demo_gender = dart_ad_cookie_gender;
mi__dart_demo_birth_year = dart_ad_cookie_birth_year;
mi__dart_demo_income = dart_ad_cookie_income;

if (mi__dart_demo_gender || mi__dart_demo_birth_year || mi__dart_demo_income) {
  mi__ad_cookie = 1;
}

if (mi__dart_demo_birth_year) {
  today_year = new Date().getFullYear();
  age = today_year - mi__dart_demo_birth_year;
  if (age < 13) {}
  else if (age >= 13 && age <=17) {
    mi__dart_demo_age = '13-17';
  }
  else if (age <= 20) {
    mi__dart_demo_age = '18-20';
  }
  else if (age >= 76) {
    mi__dart_demo_age = '76-200';
  }
  else {
    if (age % 5 == 0) {
      min_age = age - 4;
      max_age = age;
    }
    else {
      min_age = age - (age % 5) + 1;
      max_age = age + (5 - (age % 5));
    }
    mi__dart_demo_age = min_age + "-" + max_age;
  }
}

