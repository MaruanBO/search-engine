		<script type="text/javascript">
		var mouse_not_over_caros=1;
		function rotateHomepageCaros () {
			if (window.mouse_not_over_caros) {
				$('.splash_caros .splash:eq(0)').animate({'opacity':'0'},'fast',function(){});
				$('.splash_caros .splash:eq(1)').animate({'opacity':'1'},'slow',function(){});
				$('.splashes').css({'height':($('.splash').height()+35)+'px'});
				$('.splash_caros').animate({'left':'-'+$('.splashes').width()+'px'},'normal',function(){
					$('.splashes .splash:eq(0)').appendTo('.splash_caros');
					$('.splash_caros').css({'left':'0px','width':'15000px'});
				});				
			}
			setTimeout("rotateHomepageCaros()",7000);
		}
		$('.splash').mouseover(function(){window.mouse_not_over_caros=0;});
		$('.splash').mouseout(function(){window.mouse_not_over_caros=1;});
		$(document).ready(function(){
			$('.splash').css({'width':($('.splashes').width()-50)+'px'});
			$('.splashes').css({'height':($('.splash').height()+35)+'px'});
			if (1<$('.splash_caros .splash').length) {
				$('.splash_caros .splash:eq(1)').css('opacity','0');
				setTimeout("rotateHomepageCaros()",7000);
			}
		});
		</script>
		
