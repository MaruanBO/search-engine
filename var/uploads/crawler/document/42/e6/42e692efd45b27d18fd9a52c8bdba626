				$.getJSON('https://secure.isidewith.com/_scripts/ajax/get_region/',function(d){
					if (d.country!='US'){
						$('#bml').remove();
					} else {
						quiz_link = '/'+d.f_url+'-voter-guide';
						$.get(quiz_link,function(p){if ($(p).find('.subIssue').length==0){$('#vg').remove();} else {$('#vg a').attr('href',quiz_link);}});
					}
				});
