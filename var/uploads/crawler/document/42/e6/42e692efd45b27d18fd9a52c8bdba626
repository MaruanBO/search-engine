				<script type="text/javascript">
				$(document).ready(function(){
					$('.polls').each(function(){
						var maxHeight = 0;
						$(this).find('.poll').each(function(){
							if (maxHeight<$(this).height()) {
								maxHeight = Math.ceil($(this).height());
							}
						});
						$(this).find('.poll').height(maxHeight);
					});
				});
				</script>
