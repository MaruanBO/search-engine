	<script>
	function getUserInfo(){
			console.log('https://secure.isidewith.com/_scripts/ajax/gui/'+getCookie2('user_id')+'.js');
		if (getCookie2('user_id')!=null && getCookie2('user_id')!='') {
			$.ajax({
				type:'POST',
				url:'https://secure.isidewith.com/_scripts/ajax/gui/'+getCookie2('user_id')+'.js',
				data:{'pid':getCookie2('PHPSESSID'),'lid':getCookie2('logged_in')},
				//async:false,
				dataType:'json',
				success:function(user_info){
					if (user_info['id']==undefined) {
						blankUserInfo();
					} else {
						$('#tryit,#shareit,#answerit').fadeOut(function(){$(this).remove()});
						html = '';
						html += '<div class="user_name_container"><a class="user_name" href="/results/'+user_info['id']+'">'+user_info['name']+'</a></div>';
						//html += '<div class="user_options"><a href="/results/'+user_info['id']+'">My Results</a> <span class="div">&nbsp;|&nbsp;</span> <a href="#">Settings</a> <span class="div">&nbsp;|&nbsp;</span> <a href="https://secure.isidewith.com/action/logout?r='+encodeURIComponent(window.location)+'">Logout</a></div>';
						html += '<div class="user_options">'+(user_info['candidate']==undefined?'':'<a href="https://secure.isidewith.com/account/candidate_settings" style="color:#ffe9a6">Candidate&nbsp;Profile</a> <span class="div">&nbsp;|&nbsp;</span> ')+'<a href="/results/'+user_info['id']+'">My Results</a> <span class="div">&nbsp;|&nbsp;</span> <a href="https://secure.isidewith.com/action/logout?r='+encodeURIComponent(window.location)+'">Logout</a></div>';
						$('.user_account_info').html(html);
						_gaq.push(['_trackEvent','Site Actions','Account','signed_in_pageview=true']);
					}
				}
			});
		} else {
			blankUserInfo();
		}
	}
	function blankUserInfo() {
		//$('.user_account_info').html('<a href="/social-login?r='+encodeURIComponent(window.location)+'">login</a>');
		$('<div class="user_info"><span class="login"><a style="padding:7px" href="https://secure.isidewith.com/social-login?r='+encodeURIComponent(window.location)+'">login</a></span></div>').appendTo('.race_tabs_container');
		//$('#tryit').show();
	}
	$(document).ready(function(){
		getUserInfo();
	});
	</script>
