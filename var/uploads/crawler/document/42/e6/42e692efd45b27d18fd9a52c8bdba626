		function hideMainImage (t,imgs) {
			if (0 < imgs.length) {
				console.log(imgs.splice(1,1).join(','));
				$(t).replaceWith('<img onerror="hideMainImage(this,['+imgs.splice(1,1).join(',')+'])" src="//d3ky08o2dntqg3.cloudfront.net/news/images/issues/'+imgs[0]+'">');
			}
		}
