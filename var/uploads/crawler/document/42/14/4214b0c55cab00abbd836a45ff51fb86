            modal.find('.success_msg').hide();
        }

        modal.find('.modal-title').text(title);

        modal.modal('show');
    }



    $(document).ready(function(){


        $('.the_checkbox').change(function () {
            var check = ($(this).filter(":checked").length == $(this).length);

            $(this).parents('.item_list').find('.item_check').prop("checked", check);
        });

    })
