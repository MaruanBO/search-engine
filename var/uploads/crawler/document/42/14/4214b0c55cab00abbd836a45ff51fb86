    function show_ads()
        {
            var ad_units = new Array();

            
                                    ad_units.push(31);
                    ad_units.push(81);
                    ad_units.push(101);
                    ad_units.push(112);
                                                                                               
            
            var ad_unit_string = "";

            $.each(ad_units,function(key,value){
                if(ad_unit_string)
                    ad_unit_string += ',';

                ad_unit_string += value;
            })


            /**
             * Adding Categories 
             */
            var category="";
            var categories = new Array();

            
            

            $.each(categories,function(key,value){
                if(category)
                    category += ',';

                category += value;
            })

            
                        
            /*$.getJSON( "//ads.tunenerve.com/get_ads.php?ad_units="+ad_unit_string+"&category="+category, function( data ) {
                $.each(data,function(key,value){

                    var iframe= '<iframe id="tune_ads_iframe_'+value.ad_unit+'" ' +
                            'width="'+value.width+'" height="'+value.height+'" scrolling="no"' +
                            ' marginwidth="0" marginheight="0" ' +
                            'frameborder="0" src="'+value.banner_path+'" style="border:' +
                            ' 0px;"></iframe>';

                    $('#tune_ad_'+value.ad_unit).html(iframe);
                })
            });*/
            var xhrFields = {};
            
            xhrFields.withCredentials = true;

            $.ajax({
                url : "//ads.tunenerve.com/get_ads.php",
                type:'GET',
                dataType : 'json',
                data: { ad_units:ad_unit_string,  category:category},
                crossDomain : true,
                xhrFields: xhrFields,
                success : function(data)
                {
                    $.each(data,function(key,value){

                        var iframe= '<iframe id="tune_ads_iframe_'+value.ad_unit+'" ' +
                                'width="'+value.width+'" height="'+value.height+'" scrolling="no"' +
                                ' marginwidth="0" marginheight="0" ' +
                                'frameborder="0" src="'+value.banner_path+'" style="border:' +
                                ' 0px;"></iframe>';

                        $('#tune_ad_'+value.ad_unit).html(iframe);
                        $('#tune_ad_'+value.ad_unit).parent('div.empty-ads').show();
                    
                    })
                },
                complete:function(data) {
                    /*$(".ad_box,.adbox-holder").each(function(index, value){ 
            
                            var _this = $( value );
                            
                            if ( _this.find('iframe').length == 0 ) {
                                //console.log( value );
                                _this.hide();
                            }
                            
                        });*/
                }
            });
            
        }


