            function toggle_playlist_box()
            {
                $('#playlist-block-container').slideToggle(function(){
                    if($('#playlist-block-container').is(':visible'))
                    {
                        $.cookie('playlist-visible','yes',{ expires:366 });
                    }else
                    {
                        $.cookie('playlist-visible','no',{ expires:366 });
                    }
                });
                
            }
            
        </script> <script type="text/javascript">
           
            function show_alert()
            {
                $('#alert_message').show()
                .delay(10000).fadeOut('slow',function(){
                    $('#alert_message .alert_message').removeClass('text-error')
                });
            
            }
            
