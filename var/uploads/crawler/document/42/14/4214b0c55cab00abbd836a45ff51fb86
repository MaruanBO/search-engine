            $(document).ready(function(){
                
                checkMenuOpener();
                

                $('body').on('submit','#login_form',function(e){
            
                    e.preventDefault();
                    
                    if(tune_userid)
                    {
                        $('#loginform').modal('hide');
                        return false;
                    } 

            

                    var username = $(this).find('.modal-username').val();
                    var password = $(this).find('.modal-password').val();
                    var remember = $('#rememberme :checkbox:checked').length > 0 ? 'yes' : '';
                    var button = $(this).find('.modal-login');
                    var alertdiv = $(this).parent().find('.alert-danger');

                    alertdiv.hide();
                    //Pre checking values
                    if(username==''){
                        alertdiv.html('Username was empty').show();
                        return false;
                    } 

                    if(password==''){
                        alertdiv.html('Pasword was empty').show();
                        return false;
                    } 

                    button.button('loading');
                    $.ajax({
                        url : baseurl + '/ajax.php',
                        data : { mode:'login',password:password,remember:remember,username:username},
                        dataType : 'json',
                        type : 'post',
                        success : function(data){
                            if(data.error){
                                button.button('reset');

                                alertdiv.html(data.error).show();
                            }

                            if(data.success)
                            {
                                $('#guest_li').remove();
                                $('#logged_in_ul li#user-logged-in').html(data.user_li);
                                $('li#header-playlist').html(data.playlist_li);
                                

                                $('#loginform').modal('hide');

                                tune_userid = data.user.userid;
                                                            }
                        }
                    })
                })
                  
                
                $('.opener-ul').click(function(event) {
                  if($('.opener-ul').find('ul.dropdown-menu').css('display')!='block')
                    $('.opener-ul').find('ul.dropdown-menu').css('display', 'block');
                  else
                    $('.opener-ul').find('ul.dropdown-menu').css('display', '');
                });
 
            });
            
            		    
