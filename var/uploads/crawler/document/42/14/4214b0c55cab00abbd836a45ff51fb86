var admin_ajax_url = 'http://tune.pk/admin_area/admin_ajax.php';

    function tune_alert(type,title,message)
    {
        var modal = $('#modal_alert');
        if(type=='success')
        {
            modal.find('.success_msg').show().find('.msg_space').text(message);
            modal.find('.error_msg').hide();
        }
        else
        {
            modal.find('.error_msg').show().find('.msg_space').text(message);
            modal.find('.success_msg').hide();
        }

        modal.find('.modal-title').text(title);

        modal.modal('show');
    }

    function deactivate_video(videoid)
    {
        var data = $('#deactivate-form-'+videoid).serialize();
        data += '&mode=deactivate_video';



        var modal =  $('#video-deactivate-'+videoid);
        $(modal).find('.btn-action').button('loading');

        $.ajax({
            'url' : admin_ajax_url,
            data : data,
            type : 'post',
            success : function(data)
            {

                modal.modal('hide');
                if(data.success)
                {

                    tune_alert('success','Video deactivated','Video has been deactivated');
                    $('#report-'+videoid).html(data.template);
                }else
                {
                    tune_alert("error","Error!",data.error);
                }

                $(modal).find('.btn-action').button('reset');

            },
            dataType : 'json'

        })
    }


    function activate_video(videoid)
    {
        var data = $('#activate-form-'+videoid).serialize();
        data += '&mode=activate_video';


        var modal =  $('#video-activate-'+videoid);
        $(modal).find('.btn-action').button('loading');

        $.ajax({
            'url' : admin_ajax_url,
            data : data,
            type : 'post',
            success : function(data)
            {

                modal.modal('hide');
                if(data.success)
                {

                    tune_alert('success','Video activated','Video has been activated');
                    $('#report-'+videoid).html(data.template);
                }else
                {
                    tune_alert("error","Error!",data.error);
                }

                $(modal).find('.btn-action').button('reset');

            },
            dataType : 'json'

        })
    }


    function close_report(videoid)
    {
        $.ajax({
            'url' : admin_ajax_url,
            data : { videoid:videoid,mode:'close_report' },
            type : 'post',
            success : function(data)
            {
                if(data.success)
                {
                    tune_alert('success','Report closed!','Video report has been closed successfully!');
                    $('#report-'+videoid).addClass('report-closed').removeClass('report-opened').html(data.template);
                }else
                {
                    tune_alert("error","Error!",data.error);
                }

            },
            dataType : 'json'

        })
    }

    function activate_video_close(videoid)
    {
        activate_video(videoid);
        close_report(videoid);
    }

    function open_report(videoid)
    {
        $.ajax({
            'url' : admin_ajax_url,
            data : { videoid:videoid,mode:'open_report' },
            type : 'post',
            success : function(data)
            {
                if(data.success)
                {
                    tune_alert('success','Report opened!','Video report has been opened successfully!');
                    $('#report-'+videoid).addClass('report-opened').removeClass('report-closed').html(data.template);
                }else
                {
                    tune_alert("error","Error!",data.error);
                }

            },
            dataType : 'json'

        })
    }



    /**
     * Function used to make confirmation about any action
     */
    function cb_confirm(title,text,callback)
    {
        $('#confirm .modal-header h4').text(title);
        $('#confirm .modal-body').html(text);
        $('#confirm-yes').bind('click',callback);
        $('#confirm').modal('show');
    }


    /**
     * Clears confirm form events and text
     */
    function clear_confirm(){
        $('#confirm .modal-header h3').text('');
        $('#confirm .modal-body').html('');
        $('#confirm-yes').unbind('click');
    }

    function close_confirm(){

        clear_confirm();
        $('#confirm').modal('hide');
    }


    /**
     * Submit form with extra values attached
     */
    function submit_form($formId,input)
    {
        $button = '<input type="hidden" name="'+input+'" value="yes" />';
        $('#'+$formId).append($button).submit();
    }


