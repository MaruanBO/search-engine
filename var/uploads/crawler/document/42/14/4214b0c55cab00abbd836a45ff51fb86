    function show_error(msg,ID,fade)
    { 
        
        $('#alert_message .alert_message')
        .addClass('alert-inverted-err')
        .text(msg);
        
        $('#alert_message').show()
        .delay(10000).fadeOut('slow');
    }
    
    function show_message(msg,ID,fade)
    { 
        $('#alert_message .alert_message')
        .removeClass('alert-inverted-err')
        .text(msg);
        
        $('#alert_message').show().delay(10000).fadeOut('slow');
    }



   

