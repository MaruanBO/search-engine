/** Mobile nav - defined here instead of .ready so that it
  becomes usable sooner, instead of waiting for things 
  like third-party scripts to load... **/  
  $(".site-navigation--simple li a").live('click',
    function() {

      var showme = "." + $(this).data("nav_toggle");
      
      $('.site-navigation--simple li a').each(function(){
        var hideme = "." + $(this).data("nav_toggle");
        if ( showme != hideme ){
          $(hideme).removeClass("toggle--active");
        }
      });
    
      $(showme).toggleClass("toggle--active");
      
    }
  );
</script>

