		 openColumn50 : '<div class="columnComponent_Column" style="width: 50%;">',
		 rowOverlap : false,
		 columnType : 0,
		 suggestionsArray : [],
		 convertAdSizes : function(tileSize){
		 	if ((tileSize.columns == 1) && (tileSize.rows == 1)) {
		 		return 1; 
		 	}
		 	if ((tileSize.columns == 2) && (tileSize.rows == 1)) {
		 		return 2;
		 	}
		 	if ((tileSize.columns == 3) && (tileSize.rows == 1)) {
		 		return 3; 
		 	}
		 	if ((tileSize.columns == 1) && (tileSize.rows == 2)) {
		 		return 4; 
		 	}
		 	if ((tileSize.columns == 3) && (tileSize.rows == 2)) {
		 		return 5; 
		 	}
		 	if ((tileSize.columns == 3) && (tileSize.rows == 3)) {
		 		return 6; 
		 	}
		 },
		
		 reserveSpace : function(gridSlot,suggestedItem){
			 PRECISION.reserveSpaceArray[gridSlot]={
				 "ajaxURL" : suggestedItem.ajaxURL,
				 "program" : suggestedItem.program,
				 "size" : suggestedItem.adSize,
				 "columnType" : PRECISION.columnType,
				 "overlap" : false,
				 "treatmentCode" : suggestedItem.treatmentCode
			};
			if (suggestedItem.adSize == 2){
				PRECISION.reserveSpaceArray[gridSlot+1].ajaxURL = "R";
			}
			if (suggestedItem.adSize == 3){
				PRECISION.reserveSpaceArray[gridSlot+1].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+2].ajaxURL = "R";				
			}
			if (suggestedItem.adSize == 4){
				PRECISION.reserveSpaceArray[gridSlot].overlap = true;
				PRECISION.reserveSpaceArray[gridSlot+3].ajaxURL = "R";
			}	
			if (suggestedItem.adSize == 5){
				PRECISION.reserveSpaceArray[gridSlot].overlap = true;
				PRECISION.reserveSpaceArray[gridSlot+1].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+2].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+3].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+4].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+5].ajaxURL = "R";																
			}	
			if (suggestedItem.adSize == 6){
				PRECISION.reserveSpaceArray[gridSlot].overlap = true;
				PRECISION.reserveSpaceArray[gridSlot+1].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+2].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+3].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+4].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+5].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+6].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+7].ajaxURL = "R";
				PRECISION.reserveSpaceArray[gridSlot+8].ajaxURL = "R";																				
			}											
			if (PRECISION.trackingDetails == null) {
				PRECISION.trackingDetails = suggestedItem.program + "_" + suggestedItem.adSize + "_" + gridSlot;
			} else {
				PRECISION.trackingDetails += ";" + suggestedItem.program + "_" + suggestedItem.adSize + "_" + gridSlot;
			}
			PRECISION.interactTrackingDetails.push(suggestedItem.treatmentCode);
		 },
		
		 reportGrid : function(){
			dcsMultiTrack("WT.z_programList",PRECISION.trackingDetails,"WT.dl", "1");
			interactTreatmentAjax("load", PRECISION.interactTrackingDetails);
		 },
		 
		
		buildGridHTML : function(){
			var row1Result = PRECISION.buildGridHTMLRow1('');
			if (row1Result.nextRow == 2) {
				var row2Result = PRECISION.buildGridHTMLRow2(row1Result.outputHTML);
				if (row2Result.nextRow == 3) {
					return PRECISION.buildGridHTMLRow3(row2Result.outputHTML);
				}
				else { 
					return row2Result.outputHTML;
				}
			}
			else if (row1Result.nextRow == 3) {
				return PRECISION.buildGridHTMLRow3(row1Result.outputHTML);
			}
			return;
		},
		
		buildSectionHTML : function(arrayIndex,ajaxURL,addMargin){
			addMargin = typeof addMargin !== 'undefined' ? addMargin : false;
			if (addMargin) {
				var sectionHTML = '<section id="precisionMarketing' + arrayIndex + '" style="margin-top:10px;"></section>';
			}
			else {
				var sectionHTML = '<section id="precisionMarketing' + arrayIndex + '"></section>';
			}
				if (ajaxURL != 'R') {
					sectionHTML += '<script>'
							sectionHTML += '$.get("' + ajaxURL + '", function(result) {';
							sectionHTML += 'if (result == "<div style=\\"display:inline;\\" class=\\"cq-dd-paragraph\\"></div>") {';
							sectionHTML += 'console.log("tile ' + arrayIndex + ' unavailable or deactivated")';
							sectionHTML += '}';
							sectionHTML += '$result = $(result);';
							sectionHTML += '$result.appendTo("#precisionMarketing' + arrayIndex + '");'
							sectionHTML += 'initializeComponents();';
							sectionHTML += '$("#precisionMarketing' + arrayIndex + '").click(function(e) {';
								sectionHTML += 'interactTreatmentAjax("click", "' + PRECISION.reserveSpaceArray[arrayIndex].treatmentCode + '");';
							sectionHTML += '});';
						sectionHTML +='});';
					sectionHTML +='<\/script>';
				}
			return sectionHTML;
		 },
		
