 <script>
	require(["janrain", "janrain-logger"], function() {
	    GLOBAL.JANRAIN.authState = GLOBAL.JANRAIN.USER.AUTH;
	    GLOBAL.JANRAIN.flowState = GLOBAL.JANRAIN.FLOW.SOCIALCONNECT;
	    GLOBAL.JANRAIN.subflowState = GLOBAL.JANRAIN.SUBFLOW.NOTAPPLICABLE;
	    JANRAIN.janrainDefaultSettings(GLOBAL);
	    janrain.settings.capture.registerFlow = 'socialRegistration';
	    JANRAIN.janrainInitLoad();
	    janrain.ready = true;
	});
		var homepageTemplate = {
		  backgroundImageMobile: '',
		  backgroundImageTablet: '',
		  backgroundImageDesktop: '',
		  configureImageVars: function() {
		    homepageTemplate.backgroundImageMobile = homepageTemplate.backgroundImageMobile !== '' ? homepageTemplate.backgroundImageMobile : null;
		    homepageTemplate.backgroundImageTablet = homepageTemplate.backgroundImageTablet !== '' ? homepageTemplate.backgroundImageTablet : null;
		    homepageTemplate.backgroundImageDesktop = homepageTemplate.backgroundImageDesktop !== '' ? homepageTemplate.backgroundImageDesktop : null;
		  },
		  adjustBackgroundImage: function() {
		    if (homepageTemplate.backgroundImageDesktop && componentGlobalVars.windowSize == 'desktop') {
		      $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageDesktop +')');
		    } else if (homepageTemplate.backgroundImageTablet && componentGlobalVars.windowSize == 'tablet') {
		      $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageTablet +')');
		    } else if (homepageTemplate.backgroundImageMobile && componentGlobalVars.windowSize == 'mobile') {
		      $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageMobile +')');
		    } else {
		      if (homepageTemplate.backgroundImageDesktop) {
		        $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageDesktop +')');
		      } else if (homepageTemplate.backgroundImageTablet) {
		        $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageTablet +')');
		      } else if (homepageTemplate.backgroundImageMobile) {
		        $('.homepageTemplate_BackgroundWrapper').css('background-image', 'url(' + homepageTemplate.backgroundImageMobile +')');
		      }
		    }
		  }
		}
		$(document).ready(function() {
	  		homepageTemplate.configureImageVars();
  		 if(GLOBAL.pageWidth < 714) {
		      componentGlobalVars.windowSize = 'mobile';
		      homepageTemplate.adjustBackgroundImage();
		  }
		  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
		      componentGlobalVars.windowSize = 'tablet';
		      homepageTemplate.adjustBackgroundImage();
		  }
		  if(GLOBAL.pageWidth > 1065) {
		      componentGlobalVars.windowSize = 'desktop';
		      homepageTemplate.adjustBackgroundImage();
		  }
		});
		$(window).resize(function() {
			if (GLOBAL.pageWidth < 714) {
			  if (componentGlobalVars.windowSize != 'mobile') {
			      componentGlobalVars.windowSize = 'mobile';
			      homepageTemplate.adjustBackgroundImage();
			  }
			}
		  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
		    if (componentGlobalVars.windowSize != 'tablet') {
		      componentGlobalVars.windowSize = 'tablet';
		      homepageTemplate.adjustBackgroundImage();
		    }
		  }
		  if(GLOBAL.pageWidth > 1065) {
		    if (componentGlobalVars.windowSize != 'desktop') {
		      componentGlobalVars.windowSize = 'desktop';
		      homepageTemplate.adjustBackgroundImage();
		    }
		  }
		});
	</script>

