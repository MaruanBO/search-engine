						outputHTML += '</div><script>++templateVariables.columnComponentCount;templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount] = new columnComponent({wrapper: $("#adSubRow3")});templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount].init();<\/script></section>';

						outputHTML += '</div>'
						nextRow = 'end';
					}
					else if (!PRECISION.reserveSpaceArray[3].overlap && !PRECISION.reserveSpaceArray[4].overlap && PRECISION.reserveSpaceArray[5].overlap) {
						outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (66.66%,33.34%)" id="adRow2">';
						outputHTML += PRECISION.openColumn66;

						outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (50%,50%)" id="adSubRow2">';
						outputHTML += PRECISION.openColumn50;
						outputHTML += PRECISION.buildSectionHTML(3, PRECISION.reserveSpaceArray[3].ajaxURL);
						outputHTML += '</div>'
						outputHTML += PRECISION.openColumn50;
						outputHTML += PRECISION.buildSectionHTML(4, PRECISION.reserveSpaceArray[4].ajaxURL);
						outputHTML += '</div>'
						outputHTML += '</div><script>++templateVariables.columnComponentCount;templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount] = new columnComponent({wrapper: $("#adSubRow2")});templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount].init();<\/script></section>';

						outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (50%,50%)" id="adSubRow3">';
						outputHTML += PRECISION.openColumn50;
						outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL);
						outputHTML += '</div>'
						outputHTML += PRECISION.openColumn50;
						outputHTML += PRECISION.buildSectionHTML(7, PRECISION.reserveSpaceArray[7].ajaxURL);
						outputHTML += '</div>'
						outputHTML += '</div><script>++templateVariables.columnComponentCount;templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount] = new columnComponent({wrapper: $("#adSubRow3")});templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount].init();<\/script></section>';

						outputHTML += '</div>'
						outputHTML += PRECISION.openColumn34;
						outputHTML += PRECISION.buildSectionHTML(5, PRECISION.reserveSpaceArray[5].ajaxURL);
						outputHTML += '</div>'
						nextRow = 'end';
					}
					else {
						outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (33.33%,33.33%,33.33%)" id="adRow2">';
						outputHTML += PRECISION.openColumn33;
						outputHTML += PRECISION.buildSectionHTML(3, PRECISION.reserveSpaceArray[3].ajaxURL);
						if ((PRECISION.reserveSpaceArray[4].overlap || PRECISION.reserveSpaceArray[5].overlap)  && PRECISION.reserveSpaceArray[6].ajaxURL != 'R') {
							outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL,true);
						}
						outputHTML += '</div>'
						outputHTML += PRECISION.openColumn33;
						outputHTML += PRECISION.buildSectionHTML(4, PRECISION.reserveSpaceArray[4].ajaxURL);
						if ((PRECISION.reserveSpaceArray[3].overlap || PRECISION.reserveSpaceArray[5].overlap) && PRECISION.reserveSpaceArray[7].ajaxURL != 'R') {
							outputHTML += PRECISION.buildSectionHTML(7, PRECISION.reserveSpaceArray[7].ajaxURL,true);
						}
						outputHTML += '</div>'
						outputHTML += PRECISION.openColumn33;
						outputHTML += PRECISION.buildSectionHTML(5, PRECISION.reserveSpaceArray[5].ajaxURL);
						if ((PRECISION.reserveSpaceArray[3].overlap || PRECISION.reserveSpaceArray[4].overlap) && PRECISION.reserveSpaceArray[8].ajaxURL != 'R') {
							outputHTML += PRECISION.buildSectionHTML(8, PRECISION.reserveSpaceArray[8].ajaxURL,true);
						}
						outputHTML += '</div>'
						if (PRECISION.reserveSpaceArray[3].overlap || PRECISION.reserveSpaceArray[4].overlap || PRECISION.reserveSpaceArray[5].overlap) {
							nextRow = 'end';
						}
						else {
							nextRow = 3;
						}
					}
				break;
			}
			outputHTML += '</div><script>++templateVariables.columnComponentCount;templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount] = new columnComponent({wrapper: $("#adRow2")});templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount].init();<\/script></section>';
			return {nextRow: nextRow, outputHTML: outputHTML};
		},
		
		buildGridHTMLRow3 : function(outputHTML){
			switch(PRECISION.reserveSpaceArray[6].columnType) {
				case 1: 
					outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (100%)" id="adRow3">';
					outputHTML += PRECISION.openColumn100;
					outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL);
					outputHTML += '</div>'
					break;
				case 2: 
					outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (66.66%,33.34%)" id="adRow3">';
					outputHTML += PRECISION.openColumn66;
					outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL);
					outputHTML += '</div>'
					outputHTML += PRECISION.openColumn34;
					outputHTML += PRECISION.buildSectionHTML(8, PRECISION.reserveSpaceArray[8].ajaxURL);
					outputHTML += '</div>'
					break;
				case 3: 
					outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (33.34%,66.66%)" id="adRow3">';
					outputHTML += PRECISION.openColumn34;
					outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL);
					outputHTML += '</div>'
					outputHTML += PRECISION.openColumn66;
					outputHTML += PRECISION.buildSectionHTML(7, PRECISION.reserveSpaceArray[7].ajaxURL);
					outputHTML += '</div>'
					break;
				case 4: 
					outputHTML += '<section class="columnComponent"><div class="columnComponent_Table (33.33%,33.33%,33.33%)" id="adRow3">';
					outputHTML += PRECISION.openColumn33;
					outputHTML += PRECISION.buildSectionHTML(6, PRECISION.reserveSpaceArray[6].ajaxURL);
					outputHTML += '</div>'
					outputHTML += PRECISION.openColumn33;
					outputHTML += PRECISION.buildSectionHTML(7, PRECISION.reserveSpaceArray[7].ajaxURL);
					outputHTML += '</div>'
					outputHTML += PRECISION.openColumn33;
					outputHTML += PRECISION.buildSectionHTML(8, PRECISION.reserveSpaceArray[8].ajaxURL);
					outputHTML += '</div>'
					break;
			}
			outputHTML += '</div><script>++templateVariables.columnComponentCount;templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount] = new columnComponent({wrapper: $("#adRow3")});templateVariables.columnComponentTracker["component"+templateVariables.columnComponentCount].init();<\/script></section>';
			return outputHTML;
		},
		dynamicGridBuilder : function(data) {
			selectedItem = PRECISION.selectItem(1,2,3,4);
			PRECISION.reserveSpace(0,selectedItem);
			if (selectedItem.adSize > 3) {
				PRECISION.rowOverlap = true;
			}
			if (selectedItem.adSize == 4) { sizeInRow = 2; }
			else if (selectedItem.adSize == 2) { sizeInRow = 1; }
			else if (selectedItem.adSize == 1) { sizeInRow = 2; }
			else { sizeInRow = 0;}
			if (sizeInRow == 0) {
				PRECISION.columnType = 1;
				PRECISION.reserveSpaceArray[0].columnType = PRECISION.columnType;
				PRECISION.selectAdditionalRow(2);
				var finalHTML = PRECISION.buildGridHTML();
				$(".precisionMarketingComponent").html(finalHTML);
				PRECISION.reportGrid();
				return;
			}
			else {
				if (sizeInRow == 2) {
					selectedItem = PRECISION.selectItem(1,2,4);
					PRECISION.reserveSpace(1,selectedItem);
					if (selectedItem.adSize == 2) { sizeInRow = 0; } else { sizeInRow = 1; }
					if (selectedItem.adSize == 4) {
						PRECISION.rowOverlap = true;
					}					
					if (sizeInRow == 1) {
						PRECISION.columnType = 4;
						selectedItem = PRECISION.selectItem(1,4);
						PRECISION.reserveSpace(2,selectedItem);						
					}
					else {
						PRECISION.columnType = 3;
						PRECISION.reserveSpaceArray[0].columnType = PRECISION.columnType;
						PRECISION.selectAdditionalRow(2);
						var finalHTML = PRECISION.buildGridHTML();
						$(".precisionMarketingComponent").html(finalHTML);
						PRECISION.reportGrid();
						return;
					}
				}
				else {
					PRECISION.columnType = 2;
					selectedItem = PRECISION.selectItem(1,4);
					PRECISION.reserveSpace(2,selectedItem);
				}
					
				if (selectedItem.adSize == 4) { 
					PRECISION.rowOverlap = true;
				}
				PRECISION.reserveSpaceArray[0].columnType = PRECISION.columnType;
				PRECISION.selectAdditionalRow(2);
				var finalHTML = PRECISION.buildGridHTML();
				$(".precisionMarketingComponent").html(finalHTML);
				PRECISION.reportGrid();
				return;
			}

		},
		selectAdditionalRow : function(rowNumber) {
			if (rowNumber == 2) {
				rowSpace1 = 3;
				rowSpace2 = 4;
				rowSpace3 = 5;
			}
			else { 
				rowSpace1 = 6;
				rowSpace2 = 7;
				rowSpace3 = 8;
			}
			if (PRECISION.rowOverlap) {
				PRECISION.rowOverlap = false;
				if (PRECISION.columnType == 2) {
					if (PRECISION.reserveSpaceArray[rowSpace1].ajaxURL != 'R') {
						selectedItem = PRECISION.selectItem(2);
						PRECISION.reserveSpace(rowSpace1,selectedItem);
					}
					if (PRECISION.reserveSpaceArray[rowSpace3].ajaxURL != 'R') {
						selectedItem = PRECISION.selectItem(1);
						PRECISION.reserveSpace(rowSpace3,selectedItem);
					}
					if (rowNumber == '2') {
						PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
						PRECISION.selectAdditionalRow(3);
						return;
					}
					else {
						PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
						return;
					}
				}
				else {
					if (PRECISION.columnType == 3) {
						if (PRECISION.reserveSpaceArray[rowSpace1].ajaxURL != 'R') {
							selectedItem = PRECISION.selectItem(1);
							PRECISION.reserveSpace(rowSpace1,selectedItem);
						}
						if (PRECISION.reserveSpaceArray[rowSpace2].ajaxURL != 'R') {
							selectedItem = PRECISION.selectItem(2);
							PRECISION.reserveSpace(rowSpace2,selectedItem);
						}
						if (rowNumber == '2') {
							PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
							PRECISION.selectAdditionalRow(3);
							return;
						}
						else {
							PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
							return;
						}
					}
					else {
						if (PRECISION.reserveSpaceArray[rowSpace1].ajaxURL != 'R') {
							selectedItem = PRECISION.selectItem(1);
							PRECISION.reserveSpace(rowSpace1,selectedItem);
						}
						if (PRECISION.reserveSpaceArray[rowSpace2].ajaxURL != 'R') {
							selectedItem = PRECISION.selectItem(1);
							PRECISION.reserveSpace(rowSpace2,selectedItem);
						}
						if (PRECISION.reserveSpaceArray[rowSpace3].ajaxURL != 'R') {
							selectedItem = PRECISION.selectItem(1);
							PRECISION.reserveSpace(rowSpace3,selectedItem);
						}
						if (rowNumber == '2') {
							PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
							PRECISION.selectAdditionalRow(3);
							return;
						}
						else {
							PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
							return;
						}
					}
				}
			}
			else {
				if (rowNumber == 2) {
					selectedItem = PRECISION.selectItem(1,2,3,4);
				}
				else { 
					selectedItem = PRECISION.selectItem(1,2,3);
				}
				if (selectedItem.adSize == 4) { sizeInRow = 2; } 
				else if (selectedItem.adSize == 2) { sizeInRow = 1; }
				else if (selectedItem.adSize == 1) { sizeInRow = 2; }
				else { sizeInRow = 0;}
				PRECISION.reserveSpace(rowSpace1,selectedItem);
				if (selectedItem.adSize > 3) { 
					PRECISION.rowOverlap = true;
				}
				if (sizeInRow == 0) {
					PRECISION.columnType = 1;
					if (rowNumber == '2') {
						PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
						PRECISION.selectAdditionalRow(3);
						return;
					}
					else {
						PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
						return;
					}
				}
				if (sizeInRow == 1) {
					if (rowNumber == 2) {
						selectedItem = PRECISION.selectItem(1,4);
					}
					else { 
						selectedItem = PRECISION.selectItem(1);
					}
					PRECISION.reserveSpace(rowSpace3,selectedItem);
					PRECISION.columnType = 2;
					if (selectedItem.adSize == 4) { 
						PRECISION.rowOverlap = true;
					}
					if (rowNumber == '2') {
						PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
						PRECISION.selectAdditionalRow(3);
						return;
					}
					else {
						PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
						return;
					}
				}
				else {
					if (rowNumber == 2) {
						selectedItem = PRECISION.selectItem(1,2,4);
					}
					else {
						selectedItem = PRECISION.selectItem(1,2);
					}
					if (selectedItem.adSize == 4) { sizeInRow = 2; } 
					else if (selectedItem.adSize == 2) { sizeInRow = 1; }
					else if (selectedItem.adSize == 1) { sizeInRow = 2; }
					else { sizeInRow = 0;}
					PRECISION.reserveSpace(rowSpace2,selectedItem);
					if (selectedItem.adSize == 2) { 
						PRECISION.columnType = 3;
						if (rowNumber == '2') {
							PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
							PRECISION.selectAdditionalRow(3);
							return;
						}
						else {
							PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
							return;
						}
					}
					else {
						if (selectedItem.adSize == 4) {
							PRECISION.rowOverlap = true;
						}
						PRECISION.columnType = 4;
						if (rowNumber == 2) {
							selectedItem = PRECISION.selectItem(1,4);
						}
						else { 
							selectedItem = PRECISION.selectItem(1);
						}
						PRECISION.reserveSpace(rowSpace3,selectedItem);
						if (selectedItem.adSize == 4) { 
							PRECISION.rowOverlap = true;
						}
						if (rowNumber == '2') {
							PRECISION.reserveSpaceArray[3].columnType = PRECISION.columnType;
							PRECISION.selectAdditionalRow(3);
							return;
						}
						else {
							PRECISION.reserveSpaceArray[6].columnType = PRECISION.columnType;
							return;
						}
					}
				}
			}
		},
		 
		buildSuggestionsArray : function(result) {
			for (var i=0;i<result.tiles.length;i++) {
				PRECISION.suggestionsArray.push({"ajaxURL":result.tiles[i].sizes[0].url, "treatmentCode": result.tiles[i].treatmentCode, "available":true, "adSize":PRECISION.convertAdSizes(result.tiles[i].sizes[0]), "program": result.tiles[i].program});
			}
			PRECISION.dynamicGridBuilder(PRECISION.suggestionsArray);
		},
		selectItem : function(requestedSize,requestedSizeB,requestedSizeC,requestedSizeD) { 
			requestedSizeB = typeof requestedSizeB !== 'undefined' ? requestedSizeB : 0;
			requestedSizeC = typeof requestedSizeC !== 'undefined' ? requestedSizeC : 0;
			requestedSizeD = typeof requestedSizeD !== 'undefined' ? requestedSizeD : 0;						
			for (var i=0;i<PRECISION.suggestionsArray.length;i++) {
				if (PRECISION.suggestionsArray[i].available && (PRECISION.suggestionsArray[i].adSize == requestedSize || PRECISION.suggestionsArray[i].adSize == requestedSizeB || PRECISION.suggestionsArray[i].adSize == requestedSizeC || PRECISION.suggestionsArray[i].adSize == requestedSizeD)) {
					PRECISION.suggestionsArray[i].available = false;
					return PRECISION.suggestionsArray[i];
				}
			}
		}
	}

</script>
<section class="precisionMarketingComponent">
	
</section>
<script>
	$.ajax({ url:"/ajax/v1/precisionMarketing/home/tiles?numberOfOffers=20",
		success: function(result) {
			if (result.responseCode == 0) {
	    		PRECISION.buildSuggestionsArray(result);
	    	}
	    	else {
	    		console.log("error in fetched response");
	    		window.location = "/content/interests.html";
	    	}
	 	},
	 	error: function(result) {
	 		console.log("error fetching tiles");
	 		window.location = "/content/interests.html";
	 	}
	 });
</script>
