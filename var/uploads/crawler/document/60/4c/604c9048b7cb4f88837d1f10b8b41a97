function init_topmenu_dimming() {
// do an initial fadeOut to help with situations
// where the overlay doesn't fade in on first hover.
//$("#header-overlay").stop().fadeOut(0);
// we use touchstart and touchend events
var enterEvent = "touchstart mouseenter";
if(!Modernizr.touch){
// if no touch we use mouseenter and mouseleave events
enterEvent = "mouseenter";
}
$('#site-header div.menu').on(enterEvent, function() {
dim_content();
});
//added to fix MS surface issue PTP-2354
if(Modernizr.touch){
$('#site-header div.menu').on('mouseleave', function () {
reveal_content();
});
}
if(!Modernizr.touch){
$('#site-header div.menu').on('mouseleave', function () {
reveal_content();
});
}else{
//remove dim if we touch outside the menu
$(document).on('touchstart', function (e)
{
var container = $("#site-header div.menu");
if (!container.is(e.target) // if the target of the click isn't the container...
&& container.has(e.target).length === 0) // ... nor a descendant of the container
{
reveal_content();
}
});
}
function dim_content() {
$("#header-overlay").addClass("dim");
}
function reveal_content() {
$("#header-overlay").removeClass("dim");
}
}
/* Jquery plugin that makes to navigation menu work with double tap on ipad
Drop-Down Navigation: Responsive and Touch-Friendly
http://osvaldas.info/drop-down-navigation-responsive-and-touch-friendly
By Osvaldas Valutis, www.osvaldas.info
Available for use under the MIT License
*/
(function( $, window, document, undefined )
{
$.fn.doubleTapToGo = function( params )
{
if( !( 'ontouchstart' in window ) &&
!navigator.msMaxTouchPoints &&
!navigator.userAgent.toLowerCase().match( /windows phone os 7/i ) ) return false;
this.each( function()
{
var curItem = false;
$( this ).on( 'click', function( e )
{
var item = $( this );
if( item[ 0 ] != curItem[ 0 ] )
{
e.preventDefault();
curItem = item;
}
});
$( document ).on( 'click touchstart MSPointerDown', function( e )
{
var resetItem = true,
parents = $( e.target ).parents();
for( var i = 0; i < parents.length; i++ )
if( parents[ i ] == curItem[ 0 ] )
resetItem = false;
if( resetItem )
curItem = false;
});
});
return this;
};
})( jQuery, window, document );
function focus_site_search_box() {
$("#siteSearch").addClass("focused");
$("#siteSearch .magnifyingGlass").addClass("active");
}
function blur_site_search_box() {
$("#siteSearch").removeClass("focused");
$("#siteSearch .magnifyingGlass.active").removeClass("active");
}
$(document).ready(function () {
$('#site-header .menu').doubleTapToGo();
$('#site-header .dropdown').doubleTapToGo();
init_topmenu_dimming();
// main navigation affix events
var affixTimeout = false;
var clearAffixTimeout = function () {
clearTimeout(affixTimeout);
affixTimeout = false;
};
var setAffixTimeout = function (callback, delay) {
clearAffixTimeout();
if (typeof callback == 'function' && typeof delay == 'number') {
affixTimeout = setTimeout(callback, delay);
}
};
$("#site-header").on('affixed.bs.affix', function () {
$("#site-header").addClass("isAffixed");
setAffixTimeout(function () {
$("#site-header").addClass("affixedIntermediate");
setAffixTimeout(function () {
$("#site-header").addClass("affixedResolved");
}, 100);
}, 100);
});
$("#site-header").on('affixed-top.bs.affix', function () {
$("#site-header").addClass("searchCollapsed");
$("#site-header").removeClass("affixedResolved");
setAffixTimeout(function () {
$("#site-header").removeClass("affixedIntermediate");
setAffixTimeout(function () {
$("#site-header").removeClass("isAffixed");
setAffixTimeout(function () {
$("#site-header").removeClass("searchCollapsed");
}, 100);
}, 100);
}, 100);
});
// try to work around fixed positioning/scrolling problem on iOS that
// causes the header to lose its affixed state when the search box
// is in focus.
$("#siteSearch").on('mouseenter touchstart', function () {
focus_site_search_box();
});
$("#siteSearch").on('mouseleave', function () {
if ($("#siteSearch_input").val() == "") {
blur_site_search_box();
}
});
$("#siteSearch_input").on('blur', function () {
blur_site_search_box();
});
$("#pageContent, #header-overlay").on('touchstart', function () {
blur_site_search_box();
$("#siteSearch_input").blur();
});
// handle search button clicks
// if search input field is not empty, submit the search form on button click.
$("#siteSearch .magnifyingGlassWrapper").on('click touchend', function(evt) {
if ($("#siteSearch_input").val() !== "") {
$('#siteSearch_form')[0].submit();
blur_site_search_box();
} else {
if (evt.type.toString().indexOf('touch') == -1) {
// if the search input field is empty, and we aren't using
// a touch device, focus on the search field.
focus_site_search_box();
$("#siteSearch_input").focus();
}
}
});
});
