<script type="text/javascript" src="calendar/lee-calendar/resources/scripts/date.js"></script>
<script type="text/javascript" src="calendar/lee-calendar/resources/scripts/datePicker.js"></script>

<script type="text/javascript">
jQuery(document).ready(function() {
	Date.firstDayOfWeek = 0;
	jQuery('#cal-datepicker')
		.datePicker({inline:true})
		.bind(
			'dateSelected',
			function(e, selectedDate, $td) {
				var urlday = selectedDate.getDate();
				var urlmonth = selectedDate.getMonth() + 1;
				var urlyear = selectedDate.getFullYear();
				var urldate = urlmonth + '%2F' + urlday + '%2F' + urlyear;
				location.href = '/calendar/search/?byDate=1&d1=' + urldate + '&d2=' + urldate + '&month=' + urlmonth + '&day=' + urlday + '&year=' + urlyear + '&l=50';
			}
		);
});
</script>

