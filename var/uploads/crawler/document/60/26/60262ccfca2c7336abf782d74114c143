            jQuery(document).ready(function($){
                
                
                $('#centerpiece-479306').tinycarousel({
                    pager: true,
                    
                    interval: true,
                    intervaltime: 15000,
                    
                    axis: 'x',
                    duration: 1250,
                    
                    callback: function(element, index){
                        
                        $('#centerpiece-479306 .pager li').removeClass('active');
                        $('#centerpiece-479306 .pager li').eq(index).addClass('active');
                        
