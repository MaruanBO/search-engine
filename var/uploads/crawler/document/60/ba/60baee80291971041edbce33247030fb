 function addLoadEvent(func)
 {
   var oldonload = window.onload;
   if (typeof window.onload != 'function')
   {
     window.onload = func;
   }
   else {
     window.onload = function() {  func(); }
      }
 }

 addLoadEvent(function() {
   setPluckCookie();
 });

 function setPluckCookie(){

   var cookieval = '';
   //cookieval = cookieval.replace(/&amp;/g, '%26');
   cookieval = "at=" + cookieval + ";expires;domain=.progress-energy.com;path=/";

   document.cookie = cookieval;

}

