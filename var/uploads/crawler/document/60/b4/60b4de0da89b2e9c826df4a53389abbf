			 			googletag.enableServices()
			
			     			googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                                var _AU= event.slot.getAdUnitPath().split("/")[3]; 
                                var _empty= event.isEmpty;
    							var _sz= event.size;

    							var _adUnitFactory =  new adUnitFactory();
    							var _adUnit = _adUnitFactory.create({
    								adUnitName: _AU,
    								adUnitSize: _sz
    							});

    							if (_adUnit){
        							if (_empty){
        								_adUnit.onEmptyCallBack();
        							}else{
        								_adUnit.onLaod();
        							}
    							}
                 });
			 
          });
	 
	  }catch(err){
		 var errorDFPMsg = err.message;
		}
		
