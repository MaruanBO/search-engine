	function videoBoxDisplayByAgent() {
				if (jQuery.browser.msie && parseInt(jQuery.browser.version) < 8){
			var viewport = document.getElementById("videoBoxViewport");
			if (viewport){
				viewport.style.overflow="scroll";
			}
		}
