	<script type="text/javascript">
    function closeAdUnitPlazma( ){
		 						
		
		  		
				
		try {
    		var iframe_Plazma = document.getElementById('themarker.com.web.plazma').children[1];
            iframe_Plazma.style.display = 'none';
            document.getElementById('themarker.com.web.plazma').id = 'themarker.com.web.leaderboard.top';
			            	googletag.defineUnit('/9401/themarker.com.web/themarker.com.web.leaderboard.top/themarker.com.web.leaderboard.top_homepage', [[970, 90]], 'themarker.com.web.leaderboard.top').addService(googletag.pubads().setTargeting('divid', 'themarker.com.web.leaderboard.top'));
						adUnit('themarker.com.web.leaderboard.top').show();
			showLBCTAfterPlazma();
						$('#Haim_L').hide();
            $('#Haim_R').hide();
            $('#Haim_Close_button').hide();
			                document.getElementById('themarker.com.web.leaderboard.top').style.height = '100%';
        		document.getElementById('themarker.com.web.leaderboard.top').style.width = 'auto';			
                document.getElementById('themarker.com.web.leaderboard.top').style.padding = '0px 0 5px 0';
        		document.getElementById('themarker.com.web.leaderboard.top').style.left = '0px';
			
			if (document.getElementsByClassName('topSectionBanners')!= null){
				document.getElementsByClassName('topSectionBanners')[0].removeAttribute("style");
			}
		} catch(err){
			var plazmaError = err.message;
		}	
    }
	
