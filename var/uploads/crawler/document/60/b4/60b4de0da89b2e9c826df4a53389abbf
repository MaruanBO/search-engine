	 <script type="text/javascript">
 var ytb = {};
 ytb.isIos = /(iPad|iPhone|iPod)/g.test( navigator.userAgent );
 
 
 
   //configure forms-ext features
webshim.setOptions('forms', {
					customDatalist:true
});
webshim.setOptions("forms-ext", {
	replaceUI: true,
	types: "range"
	
});

//load forms and forms-ext features
webshim.polyfill('forms forms-ext');

ytb.justInitialised=false;
ytb.params = { controls : 0 ,modestbranding :1,iv_load_policy:3,wmode:"transparent",playsinline:0,showinfo:0};
ytb.videoList  = new Object;
ytb.loadYoutubePlayer = function(){
	var head= document.getElementsByTagName('head')[0];
	var tag= document.createElement('script');
	tag.type= 'text/javascript';
	tag.src= 'https://www.youtube.com/iframe_api';
	head.appendChild(tag);
}
   
function onYouTubeIframeAPIReady() {
	   $(".video").each(function(){
	        var videoIdString = $(this).attr("id");
            ytb.videoList[videoIdString] = new YT.Player('player'+videoIdString, {
              height: '270',
              width: '480',
              videoId: videoIdString,
              events: {
                'onReady': createOnReadyfunction(videoIdString),
                'onStateChange': createOnPlayerStateChangefunction(videoIdString)
              },
    		  playerVars:ytb.params
            });
		});   
}	
	
		
	function createOnReadyfunction(videoId){
		return function (event) {
			if(!ytb.isIos){
			 	window.setInterval(function(){
        			$("#"+videoId+ " .elapsed").css("width",ytb.videoList[videoId].getCurrentTime()/ytb.videoList[videoId].getDuration()*100+'%');	    	
	 			},1000);	
			}else{
				ytb.videoList[videoId].cueVideobyId(videoId,0,"default");
			}
		}
	}
	
	function createOnPlayerStateChangefunction(videoId){
		return function (event) {
			if (event.data == YT.PlayerState.PLAYING )	{
				window.setInterval(function(){
					$("#"+videoId+ " .currentTime").text(ytb.secondsToHOursAndMinutes(ytb.videoList[videoId].getCurrentTime()));
				},1000);
				$("#"+videoId+ " .duration").text(ytb.secondsToHOursAndMinutes(ytb.videoList[videoId].getDuration()));
    			$("#"+videoId+ " .play").attr("data-icon","l");
    			$("#"+videoId+" .main-play").css("display","none");
				$("#"+videoId+" .videoImage").css("display","none");
			} else {
  				$("#"+videoId+ " .play").attr("data-icon","P");    		
  			}
		}
	}
	

ytb.loadYoutubePlayer();
	
$(".main-play,.play").click(function(){  
		var videoId = $(this).closest(".video").attr("id")
  		if(ytb.videoList[videoId]!=null && typeof ytb.videoList[videoId] !== 'undefined'){
            var status = ytb.videoList[videoId].getPlayerState();
            if(status==1){
              ytb.videoList[videoId].pauseVideo();             
            } else if(status==2 || status==5 || status==-1 || status==0){
               ytb.videoList[videoId].playVideo();               
            }
  	    }else {    		
  			ytb.loadYoutubePlayer();    		
  	   }
	}
	);


$(".mute").click(function(){
		var videoId = $(this).closest(".video").attr("id")
		if(ytb.videoList[videoId].isMuted()){
			ytb.videoList[videoId].unMute();
			$(this).attr("data-icon","o");
		} else {
			ytb.videoList[videoId].mute();
			$(this).attr("data-icon","s");
		}
	}
);


$(".fullscreen").click(function(){
	var videoId = $(this).closest(".video").attr("id")
    var fullelement = document.getElementById("ytplayer"+videoId);
	var requestFullScreen = fullelement.requestFullScreen || fullelement.mozRequestFullScreen || fullelement.webkitRequestFullScreen || fullelement.msRequestFullscreen;
    var exitFullscreen = document.exitFullscreen || document.mozCancelFullScreen || document.webkitCancelFullScreen || document.msExitFullscreen;
	if (requestFullScreen && $(this).attr("data-icon")=="!") {    			
    	requestFullScreen.bind(fullelement)();    
	}else if(exitFullscreen && $(this).attr("data-icon")=='"'){
  		exitFullscreen.apply(document);    	
  	}
});	

$(document).on('webkitfullscreenchange mozfullscreenchange fullscreenchange MSFullscreenChange',function(){
	if(document.fullscreenElement || document.webkitFullscreenElement || document.mozFullScreenElement || document.msFullscreenElement || document.webkitIsFullScreen){
  		$(".fullscreen").attr("data-icon",'"');
  	}else{
  		$(".fullscreen").attr("data-icon",'!');	
    }
});


ytb.secondsToHOursAndMinutes = function(seconds){
	var minutes;
	var remainedSeconds;
	var time;
	var hours = Math.floor(seconds/3600);
	if(hours>0){
		minutes = Math.floor((seconds-hours*3600)/60);
		if(minutes>0){
			remainedSeconds=seconds - hours*3600 - minutes*60;
			time=hours+":"+minutes+":"+Math.floor(remainedSeconds);
			return time;
		}else {
			time=hours+":"+minutes;
			return time;
		}
	}else{
		minutes = Math.floor(seconds/60);
		remainedSeconds = seconds - minutes*60;
		time=minutes+":"+Math.floor(remainedSeconds);
		return time;
	}
}	


$(".progressBar").click(function(e){
             // When a click occurs on the progress bar, seek to the
             // appropriate moment of the video.
			var videoId = $(this).closest(".video").attr("id")
			var progressBar=$(this);
			var elapsed=$(".elapsed",this);
            var ratio = (e.pageX-progressBar.offset().left)/progressBar.outerWidth(false);
            elapsed.width(ratio*100+'%');
            ytb.videoList[videoId].seekTo(Math.round(ytb.videoList[videoId].getDuration()*ratio), true);
            return false;
});		




$("input.volume-level").bind('input',function(e){
	var videoId = $(this).closest(".video").attr("id")
	ytb.videoList[videoId].setVolume($(this).prop("value"));
	$("#"+videoId+ " .mute").attr("data-icon","o");
});	


function showControl(element){
	if(!ytb.isIos){
 		$(".control-pannel",element).css("visibility","visible");
	}
}

function hideControl(element){
	 if(!ytb.isIos){
	 	$(".control-pannel",element).css("visibility","hidden");
	 }
}

if(ytb.isIos){
	$(".control-pannel").css("display","none");
	$(".main-play").css("display","none");
	$(".videoImage").css("display","none");
}

  </script>	 
