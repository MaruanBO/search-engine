<script>
$(document).ready(function (){
  
  $(".menu" ).click(function() {
  $('.hidden_div').toggleClass('opendiv');
  $('.warpper, .whole-doc').toggleClass('opendiv');
  $('.navbar-default').toggleClass('open_div');
  //$('.navbar-default, .warpper, .whole-doc').toggleClass('open_div');
  //$('.navbar-default, .whole-doc').animate({'left':'80%'},300);
  $('.footer_row').hide();
});

$('.right_arrow').click(function(){
  $(this).hide();
  $('.left_arrow').show();
  $('body').css('overflow-y', 'hidden');
  $('html').css('overflow', 'hidden');
});

$('.left_arrow').click(function(){
  $(this).hide();
  $('.right_arrow').show();
  $('body').css('overflow-y', 'auto');
  $('html').css('overflow', 'scroll');
});

});
</script>
