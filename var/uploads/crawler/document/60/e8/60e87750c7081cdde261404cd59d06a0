<script>
$(function() {
  $(document.body).on('click', '.search_span', function() {
  search('');
});
  var kp = false;
  var History = window.History;
  History.Adapter.bind(window,'statechange',function() { // Note: We are using statechange instead of popstate
    $('.ui-autocomplete').hide();
          // Log the State
          if(document.URL.indexOf('search') > 0) 
          {
            var term = document.URL.substring((''+document.URL).lastIndexOf('/')+1);
            // term = decodeURI(term);
            
            if(term !== null && term !== '')
            {
              if(!kp)
              search(term);
              var f = new RegExp('-', 'g');
              
              $('#tags').val(term.replace(f, ' '));
              document.title = term.replace(f, ' ')+" | Evrystry";
            }
          }
          else if(document.URL === 'https://www.wittyfeed.com/')
          {
            $('#tags').val('');
            $('.ui-autocomplete .ui-front .ui-menu').hide();
            $.ajax({
                      url: "https://www.wittyfeed.com/load_stories",
                      type: "POST",
                      success: function(data) {
                        $('#div_main_content').html(data);
                        
                      },
                      error: function(data) {
                        console.log('error');
                      }
                    });
          }
  });
