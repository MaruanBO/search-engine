    getTextWidth = function(text) {
    // re-use canvas object for better performance
    var canvas = getTextWidth.canvas || (getTextWidth.canvas = document.createElement("canvas"));
    var context = canvas.getContext("2d");
    context.font = '16px  Verdana, Arial, sans-serif';
    var metrics = context.measureText(text);
    return metrics.width;
};
var slug = '';
function trim_title(d, w) 
{
        
        var data = d;
//    $('#div_t').html('<div id="trim_txt" style="width: auto; display:none; positon: absolute; font-family: \'Roboto\', sans-serif;  font-size: \'14px\'; >'+data+'</div>');
        while(getTextWidth(data) > w) 
        {
//            console.log($('#div_t').width());
            data = data.substring(0, data.length - 3);
            
//            $('#div_t')
//                    .html('<div id="trim_txt" style="width: auto; display:none; positon: absolute; font-family: \'Roboto\', sans-serif;  font-size: \'14px\'; >'
//                    +data+'</div>');
//        console.log(''+data);
        }
        data = data.substring(0, data.lastIndexOf(' '));
        return data;
}
    
    
    function load_story_box_view(story)
    {
        
        var story_box = document.createElement('div');
        $(story_box).addClass('col-lg-4 col-md-6 col-sm-6 col-xs-12 home-gallery'); // outer div
        
        
        var story_section = document.createElement('section'); // section
        $(story_section).addClass('clearfix section');
        $(story_box).append(story_section);
        
        var gal_div = document.createElement('div');
        $(gal_div).addClass('gallery_image_section');
        $(gal_div).css('position', 'relative');
        $(story_section).append(gal_div);
        
        var gal_link = document.createElement('a');
        $(gal_link).attr('href', '#'); // TODO: Generate story link
        
        $(gal_div).append(gal_link);
        
        var link_img = document.createElement('img');
        $(link_img).attr('src', 'https://'+'my-tss'
          +'.s3.amazonaws.com/'+story['story_id']+"/400/"+story['cover_image']);
        
        $(gal_link).append(link_img);
        
        if (story['cover_image'].indexOf(".") < 0)
        {
          var video_span = document.createElement('span');
          $(video_span).css('position', 'absolute');
          $(video_span).css('top', '37%');
          $(video_span).css('left', '43%');
          $(video_span).css('font-size', '400%');
          $(gal_link).append(video_span);
          var icon = document.createElement('i');
          $(icon).addClass('position', 'fa fa-youtube-play');
          $(video_span).append(icon);
        }
        
        var div_share_widget = document.createElement('span');
        $(div_share_widget).addClass('share_widget');
        $(div_share_widget).css('display', 'none');
        $(div_share_widget).css('height', '40px');
        $(div_share_widget).css('background', '#fff');
        $(div_share_widget).css('z-index', '999');
        $(div_share_widget).css('position', 'absolute');
        $(div_share_widget).css('bottom', '0px');
        $(div_share_widget).css('right', '0px');
        $(div_share_widget).css('width', '215px');
        $(div_share_widget).css('border-top-left-radius', '4px');
        $(div_share_widget).css('border-top-right-radius', '4px');
        $(gal_div).append(div_share_widget);
        
        var ul_share_widget = document.createElement('ul');
        $(ul_share_widget).addClass('list-unstyled list-inline');
        $(div_share_widget).append(ul_share_widget);
        
        var fb_li = document.createElement('li');
        $(ul_share_widget).append(fb_li);
        var fb_link = document.createElement('a');
        $(fb_link).attr('href', 'http://www.facebook.com/sharer.php?u=10001&v=1'); // TODO: FIX 10001 with share_link
        $(fb_link).html('<i class="fa fa-facebook-square" style="color:#3b5998;"></i> Share ');
        $(fb_li).append(fb_link);
        
        var twitter_li = document.createElement('li');
        $(ul_share_widget).append(twitter_li);
        var twitter_link = document.createElement('a');
        $(twitter_link).attr('href', 'http://twitter.com/home?status=10001+20002'); // TODO: FIX 10001 with share_title 20002 with share link
        $(twitter_link).html('<i class="fa fa-twitter-square" style="color:#9AE4E8"></i> Tweet');
        $(twitter_li).append(twitter_link);
        
        var gplus_li = document.createElement('li');
        $(ul_share_widget).append(gplus_li);
        var gplus_link = document.createElement('a');
        $(gplus_link).attr('href', 'https://plus.google.com/share?url='); // TODO: FIX 10001 with share_link
        $(gplus_link).html('<i class="fa fa-google-plus-square" style="color:#d34836"></i> Share ');
        $(gplus_li).append(gplus_link);
        
        var story_title_h2 = document.createElement('h2');
        $(story_title_h2).addClass('story_title');
        
        var story_title_link = document.createElement('a');
        $(story_title_link).attr('href', '#'); // TODO: FIX # with story_link
        $(story_title_link).html(story['story_title']);
        $(story_title_h2).append(story_title_link);
        
        $(story_section).append(story_title_h2);
        
        var story_desc_p = document.createElement('p');
        $(story_desc_p).addClass('story_description');
        
        var story_desc_link = document.createElement('a');
        $(story_desc_link).attr('href', '#'); // TODO: FIX # with story_link
        $(story_desc_link).html(story['story_desc'].replace(/(<([^>]+)>)/ig,""));
        $(story_desc_p).append(story_desc_link);
        
        $(story_section).append(story_desc_p);
        
        
        var footer = document.createElement('footer');
        $(story_section).append(footer);
        
        
        var icons_div = document.createElement('div');
        $(icons_div).addClass('home_meta');
        $(icons_div).css('padding-left', '12px');
        $(footer).append(icons_div);
        
        var footer_ul = document.createElement('ul');
        $(footer_ul).addClass('list-unstyled list-inline');
        $(icons_div).append(footer_ul);
        
        var footer_view_count_li = document.createElement('li');
        $(footer_view_count_li).html('<i class="fa fa-eye img-circle eyes_icon"></i> <span>'+story['view_count']+'</span>'); //TODO: integer to roman port javascript
        $(footer_ul).append(footer_view_count_li);
        
        var footer_image_number_li = document.createElement('li');
        $(footer_image_number_li).addClass('img_number');
        $(footer_image_number_li).html('<i class="fa fa-picture-o img-circle eyes_icon"></i> <span>'+'</span>'); //TODO: get total media port javascript
        $(footer_ul).append(footer_image_number_li);
        
        var footer_gal_category_li = document.createElement('li');
        $(footer_gal_category_li).addClass('gallery_name pull-right');
        $(footer_gal_category_li).html('<i class="fa fa-bookmark"></i> '+
                                        '<span> <a href="https://www.wittyfeed.com/'
                                        +'category/'+story['cat_id']+'">'
                                        +story['cat_name']+'</a>');
        
        $(footer_ul).append(footer_gal_category_li);
        
        $('#div_main_content').append(story_box);
    }
    
    
    function search(term)
    {
      // console.log('searchin ');
      $('#div_main_content').html();
      
          if(term === '')
          {
            term = $('#tags').val();
          }
      // return function () {
        var f = new RegExp(' ', 'g');
        
        term = term.replace(f, '-');
        term = term.replace(/[\s_]+/g, '');
        History.pushState(null, null, "https://www.wittyfeed.com/search/"+term);
        $.ajax({
                  url: 'https://www.wittyfeed.com/search/'+term,
                  type: "POST",
                  
                  success: function(data) {
                    $('#div_main_content').html(data);
                    
                  },
                  error: function(data) {
                    console.log('error');
                  },
                  
                  beforeSend: function() {
                      if ($("#loadingbar").length === 0) {
                        $("body").append("<div id='loadingbar'></div>")
                        $("#loadingbar").addClass("waiting").append($("<dt/><dd/>"));
                        
                             $("#loadingbar").width((50 + Math.random() * 30) + "%");
                            
                       
                      }
                  }
        }).always(function() {
           $("#loadingbar").width("101%").delay(200).fadeOut(400, function() {
           $(this).remove();
        });
         });
      // }();
    }
    
    $("#tags").keyup(function(event){
      kp = false;
    if(event.keyCode == 13)
    {
      kp = true;
      // console.log('sak,');
        search('');
    }
});
    
        $("#tags").autocomplete({
          
        source: function(request, response) {
                $.ajax({
                  url: 'https://www.wittyfeed.com/show_autocomplete_text/'+request.term,
                  
                  success: function(data) {
                    
                      response($.map(data, function(obj) {
                          // console.log(obj);
                          
                    return {
                        label: obj.label,
                        value: obj.value,
                        id: obj.id // don't really need this unless you're using it elsewhere.
                    };
                }));
                  }
                });
            },
        minLength: 2,
        // select: function(event, ui) {
        //     var url = ui.item.id;
        //     if(url != '#') {
        //         location.href = 'https://www.wittyfeed.com/'+url + '/'+ui.item.label;
        //     }
        // },
//      // optional (if other layers overlap autocomplete list)
        open: function(event, ui) {
            $(".ui-autocomplete").css("z-index", 1000);
        }
    }).data( "ui-autocomplete" )._renderItem = function(ul, item) {
  var v = $( "<li>" )
    .data( "ui-autocomplete-item", item )
    // .append( "<a>" + trim_title(item.value, $('#tags').width()) + "</a>" )
    .append( '<span class="search_span">' + item.value + "</span>" )
    .appendTo( ul );
  return v;
}
//// $( "#topic_title" ).autocomplete({  });
//});
});


</script>
