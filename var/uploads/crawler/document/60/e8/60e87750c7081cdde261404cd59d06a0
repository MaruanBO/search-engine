 <script>
 	 
var rowslimit=12;
var rowsdisplayed=18;
var has_galleries=true;
var under_process=false;
function load_more_galleries(cat_id)
 {
	 if(under_process==false)
	 {
		 under_process=true;
	 if(has_galleries)
	 {
	$('#div_load_more').html("<strong>Loading....</strong><img src='https://www.wittyfeed.com/assets/image/loading.gif'  />");
		 
	 method="POST";
	 ajax_url="https://www.wittyfeed.com/story/load_more_stories/"+rowsdisplayed+"/"+cat_id;
	 send_data="";
	 receive_data=null;
	 receive_data=ajax_call(method,ajax_url,send_data,true);
	 $retval='';
		$.ajax({
			type: method,
			url: ajax_url,
			data: send_data,
			async: true,
			success: function(retdata){
			receive_data=retdata;
				 $('#div_main_content').append(receive_data);
					if(receive_data.length<=3)
					{
						has_galleries=false;
						 $('#div_load_more').html("<strong>No More Galleries</strong>");
					}
					else
					{
						 $('#div_load_more').html("<strong>Load More Galleries</strong>");
					}
					 rowsdisplayed=rowsdisplayed+rowslimit;
					 under_process=false;
				
			}
		});
	 }
   }
 }

 </script>
