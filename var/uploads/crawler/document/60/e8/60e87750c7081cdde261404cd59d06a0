   <script>

   $(".navbar-header button").click(function(){ $(".navbar-default").toggleClass('navbar-fixed-top'); });
   $(window).scroll(function() {
//	   alert('window height ='+$(window).height() +",document height = "+$(document).height());
	 if($(window).scrollTop() + $(window).height() > $(document).height() - 100) 
     { 
	    	   $('#slidebox').animate({'right':'0px'},300);
	 }
		  else
	 { 
			   $('#slidebox').stop(true).animate({'right':'-430px'},50);
	 } 
   $('#slidebox .close').bind('click',function(){ $(this).parent().remove(); }); });
   </script>	


   
