                          '&lease=' +
                          '&series=';

    
    /**
     * Set the url of the iframe after the page is loaded
     **/
    set_js_iframe_url = function () {
        // set 'jsiframe' iFrame src; not using Com anymore
        document.getElementById('jsiframe').src = js_iframe_url;
    };

    // Add onLoad event callback for jsiframe
    if (typeof(Com) !== 'undefined') {
        Com.addEvent(window, 'load', set_js_iframe_url);
    }
    else if (typeof(MOBILE) === 'object') {
        $(document).bind("pageinit", set_js_iframe_url);
    }

    
