<script>
	angular.module('theSimsApp')
		.constant('endpoints', {
			authState: '/auth_state',
			gallery: {
				acceptTos: '/api/gallery/v1/users/authenticated-user/entitlements/tos',
				athena: '//athena.thesims.com/v1/images/:itemType/:hexFolder/:guid/:imageIndex.jpg',
				athenaHost: '//athena.thesims.com',
				addComment: '/api/gallery/v1/sims/:simUuid/comments',
				deleteComment: '/api/gallery/v1/sims/:simUuid/comments/:commentUuid',
				flagComment: '/api/gallery/v1/sims/:simGuid/comments/:commentUuid/flags',
				followingFeed: '/api/gallery/v1/users/authenticated-user/following-feed',
				getBio: '/api/gallery/v1/users/:nucleusId/bio',
				getComments: '/api/gallery/v1/sims/:simUuid/comments',
				getFlags: '/api/gallery/v1/users/authenticated-user/flags',
				getSim: '/api/gallery/v1/sims/:uuid',
				hud: '/api/gallery/v1/users/authenticated-user/hud',
				search: '/api/gallery/v1/sims/search',
				submitFlag: '/api/gallery/v1/sims/:guid/flags',
