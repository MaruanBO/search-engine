                                                                                    <script type="text/javascript">
                                                                                    
                                                                                    var icpForm730 = document.getElementById('icpsignup730');
                                                                                    
                                                                                    if (document.location.protocol === "https:")
                                                                                    
                                                                                        icpForm730.action = "https://app.icontact.com/icp/signup.php";
                                                                                    function verifyRequired730() {
                                                                                      if (icpForm730["fields_email"].value == "") {
                                                                                        icpForm730["fields_email"].focus();
                                                                                        alert("The Email field is required.");
                                                                                        return false;
                                                                                      }
                                                                                    
                                                                                    
                                                                                    return true;
                                                                                    }
