    <script type="text/javascript">
        (function(){
            function toggleGlobalNav(){
                if(document.getElementById("jvlGlobalNav").className == 'mobile-active'){
                    document.getElementById("jvlGlobalNav").className = "";
                }
                else{
                    document.getElementById("jvlGlobalNav").className = "mobile-active";
                }
            }

            function toggleClass(target,className){
                if(target.className == className){
                    target.className = "";
                }
                else{
                    target.className = className;
                }
            };

            var el = document.getElementById('jvlGlobalNav');
            if (el.addEventListener) {
                el.addEventListener('mousedown', toggleGlobalNav, false);
            } else if (el.attachEvent)  {
                el.attachEvent('mousedown', toggleGlobalNav);
            }

            var dropperMenu = document.getElementById('jvlGlobalTopDropper');
            if (dropperMenu.addEventListener) {
                dropperMenu.addEventListener('mouseenter', function(){toggleClass(dropperMenu,'hover');}, false);
                dropperMenu.addEventListener('mouseleave', function(){toggleClass(dropperMenu,'hover');}, false);
            } else if (el.attachEvent)  {
                dropperMenu.attachEvent('onmouseenter', function(){toggleClass(dropperMenu,'hover');});
                dropperMenu.attachEvent('onmouseleave', function(){toggleClass(dropperMenu,'hover');});
            }


        })();
    </script>
