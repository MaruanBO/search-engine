	  google.load("feeds", "1");
	  function initialize_hn() {
		var feed = new google.feeds.Feed("http://blog.triblive.com/newsfeed");
		feed.setNumEntries(8);
		feed.load(function(result) {
			var f_area = 'hn';
		  if (!result.error) {
			var container = document.getElementById("feed_hn");

			if( f_area == 'hn' ) {
				var html = '<h4 class="section-topper"><a href="http://blog.triblive.com/" style=""><span class="rssBlogHeading">Trib Blogs </span></a></h4><ul class="rssReader">';
			} else {
				var html = '<h4 class="section-topper"><a href="http://blog.triblive.com/" style=""><span class="rssBlogHeading">Trib Blogs Blog</span></a></h4><ul class="rssReader">';
			}

			for (var i = 0; i < result.feed.entries.length; i++) {
			  var entry = result.feed.entries[i];
			  html += '<li class="headinglinkrss"><a href="'+entry.link+'">'+entry.title+'</a><br /> '+entry.contentSnippet+'</li>';
			}
			html += '</ul>';
			if( f_area == 'hn' ) {
				//html += '<a href="http://blog.triblive.com/thisjustin/">More This Just In</a><br />';
				//html += '<a href="http://blog.triblive.com/trib-list/">More Trib List</a><br />';
			}				
			container.innerHTML = html; 
		  } else {
			  console.log( result.error.message );
		  }
		});
	  }
	  google.setOnLoadCallback(initialize_hn);
   </script>
