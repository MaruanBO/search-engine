<div style="width:280px; float:left;">
	
	
	<script type="text/javascript">
var ttmEvenOdd = "tribliveEventEven"

function formatMonth(month){
	var m_names = new Array("Jan", "Feb", "Mar", 
	"Apr", "May", "Jun", "Jul", "Aug", "Sep", 
	"Oct", "Nov", "Dec");
	return m_names[month];	
}
function formatDay(day){
	var d_names = new Array("","1st","2nd","3rd","4th","5th","6th","7th","8th","9th",
	"10th","11th","12th","13th","14th","15th","16th", "17th", "18th","19th",
	"20th","21st","22nd","23rd","24th","25th","26th","27th","28th","29th",
	"30th","31st");
	
	return d_names[day];
}
function formatDOW(day){
	var d_dow = new Array("Sun","Mon","Tue","Wed","Thu","Fri","Sat");
	return d_dow[day];
}
function formatYYYYMMDD(date) {
   var yyyy = date.getFullYear().toString();
   var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
   var dd  = date.getDate().toString();
   return yyyy +"-"+ (mm[1]?mm:"0"+mm[0]) +"-"+ (dd[1]?dd:"0"+dd[0]); // padding
  };

var days = [];
/* Build Dates Boxes */
var useDate = new Date();
var curMonth = "";
var usedMonth = "";
var addMonth = "";
var datesUsed = [];
for (i = 0; i <7; i++) {
	var useDate = new Date();
  	useDate.setDate(useDate.getDate()+i); 
	var sDate = (useDate.getMonth()+1) + "/" + useDate.getDate() + "/" + useDate.getFullYear();
	var dow = useDate.getDay(); 
	var dayClass = "";
	if (i == 0){dayClass="today";}
	if (dow==0 || dow==6){
			if (dayClass == "today"){ dayClass = dayClass+" ";}
			dayClass = dayClass+"weekend";
	}

	curMonth = formatMonth(useDate.getMonth());
	if (usedMonth!=curMonth){ addMonth = "<span>"+curMonth+"</span>"; usedMonth=curMonth;} else { addMonth = "";}
  	days.push( "<li  id=\"date"+i+"\" class="+dayClass+">"+addMonth+"<a  href='#http://calendar.triblive.com/events?search=&date="+sDate+"&commit=Search&st_select=any'>" +  formatDay(useDate.getDate()) + " " + formatDOW(useDate.getDay()) + "</a></li>" ); 
	datesUsed.push(useDate);

}
days.push("<li id='allEvents'><a href=\"http://calendar.triblive.com/\">All Events</a>");

	$(document).on('click', '#date0', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[0]);
		//alert('you clicked li#date0 element for date: '+ datesUsed[0]);
	});
	$(document).on('click', '#date1', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[1]);
		//alert('you clicked li#date1 element for date: ' + datesUsed[1]);
	});
	$(document).on('click', '#date2', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[2]);
		//alert('you clicked li#date2 element for date: ' + datesUsed[2]);
	});
	$(document).on('click', '#date3', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[3]);
		//alert('you clicked li#date3 element for date: ' + datesUsed[3]);
	});
	$(document).on('click', '#date4', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[4]);
		//alert('you clicked li#date4 element for date: ' + datesUsed[4]);
	});

	$(document).on('click', '#date5', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[5]);
		//alert('you clicked li#date5 element for date: ' + datesUsed[5]);
	});
	$(document).on('click', '#date6', function(e){
		e.preventDefault();
		ajaxLoad(datesUsed[6]);
		//alert('you clicked li#date6 element for date: ' + datesUsed[6]);
	});


function ajaxLoad(d){

	$( "#tribliveEvents" ).empty();
	
		var pointslocal = "https://secure.pointslocal.com/api.json?method=events.get&partner=triblive.pointslocal.com";
		var newitems = [];
		$.getJSON( pointslocal, {
		text: "",
		startdate: formatYYYYMMDD(d),
		days:1,
		count: 15
		
		}
		)
		.done(
			function( data ) {
				$.each( data.items, function( i, item ) {
							
					var eid = item.counter;
					var eTitle = item.event_title;
					var eTitleLen = eTitle.length;   
						if (eTitleLen > 33){
							eTitle = eTitle.substr(0, 33)+"..."; 
						}
						if (eTitleLen == 0){
							return true;
						}
					var eTime = item.event_date +" "+item.event_start_time;
					var eLoc = item.event_venue_address;
					var eImage = item.event_image;
					var eventDetails = "";
					if (eImage != ""){
						var eventDetails = '<a href="http://calendar.triblive.com/event/'+ item.guid +'">'+"<img src='"+eImage+"&context=event.image' />"+'</a>';
					}
					
					var eventDetails = eventDetails +'<h3><a href="http://calendar.triblive.com/event/'+ item.guid +'">' + eTitle + '</a></h3><p><a href="http://calendar.triblive.com/event/'+ item.guid +'">'+eTime+'</a></p>';
				
					if (ttmEvenOdd == "tribliveEventOdd"){ ttmEvenOdd= "tribliveEventEven";} else { ttmEvenOdd = "tribliveEventOdd";}
				
					newitems.push( "<li id='" + eid + "' class='"+ttmEvenOdd+"'>" + eventDetails + "<div class='clear'></div></li>" );

				})
				
		$( "<ul/>", {
		"class": "tribliveEvents",
		html: newitems.join( "" )
		}).appendTo( "#tribliveEvents" );
				
			});
		
				
}




$(document).ready(function() {

	$( "<ul/>", {
"class": "eventDates",
html: days.join( "" )
}).appendTo( "#tribliveEventsDates" );
});

var pointslocal = "https://secure.pointslocal.com/api.json?method=events.get&partner=triblive.pointslocal.com";
var items = [];
var useDate = new Date();
$.getJSON( pointslocal, {
days: 14,
count: 15,
when: "today"

})
.done(function( data ) {

$.each( data.items, function( i, item ) {
	var eid = item.counter;
	var eTitle = item.event_title;
	var eTitleLen = eTitle.length;   
		if (eTitleLen > 33){
			eTitle = eTitle.substr(0, 33)+"..."; 
		}
		if (eTitleLen == 0){
			return true;
		}
	var eTime = item.event_date +" "+item.event_start_time;
	var eLoc = item.event_venue_address;
	var eImage = item.event_image;
	var eventDetails = "";
	if (eImage != ""){
		var eventDetails = '<a href="http://calendar.triblive.com/event/'+ item.guid +'">'+"<img src='"+eImage+"&context=event.image' />"+'</a>';
	}
	
	var eventDetails = eventDetails +'<h3><a href="http://calendar.triblive.com/event/'+ item.guid +'">' + eTitle + '</a></h3><p><a href="http://calendar.triblive.com/event/'+ item.guid +'">'+eTime+'</a></p>';

	if (ttmEvenOdd == "tribliveEventOdd"){ ttmEvenOdd= "tribliveEventEven";} else { ttmEvenOdd = "tribliveEventOdd";}

	items.push( "<li id='" + eid + "' class='"+ttmEvenOdd+"'>" + eventDetails + "<div class='clear'></div></li>" );

});
$( "#tribliveEvents" ).empty();
$( "<ul/>", {
"class": "tribliveEvents",
html: items.join( "" )
}).appendTo( "#tribliveEvents" );

});

</script>

