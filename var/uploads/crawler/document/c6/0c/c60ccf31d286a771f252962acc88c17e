<script language="JavaScript" type="text/javascript">
  // flow selection (eg. Tire, Wheel, Cart)
  s.eVar3='Home';
  s.prop2=s.eVar3;
  s.prop3='DTD Home Page';
  jspFileName='homeContent.jsp';
  try {
	// s.prop1, s.prop2, s.prop3 and jspFileName must be defined in the including jsp 
	// before inclusion. Otherwise you'll get "undefined" messages in your s.pageName

	// product type (Tire, Wheel)
	var productTypeName = ''
	productTypeName = 'Home'

	// if manufacturer landing
	var manufacturer = ''
	manufacturer = ''
	var isManufacturerExists = (manufacturer != '')

	var vehicle = ''
	vehicle = ''
	s.evar19 = ''
	var isVehicleExists = (vehicle != '')
	if (isVehicleExists) {
		// Replace Soft-Coded Double Quotes (Example: 18")
		vehicle = vehicle.replace(/&#034;/g, '" ')
		s.evar19 = vehicle
	}

	var size = ''
	size = ''
	s.evar18 = ''
	var isSizeExists = (size != '')
	if (isSizeExists) {
		size = size.replace('&quot;', '"')
		size = size.replace('&amp;quot;', '"')
		s.evar18 = size
	}

	s.eVar4 = ''
	if (isVehicleExists) {
		s.eVar4 = s.evar19
	}

	if (isSizeExists) {
		if (s.eVar4 != '') {
			s.eVar4 += ': '
		}
		s.eVar4 += s.evar18
	}

	if (isManufacturerExists) {
		if (s.eVar4 != '') {
			s.eVar4 += ': '
		}
		s.eVar4 += manufacturer
	}

	// flow selection (eg. Tire, Wheel, Cart)
	s.prop1 = 'Home'

	var isSProp1Exists = (s.prop1 != '')
	var isSProp2Exists = (s.prop2 != '')
	var isSProp3Exists = (s.prop3 != '')
	var isJspFileNameExists = (jspFileName != '')
	if (!isSProp1Exists) {
		s.prop1 = "Site Section Not Set"
	}
	if (!isSProp2Exists) {
		s.prop2 = "Product Finding Method not set"
	}
	if (!isSProp3Exists) {
		s.prop3 = "Page Type Not Set"
	}
	if (!isJspFileNameExists) {
		jspFileName = "jspFileName Not Set"
	}

	if (s.eVar4 != '') {
		s.prop2 += ': ' + s.eVar4
	}

	var model = ''
	model = ''
	var isModelExists = (model != '')
	if (isModelExists) {
		s.prop2 += ': ' + model
	}

	s.pageName = (s.prop1 + ": " + s.prop2 + ": " + s.prop3 + ": " + jspFileName)

	// alert("s.pageName=["+s.pageName+"]");
} catch (err) {
	// alert('in catch')
	s.pageName = ("error on page: " + err.name + " = " + err.message)
	// alert(s.pageName)
}
s.prop4 = 'DTD'

s.pageName = capitalizeMe(s.pageName)

function capitalizeMe(obj) {
	var val = obj;
	newVal = '';
	val = val.split(' ');

	for (var c = 0; c < val.length; c++) {
		// find the first alpha char in the string
		for(var i = 0; i < val[c].length && !/[A-Za-z]/.test(val[c].charAt(i)); i++);
			newVal += val[c].substring(0, i) + 
					  val[c].substring(i, i + 1).toUpperCase() + 
					  val[c].substring(i + 1, val[c].length);
		// add a space on all but the last time around in the loop
		if (c < val.length - 1) {
			newVal += ' '
		}
	}

	return newVal;
}
