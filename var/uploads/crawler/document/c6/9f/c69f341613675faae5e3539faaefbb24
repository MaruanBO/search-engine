<script type="text/javascript" src="http://j.slimg.com/js/lib/stm/builds/desktop/2014-06-13.min.js"></script><script type="text/javascript" src="//cdn.optimizely.com/js/655691106.js"></script><script type="text/javascript" src="http://j.slimg.com/js/AWD/stm/include.js?nano=1&v=20150219205507"></script><script type="text/javascript">
		function register_fares_by_ip(popup_id, trigger_id, form_id, div_id) {
			AWD.ToolTip.register(popup_id, {
				event: 'click',
				display: popup_id,
				trigger: trigger_id,
				clicktoclose: false,
				offsets: {top: 20, left: -35}
			});

			// get the city id
			var form = $(form_id);
			if(!form) {
				return;
			}
			var elems = form.select('.city');
			var city = elems[0];
			var city_id = city.id;

			form.observe('submit', function(event) {
				event.stop();

				// wait to submit until LS is done doing what it does
				if (!$(city_id).hasClassName('LSValid')) {
					return false;
				}

				var elem = event.element();
				var params = elem.serialize();

				AWD.ToolTip.hide(popup_id);
				if(!$(city_id).getValue()) {
					return false;
				}

				params += '&mode=ajax';
				new Ajax.Request('/home/', {
					parameters: params,
					method: 'get',
					onSuccess: function(response) {
						$(div_id).replace(response.responseText);
						register_fares_by_ip('hp_todays_fares_popup', 'hp_todays_fares_popup_trigger', 'hp_todays_fares_form', 'hp_todays_fares');
					}
				});

				return true;
			});

			new AWDLocationSuggest(city_id, ['airport'], '', 'departure');
		}

		register_fares_by_ip('hp_todays_fares_popup', 'hp_todays_fares_popup_trigger', 'hp_todays_fares_form', 'hp_todays_fares');
</script><script type="text/javascript" src="/js/AWD.Strings.php??nano=1&v=20150219205507"></script><script type="text/javascript">AWD.User.init();</script><script type="text/javascript">BookingBuddy.queryString = {};</script><script type="text/javascript">	AWD.init();</script><script type="text/javascript">window.awd_user_subs = window.jQuery.parseJSON(unescape('%5B%5D'));</script><script type="text/javascript">	AWD.ToolTip.register('fare_alerts_popup', {
