$(".menu_tab").click(function() {
	  $(this).addClass("current").siblings().removeClass("current");
	  var index = $(this).index();
		var z_index_temp = 0;
		var z_index = 0; 
	  for(var jj=0;jj<4;jj++) { z_index_temp = parseInt($("#blocks"+jj).css("zIndex")); z_index = (z_index_temp>=z_index)?z_index_temp:z_index; }
		 z_index_n= parseInt(z_index)+1;  
		$("#blocks"+index).css("zIndex",z_index_n);
});
  
</script><!-- slide end -->
