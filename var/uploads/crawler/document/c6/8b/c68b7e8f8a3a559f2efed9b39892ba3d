<script>
  $(function() {    
    var cache = {};
    $( "#tags" ).autocomplete({
      position: { my : "left+12 top", at: "left bottom" },
      minLength: 2,
      source: function(request,response){
        // hit a json source, caching results
        if(request.term in cache) {
          response($.map(cache[request.term], function(item){
            return { value: item.value, id: item.id}
          }));
          return;
        }
        $.ajax({
          url: "/autocomplete/autocomplete_query_new.php",          
          data: "term=" + request.term,
          dataType: "json",
          type: "GET",
          contentType: "application/json; charset=utf-8",
          dataFilter: function(data) {return data;},
          success: function(data) {                              
            cache[request.term] = data;
            response($.map(data, function(item) {
              return {
                value: item.value,
                id: item.id
              }
            }))
          },
          error: function() {
            if ( window.console && window.console.log ) {
              // console is available
              console.log('problem with autocomplete ajax call!');
              }
            }
        });
      }
    });
  });
