<script type="text/javascript">
	//
	// JS for main navigation and search bar
	//
	jQuery(window).ready(function(){

		// set active tab
		var cur_url = document.location.href;
        jQuery('navigation ul#menu > li').each(function() {
            switch(jQuery(this).attr('id'))
            {
                case "mindhut":
                if(cur_url.indexOf('http://www.sparknotes.com/mindhut') === 0) { // match http://www.sparknotes.com/mindhut
                    jQuery(this).addClass('selected');
                }
                break;
                case "sparklife":
                if(cur_url.indexOf('http://community.sparknotes.com/') === 0) { // match http://community.sparknotes.com/
                    jQuery(this).addClass('selected');
                }
                break;
                case "video":
                if(cur_url.indexOf('http://www.sparknotes.com/video/') === 0 || cur_url.indexOf('http://www.sparknotes.com/sparknotes/video/') === 0 ) { // match http://www.sparknotes.com/video/
                    jQuery(this).addClass('selected');
                }
                break;
                case "test-prep":
                if(cur_url.indexOf('http://www.sparknotes.com/testprep/') === 0) { // match http://www.sparknotes.com/testprep/
                    jQuery(this).addClass('selected');
                }
                break;
                case "sparknotes-tab":
                if(cur_url.indexOf('http://www.sparknotes.com/lit/') === 0 || cur_url == 'http://www.sparknotes.com/sparknotes/') { // match http://www.sparknotes.com/sparknotes
                    jQuery(this).addClass('selected');
                }
                break;
                case "no-fear":
                if(cur_url.indexOf('http://nfs.sparknotes.com') === 0) { // match http://nfs.sparknotes.com/
                    jQuery(this).addClass('selected');
                }
                break;
            }
        });
        var dropdown_loaded1 = false;
        jQuery("#no-fear").mouseover(function(){
            //console.log("http://www.sparknotes.com/featured/nofear"); 
            //console.log('/featured/nofear');
            var nfurl =  "/featured/nofear";    
            nfurl = "/featured/nofear";   
            if(!dropdown_loaded1)
            {
                jQuery.get(nfurl, function(data){                                                                        
                    jQuery('.dropdown-no-fear').html(data);
                    dropdown_loaded1 = true;
                });
            }
        });
   
        var dropdown_loaded2 = false;
        jQuery("#video").mouseover(function(){
            //console.log("http://www.sparknotes.com/featured/video2013"); 
            //console.log('/featured/video2013');
            var vurl =  "/featured/video2013";    
            vurl = "/featured/video2013";   
            if(!dropdown_loaded2)
            {
                jQuery.get(vurl, function(data){
                    jQuery('.dropdown-video').html(data);
                    dropdown_loaded2 = true;
                });
            }
        });
        var dropdown_loaded3 = false;
        jQuery("#sparklife").mouseover(function(){
            //console.log("http://www.sparknotes.com/featured/sparklife"); 
            //console.log('/featured/sparklife');
            var surl =  "/featured/sparklife";    
            surl = "/featured/sparklife"; 
            if(!dropdown_loaded3)
            {
                jQuery.get(surl, function(data){
                    jQuery('.dropdown-sparklife').html(data);
                    dropdown_loaded3 = true;
                });
            }
        });
        var dropdown_loaded4 = false;
        jQuery("#mindhut").mouseover(function(){
            //console.log("http://www.sparknotes.com/featured/mindhut"); 
            //console.log('/featured/mindhut');
            var murl =  "/featured/mindhut";    
            murl = "/featured/mindhut"; 
            if(!dropdown_loaded4)
            {
                jQuery.get(murl, function(data){
                    jQuery('.dropdown-mindhut').html(data);
                    dropdown_loaded4 = true;
                });
            }
        });
        
        var cache2 = {};
        jQuery( "#search-sparknotes" ).autocomplete({
          position: { my : "left+10 top", at: "left bottom" },
          appendTo: ".small-search",
          minLength: 2,
          source: function(request,response){
            // hit a json source, caching results
            if(request.term in cache2) {
              response($.map(cache2[request.term], function(item){
                return { value: item.value, id: item.id}
              }));
              return;
            }
            jQuery.ajax({
              url: "http://" + window.location.hostname + "/autocomplete/autocomplete_query.php",          
              data: "term=" + request.term,
              dataType: "json",
              type: "GET",
              contentType: "application/json; charset=utf-8",
              dataFilter: function(data) {return data;},
              success: function(data) {                              
                cache2[request.term] = data;
                response(jQuery.map(data, function(item) {
                  return {
                    value: item.value,
                    id: item.id
                  }
                }))
              },
              error: function() {
                if ( window.console && window.console.log ) {
                  // console is available
                  console.log('problem with autocomplete ajax call!');
                  }
                }
            });
          },
          select: function (a, b) { // submit form with the selected item
            jQuery(this).val(b.item.value);
            jQuery("#searchform").submit();            
          }
        });
    });
  </script>
