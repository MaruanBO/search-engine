<script type="text/javascript">
KQED.apiCall( "/webapi/q/v3/highlight/feature/homepage", function( highlights ) {
    if( highlights.errors == undefined )
    {
        if (highlights && highlights.data && highlights.data.highlights && highlights.data.highlights.length > 0)
        {
            var highlightIndex = 0; // OLD VALUE:: highlights.data.highlights.length - 1;

            highlights.data.highlights[highlightIndex].encodedURL = encodeURIComponent( highlights.data.highlights[highlightIndex].url );
            if ( highlights.data.highlights[highlightIndex].epTitle )
                highlights.data.highlights[highlightIndex].encodedEpTitle = encodeURIComponent( highlights.data.highlights[highlightIndex].epTitle );
            else
                highlights.data.highlights[highlightIndex].encodedEpTitle = encodeURIComponent( highlights.data.highlights[highlightIndex].title );

            highlights.data.highlights[highlightIndex].encodedDescription = encodeURIComponent( highlights.data.highlights[highlightIndex].description );

	    // this stupidness because mustache doesn't know how to test for "array exists"  only
	    if (highlights.data.highlights[highlightIndex].webExtra && highlights.data.highlights[highlightIndex].webExtra.length >= 0)
	        highlights.data.highlights[highlightIndex].webExtraLength = highlights.data.highlights[highlightIndex].webExtra.length;

            KQED.substitute( "featured", highlights.data.highlights[highlightIndex] );
            
            KQED.checkHref('#featuredUrl', 'article');
            
            // Show and hide "Share" buttons upon click.
            $('.featured .bu-share').click(function () {
                $('.featured .share').toggleClass('open');
                $('.featured .share ul').toggle();
            });
        }
    }
    else
    {
        KQED.lightup("featured", false);
    }
}, "featured", function() { KQED.lightup("featured", false); });
</script>

