<script type="text/javascript">

function _whatson_LOAD()
{
  var UND;

  //console.log("LOADING!...");

  // NOTE: there is a race condition that is handled elsewhere in this code.
  // These apiCall()'s use ajax, and will return control back to js, causing
  // other code to run, including the jquery resize command. THAT code protects
  // THIS code from being entered more than once. 
  // When we are done with creating the data (KQED.substitute()...) then we
  // notify the MOVE_TO object that we're LOADED.

  KQED.apiCall("/webapi/q/v3/whatsonradio", function(WHATRADIO){
    KQED.apiCall("/webapi/q/v3/highlight/feature/radio", function(RADIO){
      KQED.apiCall("/webapi/q/v3/whatsontv", function(WHATTV){
        KQED.apiCall("/webapi/q/v3/highlight/feature/tv", function(TV){

        var data = {};
        data.RADIO_now = WHATRADIO.data.whatsOnListRadio[0];
        data.RADIO_next = WHATRADIO.data.whatsOnListRadio[1];
        if (RADIO.data.highlights.length > 0)
          data.RADIO_high = KQED.getNextShow(RADIO.data.highlights);
        data.TV_now = WHATTV.data.whatsOnListTv[0];
        data.TV_next = WHATTV.data.whatsOnListTv[1];
        if (TV.data.highlights.length > 0)
          data.TV_high = KQED.getNextShow(TV.data.highlights);

        KQED.substitute("whatson", data);

        //console.log("...DONE LOADING!");

        _whatson_MOVE_TO.LOADED();

       jQuery(document).foundation(); // this is to force foundation to deal with new data on the page

  }, UND, function(){ _whatson_MOVE_TO.LOADED();jQuery(document).foundation(); })
  }, UND, function(){ _whatson_MOVE_TO.LOADED();jQuery(document).foundation(); })
  }, UND, function(){ _whatson_MOVE_TO.LOADED();jQuery(document).foundation(); })
  }, UND, function(){ _whatson_MOVE_TO.LOADED();jQuery(document).foundation(); });
}

// sorry about the complexity here -
// this is a closure, aka "object", with two private variables and two exposed methods
// this object is used in various places to move <sections> from place two positions
// in the dom - note this code doesn't know WHY they're to be moved, just how to move 'em.
var _whatson_MOVE_TO = (function () 
{
  // Used to store the location that the <sections> are in NOW, 1 or 2
  // 0 means they haven't been created yet.
  var location = 0;

  // Used during the wait for the ajax calls to return, to store the latest desired location
  // for the <sections> to be placed.
  // The example used is: with a tablet in hand, hit the page, then turn the table - during the
  // refresh of the page, the size changes enough to warrant moving the <sections> from where
  // they would have been at the start of the refresh.
  // 0 means there is no desire to move the data anywhere.
  var wantedLocation = 0;

  return { 
  // call me when you're loaded -
  LOADED : function()
  {
    if (location == 0) // this should be true right here, you know...
      location = 1;

    // This means that somebody resized the window during the ajax calls,
    // move the data NOW to the desired location and reset to 0 (no moves).
    if (wantedLocation != 0)
    {
      this.MOVE_TO(wantedLocation);
      wantedLocation = 0;
    }
  },
  MOVE_TO : function(where)   
  {
    // This can be called asychronously, before, during or after ajax calls, and we
    // have to handle all cases.

    // If the data is not yet anywhere, remember where we WANT to move, and bail
    if ( location == 0 )
    {
      //console.log("MOVE LATER:"+where);
      wantedLocation = where;
    }
    // ELSE, if we're already where we need to be, done, bail
    else if (where == location )
    {
      //console.log("NO MOVE");
      // done!
    }
    // ELSE, if nobody's done anything stupid, and we have real from/to locations to move,
    // move the data
    else if (where != 0 && location != 0)
    {
      // OLD...
      // var ad = $('#moveableContentPosition' + location+'>section').eq(0).html(); // detach();

      // OH, FOR CHRISSAKES - 
      // I'm removing perfectly good working code because
      // this search doesn't work in software written by those dodos at Microsoft...
      // changing to use direct id references.
      //var whatson = jQuery('#moveableContentPosition' + location+'>section').eq(0).detach();
      //var ad = jQuery('#moveableContentPosition' + location+'>section').eq(0).detach();
      //var support = jQuery('#moveableContentPosition' + location+'>section').eq(0).detach();

      var whatson = jQuery('#whatson').detach();
      var ad = jQuery('#lrec').detach();
      var adThreeone = jQuery('#ad-threeone').detach();
      var support = jQuery('#support').detach();

      whatson.appendTo(jQuery('#moveableContentPosition' + where));

      ad.appendTo(jQuery('#moveableContentPosition' + where));

      // OLD....
      //  jQuery('#moveableContentPosition' + where ).append(ad);

      adThreeone.appendTo(jQuery('#moveableContentPosition' + where));
      support.appendTo(jQuery('#moveableContentPosition' + where));

      _lrec_MOVE_AD();

      //console.log("MOVE FROM:" + location+" TO:"+where);
      location = where;
    }
    else // ELSE, well, shucks, somebody's stupid
      console.log("MOVE_TO:ERR:FROM"+location+" TO:"+where);
  } };
})();   // <== CALL THE FUNCTION ONCE TO CREATE THE *ONE* OBJECT!


// This is a re-entrant function to call to decide WHETHER OR NOT
// to load data.
// We call it once at the beginning, and it's called anytime there's
// a window resize, which on some browsers can be a lot.
// PS, this is not the function! This is an object, the function is RETURNED as a closure!
var _whatson_ = (function ()
{
  var loaded = false;

  return function ()
  {
    // NOTE:: THIS FUNCTION IS RE-ENTRANT!!

    //console.log("WIN.SIZE:"+jQuery(window).width()+":"+jQuery(window).height()+":"+loaded);

    var orig_loaded = loaded;

    //console.log("WIN:orig:"+orig_loaded+":WHATload:"+loaded);

    if (jQuery(window).width() <= 899)
    {
        loaded = true;
        if (! orig_loaded)
        {
           _lrec_LOAD();
           _whatson_LOAD(); // ONLY EVER ENTER THIS FUNCTION *ONCE*
        }

        _whatson_MOVE_TO.MOVE_TO(1);
    }
    else if (jQuery(window).width() >= 900)
    {
        loaded = true;
        if (! orig_loaded)
        {
           _lrec_LOAD();
           _whatson_LOAD(); // ONLY EVER ENTER THIS FUNCTION *ONCE*
        }

        _whatson_MOVE_TO.MOVE_TO(2);
    }
  };
})();   // <== CALL THE FUNCTION ONCE TO CREATE THE *ONE* OBJECT!

$(document).ready(function() { jQuery(window).resize( _whatson_ ); });

// this for testing, just loads the raw html
//jQuery("#whatson").html($("#whatson_s").html());

$(document).ready(function() { _whatson_(); }); // this is to check window size on entry (some browsers don't call resize() on startup).

</script>

