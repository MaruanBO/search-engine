<script type="text/javascript">
// show the bundle via URL

if ( window.location.hash == "#showBundle" ) 
{
KQED.apiCall( "/webapi/q/v3/highlight/bundle/homepage", function( bundle ) {
    if (bundle.data.highlights.length > 0)
    {
        KQED.substitute( "bundle", bundle.data.highlights[0]);
        
        if ( bundle.data.highlights[0].webExtra ) {
            var itemsClass = "items-" + bundle.data.highlights[0].webExtra.length;
            $("#bundle").addClass(itemsClass);
        }
    }
    KQED.lightup("bundle", bundle.data.highlights.length > 0);
}, "bundle", function() { KQED.lightup("bundle", false); });
} else {
    KQED.lightup("bundle", false);
}
</script>


