  _threeone_googled();
  LREC_GOOGLED = true;
}

function _lrec_MOVE_AD()
{
     // $('#div-gpt-ad-1378345893481-17>*').detach(); // this doesn't seem to be needed.... apparently google clears it

     //googletag.cmd.push(function() { googletag.display('div-gpt-ad-1378345893481-17'); }); // and this shouldn't be HERE...
     
    if (LREC_GOOGLED)
    {
      googletag.cmd.push(function() { googletag.pubads().refresh(); });
      //console.log('LREC MOVE/REFRESH');
    } 
}

$(document).ready(function() { _lrec_MOVE_AD(); }); // execute at least once, the first time around....

</script>

