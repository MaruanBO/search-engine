<!-- Facebook Configuration Start -->
<div id="fb-root"></div>
<script>
    window.fbAsyncInit = function () {
        FB.init({
            appId: '249076878461105',
            channelUrl: '//'+'www.canistream.it'+'/channel.html',
            status: true,
            cookie: true,
            xfbml: true
        });


        FB.Event.subscribe('auth.login', function (response) {

        });
    };

    function fblogin() {
        FB.login(function(response) {
            if (response.authResponse) {
                // connected
                FB.api('/me', function (response) {
                    $.post(
                        "/users/fblogin",
                        response, function (data) {

                            $(".nav-logoff .dropdown").removeClass("open");
                            $("#signupDialog").modal("hide");
                            if (data.status) {
                                $.setCurrentUser(data.User._id);
                                $.handleUserLoggedIn(data);
                                if (window.wantsToBeReminded != null) {
                                    $.showReminderPopup(window.wantsToBeReminded.type, window.wantsToBeReminded.affiliate, window.wantsToBeReminded.movie);
                                    window.wantsToBeReminded = null;
                                }
                            }
                            else {
                                alert("There was an error: " + data.status_message);
                            }
                        },
                        'json'
                    );
                });
            } else {
                // cancelled
            }
        },{perms:'email'});
    }

    // Load the SDK Asynchronously
      (function(d){
         var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
         if (d.getElementById(id)) {return;}
         js = d.createElement('script'); js.id = id; js.async = true;
         js.src = "//connect.facebook.net/en_US/all.js";
         ref.parentNode.insertBefore(js, ref);
       }(document));
</script>
<!-- Facebook Configuration End -->
