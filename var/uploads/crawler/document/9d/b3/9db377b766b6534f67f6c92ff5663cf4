                var Timezone = {
					detectTimezone: function() {
						if (typeof($) === 'undefined' || typeof($.cookie) === 'undefined') return;
						var offset = parseInt($.cookie('usertz'));
						var realOffset = - new Date().getTimezoneOffset();
						
						if (isNaN(offset) || offset < -720 || offset > 720 || offset !== realOffset) {
							$.cookie('usertz', realOffset, { path: '/' });
						}
					}
				};
