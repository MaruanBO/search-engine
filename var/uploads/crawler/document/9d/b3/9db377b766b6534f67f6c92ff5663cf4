<!-- original video -->
<div class="links-block original-video">
    <h2 class="block-title"><a href="/videos/originals/">p1 original videos</a></h2>
    <div class="video-links-block">
        <h2><a href="/videos/originals/training/8207122-How-dispatchers-should-handle-active-shooter-calls/">How dispatchers should handle active shooter calls</a></h2>
<div class="video-container">
<video data-account="104403126001"
       data-player="dce76391-c587-41d9-bd9e-e263f601112a"
       data-embed="default"
       data-video-id="4023097173001"
       id="video4023097173001"
       class="video-js" controls></video>
<script src="//players.brightcove.net/104403126001/dce76391-c587-41d9-bd9e-e263f601112a_default/index.min.js"></script>
<script>
    videojs("video4023097173001").ready(function () {

        var endcapImageLink = "";
        var endcapUrl = "";
        var hasEndcap = false;

        var myPlayer = this;

        var countView = function () {

            var myPlayer = this;
            var videoId = myPlayer.options()["data-video-id"];

            if (isFinite(parseInt(videoId))) {
                $.ajax({
                    type: "POST",
                    url: "/Videos/VideoWatched",
                    data: JSON.stringify({ "videoId": videoId }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json"
                });
            }
        };

        var showEndcap = function () {
            
            if (!hasEndcap) {
                return;
            }

            var myPlayer = this;
            var playerContainer = $(this.el());

            var endcapContainer = document.createElement("div");

            var endcapImage = document.createElement("img");
            endcapImage.src = endcapImageLink;

            var endcapLink = document.createElement("a");
            endcapLink.href = endcapUrl;
            endcapLink.className = "endcap-image";
            endcapLink.appendChild(endcapImage);

            endcapContainer.appendChild(endcapLink);
            
            playerContainer.replaceWith(endcapContainer);
        }

        myPlayer.one("play", countView);
        myPlayer.one("ended", showEndcap);
    });
</script>
</div>

        
        
    </div>
