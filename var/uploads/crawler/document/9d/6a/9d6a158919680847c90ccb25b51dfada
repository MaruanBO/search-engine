   document.write ("&amp;loc=" + escape(window.location));
   if (document.referrer) document.write ("&amp;referer=" + escape(document.referrer));
   if (document.context) document.write ("&context=" + escape(document.context));
   if (document.mmm_fo) document.write ("&amp;mmm_fo=1");
   document.write ("'><\/scr"+"ipt>");
//]]>--></script><noscript><a href='http://ads2.opensubtitles.org/www/delivery/ck.php?n=a6270e61&amp;cb=823147' target='_blank'><img src='http://ads2.opensubtitles.org/www/delivery/avw.php?zoneid=7&amp;cb=823147&amp;n=a6270e61' border='0' alt='' /></a></noscript></div><script type="text/javascript">
