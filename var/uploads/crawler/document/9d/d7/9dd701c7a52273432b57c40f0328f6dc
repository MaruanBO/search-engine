		function resizeWidgetMovieHeight() {
			var maxHeight = -1;
			$('.widgets .schedule .movie-viewport .movie-shadow-container').each(function() {
		    	maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
		    });

		    $('.widgets .schedule .movie-viewport .movie-shadow-container').css('min-height', (maxHeight+18)+'px');
		}
		function loadWidgetImage() {
			var now_playing_gpms = $('#now_playing .now-playing-container .movie-shadow-container').attr('data-gpms');
			//console.log(now_playing_gpms+' Window Load');
			$('#now_playing .movie-img[data-gpms="'+now_playing_gpms+'"]').first().fadeIn();
			$('#now_playing').fadeIn(800);
			setTimeout(function(){
				var now_playing_gpms = $('#now_playing .now-playing-container .movie-shadow-container').attr('data-gpms');
				//console.log(now_playing_gpms+' Window 200');
				$('#now_playing .movie-img[data-gpms="'+now_playing_gpms+'"]').first().fadeIn();
				$('#now_playing').fadeIn(800);
			}, 200);
			setTimeout(function(){
				var now_playing_gpms = $('#now_playing .now-playing-container .movie-shadow-container').attr('data-gpms');
				//console.log(now_playing_gpms+' Window 400');
				$('#now_playing .movie-img[data-gpms="'+now_playing_gpms+'"]').first().fadeIn();
				$('#now_playing').fadeIn(800);
			}, 400);
			setTimeout(function(){
				var now_playing_gpms = $('#now_playing .now-playing-container .movie-shadow-container').attr('data-gpms');
				//console.log(now_playing_gpms+' Window 600');
				$('#now_playing .movie-img[data-gpms="'+now_playing_gpms+'"]').first().fadeIn();
				$('#now_playing').fadeIn(800);
			}, 600);
			setTimeout(function(){
				var now_playing_gpms = $('#now_playing .now-playing-container .movie-shadow-container').attr('data-gpms');
				//console.log(now_playing_gpms+' Window 800');
				$('#now_playing .movie-img[data-gpms="'+now_playing_gpms+'"]').first().fadeIn();
				$('#now_playing').fadeIn(800);
			}, 800);
		}
		function sizeWidgetMovies() {
			setTimeout(function() {
                $(".widgets .schedule #movie-container .movie-shadow-container .movie").each(function(e) {
                    var t = this;
                    var maxHeight = -1;
                    $('.widgets .schedule .movie-viewport .movie-shadow-container').each(function() {
                        maxHeight = maxHeight > $(this).find('.movie').height() ? maxHeight : $(this).find('.movie').height();
                    });
                    $(t).parent().css('min-height', (maxHeight+18)+'px');
                })
            }, 300);
		}
	</script>

