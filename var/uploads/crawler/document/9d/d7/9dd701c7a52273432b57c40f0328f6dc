	<script src="/sites/all/themes/recess/js/libs/jquery.widowFix-1.3.2.min.js"></script>

	<script type="text/javascript">
		$(window).resize(function() {
			sizeWidgetMovies();
		});
		// $('.slide-content p').widowFix();

		// Fix caousel text wrapping
		String.prototype.replaceAt=function(index, character) { return this.substr(0, index) + character + this.substr(index+1); }
		$('#home-carousel .slide-content').each( function()
			{
				if( $(window).width() > 500 )
				{
					var lh = parseInt($(this).find('p:first').css('line-height'));

					if( $(this).find('p:first').height() == lh*2 ) // 2 ROWS
					{
						var str = $(this).find('p:first').text();
						var spaces = str.split(" ").length-1;
						var half = parseInt(spaces/2) + 1;
						var nth = 0;
						var splitter = 0;

						for (var i = 0, len = str.length; i < len; i++) {  
						  if(str[i] == " "){ nth++; }
						  if(nth == half && nth > -1){ splitter = i; nth = -1; }
						}

						$(this).find('p:first').html( str.replaceAt( splitter, "<br>") );
					}
					else if( $(this).find('p:first').height() == lh*3 ) // 3 ROWS
					{
						var str = $(this).find('p:first').text();
						var spaces = str.split(" ").length-1;
						var third = parseInt(spaces/3) - 3;
						var nth = 0;
						var splitter1 = 0;
						var splitter2 = 0;

						for (var i = 0, len = str.length; i < len; i++) {  
						  if(str[i] == " "){ nth++; }
						  if(nth == third && nth > -1){ splitter1 = i; nth = -1; }
						}
						nth = 0;
						for (var i = 0, len = str.length; i < len; i++) {  
						  if(str[i] == " "){ nth++; }
						  if(nth == (third*2) && nth > -1){ splitter2 = i; nth = -1; }
						}

						$(this).find('p:first').html( str.replaceAt( splitter1, "<br>") );
						$(this).find('p:first').html( str.replaceAt( splitter2, "<br>") );
					}
				}
				else // MOBILE
				{
					$(this).find('p:first').widowFix();
				}
			}
		);



		var jan = new Date( 2009, 0, 1, 2, 0, 0 ), 
		   	 jul = new Date( 2009, 6, 1, 2, 0, 0 );

		var tzAbbrev = getTimeZoneAbbreviation(),
			tzOffset = ( jan.getTime() % 24 * 60 * 60 * 1000 ) > 
			           ( jul.getTime() % 24 * 60 * 60 * 1000 )
			           ? -jan.getTimezoneOffset()/60 : -jul.getTimezoneOffset()/60;


		//console.log(tzAbbrev + ' '+tzOffset);
		$.ajax({
			type: "GET",
			dataType: "json",
            url: "/sites/all/themes/recess/php/star.php",
			data: { 'tzOffset' : tzOffset, 'tzAbbrev' : tzAbbrev },
          	success: function( data ) {
          		$('#home-carousel .slide.current_icon').each(function(){
          			var todaysDate = new Date();
          			var starName = $(this).find('h2').text().replace(/\s+/g, '');
          			var idx=$.map(data, function(item,i){
          				var artistlist = new String(item.artists);
          				var newartistlist = artistlist.replace(/\s+/g, '').split(',');

          				//console.log(newartistlist);
					    if( ( $.inArray(starName, newartistlist) > -1/*(new String(item.artists)).toLowerCase().split(',').indexOf(starName.toLowerCase()) >= 0*/ ) && ((new Date(item.timestampstars * 1000)).getTime() >= todaysDate.getTime()) )
					        return i;
					})[0];
					/*var idx2=$.map(data, function(item,i){
					    if( (item.artists).toLowerCase().indexOf(starName.toLowerCase()) >= 0 )
					        return i;

					})[0];*/
          			if (idx != undefined) {
	          			$(this).find('.next-movie .movie-title').text(data[idx].title);
	          			$(this).find('.next-movie .time').show().text(data[idx].date + ' at ' + data[idx].timestampfull + ' '+tzAbbrev);
          			} else {
	          			$(this).find('.next-movie').hide();
	          			$(this).find('.next-movie .movie-title').hide();
	          			$(this).find('.next-movie .time').hide();
          			}
          		});
			}, 
			complete: function() {
				$('#home-carousel .slide.current_icon .next-movie').animate({'opacity': '1'});
			}
		});

		$(document).ready(function(){
			getNowPlaying('02/19/2015', '.now-playing-container');
			//getNowPlaying('11/01/2014', '.now-playing-container');
			$('.b-test-home .widgets .right-widgets').insertBefore($('.b-test-home .widgets .eight-col'));
			$('.box.trivia').hide();
			loadWidgetImage(); 
			


			setTimeout(function(){
				$('#dates .date').first().click();
				
				
				var nowPlayingTitle = ($.trim($('.now-playing .main-movie-title').text())).toLowerCase();
				$('.trivia[data-movie-title="'+nowPlayingTitle+'"]').show();
				var nowplayHeight = $('.widgets .eight-col').height();
				$('.widgets .schedule #movie-container').height(nowplayHeight - 125);
				$('.widgets .now-playing .schedule-now-playing').attr('style', 'display:none!important');

