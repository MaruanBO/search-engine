		jQuery(document).ready(function($){
		
			$('.fc-nav-366 li').each(function(i) {
				$(this).addClass( 'item'+i );
				$(this).click(function() {
					$('.fs-366').trigger( 'slideTo', [i, 0, true] );
				return false;
				});
			});
			$('.fc-nav-366 li.item0').addClass( 'active' );
		
			var carou_items = 6;
			if ($(window).width() < 768) {
					carou_items = 5;
			}
			if ($(window).width() < 568) {
					carou_items = 4;
			}

			if ($(window).width() < 480 ) {
					carou_items = 2;
			}

			$(".fs-366").carouFredSel({
					circular: true,
                                        responsive: true,
					swipe: {
						onTouch: true,
						fx : 'scroll'
					},
					items: 1,
					auto: {
                                             play: true,
                                             duration: 600,
                                             timeoutDuration: 4000,
                                             },
					prev: '.fc-nav-366 .fs-prev, .fs-dnav-366 span.fsd-prev',
					next: '.fc-nav-366 .fs-next, .fs-dnav-366 span.fsd-next',
					pagination: '.fs-nav-366',
					scroll: {
						fx: 'crossfade',
                                                  duration : 600,
                                                easing  : 'easeInOutCubic',
						pauseOnHover : true,
                                        	onBefore: function() {
							var pos = $(this).triggerHandler( 'currentPosition' );
							$('.fc-nav-366 li').removeClass( 'active' );
							$('.fc-nav-366 li.item'+pos).addClass( 'active' );
							var page = Math.floor( pos / carou_items );
							$('.fc-nav-366 ul').trigger( 'slideToPage', page );

						},
						onAfter: function() {
						}

					}
			});

			$(".fc-nav-366 ul").carouFredSel({
						auto: false,
						circular:true,
                                        responsive: true,
						swipe: {
							onTouch: true
						},
						items: carou_items,
						scroll: {
							items:carou_items,
						}
			});			
		});

	</script>
