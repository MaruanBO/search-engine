<script type="text/javascript">
    $(document).ready(function(){
	$("#tab1").click(function() {
	    $(".wxtab").removeClass("current");
	    $("#sForecast").css("display","block");
	    $("#sRadar").css("display","none");
	    $("#sCurrent").css("display","none");
	    $("#tab1").addClass("current");
	    return false;
	});
	$("#tab2").click(function() {
	    $(".wxtab").removeClass("current");
	    $("#sForecast").css("display","none");
	    $("#sRadar").css("display","block");
	    $("#sCurrent").css("display","none");
	    $("#tab2").addClass("current");
	    return false;
	});
	$("#tab3").click(function() {
	    $(".wxtab").removeClass("current");
	    $("#sForecast").css("display","none");
	    $("#sRadar").css("display","none");
	    $("#sCurrent").css("display","block");
	    $("#tab3").addClass("current");
	    return false;
	});
});
</script>
