	<script type="text/javascript">
	function getInternetExplorerVersion()
  {  var rv = -1; 
  if (navigator.appName == 'Microsoft Internet Explorer')
  {
    var ua = navigator.userAgent;
    var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
    if (re.exec(ua) != null)
      rv = parseFloat( RegExp.$1 );
  }
  return rv;
  }
  var contentDisplay = {
				delayTimer: 1500, 				runHide: false, 				runHideSet: false, 				cameFromNav: false, 				animationRunning: false, 				hoveringOverMain: true, 				defaultPreviewHeight: 400, 				previewAnimateHeightTime: 900, 				fadeOutTime: function(){ if(getInternetExplorerVersion() > -1){15}else{150}}, 				fadeInTime: function(){if(getInternetExplorerVersion() > -1){30}else{300}}, 				navHoverDelay: 250, 				videoPlaying: false, 				overTrigger: false, 				preview: $("#preview"), 				
				navTop: function(){if($("#nav-top").length){$("#nav-top")}else{$("nav")}},
				
				subNav: $("#home-subnav"),
				easing: "easeOutSine", 				t:"",
				api: {
					showFunction: function(element){
												contentDisplay.runHide = false;
						var id = element.href.replace('http://www.wbng.com/', '');
						if(id) {
							var navId = "subnav-" + id;
							var id = "preview-" + id;
							hoveringOverMain = false;
						} else {
							var navId = $(contentDisplay.subNav).find("ul:first").attr("id");
							var id = $(contentDisplay.preview).find("div.preview-item:first").attr("id");
							hoveringOverMain = true;
						}
						var navId = navId.toLowerCase();
						var id = id.toLowerCase();
						contentDisplay.cameFromNav = true;
												$(contentDisplay.navTop).find(".top-level-item a.current, li.current").removeClass("current");
						$(element).addClass("current");
						contentDisplay.api.showPreview(id, navId);
					},
					videoPlaying: function(){
						if(typeof $f != "undefined" && $f().isPlaying()){
							return true;
						} else if(typeof $f != "undefined" && !$f().isPlaying()){
							return false
						} else {
							return false;	
						}
					},
					setHeight: function(id){
																		var newHeight = $(contentDisplay.preview).find("#" + id).height() + $(contentDisplay.subNav).height() + $(contentDisplay.preview).find("#breaking-event-container").height() + $(contentDisplay.preview).find(".breaking-event-streaming").height();
						if(newHeight == 0) {
							newHeight = contentDisplay.defaultPreviewHeight;	
						}
						jQuery.easing.def = contentDisplay.easing;
												$(contentDisplay.preview).stop().animate({
								height: newHeight
							}, contentDisplay.previewAnimateHeightTime);	
					},
					resetPreview: function() {
														var element = $(contentDisplay.navTop).find(".top-level-item a:first");
							var id = $(element).attr("href");
							if(id = "/") {
								var id = "news";	
							}
							hoveringOverMain = true;
							$(contentDisplay.navTop).find(".top-level-item a.current").removeClass("current");
							$(element).addClass("current");
							var id = id.replace('/','');
							var id = "preview-" + id;
							var id = id.toLowerCase();
							var navId = $(contentDisplay.subNav).find("ul:first").attr("id");
														contentDisplay.api.showPreview(id, navId);
					},
					showPreview: function(id, navId) {
						
											
						if ($("#" + id + ":not(.open)").length && !contentDisplay.animationRunning) {
							$(contentDisplay.preview).find("#flashcontent, #player_companion").remove();
							removeSwf();
							$(contentDisplay.preview).find("#home.top-story .hentry").hide();
							var slideContainer = $(contentDisplay.preview).find("#home.top-story .item1");
							$(contentDisplay.preview).find("#home.top-story .item1").removeClass("hide").fadeIn().addClass("currentSlide");
							$(contentDisplay.preview).find("#home.top-story .hentry").slice(1).fadeOut().removeClass("currentSlide hide");
							$(contentDisplay.preview).find("#top-story-rotate .item1").addClass("currentRotate");
							contentDisplay.changeTop(slideContainer)
													$(contentDisplay.preview).find("#bimvidplayer0_cb").remove();
							$(contentDisplay.preview).find("#bimvidplayer0_api").remove();
													$(contentDisplay.preview).find("#flashcontent").remove();
							$(contentDisplay.preview).find(".top-story .story .placeholder").show();
							var currentHeight = $(contentDisplay.preview).height();
							$(contentDisplay.preview).css("height", currentHeight);
							var fadeDelay = contentDisplay.fadeOutTime + 100;
							
							contentDisplay.animationRunning = true;
							$(contentDisplay.preview).find(".open").stop().fadeOut(contentDisplay.fadeOutTime).removeClass("open").addClass("hide");
							$(contentDisplay.subNav).find(".open").stop().fadeOut(contentDisplay.fadeOutTime).removeClass("open").addClass("hide");
							$(contentDisplay.preview).find(".preview-item").css("opacity","0");
							$(contentDisplay.subNav).find(".sub-level").css("opacity","0");
							setTimeout(function() {
																		$("#" + id).addClass("open").stop().fadeTo(contentDisplay.fadeInTime,1, function() {
										contentDisplay.animationRunning = false;
											if(jQuery.browser.msie){
												this.style.removeAttribute("filter");
											}
										});
																		$("#" + navId).addClass("open").stop().fadeTo(contentDisplay.fadeInTime,1, function() {
										contentDisplay.animationRunning = false;
											if(jQuery.browser.msie){
												this.style.removeAttribute("filter");
											}
										});
								}, fadeDelay)
							contentDisplay.api.setHeight(id);
						} else if ($("#" + id + ":not(.open)").length && contentDisplay.animationRunning) {
							setTimeout(function() {
								$(":animated", contentDisplay.preview).stop();
								contentDisplay.animationRunning = false;
								contentDisplay.api.showPreview(id);
							}, 100)
						}
					},
					hideDelay: function(elemet){
							
														if(contentDisplay.runHideSet) clearTimeout(hideDelayTimer);
							contentDisplay.runHideSet = true;					
							hideDelayTimer = setTimeout(function () {
								contentDisplay.runHideSet = false;
																if(contentDisplay.runHide && !contentDisplay.videoPlaying) {
							        contentDisplay.runHide = false;
							        									contentDisplay.api.resetPreview();
								}
								}, contentDisplay.delayTimer);			
					},
					videoPlayingWarning: function(callback) {
						var videoAlert = $(contentDisplay.preview).find("#video-playing");
												$(videoAlert).find("#continue-browsing").unbind("click");
						$(videoAlert).removeClass("hide").stop().slideDown();
						$(videoAlert).find("#continue-browsing").click(function(event){
								event.preventDefault();
								$(videoAlert).slideUp();
																								contentDisplay.overTrigger = false;
								contentDisplay.videoPlaying = false;
								callback.call();
						});
						$(videoAlert).find("#continue-video").click(function(event){
								event.preventDefault();
								$(videoAlert).slideUp();
						})
					},
					hoverOutFunction: function() {
						contentDisplay.runHide = true;
												contentDisplay.api.hideDelay();	
					},
					preLoadVidXML: function() {
												
						if($("#preview #BIMvidPlayer").length){
							bimso.addVariable("autostart", "true");
						}
						
						$(contentDisplay.preview).find(".preview-item .top-story .hasvideo").each(function() {
						    var vidUrl = $(this).find(".entry-title a").attr("href");
						    if(vidUrl){
						        vidUrl = vidUrl.replace('http://www.wbng.com/', '');
													        vidUrl = vidUrl + "?player=ext";
													        vidUrl = 'http://www.wbng.com/' + vidUrl;
						        jQuery.data(this, "videoXmlUrl", vidUrl);
						    }
							if($(this).attr("id") == "preview-weather" && vidUrl) {
								$(this).find(".top-story .entry-title a").attr("href", "/weather");
							}
						});
					}
				}, 
				rotateAuto: function() {
					if($(contentDisplay.preview).find(".top-story .currentSlide").next("li").length) {
						var container = $(contentDisplay.preview).find(".top-story .currentSlide").next("li");
						var navContainer = $(contentDisplay.preview).find("#top-story-rotate .currentRotate").next("li");
					}else{
						var container = $(contentDisplay.preview).find("#home.top-story .item1");
						var navContainer = $(contentDisplay.preview).find("#top-story-rotate .item1");
					}
										contentDisplay.changeTop(container,"auto",navContainer);
				},
				changeTop: function(newElement, from, navContainer) {
					clearTimeout(contentDisplay.t);
					if($(newElement + ":not(.currentSlide)").length) {
						$(newElement).find(".placeholder").show();
												if(!contentDisplay.videoPlaying) {
														$(contentDisplay.preview).find(".top-story .currentSlide").removeClass("currentSlide").fadeOut("slow", 
							function(){
								$(contentDisplay.preview).find("#top-story-rotate .currentRotate").removeClass("currentRotate");
								$(newElement).fadeIn().addClass("currentSlide");
								$(navContainer).addClass("currentRotate");
															$(contentDisplay.preview).find("#bimvidplayer0").remove();
								$(contentDisplay.preview).find("#flashcontent, #bimvidplayer0_cb").remove();
														});
						} else if(contentDisplay.videoPlaying && from == "user") {
							contentDisplay.api.videoPlayingWarning(function() {	
								contentDisplay.changeTop(selector,"user")
															$(contentDisplay.preview).find("#bimvidplayer0").remove();
								$(contentDisplay.preview).find("#flashcontent, #bimvidplayer0_cb").remove();
														});
						}
						contentDisplay.timedChng();
					} else { contentDisplay.timedChng(); }
				},
				timedChng: function() {
					contentDisplay.t=setTimeout("contentDisplay.rotateAuto()",7000);
				},
				slideShowInit: function() {
					$(contentDisplay.preview).find("#home.top-story .item1").removeClass("hide");
					$(contentDisplay.preview).find("#home.top-story .hentry").slice(1).fadeOut().removeClass("currentSlide");
					$(contentDisplay.preview).find("#top-story-rotate .item1").addClass("currentRotate");
					$(contentDisplay.preview).find("#top-story-rotate li").each(function(index) {
					    var that = this;
					    var item = index + 1;
					    $(that).attr("id","item" + item);
					})
					
					function changeTopStory(container){
						var selector = $(contentDisplay.preview).find(".top-story ." + container);
						var itemClicked = $(contentDisplay.preview).find("#top-story-rotate #" + container);
						contentDisplay.changeTop(selector,"user",itemClicked);	
					}
					
					$(contentDisplay.preview).find("#top-story-rotate li .placeholder a").click(function(e){
						e.preventDefault();
						var that = $(this);
						var container = $(that).parent(".placeholder").parent("li").attr("id");
						changeTopStory(container)
						return false;
					})
					$(contentDisplay.preview).find("#top-story-rotate li .entry-title a").click(function(e){
						e.preventDefault();
						var that = $(this);
						var container = $(that).parent(".entry-title").parent("li").attr("id");
						changeTopStory(container)
						return false;
					})
					contentDisplay.timedChng();
				},
				init: function() {
					var hoverItems = $(contentDisplay.navTop).find(".top-level-item a").slice(0,6);
										contentDisplay.slideShowInit();
															var config = {
						over: function() {
								if(contentDisplay.videoPlaying) {
									contentDisplay.overTrigger = true;
																		contentDisplay.cameFromNav = true;
									contentDisplay.api.videoPlayingWarning(function() {contentDisplay.api.showFunction(this);});
								}else{
																		contentDisplay.api.showFunction(this);
								}
							}, 
						interval: contentDisplay.navHoverDelay, 
						out: function() {
							if(contentDisplay.videoPlaying) {
																contentDisplay.api.videoPlayingWarning(function() {contentDisplay.api.hoverOutFunction();});
							}else{
																contentDisplay.cameFromNav = true;
								contentDisplay.api.hoverOutFunction();
							}
						}
					}//End Config
					
										
																jQuery.ajax({
						  url: '/internal?previewPane=y&c=y',
										
					  dataType: "html",
					  type: "GET",
					  success: function(data) {  	
					  	$(contentDisplay.preview).append(data);
					  	contentDisplay.api.preLoadVidXML();
						$(contentDisplay.navTop).find(".top-level-item a").slice(0,6).hoverIntent(config);
						$(contentDisplay.preview).hover(function(){
														contentDisplay.runHide = false;
																											}, function() {
														contentDisplay.runHide = true;
														contentDisplay.api.hideDelay();
						})
						$(contentDisplay.preview).hover(function(){
							setTimeout(function() {
							    contentDisplay.runHide = false;
							    							}, 200);
						}, function(){
							setTimeout(function() {
							    contentDisplay.runHide = true;
							    							}, 200);
						})
                                                                                                                                                        					  }
					});				} 	};
	hovertool = contentDisplay.init();
	


	function removeSwf(){
		var player = document.getElementById("BIMvidPlayer");
		if (typeof player == 'object' && player != null){
			player.style.display = 'none';
			for (var x in player) {
				if (typeof player[x] == 'function') {
					player[x] = function(){};
				}
			}
		}
		
	}
	
	function resumeAfterVideo() {
		contentDisplay.videoPlaying = false;
	}
	</script>

  

