function getParameterByName(name){
    name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regexS = "[\\?&]" + name + "=([^&#]*)";
    var regex = new RegExp(regexS);
    var results = regex.exec(window.location.href);
    if(results == null) 
    { 
        return "";
    } 
    else 
    { 
        return decodeURIComponent(results[1].replace(/\+/g, " "));
    }
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

function setCookie ( name, value, path, domain, exp_y, exp_m, exp_d, secure ) {
    var cookie_string = name + "=" + escape ( value );
    if ( exp_y ) {
        var expires = new Date ( exp_y, exp_m, exp_d );
        cookie_string += "; expires=" + expires.toGMTString();
    }
    if ( path )
        cookie_string += "; path=" + escape ( path );
    if ( domain )
        cookie_string += "; domain=" + escape ( domain );
    if ( secure )
        cookie_string += "; secure";
    document.cookie = cookie_string;
}

function saveParametersToCookies(fields) {
    var value = "";
    var parameter = "";

    for (var i = 0; i < fields.length; i++) {
        if ((parameter = fields[i].from_parameter) && (value = getParameterByName(parameter))) {
            setCookie(parameter, value, false, '.singleplatform.com');
        }
    }
}

function getValueFromCookie(name) {
    return readCookie(name);
}

function getValueFromCookieOrParameter(name) {
    if (!(value = getParameterByName(name))) {
        value = getValueFromCookie(name);
        if (value == null)
            value = "";
    }
    return value;
}

function getFormFields() {
    var fields = [
        {
			"label": "EFID__c",
            "type": "hidden",
            "name": "EFID__c",
            "id": "EFID__c",
            "from_parameter": "ef_id"
        },{
			"label": "adwords_keyword",
            "type": "hidden",
            "name": "adwords_keyword",
            "id": "adwords_keyword",
            "from_parameter": "keyword"
        },{
            "label": "sp_channel",
            "type": "hidden",
            "name": "sp_channel",
            "id": "sp_channel",
            "from_parameter": "sp_channel"
        },
        {
            "label": "sp_adgroup",
            "type": "hidden",
            "name": "sp_adgroup",
            "id": "sp_adgroup",
            "from_parameter": "sp_adgroup"
        },
        {
            "label": "sp_source",
            "type": "hidden",
            "name": "sp_source",
            "id": "sp_source",
            "from_parameter": "sp_source"
        },
        {
            "label": "sp_campaign",
            "type": "hidden",
            "name": "sp_campaign",
            "id": "sp_campaign",
            "from_parameter": "sp_campaign"
        },
                {
            "label": "Medium",
            "type": "hidden",
            "id": "medium",
            "name": "00Nd0000006R6R5",
            "maxlength": "255",
            "autofill": function () { setTimeout(function() {
                var utmz = readCookie('__utmz'); //using a cookie reading function

                if (!utmz) {
                    return;
                }

                var vals = (function() {
                    var pairs = utmz.split('.').slice(4).join('.').split('|');
                    var ga = {};
                    for (var i = 0; i < pairs.length; i++) {
                        var temp = pairs[i].split('=');
                        ga[temp[0]] = temp[1];
                    }
                    return ga;
                })();

                var medium = vals.utmcmd;
                $('#medium').val(medium);
            },1000);
                                      return "";
                                    }
        },
        {
            "label": "spuq",
            "type": "hidden",
            "name": "00Nd0000006RCbh",
            "id": "00Nd0000006RCbh",
            "maxlength": "40",
            "autofill": function() {
                var cookiename = "spuq";
                var timestamp = new Date().getTime();
                var unique_identifier = readCookie(cookiename) || (timestamp + "x" + Math.random());
                setCookie(cookiename, unique_identifier, false, '.singleplatform.com');
                return unique_identifier;
            }
        }
    ];
    
    return addAutoFillToFields(fields);
}

function addAutoFillToFields(fields) {
    for (var i = 0; i < fields.length; i++) {
        if (fields[i].from_parameter) {
            fields[i].autofill = function (parameter) {
                return function () {
                    return getValueFromCookieOrParameter(parameter);
                }
            }(fields[i].from_parameter);
        }
    }
 
    return fields;
}

function instantiate_lead_form() {
    var fields = getFormFields();
    saveParametersToCookies(fields);

    $.each(fields, function(fieldkey, fieldvalue) {
        $("input[name=" + fieldvalue.name + "]").each(function() {
            if (fieldvalue.autofill) {
                $(this).val(fieldvalue.autofill());
            }
        });    
    });
}

        
        function validate_lead_form(placement) {
                var result = {
                        'form_is_valid': true,
                        'company_field_error': false,
                        'name_field_error': false,
                        'email_field_error': false,
                        'phone_field_error': false,
                        'zip_field_error': false
                };
                
                switch(placement) {
                        case "nav_lead_form":
                                result['company_field_error'] = ($.trim($("#company_nav_lead").val()) == "" || $.trim($("#company_nav_lead").val()) == "company name");
                                result['name_field_error'] = ($.trim($("#name_nav_lead").val()) == "" || $.trim($("#name_nav_lead").val()) == "your name");
                                result['email_field_error'] = ($.trim($("#email_nav_lead").val()) == "" || $.trim($("#email_nav_lead").val()) == "email");
                                result['phone_field_error'] = ($.trim($("#phone_nav_lead").val()) == "" || $.trim($("#phone_nav_lead").val()) == "phone" || $.trim($("#phone_nav_lead").val()).length <= 9);
                                result['zip_field_error'] = ($.trim($("#zip_nav_lead").val()) == "" || $.trim($("#zip_nav_lead").val()) == "zip" || $.trim($("#zip_nav_lead").val()).length <= 4);
                                break;
                        case "top_lead_form":
                                result['company_field_error'] = ($.trim($("#company_top_lead").val()) == "" || $.trim($("#company_top_lead").val()) == "company name");
                                result['name_field_error'] = ($.trim($("#name_top_lead").val()) == "" || $.trim($("#name_top_lead").val()) == "your name");
                                result['email_field_error'] = ($.trim($("#email_top_lead").val()) == "" || $.trim($("#email_top_lead").val()) == "email");
                                result['phone_field_error'] = ($.trim($("#phone_top_lead").val()) == "" || $.trim($("#phone_top_lead").val()) == "phone");
                                result['phone_field_error'] = ($.trim($("#phone_top_lead").val()) == "" || $.trim($("#phone_top_lead").val()) == "phone" || $.trim($("#phone_top_lead").val()).length <= 9);
                                result['zip_field_error'] = ($.trim($("#zip_top_lead").val()) == "" || $.trim($("#zip_top_lead").val()) == "zip" || $.trim($("#zip_top_lead").val()).length <= 4);
                                break;
                        case "bottom_lead_form":
                                result['company_field_error'] = ($.trim($("#company_bottom_lead").val()) == "" || $.trim($("#company_bottom_lead").val()) == "company name");
                                result['name_field_error'] = ($.trim($("#name_bottom_lead").val()) == "" || $.trim($("#name_bottom_lead").val()) == "your name");
                                result['email_field_error'] = ($.trim($("#email_bottom_lead").val()) == "" || $.trim($("#email_bottom_lead").val()) == "email");
                                result['phone_field_error'] = ($.trim($("#phone_bottom_lead").val()) == "" || $.trim($("#phone_bottom_lead").val()) == "phone" || $.trim($("#phone_bottom_lead").val()).length <= 9);
                                result['zip_field_error'] = ($.trim($("#zip_bottom_lead").val()) == "" || $.trim($("#zip_bottom_lead").val()) == "zip"  || $.trim($("#zip_bottom_lead").val()).length <= 4);
                                break;
                        case "subscription_price_lead_form":
                                // The one where you click on a price and then the form pops up
                                result['company_field_error'] = ($.trim($("#company_subscription_price_lead").val()) == "" || $.trim($("#company_subscription_price_lead").val()) == "company name");
                                result['name_field_error'] = ($.trim($("#name_subscription_price_lead").val()) == "" || $.trim($("#name_subscription_price_lead").val()) == "your name");
                                result['email_field_error'] = ($.trim($("#email_subscription_price_lead").val()) == "" || $.trim($("#email_subscription_price_lead").val()) == "email");
                                result['phone_field_error'] = ($.trim($("#phone_subscription_price_lead").val()) == "" || $.trim($("#phone_subscription_price_lead").val()) == "phone" || $.trim($("#phone_subscription_price_lead").val()).length <= 9);
                                result['zip_field_error'] = ($.trim($("#zip_subscription_price_lead").val()) == "" || $.trim($("#zip_subscription_price_lead").val()) == "zip" || $.trim($("#zip_subscription_price_lead").val()).length <= 4);
                                break;
                        default:
                                break;
                }
                
                result['form_is_valid'] = (!result['company_field_error'] && !result['name_field_error'] && !result['email_field_error'] && !result['phone_field_error'] && !result['zip_field_error']);
                return result;
        }

        $(document).ready(function(){
            instantiate_lead_form();
        
            $("form.signup").each(function() {
                    var parent_form = $(this);
                        
                        parent_form.submit(function(submit_event) {
                                var lead_form_data = "";
                                var form_validation_result = {};
                                var company_elem = null;
                                var name_elem = null;
                                var email_elem = null;
                                var phone_elem = null;
                                var zip_elem = null;
                                var subscription_price_elem = null;
                                
                                switch(parent_form.attr("id")) {
                                        case "lead_form_nav":
                                                lead_form_data = $("#lead_form_nav").serialize();
                                                form_validation_result = validate_lead_form("nav_lead_form");
                                                company_elem = $("#company_nav_lead");
                                                name_elem = $("#name_nav_lead");
                                                email_elem = $("#email_nav_lead");
                                                phone_elem = $("#phone_nav_lead");
                                                zip_elem = $("#zip_nav_lead");
                                                break;
                                        case "lead_form_top":
                                                lead_form_data = $("#lead_form_top").serialize();
                                                form_validation_result = validate_lead_form("top_lead_form");
                                                company_elem = $("#company_top_lead");
                                                name_elem = $("#name_top_lead");
                                                email_elem = $("#email_top_lead");
                                                phone_elem = $("#phone_top_lead");
                                                zip_elem = $("#zip_top_lead");
                                                break;
                                        case "lead_form_bottom":
                                                lead_form_data = $("#lead_form_bottom").serialize();
                                                form_validation_result = validate_lead_form("bottom_lead_form");
                                                company_elem = $("#company_bottom_lead");
                                                name_elem = $("#name_bottom_lead");
                                                email_elem = $("#email_bottom_lead");
                                                phone_elem = $("#phone_bottom_lead");
                                                zip_elem = $("#zip_bottom_lead");
                                                break;
                                        case "lead_form_subscription_price":
                                                lead_form_data = $("#lead_form_subscription_price").serialize();
                                                form_validation_result = validate_lead_form("subscription_price_lead_form");
                                                company_elem = $("#company_subscription_price_lead");
                                                name_elem = $("#name_subscription_price_lead");
                                                email_elem = $("#email_subscription_price_lead");
                                                phone_elem = $("#phone_subscription_price_lead");
                                                zip_elem = $("#zip_subscription_price_lead");
                                                break;
                                }
                                
                                if (form_validation_result['form_is_valid']) {
                                        /**
                                        $.ajax({
                                                type: "POST",
                                                url: "http://www.singleplatform.com/wp-admin/admin-ajax.php",
                                                data: lead_form_data,
                                                success: function(result) {
                                                        location.href = "http://www.singleplatform.com/thank-you/";
                                                }
                                        });
                                        */
                                        return true;
                                }else {
                                        if (form_validation_result['company_field_error']) {
                                                $(company_elem).addClass("lead_form_field_error");
                                        }else {
                                                if ($(company_elem).hasClass("lead_form_field_error")) {
                                                        $(company_elem).removeClass("lead_form_field_error");
                                                }
                                        }
                                        
                                        if (form_validation_result['name_field_error']) {
                                                $(name_elem).addClass("lead_form_field_error");
                                        }else {
                                                if ($(name_elem).hasClass("lead_form_field_error")) {
                                                        $(name_elem).removeClass("lead_form_field_error");
                                                }
                                        }
                                        
                                        if (form_validation_result['email_field_error']) {
                                                $(email_elem).addClass("lead_form_field_error");
                                        }else {
                                                if ($(email_elem).hasClass("lead_form_field_error")) {
                                                        $(email_elem).removeClass("lead_form_field_error");
                                                }
                                        }
                                        
                                        if (form_validation_result['phone_field_error']) {
                                                $(phone_elem).addClass("lead_form_field_error");
                                        }else {
                                                if ($(phone_elem).hasClass("lead_form_field_error")) {
                                                        $(phone_elem).removeClass("lead_form_field_error");
                                                }
                                        }
                                        
                                        if (form_validation_result['zip_field_error']) {
                                                $(zip_elem).addClass("lead_form_field_error");
                                        }else {
                                                if ($(zip_elem).hasClass("lead_form_field_error")) {
                                                        $(zip_elem).removeClass("lead_form_field_error");
                                                }
                                        }
                                        return false;
                                }
                        });
                });
        });
        </script>
