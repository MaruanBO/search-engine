<script>
    function validateCompany3(company3){
      var ck_company = /^(?!static$|company name$)[A-Za-z0-9 '-,+]{2,}$/;
      if(ck_company.test(document.getElementById(company3).value)){
        document.getElementById(company3 + 'Error').style.display = "none";
        return true;
      }else{
        document.getElementById(company3 + 'Error').style.display = "block";
        return false; 
      }
    }
    function validateName3(name3){
      var ck_name = /^(?!static$|name$)[A-Za-z '-]{2,}$/;
      if(ck_name.test(document.getElementById(name3).value)){
        document.getElementById(name3 + 'Error').style.display = "none";
        return true;
      }else{
        document.getElementById(name3 + 'Error').style.display = "block";
        return false; 
      }
    }
    function validateLocations3(locations3){
      var ck_locations = /^[0-9]{1,10}$/;
      if(ck_locations.test(document.getElementById(locations3).value)){
        document.getElementById(locations3 + 'Error').style.display = "none";
        return true;
      }else{
        document.getElementById(locations3 + 'Error').style.display = "block";
        return false; 
      }
    }
    function validatePhone3(phone3){
      var ck_phone = /^[0-9]{10,11}$/;
      if(ck_phone.test(document.getElementById(phone3).value)){
        document.getElementById(phone3 + 'Error').style.display = "none";
        return true;
      }else{
        document.getElementById(phone3 + 'Error').style.display = "block";
        return false; 
      }
    }
    function validateEmail3(email3){ 
      var ck_email = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      if(ck_email.test(email3)){
        document.getElementById('email3Error').style.display = "none";
        return true;
      }else{
        return false;
      }
    }   
    function validateForm3(){
      var error = 0;
      if(!validateCompany3('company3')){
        document.getElementById('company3Error').style.display = "block";
        error++;
      }
      if(!validateName3('name3')){
        document.getElementById('name3Error').style.display = "block";
        error++;
      }
      if(!validateLocations3('locations3')){
        document.getElementById('locations3Error').style.display = "block";
        error++;
      }
      if(!validatePhone3('phone3')){
        document.getElementById('phone3Error').style.display = "block";
        error++;
      }
      if(!validateEmail3(document.getElementById('email3').value)){
        document.getElementById('email3Error').style.display = "block";
        error++;
      }
      if(error > 0){
        return false;
      }
    }           
  </script>
    <a class="close" onclick="return hide6();"><span class="icon-xout"></span></a>
