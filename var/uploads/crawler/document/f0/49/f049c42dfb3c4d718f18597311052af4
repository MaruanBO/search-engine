    <script type='text/javascript'>
    if (screen.width < 768) {
        $(document).ready(function() {
            
            $('#header-wrapper').addClass( 'show-top-nav' );

            var lastScrollTop = 0;

            if (! 'ontouchmove' in document.documentElement) {

                $(window).bind('scroll', function() {
                    var currentScrollTop = $(this).scrollTop();

                    if (currentScrollTop < lastScrollTop && ! expandedSideNav) {
                        if ($('#header-wrapper').css('position') != 'fixed') {
                            $('#header-wrapper').hide();
                            $('#header-wrapper').css('position' ,'fixed');
                            $('#header-wrapper').slideDown('fast');
                        }
                        $('#privacy_changes').css('display' ,'none');
                        $('#menu-menu-header li a').css('color' ,'#9a9a9a');
                        $(".start_trial").css('background-color','#05cc7f');
                        $(".start_trial a").css('color','#FFF');
                        $(".menu-login").css('background-color','transparent');
                        $(".menu-login a").css('color','#9A9A9A');
                    }
                    
                                        else {
                        $('#header-wrapper').css('background-color','transparent');
                        $('#header-wrapper').show('fast', function() {$('#header-wrapper').css('position' ,'absolute')});
                        $('#privacy_changes').css('display' ,'block');
                        $('#menu-menu-header li a').css('color' ,'#FFFFFF');
                        $('#menu-menu-header li .login').css('color' ,'#FFFFFF');
                        $('#menu-menu-header li ul li a').css('color' ,'#999999');
                        $(".menu-login").css('background-color','#00D2D3');
                    }
                                                        lastScrollTop = currentScrollTop;
                });

            } else if ('ontouchmove' in document.documentElement) {
                var swipeFunc = {
                    touches : {
                        "touchstart": {"x":-1, "y":-1}, 
                        "touchmove" : {"x":-1, "y":-1}, 
                        "touchend"  : false,
                        "direction" : "undetermined"
                    },
                    touchHandler: function(event) {
                        var touch;
                        if (typeof event !== 'undefined'){  
                            if (typeof event.touches !== 'undefined') {
                                touch = event.touches[0];
                                switch (event.type) {
                                    case 'touchstart':
                                    case 'touchmove':
                                        swipeFunc.touches[event.type].x = touch.pageX;
                                        swipeFunc.touches[event.type].y = touch.pageY;
                                        break;
                                    case 'touchend':
                                        swipeFunc.touches[event.type] = true;
                                        if (swipeFunc.touches.touchstart.x > -1 && swipeFunc.touches.touchmove.x > -1) {
                                            swipeFunc.touches.direction = swipeFunc.touches.touchstart.y < swipeFunc.touches.touchmove.y ? "up" : "down";
                                            switch(swipeFunc.touches.direction) {
                                                case 'undetermined':
                                                case 'up':
                                                    showTopNav();
                                                    break;
                                                case 'down':
                                                    var expandedSideNav = $('#header-wrapper').hasClass('menu-expanded');
                                                    if (! expandedSideNav) {
                                                        hideTopNav();
                                                    }
                                                    break;
                                            }
                                        }
                                    default:
                                        break;
                                }
                            }
                        }
                    },
                    init: function() {
                        document.addEventListener('touchstart', swipeFunc.touchHandler, false); 
                        document.addEventListener('touchmove', swipeFunc.touchHandler, false);  
                        document.addEventListener('touchend', swipeFunc.touchHandler, false);
                    }
                };
                swipeFunc.init();

                function showTopNav() {
                    $('#header-wrapper').removeClass('fixed');
                    $('#header-wrapper').addClass('show-top-nav');
                }
                function hideTopNav() {
                    $('#header-wrapper').removeClass('show-top-nav');
                    $('#header-wrapper').addClass('fixed');
                }
            }
        });
    }
    </script>
