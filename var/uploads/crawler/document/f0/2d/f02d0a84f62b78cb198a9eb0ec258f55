<script language="javascript">
    $(document).ready(function() {
    $("#TopPaging").html($("#BottomPaging").html());
    $('body').append('<div id="toTop">^ Back to Top</div>');
    $(window).scroll(function () {
    if ($(this).scrollTop() != 0) {
    $('#toTop').fadeIn(50);
    } else {
    $('#toTop').fadeOut(50);
    }
    });
    $('#toTop').click(function(){
    $("html, body").animate({ scrollTop: 0 }, 100);
    return false;
    });

    });
