				jQuery.extend( $.ui.autocomplete.prototype, {			
					close: function( event ) {
						var o = this;
						o.cancelSearch = true;
						if (o.options.keep_open)
						{
							o._adab_close( event );
						}
						else
						{
							o._close( event );
						}
					},
					_close: function( event ) {
						if ( this.menu.element.is( ":visible" ) ) {
							this.menu.element.hide();
							if (this.menu.blur)
							{
							this.menu.blur();
							}
							this.isNewMenu = true;
							this._trigger( "close", event );
						}
					},
					_adab_close: function( event ) {
						this._trigger( "close", event );
					}
				});
				
				var settings = {
					account  : "sp1004eca2",
					searchDomain  : "http://s.aafp.org/",
					inputElement  : "input#q",
					inputFormElement  : "form#global_search",
