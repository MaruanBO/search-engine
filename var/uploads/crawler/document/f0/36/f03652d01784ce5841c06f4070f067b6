		else if (ref.indexOf("Debit")>=0)
	       	categoryId2 += " - CADDY";
		else if (ref.indexOf("Note-Cards")>=0)
	       	categoryId2 += " - NOTECARDS";
		else if (ref.indexOf("Organizer")>=0)
	       	categoryId2 += " - ORGANIZERS";
		else if (ref.indexOf("Register")>=0)
	       	categoryId2 +=" - DEPOSIT TICKETS AND REGISTERS";
		else if (ref.indexOf("Deposit-Tickets")>=0)
			categoryId2 +=" - SLIPS";

		if(pageTitle.indexOf("Wallet")>=0)
			categoryId2 +=" - WALLET";
			
                var productId=pageURL.substring(pageURL.indexOf("/products/")+10,pageURL.indexOf("-",coremetricsClientDomain.length+7));
                pageId=productId + " - " + pageTitle;
                cmCreatePageviewTag(pageId,categoryId2);
                cmCreateProductviewTag(productId, pageTitle, categoryId2, categoryId2);
		categoryId=categoryId2;
		}
	else
		{
		lastProduct=getCookie("lastProduct");
		if(lastProduct!=null)
			{
			prodViewCatID=lastProduct.toUpperCase()+" CROSS SELL - PRODUCT DETAIL BUY";
			categoryId=prodViewCatID;
			pageId=productId + " - " + pageTitle;
                	cmCreatePageviewTag(pageId,categoryId);
			cmCreateProductviewTag(productId, pageTitle, categoryId, categoryId);
			}
		}

	prodViewCatID=categoryId;
	}
