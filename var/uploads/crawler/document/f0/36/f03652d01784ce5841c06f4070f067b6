<script type="text/javascript">

// Get environment info
function getCoremetricsEnv(hostName)
{
	if (hostName.search(/bradfordexchangechecks\.com/) > -1)	{
		coremetricsClientId = "90051220";
		coremetricsClientDomainName = "bradfordexchangechecks.com";
		coremetricsClientDomain = hostName;
		coremetricsDomain = "data.coremetrics.com";
		clientManaged = true;
	}
	else {
		// Should not reach here
		// set the client id to Test Bradford
		coremetricsClientId = "60051220";
		coremetricsClientDomainName = hostName;
		coremetricsClientDomain = hostName;
		coremetricsDomain = "testdata.coremetrics.com"
		clientManaged = false;
	}
	
}


function setCookie(NameOfCookie, value, expiredays)
{

var ExpireDate = new Date ();
ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 3600 * 1000));

document.cookie = NameOfCookie + "=" + escape(value) +
((expiredays == null) ? "" : "; expires=" + ExpireDate.toGMTString())+"; path=/; domain=.bradfordexchangechecks.com";;
}

function getCookie(NameOfCookie)
{

if (document.cookie.length > 0)
{


begin = document.cookie.indexOf(NameOfCookie+"=");
if (begin != -1) 
	{
	begin += NameOfCookie.length+1;
	end = document.cookie.indexOf(";", begin);
	if (end == -1) end = document.cookie.length;
	return unescape(document.cookie.substring(begin, end)); 
	}
}
return null;

}


function find_bread_crumb() {

        returnText='';

        var divCollection = document.getElementsByTagName("div");
        for (var i=0; i<divCollection.length; i++) {
                if(divCollection[i].innerHTML.indexOf('You Are Here:')!=-1)
                        returnText = divCollection[i].innerHTML;

        }
        return returnText;
    }

function bc_last_cat(text)
        {
        text=text.toUpperCase();
        while(text.indexOf("<A",2)!=-1)
                text = text.substring(text.indexOf("<A",2),text.length);
        text=text.substring(0,text.indexOf('A>')+2);
        text=text.substring(text.indexOf(">")+1,text.indexOf("<",1));
        return text;
        }


