function storeDataForCartPage()
	{
	var products= Array();
	var checkProduct="";
	var idx=0;
	for(i=0;i<document.getElementsByTagName("input").length;i++)
		{
		if(document.getElementsByTagName("input")[i].type == "checkbox")
			{
			if(document.getElementsByTagName("input")[i].checked == true)
				{
				//alert(document.getElementsByTagName("input")[i].name);
				if(document.getElementsByTagName("input")[i].name.indexOf('checks-')==0)
					{
					products[idx]=document.getElementsByTagName("input")[i].name.substring(7,document.getElementsByTagName("input")[i].name.length)+prodViewCatID;
					checkProduct=products[idx];
					idx++;
					}
				if(document.getElementsByTagName("input")[i].name.indexOf('cross-sell-')==0)
					{
                                        products[idx]=document.getElementsByTagName("input")[i].name.substring(11,document.getElementsByTagName("input")[i].name.length)+"Cross-sells - Check Product Detail ";
                                        if(document.getElementsByTagName("input")[i].id=="coorItemCC")
                                        	{products[idx] +="- COVERS";pageTitle2=pageTitle.replace("Personal Checks","Checkbook Cover");}
                                        if(document.getElementsByTagName("input")[i].id=="coorItemL")
                                        	{products[idx] +="- LABELS-S";pageTitle2=pageTitle.replace("Personal Checks","Return Address Label");}
                                        if(document.getElementsByTagName("input")[i].id=="coorItemNC")
                                        	{products[idx] +="- NOTECARDS";pageTitle2=pageTitle.replace("Personal Checks","Note Cards");}
//                                        if(document.getElementsByTagName("input")[i].id=="coorItem4" )
//                                        	{products[idx] +="- SLIPS";pageTitle2="Single Deposit Tickets";}
//                                        if(document.getElementsByTagName("input")[i].id=="coorItem5" )
//                                    	{products[idx] +="- CADDY";pageTitle2="Debit Card Holder";}
                                        if(document.getElementsByTagName("input")[i].id=="coorItem1" ||
                                        		document.getElementsByTagName("input")[i].id=="coorItem2" ||
                                        		document.getElementsByTagName("input")[i].id=="coorItem3"
                                        		|| document.getElementsByTagName("input")[i].id=="coorItem4" ||
                                        		document.getElementsByTagName("input")[i].id=="coorItem5" ||
                                        		document.getElementsByTagName("input")[i].id=="coorItem6" ||
                                        			document.getElementsByTagName("input")[i].id=="coorItem7" ||
                                            		document.getElementsByTagName("input")[i].id=="coorItem8")
                                		{	
                                        	var tempElementID = "";
                                        	tempElementID = document.getElementsByTagName("input")[i].id;
//                                        	var tempLabel = "label[for=" + tempElementID + "]";
//                                        	tempLabel = $(tempLabel).text();
//                                        	alert ("tempLabel222: " + tempLabel);
                                        	var tempLabel = "";
                                        	tempLabel = $("label[for=" + tempElementID + "]").text();

                                        	if (tempLabel == "Deposit Tickets")
                                                {products[idx] +="- SLIPS";pageTitle2="Single Deposit Tickets";}
                                           	if (tempLabel == "Debit Card Holder")
                                                {products[idx] +="- CADDY";pageTitle2="Debit Card Holder";}
                                            if (tempLabel == "Wallet")
                                                {products[idx] +="- WALLET";pageTitle2="Wallet";}
                                		
                                		}
                                        
					cat = products[idx].substring(10,products[idx].length);
                    cmCreateProductviewTag(document.getElementsByTagName("input")[i].name.substring(11,document.getElementsByTagName("input")[i].name.length), pageTitle2, cat, cat);
                    					idx++;

					}
				}
			}

		}
	if(products.length>0 && checkProduct.length>0)
		{
        	for(i=0;i<document.getElementsByTagName("input").length;i++)
                {
                if(document.getElementsByTagName("input")[i].type == "checkbox")
                        {
                        if(document.getElementsByTagName("input")[i].checked == true)
                                {
                                if(document.getElementsByTagName("input")[i].name.indexOf('bonusbuy')==0)
									{
	                                        products[idx]=checkProduct.substring(0,8)+"20"+"Cross-sells - Check Product Detail - COVERS";
	                                        pageTitle2=pageTitle.replace("Personal Checks","Checkbook Cover");
	                    					cat = products[idx].substring(10,products[idx].length);
	                                        cmCreateProductviewTag(checkProduct.substring(0,8)+"20", pageTitle, cat, cat);
	
	                                        idx++;
											products[idx]=checkProduct.substring(0,8)+"21"+"Cross-sells - Check Product Detail - LABELS-S";
											pageTitle2=pageTitle.replace("Personal Checks","Return Address Label");
						
											cat = products[idx].substring(10,products[idx].length);
						                    cmCreateProductviewTag(checkProduct.substring(0,8)+"21", pageTitle2, cat, cat);
											
						                    idx++;
											break;
									}
                                }
                        }

                }



		}
		for(i=0;i<document.getElementsByTagName("input").length;i++)
                {

                if(document.getElementsByTagName("input")[i].type == "hidden" && document.getElementsByTagName("input")[i].name=="prdid1")
                        {
                        products[idx]=document.getElementsByTagName("input")[i].value+prodViewCatID;
                        idx++;
                        }
		}

        //for(i=0;i<products.length;i++) products[i]+=prodViewCatID;
	storePurchaseInCookie(products);
	}
function storePurchaseInCookie(products)
	{
		var items=getCookie("buyItemsCM");
	if(items==null || items.length==0)
		{
		setCookie("buyItemsCM",products.join('|'),365);
		}
	else
		{
		
	
		for(i=0;i<products.length;i++)
			{
			if(items.indexOf(products[i].substring(0,10))==0)
				{
				if(items.indexOf("|",items.indexOf(products[i].substring(0,10)))==-1)
					items=products[i];
				else
					items=products[i]+items.substring(items.indexOf("|",items.indexOf(products[i].substring(0,10))),items.length);
				products[i]='';
				//alert("item replaced"+items);
				}
			if(items.indexOf(products[i].substring(0,10))>0)
				{
				//alert(items.substring(items.indexOf("|",items.indexOf(products[i].substring(0,10))),items.length));
				if(items.indexOf("|",items.indexOf(products[i].substring(0,10)))==-1)
                                	items=items.substring(0,items.indexOf(products[i].substring(0,10)))+products[i];
				else
					items=items.substring(0,items.indexOf(products[i].substring(0,10)))+products[i]+items.substring(items.indexOf("|",items.indexOf(products[i].substring(0,10))),items.length);
				products[i]='';
				}	

			}
		var storedProducts = items.split('|');
		
		for(i=0;i<products.length;i++)
			{
			if(products[i].length>0)
				{
				storedProducts.push(products[i]);
				}
			}
		setCookie("buyItemsCM",storedProducts.join('|'),365);
		}
	}

function storeUpsellDataForCartPage(formName)
	{
	var products = new Array();
	var productId="";
	var desc="";

	var categoryId2 = "Cross-sells - View Cart";


		if(typeof document.forms[formName].elements['desc1']!='undefined')
		{

			desc = document.forms[formName].elements['desc1'].value;
		
			if(desc.indexOf("Checks")>=0)
		        categoryId2 +=" - CHECKS";
			else if (desc.indexOf("Label")>=0 && desc.indexOf("Address")>=0)
		        categoryId2 +=" - LABELS-S";
			else if (desc.indexOf("Checkbook")>=0 && desc.indexOf("Cover")>=0)
		        categoryId2 +=" - COVERS";
		
			else if (desc.indexOf("Debit")>=0)
				categoryId2 += " - CADDY";
			else if (desc.indexOf("Note")>=0)
				categoryId2 += " - NOTECARDS";
			else if (desc.indexOf("Organizer")>=0)
				categoryId2 += " - ORGANIZERS";
			else if (desc.indexOf("Register")>=0)
				categoryId2 +=" - REGISTER";
			else if (desc.indexOf("Deposit")>=0)
		        categoryId2 +=" - SLIPS";
			else if (desc.indexOf("Wallet")>=0)
		        categoryId2 +=" - WALLET";
		
			productId=document.forms[formName].elements['prdid1'].value;
		
		
			products.push(productId+categoryId2);
			cmCreateProductviewTag(productId, desc, categoryId2, categoryId2);
		}



		if(typeof document.forms[formName].elements["bundleid1"]!='undefined')
		{

			var prefix="B"+document.forms[formName].elements['bundleid1'].value+"_Product";

			for(i=1;i<10;i++)
			{
				if(typeof document.forms[formName].elements[prefix+i+"_prdid"]!='undefined' && typeof document.forms[formName].elements[prefix+i+"_name"]!='undefined')
				{
					productId=document.forms[formName].elements[prefix+i+"_prdid"].value;
					desc=document.forms[formName].elements[prefix+i+"_name"].value;

					if(desc.indexOf(" Cover")>0)
						categoryId2 +=" - COVERS";
					if(desc.indexOf(" Label")>0)
						categoryId2 +=" - LABELS-S";


					cmCreateProductviewTag(productId, desc, categoryId2, categoryId2);
					products.push(productId+categoryId2);
				}
			}
		}

		storePurchaseInCookie(products);
		
	}


function storeUpsellDataForReorderPage(formName)
{
var products = new Array();
var productId="";
var desc="";
var price = "";
var quantity = "";

var categoryId2 = "Cross-sells - Quick Reorder";
var categoryIdInit = "Cross-sells - Quick Reorder";


	if(typeof document.forms[formName].elements['desc1']!='undefined')
	{

		desc = document.forms[formName].elements['desc1'].value;
	
		if(desc.indexOf("Checks")>=0)
	        categoryId2 +=" - CHECKS";
		else if (desc.indexOf("Label")>=0 && desc.indexOf("Address")>=0)
	        categoryId2 +=" - LABELS-S";
		else if (desc.indexOf("Checkbook")>=0 && desc.indexOf("Cover")>=0)
	        categoryId2 +=" - COVERS";
	
		else if (desc.indexOf("Debit")>=0)
			categoryId2 += " - CADDY";
		else if (desc.indexOf("Note")>=0)
			categoryId2 += " - NOTECARDS";
		else if (desc.indexOf("Organizer")>=0)
			categoryId2 += " - ORGANIZERS";
		else if (desc.indexOf("Register")>=0)
			categoryId2 +=" - REGISTER";
		else if (desc.indexOf("Deposit")>=0)
	        categoryId2 +=" - SLIPS";
	
		productId=document.forms[formName].elements['prdid1'].value;
		price=document.forms[formName].elements['price1'].value;
	
	
		products.push(productId+categoryId2);
		cmCreateProductviewTag(productId, desc, categoryId2, categoryId2);
		cmCreateShopAction5Tag(productId, desc , 1 , price, categoryId2);
		
	}



	if(typeof document.forms[formName].elements["bundleid1"]!='undefined')
	{

		var prefix="B"+document.forms[formName].elements['bundleid1'].value+"_Product";

		for(i=1;i<10;i++)
		{
			if(typeof document.forms[formName].elements[prefix+i+"_prdid"]!='undefined' && typeof document.forms[formName].elements[prefix+i+"_name"]!='undefined')
			{
				productId=document.forms[formName].elements[prefix+i+"_prdid"].value;
				desc=document.forms[formName].elements[prefix+i+"_name"].value;
				price=document.forms[formName].elements[prefix+i+"_price"].value;
		//		quantity=document.forms[formName].elements[prefix+i+"_quantity"].value;
				

				if(desc.indexOf(" Cover")>0)
					categoryId2 = categoryIdInit + " - COVERS";
				if(desc.indexOf(" Label")>0)
					categoryId2 = categoryIdInit + " - LABELS-S";


				cmCreateProductviewTag(productId, desc, categoryId2, categoryId2);
				products.push(productId+categoryId2);
				
				cmCreateShopAction5Tag(productId, desc , 1 , price, categoryId2);

				
			}
		}
	}

	storePurchaseInCookie(products);
	cmDisplayShop5s();

}



function storeDataForCoordinatedItems(name)
{
	var cat="Cross-sells";
    var ref=pageURL;



    setCookie("coordItemCM",cat,cookieExpDays);

}

function callLog(logtype, parameter1, parameter2, parameter3, parameter4, parameter5 ) {
 	  var xhReq = new XMLHttpRequest();
  	  xhReq.open("GET", "/includes/coremetrics-log.jsp?logtype=" + logtype + "&p1=" + parameter1 +"&p2=" + parameter2 + "&p3=" + parameter3 + "&p4=" + parameter4 + "&p5=" + parameter5 , false);
 	  xhReq.send(null);
	 }

function handleClick(e)
{


        e = e || event;
        var target = e.srcElement || e.target;
        if(target.parentNode.parentNode.className=='recentlyViewedInfo' || target.parentNode.className=='recentlyViewedInfo' || target.parentNode.parentNode.className=='recentlyViewedItem')
		{setCookie("recentlyViewedCM","Recently Viewed",cookieExpDays);}
	if(target.type=='submit' && pageURL.indexOf("/products/")>0)
		{
		storeDataForCartPage();
		}
//	else if(target.type=='submit' && pageURL.search(/\/reorder\/reorderCheck-crossSell.jsp\?action=/)>=0)
//	{
//		storeDataForCartPage();
//	}
	else if(!(typeof target.parentNode.name=='undefined') && target.parentNode.name.indexOf("form")==0)
		{
			if (pageURL.indexOf("reorderCheck-crossSell.jsp")>0){
				storeUpsellDataForReorderPage(target.parentNode.name);
			} else{
				storeUpsellDataForCartPage(target.parentNode.name);
			}
		}
    else if(target.parentNode.parentNode.className=="coordinatedItem")
    	{
    	storeDataForCoordinatedItems(target);
    	}


}

document.body.onclick = handleClick;




</script>
