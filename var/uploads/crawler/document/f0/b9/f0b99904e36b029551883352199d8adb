<script type="text/javascript" lang="JavaScript">
    		(function(){
			// Footer link handling
			var footerLinkToggles = $('.no-treatment').prev('h5');
			footerLinkToggles.on('click', function(e){
				e.preventDefault();
				var target = $(this).next('.no-treatment');
				if(target.hasClass('visible')) {					
					target.removeClass('visible');
					$('.caret').removeClass('flip');
				} else {
					$('.no-treatment').removeClass('visible');
					$('.caret').removeClass('flip');
					target.addClass('visible');
					$(this).find('.caret').addClass('flip');
				}
			});
		
			// SEO footer links code
	    var $seoFooterContentContainer = $("#seoFooterContentContainer"),
	        seoContentPanes = $('.seoFooterContent'),
					seoTabs = $('.seoFooterTabsItem'),
					currentID,
					currentNode;
					
			// Move content depending on screen size
			function shuffleSEOContent() {
				if($(window).width() < 753) {
					seoContentPanes.each(function(idx) {
						currentID = $(this).attr('id');
						currentNode = $(this);
						seoTabs.each(function(idx){
							if($(this).find('a').attr('id') == currentID) {
								currentNode.appendTo($(this));
							}
						});
					});
				} else {
					seoContentPanes.each(function(idx){
						$(this).appendTo('#seoFooterContentContainer');
					});
				}
			}
				
	    // Hide SEO footer tab content :: all shown initially for SEO				
			seoContentPanes.hide();
		
			// Click handling
			seoTabs.each(function(idx){
				$(this).on('click', function(e){
					e.preventDefault();
					currentID = $(this).find('a').attr('id');
					$(this).toggleClass('active').siblings().removeClass("active");
					if($(this).hasClass('active')) {
						seoContentPanes.each(function(idx){
							if($(this).attr('id') == currentID) {
								seoContentPanes.hide();
								$(this).fadeIn('slow');
								return false;
							}
						});
					} else {
						seoContentPanes.hide();
					}
					$('html, body').stop().animate({
						scrollTop: $(this).offset().top - 20
					}, 500);
				});
			});
		
			// responsive handling below
			if($('.hg-responsive').length) {
				// Check position on init
				shuffleSEOContent();
				// Window resize handler
				$(window).resize(function() { 
					shuffleSEOContent(); 
					// reset accordion
					$('.no-treatment').removeClass('visible');
					$('.caret').removeClass('flip');
				});
			}
			
			// dispatch event once loaded
			setTimeout(function(){
				$(document).trigger('hg-footer-loaded');
			}, 20);
			
		}());
</script>
<!-- mobile: Google Tag Manager -->
<noscript id="hg-google-tag-manager-ns"><iframe src="//www.googletagmanager.com/ns.html?id=GTM-WJTBCH"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<script id="hg-google-tag-manager">
    var goGoogleTagMan = function(){
        //debugger;
        (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
'//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
        })(window,document,'script','dataLayer','GTM-WJTBCH');
    }
</script>

<script>
    (function(){
        //move script tag
        var googleTagManagerNs = $('#hg-google-tag-manager-ns');
        var googleTagManager = $('#hg-google-tag-manager');
        if( googleTagManagerNs.parent().prop('tagName') !== 'BODY' ){
            //console.log('moved GTM NS from ' + googleTagManagerNs.parent().prop('tagName'));
            $('body').append($(googleTagManagerNs));

        }
        if( googleTagManager.parent().prop('tagName') !== 'BODY' ){
            //console.log('moved GTM from ' + googleTagManager.parent().prop('tagName'));
            $('body').append($(googleTagManager));

        }
        //execute it
        goGoogleTagMan();
    })();
</script>
