        $(document).ready(function () {
            $(".jqmWindow").jqm();

            $(".jqmDelete").click(function () {
                var session = new Hg.Session();
                session.KillCookie(true);
                getData();
            });

	        var debugCookieName = "hg.debugCookie";
	        $(".showCookieDebugger").click(function() {
		        var debugCookie = $.cookie(debugCookieName);
		        if (debugCookie && debugCookie.length > 0) {
			        $('#debugCookieLength').html(debugCookie.length);
		        } else {
			        $('#debugCookieLength').html(0);
		        }

		        $("#cookieDebuggerTool").show();
	        });
	        $(".showWithNoMatrix").click(function (e){
	            e.preventDefault();
	            var delimiter = (window.location.href.indexOf('?') <= -1) ? '?' : '&';
	            window.location.href += delimiter + "pdcOverride=true";
	        });
	        $(".showWithNoJsCompress").click(function (e) {
	            var delimiter = (window.location.href.indexOf('?') <= -1) ? '?' : '&';
	            window.location.href += delimiter + "hg.jsdebug=true";
	        });
	        $(".showWithNoCssCompress").click(function (e) {
	            var delimiter = (window.location.href.indexOf('?') <= -1) ? '?' : '&';
	            window.location.href += delimiter + "hg.cssdebug=true";
	        });
	        $(".hideAdDebugger").click(function () {
	            var delimiter = (window.location.href.indexOf('?') <= -1) ? '?' : '&';
	            window.location.href += delimiter + "hg.noad=true";
	        });
	        $(".informationLogging").click(function () {
	            var delimiter = (window.location.href.indexOf('?') <= -1) ? '?' : '&';
	            window.location.href += delimiter + "hg.logging.level=Information";
	        });

            $(document).on("click", "#pdcDebuggerDialog .jqmClose", function () {
                $("#pdcDebuggerDialog").hide();
            });

            $(".showPdcDebugger").click(function () {
                $("#cookieDialog").jqmHide();
                showPdcDebugger();
            });

            $(".showCacheTool").click(function () {
                if (typeof currentProviderID !== 'undefined') {
                    $("#cacheToolProviderID").val(currentProviderID);
                }
                $("#cacheTool").show();
            });
	            
            $("#clearProviderOasCache").click(function () {
                var providerID = $("#cacheToolProviderID").val();
                if (providerID) {
                    $.ajax({
                        cache: false,
                        url: "/Scheduling/InvalidateProviderCache?providerID=" + providerID,
                        dataType: 'json',
                        success: function (data) {
                            $("#cacheInvalidationStatus").html(data.items_cleared + " items removed");
                        }
                    });
                }
            });
            $(".mboxHighlighter").click(function() {
                var title = false, j = 0, H = [];
                try {
                    mboxFactoryDefault.getMboxes().each(function(x) {
                        var t = '', mt = '', nmt = '', ttl = '';
                        n = x.getName(), m = x.getDiv(), I = m.innerHTML, T = (!m.id.match('Impo')) ? 'mbox' : 'mbox-Offer', D = (T == 'mbox'), E = typeof(window.ttMBX) != 'undefined', O = 'default', U = decodeURI(x.getURL()), c = document.createElement('code');
                        c.innerHTML = I.split("&").join("&amp;").split("<").join("&lt;").split(">").join("&gt;").split('"').join("&quot;").split(" ").join("&nbsp;").split("&lt;!--").join("<br><br>&lt;!--").split("--&gt;").join("--&gt;<br>");
                        c.style.border = '1px solid red';
                        c.style.display = 'inline-block';
                        c.style.cursor = 'auto';
                        H[j] = c;
                        m.style.border = '2px solid red';
                        m.style.boxShadow = '10px 10px 5px #888';
                        if (!D) {
                            O = I.split('fer Id: ')[1].split('--')[0];
                        }
                        if (E) {
                            M = ttMBX(x);
                            (!M) ? mt += 'mbox: ' + n + '<br>(deactivated)' : '';
                            for (i in M) {
                                mt += i + ': ' + eval('M.' + i) + '&#10;';
                                if (!title) {
                                    mt += '<br>';
                                }
                            }
                        }
                        nmt += 'mbox: ' + n + '<br>&#10;offer: ' + O;
                        if (E) {
                            if (title) {
                                ttl += 'title="' + mt + '"style="cursor:help;color:red"';
                                t = nmt;
                            } else {
                                (mt == 'deact') ? t += nmt + '<br><i>' + mt + '</i>' : t = mt;
                            }
                        } else {
                            t = nmt;
                        }
                        m.innerHTML = '<h2 ' + ttl + 'style="cursor:pointer;color:red"onclick=this.appendChild(H[' + j + '])>' + t + '</h2>' + I;
                        j++;
                    });
                } catch(e) {
                    alert('None');
                }
                $(".jqmWindow").jqmHide();
                $(".jqmWindow").hide();
            });
            $(".omDebugger").click(function() {
                window.open("", "dp_debugger", "width=600,height=600,location=0,menubar=0,status=1,toolbar=0,resizable=1,scrollbars=1").document.write("<script language='JavaScript' id=dbg src='https://www.adobetag.com/d1/digitalpulsedebugger/live/DPD.js'></" + "script>");

                $(".jqmWindow").jqmHide();
                $(".jqmWindow").hide();
            });

            // Optimus Test harness section
            var $editTestHarness = $("#editTestHarness"),
                simulatorCookie = $.multiValueCookie('hg.mbox.simulator');

            if (simulatorCookie && simulatorCookie.values) {
                var host = simulatorCookie.values.host;
                $editTestHarness.find("#host").val(host);
            }

            $(".optimusTestHarness").click(function () {
                $('#testHarnessForm').slideToggle();
            });
            $(".hideTestHarnessForm").click(function () {
                $('#testHarnessForm').slideToggle();
            });

            $editTestHarness.on("submit", function (event) {
                var host = $editTestHarness.find("#host").val();
                $.multiValueCookie('hg.mbox.simulator').setValue('host', host).save({ expires: 356 });

                event.preventDefault();
            });
            // Optimus Test harness section

	        $(document).on('keyup', function(e) {
                if (e.keyCode == 192 && !e.shiftKey) {
                    if ($("#cookieDialog").is(":visible")) {
                        $("#cookieDialog").jqmHide();
                    }
                    else {
                        $("#cookieDialog").jqmShow();
                    }
                    getData();
                } else if (e.keyCode == 192 && e.shiftKey) {
                    showPdcDebugger();
                }
            });

	        function showPdcDebugger() {
                if ($("#pdcDebuggerDialog").is(":visible")) {
                    $("#pdcDebuggerDialog").jqmHide();
                }
                else {
                    if (typeof (listOfPdcCalls) != "undefined") {
                        showListSimple(listOfPdcCalls, "#targetingCalls", "Targeting Calls");
                    }

                    if (typeof (pdcDebugMarketData) != "undefined") {
                        showListSimple(pdcDebugMarketData, "#pdcSelectMarketShareData", "Sponsorship Data");
                    }

                    if (typeof (HgTargeting) != "undefined" && HgTargeting != null && HgTargeting.Response != null) {
                        $("#pdcTargetingGrid .layout").html(HgTargeting.Response.Layout);

                        if (HgTargeting.Response.SponsorCodes == null) {
                            $("#pdcTargetingGrid .sponsor").html("None--<strong>Appears to be in an Error State</strong>");
                        }
                        else {
                            var listOfSponsors = HgTargeting.Response.SponsorCodes.join();
                            $("#pdcTargetingGrid .sponsor").html(listOfSponsors);
                            $("#pdcTargetingGrid .sponsorFound").html(HgTargeting.Response.SponsorFound ? HgTargeting.Response.SponsorFound.toString() : 'false');
                            $("#pdcTargetingGrid .facilityCode").html(HgTargeting.Response.FacilityCode);
                            $("#pdcTargetingGrid .practiceCode").html(HgTargeting.Response.PracticeCode);
                            $("#pdcTargetingGrid .officeCode").html(HgTargeting.Response.OfficeCode);
                            $("#pdcTargetingGrid .renderDartTag").html(HgTargeting.Response.RenderPdcDartTag ? HgTargeting.Response.RenderPdcDartTag.toString() : 'false');
                            
                        }
                    }

                    $("#pdcDebuggerDialog").show();
                }
            }

            function showListSimple(list, targetDiv, name) {
                $(targetDiv).html("<h4>" + name + "</h4>");

                var ul = $('<ol/>').addClass('debuggerSimpleList').appendTo($(targetDiv));                    
                for (var i = 0; i < list.length; i++) {
                        
                    var subList = list[i];
                    for (var j = 0; j < subList.length; j++) {
                        var item = subList[j];
                        var li = $("<li/>").appendTo(ul);
	                    $('<a/>').appendTo(li)
	                        .attr('target', '_blank')
	                        .attr('href', item.TraceUri)
	                        .html(item.TraceUri 
                                + '<br/>length: ' + item.ContentLength
                                + ' - elapsed: ' + item.ElapsedMilliseconds);
	                }
                }
            }
                
            function showList(list, targetDiv, name) {
                if (!list) {
                    return;
                }
                $(targetDiv).html("");

                $.each(list, function (index, val) {
                    $(targetDiv).jsonviewer({
                        json_name: name,
                        json_data: "<a href='" + val + "' target=_blank>" + val + "</a>"
                    });
                });

                $(targetDiv).find(".ui-widget-content").each(function () {
                    // Remove the "No Wrap" style for these elements.
                    var style = $(this).attr("style");
                    $(this).attr("style", style.replace("nowrap", ""));
                });
            }

            function getData() {
                if (typeof listOfObjectCounts !== 'undefined' && listOfObjectCounts.length > 1)
                    showList(listOfObjectCounts, "#objectCounter", "ObjectCounter");
                else
                    $("#objectCounter").hide();

                if (typeof listOfSolrCalls !== 'undefined' && listOfSolrCalls.length > 0) {
                    showListSimple(listOfSolrCalls, "#solrquery", "Service and Data Calls: WIN-T5N3D6AS4Q9");
                }
                if (typeof listOfTridionCalls !== 'undefined' && listOfTridionCalls.length > 0) {
                    showList(listOfTridionCalls, "#tridionQueries", "Tridion Query Urls");
                }

                var session = new Hg.Session();
                $('#searchparamsdebug').html("");
                $('#searchparamsdebug').jsonviewer({
                    json_name: 'Provider Search Parameters', // specifies a display name for the JSON object
                    json_data: session.ProviderSearchParameters    // specifies the JSON object data
                });

                $('#sessiondebug').html("");
                $('#sessiondebug').jsonviewer({
                    json_name: 'User Session', // specifies a display name for the JSON object
                    json_data: session.UserSession    // specifies the JSON object data
                });

                $(".jqmWindow").find(".ui-widget-header").click(); // Hide the solr query sections by default
            }

            $(".urlNavigator").change(function () {
                window.location = $(this).val();
            });

            $(".showRenderTools").click(function () {
                $("body").addClass("renderToolEngaged");

                $(".componentPresentation").each(function () {
                    if ($(this).prev("div.renderToolContent").length == 0) { // Don't keep re-adding this
                        var viewName = $(this).attr("data-view-name");

                        if (!viewName) {
                            viewName = "_" + $(this).attr("id") + "Partial";
                        } else {
                            viewName = "_" + viewName + "Partial (Tridion Template: " + $(this).attr("id") + ")";
                        }

                        $(this).before("<div class='renderToolData'><h4 class='renderToolTitle'>View Name: " + viewName + "</h4></div>");
                    }
                });

                $(".renderToolContent:not(:visible) .renderToolDetail").before("<a class='showRenderToolDetail'>Details...</a>");
                $(".renderToolContent").slideDown(1500);

                $(".renderToolContent[data-viewname]").each(function () {
                    if ($(this).attr("data-placementcode") != "undefined") {
                        if ($(this).prev("div.renderToolData").length == 0) { // Don't keep re-adding this
                            $(this).addClass("renderToolRenderedContent");
                            $(this).before("<div class='renderToolData'><h4 class='renderToolTitle'>Placement Code:" + $(this).attr("data-placementcode") + "</h4><h4>View Name: " + $(this).attr("data-viewname") + "Presentation</h4></div>");
                        }
                    }
                });

                $(".jqmWindow").jqmHide();
                $(".jqmWindow").hide();
            });

                
            $(".showPsrScoreDebugger").click(function () {
                if (document.location.search.indexOf("hg.psr.score.debugger") < 0) {
                    if (document.location.search.length > 0) {
                        document.location.search += "&hg.psr.score.debugger=true";
                    } else {
                        document.location.search = "hg.psr.score.debugger=true";
                    }
                }
                else {
                    document.location.search = "";
                }
            });

            $(".showContentLocator").click(function () {
                if (document.location.search.indexOf("hg.content.locator") < 0) {
                    if (document.location.search.length > 0) {
                        document.location.search += "&hg.content.locator=true";
                    } else {
                        document.location.search = "hg.content.locator=true";
                    }
                }
                else {
                    document.location.search = "";
                }
            });

            $(document).on("click", ".showRenderToolDetail", function () {
                $(this).next(".renderToolDetail").toggle();
            });

            function growDebugCookie(growBySize) {
                var debugCookie = $.cookie(debugCookieName);
                if (debugCookie == null) {
                	debugCookie = "";
                }
                var newCookieValue = debugCookie;
                for (var countSize = 0; countSize < growBySize; countSize++) {
                	newCookieValue += 'a';
                }
                $.cookie(debugCookieName, newCookieValue, { path: "/" });
                $('#debugCookieLength').html($.cookie(debugCookieName).length);
            }
				
	        $('#growCookieButton').click(function() {
		        var growBySize = $.trim($('#growCookieByLength').val());
		        growDebugCookie(growBySize);
	        });

	        $('#clearCookieButton').click(function() {
	            $.cookie(debugCookieName, "", { path: "/" });
		        $('#debugCookieLength').html(0);
	        });

	        $(document).on("targetingReady", function (e, result) {
                if (result.success == true) {
                    $(result.listOfPlacementsOnPageWithNoData).each(function () { $("#listOfPlacementsOnPageWithNoData").append("<div>" + this + "</div>"); });
                    $(result.listOfPlacementsOnPageWithData).each(function () { $("#listOfPlacementsOnPageWithData").append("<div>" + this + "</div>"); });
                    $(result.contentHtml).children("[id]").each(function () { $("#listOfAllMatrixData").append("<div>" + $(this).attr("id") + "</div>"); });
                }
                else {
                    $("#pdcUrl").css("background: red;");
                }
	        });

	        $('#setSponsorCode').val(getParameterByName('Sponsor'));
	        $('#setFacilityCode').val(getParameterByName('Facility'));
	        if ($('#setSponsorCode').val() != '' || $('#setFacilityCode').val() != '')
	            $('#sponsorFacilityCodesLink').show();
	        $('#setSponsorFacilityCodes input').on('keyup', function (e) {
	            setCodesLink();
            });
            $('#setSponsorFacilityCodes input').on('change', function(e) {
                setCodesLink();
            });
            function setCodesLink() {
                var sponsorCode = $.trim($('#setSponsorCode').val());
                var facilityCode = $.trim($('#setFacilityCode').val());
                var isLinkVisible = sponsorCode.length > 0 || facilityCode.length > 0 ? true : false;
                if (isLinkVisible) {
                    var location = getCodesLocation(sponsorCode, facilityCode);
                    $('#sponsorFacilityCodesLink').attr('href', location);
                    $('#sponsorFacilityCodesLink').show();
                } else {
                    $('#sponsorFacilityCodesLink').hide();
                }
            }
            function getCodesLocation(sponsorCode, facilityCode) {
                var search = window.location.search;
                if (search == '' && location.hash != '')
                    search = window.location.hash;

                search = setSearch(search, 'Sponsor', sponsorCode, getParameterByName('Sponsor'));
                search = setSearch(search, 'Facility', facilityCode, getParameterByName('Facility'));

                return window.location.pathname + search;

                function setSearch(searchQuery, name, val, curVal) {
                    if (curVal != '') {
                        var replaceVal = val.length > 0 ? name + '=' + val : '';
                        searchQuery = searchQuery.replace(name + '=' + curVal, replaceVal);
                        searchQuery = searchQuery.replace(/&$/, ''); // Remove trailing '&'
                    } else {
                        if (val != '') {
                            if (searchQuery.length == 0) {
                                searchQuery = '?';
                            } else {
                                searchQuery += '&';
                            }
                            searchQuery += name +'='+ val;
                        }
                    }
                    return searchQuery;
                }
            }
        });
    </script>
