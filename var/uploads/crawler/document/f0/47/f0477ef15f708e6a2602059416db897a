        <script type="text/javascript" language="javascript">

            function ShowDivCartEmpty() {
                var objDvCartEmpty = document.getElementById("divProgress");

                if (objDvCartEmpty.style.display == "block") {
                    objDvCartEmpty.style.display = "none";
                }
                else {
                    var dvCartDetail = document.getElementById("divMiddle");
                    dvCartDetail.style.top = ((document.documentElement.clientHeight / 2) + document.documentElement.scrollTop) - 20 + 'px';
                    dvCartDetail.style.left = (document.documentElement.clientWidth / 2) - 60 + 'px';
                    dvCartDetail.style.display = "inline";
                    objDvCartEmpty.style.height = '740px';
                    objDvCartEmpty.style.top = 0;
                    objDvCartEmpty.style.left = 0;
                    objDvCartEmpty.style.display = "block";
                    centerPopup();
                    loadPopup();
                }
            }

            function HideCartPopUp() {
                var objdvCartEmpty = document.getElementById("divProgress");

                if (objdvCartEmpty.style.display == "block") {
                    objdvCartEmpty.style.display = "none";
                    disablePopup();

                }
                return false;

            }

            function ShowDivProcessBundle() {

                var objDP = document.getElementById("divProgressBundle");
                if (objDP.style.display == "block") {
                    objDP.style.display = "none";

                }
                else {
                    objDivMiddle = document.getElementById("divMiddleBundle");
                    objDivMiddle.style.top = ((document.documentElement.clientHeight / 2) + document.documentElement.scrollTop) - 20 + 'px';
                    objDivMiddle.style.left = (document.documentElement.clientWidth / 2) - 60 + 'px';
                    objDivMiddle.style.display = "inline";
                    objDP.style.height = '666px';
                    objDP.style.top = 0;
                    objDP.style.left = 0;
                    objDP.style.display = "block";

                }
            }


            //centering popup
            function centerwaitingPopup() {
                var wh = document.documentElement.clientHeight;
                ////only need force for IE6
                $("#backgroundPopupWaiting").css({
                    "height": wh
                });
            }


            //disabling popup with jQuery magic!
            function disableWaitingPopup() {
                //disables popup only if it is enabled
                if (popupWaitingStatus == 1) {
                    $("#backgroundPopupWaiting").fadeOut("slow");
                    $("#divWaiting").fadeOut("slow");
                    popupWaitingStatus = 0;
                }
            }

            //SETTING UP OUR POPUP
            //0 means disabled; 1 means enabled;
            var popupWaitingStatus = 0;
            //loading popup with jQuery magic!
            function loadWaitingPopup() {

                //loads popup only if it is disabled
                if (popupWaitingStatus == 0) {
                    $("#backgroundPopupWaiting").css({
                        "opacity": "0.5"
                    });
                    $("#backgroundPopupWaiting").fadeIn("slow");
                    $("#divWaiting").fadeIn("slow");
                    popupWaitingStatus = 1;
                }
            }

            //SETTING UP OUR POPUP
            //0 means disabled; 1 means enabled;
            var popupStatus = 0;
            //loading popup with jQuery magic!
            function loadPopupBundle() {

                //loads popup only if it is disabled
                if (popupStatus == 0) {
                    $("#backgroundPopup").css({
                        "opacity": "0.7"
                    });
                    $("#backgroundPopup").fadeIn("slow");
                    $("#divProgressBundle").fadeIn("slow");
                    popupStatus = 1;
                }
            }

            //disabling popup with jQuery magic!
            function disablePopupBundle() {
                //disables popup only if it is enabled
                if (popupStatus == 1) {
                    $("#backgroundPopup").fadeOut("slow");
                    $("#divProgressBundle").fadeOut("slow");
                    popupStatus = 0;
                }
            }

            //centering popup
            function centerPopupBundle() {
                var windowHeight = document.documentElement.clientHeight;
                ////only need force for IE6
                $("#backgroundPopup").css({
                    "height": windowHeight
                });
            }

            $(".summary").each(function () {
                $(this).next().toggle(this.scrollHeight > this.offsetHeight);
            });

            $(".author_summary").each(function () {
                $(this).next().toggle(this.scrollHeight > this.offsetHeight);
            });

        
        </script>
        <script async src="Scripts/cycle.js" type="text/javascript"></script>
        <script language="javascript" type="text/javascript">
     
            // Open Blank Window
            $(function () {
                $('a.viewWebsite').click(function () {
                    window.open(this.href);
                    return false;
                });
            });

            // Smooth Scrolling
            $(document).ready(function () {
                $('a[href*=#]').click(function () {
                    if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '')
                      && location.hostname == this.hostname) {
                        var $target = $(this.hash);
                        $target = $target.length && $target
                          || $('[name=' + this.hash.slice(1) + ']');
                        if ($target.length) {
                            var targetOffset = $target.offset().top;
                            $('html,body')
                              .animate({ scrollTop: targetOffset }, 200);
                            return false;
                        }
                    }
                });
            });

            //-->
     
            $(window).load(function () {
                $('#fW_Content').cycle({
                    //fx:    'zoom',
                    //pager: '#fW_Controls',
                    fx: 'fade',
                    speed: 1500,
            timeout: 9000,
            next: '#next',
            prev: '#prev'
        });

        $('#pause').click(function () { $('#fW_Content').cycle('pause'); $('#pause').hide(); $('#play').show(); return false; });
        $('#play').click(function () { $('#fW_Content').cycle('resume'); $('#play').hide(); $('#pause').show(); return false; });
    });

    function AddBundleItem(a,idBundle, idSite, warehouseDesc, bundleDescription) {
        PageMethods.addbundleitem(idBundle,idSite, warehouseDesc, bundleDescription, pmSuccess, pmError);

        return false;
    }

    function pmSuccess(result) {
        //alert("Su:"+result);
        var txtArray = result.split("~~^~~");
        if (txtArray[0] != "") {
            $('#spanItemCount').html(txtArray[0]);
            $('#spanItemPrice').html(txtArray[1]);
            $('#' + 'ctl00_ContentPlaceHolder1_divAddToCartInnerHtmlProgressBundle').html(txtArray[2]);
              $('#spanCheckOut').html('<a href="http://www.thriftbooks.com/cart/shoppingcart.aspx" class="checkout">Checkout</a>');
              $('#spanCartImg').html('<a href="http://www.thriftbooks.com/cart/shoppingcart.aspx"><img src="http://www.thriftbooks.com/images/cart.png" alt="" /></a>');

              ShowDivBundleDetail();
              ShowDivProcessBundle();
              $('#divMiddleBundle').show();
              centerPopup();
              loadPopupBundle();
          }

      }
      function pmError(result) {
          //alert('Err:' +  result.get_message());
          location.href = location.href
      }
      
      

      function ShowDivWaiting(str) {

          var objDivWaiting = document.getElementById("divWaiting");

          if (objDivWaiting.style.display == "block") {
              objDivWaiting.style.display = "none";
              if (str != "delete")
                  ShowDivProcess(str);
              centerwaitingPopup();
              loadWaitingPopup();
              disableWaitingPopup();
          }
          else {
              //alert(document.documentElement.clientHeight + ' ' + document.documentElement.scrollTop);
              objdivWaitingMiddle = document.getElementById("divWaitingMiddle");
              objdivWaitingMiddle.style.top = (document.documentElement.clientHeight / 2) + 'px';
              objdivWaitingMiddle.style.left = (document.documentElement.clientWidth / 2) - 60 + 'px';
              objdivWaitingMiddle.style.display = "block";
              //alert(objDivMiddle.style.top);
              objDivWaiting.style.height = document.body.offsetHeight + 'px';
              objDivWaiting.style.top = 0;  //getScrollTop();
              objDivWaiting.style.left = 0;
              objDivWaiting.style.display = "block";
              centerwaitingPopup();
              loadWaitingPopup();
          }
      }

      function ShowDivBundleDetail() {
          var objDivBundleDetailProgress = document.getElementById("divBundleDetailProgress");

          if (objDivBundleDetailProgress.style.display == "block") {
              objDivBundleDetailProgress.style.display = "none";
              popupStatus = 1;
              disableBundleDetailPopup();
          }
          else {
              var dvBundleDetail = document.getElementById("ctl00_ContentPlaceHolder1_dvBundleDetail");
              dvBundleDetail.style.top = ((document.documentElement.clientHeight / 2) + document.documentElement.scrollTop) - 20 + 'px';
              dvBundleDetail.style.left = (document.documentElement.clientWidth / 2) - 60 + 'px';
              dvBundleDetail.style.display = "inline";
              objDivBundleDetailProgress.style.height = '740px';
              objDivBundleDetailProgress.style.top = 0;
              objDivBundleDetailProgress.style.left = 0;
              objDivBundleDetailProgress.style.display = "block";

          }
      }

      function ShowDivProcess() {
          var objDivProcess = document.getElementById("divProgress");
          if (objDivProcess.style.display == "block") {
              objDivProcess.style.display = "none";
          }
          else {
              objDivMiddle = document.getElementById("divMiddle");
              objDivMiddle.style.top = ((document.documentElement.clientHeight / 2) + document.documentElement.scrollTop) - 20 + 'px';
              objDivMiddle.style.left = (document.documentElement.clientWidth / 2) - 60 + 'px';
              objDivMiddle.style.display = "inline";
              objDivProcess.style.height = document.body.offsetHeight + 'px';
              objDivProcess.style.top = 0;
              objDivProcess.style.left = 0;
              objDivProcess.style.display = "block";
          }
      }
      function HideFasterShippingDetail(a) {
          var loader = a.id.replace("btnKeepShopping", "divAddtoCart")
          $('#' + loader).hide();
          ShowDivProcess();
          disablePopup();
          return false;
      }

      //centering popup
      function centerwaitingPopup() {
          var windowHeight = document.documentElement.clientHeight;
          ////only need force for IE6
          $("#backgroundPopupWaiting").css({
              "height": windowHeight
          });
      }


      //disabling popup with jQuery magic!
      function disableWaitingPopup() {
          //disables popup only if it is enabled
          if (popupWaitingStatus == 1) {
              $("#backgroundPopupWaiting").fadeOut("slow");
              $("#divWaiting").fadeOut("slow");
              popupWaitingStatus = 0;
          }
      }

      //SETTING UP OUR POPUP
      //0 means disabled; 1 means enabled;
      var popupWaitingStatus = 0;
      //loading popup with jQuery magic!
      function loadWaitingPopup() {

          //loads popup only if it is disabled
          if (popupWaitingStatus == 0) {
              $("#backgroundPopupWaiting").css({
                  "opacity": "0.5"
              });
              $("#backgroundPopupWaiting").fadeIn("slow");
              $("#divWaiting").fadeIn("slow");
              popupWaitingStatus = 1;
          }
      }

      //SETTING UP OUR POPUP
      //0 means disabled; 1 means enabled;
      var popupStatus = 0;
      //loading popup with jQuery magic!
      function loadPopup() {

          //loads popup only if it is disabled
          if (popupStatus == 0) {
              $("#backgroundPopup").css({
                  "opacity": "0.7"
              });
              $("#backgroundPopup").fadeIn("slow");
              $("#divProgress").fadeIn("slow");
              popupStatus = 1;
          }
      }

      function loadBundleDetailPopup() {

          //loads popup only if it is disabled
          if (popupStatus == 0) {
              $("#backgroundPopup").css({
                  "opacity": "0.7"
              });
              $("#backgroundPopup").fadeIn("slow");
              $("#divBundleDetailProgress").fadeIn("slow");
              popupStatus = 1;
          }
      }

      //disabling popup with jQuery magic!
      function disablePopup() {
          //disables popup only if it is enabled
          if (popupStatus == 1) {
              $("#backgroundPopup").fadeOut("slow");
              $("#divProgress").fadeOut("slow");
              popupStatus = 0;
          }
      }

      //disabling popup with jQuery magic!
      function disableBundleDetailPopup() {
          //disables popup only if it is enabled
          if (popupStatus == 1) {
              $("#divBundleDetailProgress").fadeOut("slow");
              popupStatus = 0;
          }
      }

      //centering popup
      function centerPopup() {
          var windowHeight = document.documentElement.clientHeight;
          ////only need force for IE6
          $("#backgroundPopup").css({
              "height": windowHeight
          });
      }

      var SlidingBookListDisplayLength = 4;
      var recLists;

      $(document).ready(function () {
          recLists = jQuery.parseJSON($("#ctl00_ContentPlaceHolder1_hdnBookLists").val());

                var recLength = recLists.length;

                $.each(recLists, function (index, value) {
                    //create book list for each list returned w/ more than 0 books
                    if (index < recLength - 1 && recLists[index].length > 0) {
                        var newBBItem = $("#homeSliderItemModel .homeSliderBookList").clone();
                        $(newBBItem).find(".homeSliderBookList_title a").text(recLists[recLength - 1][index].Title);

                        if (recLists[recLength - 1][index].SeeAllUrl != null) {
                            $(newBBItem).find(".homeSliderBookList_link")[0].href = recLists[recLength - 1][index].SeeAllUrl;
                            $(newBBItem).find(".homeSliderBookList_link").show();
                        }

                        newBBItem.append("<div class='homeSliderBookListContainer'></div>");
                        var recBookListContainer = $(newBBItem).find(".homeSliderBookListContainer");

                        $.each(recLists[index], function (index2, value2) {
                            if (index2 < SlidingBookListDisplayLength) {
                                recBookListContainer.append(CreateHomeSliderItem(recLists[index][index2]));
                            }
                        });

                        recLists[recLength - 1][index].scrollPos = 0;

                        //bb_arrow_left bb_arrow_right
                        $(newBBItem).find(".bb_arrow_right").click(function () {
                            LoadHomeSliderBooksFromList(index, $(this), 1);
                        });

                        //bb_arrow_left bb_arrow_right
                        $(newBBItem).find(".bb_arrow_left").click(function () {
                            LoadHomeSliderBooksFromList(index, $(this), -1);
                        });

                        $(newBBItem).find(".bb_arrow_left").mouseover(function () {
                            $(this).attr("src", "/images/ar_left2.png");
                        });

                        $(newBBItem).find(".bb_arrow_left").mouseout(function () {
                            $(this).attr("src", "/images/ar_left.png");
                        });

                        $(newBBItem).find(".bb_arrow_right").mouseover(function () {
                            $(this).attr("src", "/images/ar_right2.png");
                        });

                        $(newBBItem).find(".bb_arrow_right").mouseout(function () {
                            $(this).attr("src", "/images/ar_right.png");
                        });

                        if (recLists[index].length <= SlidingBookListDisplayLength) {
                            $(newBBItem).find(".bb_arrow_left").hide();
                            $(newBBItem).find(".bb_arrow_right").hide();
                        }


                        $("#divSlidingBookLists").append(newBBItem);
                        $("#divSlidingBookLists").append(newBBItem);
                        $("#divSlidingBookLists").append("<div style='clear:both;'></div>");
                    }
                });
            });


          function CreateHomeSliderItem(item) {
              if (item.ImageUrlMedium == null) {
                  item.ImageUrlMedium = "/images/no-image.jpg";
              }

              var itemURL = "/w/" + item.WorkCleanName.toLowerCase() + "/" + item.idWork.toString() + "/";

              var newBBItem = $("#homeSliderItemModel .homeSliderListItem").clone();
              $(newBBItem).find("a")[0].href = itemURL;

              $(newBBItem).find(".price").text("$" + item.ThriftBookPrice);
              $(newBBItem).find(".author").text(item.Authors);
              $(newBBItem).find(".bbImage")[0].src = item.ImageUrlMedium;
              $(newBBItem).find(".bbImage")[0].alt = item.ProductName;
              $(newBBItem).find(".bbImage")[0].title = item.ProductName;
              $(newBBItem).find(".bargain_summary h4").text(item.ProductName);
              $(newBBItem).find(".mediaType").text(item.Media);
              return newBBItem;
          }

            
          function ShowBundlePopUp(idBundle) {
              var dvBundleDetail = document.getElementById("ctl00_ContentPlaceHolder1_dvBundleDetail");
                dvBundleDetail.id = dvBundleDetail.id.replace("ctl00_ctl00_", "ctl00_");
                $('#fW_Content').cycle('pause'); $('#pause').hide(); $('#play').show();
                $.ajax({
                    type: "POST",
                    url: "calldata.aspx/getbundledetail",
                    data: '{idBundle:"' + idBundle + '",id:"' + dvBundleDetail + '"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        var text = data.d;
                        var txtArray = text.split("~~^~~");
                        dvBundleDetail.innerHTML = txtArray[0];
                        ShowDivBundleDetail();
                        $('#dvBundleDetail').show();
                        centerPopup();
                        //load popup
                        loadBundleDetailPopup();
                    },
                    error: function (request, status, error) {
                        var errortext = request.responseText;
                    }
                });
            }


            function LoadHomeSliderBooksFromList(ListIndex, clicked, scrollIndexChange) {
                var scrollPost = recLists[recLists.length - 1][ListIndex].scrollPos;
                recLists[recLists.length - 1][ListIndex].scrollPos = scrollPost + scrollIndexChange;
                scrollPost = scrollPost + scrollIndexChange;
                //            recLists[ListIndex];


                var maxItemIndex = recLists[ListIndex].length - 1;
                var startIndex = (scrollPost % Math.ceil(recLists[ListIndex].length / SlidingBookListDisplayLength)) * SlidingBookListDisplayLength;

                if (startIndex < 0) {
                    startIndex = maxItemIndex + startIndex + 1;
                }

                var endIndex = startIndex + (SlidingBookListDisplayLength - 1);

                if (endIndex > maxItemIndex) {
                    endIndex = maxItemIndex;
                }

                var recBookListContainer = $(clicked).closest(".homeSliderBookList").find(".homeSliderBookListContainer");

                $(recBookListContainer).fadeOut(350, 0, function () {
                    $(recBookListContainer).text("");

                    $.each(recLists[ListIndex], function (i, value) {
                        if (i >= startIndex && i <= endIndex) {
                            $(recBookListContainer).append(CreateHomeSliderItem(recLists[ListIndex][i]));
                        }
                    });

                    $(recBookListContainer).fadeIn(350, 0);
                });
            }

            dataLayer.push({
                'event': 'HomePage'
            });
    </script>

        </div>
        
