<div class="jquery_helper"></div>
 
<script>

    var imgPaths = ['../content/images/sequences/homepage_animation_6a.gif',
                    '../content/images/sequences/homepage_animation_6b.gif'];
    var imgList = [];

    (function ($) {
        $.extend({
            preload: function (imgArr, option) {
                var setting = $.extend({
                    init: function (loaded, total) { },
                    loaded: function (img, loaded, total, bild) { },
                    loaded_all: function (loaded, total) { }
                }, option);
                var total = imgArr.length;
                var loaded = 0;

                setting.init(0, total);
                for (var i = 0; i < total; i++) {
                    // wrap in function to get access to the right i
                    (function (img) {
                        imgList[img] = ($("<img />")
                            .attr("src", imgArr[img])
                            .load(function () {
                                loaded++;
                                setting.loaded(this, img, total, this.src);  // img instead of i
                                if (loaded == total) {
                                    setting.loaded_all(loaded, total);
                                }
                            })
                        );
                    })(i);
                }
            }
        });
    })(jQuery);

    $(document).ready(function () {
        if ($(".jquery_helper").css("float") == "left") {
            $(function () {
                $.preload(imgPaths,
                    {
                        init: function (loaded, total) {
                            var z = 0;
                            while (z < total) {
                                z++;
                                $("section nav").append('<img id="e' + z + '" src="all/blank.png" alt=""/>');
                            }
                        },
                        loaded: function (img, loaded, total, bild) {
                            $("#e" + loaded).prop("src", bild);
                        },
                        loaded_all: function (loaded, total) {
                            //all done!
                            //console.log('complete');
                            runAnimation();
                        }
                    });
            });
        };
    });

    function runAnimation() {

        $(".man_standing_home").attr('src', imgPaths[0]);
        var myAnim = setTimeout(function () {
            $(".man_standing_home").attr('src', imgPaths[1]);
            clearTimeout(myAnim);
        }, 13250);
    }
</script>

