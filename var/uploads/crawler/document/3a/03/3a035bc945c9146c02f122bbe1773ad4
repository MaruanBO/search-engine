    <script type="text/javascript">
        //<![CDATA[
        var isCookied = 'false';

        /* Some utility functions */
        function getCookie(c_name)
        {
            if (document.cookie.length > 0)
            {
                c_start = document.cookie.indexOf(c_name + "=");
                if (c_start != -1)
                {
                    c_start = c_start + c_name.length + 1;
                    c_end = document.cookie.indexOf(";", c_start);
                    if (c_end == -1) c_end = document.cookie.length;
                    return document.cookie.substring(c_start, c_end);
                }
            }
            return "";
        }

        function addEvent(newEvent)
        {
            if (newEvent != "" ) {
                if ( typeof(s.events) != 'undefined' && s.events != "") {
                    s.events += "," + newEvent;
                } else {
                    s.events = newEvent;
                }
            }
        }

        var CONSUMER = "Consumer";
        s.linkInternalFilters = "";

        /* SiteCatalyst Variables */
        var hostName = window.location.hostname;
        var channel = CONSUMER;

        if (hostName.indexOf("shopping") == 0) {
            channel = "Shopping";
        } else if ( hostName.indexOf("corporate") == 0) {
            channel = "Corporate";
        } else if ( hostName.indexOf("search") == 0) {
            channel = "Search";
        } else if ( hostName.indexOf("business") != -1) {
            channel = "Business";
        }

        //set the channel
        s.channel = channel;
        s.eVar10 = s.channel;

        //page name
        s.pageName="Cons:Home";
        if ( typeof(screenName) != 'undefined' && screenName != "") {
            s.pageName += ":" + screenName;
        }
        //Add any page suffixes
        if ( typeof( "" ) != 'undefined' && "" != "") {
            s.pageName += ":" + "";
        }

        addEvent("event14");

        //Site sub-section
        var path = window.location.pathname;
        var end = path.lastIndexOf("/",path.length-2);
        if ( end != -1) {
            s.prop1 = path.slice(1,end);
        }
        if ( s.prop1 == "") {
            s.prop1 = "home";
        }
        s.eVar12 = s.prop1;

        //Error Page Tracking
        if ( typeof(omniErrorStr) != 'undefined') {
            s.prop4 = s.pageName;
            s.prop5 = omniErrorStr;
            s.eVar5 = s.prop5;
            addEvent("event5");
        }
        //Hierarchy
