<script type="text/javascript">
var isSharethroughBlank = 'untested'; //declare window global
var storeExtraCover;

function adCallBack(obj) {

    if (obj.slot == XFP_sharethrough_top && isSharethroughBlank == 'untested') {

        if (obj.isEmpty) { // ad is blank
            isSharethroughBlank = 'blank'; // reset to false for next time ad is refreshed
            showHiddenCover();

        } else {
            isSharethroughBlank = 'filled';
            // ad is not blank
            //video component
            if ('home' == 'blogs_eye_entry') {
                var length = $('#vidComponent table').length;
                $('#vidComponent table').eq(length - 1).hide();
                //$('#sharethrough_top').removeAttr('style');
            }
            //homepage
            //Six pack
            if ($('.featureComponent .coverItem').length == 6) {
                storeExtraCover = $('.featureComponent .coverItem').eq(4).html();
                $('.featureComponent .coverItem').eq(4).replaceWith($("#sharethrough_top"));
                $("#sharethrough_top").addClass('coverItem');
                $('#sharethrough_top').show();
            }
            //Three pack
            if ($('.featureComponent .coverItem').length == 3) {
                storeExtraCover = $('.featureComponent .coverItem').eq(1).html();
                $('.featureComponent .coverItem').eq(1).replaceWith($("#sharethrough_top"));
                $("#sharethrough_top").addClass('coverItem');
                $('#sharethrough_top').show();
            }
            if ($('.featureComponent .coverItem').length == 9) {
                storeExtraCover = $('.featureComponent .coverItem').eq(7).html();
                $('.featureComponent .coverItem').eq(7).replaceWith($("#sharethrough_top"));
                $("#sharethrough_top").addClass('coverItem');
                $('#sharethrough_top').show();
            }
        }
    }
}
 
function blankAdCallback(width, height, pos) {
    
    if (width == 11 && height == 11 && pos == 'top' && (isSharethroughBlank == 'filled' || isSharethroughBlank == 'untested') ) {
        isSharethroughBlank = 'scheduled'; // set global
        showHiddenCover();
    }
}

function showHiddenCover() {
        if ($('.featureComponent .coverItem').length == 6) {
            $('.featureComponent .coverItem').eq(4).html(storeExtraCover);
            $('.featureComponent .coverItem').eq(4).show();
        }
        if ($('.featureComponent .coverItem').length == 9) {
            $('.featureComponent .coverItem').eq(7).html(storeExtraCover);
            $('.featureComponent .coverItem').eq(7).show();
        }
        if ($('.featureComponent .coverItem').length == 3) {
            $('.featureComponent .coverItem').eq(1).html(storeExtraCover);
            $('.featureComponent .coverItem').eq(1).show();
        }
        if ('home' == 'blogs_eye_entry') {
            var length = $('#vidComponent table').length;
            $('#vidComponent table').eq(length - 1).show();
        }
    
}

</script>
