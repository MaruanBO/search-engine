<script type='text/javascript'>
function pidExchange(){ //ajax to pass pids from certona to server side code
jQuery(".crossSellProduct").each(function (index, element) {
var PidsFromCertona = jQuery(this).attr('id');
jQuery.ajax({
type: "POST",
url: "/on/demandware.store/Sites-ninewest-Site/default/certonaProduct-StartOne",
data: {
certonaPidVals: PidsFromCertona
}
}).done(function (data) {
try{
if(!!data){
jQuery(element).html(data);
}
}catch(e){}
});
});
jQuery('.tabTrigger').eq(0).addClass('activeLink');
jQuery('.tabTrigger').live('click',function(){
jQuery('.tabContent').hide();
jQuery('.tabContent').eq(jQuery(this).index('.tabTrigger')).show();
jQuery('.tabTrigger').removeClass('activeLink');
jQuery(this).addClass('activeLink');
});
}
</script>


