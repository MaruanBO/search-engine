//used to update the page_name and page_type in tealium and push to omniture
function updateOmniturePageName(_pageName,_pageType) {
//set up page type data
//ensure that regardless of how we get here, at least the checkout start will be called.
utag_data.page_name= "nine west" + ":checkout:checkout Info";
utag_data.page_type= "nine west" + ":checkout:checkout Info";
utag_data.site_section="nine west" + ":checkout:checkout Info";
