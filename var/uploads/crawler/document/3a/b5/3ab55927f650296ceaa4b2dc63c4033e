url: "/on/demandware.store/Sites-ninewest-Site/default/FluidCheckout-AssignShippingMethodToBasket",
data: { shippingMethodID: "01", token: jQuery('#ajaxToken').val() },
context: document.body
}).done(function() {
window.location.reload();
});
}
