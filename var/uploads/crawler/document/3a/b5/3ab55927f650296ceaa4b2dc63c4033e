	if(typeof(lpSendData)!='undefined') lpSendData(varscope,varname,varvalue);
}

if(sShippingCountry == 'United States') {
	callLpAddVars('page','ShippingCountry',sShippingCountry); // set in fiftyone_countrySelect2.isml
	callLpSendData('page','cartTotal',window.lpCurrentTotalCart); // set in minicartcontent.isml
	
	// get Section
	var sSection = 'none';
	if(typeof(forlpSection)!='undefined') sSection = forlpSection;
	else if(typeof(staticPageName)!='undefined') sSection = staticPageName;
	else if(jQuery('#homepageMain').size() > 0) sSection = 'Home';
	else if(jQuery('#zoomImg').size() > 0) sSection = 'Product Detail';
	else if(jQuery('#productsgallery').size() > 0) sSection = 'Product Category';
	callLpAddVars('page','Section',sSection);
	
	// get ShoppingCartError
	if(sSection == 'Shopping Bag') {
		if(jQuery('#promoCodeError').text().length > 0 || jQuery('.errormessage').text().length > 0) callLpAddVars('page','ShoppingCartError','True');
		else callLpAddVars('page','ShoppingCartError','False');
	}
	
	// get CheckoutError
	if(sSection == 'Checkout' || sSection == 'Checkout Summary' || sSection == 'Checkout Confirmation') {
		if(jQuery('.errormessage').text().length > 0
			|| jQuery('.errorMessages').text().length > 0
			|| jQuery('.errorMessages2').text().length > 0) callLpAddVars('page','CheckoutError','True');
		else callLpAddVars('page','CheckoutError','False');
	}
	
	// get AccountLoginError
	if(sSection == 'Checkout Login' || sSection == 'Account: Login') {
		if(jQuery('#errorText').text().length > 0 || jQuery('.errormessage').text().length) callLpAddVars('page','AccountLoginError','True');
		else callLpAddVars('page','AccountLoginError','False');
	}
	
	// get productCategories
	var sProductCategories = 'none';
	if(jQuery('#productCrumbs').text().length > 0) sProductCategories = jQuery('#productCrumbs a:last').attr('content').replace('>','').trim();
	else if(jQuery('#contentBreadCrumbs').text().length > 0) sProductCategories = jQuery('#contentBreadCrumbs').text().replace(/\n/g,'').replace('Home','').trim();
	callLpAddVars('page','productCategories',sProductCategories);
	
	// get SizeError
	callLpAddVars('page','SizeError','False');
	
	/*
	// wait for LivePerson to load
	var t = 0;
	function lpTimer() {
		if(typeof(lpMTagConfig)!='undefined') {
			clearInterval(lpWait);
			
			// add listeners for QuickView links
			jQuery('.qvLinkA').click(function() {
				lpMTagConfig.onLoadAll()
			});
		}
		else jQuery('#lpButtonHolder').attr('title',t++);
	}
	var lpWait = setInterval(function() {lpTimer()},1000);
	*/
}
</script></span>
