function embedVideo(url) {
	jQuery("#ytVideo").attr('src', url);
	showVideo();
}
function showVideo() {
	jQuery("#fade").show();
	jQuery("#videoWindow").show();
}


function closeVideo() {
    jQuery('#playerholder').html('<iframe id="ytVideo" width="638" height="359" src="" frameborder="0" allowfullscreen></iframe>');
	//alert('close')
	jQuery("#videoWindow").hide();
	jQuery("#fade").hide();
}

