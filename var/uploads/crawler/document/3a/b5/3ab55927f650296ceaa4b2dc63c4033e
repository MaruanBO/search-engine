<script type="text/javascript">
jQuery(document).ready(function() {
	var slider = jQuery('#slider1').bxSlider({
		infiniteLoop: false,
		hideControlOnEnd: true,
		nextText: '>',
		prevText: '<'
	});

	var slider2 = jQuery('#slider2').bxSlider({
		infiniteLoop: false,
		hideControlOnEnd: true,
		nextText: '>',
		prevText: '<'
	});
	
	var nextBound = true;
	var prevBound = false;
	var slider3 = jQuery('#slider3').bxSlider({
		controls: true,
		infiniteLoop: true,
		auto: true,
		autoHover: true,
		pause: 6000,
		speed:500,	
		onAfterSlide: function(currentSlideNumber, totalSlideQty, currentSlideHtmlObject){
			clearAllCalloutsAndHighlight(currentSlideNumber+1);
			//if (currentSlideNumber != 0 && prevBound == false) {
			//	prevBound = true;
			//	jQuery('#previousBtn').show();
			//}
			//if (currentSlideNumber == 0) {
			//	prevBound = false;
			//	jQuery('#previousBtn').hide();
			//}		
			//if (currentSlideNumber == (totalSlideQty-1)) {
			//	nextBound = false;
			//	jQuery('#nextBtn').hide();
			//}
			//if ((currentSlideNumber != (totalSlideQty-1)) && nextBound == false ) {
			//	nextBound = true;
			//	jQuery('#nextBtn').show();
			//}
			//var counterText = currentSlideNumber + " / " + (totalSlideQty-1);
			//jQuery('#slideCounter').html(counterText);
    	}
	});
	jQuery('#nextBtn').click(function() {
		slider3.goToNextSlide();
		return false;
	});
	jQuery('#previousBtn').click(function() {
		slider3.goToPreviousSlide();
		return false;
	});
	jQuery('#callout1').click(function() {
		slider3.goToSlide(0);
	});
	jQuery('#callout2').click(function() {
		slider3.goToSlide(1);
	});
	jQuery('#callout3').click(function() {
		slider3.goToSlide(2);
	});

});
function clearAllCalloutsAndHighlight(slideNumber) {
	//var count = (jQuery("#slider1").children().length - 1);
	var count = 2;
	for(i = 0; i < count; i++) {
		var calloutName = '#callout' + (i + 1);
		var src = jQuery("img", calloutName).attr("src").replace("_on.gif", ".gif");
		jQuery("img", calloutName).attr("src", src);
	}
	var calloutName = '#callout' + slideNumber;
	var src = jQuery("img", calloutName).attr("src").replace(".gif", "_on.gif");
    jQuery("img", calloutName).attr("src", src);
}
</script>

