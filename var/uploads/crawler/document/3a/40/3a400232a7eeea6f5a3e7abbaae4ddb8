			if((sliderAreaWidth-55-(columnAdjust*3)<(jQuery("#slider-0").find(".item").width())*3)||(false)){
				if(ninja){ninjaLog.Update({text:"Width of items require adjustment",style:""});};
				jQuery("#slider-0").find(".item").width(Math.ceil((sliderAreaWidth-55-(columnAdjust*3))/3));
			}
		}else{
			if(typeof(ninjaLog)!="undefined"){
				ninjaLog.Update({text:"Adjusting for a single column: 70px"});
			}
			jQuery("#slider-0").find(".item").width(sliderAreaWidth-70);
		}

		jQuery(jQuery("#slider-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<3){
    				jQuery(this).width("100%");
			}
		});

		if(jQuery.browser.msie && Number(jQuery.browser.version)<7){
//alert("#"+jQuery(jQuery("#slider-0").parents(".tncms-region")[0]).attr("id")+" width: "+jQuery(jQuery("#slider-0").parents(".tncms-region")[0]).width());

			//jQuery("#slider-0").find(".group .item .image").hide();
			jQuery("#slider-0").find(".group .item .text").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#slider-0").find(".group .item .text .description").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#slider-0").width(jQuery("#slider-0").parent().width());

			if(jQuery("#slider-0").hasClass("showcase")){
				jQuery('#slider-0').find(".group").css({"padding":"0px"});

				var itemWidth = jQuery("#slider-0").find(".item").width()-((3) * 10);

				var groupWidth = jQuery('#slider-0').find(".group").width();
var itemWidth = (groupWidth/3)-40;
jQuery('#slider-0').find(".item").css({"width":itemWidth});
			}else if(jQuery("#slider-0").hasClass("playlist")){
				jQuery('#slider-0').find(".group").css({"padding":"0px"});

				var groupWidth = jQuery('#slider-0').find(".group").width();
				var itemWidth = (groupWidth/3)-40;

				jQuery('#slider-0').find(".item").css({"height":"154px","width":itemWidth});
				jQuery('#slider-0').find(".item").find(".image").css({"width":itemWidth});

			}
		}

		


	jQuery('#slider-0 .prev').show();
	jQuery('#slider-0 .next').show();


        // BEFORE TRANSITION
        function onBeforeslider0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAfterslider0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#slider-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#slider-0 .item-list').cycle({
		fx: effectslider0,
		sync: true,
		
		
		timeout: 0,
                before: onBeforeslider0,
		after:	onAfterslider0,
        pause:true,
		pager:'#slider-0 .nav',
		prev:'#slider-0 .prev',
		next:'#slider-0 .next'
	});
        

        

}



// ]]> -->
</script>



</div></div></div>
