		</li>

		</ul>
            <div class="clear"></div>
	</div>
        <div class="clear"></div>

	
	
	
	
	
	
	
	<script type="text/javascript">

		var sliderTabsId = "#slideshow-tabs-1";
		var maxSlideHeight = 0;
		$("#slideshow-tabs-1 .slideshow-tabs-img").eq(($("#slideshow-tabs-1 .slideshow-tabs-img").length - 1)).load(function() {
			$(sliderTabsId + " li.slideshow-tabs-panel-item").each(function() {
				$(this).css("display", "block");
				if($(this).height() > maxSlideHeight) {
					maxSlideHeight = $(this).height() + 15;
				}
				var slideshowTabsImage = $("div.photo-container .slideshow-tabs-img", this);
				if(slideshowTabsImage.height() > maxSlideHeight) {
					maxSlideHeight = slideshowTabsImage.height() ;
				}
				if(!$(this).hasClass("active-panel")){
					$(this).css("display", "none");
				}
			});
			var ht = maxSlideHeight;
			$(sliderTabsId + " li.slideshow-tabs-panel-item").css("height", ht + "px"); 
		});
	
	
		$(document).ready(function() {
			var sliderTabsId = "#slideshow-tabs-1";
			$(sliderTabsId + " a.slideshow-tabs-tab-item").click(function(event) {
				event.preventDefault();
			});

			
			
			
			
			
		/* rotation function */
			var changeSlideshowTabsPanelTimeout;
			var currIndex = -1;
			var currPanel;
			var currNav;
			var newIndex;
			var newPanel;
			var totalPanels;
			var newIndex = -1;
			
			function changeSlideshowTabsPanel(newIndex){
				clearTimeout(changeSlideshowTabsPanelTimeout);
				var currPanel = $(sliderTabsId + " li.slideshow-tabs-panel-item.active-panel");
				var currNav = $(sliderTabsId + " a.slideshow-tabs-tab-item.active-nav");
				if(typeof newIndex == "undefined") {
					if(currPanel.next().length == 0) {
						currPanel.clearQueue().fadeTo(100,0).removeClass("active-panel");
						/* currNav.find(".slideshow-tabs-arrow").hide(); */
						currNav.removeClass("active-nav");
						currPanel.css("display", "none");
						$(sliderTabsId + " a.slideshow-tabs-tab-item").eq(0).addClass("active-nav");
						$(sliderTabsId + " li.slideshow-tabs-panel-item").eq(0).clearQueue().fadeTo(100,1).addClass("active-panel");
						/* $(sliderTabsId + " li.slideshow-tabs-panel-item").eq(0).find(".slideshow-tabs-arrow").show(); */
					} else { 
						/* currNav.find(".slideshow-tabs-arrow").hide(); */
						currNav.removeClass("active-nav");
						currPanel.clearQueue().fadeTo(100,0).removeClass("active-panel");
						currPanel.css("display", "none");
						currPanel.clearQueue().next().fadeTo(100,1).addClass("active-panel");
						currNav.next().addClass("active-nav");
						/* currNav.next().find(".slideshow-tabs-arrow").show(); */
					}
				} else { 
						/* currNav.find(".slideshow-tabs-arrow").hide(); */
						currNav.removeClass("active-nav");
						currPanel.clearQueue().fadeTo(100,0).removeClass("active-panel");
						currPanel.css("display", "none");
						$(sliderTabsId + " li.slideshow-tabs-panel-item").eq(newIndex).clearQueue().fadeTo(100,1).addClass("active-panel");
						$(sliderTabsId + " a.slideshow-tabs-tab-item").eq(newIndex).addClass("active-nav");
						/* $(sliderTabsId + " a.slideshow-tabs-tab-item").eq(newIndex).find(".slideshow-tabs-arrow").show(); */
				}
				changeSlideshowTabsPanelTimeout = setTimeout(changeSlideshowTabsPanel,5500);     
			}

			changeSlideshowTabsPanel(0);
						
		/* Stop rotation on panel hover */
			$(sliderTabsId + " li.slideshow-tabs-panel-item").hover(
			function() {
					clearTimeout(changeSlideshowTabsPanelTimeout);
				},
				function() {
					changeSlideshowTabsPanelTimeout = setTimeout(changeSlideshowTabsPanel,5500);
				});

		/* Navigation item behavior */
				$(sliderTabsId + " a.slideshow-tabs-tab-item").click(function() {
					changeSlideshowTabsPanel($(sliderTabsId + " a.slideshow-tabs-tab-item").index(this));
				});
			
		});
	</script>
</div></div>
        </div>
        
