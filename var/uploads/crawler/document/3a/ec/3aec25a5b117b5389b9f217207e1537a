                 })
                 .blur(function(){
                     var thisInput = jQuery(this);
                     thisInput.parent().removeClass("focus");
                     thisInput.width(
                             thisInput.attr("data-originalWidth")
                         );

                     thisInput.siblings("label").css("left","0");
                 }) */
