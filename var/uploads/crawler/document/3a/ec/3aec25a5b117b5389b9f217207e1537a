     function initJqueryUI() {
         if (touchState) {
             jQuery.getScript('/common/js/rd/libs/jquery.ui.touch-punch.min.js')
                 .done(function( script, textStatus ) {
                 jQuery(".tabs").tabs();
             })
         } else {
            jQuery(".tabs").tabs();
         }
     }

     // Allow deferred elements
     jQuery(".defer").removeClass("defer");

