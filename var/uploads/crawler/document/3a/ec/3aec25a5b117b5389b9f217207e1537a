     jQuery(".compact")
             .addClass("compacted")
             .removeClass("compact")
             .children("input")
                 .attr("data-originalWidth",function(){
                     return jQuery(this).width();
                 })
                 .focus(function(){
                     var thisInput = jQuery(this);
                     thisInput.parent().addClass("focus");
                     thisInput.width(
                            Number(thisInput.attr("data-originalWidth"))
                            + thisInput.siblings("label").width()
);

