<script type="text/javascript">
    function refreshDataNB(parameters, responseHandler) {
        jQuery.ajax({
            type: "POST",
            url: "/ajax/mmy.php",
            data: parameters,
            success: this[responseHandler]
        });
    }
    function refreshModelsUsedNB(data) {
        var data = eval(data);
        select = document.getElementById('navbarModelUsed');
        select.options.length = 0;
        select.add(new Option("Select A Model", "0"));
        for (i=0; i<data.length; i++) {
            select.add(new Option(data[i].NAME, data[i].ID));
        }
    }
    function refreshModelsNewNB(data) {
        var data = eval(data);
        select = document.getElementById('navbarModelNew');
        select.options.length = 0;
        select.add(new Option("Select A Model", "0"));
        for (i=0; i<data.length; i++) {
            select.add(new Option(data[i].NAME, data[i].ID));;
        }
        // reset trims
        select = document.getElementById('navbarTrimNew');
        select.options.length = 0;
        select.options[0] = new Option("Select A Trim", "0");
    }
    function refreshTrimsNewNB(data) {
        var data = eval(data);
        select = document.getElementById('navbarTrimNew');
        select.options.length = 0;
        select.add(new Option("Select A Trim", "0"));
        select.add(new Option("Not Sure", "0"));
        for (i=0; i<data.length; i++) {
            select.add(new Option(data[i].NAME, data[i].ID));;
        }
    }
    function refreshModelsResearchNB(data) {
        var data = eval(data);
        select = document.getElementById('navbarModelResearch');
        select.options.length = 0;
        select.add(new Option("Select A Model", "0"));
        for (i=0; i<data.length; i++) {
            select.add(new Option(data[i].NAME, data[i].ID));;
        }
        // reset years
        select = document.getElementById('navbarYearResearch');
        select.options.length = 0;
        select.options[0] = new Option("Select A Year", "0");
    }
    function refreshYearsResearchNB(data) {
        var data = eval(data);
        select = document.getElementById('navbarYearResearch');
        select.options.length = 0;
        select.add(new Option("Select A Year", "0"));
        for (i=0; i<data.length; i++) {
            select.add(new Option(data[i].NAME, data[i].ID));;
        }
    }


    function modifyForUrlNB(name) {
        return name.replace(/ /g, "-").replace(/&/g, "and").toLowerCase();
    }

    function submitNB(form) {
        // all the forms have make/model
        var makeSelect = form.make;
        var modelSelect = form.model;
        var makeName = makeSelect.options[makeSelect.selectedIndex].text;
        var modelName = modelSelect.options[modelSelect.selectedIndex].text;
        var makeId = makeSelect.options[makeSelect.selectedIndex].value;
        var modelId = modelSelect.options[modelSelect.selectedIndex].value;

        var link = ""
        var error = false;
        var errorMessage = "";

        // validate
        if (makeId < 1) {
            error = true;
            errorMessage += "Please choose a Make\n";
        }
        if (modelId < 1) {
            error = true;
            errorMessage += "Please choose a Model\n";
        }
        if (form.name == "navbarUsedSearch") {
            var zip = form.zip.value;
            var radiusSelect = form.radius;
            if (zip.length != 5 || (/\D/.test(zip))) {
                error = true;
                errorMessage += "Please enter a valid Zip\n";
            }
            var radius = radiusSelect.options[radiusSelect.selectedIndex].value;
            link = makeId + "/" + modelId + "/" + zip + "/" + radius + "/" + modifyForUrlNB(makeName) + "/" + modifyForUrlNB(modelName) + "/used-cars";

        } else if (form.name == "navbarNewSearch") {
            var zip = form.zip.value;
            if (zip.length != 5 || (/\D/.test(zip))) {
                error = true;
                errorMessage += "Please enter a valid Zip\n";
            }
            link = makeId + "/" + modelId + "/" + zip + "/new-cars/price-quote/" + modifyForUrlNB(makeName) + "/" + modifyForUrlNB(modelName);

        } else if (form.name == "navbarResearchSearch") {
            var yearSelect = form.year;
            var year = yearSelect.options[yearSelect.selectedIndex].value;
            if (year < 1) {
                error = true;
                errorMessage += "Please choose a Year\n";
            }
            link = modifyForUrlNB(makeName) + "/" + modifyForUrlNB(modelName) + "/" + year;
        }

        // problem
        if (error) {
            alert(errorMessage);
            return false;
        }

        // overide for new..
        if (form.name == "navbarNewSearch") {
            return true;
        }

        var linkDomain = "http://www.web2carz.com/";
        location.href=linkDomain+link;
        return false;
    }


    jQuery(document).ready(function() {
        jQuery('#navbarMakeUsed').change(function() {
            var makeId = jQuery(this).val();
            var parameters = "action=getAllModels&args[]=" + makeId;
            refreshDataNB(parameters, "refreshModelsUsedNB");
        });
        jQuery('#navbarMakeNew').change(function() {
            var makeId = jQuery(this).val();
            var parameters = "action=getNewModels&args[]=" + makeId;
            refreshDataNB(parameters, "refreshModelsNewNB");
        });
        jQuery('#navbarModelNew').change(function() {
            var modelId = jQuery(this).val();
            var makeId = jQuery('#navbarMakeNew').val();
            var parameters = "action=getNewTrims&args[]=" + makeId + "&args[]=" + modelId;
            refreshDataNB(parameters, "refreshTrimsNewNB");
        });
        jQuery('#navbarMakeResearch').change(function() {
            var makeId = jQuery(this).val();
            var parameters = "action=getAllModels&args[]=" + makeId;
            refreshDataNB(parameters, "refreshModelsResearchNB");
        });
        jQuery('#navbarModelResearch').change(function() {
            var modelId = jQuery(this).val();
            var makeId = jQuery('#navbarMakeResearch').val();
            var parameters = "action=getAllYears&args[]=" + makeId + "&args[]=" + modelId;
            refreshDataNB(parameters, "refreshYearsResearchNB");
        });
    });
</script>

