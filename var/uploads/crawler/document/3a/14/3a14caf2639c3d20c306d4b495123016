$(function(){
	$('#mobile_nav_btn').click(function(){mobileNavToggle();});
	$('#mobileNavCloseBtn').click(function(){mobileNavHide();});
});

function mobileNavToggle(){
	if($('#mobile_nav_cont').is(':hidden')){mobileNavShow();}
	else{mobileNavHide();}
}
function mobileNavHide(){$('#mobile_nav_cont').hide();}
function mobileNavShow(){$('#mobile_nav_cont').show();}
</script>
