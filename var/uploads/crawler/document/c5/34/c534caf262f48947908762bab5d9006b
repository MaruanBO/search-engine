<script type="text/javascript">
	(function(w, undefined) {
		var aio = w.aio;
		if (!aio) return false;
		aio.on('ready', function (articulate) {
	  		
	  		var gaFn = window['ga'] || function() {}; // Init the ga tracking func
	  		var aioLoadTimer = new Date().getTime();

			articulate.set('account', '522668fba43d92000000000c');
			articulate.set('config', '5331b626b608bdc62500002a');
			articulate.set('auto', false);
			articulate.set('root', 'aio-container');
			articulate.set('logging', true);
			articulate.set('balloons', true);
			articulate.set('sounds', true);
 
	  		articulate.set('proactive', {
				enabled: true,
				disableAfterRejections: 2,
				initialTime: 30,
				defaultTime: 30,
				selectors: {
		  			display: '.js-aio-proactive',
		  			ok: '#proactive_accept',
		  			cancel: '#proactive_reject'
				}
	  		});

	  		articulate.set('reactive', {
				enabled: true,
				selectors: {
		  			display: '.js-aio-reactive'
				}
	  		});

	  		articulate.set('openOnMessages', true);
	  		articulate.set('phone', '1-888-560-6072');
	  		articulate.store('phone', '1-888-560-6072');

			articulate.subscribe('ui:maximized', function(){

				hQ.store('show - articulate opened', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'aio',
					'eventAction': 'opened',
					'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('ui:minimized', function(){

				// Remove proactive if Articulate is minimized
				var aioProactive = document.getElementsByClassName('aio-proactive');
				aioProactive[0].style.display = 'none';

				hQ.store('hide - articulate minimized', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'aio',
					'eventAction': 'minimized',
					'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('didAutoReopen', function(){

				hQ.store('show - articulate auto open', {});

				gaFn('send', {
				'hitType': 'event',
				'eventCategory': 'aio',
				'eventAction': 'autoOpen',
				'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('chat:start', function(){

				hQ.store('start - articulate chat', {});

				// Set the "chat" metric
				gaFn('set', 'metric1', 1);

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'aio',
					'eventAction': 'chatted',
					'eventLabel': 'Articulate'
				});

			});

			hQ.on('initialized', function () {
				var hud = halcyon.cookie.getHUD();
				articulate.store('htoken', hud.i + ':' + hud.h);
				articulate.store('exp', hud.x);
				articulate.set('phone', hud.n); // Set the phone var to their hailo phone instead
				articulate.store('phone', hud.n); // Store with the chat doc
			});

			articulate.subscribe('proactive:show', function () {

				articulate.publish('ui:maximized', {isMinimized: false});
				articulate.store('chattype', 'proactive');

				hQ.store('show - proactive displayed', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'aio-proactive',
					'eventAction': 'displayed',
					'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('reactive:click', function () {

				articulate.store('chattype', 'reactive');

				hQ.store('click - chat reactive', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'aio-reactive',
					'eventAction': 'clicked',
					'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('proactive:reject', function () {

				hQ.store('click - proactive nothanks', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'chat-proactive',
					'eventAction': 'optout',
					'eventLabel': 'Articulate'
				});

			});

			articulate.subscribe('proactive:accept', function () {

				hQ.store('click - proactive accept', {});

				gaFn('send', {
					'hitType': 'event',
					'eventCategory': 'chat-proactive',
					'eventAction': 'clicked',
					'eventLabel': 'Articulate'
				});

			});

			
			// boots the angular application
			articulate.load(function() {

				gaFn('send', {
					'hitType': 'timing',
					'timingCategory': 'Articulate',
					'timingVar': 'Bootstrap',
					'timingValue': new Date().getTime() - aioLoadTimer,
					'timingLabel': 'Articulate'
				});

			});

