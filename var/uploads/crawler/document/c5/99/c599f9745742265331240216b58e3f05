  		<script type="text/javascript" charset="utf-8">
  		  jQuery('.ipsList_withminiphoto').ready(function() {
  		    var mMaxL = 34;
  		    var mSaleIcon = "<img style='display:inline-block;margin-right:5px;position:relative;top:3px;' src='http://sponsors.golfwrx.com/sale_tag_16_3.png' />";
          jQuery('a.ipsType_small').each(function() {
            var text = jQuery(this).text();
            if(text.length > mMaxL) {
              var begin = mSaleIcon + text.substr(0, mMaxL) + "...";
              jQuery(this).html(begin);
            }
            else {
              jQuery(this).html(mSaleIcon + text);
            }
          });
          jQuery('.ipsType_smaller').each(function() {
            jQuery(this).hide();
          });
  		  });
