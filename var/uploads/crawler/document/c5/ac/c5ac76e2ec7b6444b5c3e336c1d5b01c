<script>
var ajaxurl = 'http://www.unrefugees.org/wp-admin/admin-ajax.php';
jQuery(function($) {
    $signup_forms = $('.subscribe-form');
    $signup_forms.each(function() {
        var action = this.action;
        var $form = $(this);
        $form.submit(function(event) {
            var data = $(this).serialize();
            data += '&action=unhcr_en_submit&_ajax_nonce=ed7155f0c9';
            if(action) data+= '&remotepost=' + action;
            $form.find('.signup-message').remove();
            $.post(ajaxurl,data,function(response) {
            	$form.prepend(response);
            });
            event.preventDefault();
            return false;
        });
    });
});
</script>
