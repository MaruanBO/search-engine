      $(document).ready(function() {
        $('.popup-travel').magnificPopup({
          type: 'inline',
          preloader: false,
          focus: '#name',

          // When elemened is focused, some mobile browsers in some cases zoom in
          // change for min width on mobile broswers
          callbacks: {
            beforeOpen: function() {
              if($(window).width() < 320) {
                this.st.focus = false;
              } else {
                this.st.focus = '#name';
              }
            }
          }
