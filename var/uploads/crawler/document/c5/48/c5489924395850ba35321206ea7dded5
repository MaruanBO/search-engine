				$(document).ready(function() {
					$('.viewpoint-list .article-list').first().toggle();

					$('.viewpoint-selector .selector-tile').mouseenter(function() {
						$('.viewpoint-list .selector-tile').removeClass('selected');
						$(this).addClass('selected');

						var authorElement = $(this).attr('id');
						var authorParts = authorElement.split('_');
						var authorUser = authorParts[1];

						$('.viewpoint-list .article-list').hide();
						$('.viewpoint-list .article-list').promise().done(function() {
							$('#posts_'+authorUser).show();
						});

					});

				});
			</script>



