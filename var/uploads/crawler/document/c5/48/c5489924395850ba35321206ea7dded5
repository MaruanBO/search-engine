		<!-- Shared PageBuilder Logic -->
		<script type="text/javascript">

			$(document).ready(function () {

				// Gets Domain
			    var s = location.hostname.split('.');
			    var domain = s[s.length-2]+'.'+s[s.length-1];

			    // Modifies External Links To Open In A New Window, Excluding DoubleClick
			    $('a').filter(function() {
			        if (this.hostname && this.hostname != 'ad.doubleclick.net') {
			            var t = this.hostname.split('.');
			            var test = t[t.length-2]+'.'+t[t.length-1];
			            if (test != domain)
			                return true;
			            else
			                return false;
			        }
			    }).attr('target', '_blank');

			});

			// http://stackoverflow.com/questions/12393667/undefined-appearing-lots-of-time-in-my-404-error-log
			window.suggestmeyes_loaded = true;

		</script>

