<script language="javascript">
// Network Newsletter
//var ESSENCE = window.ESSENCE || [];
ESSENCE.newsletter = {
    success: function(el){
        var wrapper = el.parent(),
            msg1 = "Thank you!",
            msg2 = "Your request may take up to one week to be processed.",
            minHeight = wrapper.height();
        wrapper.css("min-height", minHeight + "px");
        el.prev("p").remove();
        el.remove();
        wrapper.append("<div class='msg-success'><h6>" + msg1 + "</h6><p>" + msg2 + "</p></div>");
		return false;
    },
    fail: function(el, m){
		jQuery("span.err").remove();
        jQuery("p.email").prepend("<span class='err'>" + m + "</span>");
		return false;
    },
    init: function (){
            jQuery("#newsletter_signup").submit(function(e){
                e.preventDefault();
                var el = jQuery(this),
                    re = /\S+@\S+\.\S+/,
                    url = jQuery(this).attr("action"),
                    email = jQuery(this).find("#newsletter-email").val(),
                    aid = jQuery(this).find("#aid").val(),
                    n = jQuery(this).find("#n").val(),
                    fsub = jQuery(this).find("#fsub").val(),
					a = jQuery(this).find("#a").val(),
                    ESSENCE_SOURCE = jQuery(this).find("#ESSENCE_SOURCE").val(),
					sub_v = '';
				if(jQuery('#sub1').is(':checked')){
				   sub_v = jQuery('#sub1').val();
				} 
				if(jQuery('#sub2').is(':checked')){
				   sub_v += '|' + jQuery('#sub2').val();   
				} 
				if(jQuery('#sub3').is(':checked')){
				   sub_v += '|' + jQuery('#sub3').val();   
				}
                if(re.test(email)) {
                    jQuery.post(url, {
                        "email": email,
                        "aid": aid,
                        "n": n,
                        "fsub": fsub,
						"a": a,
						"sub": sub_v,
                        "ESSENCE_SOURCE": ESSENCE_SOURCE
                    }); // we can't get any data back from remote url, so we won't support 'fail'
                    ESSENCE.newsletter.success(el);
                } else{
                    ESSENCE.newsletter.fail(jQuery(this), "* This is not a valid email. Please try again.");
                }
				return false;
			});
			
       }
};
ESSENCE.newsletter.init();

</script>  </div>
</div>
  </div>
					</div> <!-- /.section -->
				</div> <!-- /#sidebar-second -->
			</div>
		
		
		

