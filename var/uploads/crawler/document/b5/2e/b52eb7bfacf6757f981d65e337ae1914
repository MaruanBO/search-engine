<script type="text/javascript">

  $(function() {

    var btn = $("#button1 strong"), input_text = btn.text().split("|"), f = $("<span>",{ "class" : "frames" });
    btn.empty().append(f);

    $(".text .note").html($(".text .note").html().replace(input_text.join("|"),input_text[0]));

    $.each(input_text,function(i,value) {
      var frame = $("<span>",{ "class" : "frame" }).append($("<span>",{ "class" : "td" }).append($("<span>",{ "class" : "tdin" }).text(value)));
      f.append(frame);
    });

    $.fn.cycle = function(timeout) {

      var that = $(this), cycle = function(index) {
        (index == that.length) ? index = 0 : null ;
        that.fadeOut(400).eq(index).fadeIn(400);
        setTimeout(function() { cycle(++index); },timeout);
      }

      cycle(0);

    }

    f.find(".frame").cycle(3000);

    $(document).on("click",".view-features",function(e) {
      e.preventDefault(); $("#view-search-more").click();
    });

  });

</script>

