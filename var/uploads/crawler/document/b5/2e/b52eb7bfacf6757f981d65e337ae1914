	<body class="lng_EN BG_custom_4">    <script type="text/javascript">
      var __clicked=0;
      function __trackClick(elm,btn) {
        if (!__clicked) {
          var track = function (url) {
            var img = document.createElement('img');
            img.onload = img.onerror = function () { setTimeout(function () { try { document.body.removeChild(img) } catch (e) { } }, 100) };
            img.src = url;
            img.style.cssText = "width:1px;height:1px;position:absolute;top:0;left:-2000px";
            document.body.appendChild(img);
          }
          track('/WebStat2.asmx/SaveAction?sDate=20150219&sModule=wst&sCID=82639&sName=LPWST_INST_Facebook_US&sAction=&sUID=I23d18728aa3a43719bec652598a2b880');
          _gaq.push(['_trackEvent', 'ToolbarLP','DownloadButton' + btn + '_Click','Facebook_US']);
          __clicked=1;
          var nav=function(){location.href=elm.href};
          setTimeout(nav,600);
          return false;
        }
      }
    </script>

