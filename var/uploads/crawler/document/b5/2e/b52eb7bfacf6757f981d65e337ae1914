<script type="text/javascript">

  $(document).ready(function() {
    if($("body").hasClass("BG_custom_3")) {
      var click = false;
      $("a.view-features").click(function(e) {

        if(click == false) {
          $(".footer .custom-space").slideUp();
          $(".footer").css({"background-position":"center 220px"});
          click = true;
        }

        else {
          $(".footer .custom-space").slideDown();
          $(".footer").css({"background-position":"center 125px"});
          click = false;
        }

      });
    }
  });

</script>
