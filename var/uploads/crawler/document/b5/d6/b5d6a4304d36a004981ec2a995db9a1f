<script type="text/javascript">
$(document).ready(function(){  
	$('#searchByTitle').autocomplete({
		source: '/TagServlet?page=tagByPattern',
		minLength: 0,
		select: function(item, ui){
            var origEvent = event;
            while (origEvent.originalEvent !== undefined){
                origEvent = origEvent.originalEvent;
            }
            if (origEvent.type == 'click'){
                document.getElementById('searchByTitle').value = ui.item.value;
                document.getElementById('lSearchForm').submit();
            } else {
                document.getElementById('lSearchForm').submit();
            }
		},
		change: function(item, ui){  
			
		} 
	});	
});

function submitLocalSearchForm(){
	var searchableText=ge('searchByTitle').value;
	if(searchableText=='' || searchableText=="Search your Health..."){
		alert("Please enter a keyword to search");
		return false;
	}else{
		ge('lSearchForm').submit();
	}
}
