<script type="text/javascript">
	// Get an instance of jQuery that doesn't collide with the $ velocity syntax or mooTools
	var cjq = jQuery.noConflict();

	cjq(document).ready(function(){

		cjq('.requirementText h4').click(function(){
			cjq(this).parent().find('.infoText').toggle(500);
			cjq(this).parent().find('.plusandminus').toggle();
			return false;
		});

		// Main Navigation hover
		cjq('#mainNav').find('li').each(function(){
			var ul = cjq(this).find('ul');
			if(ul.length > 0){
				cjq(this).hover(function(){
					ul.stop(true, true).slideDown();
				}, function(){
					ul.stop(true, true).slideUp();
				});
			}

			//cjq("#mainNav ul li a").ieffembedfix();
		});

		// merchant login box
		var loginFormOpen = false;
		cjq(".merchantLogin").click(function() {
			if (loginFormOpen) {
				cjq("#merchantLoginBox").fadeOut(400);
				loginFormOpen = false;
			} else {
				cjq("#merchantLoginBox").fadeIn(400);
				loginFormOpen = true;
			}
		});

		try{
			// add classes to each list item
			cjq('ul').each(function() {
				cjq(this).find('> li:first').addClass('li-first');
				cjq(this).find('> li:last').addClass('li-last');
				cjq(this).find('> li:even').addClass('li-even');
				cjq(this).find('> li:odd').addClass('li-odd');
			});

			// Home page banner
			cjq('#topBanners').cycle({
				fx: 'fade',
				timeout: 4000,
				pager: '#bannerNav'
			});

			// home page partner list
			cjq('.partnerListContainer').cycle({
				fx: 'scrollHorz',
				timeout: 4000,
				next: '.arrowRight',
				prev: '.arrowLeft'
			});

			// pricing page premium services
			cjq('#premiumServicesHeader , .pullDown').click(function(){
				cjq('#premiumServices').slideToggle('slow');
			});

			cjq('#cartCompareLink').click(function(){
				var link = cjq(this).attr('href');
				cjq('html,body').stop().animate({
					scrollTop: link.offset().top
				}, 2000);
				return false;
			});
		} catch(e){}

		/**
		 * SIGNUP LIGHTBOX
		 * =============================================================================================================
		 */
		function hasPlaceHolderSupport(){
			var input = document.createElement('input');
			return ('placeholder' in input);
		}

		var signupSubmitted = false;
		function checkAndSubmit(){
			// check if form has been submitted
			if(signupSubmitted) return;

			try{
				_gaq.push(['_trackEvent', 'Signup', 'Success']);
			} catch(e){}

			document.signupForm.submit();
			signupSubmitted = true;
		}

		function formError(inputError){
			inputError.stop(true, true).animate({backgroundColor: '#f8d5d5', color:'#000'}, 1000);
			inputError.stop(true, true).animate({backgroundColor: '#eee', color: '#666'}, 1000);
		}

		cjq('.lightbox_trigger').click(function(e) {
			cjq('#lightbox').fadeIn('fast');
			cjq('.stepNumber').text('1');
			cjq('.content1, .steps').show();
			cjq('.content2, .content3, .processing').hide();

			e.preventDefault();
			e.stopPropagation();
			return false;
		});

		cjq('.password-note').live('click', function(){
			cjq(this).hide().prev('.password').focus();
		});

		// check input boxes
		cjq('#lightbox form input').each(function(){
			var input = cjq(this),
				defaultVal = cjq(this).attr('title');

			// check if the placeholder attr is supported
			if(!hasPlaceHolderSupport()){
				// show default value if not a password
				if(input.attr('type')  !== 'password' && input.val() === ''){
					input.val(defaultVal);
				}

				//function and blur event handlers
				input.focus(function(){
					// if password, hide note
					if(input.attr('type')  === 'password'){
						input.next('.password-note').css('display', 'none').hide();
					}
					// otherwise, check default value
					else if(input.val() === defaultVal){
						// hide the note if it's a password
						input.val('');
					}
				}).blur(function(){
					// on blur, if the value is nothing...
					if(!input.val()){
						// show the note if it's a password
						if(input.attr('type')  === 'password'){
							input.next('.password-note').css('display', 'block').show();
						} else { // reset it to the title
							input.val(defaultVal);
						}
					}
				})
			} else{
				cjq('.password-note').hide();
			}
		});

		// the weirdest way to not have the text selected.
		cjq('#url').focus(function(){
			if( cjq.inArray(cjq(this).val() , ['URL', '']) > -1 ){
				cjq(this).val('');
				if(this.createTextRange){
					cjq('#url').val('http://');
					var r = this.createTextRange();
					r.collapse(false);
					r.select();
				} else {
					setTimeout(function(){
						cjq('#url').val('http://');
					}, 10);
				}
			}
		}).blur(function(){
			if( cjq.inArray(cjq(this).val(), ['URL', 'http://']) > -1){
				cjq(this).val('');
			} else {
				// if user has not entered http:// https:// or ftp:// assume they mean http://
				if(!/^(https?|ftp):\/\//i.test(cjq(this).val())) {
					cjq(this).val('http://'+cjq(this).val()); // also update the form element
					signupForm.element('#url');
				}
			}
		});

		cjq.validator.addMethod('invalidValue', function(value, element){
			return element.value != element.title;
		}, 'invalid');

		cjq.validator.addMethod('password', function(value){
			return /^(?=.*[\d])(?=.*[a-zA-Z])([a-zA-Z\d\W_-]+)$/.test(value);
		}, 'weak*');

		cjq.validator.addMethod('checkMerchantId', function(value){
			return /^[A-Z0-9]{1,5}$/i.test(value);
		}, 'The Merchant ID must only be letters and/or numbers.');

		cjq.validator.addMethod('completeUrl', function(value, element){
			if (value.length === 0 || cjq.inArray(value, ['http://', 'https://', 'ftp://']) > -1 ){
				cjq(element).val(value);
				return true;
			}

			// now check if valid url
			// http://docs.jquery.com/Plugins/Validation/Methods/url
			// contributed by Scott Gonzalez: http://projects.scottsplayground.com/iri/
			return /^(https?|ftp):\/\/(((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&amp;'\(\)\*\+,;=]|:)*@)?(((\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5])\.(\d|[1-9]\d|1\d\d|2[0-4]\d|25[0-5]))|((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?)(:\d*)?)(\/((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&amp;'\(\)\*\+,;=]|:|@)+(\/(([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&amp;'\(\)\*\+,;=]|:|@)*)*)?)?(\?((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&amp;'\(\)\*\+,;=]|:|@)|[\uE000-\uF8FF]|\/|\?)*)?(\#((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&amp;'\(\)\*\+,;=]|:|@)|\/|\?)*)?$/i.test(value);

		}, 'invalid url');
		var signupForm = cjq('#signupForm').validate({
			rules: {
				fullName:{
					required:true,
					invalidValue: true
				},
				company: {
					required:true,
					invalidValue:true
				},
				email: {
					required: true,
					email: true
				},
				password: {
					required: true,
					minlength: 8,
					password:true
				},
				url: 'completeUrl',
				username: {
					required: true,
					invalidValue: true
				},
				confirm_password: {
					required: true,
					minlength:5,
					equalTo: '#password'
				}
			},
			messages: {
				fullName: {
					required: 'required'
				},
				company: {
					required: 'required'
				},
				email: {
					required: 'required',
					email: 'invalid email'
				},
				password: {
					required: 'required',
					minlength: 'too short!'
				},
				username: {
					required: 'required'
				},
				confirm_password: {
					required: 'required',
					minlength: 'too short!',
					equalTo: 'not equal'
				}
			},
			ignoreTitle: true
		});

		cjq('#signupForm').bind('keypress', function(e){
			if(event.which == '13'){
				e.preventDefault();
				e.stopPropagation();
				return false;
			}
		});

		// next button
		cjq('.nextStep').live('click', function(e) {
			if(signupForm.form()){

				// are we still in the first step?
				if (cjq('.stepNumber').text() === '1') {
					// analytics event
					_gaq.push(['_trackEvent', 'Signup', 'Next1']);
					cjq('.content1').fadeOut(300,function(){
						cjq('.content2').fadeIn();
						cjq('.stepNumber').text('2');
					});

					var json;
					cjq('#merchantId').rules('add',{
						required:true,
						maxlength:5,
						checkMerchantId: true,
						remote:{
							url: '/merchant/signup/signupAjax.do',
							type:'post',
							data: {
								q: function(){
									return cjq('#merchantId').val();
								},
								format:'json'
							},
							dataType: 'json',
							dataFilter: function(data){
								json = cjq.parseJSON(data);
								return !json.used;
							}
						},
						messages:{
							required:'required',
							maxlength: 'too long',
							remote: function(){
								return 'That Merchant ID is already taken. Suggestion: <strong>'+json.suggestion+'</strong>';
							}
						}
					});

					cjq('#agree').rules('add',{
						required:true,
						messages: {
							required: 'required'
						}
					});
				} else { // we are in the second step. show animation

					// check merchant id for duplication
					cjq.ajax({
						type:'post',
						url: '/merchant/signup/signupAjax.do',
						dataType: 'json',
						data:{
							q: cjq('#merchantId').val(),
							format: 'json'
						},
						success: function(data){
							var json = cjq.parseJSON(data);
							if(data.used){
								formError(cjq('#signupForm input.error'));
								return;
							}

							_gaq.push(['_trackEvent', 'Signup', 'Next2']);
							cjq('.content2').fadeOut(300,function(){
								cjq('.content3').fadeIn(300, function(){
									try{
										var vid = document.getElementById('vidBuild');
										vid.play();
										vid.addEventListener('ended', checkAndSubmit, false);
										setTimeout(checkAndSubmit, 8000);
									} catch(e){
										//alert(e);
										cjq('.signup-building-1').fadeIn(1000, function(){
											cjq('.signup-building-2').fadeIn(1000, function(){
												cjq('.signup-building-3').fadeIn(1000, function(){
													cjq('.signup-building-4').fadeIn(1000, checkAndSubmit);
												});
											});
										});
									}
								});
							});

							cjq('.steps').fadeOut(300,function(){
								cjq('.processing').fadeIn();
							});
						}
					}); // end merchant id ajax
				} // end else
			} else { // not validated
				formError(cjq('#signupForm input.error'));
			}

			e.preventDefault();
			e.stopPropagation();
			return false;
		});

		//Click anywhere on the page to get rid of lightbox window
		cjq('.closeBtn , .overlay').live('click', function(){ //must use live, as the lightbox element is inserted into the DOM
			if( cjq('#lightbox .content3').is(':visible') ) return false;
			_gaq.push(['_trackEvent', 'Signup', 'Close']);
			cjq('#lightbox').fadeOut();
			cjq.ajax({
				url: '/merchant/signup/signup2AbandonSave.do',
				type: 'post',
				data: {
					fullName: cjq('#fullName').val(),
					company: cjq('#company').val(),
					email: cjq('#email').val(),
					phone: cjq('#phone').val(),
					password: cjq('#password').val(),
					url: cjq('#url').val(),
					username: cjq('#username').val(),
					friendCode: cjq('#friendCode').val(),
					merchantId: cjq('#merchantId').val()
				},
				success: function(data){

				}
			});
		});

		if(document.URL.indexOf("#freeTrial") >= 0){
			cjq('#lightbox').fadeIn('fast');
			cjq('.stepNumber').text('1');
			cjq('.content1, .steps').show();
			cjq('.content2, .content3, .processing').hide();

			e.preventDefault();
			e.stopPropagation();
			return false;
		}
	}); // end document.ready
</script>


<script type="text/javascript">
	adroll_adv_id = "LBJICOTBDNG4REHQFV5S72";
	adroll_pix_id = "6H4F2K4X2FE2LDUYPTESUN";
  
	(function () {
		var oldonload = window.onload;
		window.onload = function(){
  		 __adroll_loaded=true;
   		var scr = document.createElement("script");
   		var host = (("https:" == document.location.protocol) ? "https://s.adroll.com" : "http://a.adroll.com");
          
   		scr.setAttribute('async', 'true');
   		scr.type = "text/javascript"; 
  		scr.src = host + "/j/roundtrip.js";
          
   		((document.getElementsByTagName('head') || [null])[0] ||
    		document.getElementsByTagName('script')[0].parentNode).appendChild(scr);
   		if(oldonload){oldonload()}};
	}());
</script>

<script type="text/javascript">
  function gclidSetCookie(name, value, days){
    var date = new Date();
    date.setTime(date.getTime() + (days*24*60*60*1000)); 
    var expires = "; expires=" + date.toGMTString();
    var pathAndDomain = "; path=/; domain=.ultracart.com";
    document.cookie = name + "=" + value + expires + pathAndDomain;
  }
  function gclidGetParam(p){
    var match = RegExp('[?&]' + p + '=([^&]*)').exec(window.location.search);
    return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
  }

  jQuery(document).ready(function(){
    var gclid = gclidGetParam('gclid');
    if(gclid){
      var gclsrc = gclidGetParam('gclsrc');
      if(!gclsrc || gclsrc.indexOf('aw') !== -1){
	    gclidSetCookie('gclid', gclid, 90);
	  }
    }
  });
</script> 




</body>
</html>";i:2;s:4:"4750";}