<input type="hidden" id="saytArea" value="saytgec"/>
<input type="hidden" id="saytShowCat1" value="true"/>
<script src="/js/sayt.js"></script>

<script>

	
	$('#searchInput').sayt({
        minLength: 1,
		source: function(request, response) {
			   minLength: 1,
           $.getJSON(saytDomainId + 'autocomplete?callback=?',
                { // request block, see parameters above
                    q:request.term,
                    s:'globalindustrialcom',
                    a:'saytgec',
                    c:'systemax'
                }, response);
        },
        focus: function(event, ui) {
           return false;
       },
        select: function(event, ui) {
			return false;
        }
				
    });
	
	$('ul.ui-autocomplete').removeClass('ui-front ui-menu ui-widget ui-widget-content ui-corner-all');	
	
	if ( (	$(window).width() >= 1300) && ($('div.responsive').length) ) {
		$('ul.ui-autocomplete').addClass('responsive');
	}
	
	$(window).resize(function () {
		if ( ($(window).width() >= 1300) && ($('div.responsive').length) ) {
			$('ul.ui-autocomplete').addClass('responsive');
		}
		
		else {
			$('ul.ui-autocomplete').removeClass('responsive');
		}
		
		if ($('#sayt-menu').length) {
			var offsetSearch = $('#searchInput').offset();
			$('#sayt-menu').css('left',offsetSearch.left);
		
			if ($('#sayt-menu').hasClass('responsive')) {
				$('#sayt-menu').width($('#searchInput').width() + 5)
			}
				
			else {
				var $searchInput = $('#searchInput').innerWidth();
				var $searchBtn = $('.search_nav').innerWidth();
				$('#sayt-menu').width(($searchInput + $searchBtn) - 2)
			}
			
		}
	});

</script>
        
	





