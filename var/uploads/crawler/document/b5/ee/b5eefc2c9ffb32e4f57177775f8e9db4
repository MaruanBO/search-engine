		$('[class^="linkhover"]').mouseover(function(){
			$("."+$(this).attr('class')).addClass("hovered");
		})
		$('[class^="linkhover"]').mouseout(function(){
			var myclass = $(this).attr('class');
			var realclass = myclass.replace(" hovered", "");
			$("."+realclass).removeClass("hovered");
		});
		$('.showExtraArchive').click(function(){
			$(".extraArchive").show();
			$(".hideExtraArchive").show();
			$(".showExtraArchive").hide();
			$(".fewArchive").hide();
		});
		$('.hideExtraArchive').click(function(){
			$(".extraArchive").hide();
			$(".hideExtraArchive").hide();
			$(".showExtraArchive").show();
			$(".fewArchive").show();
		});
