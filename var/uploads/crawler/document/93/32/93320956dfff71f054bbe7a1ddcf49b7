<script type="text/javascript">
    $(document).ready(function() {
        $(".sale").each(function(){
            if ($(this).attr("propCode")!= undefined) {
                var $rHolder = $(this);
                $.fn.getLowestRate($(this).attr("propCode"),function(data){
                    if(data != undefined){
                        $rHolder.html("$"+data)
                    }else{
                        $rHolder.closest(".deal-box-rates").html("");
                    }
                });
            }else{
                $(this).closest(".deal-box-rates").html("");
            }
        });
    });
</script>

