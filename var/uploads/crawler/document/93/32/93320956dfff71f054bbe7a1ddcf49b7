<script type="text/javascript">

    (function()
    {
        var tier="";
        var accountBalance="";
        var offersCode = "";
        var lastUpdate = "";
        var isSignedIn = "notSignedIn";
        var tierNumber = "";
        var tierEmailAddress = "";
        var fullURL = document.URL;
        var normalizedURL = location.host + location.pathname;
        var view = window.navigator.userAgent;

        try{
            if(!(typeof __userObj === 'undefined')){
                if ((__userObj.isLoggedIn == "true")) {
                    accountBalance = __userObj.accountBalance;
                    tier = __userObj.tier;
                    lastUpdate = __userObj.lastTrUpdate;
                    isSignedIn = "signedIn";

                    accountBalance = accountBalance.replace(",","");
                }
            }

            window.dataLayer = [{
                'view':view,
                'L1':'Caesars Entertainment',
                'L2':'Caesars Entertainment: Home',
                'L3':'Caesars Entertainment: Home',
                'pageCategory':'Home',
                'propertyCode':'',
                'hotel_id':'',
                'market_code':'',
                'signinStatus':isSignedIn,
                'email_addr': tierEmailAddress,
                'tr_number': tierNumber,
                'acct_balance': accountBalance,
                'last_activity': lastUpdate,
                'offer_code': offersCode,
                'tier':tier,
                'URL': fullURL,
                'nURL': normalizedURL
            }];

            window.dataLayer.push({'event': 'eventNameXYZ'});

        }catch(e){
            //do nothing?!
        }

    })();

</script>

