					onTouch : true
				},
			});
			
						
			//trigger movie page click for .cmovie
			$('.more-info').click(function() {
				var url = $(this).attr("data-role");
				window.location.href = url;
			})
			
			//Bind click events to Facebook and Twitter buttons
			$('#facebook-button').click(function() {
				$('#twitter').hide();
				$('#facebook').show();
				$('#feed-wrapper').css('background-color','#3b5998');
			});
			
			$('#twitter-button').click(function() {
				$('#facebook').hide();
				$('#twitter').show();
				$('#feed-wrapper').css('background-color','#55acee');
			});
		});
