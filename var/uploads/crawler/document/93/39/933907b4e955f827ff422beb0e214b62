    <script type=text/javascript>
        function checkKey(e) {
            if (window.event && window.event.keyCode == 13) {
                launchMiLocator(); 
            }
            else if (e && e.keyCode == 13) {
                launchMiLocator(); 
            } 
        }
        function launchMiLocator() {
            srchText = document.getElementById('txtLocationSearch').value;
            //Production:
            window.open("http://www.mcgi.state.mi.us/milocator/default.aspx?search=" + srchText, "MiLocator");
 
        }
                        </script>
    <input title='Go' style='position:relative; top:4px' onclick="launchMiLocator('')" alt='GO' src="/images/som/SOM_SEARCH_GO_322960_7.gif" type='image' name='btnLocationSearch'>
