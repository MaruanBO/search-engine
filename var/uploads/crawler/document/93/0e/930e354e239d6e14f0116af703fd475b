$(function(){
  $('#scrollheader').data('size','big');
});

$(window).scroll(function(){
  if($(document).scrollTop() > 30)
{
    if($('#scrollheader').data('size') == 'big')
    {
        $('#scrollheader').data('size','small');
        $('#scrollheader').show();
    }
}
else
  {
    if($('#scrollheader').data('size') == 'small')
      {
        $('#scrollheader').data('size','big');
        $('#scrollheader').hide();
      }  
  }
});

</script>
