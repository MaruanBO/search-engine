(function($){
	$('#latest-news ul li').css({display:'none'}).find('a').css({height:'28px'});
	$('#latest-news ul').each(function(){
		$(this).find('li:first').show();
	});
	setInterval('rotateheadlines()',6000);
	
	var scrollbarWidth = function() {
	    var div = $('<div style="width:50px;height:50px;overflow:hidden;position:absolute;top:-200px;left:-200px;"><div style="height:100px;"></div>');
	    $('body').append(div);
	    var w1 = $('div', div).innerWidth();
	    div.css('overflow-y', 'scroll');
	    var w2 = $('div', div).innerWidth();
	    $(div).remove();
	    return (w1 - w2);
	};
	var scrollPx = scrollbarWidth();
  $('#filter-bar .thumb-view').click(function(ev){
      ev.preventDefault();
      $('div.article:visible').fadeOut('fast',function(){
        $('#river').removeClass('list');
        $(this).fadeIn("fast");
      });      
    });
    $('#filter-bar .list-view').click(function(ev){
      ev.preventDefault();
      $('div.article:visible').fadeOut('fast',function(){
        $('#river').addClass('list');
        $(this).fadeIn("fast");
      });
    });

 
  $('#river div.article').find('.header').append('<a class="expander" href="javascript:void()">Quick Look</a>');
  $('#river .expander').live('click',function(ev){
    ev.preventDefault();
    var parent = $(this).closest('div.article');
    var origurl = parent.find('.header').first().find('a').first().attr('href');
    var posturl = origurl.replace(/^https?:\/\/[^\/]+/,'/services/show-post');
    // var posturl = origurl.replace(/^https?:\/\/[^\/]+/,'http://quicklook.sandbox.tuaw.com/services/show-post');
    $('body').append($('<div id="articleoverlay">'));
    $(window).bind('keydown', function (e) {
      if (e.keyCode === 27) {
        $(window).unbind('keydown');
        $('#articleoverlay').fadeOut('fast',function(){
           $(this).remove();
           $('body').css({overflow:'auto',marginRight:'0px'});
        });
      }
      return true;
    });
    $.get(posturl,function(data){
      $('#articleoverlay').append($('<div id="quicklook" style="display:none">').append($('<div class="expanded">').append(data)).append($('<a href="'+origurl+'" id="qlfullstory">Full Story</a>')));		
      $('body').css({overflow:'hidden',marginRight:scrollPx+'px'});
	$("[name=aol-share]","#quicklook").aolShare({
		services: ["facebook", "twitter", "stumbleupon", "pinterest", "reddit", "plusone"],
		templates:{ "twitter":{"twitterStatus":"url={{url}}&text={{title}}&via=TUAW"}},
		plugins: {
			"plusone": {
				init: function ($elem, postUrl, options) {
					$.getJS("http://o.aolcdn.com/os/aol/1-jquery.plusone.js", function () {
						$elem.plusone({"size": "tall", "annotation": "none", "href": postUrl });
						$elem.css("background", "none");
					});
				}
			}
		}
	});
	$(".aol-share-print").attr("href","print");
	//$.facebookSocial({
	//	options: {
	//		appId: 110670252318532    
	//	}
	//});

	$("[name=aol-facebook-like]","#quicklook").facebookSocial();

      $('#quicklook').prepend($('<a href="#" id="qlclose">CLOSE</a>').css({'display':'block','textIndent':'-99999px','width':'30px','height':'30px','position':'absolute','left':'0','top':'0'})).fadeIn("fast");
    });
  });
  $('#qlfullstory').live('click',function(e){
    e.preventDefault();
    window.location.href = this.href;
    return false;
  });
  $('#articleoverlay,#qlcose').live('click',function(e){
    if (!$(e.target).closest('div').hasClass('expanded')) {
      e.preventDefault();
      $(this).fadeOut('fast',function(){ 
        $(this).remove(); 
        $('body').css({overflow:'auto',marginRight:'0px'});
      });
			return false;
    }
		return true;
  });
})(jQuery);

function rotateheadlines() {
	$('#latest-news ul').each(function(){
		var el = this;
		var current = ($('li.show',el)?  $('li.show',el) : $('li:first',el));
	  if ( current.length == 0 ) current = $('li:first',el);
		var next = ((current.next().length) ? ((current.next().hasClass('show')) ? $('li:first',el) :current.next()) : $('li:first',el));
		current.fadeOut('fast',function(){
			$(this).removeClass('show');
			next.addClass('show').fadeIn("fast");
		});			
	});
};
function forceLazyScrollUpdate() {
  var i = 1;

  do {
    $("body, html").animate({ scrollTop: window.pageYOffset + i }, 10);
  } while ((i -= 2) > -2);
} 
(function($){
	$(".aol-share-perm").aolShare({
		services: ['facebook', 'twitter','googleplus'],
	        templates: {
	            twitter: {
	                twitterStatus: "url={{url}}&text={{title}} &via=TUAW"
	            }
	        },
		css: {
