$(document).ready(function(){
// Removing empty paragraphs first
$("#content-body-711931 p").filter( function() {
var html = $.trim($(this).html());
if(html == '' || html == '&nbsp;')
return true;
}).remove();
// Taking a subset of paragraphs using offset and maximum
var offset = 1;
var max = -1;
if(offset > 1){
$("#content-body-711931").children().slice(0, offset-1).remove();
}
if(max > -1){
$("#content-body-711931").children().slice(max).remove();
}
// Show dateline field at the beginning of the first paragraph
var showDateline = true;
var datelineStr = "";
if(offset <= 1 && showDateline == true && datelineStr != "") {
$("#content-body-711931 p:first").prepend("<span>"+ datelineStr + "- </span>");
}
});
</script>
