<section class="element_spacing_large">
<div id="content-body-711925">
<div class="heading"><a href="/site-services/public-insight-network/">Public Insight Network</a></div>
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">
google.load("feeds", "1");
function initialize() {
var content = "";
var feed = new google.feeds.Feed("https://www.publicinsightnetwork.org/rss/MiamiHeraldWLRN");
feed.setNumEntries(1);
feed.setResultFormat(google.feeds.Feed.MIXED_FORMAT);
feed.load(function(result) {
if (!result.error) {
for (var i = 0; i < result.feed.entries.length; i++) {
var entry = result.feed.entries[i];
content += '<article class="headline-list"><div class="teaser">';
content += '<div class="title element_spacing_small">';
content += '<a href="'+entry.link+'">'+entry.title+'</a>';	content += '</div>';
content += '</div></article>';
}
$("#pinfeed").html(content);
}
});
}
google.setOnLoadCallback(initialize);
</script>
<div id="pinfeed"></div>
