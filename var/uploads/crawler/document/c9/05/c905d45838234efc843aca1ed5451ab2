<section class="element_spacing_large">
<div id="content-body-711924">
<div class="heading"><a href="http://www.miami.com/">Miami.com</a></div>
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">
google.load("feeds", "1");
function initialize() {
var content = "";
var feed = new google.feeds.Feed("http://www.miami.com/rss/nodequeue/1/");
feed.setNumEntries(3);
feed.setResultFormat(google.feeds.Feed.MIXED_FORMAT);
feed.load(function(result) {
if (!result.error) {
for (var i = 0; i < result.feed.entries.length; i++) {
var entry = result.feed.entries[i];
var thumb = entry.xmlNode.getElementsByTagName("enclosure")[0].getAttribute("url");
if (i<1) {
content += '<article id="trending" class="media"><div class="teaser cover">';
content += '<div class="img-container picture">';
content += '<a href="'+entry.link+'">';
content += '<img src="'+thumb+'" class="img-responsive">';
content += '</a>';
content += '</div>';
content += '<div class="headline element_spacing_small">';
content += '<h4 class="title"><a href="'+entry.link+'">'+entry.title+'</a></h4>';
content += '</div>';
content += '</div></article>';
} else {
content += '<article class="headline-list media"><div class="teaser">';
content += '<div class="img-container pull-left picture" style="width:30%;">';
content += '<a href="'+entry.link+'">';
content += '<img src="'+thumb+'" class="img-responsive">';
content += '</a>';
content += '</div>';
content += '<div class="title element_spacing_small">';
content += '<a href="'+entry.link+'">'+entry.title+'</a>';
content += '</div>';
content += '</div></article>';
}
}
$("#miamicomfeed").html(content);
}
});
}
google.setOnLoadCallback(initialize);
</script>
<div id="miamicomfeed"></div>
