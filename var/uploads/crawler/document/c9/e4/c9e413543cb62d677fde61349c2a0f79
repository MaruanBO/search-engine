    <script type="text/javascript">	

		jQuery(function() {

			var currentLiIndex = 0,
				liWidth = 913,
				$carouselWrapper = jQuery('#videohaveyouseen ul.carousel'),
				totalLis = $carouselWrapper.children('li').length,
				$currentVideoLi = jQuery("#currentVideoLi");

			function nextCarousel() {
				if(currentLiIndex == totalLis-1) {
					$carouselWrapper.animate({
				    	left: "0"
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				  	currentLiIndex = 0;
				} else {
					currentLiIndex++;
					$carouselWrapper.animate({
				    	left: "-" + (liWidth * currentLiIndex)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function prevCarousel() {
				if(currentLiIndex === 0) {
					$carouselWrapper.animate({
						left: "-" + ((totalLis - 1) * liWidth)
				  	}, 500, function() {
						anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				  	currentLiIndex = totalLis -1;
				} else {
					currentLiIndex--;
					$carouselWrapper.animate({
				    	left: (liWidth * currentLiIndex)*(-1)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function videoCarouselInit(jsonObj) {
				if($currentVideoLi.length > 0)
					jQuery("#totalVideoLi").text(totalLis);

				jQuery('#videohaveyouseen div.controls a').on('click', function(e) {
					e.preventDefault();
					if(jQuery(this).hasClass('pg-nextBtn'))
						nextCarousel.call(this);
					else if(jQuery(this).hasClass('pg-prevBtn'))
						prevCarousel.call(this);
			    });

			    jQuery('#videohaveyouseen ul.carousel li a').on("click", function(e) {
			    	e.preventDefault();
			    	jsonObj[0].playlist["initialIndex"] = jQuery(this).data('index');
			    	jQuery.publish('launchVideoOverlay', jsonObj);
			    	anTrackLink(this, 'espn', 'page1videostripe', 'video_'+jQuery(this).data('videoid'));
			    });
			}

			videoCarouselInit([{playlist:{title:"Video Playlist",videos:[{id:12347735,thumbnail:"/media/motion/2015/0219/dm_150219_nba_blazers_afflalo_trade/dm_150219_nba_blazers_afflalo_trade.jpg",title:"Afflalo Traded To Blazers",description:"Amin Elhassan and Brad Daugherty break down the Trail Blazers' acquisition of Arron Afflalo from the Nuggets in a package involving five players and a first-round draft pick.",caption:"Afflalo Traded To Blazers",footer:"00:01:24"},{id:12347035,thumbnail:"/media/motion/2015/0219/dm_150219_nhl_levymelrose_weirdtrades/dm_150219_nhl_levymelrose_weirdtrades.jpg",title:"Hockey's Weirdest Trades",description:"Steve Levy and Barry Melrose talk about some of hockey's weirdest trades including the time in juniors when Tom Martin was traded for a bus.",caption:"Hockey's Weirdest Trades",footer:"00:01:33"},{id:12347240,thumbnail:"/media/motion/2015/0219/dm_150219_towson_dunk/dm_150219_towson_dunk.jpg",title:"Towson's Towering Dunk",description:"Towson's Timajh Parker-Rivera threw down a one-handed jam against Hofstra.",caption:"Towson's Towering Dunk",footer:"00:00:13"},{id:12347284,thumbnail:"/media/motion/2015/0219/dm_150219_nascar_craven_daugherty_on_dale/dm_150219_nascar_craven_daugherty_on_dale.jpg",title:"Expectations For Junior",description:"Ricky Craven and Brad Daugherty explain why Dale Earnhardt Jr. has been so successful in restrictor plate races and how Earnhardt will adjust to working with new crew chief Greg Ives.",caption:"Expectations For Junior",footer:"00:03:04"},{id:12347313,thumbnail:"/media/motion/2015/0219/dm_150219_nfl_inside_the_huddle_draft_full/dm_150219_nfl_inside_the_huddle_draft_full.jpg",title:"Clayton: Inside The Huddle",description:"John Clayton compares this year's rich draft class to last year's strong group, looks at the low profile of the tight end draft class, reviews the effects of a deep running back class on veteran RBs, and says the quarterback draft class is weak.",caption:"Clayton: Inside The Huddle",fo