<script type="text/javascript">
$(document).ready(function() {
	$('.follow-content').hide();
	$('.follow-content').eq(0).show();
	$('.follow-content').eq(0).addClass('active');
	$('.hm-pointer').eq(0).addClass('active');

	$('.hm-share').click(function() {
		$('.follow-content').removeClass('active');
		$('.hm-pointer').removeClass('active');
		$('.hm-share').removeClass('active');
		$(this).addClass('active');
		$(this).nextAll('.hm-pointer').first().addClass('active');
	});
	$('.hm-share-twitter').click(function() {
		$('.follow-content').hide();
		$('.twitter').show();
	});
	$('.hm-share-facebook').click(function() {
		$('.follow-content').hide();
		$('.facebook').show();
	});
	$('.hm-share-rss').click(function() {
		$('.follow-content').hide();
		$('.rss').show();
	});
	$('.hm-share-mobile').click(function() {
		$('.follow-content').hide();
		$('.mobile').show();
	});
	$('.hm-share-google').click(function() {
		$('.follow-content').hide();
		$('.google').show();
	});
	$('.hm-share-pinterest').click(function() {
		$('.follow-content').hide();
		$('.pinterest').show();
	});
});
</script>

