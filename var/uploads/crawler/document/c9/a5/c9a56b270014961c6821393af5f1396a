<div id="center-universal" class="clearfix">
  <div id="lower-right-sidebar" class="right clearfix">
	<div id="lower-right-sidebar-item" style="background-image:none;">
    <div id="adv_search"> <img id="finder-logo" src="/images/find-a.png" width="135" />
      <div class="wrapper">
    <script language="javascript">
  function changeSuperFinder(el) {
    idlist = ['1', '2', '3', '4', '5', '6', '7'];
    selid = el[el.selectedIndex].value;
    var block_set = 0;
    var i;
    var j;
    for (i=1; i <= idlist.length; i++) {
      elid = 'superfinder_' + i;
      formid = 'superfinder_' + i + '_form';
      for (j = 0; j <= idlist.length; j++) {
        selectorelement = document.getElementById(formid)['SuperFinderSelector'][i-1];
        selectorelement.selected = false;
      }
      if (i == selid) {
        document.getElementById(elid).style.display='block';
        document.getElementById(formid)['SuperFinderSelector'][i-1].selected = true;
        block_set = 1;
      }
      else {
        document.getElementById(elid).style.display='none';
      }
    }
  }
</script>
<div class="box-finda" id="superfinder_1">
  <!-- START Location Finder -->
  <form name="superfinder_1_form" id="superfinder_1_form">
    <fieldset>
      <legend>Find A ...</legend>
      <ol>
        <li class="first">
                    <select name="SuperFinderSelector" onchange="javascript: changeSuperFinder(this); return false;">
            <option selected="selected"  value="1">Home, Restaurant, etc...</option>
            <option  value="2">Restaurant</option>
            <option  value="3">Where &amp; When / Event</option>
            <option  value="4">Happy Hour</option>
            <option  value="5">Spa</option>
            <option  value="6">Home</option>
            <option  value="7">Hotel</option>
          </select>        </li>
        <li class="last">
          <input type="submit" value="search" id="submit" onclick="alert('Please select a Location Type');return false;" />
        </li>
      </ol>
    </fieldset>
  </form>
  <!-- END Location Finder -->
</div>
