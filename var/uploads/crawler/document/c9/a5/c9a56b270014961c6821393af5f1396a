<script type="text/javascript">
$(document).one("ready", openPopper);

function openPopper() {
    if(!readCookie('visitedBefore')) {
        $("div#sub-footer").fadeIn(100, function() {
            createCookie('visitedBefore', 'visitedBefore', 1);
                                                
        });
    }

    
    // close panel
    $("a#close").click(function(){
        $("div#sub-footer").fadeOut(300);            
    });

}

    
// Timer Script
     $(function(){
      window.setInterval(function() {
         $('#sub-footer').fadeOut(300);
            }, 24000);

        });

</script>

