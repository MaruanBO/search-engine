<script type="text/javascript">

var icpForm1674 = document.getElementById('icpsignup1674');

if (document.location.protocol === "https:")

	icpForm1674.action = "https://app.icontact.com/icp/signup.php";
function verifyRequired1674() {
  if (icpForm1674["fields_email"].value == "") {
    icpForm1674["fields_email"].focus();
    alert("The Email field is required.");
    return false;
  }

    if (
      !icpForm1674["listid:9857616"].checked &&
      !icpForm1674["listid:9857617"].checked &&
      !icpForm1674["listid:9857618"].checked &&
      !icpForm1674["listid:9857619"].checked &&
      !icpForm1674["listid:9857621"].checked &&
      !icpForm1674["listid:9871285"].checked &&
      !icpForm1674["listid:9873074"].checked &&
      !icpForm1674["listid:9875605"].checked &&
      !icpForm1674["listid:9878551"].checked &&
      !icpForm1674["listid:9878230"].checked &&
      true)  {
    alert("The Lists field is required.");
    return false;
  } 

return true;
}
</script>

