      jQuery(document).ready(function(){
        jQuery('.ltbp_tms_tabs').tabs({
          activate: function( event, ui ) {
            var frame = ui.newPanel.find('.slidedeck_frame');
            window.FT.create_flexslider( frame );
          },
          create: function( event, ui ) {
            jQuery('.ltbp_tms_tabs').removeClass('loading');
            Footer_Testimonials.tabs_loaded = true;
            var frame = ui.panel.find('.slidedeck_frame');
            window.FT.create_flexslider( frame );
          }
        });
        jQuery('.fbx-instance').on('foobox.afterShow', function( e ) {
          jQuery( '.fbx-show' ).find('img').each(function( i, img ) {
            img = jQuery( img );
            if ( img.data('thumbnail-src') ) {
              img.attr( 'src', img.data( 'thumbnail-src' ) );
            }
          });
        });
      });
