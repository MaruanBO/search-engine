      <script>
        (function(){

          window.Footer_Testimonials = {};
          window.FT = window.Footer_Testimonials;
          window.FT.num_slidedecks = 9;
          window.FT.num_slidedecks_loaded = 0;
          window.FT.tabs_loaded = false;

          window.FT.widget_loaded = function() {
            return window.FT.num_slidedecks_loaded == window.FT.num_slidedecks && window.FT.tabs_loaded;
          }

          window.load_thumbnails = function() {

            var widget = jQuery('.widget_ltbp_testimonials_widget');
            var open_panel = widget.find('.ui-tabs-panel:visible');
            var slider = open_panel.find('.slidedeck_frame').data('flexslider');
            var current_slide = jQuery( open_panel.find('li').get( slider.currentSlide ) );

            if ( current_slide.hasClass('thumbnails_loaded') ) return;

            current_slide.addClass('thumbnails_loaded');
            current_slide.find('.lazy-load-bg').each(function(e){
              var el = jQuery(this);
              el.attr('style', el.data('thumbnail-src'));
            });
          };

          window.FT.create_flexslider = function( obj ) {

            if ( !obj.data('flexslider') ) {
              obj.flexslider({
                animation: 'slide',
                animationLoop: false,
                slideshow: false,
                controlNav: false,
                directionNav: false,
                keyboard: false,
                itemWidth: 862, //199,
                itemMargin: 0, //12,
                start: function( slider ) {

                  // FlexSlider doesn't have an actual "load" event that I know of yet,
                  // so we just remove the loading class from the widget on the first
                  // slide load.
                  window.FT.num_slidedecks_loaded += 1;
                  window.load_thumbnails();
                  slider.first().removeClass('loading');
                },
                after: function( slider ) {

                  window.load_thumbnails();
                }
              });
            }
          }

        }());
      </script>

