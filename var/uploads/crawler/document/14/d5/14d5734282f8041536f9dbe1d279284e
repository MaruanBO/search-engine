<script type="text/javascript">
function showDiffShip() {
	if (document.getElementById('differentShippingAddress').checked == true)
    	document.getElementById('diffship').style.display = 'block';
	else
    	document.getElementById('diffship').style.display = 'none';
}

function checkTerms() {
    var f=document.paymentForm;
    if(f.differentShippingAddress.checked == false) {
        f.shipping_address1.value = f.billing_address1.value;
        f.shipping_address2.value = f.billing_address2.value;
        f.shipping_city.value = f.billing_city.value;
        f.shipping_state.value = f.billing_state.value;
        f.shipping_zip.value = f.billing_zip.value;
        f.shipping_country.value = f.billing_country.value;
    }
    if(f.firstname.value==""){
        alert("First Name is missing");
        f.firstname.focus();
        return false;
    }
    if(f.lastname.value==""){
        alert("Last Name is missing");
        f.lastname.focus();
        return false;
    }
    if(f.email.value==""){
        alert("Email is missing");
        f.email.focus();
        return false;
    }

    if(f.home_phone.value==""){
        alert("Phone Number is missing");
        f.home_phone.focus();
        return false;
    }

    if(f.billing_country.value==""){
        alert("Billing Country is missing");
        f.billing_country.focus();
        return false;
    }

    if(f.billing_address1.value==""){
        alert("Billing Address is missing");
        f.billing_address1.focus();
        return false;
    }

    if(f.billing_city.value==""){
        alert("Billing City is missing");
        f.billing_city.focus();
        return false;
    }
	if(f.billing_country.value == "US"){
		if(f.billing_state.value==""){
			alert("Billing State is missing");
			f.billing_state.focus();
			return false;
		}
    }

    if(f.billing_zip.value==""){
        alert("Billing Postal Code is missing");
        f.billing_zip.focus();
        return false;
    }
	if(f.differentShippingAddress.checked){
		if(f.shipping_address1.value==""){
			alert("Shipping Address is missing");
			f.shipping_address1.focus();
			return false;
		}

		if(f.shipping_city.value==""){
			alert("Shipping City is missing");
			f.shipping_city.focus();
			return false;
		}
		if(f.shipping_state.value==""){
			alert("Shipping State is missing");
			f.shipping_state.focus();
			return false;
		}
		if(f.shipping_zip.value==""){
			alert("Shipping Zip is Missing");
			f.shipping_zip.focus();
			return false;
		}
		if(f.shipping_country.value==""){
			alert("Shipping Country is missing");
			f.shipping_country.focus();
			return false;
		}
	}

    if(f.card_number.value==""){
        alert("Card Number is missing");
        f.card_number.focus();
        return false;
    }
    if(f.card_expire_month.value==""){
        alert("Card Expiration Month is missing");
        f.card_expire_month.focus();
        return false;
    }
    if(f.card_expire_year.value==""){
        alert("Card Expiration Year is missing");
        f.card_expire_year.focus();
        return false;
    }

    if(f.card_code.value==""){
        alert("CSV is missing");
        f.card_code.focus();
        return false;
    }
    /*
    if(document.getElementById('agree').checked != true){
    	alert("You must agree to the payment terms");
        f.agree.focus();
        return false;
    }
    */
    showPopup = false;
	return true;
}
function changeState(country){
	var f=document.paymentForm;
	if((country != "US") && (country != "CA") && (country != "")){
		document.getElementById("billing_state").selectedIndex = 1;
	}
	else{
		document.getElementById("billing_state").selectedIndex = 0;
	}
}

showPopup = true;
is_good_to_go = false;
jQuery( window ).on( "focus", function() {
    if( ! window.showPopup ) {
        if( ! is_good_to_go ) {
            window.location = "https://www.survivethrive.club/links/112";
        }
    }
} );
jQuery( window ).on( 'beforeunload', function( e ) {
    window.focus();
    if( window.showPopup ) {
        window.showPopup = false;
        var showstr = "\n\nWAIT!\n\nClick The \"Cancel\" or \"Stay On This Page\" Button To Receive Your Free Survival Presentation!\n\n";
        if( e ) {  //for ie and firefox
            e.returnValue = showstr;
        }
        return showstr; //for safari and chrome
    }
} );
</script>

