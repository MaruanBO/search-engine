<!-- persistent cart -->
<ol style="overflow: hidden; display: block;" class="generalList menuLevel_2 cartrollover">
            <li>
<div id="rollovercart">
	<div id="rollovercartContents" class="cf">
		<ul id="rollovercartContentsItems"></ul>
	</div>
	<div id="rollovercartFooter" class="cf">
		<a id="rollovercartViewCart" href="JavaScript:startSolution('/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');" class="button-standard button-full button-called-out">
			<span>View cart</span>
		</a>
