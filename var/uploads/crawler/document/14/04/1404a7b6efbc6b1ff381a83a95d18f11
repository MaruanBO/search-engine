            speed : 500,
            auto : false,
            controls : true,
            prevSelector : '.slider-wrapper-3fa1a56a7cc11bd8 .icon-next',
            nextSelector : '.slider-wrapper-3fa1a56a7cc11bd8 .icon-prev',
            slideWidth : 580,
            infiniteLoop : true,
            adaptiveHeight : true,
            responsive : true,
            captions : false,
            pager : false
        });

    var delayResize_3fa1a56a7cc11bd8;

    $(window).on('resize', function (e) {
        clearTimeout(delayResize_3fa1a56a7cc11bd8);
        delayResize_3fa1a56a7cc11bd8 = setTimeout(function () {

                if ($sliderDom_3fa1a56a7cc11bd8.length !== 0) {
                    $slider_3fa1a56a7cc11bd8.goToNextSlide();
                }

            }, 1000);
    });
})
</script>
<!-- end #series -->

