	<script type="text/javascript">
    var googletag = googletag || {};$(this).data('postId')
    googletag.cmd = googletag.cmd || [];
    (function() {
        var gads = document.createElement("script");
        gads.async = true;gads.type = "text/javascript";
        var useSSL = "https:" == document.location.protocol;
        gads.src = (useSSL ? "https:" : "http:") + "//www.googletagservices.com/tag/js/gpt.js";
        var node = document.getElementsByTagName("script")[0];
        node.parentNode.insertBefore(gads, node);
    })();
</script>

<script>
    var adZone = {
            _300x250_2: 'mid_300x250',
            _300x250_3: 'bottom_300x250',
            _300x250_4: 'promo_300x250',
            _2000x420: 'peek_2000x420'
        },

        adSlot = {
            _320x50: 'div-gpt-ad-320x50',
            _728x90: 'div-gpt-ad-728x90',
            _970x90: 'div-gpt-ad-970x90',
            _300x250_1: 'div-gpt-ad-300x250-1',
            _300x250_2: 'div-gpt-ad-300x250-2',
            _300x250_3: 'div-gpt-ad-300x250-3',
            _300x250_4: 'div-gpt-ad-300x250-4',
            _2000x420: 'div-gpt-ad-2000x420',
            _200x200: 'div-gpt-ad-200x200'
        },

        adTarget = {
            _category: mm_globals.category,
            _subCategory: mm_globals.sub_category,
            _subSubCategory: mm_globals.sub_sub_category,
            _channel: mm_globals.channel,
            _postId: mm_globals.post_id,
            _tags: mm_globals.tags,
            _template: mm_globals.template_type
        },

        adUnit = '/4678/brk.mdmn/',

        winWidth = $(window).innerWidth(),
        definedAds = [],
        hptoContainer,
        currentTakeoverSize = 0,
        adCreative = []; // Record ad creative IDs and line item IDs that load. Use this for troubleshooting ads.


    // Homepage ad zone is "home", other pages will be "ros"
    if(adTarget._category == 'home') {
        mainZone = 'home';
    } else {
        mainZone = 'ros';
    }

    // Set header ad unit and first 300x250 ad zone to the main zone
    adZone._200x200 = adZone._320x50 = adZone._728x90 = adZone._970x90 = adZone._300x250_1 = mainZone;

    // Mobile devices will get different ad zones
    if(MMUI.globals.isMobile()) {
        adZone._320x50 = '320x50_mobileweb';
        adZone._728x90 = '728x90_mobileweb';
    }

    // Use swapAdZone for testing
    if(MMUI.globals.getQueryVariable('swapAdZone')) {
        adZone._200x200 = adZone._320x50 = adZone._728x90 = adZone._970x90 = adZone._300x250_1 = adZone._2000x420 = MMUI.globals.getQueryVariable('swapAdZone'); // Middle and bottom 300x250 units cannot have the same adZone as the top 300x250
    }

    if(MMUI.globals.getQueryVariable('swapAdZone2')) {
        adZone._300x250_2 = MMUI.globals.getQueryVariable('swapAdZone2');
    }

    if(MMUI.globals.getQueryVariable('swapAdZone3')) {
        adZone._300x250_3 = MMUI.globals.getQueryVariable('swapAdZone3');
    }

    if(MMUI.globals.getQueryVariable('swapAdZone4')) {
        adZone._300x250_4 = MMUI.globals.getQueryVariable('swapAdZone4');
    }

    if(MMUI.globals.getQueryVariable('cid')) {
        adTarget._channel = MMUI.globals.getQueryVariable('cid');
    }

    function showAdSlot(zone, dimensions, slot) {
        if($.inArray(slot, definedAds) == -1 && $('#'+slot).length > 0) { // Make sure ad slot hasn't already been defined and ad zone exists on page
            googletag.cmd.push(function () { 
                googletag.defineSlot(adUnit + zone, dimensions, slot).setCollapseEmptyDiv(true, true).addService(googletag.pubads())
                    .setTargeting('category',adTarget._category)
                    .setTargeting('subcategory',adTarget._subCategory)
                    .setTargeting('subsubcategory',adTarget._subSubCategory)
                    .setTargeting('channel',adTarget._channel)
                    .setTargeting('postid',adTarget._postId)
                    .setTargeting('tags',adTarget._tags)
                    .setTargeting('template',adTarget._template);
                googletag.enableServices();
                googletag.display(slot); 
            });
        
            definedAds.push(slot);

            if(typeof MMUI.tracking != 'undefined' && (!MMUI.globals.getQueryVariable('swapAdZone') && !MMUI.globals.getQueryVariable('swapAdZone2') && !MMUI.globals.getQueryVariable('swapAdZone3') && !MMUI.globals.getQueryVariable('swapAdZone4'))) {
                MMUI.tracking.sendEvent(dimensions[0] + 'x' + dimensions[1] + '_' + adUnit + zone, 'Call', mm_globals.pageView);
            }
        }
    }


    function showHeaderAd() {
        if(MMUI.globals.isMobile()) {

            // Mobile devices will only support 2 header units, 320x50 and 728x90
            if(winWidth < 728) {
                showAdSlot(adZone._320x50, [320, 50], adSlot._320x50);
            } else {
                showAdSlot(adZone._728x90, [728, 90], adSlot._728x90);
            }

        } else {
            
            if(winWidth <= 480) {
                if(currentTakeoverSize != 320) {
                    showAdSlot(adZone._320x50, [320, 50], adSlot._320x50);
                    currentTakeoverSize = 320;
                }
            } else if(winWidth <= 970) {
                if(currentTakeoverSize != 728) {
                    showAdSlot(adZone._728x90, [728, 90], adSlot._728x90);
                    currentTakeoverSize = 728;
                }
            } else if(winWidth > 970) {
                if(currentTakeoverSize != 970) {
                    showAdSlot(adZone._970x90, [970, 90], adSlot._970x90);
                    currentTakeoverSize = 970;
                }
            }

        }
    }



    $(window).load(function() {

        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            if(!event.isEmpty) {
                if(typeof MMUI.tracking != 'undefined' && (!MMUI.globals.getQueryVariable('swapAdZone') && !MMUI.globals.getQueryVariable('swapAdZone2') && !MMUI.globals.getQueryVariable('swapAdZone3') && !MMUI.globals.getQueryVariable('swapAdZone4'))) {
                    MMUI.tracking.sendEvent(event.size[0]+'x'+event.size[1]+'_'+event.slot.getName(), 'View', mm_globals.pageView);
                }
                adCreative.push({unit : event.slot.getName(), size : (event.size[0]+'x'+event.size[1]), lineItem : event.lineItemId , creativeId : event.creativeId});
            }
        });

        hptoContainer = $('#hptoContainer'); // Container for custom takeovers (i.e. non-standard 970x90 ads)

        
        //Display correct ad size on intial load
        showHeaderAd();

        if(!MMUI.globals.isMobile()) { // No need to support multiple header ad sizes on mobile

            $(window).resize(function() {
                if(winWidth != $(window).innerWidth()) { // Only run if width changes, ignore height changes
                    winWidth = $(window).innerWidth();
                    showHeaderAd();
                }
            });

        }

       
        showAdSlot(adZone._2000x420, [2000, 420], adSlot._2000x420);
        showAdSlot(adZone._200x200, [200, 200], adSlot._200x200);
        
        if (mm_globals.template_type === 'Prem-Article-C') {
            showAdSlot(adZone._300x250_4, [[300, 250],[300, 600]], adSlot._300x250_4);
        }

        //Check to see if 300x250 ads are already in view. If yes, display now otherwise check again when user scrolls
        if (mm_globals.template_type !== 'Prem-Gallery-C') {
            if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_1)) {
                showAdSlot(adZone._300x250_1, [300, 250], adSlot._300x250_1);
            }
        }
        
        if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_2)) {
            showAdSlot(adZone._300x250_2, [300, 250], adSlot._300x250_2);
        }
        if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_3)) {
            showAdSlot(adZone._300x250_3, [300, 250], adSlot._300x250_3);
        }

        $(window).scroll(function() { //Display middle and bottom 300x250 ads only when it scrolls into view
            if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_1)) {
                showAdSlot(adZone._300x250_1, [300, 250], adSlot._300x250_1);
            }
            if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_2)) {
                showAdSlot(adZone._300x250_2, [300, 250], adSlot._300x250_2);
            }

            if(MMUI.globals.isScrolledIntoView('#'+adSlot._300x250_3)) {
                showAdSlot(adZone._300x250_3, [300, 250], adSlot._300x250_3);
            }
        });

    });
</script>

