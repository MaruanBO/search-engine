$(document).ready(function (argument) {
    var $sliderDom_3fa1a56a7cc11bd8 = $('#slider_3fa1a56a7cc11bd8');

    var $slider_3fa1a56a7cc11bd8 = $sliderDom_3fa1a56a7cc11bd8.bxSlider({
