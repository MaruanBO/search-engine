var mm_globals = {
    category : "home",
    sub_category : "",
    sub_sub_category : "",
    post_id : "",
    publish_date : "",
    author : "",
    template_type : "index",
    video_user : "No",
    channel : "",
    series_name : "",
    tags : [],
    ab_testing : {A : 100, B : 0, C : 0, D : 0},
    pageView : window.location.href.replace('http://' + document.domain, '')
}
</script>
