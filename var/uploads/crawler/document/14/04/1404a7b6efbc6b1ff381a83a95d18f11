					</footer>
					{{/is_more}}

					{{^is_more}}
					<div class="left">
						<h1>{{{left.title}}}</h1>
						<ul>
							{{#left.links}}
								<li><a href="{{{url}}}">{{{title}}}</a></li>
							{{/left.links}}
						</ul>
					</div>
					<div class="right">
						<h1>{{{right.title}}}</h1>
						<div class="clearfix list">
							{{#right.posts}}
								<article class="column-item column-4">
									<div class="column-content">
										<span class="image-container"><a href="{{{url}}}"><img src="{{{image}}}" alt="{{{imgname}}}"></a></span>
										<div class="content">
											<em>{{type}}</em>
											<h1><a href="{{{url}}}">{{{title}}}</a></h1>
										</div>
									</div>
								</article>
							{{/right.posts}}
						</div>
					</div>
					<footer class="clearfix">
						{{#bottom}}
							<article class="column-item column-3">
								<div class="column-content">
									<span class="image-container">
										{{#image}}
										<a href="{{{url}}}">
											<img src="{{{image}}}" alt="{{{imgname}}}">
										</a>
										{{/image}}
										{{^image}}
											&nbsp;
										{{/image}}
									</span>
								</div>
							</article>
						{{/bottom}}
					</footer>
					{{/is_more}}
				</script>
			</div>
		</header>
		<!-- end #header -->
