	$(document).ready(function() {
	  $(".item-scroller").carouFredSel({ "items": 3, "direction": "up", "auto": { "timeoutDuration": 7000 }, pauseOnHover: true });

		$("#category_list").change(function(e) {
			var el = $(this);

			if (el.val() != "")
			{
				$("#category_brands").slideUp(function() {
					$("#category_brands").html($("#category_brands_" + el.val()).html());
					$("#category_brands").slideDown();
				});

			}
			else
			{
				$("#brand_list").html('');
			}
		});

		$("#category_list").trigger('change');
	});
</script>




