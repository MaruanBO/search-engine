    <script>

      function trackEvent(category, action, label)
      {
        _gaq.push(['_trackEvent', category, action, label]);
      }

      function trackClick(options) {
        cc = Math.floor((Math.random()*999999999));

        if (options['source'] == "pricegrabber" && options['visit_source'] != "search")
        {
          return true;
        }

                  var url = '/track.gif?cc=' + cc + '&s=' + options['source'] + '&p=' + options['position'];
        
        var track_img = document.createElement("img");
        track_img.src = url;
        track_img.width = 1;
        track_img.height = 1;
        document.body.appendChild(track_img);

        if (options['event_name'] != undefined)
          event_name = options['event_name'];
        else
          event_name = "";

        if (options['event_key'] != undefined)
          event_key = options['event_key'];
        else
          event_key = "";

        if (options['event_label'] != undefined)
          event_label = options['event_label'];
        else
          event_label = "";

        _gaq.push(['_trackEvent', event_name, event_key, event_label]);
        return true;
      }
    </script>

      
  
