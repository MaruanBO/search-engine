						$("#header ul.tabs li.n").hover(function(){
							$("#header ul.tabs li.js").removeClass("js");
							$(this).addClass("js");
							canHide = false;
						}, function(){
							canHide = $(this);
							setTimeout(function(){
								if(canHide) { canHide.removeClass("js"); }
							}, 500);
						});
$("#sitesearch").suggestClick("http://www.indiedb.com/scripts/autocomplete.php?a=search&p=home",{resultsClass: "suggest_search", positionIn: "#searchbox", limit: 10});

			$("#logintoggle a.loginlink").click(function(){
				if($("#subheader ul.session").hasClass("sessionlogin")) {
					$("#subheader ul.session").removeClass("sessionlogin");
				} else {
					$("#subheader ul.session").addClass("sessionlogin");
					$("#loginusername").focus();
				}
				return false;
			});
			$("#logintoggle .form").mouseup(function() {
				return false;
			});
			$(document).mouseup(function(e) {
				if($(e.target).parent("#logintoggle").length==0) {
					$("#subheader ul.session").removeClass("sessionlogin");
				}
			});

			allgood = measureHeight("div.mediabox div.media") > 10;
