						pause: 1,
						next: "#rotatornext",
						prev: "#rotatorprev",
						before: onBefore,
						after: onAfter
					});

					function onBefore(curr, next, opts) {
						$("#rotator"+(opts.nextSlide > opts.currSlide ? "next" : "prev")).addClass((opts.nextSlide > opts.currSlide ? "next" : "prev")+"on");
					}

					function onAfter() {
						$("#rotatornext").removeClass("nexton");
						$("#rotatorprev").removeClass("prevon");
					}

