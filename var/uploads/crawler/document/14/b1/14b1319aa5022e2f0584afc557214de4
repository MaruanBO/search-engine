<script type="text/javascript">
jQuery(document).ready(
    function () 
    {
        jQuery("#nga-share-content-ngaweb-jcr-content-socialshare-emailshare").on("click", "button", 
            function () 
            {
                var url = "/content/ngaweb/emailShare.html?url=" + encodeURIComponent("/content/ngaweb");
                if (nga.favorites.sharedUrl)
                {
                    var parsedUrl = parseUrl(nga.favorites.sharedUrl);
                    var shareUrl = parsedUrl.pathname.replace(/\.html$/,'');
                    if (shareUrl.indexOf("/")!=0)
                        shareUrl = "/"+shareUrl;
                    
                    url = "/content/ngaweb/emailShare.html?url=" + encodeURIComponent(shareUrl);
                }
                window.open(url, "ngaEmailShare", "status=0, toolbar=0, location=0, resizable=1, width=600, height=500" );
            });
        
    });
</script>
