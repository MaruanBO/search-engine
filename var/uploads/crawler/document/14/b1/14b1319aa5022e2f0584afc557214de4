<script>
    var pathname = window.location.pathname;
    var lastInd = pathname.lastIndexOf("/");
    pathname = pathname.substring(0, lastInd); 

    var httpRequest;
    var baseURL = "/content/ngaweb/jcr:content/homepage/announcement";
    var myurl = baseURL + '.text' + '.json';
   
    console.log(encodeURI(myurl) + " <br> " + myurl);
    myurl = encodeURI(myurl);
    var htmlText;

    
        

    jQuery(document).ready(function() {
 
        jQuery.ajax({   
            
            url: myurl,    
            type: "GET",     
            dataType: "json",
            cache: true,
            success: function(data, status) {
              
                if (status == "success") {
                    console.log("data: " +  data);
                    
                    var objJSON = data;
                 
               
                   
                    var isHomePg =  true; 
                    
                    var tempAnnouncement  = objJSON.announcement;

                   if ((tempAnnouncement != null) && (tempAnnouncement != "")) {
                         
                        if (tempAnnouncement.indexOf("##red##") >= 0) {
                            tempAnnouncement = tempAnnouncement.replace("##red##", "");
                            document.getElementById("announcementID").className = "anncmnt-notice closure warning";
                        } else {
                            document.getElementById("announcementID").className = "anncmnt-notice closure";
                        }
                        document.getElementById("announcementID").innerHTML = "<p> <b> Notice: </b>" + tempAnnouncement +"</p>";
                   } else {
                        if (isHomePg) {
                            document.getElementById("announcementID").className = "anncmnt-placehold";
                            document.getElementById("announcementID").innerHTML = ",,,"; 
                        } else {
                            document.getElementById("announcementID").className = "anncmnt-nonotice";
                        }
                   }
                    
                   
                } else {
                    console
                            .log("ajax response failed in function mycallback");
                }
            
            
            }
           
            });
        });
</script>


