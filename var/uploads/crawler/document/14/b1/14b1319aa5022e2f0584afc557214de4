<img id="fullbg" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" alt=""/>

<script type="text/javascript">
console.log("Calling load function");
    jQuery(document).on("ready", function () { 
    console.log("load function executing");
    var $ = jQuery; 
    var win = $(window);

    //Set the background
    
    var win_w = win.width(),
        win_h = win.height(),
        $bg   = $("#fullbg");

    // Load narrowest background image based on 
    // viewport width, but never load anything narrower 
    // that what's already loaded if anything.
    var available = [
      480, 1024, 1280, 1900
    ];
    
    var current = $bg.attr('src').match(/([0-3]+)/) ? RegExp.$1 : null;
    
    if (!current || ((current < win_w) && (current < available[available.length - 1]))) {
      
      var chosen = available[available.length - 1];
      
      for (var i=0; i<available.length; i++) {
        if (available[i] >= win_w) {
          chosen = available[i];
          break;
        }
      }
      
      // Set the new image
      var randomIndex = Math.floor(Math.random() * 9 + 1);
      
      if (randomIndex < 10)
      {
          randomIndex = "0" + randomIndex;
      }
      $bg.attr('src', '/content/dam/ngaweb/homepage/bgresolutions/' + chosen + '/home' + randomIndex + '.jpg');
      
      // for testing...
      console.log('Chosen background: ', [chosen, $bg.attr('src')]);
      
    }
    
    win.resize(function() {
        console.log("Window has been resized");
        var win_w = win.width(),
            win_h = win.height(),
            $bg   = $("#fullbg");
    
    
        // Determine whether width or height should be 100%
        if ((win_w / win_h) < ($bg.width() / $bg.height())) {
          $bg.css({height: '100%', width: 'auto'});
        } else {
          $bg.css({width: '100%', height: 'auto'});
        }
        
      }).resize();
      
    })
</script></div>
    
</div>

    

	        

