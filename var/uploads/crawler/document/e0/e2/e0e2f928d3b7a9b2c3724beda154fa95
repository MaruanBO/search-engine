    /*================================================================*/
    /*  Enter Zip
    /*================================================================*/
    $('#zipSubmit').click(function(e){
        e.preventDefault();
        saveLocalZipCode();
    });
    
    $('#zipSubmit_form').keydown(function(e){
    	if (e.keyCode == 13) {
            e.preventDefault();
            saveLocalZipCode();
        }
    });
    
    
    function saveLocalZipCode(){
        
        //if logged in
        var userID = 'null';
    
        if(userID != null && userID != "" && userID != 'null'){
            $.cookie("bypass","true", { expires: expireDate, path: '/' });
        }else{
            $.cookie("bypass","false", { expires: expireDate, path: '/' });
        }
        
        var localZipCode = $('#localZipCode').val();	//get the zip from the input;	
        
    
         //test if its a valid zip or not
         if( localZipCode.length != 5 || isNaN(localZipCode) || localZipCode == "00000" || /^\d+$/.test(localZipCode) == false){
                $("#invalidZip").show();
                return false;
         }else{
             $('#invalidZip').fadeOut();
             postRequest(localZipCode, "header"); 
         } 	
    }
    
    function postRequest(zipcode,fromPage) {
    
        var cordinateURL="/d/cordinates_finder.jsp?zip="+zipcode;
        var xmlHttp;
           
        if (window.XMLHttpRequest) { // Mozilla, Safari, ...
                var xmlHttp = new XMLHttpRequest();
           }else if (window.ActiveXObject) { // IE
                var xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
           }
           
        xmlHttp.open('POST', cordinateURL, true);  
           
        xmlHttp.onreadystatechange = function() {
            if (xmlHttp.readyState == 4) {
                var response = xmlHttp.responseText; 
                response = $.trim(response);
                
                if (xmlHttp.status == 200) {
                    if(response == null || response == "null" || response == ""){
                        //console.log('its null');
                        $('#localZipCode').val('');
                        alert('Address ' + zipcode + ' not found. Please enter a valid location.');
                    }else{
                        $.cookie('fltStr', '', { path: '/', domain: 'searsoutlet.com' });
                        $.cookie('customer_zip', zipcode, { expires:  expireDate, path: '/', domain: 'searsoutlet.com' });
                        
                        $('#openLocalZipCode').html(zipcode);
    
                        var local = '&lsStores=local';
                        var newURL = document.URL;
                        var finalURL  = document.URL;
                        
                        if(fromPage == "products"){
                            if(newURL.indexOf(local) == -1){
                                finalURL = newURL.concat(local);	
                            }
                        }
                        
                        $.cookie('lsRedPage', finalURL, { path: '/', domain: 'searsoutlet.com' });
                        
                        $("#openLocalZipCode").html(zipcode); 
                         
                        var url = "/d/localZipSubmit.jsp?localZipCode=" + zipcode + "&actionName=zipCode";
                        window.location = url;
                   }
                }
            }
           }
        xmlHttp.send(cordinateURL);
    
       }
    
    //close without submitting
    $('.changeZipClose').click(function(){
        $.cookie('zip_popup_closed', 'closed', { path: '/', domain: 'searsoutlet.com' });
    });
    
    /*** end: enter zip ***/
    
    
    //if zipcode cookie is set, display it to the user in the upper shopperbar
    if( $.cookie('customer_zip') ){
        $('#openLocalZipCode').text( $.cookie('customer_zip') );         
    }
                                                             
    //set the correct logged in container
    if( $.cookie('userType') === 'Registered' && $.cookie('emailId') ){
        $('#loggedInWrap .myProfile').text( $.cookie('emailId') );
        $('#anonControlWrap').addClass('hide');    
        $('#loggedInWrap').removeClass('hide');
        $('#managerControlWrap').addClass('hide');
    }else if( $.cookie('t_usr') === 'ACCESS_AUTHORIZED' ){
        $('#anonControlWrap').addClass('hide');    
        $('#loggedInWrap').addClass('hide');
        $('#managerControlWrap').removeClass('hide');    
    }else{
        $('#anonControlWrap').removeClass('hide');    
        $('#loggedInWrap').addClass('hide');
        $('#managerControlWrap').addClass('hide');
    }
    
                                                             
    //set the store location if previously set
    if( $.cookie('myStoreName_view') && $.cookie("my_own_store") ){
        $('#storeHolderWrap').removeClass('hide');
        $('#noStoreHolderWrap').addClass('hide');
        $('#storeInfo strong a').text( $.cookie('myStoreName_view') ).attr('href', '/d/store.jsp?' + $.cookie('myStoreName_view') + '&store=' + $.cookie("my_own_store") );
    }
                                                             
