    /*================================================================*/
    /*  Set Values for Omniture (Products Page)
    /*================================================================*/
    function setvaluesForOmniture(filterName , filterValue){
        jQuery("#currentFacetForCascading").val(filterName);
        
        var refinements = $('[name="refinementsForOmniture"]').val();
        //console.log(refinements);
        
        var valuetoSet;
        if(refinements==""){
            valuetoSet = filterName + "$" + filterValue ;
        }else{
          if(refinements.indexOf(filterName)== -1){
              valuetoSet = refinements + '#' + filterName + '$' + filterValue ;
          }else{
             var refList = refinements.split('#');
             for(var i=0;i<refList.length;i++){
                 var ref = refList[i].split('$');
                 var value = refList[i];
                 var refname = ref[0];
                 if(refname==filterName){
                     value =  value + ','+ filterValue ;
                 }
                 if(i==0){
                     valuetoSet = value ;
                 }else{
                     valuetoSet = valuetoSet + '#'+ value
                 }
             }
          }
            
        }
        
        $('[name="refinementsForOmniture"]').val(valuetoSet);
    }
    
    /*================================================================*/
    /*  Set Values for Omniture (Inventory Page)
    /*================================================================*/
    function setCurrentPriceForOmniture(price,div,itemNum,sku){
        var partNumber = padding(div+"",'000')+ padding(itemNum+"",'00000') +padding(sku+"",'000');
        $('#itemPrice').val(price);
        $('#partNum').val(partNumber);
        //console.log('omniture from inventory page');
    }
    
    function padding(s,l) { return( l.substr(0, (l.length-s.length) )+s ); }
    
    
