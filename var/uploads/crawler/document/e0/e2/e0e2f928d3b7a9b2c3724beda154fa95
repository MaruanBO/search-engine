    //on zip submit
    function searchLocations1(userLocation1, storeIds, arg, loadStores) {
    
        //get the user location if not already set
        if (userLocation1 == null) {
            userLocation1 = $('#userLocation1').val();
        }
        
        //alert('users location: ' + userLocation1);
        
        var atLeastOneChecked = false;	//init to false for checkboxes
        
        //test if checked or not
        if ( !$('#chkOutlet1').prop('checked') ) {
            outletId = "unchecked";
            //alert('outlet unchecked');
        }else{
            outletId = "checked";
            atLeastOneChecked = true;
            //alert('outlet checked');
        }
    
        if ( !$('#chkHardware1').prop('checked') ) {
            hardwareId = "unchecked";
            //alert('hardware unchecked');
        }else{
            hardwareId = "checked";
            atLeastOneChecked = true;
            //alert('hardware checked')
        }
    
        if ( !$('#chkFullline1').prop('checked') ) {
           fullLineId = "unchecked";
           //alert('retail unchecked')
        }else{
            fullLineId = "checked";
            atLeastOneChecked = true;
            //alert('retail checked');
        }
    
        if ( !$('#chkHometown1').prop('checked') ) {
            hometownId = "unchecked";
            //alert('hometown unchecked');
        }else{
            hometownId = "checked";
            atLeastOneChecked = true;
            //alert('hometown checked')
        }
    
        if ( !$('#chkApparel1').prop('checked') ) {
            apparelId = "unchecked";
            //alert('apparel unchecked');
        }else{
            apparelId = "checked";
            atLeastOneChecked = true;
            //alert('apparel checked');
        }
        
        if ( !$('#chkHomeApp1').prop('checked') ) {
            homeApplId = "unchecked";
            //alert('apparel unchecked');
        }else{
        	homeApplId = "checked";
            atLeastOneChecked = true;
            //alert('apparel checked');
        }
        
        //if nothing is checked, warn them
        if (!atLeastOneChecked) {
            alert("Please select at least one store type.");
            arg.checked = "checked";
            return false;
        }
        
        geocoder1.geocode({
            'address': userLocation1
        }, function (results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                hideLocatedStores1();
                
                var latlng = results[0].geometry.location;
    
                map1.setCenter(latlng);
                //phone horiz just get the location automatically
                if ($(window).width() < 481 && $(window).width() > 370) { 
                    map1.setZoom(11);
                }else{
                    map1.setZoom(9);   
                }
                
                
                
                var searchedZip = $('#userLocation1').val();
                
                $.cookie('userlocationsearch1', searchedZip, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
                
                searchLocationsNear1(latlng, storeIds, loadStores);
				
				// Add zip set
				if(searchedZip.length != 5 || isNaN(searchedZip) || searchedZip == "00000"){
					return;
				}
					
				$.cookie("customer_zip",searchedZip, { expires:  expireDate, path: '/', domain: 'searsoutlet.com' });
				$.cookie("lsRedPage", document.URL);   //store the page URL in cookie to use for redirection
				
				var url = "/d/localZipSubmit.jsp?localZipCode=" + searchedZip ;
            }else{
                alert('Address not found. Please enter a valid location.');
            }
        });
    }	//searchLocations1
    
    //hide existing markers
    function hideLocatedStores1() {
        if (markersArray1 && markersArray1.length > 0) {
            for (var i = 0; i < markersArray1.length; i++) {
                markersArray1[i].setMap(null);
                markersArray1[i] = null;
            }
        
            markersArray1 = [];
        }
    
        $("#sl-store-panel .row-fuild .span12").innerHTML = "";
    }	//hideLocatedStores1
    
    //hide the stores and get store ids
    function searchLocationsNear1(point, storeIds, loadStores) {
    
        hideLocatedStores1();
        if (storeIds == null) {
            var storeIdsCookie = $.cookie("sh_by_store");
            
            if (storeIdsCookie != null && storeIdsCookie.length > 0) {
                storeIds = storeIdsCookie.split(",");
            }
        }
        
        //modified value of FORM_FIELD_RADIUS from 200 to 150 on 30th July
        var searchUrl = "/d/getNearByStores?action=nbst&lat=" + point.lat() + "&lng=" + point.lng() + "&ind=" + ((typeof storeIds != 'undefined') ? storeIds : "") + "&r=150" + "&chkOutlet=" + outletId + "&chkHardware=" + hardwareId + "&chkFullline=" + fullLineId + "&chkHometown=" + hometownId + "&chkApparel=" + apparelId + "&chkHomeApp=" + homeApplId;
    
        processSearch1(searchUrl, storeIds, loadStores);
    }	//searchLocationsNear1
    
    
    //process search request
    function processSearch1(searchUrl, storeIds, loadStores) {
        
        
        //get the data
        $.ajax({
            type: "GET",
            url: searchUrl,
            processData: true,
            data: {},
            dataType: "json",
            success: function(data){
                
                //show loading mask
                $('.loadingMaskAnimated').remove();
                
                //print stores	    	
                //console.log(data.stores[0]);
                
                //check if already has an array
                var haveStores = $.isArray(storeIds);
                
                //hide any existing stores in the locator side panel
                //$('#sl-store-panel .row-fluid .span12').fadeOut();
                
                var storeNames = '';
                var myOwnStore = $.cookie('my_own_store');
                
                //if myOwnStore has been set
                if (myOwnStore != null && myOwnStore != "") {
                    createStoreEntryForMyStore();
                
                    //add marker for my store
                    for (var p = 0; p < data.stores.length; p++) {
                        if (data.stores[p].unit == myOwnStore) {
                            addMarker1(data.stores[p], 0, true);
                            data.stores.splice(p, 1);
                        }
                    }
                }
                    
                //console.log(data.stores.length);
                
                
                //get an array of stores
                var storesArray = '';
                
                
                //create the rest of the markers
                for (var i = 0; i < data.stores.length ; i++) {
    
                    var center = map1.getCenter();
                    var distance = google.maps.geometry.spherical.computeDistanceBetween(center, new google.maps.LatLng(data.stores[i].lat, data.stores[i].lng));
                    distance = distance / 1000; //km
                    distance = distance * 0.621371192 //miles
                    distance = distance.toFixed(1);
                    
                    //get store data
                    var store = data.stores[i];
                    var type = store.type;
                    
                    //if store numbers less than 10
                    if(i < 11){
                        storeNames = storeNames + store.unit + "~~";
                    }
                    
                    //show all checked hardware & full-line stores from cookie
                    var checked = ((!haveStores && type == 4 && distance <= 60) || (haveStores && type == 4 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 1 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 5 && storeIds.indexOf(data.stores[i].id.toString()) >= 0)|| (haveStores && type == 7 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 3 && storeIds.indexOf(data.stores[i].id.toString()) >= 0));
    
                    //preselect outlet stores even if there are user selected store
                    if (loadStores == null) {
                        checked = ((type == 4 && distance <= 60) || (haveStores && type == 4 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 1 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 5 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 7 && storeIds.indexOf(data.stores[i].id.toString()) >= 0) || (haveStores && type == 3 && storeIds.indexOf(data. stores[i].id.toString()) >= 0));
                    }
                    
	                 // added by ssatush starts
	   				 var desc = "";
	   	             var storeType = store.type;
	   	             if (storeType == 1) {
	   	                 desc = "Sears Department Stores";
	   	             } else if (storeType == 3) {
	   	                 desc = "Sears Hardware";
	   	             } else if (storeType == 4) {
	   	                 desc = "Sears Outlet";
	   	             }else if (storeType == 5) {
	   	                 desc = "Sears Hometown";
	   	             } else if (storeType == 7) {
	   	                 desc = "Sears Home Appliance Showroom";
	   	             }
	   				// added by ssatush ends
        
                    //create the list for all other stores 
                    
                    //Jira OUTL-5896 : Issue with 4 digit ZipCode. Javascript internally applying Octal conversion for zip codes starting with 0.  Resolved by treating zipcode as string.
                    var makeMyStoreHRef = '<a href="#" onclick="makeThisYourStore(\'' + store.name + '\', ' + store.unit + ',this,\''+ store.zip +'\');return false;" class="myStore primary"><span class="hidden-phone">Make</span> Your Store</a>';	
                    //Create a Store Element
                    storesArray = storesArray + '<div class="aside" id="map' + store.id + '"><span class="for-checkbox"></span><div class="righty">' + distance + ' mi</div><strong class="st-info" id="storeEntry_' + store.id + '">' + store.city + ', ' + store.state +' - <span class="dark">'+desc + '</span><br /></strong><small class="st-address">' + store.address + ', ' + store.state + ' ' + store.zip + '</small><br />' + store.phone + ' - Store #' + store.unit + '<br /><br /><i class="icon-sort-down righty"></i><span class="righty detail-link hidden-phone">Hours &nbsp;</span> <a href="/d/store.jsp?' + store.name + '&amp;store=' + store.unit + '" class="visitStoreLink primary text-error"><strong>Shop Store <span class="hidden-phone">Inventory</span></strong></a> &nbsp;|&nbsp; '+makeMyStoreHRef+' | <a class="primary" href="http://maps.apple.com/?saddr=Current%20Location&daddr= ' + store.address + ' ' + store.zip + '" target="_blank">Directions</a><div class="more-info"><table><tr><td class="text-right"><b>Mon:</b></td><td>&nbsp;&nbsp;' + store.monHours + '</td></tr><tr><td class="text-right"><b>Tue:</b></td><td>&nbsp;&nbsp;' + store.tueHours + '</td></tr><tr><td class="text-right"><b>Wed:</b></td><td>&nbsp;&nbsp;' + store.wedHours + '</td></tr><tr><td class="text-right"><b>Thu:</b></td><td>&nbsp;&nbsp;' + store.thrHours + '</td></tr><tr><td class="text-right"><b>Fri:</b></td><td>&nbsp;&nbsp;' + store.friHours + '</td></tr><tr><td class="text-right"><b>Sat:</b></td><td>&nbsp;&nbsp;' + store.satHours + '</td></tr><tr><td class="text-right"><b>Sun:</b></td><td>&nbsp;&nbsp;' + store.sunHours + '</td></tr></table></div></div>';
  
                    
                    //performance killer moving this out and appending everything
                    //createStoreEntry1(data.stores[i], distance, checked);
                    
                    var showStore = $('#storeEntry_' + store.id);
                    //console.log(showStore.get(0));
                    
                    
                    //add the markers
                    addMarker1(data.stores[i], distance, checked);
                    
                    //set the cookies
                    $.cookie('st_na_aft_srh', storeNames, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
                  
                    //get the center of the map again
                    var point = map1.getCenter();
                    
                    //set the c_ul cookie where the map center lat/lng is
                    $.cookie("c_ul", point.lat() + ',' + point.lng(), { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
    
                    //update store numbers/names cookie
                    $.cookie("sh_by_store", storeNames, { path: '/', domain: 'searsoutlet.com' });
                }
                
                saveSelection1(false);
                
                //console.log(myOwnStore);
                if(myOwnStore != null && myOwnStore != ""){
                    //if myOwnStore is set, cool great, we can just dump im the array
                    $('#sl-store-panel .row-fluid .span12').find('.aside').after(storesArray);
                }else{
                    $('#sl-store-panel .row-fluid .span12').html('' + storesArray + '');
                }
                
                
                
                //expand asides with store info
                $('#sl-store-panel .row-fluid .span12 .aside').click(function(){
                    $('.more-info').hide();
                    $('#sl-store-panel .row-fluid .span12 .aside').removeAttr('style').find('.icon-sort-down, .detail-link').show();
                    
                    $(this).css({ 'background' : '#fff' });
                    $(this).find('.more-info').show();
                    $(this).find('.icon-sort-down, .detail-link').hide();
                });
                
                //save Selection
                function saveSelection1(hide) {
                    hide = (hide == null || hide);
                    var point = map1.getCenter();
    
                    var storeIds = $.cookie('sh_by_store');
                    
                    if (storeIds.length > 0 && storeIds[0] != ',') {
                        $.cookie("c_ul", point.lat() + "," + point.lng(), { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
                    }else{
                        //do nothing
                    }
    
                }	//saveSelection1
           
            },
            error: function(xhr, status, error){
                alert("There was a problem with your request. " + xhr.status);
            }
            
        });
    } //processSearch1
    
    
    //create store entry for My Store
    function createStoreEntryForMyStore() {
        
    }	//createStoreEntryForMyStore
    
    //make this my store
    function makeThisYourStore(storeDisplayName, storeName, element, storeZip) {
        if (storeName != null){
        	$.cookie("my_own_store", storeName, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
            $.cookie('myStoreName_view', storeDisplayName, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
            //Saving zip code in coockie so that it can be utilized in store locator
            $.cookie("customer_zip", storeZip, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
            
            //save the stores for selected favourite store  
            $.cookie('storesList_for_favourite_store', $.cookie('st_na_aft_srh'), { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
          
            $.cookie("makeMyStore","true");
        }
        
        $('#sl-store-panel .aside').removeClass('active');
        $('#map' + storeName).addClass('active');
        
        location.reload();	//reload to update the page
    }	//makeThisYourStore
    
    //show details on click
    $('#sl-store-panel .aside').click(function(){
        $('#sl-store-panel .aside').find('.more-info').hide();
        $(this).find('.more-info').show();
    });
    
    //add map marker
    function addMarker1(store, distance, checked) {
        //alert('lets make a marker');
        var point = new google.maps.LatLng(store.lat, store.lng);
        var iconSears;
        
        if (checked)
            iconSears = '/s/rwd/assets/images/map-marker.png';
        else
            iconSears = '/s/rwd/assets/images/map-marker2.png';
    
            var marker = new google.maps.Marker({
                position : point,
                map : map1,
                icon : iconSears,
                title : store.name
            });
    
            markersArray1.push(marker);
            
            google.maps.event.addListener(marker, 'click', function() {
                if (infoWindow1) {
                    infoWindow1.close();
                }
    
                infoWindow1 = new google.maps.InfoWindow();
                infoWindow1.setContent("<div class='infoWindow'>" + "<strong>"
                        + store.name + "</strong>" + "<br>" + store.address
                        + "<br>" + store.city + ", " + store.state + "  "
                        + store.zip + " <b>(" + distance + " mi.)</b>"
                        + "<p><br /><strong>Phone Number:</strong> " + store.phone + "</p></div>");
    
                infoWindow1.open(map1, marker);
            });
            
            $("#map" + store.id).live('click', function(){
    
                if (infoWindow1) {
                    infoWindow1.close();
                }
                
                infoWindow1 = new google.maps.InfoWindow();
                infoWindow1.setContent("<div class='infoWindow'>" + "<strong>"
                        + store.name + "</strong>" + "<br>" + store.address
                        + "<br>" + store.city + ", " + store.state + "  "
                        + store.zip + " <b>(" + distance + " mi.)</b>"
                        + "<p><br /><strong>Phone Number:</strong> " + store.phone + "</p></div>");
    
                infoWindow1.open(map1, marker);
            });
        } //addMarker1
    
        //create store entry for all stores
        function createStoreEntry1(store, distance, checked) {
            $('#sl-store-panel .row-fluid .span12 .aside').after('<div>Test</div>');
            //create the container
            var aside = "";
            aside = document.createElement("div");
            aside.id = "map" + store.id;
            aside.className = ("true" ? "checked " : "") + "aside";
    
            //add the checkbox
            var spanCheckBox = document.createElement("span");
            spanCheckBox.className = "for-checkbox";
            aside.appendChild(spanCheckBox);
    
            //create the right container w distance, make my store, view store links
            var storeInfoDiv = document.createElement('div');
            storeInfoDiv.className = 'righty';
            aside.appendChild(storeInfoDiv);
    
            var storeType = store.type;
    
            if (storeType == 6) {
                //dont' make this my store
            } else {
                var spanStoreLink1 = "<a href='#' onclick='makeThisYourStore("
                        + store.name
                        + store.unit
                        + ",this,"
                        + store.zip
                        + ");return false;' id='myStore' data-original-title='Make Your Store' class='icon-star tool-tip'>&nbsp;</a>";
            }
    
            if (storeType == 6) {
                //dont' show store link
            } else {
                var spanStoreLink2 = "<a href='/d/store.jsp?"
                        + store.name
                        + "&store="
                        + store.unit
                        + "' data-original-title='Visit Store' class='icon-visit tool-tip'>&nbsp;</a>";
            }
    
            storeInfoDiv.innerHTML = spanStoreLink2 + spanStoreLink1 + distance + ' mi';
    
            //create the title
            var strongStInfo = document.createElement("strong");
            strongStInfo.className = "st-info";
            strongStInfo.innerHTML = store.name + ", " + store.state + "<br />";
            strongStInfo.id = "storeEntry_" + store.id;
            aside.appendChild(strongStInfo);
    
            //create the address
            var spanStAddress = document.createElement("small");
            spanStAddress.className = "st-address";
            spanStAddress.innerHTML = store.address + ", " + store.city + ", "
                    + store.state + " " + store.zip;
            aside.appendChild(spanStAddress);
    
            //extra store info
            var moreInfo = document.createElement("div");
            moreInfo.className = "more-info";
            moreInfo.innerHTML = "M-SAT: " + store.monFriHours + "<br /> SUN: "
                    + store.sunHours + "<br />" + store.phone + "<br /> Store #"
                    + store.id;
            aside.appendChild(moreInfo);
    
        } //createStoreEntry1
    
    /*** end: store locator ***/
    
    
    $('.grand-toggle').click(function(e){
        e.preventDefault();
        $('.grand-toggle-child').slideToggle('fast');
    });
    
    /*Feedback form starts*/
        function submitFeedback() { 
        var email = $('#customerEmail').val(); // get the value of the email text box							
        var reg = /^([A-Za-z0-9])([a-zA-Z0-9\.\_\-])*@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
        var address = email;
        
    if($('input:radio[name=feedbackrating]:checked').val() === undefined || $('#visitReason option:selected').text() == "Select one" || $('#feedbackcontent').val() == '' || $('#customerName').val() == '' || $('#customerEmail').val() == ''){
        $('#invalidInp').show();
        $(".chkbox, select, textarea, #customerEmail, #customerName").css('border','1px solid #FF0000');
    }
    else if (email != '' && reg.test(address) == false) {
        alert("Please check your email !!");
    } 
    else{
        var updateFeedbackStatus = $.post("/d/updateUserFeedback",  $('#userFeedbackForm').serialize()); 
        $('#feedbackContainer').hide();
        $('#feedbackMessaging').show();
    }
    }
    /*Feedback form ends*/
                               
