            	 var responsiveStandard = 	$(this).attr('data-d'),
            	 	 responsiveMobile = 	$(this).attr('data-m'),
            	 	 responsiveTablet = 	$(this).attr('data-t'),
            	 	 responsiveBig = 		$(this).attr('data-xl');
            	 
             	if( document.documentElement.clientWidth > 1199 ){
             		$(this).attr('src', responsiveBig);
				}else if (document.documentElement.clientWidth > 767 && document.documentElement.clientWidth < 1200) { 
                     $(this).attr('src', responsiveStandard); //swap out bigger image if not mobile
                }else if (document.documentElement.clientWidth > 480 && document.documentElement.clientWidth < 768){
                	 $(this).attr('src', responsiveTablet); //swap out smaller image 
                }else{
					 $(this).attr('src', responsiveMobile); //swap out smaller image 
                }
             });
    
             $('.slides').fadeIn(); //show the slides
	    },
