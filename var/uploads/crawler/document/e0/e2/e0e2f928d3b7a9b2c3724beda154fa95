    <script> 
       
    /* ================================================================ */
    /* Truncate a string
    /* ================================================================ */   
    function cut(word, n) {
        word = word.substring(0, n) + "...";
        return word;
    }
        
    /* ================================================================ */
    /* Truncate a string
    /* ================================================================ */
    function stripIt(word){
    	word = word.replace(/<(?:.|\n)*?>/gm, '');
        return word;
    }
        
    /* ================================================================ */
    /* Cart Crumbs Highlight
    /* ================================================================ */   
    $(window).load(function(){
    	$('#cart-tabs ul li a').parent().css({ 'border-top' : '1px solid #e77917' });   //change border color based on progress
	    $('#cart-tabs ul li').first().css({ 'padding-left' : 0 });                      //remove padding on first item 
	    $('#cart-tabs ul li').last().css({ 'padding-right' : 0 });                      //remove padding on second item
	    $('#cart-tabs .active').append('<i id="cartProgress" class=icon"></i>');                          //add the cart icon
	    $('#cart-tabs .active i').addClass('icon-shopping-cart');                       //make it a cart icon
    });
        
    /* ================================================================ */
    /* Error Message Creation
    /* ================================================================ */   
    function displayErrorMessage(errorText){
        $('#error').remove();
        $('body').append('<div id="error" class="alert alert-error">' + errorText + '</div>');
    }
        
        
    /* ================================================================ */
	/* iFrame Modal
	/* =================================================================*/
    function iFrameModal(src){
        $('footer').after('<div class="soModal soModal-blank"><i class="icon-remove fa fa-times soModalClose"></i><iframe src="' + src + '" width="100%" height="600px" frameborder="0"></iframe></div>');    
    }
    $('.soModalClose').live('click', function(){
        $(this).parent().remove();    
    });
        
    /* ================================================================ */
	/* Be in the know emailer
	/* =================================================================*/
	if (document.documentElement.clientWidth > 1023) {
	    $(document).ready(function() {
		  // check emailer 
			var docUrl = document.location.href;
			var locUrl = docUrl.split('#');
			var emailSavingsStatus = "N";
			if (locUrl[1] == "emailSignup"){
			  emailSavingsStatus = "Y";
			}
            
			if(emailSavingsStatus == "Y"){
				 $("#emailerTrigger").removeClass('hide');
				 $("#emailIdFromPopup").focus();				 				
			}else{
				 $("#emailerTrigger").addClass('hide');
			}
		});
	}
	
    $("#emailerClose").click(function(){
        //hide the popup on close click
        $("#emailerTrigger").addClass('hide');
        $.cookie("email_modal_show", false, { expires: 15, path: '/', domain: 'searsoutlet.com' });
    });
    
	$('.emailSavingsFromPopUp').live('click', function(){
		var email = document.getElementById('emailIdFromPopup').value;
		var reg = /^([A-Za-z0-9])([a-zA-Z0-9\.\_\-])*@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
		var address = email;
			if (reg.test(email) == false){
				$("#invalidEmail").show();
			} else{
				$("#invalidEmail").hide();
				var dealFlag = true;
				var promotionFlag = true;
                $.cookie("email_modal_show", false, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
				var url="/d/email_submit.jsp?emailId="+email+"&dealFalg="+dealFlag+"&promotionFlag="+promotionFlag;
				postRequestEmailSavings(url);
				$("#emailerTrigger").addClass('hide');
			}
	});
	
	function postRequestEmailSavings(strURL) {
		var xmlHttp;
		if (window.XMLHttpRequest) {
			var xmlHttp = new XMLHttpRequest();
		} else if (window.ActiveXObject) { // IE
			var xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlHttp.open('POST', strURL, true);
		xmlHttp.onreadystatechange = function() {
			if (xmlHttp.readyState == 4) {
				var response = xmlHttp.responseText;
			}
		}
		xmlHttp.send('POST', strURL, true);
	}
