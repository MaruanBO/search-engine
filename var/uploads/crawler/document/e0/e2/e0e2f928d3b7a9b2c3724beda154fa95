    /*================================================================*/
    /*  Zip Init
    /*================================================================*/
    $(window).load(function(){
        storeUserZip();
    });
    
    function storeUserZip(){
        var userId = '0';
        var bypass = $.cookie("bypass");
        var zipVar = $.cookie("customer_zip");
        
        if(zipVar != null && zipVar != "" && zipVar.length > 0){
            $("#openLocalZipCode").html(zipVar);	
        }
        
        /* TODO: move zip code to model */
        var localZip = 'null';
        
        if(zipVar == localZip){
            $.cookie("bypass","true");
            return;
        }
        
        if(bypass == "true"){
            //alert(bypass);
        }
        
        if(userId != null && userId != "" && bypass != "true"){		
            getUserZip();
        }
    }	//storeUserZip
    
    //get user zip
    function getUserZip(){
        /* TODO: move zip code to model */
        var localZipCode = 'null';
        
        if(localZipCode.length != 5 || isNaN(localZipCode) || localZipCode == "00000"){
            return;
        }
            
        $("#openLocalZipCode").html(localZipCode);
        $.cookie("customer_zip",localZipCode, { expires:  expireDate, path: '/', domain: 'searsoutlet.com' });
        $.cookie("lsRedPage", document.URL);   //store the page URL in cookie to use for redirection
        
        var url = "/d/localZipSubmit.jsp?localZipCode=" + localZipCode ;
        document.location = url;
    }
    /*** end: zip init ***/
    
    
