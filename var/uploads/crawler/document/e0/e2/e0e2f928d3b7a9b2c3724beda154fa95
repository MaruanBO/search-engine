    /* ================================================================ */
    /* Email Savings in Header
    /* ================================================================ */
    function submitMailer(){
        var email = $('#validateEmail').val(); // get the value of the email text box							
        var reg = /^([A-Za-z0-9])([a-zA-Z0-9\.\_\-])*@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
        var address = email;
    
        if (reg.test(address) == false) {
            $("#invalidEmail2").show();
        } else {
            $('#invalidEmail2').hide();
            $('#signUpMessaging').show();
            $(".emailSubmit").hide();
    
            var dealFlag = true;
            var promotionFlag = false;
            var url = "/d/email_submit.jsp?emailId="
                    + email
                    + "&dealFalg="
                    + dealFlag
                    + "&promotionFlag="
                    + promotionFlag;
    
            $.ajax({
                    type : "POST",
                    url : url,
                    success : function() {
                        //console.log('ajax success');
                    }
            });
        }
    }
    
    
    /* ================================================================ */
    /* Google Plus Tracking
    /* ================================================================ */
    $(window).load(function(){
        if( $.cookie('gp_share') ){
            $('.google-plus-modal-body').addClass('solo');
            $('#googlePlusModalMask .chatClose').addClass('solo');
        }
           
        if( !$.cookie('gp_share') || !$.cookie('email_modal_show') ){
            $.ajax({
                url: '/d/get-coupon-type',
                cache: false,
                success: function(data){
                    if( !$.cookie('gp_share') && data === 'social' && ( mq === 'desktop' || mq === 'xl' ) ){
                        $('#googlePlusModalMask').show();
                        $('.g-copy').removeClass('hide');
                        $('.google-plus-modal-body').addClass('active');
                    }else if( data === 'email' && $('emailTrigger') && $.cookie('email_modal_show') != 'false' && ( mq === 'desktop' || mq === 'xl' ) ){
                        $("#emailerTrigger").removeClass('hide');        
                    }
                },
                error: function() {
                    console.log('there was an error with the social/email modal');
                }
            });   
        }   
    });
        
