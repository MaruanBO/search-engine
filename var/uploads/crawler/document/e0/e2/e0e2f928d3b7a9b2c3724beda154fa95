         cookieForCheckBoxes();
         
         //hide markers
         hideLocatedStores1();
		 
        //refresh the page
         window.location.reload();
      });
     
     /* persistCookie
     ***************************************************************/ 
     function persistCookie() {
         
         //if the cookie isn't set, set it
         if( !$.cookie('checkBoxCookie') ){
             $.cookie('checkBoxCookie', '1,1,1,1,1,1', { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
         }
         
         var checkBoxCookie = $.cookie('checkBoxCookie').split(',');
         
          
         /*
         // go ahead, test it
         alert(checkBoxCookie[0]);
         alert(checkBoxCookie[1]);
         alert(checkBoxCookie[2]);
         alert(checkBoxCookie[3]);
         alert(checkBoxCookie[4]);
         */
         
         if( checkBoxCookie[0] == '1' ){
             $('#chkOutlet1').prop('checked', true);
         }else{
             $('#chkOutlet1').prop('checked', false);
         }
         
         if( checkBoxCookie[1] == '1' ){
             $('#chkHardware1').prop('checked', true);
         }else{
             $('#chkHardware1').prop('checked', false);
         }
         
         if( checkBoxCookie[2] == '1' ){
             $('#chkFullline1').prop('checked', true);
         }else{
             $('#chkFullline1').prop('checked', false);
         }
         
         if( checkBoxCookie[3] == '1' ){
             $('#chkHometown1').prop('checked', true);
         }else{
             $('#chkHometown1').prop('checked', false);
         }
         
         if( checkBoxCookie[4] == '1' ){
             $('#chkApparel1').prop('checked', true);
         }else{
             $('#chkApparel1').prop('checked', false);
         }
         
         if( checkBoxCookie[5] == '1' ){
             $('#chkHomeApp1').prop('checked', true);
         }else{
             $('#chkHomeApp1').prop('checked', false);
         }
         
    } //persistCookie
    
    /* Cookie for Checkboxes
     ***************************************************************/ 
     function cookieForCheckBoxes() {
        
        var checkBoxCookie = "1,0,0,0,0,0";
        var outletId = "0";
        var hardwareId = "0";
        var fullLineId = "0";
        var hometownId = "0";
        var apparelId = "0";
        var homeApplId = "0";
        
        //text if they are checked
        if ($('#chkOutlet1').is(':checked')) {
            outletId = "1";
        }
        if ($('#chkHardware1').is(':checked')) {
            hardwareId = "1";
        }
        if ($('#chkFullline1').is(':checked')) {
            fullLineId = "1";
        }
        if ($('#chkHometown1').is(':checked')) {
            hometownId = "1";
        }
        if ($('#chkApparel1').is(':checked')) {
            apparelId = "1";
        }
        if ($('#chkHomeApp1').is(':checked')) {
        	homeApplId = "1";
        }
        
        checkBoxCookie = outletId + "," + hardwareId + "," + fullLineId + "," + hometownId + "," + apparelId + "," + homeApplId;
        //set the cookie
        $.cookie("checkBoxCookie", checkBoxCookie, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
        
    }
    
    /* Init the Map
     ***************************************************************/
    function initStoreSelector1() {
        //load the map script
        if (typeof google === 'object' && typeof google.maps === 'object') {
            //console.log('google map is read');
        }else{
            var scriptLoaded = false;
        	var script = document.createElement("script");
            script.type = "text/javascript";
            script.src = window.location.protocol + "//maps.googleapis.com/maps/api/js?v=3&sensor=false&&client=gme-sears&channel=10150-outlet-shopByStoreLocation&callback=initMap1&libraries=geometry";
            script.async = false;
            
            document.getElementsByTagName("head")[0].appendChild(script);
             
        }
        
        //put focus on the user location box
        $("#userLocation1").focus();
        
        var storeIds = $.cookie('sh_by_store');
        //alert(storeIds);
    } //initStoreSelector1
    
    

    //make the map
    function initMap1() {
        var defaultMapView = new google.maps.LatLng(41, -98);
        // sets co-ords for default view
        var mapOptions = {
            zoom: 3,
            center: defaultMapView, // center map display based on coords
            // defined above (default is USA)
            disableDefaultUI: true, // displays panning tool, street view, map,
            // satellite vies
            panControl: true, // display circular panning tool
            zoomControl: true, // displays vertical slider control
            scaleControl: false, // Displays scale in 1000km/mi
            mapTypeControl: true,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        }
    
        map1 = new google.maps.Map(document.getElementById('sl-map'), mapOptions);
    
        geocoder1 = new google.maps.Geocoder();
        
    }	//initMap1
    
