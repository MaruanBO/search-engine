    /** compare inits **/
    var numbCompare = 0;
    var compareIds = ",";
    var numbCompare2 = 0;
    var compareIds2 = ",";

    /** click event **/
    $('.compareInput .checkbox input').live('click', function(){
        var compareId = $(this).val();
        addRemoveCompare(compareId, $(this),'grid');
    });

    $(document).ready(function(){
        $('.compareInput .checkbox input').each(function(){
            if( $(this).prop('checked') == true ){
                var compareId = $(this).val();
                addRemoveCompare(compareId, $(this),'grid');
            }    
        });
    });
    
    /** add to compare **/
    function addRemoveCompare(compareElemId, addCompare, layout){
        console.log(compareElemId, addCompare, layout);
        var maxCompareItems;
       
			maxCompareItems = 5;
       
        if( addCompare.prop('checked') ){
            if (numbCompare >= maxCompareItems) {   // if over the allowed # of caparable items
                addCompare.prop('checked', false);  // uncheck it
                addCompare.parent().next().slideUp('fast'); //hide the button
                alert('Please select only ' + maxCompareItems + ' items for comparison.');  //let them know
                return;
            }
            
            if( numbCompare > 0){   //if the number to compare is less than the max
                addCompare.parent().next().slideToggle('fast'); //show the button
                $('.compare-now').animate({ 'opacity' : 1 });   //fade it in
            }else{
                addCompare.parent().next().slideToggle('fast').css({ 'opacity' : 0.0 });    //fade the button out
            }
            
            numbCompare++;  //increment the counter
            
            compareIds += compareElemId + ",";  //add the items to a fake array
        }else{
            numbCompare--;  //reduce the counter
            compareIds = compareIds.replace("," + compareElemId+ ",", ',');
            addCompare.parent().next().slideToggle('fast');
        }
    }

    $('.btn-compare-now').live('click', function(e){
        e.preventDefault();

        var compareElemId = $(this).attr('data-itemId'),
            layout = $(this).attr('data-layout');

        loadComparePage(compareElemId, layout);
    });
    
    function loadComparePage(compareElemId, layout){
        if($("#compare_check_"+layout+"_"+ compareElemId).prop('checked', true)){
            
            if(numbCompare == 1){   //if only one item is checked
                alert("You must select at least two items to compare.");
                return;
            }

            //create the url
            var pageUrl = '/d/products_compare.jsp';
            var catname = 'null';
            pageUrl = pageUrl  + "?catName=null" ; 

            //if the hidden field has a value append to the url
            if ($("#lsStores").val() != null && $("#lsStores").val() != "null") {
                pageUrl = pageUrl  + "&lsStores=local" ;
            } 

            rewriteUrl("pids", compareIds.substring(1, compareIds.length), pageUrl, true);
        }else{
            $("#compare_check_"+layout+"_"+ compareElemId).prop('checked', true);
            addRemoveCompare(compareElemId, compareElemCheck.checked, layout);
        }
    }

    /*================================================================*/
    /*  Rewrite URL - From Util.js
    /*================================================================*/
    function rewriteUrl(pName, pValue, pathname, addToBrowserHistory) {
    
        pValue = pValue == null ? "" : pValue;
        var loc = window.location;
        var query = new QueryString();
        var newPathname = loc.pathname;
        if(pathname != null || pathname != 'null'){
            newPathname = pathname;
        }
       
        //<protocol>//<host>[:<port>]/<pathname>[<hash>][<search>]
        var newUrl =loc.protocol + "//" + loc.host + newPathname ;
        if(newUrl.indexOf("?") == -1) {
            newUrl =  newUrl + "?" ;
        }else{
            newUrl =  newUrl + "&" ;
        }
        
        var params = query.getParameterNames();
        for (var i=0; i < params.length; i++) {
            
            if (pName == "ps") {
                if (params[i] != pName && params[i] != 'pn') {
                        var vals = query.get(params[i]);
                        for (var v=0; v < vals.length; v++)
                            if(params[i]!="disp"){
                            newUrl += params[i] + "=" + vals[v] + "&";
                            }
                }        
            } else {        
            if (params[i] != pName) {
                var vals = query.get(params[i]);
                for (var v=0; v < vals.length; v++)
                    if(params[i]!="disp"){
                    newUrl += params[i] + "=" + vals[v] + "&";
                    }
            }
        }
        }
        if (pValue != null && pValue != "null") {
            newUrl += pName+"="+pValue;
        }
    
        if (addToBrowserHistory)
            window.location = newUrl;
        else
            window.location.replace(newUrl);
    }
    
    
    function rewriteUrlInventory(pName, pValue, pathname,delMode , addToBrowserHistory) {
    
        pValue = pValue == null ? "" : pValue;
        var loc = window.location;
        var query = new QueryString();
        var newPathname = loc.pathname;
        newPathname = pathname;
        
       
        //<protocol>//<host>[:<port>]/<pathname>[<hash>][<search>]
        var newUrl =loc.protocol + "//" + loc.host + newPathname + "?";
        var params = query.getParameterNames();
        var flag="false";
        for (var i=0; i < params.length; i++) {
            if (pName == "ps") {
                if (params[i] != pName && params[i] != 'pn') {
                    var vals = query.get(params[i]);
                    for (var v=0; v < vals.length; v++)
                        if(params[i]!="disp") {
                            newUrl += params[i] + "=" + vals[v] + "&";
                        }
                }        
            } else {        
                if (params[i] != pName) {
                    var vals = query.get(params[i]);
                    for (var v=0; v < vals.length; v++)
                        if(params[i]!="disp"){
                            newUrl += params[i] + "=" + vals[v] + "&";
                        }
                }
            }
            if(params[i]=="ffmChannelDelModeInApp"){
                flag="true";
          }
        }
        
        if (pValue != null && pValue != "null") {
            newUrl += pName+"="+pValue;
        }
         if(flag=="false")
           newUrl +="&ffmChannelDelModeInApp"+"="+delMode;
      
    
        if (addToBrowserHistory)
            window.location = newUrl;
        else
            window.location.replace(newUrl);
    }
    
    
    function rewriteUrlSort(sortMode , pathname,addToBrowserHistory) {
        var loc = window.location;
        var query = new QueryString();
        var newPathname = loc.pathname;
        
        newPathname = pathname;
       
       
        //<protocol>//<host>[:<port>]/<pathname>[<hash>][<search>]
        var newUrl =loc.protocol + "//" + loc.host + newPathname + "?";
        var params = query.getParameterNames();
        var flag="false";
        for (var i=0; i < params.length; i++) {
             flag=false;
             if(params[i]=="sort"){
                 flag=true;
              }
                 
                var vals = query.get(params[i]);
                for (var v=0; v < vals.length; v++){
                    if(params[i]!="disp"&&params[i]!="sort"){
                    newUrl += params[i] + "=" + vals[v] + "&";
                    }
                    
              }
                
               if(flag){
                   if(i<(params.length-1))
                    newUrl +=  params[i] + "=" +sortMode + "&";
                   else
                      newUrl +=  params[i] + "=" +sortMode ;
                }
                
            }
           
        if(!flag){
             newUrl += "sort"+ "=" +sortMode;
            }
      
    
        if (addToBrowserHistory)
            window.location = newUrl;
        else
            window.location.replace(newUrl);
    }
