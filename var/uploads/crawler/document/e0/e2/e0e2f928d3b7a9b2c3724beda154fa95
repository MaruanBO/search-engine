    function showLocation(position) {
      var latitude = position.coords.latitude;
      var longitude = position.coords.longitude;
        
        $('#myLocation').val(latitude + '+' + longitude); 

        //get the zip from google
        $.ajax({
            url: 'http://maps.googleapis.com/maps/api/geocode/json?latlng=' + latitude + ',' + longitude + '&sensor=true',
            success: function(data) {
                searchLocations1( latitude + ',' + longitude );    //perform the location search with the lat/long coords  
                $('#userLocation1').val(data.results[0].formatted_address);
                searchLocations1();
            },
            error: function() {
                //address not found
            }
        });
    }
        
     /* On modal shown
      ***************************************************************/
      //when the modal is called
     $('#storeLocatorModal').on('shown', function () { 
         getLocation();
         
            
         function getLocation(){
            if ( navigator.geolocation && $.cookie('geoLocation') !== 'no' && (mq == 'mobile' || mq == 'tabletV' ) ){
                navigator.geolocation.getCurrentPosition(showLocation, function(errorCode){
                    //console.log(errorCode.code);
                    if( errorCode.code == 1){
                        $.cookie('geoLocation', 'no');
                    }
                });
            }
        }
         
        function showPosition(position){
            console.log(position.coords.latitude + ", " + position.coords.longitude);	
            $('#userLocation1').val(position.coords.latitude + ", " + position.coords.longitude);
            searchLocations1();
            //$('#userLocation1').val('');
            
            var imHereMarker = new google.maps.Marker({
                position: new google.maps.LatLng(position.coords.latitude, position.coords.longitude),
                map : map1,
                title: "You Are Here",
                icon: '/s/rwd/assets/images/map-user.png'
            });
        }
         
         persistCookie();
         
         // alert('cookie: ' + $.cookie('sh_by_store'));
         
         var storeIds = $('sh_by_store');
         
         if (storeIds != null && storeIds.length > 0 && storeIds[0] != ',') {
             var userlocsearch = $.cookie('userlocationsearch1');
             if (userlocsearch != null && userlocsearch.length > 0 && userlocsearch != "userlocationsearch1") { // IE fix		
                 $('#userLocation1').val(userlocsearch);
             }
    
         }
         
         //if the map hasn't been rendered
         if(map1 == null){
             initStoreSelector1();
         }

         //search the stores depend on selected zip code
         if($.cookie('customer_zip') != null)
         {
        	 $('#userLocation1').val($.cookie('customer_zip'));
        	 if (map1 == null) {
	             setTimeout(function () {
	            	 searchLocations1();
	             }, 500);
	             return;
	         }
        	 else
        	 {
        		 searchLocations1();	 
        	 }
         }
	    
         
     });	// end on storeLocatorModal shown
     
     
     //Load the previous stores
     function loadPreviousStores1(userLocationCookie) {
         //if the map hasn't loaded yet, go back & try again
         if (map1 == null) {
             setTimeout(function () {
                 loadPreviousStores1(userLocationCookie);
             }, 500);
             return;
         }
         
         var storeIdsCookie = $.cookie('userlocationsearch1');
         
         if (storeIdsCookie != null && storeIdsCookie.length > 0) {
            var storeIds = storeIdsCookie.split("~~");
    
            $('#userLocation1').val(storeIdsCookie);
            searchLocations1(userLocationCookie, storeIds, null, "");
         }
         
         previousStoresLoaded1 = true;
     }	//loadPreviousStores1
     
     //when the modal is dismissed
      $('#storeLocatorModal').on('hide', function () { 	 
