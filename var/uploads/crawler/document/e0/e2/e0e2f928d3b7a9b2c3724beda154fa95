    /* Google Plus +1 Icon
    /* ================================================================ */
    $('.google-plus-modal-window').live('click', function(e){
        e.preventDefault();
        $('#googlePlusModalMask').show();
        $('.google-plus-modal-body').addClass('active');
        
        if( !$.cookie('gp_share') ){
            $('.g-copy').removeClass('hide');
        }
    });

    //close g+ modal
    $('#googlePlusModalMask').live('click', function(e){
        e.preventDefault();
        $(this).hide();
        $('.google-plus-modal-body').removeClass('active');
        plusOneClose();  //set the cookie even if they close the modal
    });

    function plusOneCallback(){
        if( $.cookie('gp_share') != 'true' ){
           $.ajax({
                url: '/d/get-gplus-coupon-detail',
                cache: false,
                dataType: 'json',
                success: function(data){
                    $('.g-coupon').removeClass('hide');
                    $('.g-copy').removeClass('hide');
                    $('.google-plus-modal-body').removeClass('solo').css({'margin-left': '-40%'});
                    $('#googlePlusModalMask .chatClose').removeClass('solo');
                    $('#googlePlusModalMask .chatClose').css({'right': '34%'});
                    $.cookie("gp_share", true, { expires: expireDate, path: '/', domain: 'searsoutlet.com' });
                    $('.google-plus-modal-body iframe').hide();
                    $('.g-coupon-text').html(data.couponNumber);
                    $('.g-validTill').html(data.couponValidity);
                    $('.g-couponOffer').html(data.couponOffer);
                    $('.g-coupon-container').addClass('loaded');
                },
                error: function() {
                    console.log('error getting coupon code');
                }
            });  
        }
    }
        
    function plusOneClose(){
        if( !$.cookie('gp_share') ){
            $.cookie("gp_share", false, { expires: 15, path: '/', domain: 'searsoutlet.com' });
        }
    }
        
        
