    /*================================================================*/
    /*  Bloomreach Auto Suggest
    /*================================================================*/
    $(window).load(function(){
        var defaultConfig = {
	      accountId: 4075,
	      authKey: '',
	      domainKey: 'searsoutlet_com',
	      environment: 'prod',
	      searchCallback: function(searchUrl, searchData) {
	    
	        //if the user clicks on a category
	        var searchByNames = null;
	        searchByNames = searchData.parameters.names;
	    
	        //if the user clicks on a search term
	        var searchByTerms = null;
	        searchByTerms = searchData.parameters.query;
	    
	        if(searchByTerms != null && searchByNames == null){
	          $('#hiddensearchtext').val(searchByTerms);
	          $("#search-form").submit();
	        }
	    
	        if(searchByNames != null) {
	          searchByNames = searchData.parameters.names[searchData.parameters.names.length - 1];
	          $('#hiddensearchtext').val(searchByNames);
	          $("#search-form").submit();
	        }
	        
	      },
	      onFocusCallback: function(event) {
	      setTimeout(function(){
	        //do nothing, especially dont scroll the window
	      }, 200);
	    }
	    };
    	
      try {
            if ($(window).width() > 1023) { 
            
                $('#search-input').attr("autocomplete", "off");               //hide the autocomplete so it doesn't compete with brm
                $('#search-input').brm_suggest(defaultConfig);                //init the brm autosuggest
              
                var brmFromTop = $('#search-input').offset();                 //get the distance from the top of the searchbox
                    brmFromTop = brmFromTop.top + 32;                           //set it with height of the box + border
                var brmFromLeft = $('#search-input').offset();
                var brmWidth = $('#search-input').width() + 31;
                    
            }else{
                $('#search-input2').attr("autocomplete", "off");               //hide the autocomplete so it doesn't compete with brm
                $('#search-input2').brm_suggest(defaultConfig);                //init the brm autosuggest
              
                var brmFromTop = $('#search-input2').offset();                 //get the distance from the top of the searchbox
                    brmFromTop = brmFromTop.top + 29    ;                           //set it with height of the box + border
                var brmFromLeft = $('#search-input2').offset();
                var brmWidth = $('#search-input2').width() + 24;    
            }
            
            $('.brm-autosuggest-menu').css({ 'top' : brmFromTop, 'left' : brmFromLeft.left - 13, 'width' : brmWidth });    //move the brm autocomplete below the searchbox  
            var chosen = "";   
            
            //make the keyboard interact with brm
            $('body').keydown(function(e){
                
                if($('.brm-autosuggest-menu').is(':visible')) {    //only perform if brm suggest is visible
    
                    //if down is pressed 
                    if (e.keyCode == 40) {
                        if(chosen === "") {
                            chosen = 0; //init the selection
                        } else if((chosen) < 9) {   //if its pressed more than the total number of lis
                            chosen++; //increment
                        }
                        $('.brm-autosuggest-menu ul li').removeClass('selected').css({ 'background-color' : 'white' }).find('a').css({ 'color' : '#3a81ce' });                 //remove class and un-highlight
                        $('.brm-autosuggest-menu ul li:eq('+chosen+')').addClass('selected').css({ 'background-color' : '#3a81ce' }).find('a').css({ 'color' : '#fff' });      //add class and highlight
                       
                        //unfocus the form so we can use enter again
                        $('#search-input').blur();
                        
                        return false;
                    }
                    
                    //if up is pressed
                    if (e.keyCode == 38) { 
                        if(chosen === "") {
                            chosen = 0; //init the selection
                        } else if(chosen > 0) { //if you try to go above the first one
                            chosen--;   //decrease     
                        }
                        $('.brm-autosuggest-menu ul li').removeClass('selected').css({ 'background-color' : 'white' }).find('a').css({ 'color' : '#3a81ce' });                 //remove class and un-highlight
                        $('.brm-autosuggest-menu ul li:eq('+chosen+')').addClass('selected').css({ 'background-color' : '#3a81ce' }).find('a').css({ 'color' : '#fff' });      //add class and highlight
                        
                        //unfocus the form so we can use enter again
                        $('#search-input').blur();
                        
                        return false;
                    }
                    
                    //if enter is pressed
                    if (e.keyCode == 13) {
                        e.preventDefault();
                        var brmSearch = $('.brm-autosuggest-menu ul li.selected').text();      //store the selected search term
                        $('#hiddensearchtext, #search-input').val(brmSearch);                  //set the value of the hidden field
                        
                        $('#search-form').submit();                                            //submit the form based on the selected field
                        return false;
                    }
                }
            });
      }catch(e){} 
    });
    
    $(document).mousedown(function (e) {
        var container = $(".brm-autosuggest-menu");
        
        if (container.has(e.target).length === 0) {
            $(".brm-autosuggest-menu").hide();
        }
    });
        
