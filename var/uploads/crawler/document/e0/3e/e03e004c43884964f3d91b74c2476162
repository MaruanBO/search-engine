	window.sidebarHeight = jQuery('#home-sidebar').height();
}
});

jQuery(function() {
if (window.width > 1099) {
      var sidebar = jQuery(".flex-footer");
      jQuery(window).scroll(function() {
      
      if(jQuery(this).scrollTop() > (window.sidebarHeight-220)) { sidebar.addClass('fixed-footer'); }
      
      else if(jQuery(this).scrollTop() <= (window.sidebarHeight-220) && sidebar.hasClass('fixed-footer')){ sidebar.removeClass('fixed-footer'); }
      })
}
});
