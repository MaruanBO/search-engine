<div id="feedback-wrap">
 

<div class="container">

<div id="feedback">
<div id="exit">X</div>

<div class="head">
<h3>Have feedback / suggestions? Let us know!</h3>
</div>

<form action="#" id="form" method="post" name="form">
<input id="feedback-name" name="vname" placeholder="Your Name" type="text" value="">
<input id="feedback-email" name="vemail" placeholder="Your Email" type="text" value="">
<input id="feedback-sub" name="sub" placeholder="Subject" type="text" value="">
<textarea id="feedback-msg" name="msg" placeholder="Enter feedback..."></textarea>
<input id="send" name="submit" type="submit" value="Submit">
</form>


<script>
jQuery("#send").click(function(){
	
	function validateEmail($email) {
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		if( !emailReg.test( $email ) ) {
			return false;
		} else {
			return true;
		}
	}

	jQuery('input[name=vname]').css('border-color','rgb(200, 196, 196)');
	jQuery('input[name=vemail]').css('border-color','rgb(200, 196, 196)');
	jQuery('input[name=sub]').css('border-color','rgb(200, 196, 196)');
	jQuery('textarea[name=msg]').css('border-color','rgb(200, 196, 196)');
	var proceed_name=true;
	var proceed_email=true;
	var proceed_subject=true;
	var proceed_feedback=true;
			
	if (document.getElementById('feedback-name').value == '') {
	jQuery('input[name=vname]').css('border','1px solid red');
		proceed_name=false;
	}
	
	if (document.getElementById('feedback-email').value == '') {
	jQuery('input[name=vemail]').css('border','1px solid red');
		proceed_email=false;
	}
	
	if (document.getElementById('feedback-sub').value == '') {
	jQuery('input[name=sub]').css('border','1px solid red');
		proceed_subject=false;
	}
	
	if (document.getElementById('feedback-msg').value == '') {
	jQuery('textarea[name=msg]').css('border','1px solid red');
		proceed_feedback=false;
	}
	
	if( !validateEmail(document.getElementById('feedback-email').value)) {
	jQuery('input[name=vemail]').css('border','1px solid red');
		proceed_email=false;
	}
	
	
	if (proceed_name==false || proceed_email==false || proceed_subject==false || proceed_feedback==false){
		return false;
	}
	

});

</script>



</div>

</div>
</div>

