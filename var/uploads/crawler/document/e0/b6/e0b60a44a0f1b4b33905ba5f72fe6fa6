    <script type="text/javascript">
    $(document).ready(function(){
        $(function(){
            $('#tabIncludesidebartop .tabs-container li:last').after('<li class="clear"></li>');

            $('#tabIncludesidebartop .tab:first').addClass('active');
            $('#tabIncludesidebartop .tab:not(:first)').hide();
            $('#tabIncludesidebartop .tabs-container li a span').each(function(index){
                var newText = $('#tabIncludesidebartop .tab').eq(index).find('.blox-title h3').eq(0).text();
                if(newText != ''){
                    $(this).text(newText);
                    $('#tabIncludesidebartop').removeClass('empty');
                } else {
                    $(this).text('no block');
                    
                        $('#tabIncludesidebartop .tab').eq(index).html('<p>No block title found.  This block may not be using the title macro from the core base library component.</p>');
                        $(this).parents('li').hide();
                        $('#tabIncludesidebartop .tab').eq(index).hide();
                    
                }
            });
            $('#tabIncludesidebartop .tabs-container a').click(function(){
                // adjust tab states
                $('#tabIncludesidebartop .ui-tabs-selected').removeClass('ui-tabs-selected');
                $(this).parent().addClass('ui-tabs-selected');

                // activate windows
                $('#tabIncludesidebartop .tab').hide().removeClass('active');
                t = $(this).attr('href');
                t = t.split('#');
                t = '#tabset_sidebartop_'+t[1];
                $(t).show().addClass('active');
                return false;
            });
            
                if(!$('#tabIncludesidebartop').hasClass('empty')) $('#tabIncludesidebartop').show();
            
        });
    });
    </script>
