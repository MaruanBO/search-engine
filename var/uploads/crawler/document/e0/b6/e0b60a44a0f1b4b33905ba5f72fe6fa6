		<script>
		$(window).load(function(){
			var advancedMode = true;
			if(sniffer.info_b=='msie' && sniffer.info_v=='v7_0') advancedMode = false;

			// widths
			var $target = $('#block-967884');
			var $bWidth = $('#block-967884').outerWidth();
			var columns = 2;
				if($bWidth<=220) columns = 1; /* try to force 1 column if using a smaller region */
			var columnWidth = (($bWidth - 20)/columns) - 16;
			$target.find('.item').each(function(index){
				var $item = $(this);
				$item.width(columnWidth);
				if(columnWidth<=150)
					$item.addClass('small');
			});
			
			if($target.parent().width()<180){
				$target.parent().addClass('micro');
			}else if($target.parent().width()<550){
				$target.parent().addClass('mini');
			}
			
			if(advancedMode){
				$target.find('.item').each(function(index){
					//after last loop
					if( (index+1) == $target.find('.item').length){
						$target.removeClass('blox-loading');
						//arrange items on board
						$target.find('.pins').masonry({
							itemSelector:'#block-967884 .item'
						});
					};
				});
				$('#bulletins-modal-967884').dialog({
					title:'Details',
					modal:true,
					draggable:false,
					resizable: false,
					autoOpen:false,
					open: function(event, ui){
						//work with content
						$(this).css({ padding:0 }).parent().addClass('closeOnly').width(800).height(650).end().dialog("option", "position", "center");
						$(this).parent().find('.ui-dialog-titlebar .ui-dialog-titlebar-close').html('Close').css({
							top:'auto',
							lineHeight:'34px'
						});
					}
				});
				$target.find('.display-ad-window').click(function(event){
					event.preventDefault();
					if(window.location.protocol.indexOf('https')!=-1){
						window.location = $(this).attr('href')+'?mode=bulletins';
					}else{
						$('#bulletins-modal-967884').html('<iframe class="bulletinsIFRAME" src="'+$(this).attr('href')+'?mode=bulletins" height="650" width="800" scrolling="no" frameborder="0"></iframe>');
						$('#bulletins-modal-967884 iframe.bulletinsIFRAME').bind('external',function(e,data){
							window.location.href = data.url;
						});
						
						$('#bulletins-modal-967884').dialog('open');
					}
				});
			}else{
				$target.removeClass('blox-loading');
			};
			
			if(browser_pre_version == 'v7_0'||browser_pre_version == 'v8_0')
				$target.addClass('CSS2');
		});
		</script>
	
		
