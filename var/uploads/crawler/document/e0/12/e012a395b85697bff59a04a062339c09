$(".print_recipe_button").on("click", function(event) {
	var recipeDiv = $(this).closest(".recipe");
  var strFrameName = "print_frame_" + recipeDiv.find(".recipe_title").text().replace(/ /g,"_");
    var objFrame = window.frames[ strFrameName ];
    if (objFrame == null) {
        var jFrame = $("<iframe name='" + strFrameName + "'>");
        jFrame.css( "width", "672px").css("position", "absolute").css("left", "-4000px").appendTo($("body:first"));
    }
    objFrame = window.frames[strFrameName];
    var objDoc = objFrame.document;

    objDoc.open();
    objDoc.write("<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">" );
    objDoc.write("<html>");
    objDoc.write("<head>");
    objDoc.write("<title>");
    objDoc.write(document.title);
    objDoc.write("</title>");
    objDoc.write("<style type=\"text/css\">");
    objDoc.write(".recipe_photo {float:right; margin-left:20px;margin-bottom:10px;padding:4px;border:1px solid #D6D6D6;display:none;}");
    objDoc.write(".details {font-family: Georgia, serif;font-size: 12px;color: #9E9B94;margin-top:6px;}");
    objDoc.write(".yeild {white-space: nowrap;margin-right:10px;}");
    objDoc.write(".summary {margin:6px 0 0 0;line-height:1em;}");
    objDoc.write(".buttonbar {float:right;position:relative;z-index:1;}");
    objDoc.write(".buttonbar img {padding:0;border:none;}");
    objDoc.write("ul {font-family: Georgia, serif;font-size: 14px;list-style-type: none;margin: 0 0 20px 0;padding: 0;line-height:normal;}");
    objDoc.write("ol {font-family: Georgia, serif;font-size: 14px;list-style-type: decimal;margin: 0;padding: 0 0 0 26px;}");
    objDoc.write("h4 {font-family: Georgia, serif;color: #900;font-size: 20px;padding:0;margin:0;}");
    objDoc.write("h5 {font-family: MergeRegular, Arial, sans-serif, Helvetica;font-size: 18px;color:#990000;font-weight:normal;margin:10px 0 0 0;opacity:0.8;}");
    objDoc.write("h6 {font-family: 'Trebuchet MS', Trebuchet, Arial, Verdana, sans-serif;font-size: 11px;font-weight: normal;letter-spacing: 2px;margin: 8px 0 0 0px;text-transform: uppercase;color:#999;}");
    objDoc.write(".instructions h6 {margin: 0.8em 0 0.1em -26px;}");
    objDoc.write(".note {color:#888; padding-left: 26px; text-indent: -26px; display: inline-block;}");
    objDoc.write(".nutrition {margin-top:15px;font-family: Georgia, serif;font-size: 14px; padding-left: 26px; text-indent: -26px;}");
    objDoc.write(".nutrition span {white-space: nowrap;}");
    objDoc.write(".edamam {float:right;margin:0 -5px 0 10px;font-size:8px;color:gray;}");
    objDoc.write(".edamam img {width: 70px;border:none;}");
    objDoc.write("body {width:664px !important;}");
    objDoc.write("a {text-decoration:none !important;color: inherit !important;}");
    objDoc.write(".recipe_photo {display:inline !important;}");
    objDoc.write(".buttonbar {display:none !important;}");
    objDoc.write(".details .time, .details {display:inline !important;}");
    objDoc.write(".edamam {display:none !important;}");
    objDoc.write(".ingredients, .instructions, .notes, .nutrition {width:384px !important;}");
    objDoc.write("</style>");
    objDoc.write("</head>");
    objDoc.write("<body>");
  objDoc.write("<img alt=\"Closet Cooking Logo\" src=\"http://lh6.googleusercontent.com/-Ni2hfHOLXDM/T2ZtNXmC-JI/AAAAAAAAOFI/sOy81uVKvJk/s1600/logo.jpg\" width=\"380\" style=\"display:block;padding-bottom:10px;\">");
    objDoc.write(recipeDiv.html());
    objDoc.write("</body>");
    objDoc.write("</html>");
    objDoc.close();
	setTimeout(function(){objFrame.focus();objFrame.print();}, 200);
