        event.preventDefault();
        if ($j('#panel_search_field').val()=='E.g. What is normal blood pressure?') {
            $j('#panel_search_field').val('');
        }
        $j('#panel_search_medhelp').submit()
    })

    $j('#panel_search_medhelp').submit(function() {
        ga('send', 'event', 'Home_GetAnswers_Search', 'click', 'Search');
    });

          });
        </script>

</div>



  </body>
</html>
