    function fixFooter () {
        var article = document.getElementById('article');
        var foot = document.getElementById('foot');
        var htmlH = article.offsetHeight + 73 + 111; // H body = (h article) + (h header) + {h footer}
        var viewH = window.innerHeight;
        if(htmlH <= viewH) {
            foot.className = 'fix_footer';
        } else{
            foot.className = '';
        }
    }

