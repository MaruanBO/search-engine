function commitSign() {
    $('#compat-meter-form span.jquery-selectbox-currentItem').removeClass('error');
    if ($('#compatsign1').val() != '' && $('#compatsign2').val() != ''){
        $('#compat-meter-form').submit();
    } else {
        /* "value=" indicates empty string, rather than "value=''." */
        $('#compat-meter-form select[value=]').siblings('span.jquery-selectbox-currentItem').addClass('error');
    }
}

/* Set in a SELECT element with onchange="selectNavGoTo(this.value)" 
Gets the value (this.value) of the select then uses to pass the window's location.
Be sure the URLs are correct in the OPTION element's VALUE attribute.*/

function selectNavGoTo(locationValue) {
    window.location = locationValue;
}


