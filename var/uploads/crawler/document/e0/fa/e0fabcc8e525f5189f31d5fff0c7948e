										$("#dh_slide-sidebar a.sign-glyph-link").attr('href', '/horoscope-signs');
										$("#dh_slide-sidebar div.sign-date-range").html('');
										$("#dh_slide-body h2").html("Daily Horoscope for Everyone");
										$("#dh_slide-sidebar p.about").html('');
									} else {
										$("#dh_slide-sidebar a.sign-glyph-link").attr('href', '/horoscope-signs/'+sign_name.toLowerCase());
										$("#dh_slide-sidebar div.sign-date-range").html(sign_date_range);
										$("#dh_slide-sidebar p.about").html('About <a href="/horoscope-signs/'+sign_name.toLowerCase()+'">'+sign_name+'</a>');
										$("#dh_slide-body h2").html("Daily "+sign_name+" Horoscope");
									}
									// Love Scope
									$("#lh_slide-sidebar img").attr("src","http://gfx.dailyhoroscope.com/images/dailyhoroscope.com/glyphs/105x120/hearts/"+sign+".png").attr("alt", sign_name);
									$("#lh_slide-body p.interp").html("<strong>"+formatted_date[scope_date]+"</strong> -- "+ar_interps_love[scope_date][sign]['sms_interp']);
									$("#lh_slide-body div.addthis_toolbox").attr("addthis:description", ar_interps_love[scope_date][sign]['sms_interp']);
									if (sign == 0) {
										$("#lh_slide-sidebar a.sign-glyph-link").attr('href', '/love-horoscopes/this-week');
										$("#lh_slide-sidebar div.sign-date-range").html('');
										$("#lh_slide-body h2").html("Daily Love Horoscope for Everyone");
										$("#lh_slide-sidebar p.about").html('');
									} else {
										$("#lh_slide-sidebar a.sign-glyph-link").attr('href', '/horoscope-signs/in-love/'+sign_name.toLowerCase());
										$("#lh_slide-sidebar div.sign-date-range").html(sign_date_range);
										$("#lh_slide-sidebar p.about").html('<a href="/horoscope-signs/in-love/'+sign_name.toLowerCase()+'">'+sign_name+' &amp; Love</a><br><a href="/love-horoscopes/this-week/'+sign_name.toLowerCase()+'">Weekly Love</a>');
										$("#lh_slide-body h2").html("Daily "+sign_name+" Love Horoscope");
									}
								}

								$(document).ready(function(){
									$("#dh_sign-select, #lh_sign-select").val(scope_sign);
									getHoroscope(scope_sign);
									$("div.date-select a.day_"+scope_date).addClass("selected");
									$("div.date-select a.day_"+scope_date+".selected").click(function () {return false;});
									$('#widget-flag').attr('src', 'http://gfx.dailyhoroscope.com/images/dailyhoroscope.com/main-module/flag-0'+userWidgetFlagsCompleted+'.gif');

									if (startSlide > 0) {
										var slide = startSlide;
										// Get the proper tip.
										$('#hp_tip-msg').html(tip_box_ar[slide]);
										// Fancy drop-down fix.
										$('#home-slides div.arrow.inactive').removeClass('inactive');
										$("#home-slides-view, #home-slides-scroll").addClass("allow-overflow");
										$("#home-slides-view div.slide").addClass("hide");
										$("#hp_slide"+slide).removeClass("hide");
										$("#home-slides-scroll").addClass("shorten");
									} else {
										slide=0;
										// Get the proper tip.
										$('#hp_tip-msg').html(tip_box_ar[slide]);
										// Fancy drop-down fix.
										$("#home-slides-view,  #home-slides-scroll").addClass("allow-overflow");
										$("#home-slides-view div.slide").addClass("hide");
										$("#hp_slide"+slide).removeClass("hide");
										$("#home-slides-scroll").addClass("shorten");
									}

									if (completed == 1) {
										$('#home-slides div.arrow.inactive').removeClass('inactive');
										$("#flag-completion").addClass('completed');
										_gaq.push(['_trackEvent', 'karma_flag', 'raised']);
										contest_counter = enterKarmaContest(0);
									}

									$('#home-slides div.arrow').hover(function(){
										if ($(this).hasClass('inactive')) {
											return;
										}else{
											$(this).addClass('over');
										}
									}, function(){
										$(this).removeClass('over');
									});

									$('#home-slides div.arrow.next').click(slideNext);
									$('#home-slides div.arrow.prev').click(function(e){
										if ($(this).hasClass('inactive')) {
											return;
										} else {
											slidePrev(e);
										}
									});
									function slideNext(e) {
										$("#home-slides-scroll").removeClass("shorten");
										$("#home-slides-scroll").css('marginLeft', slide*(-480));
										$("#home-slides-view").removeClass("allow-overflow");
										$("#home-slides-view div.slide").removeClass("hide");
										(slide < maxSlide) ? slide++ : slide = 0;

										if ( e ) { // this was a real click event
											e.preventDefault();
											e.stopPropagation();
											_gaq.push(['_trackEvent', 'homepage_widget', 'next_btn', 'slide_'+slide]);
										}
										if (userWidgetFlagsCompleted < (slide+1) || userWidgetFlagsCompleted == 1) {
											(userWidgetFlagsCompleted < 5) ? userWidgetFlagsCompleted++ : userWidgetFlagsCompleted = 5;
										}
										sessionStorage.userWidgetFlagsCompleted = userWidgetFlagsCompleted;
										$('#widget-flag').attr('src', 'http://gfx.dailyhoroscope.com/images/dailyhoroscope.com/main-module/flag-0'+userWidgetFlagsCompleted+'.gif')
										if (slide > 0 || completed == 1) {
											$('#home-slides div.arrow.inactive').removeClass('inactive');
										} else {
											$('#home-slides div.arrow.prev').addClass('inactive');
										}
										if (slide == maxSlide) {
											completed = 1;
											sessionStorage.userWidgetCompleted = 1;
											$("#flag-completion").addClass('completed');
											enterKarmaContest(0);
											contest_counter = enterKarmaContest(0);
										}
										// Switch the tip for this slide.
										$('#hp_tip-msg').html(tip_box_ar[slide]);
										var distance = slide*(-480);
										/* To allow the fancy drop-down select menu to work, we must disable "overflow: hidden" on #home-slides-view.
										This reveals all other slides, so they must be hidden. The animation's callback is what executes this behavior.
										The line at the beginning of the function switches overflow off for the sliding effect.
										*/
										$('#home-slides-scroll').animate({marginLeft: distance}, 200, function () {
											$("#home-slides-scroll").css('marginLeft', '0');
											$("#home-slides-view").addClass("allow-overflow");
											$("#home-slides-view div.slide").addClass("hide");
											$("#hp_slide"+slide).removeClass("hide");
											$("#home-slides-scroll").addClass("shorten");
										});
										sessionStorage.userWidgetSlide = slide;
									}

									function slidePrev(e) {
										$("#home-slides-scroll").removeClass("shorten");
										$("#home-slides-scroll").css('marginLeft', slide*(-480));
										$("#home-slides-view").removeClass("allow-overflow");
										$("#home-slides-view div.slide").removeClass("hide");
										(slide > 0) ? slide-- : slide = maxSlide;
										if ( e ) { // this was a real click event
											e.preventDefault();
											e.stopPropagation();
											_gaq.push(['_trackEvent', 'homepage_widget', 'prev_btn', 'slide_'+slide]);
										}
										if (slide > 0 || completed == 1) {
											$('#home-slides div.arrow.inactive').removeClass('inactive');
										} else {
											$('#home-slides div.arrow.prev').addClass('inactive');
										}
										// Switch the tip for this slide.
										$('#hp_tip-msg').html(tip_box_ar[slide]);
										var distance = slide*(-480);
										/* To allow the fancy drop-down select menu to work, we must disable "overflow: hidden" on #home-slides-view.
										This reveals all other slides, so they must be hidden. The animation's callback is what executes this behavior.
										The line at the beginning of the function switches overflow off for the sliding effect.
										*/
										$('#home-slides-scroll').animate({marginLeft: distance}, 200, function () {
											$("#home-slides-scroll").css('marginLeft', '0').addClass("allow-overflow");
											$("#home-slides-view").addClass("allow-overflow");
											$("#home-slides-view div.slide").addClass("hide");
											$("#hp_slide"+slide).removeClass("hide");
											$("#home-slides-scroll").addClass("shorten");
										});
										sessionStorage.userWidgetSlide = slide;
									}
									$("#nod_get-bdate").submit(function(){
										var date_test = new RegExp("[^0-9]");
										if (date_test.test($("#nod_day, #nod_month, #nod_year").val())){
											$("#nod_get-bdate div.error").html("Invalid date format. Please only use numbers.");
											return false;
										} else if (1>$("#nod_month").val() || $("#nod_month").val()>12 || 1>$("#nod_day").val() || $("#nod_day").val()>31 || $("#nod_year").val()>2015 || $("#nod_year").val()<1900) {
											$("#nod_get-bdate div.error").html("Invalid date. Please enter your birth date as MM DD YYYY.");
											return false;
										}
									});

									$("#hp_tip-box").hover(function () {
											$("#hp_tip-tab.free, #hp_tip-msg.free").stop(true, true);
											$("#hp_tip-tab.free").animate({marginTop: 0}, 100);
											$("#hp_tip-msg.free").animate({marginTop: 5}, 200);
										}, function () {
											$("#hp_tip-tab.free").animate({marginTop: 10}, 200);
											$("#hp_tip-msg.free").animate({marginTop: 40}, 100);
										}
									)

									$("#hp_tip-tab").click(function(){
										$("#hp_tip-msg").toggleClass("free");
										$("#hp_tip-tab").toggleClass("free");
										$("#hp_tip-tab img").attr("src", "http://gfx.dailyhoroscope.com/images/dailyhoroscope.com/main-module/tip-tab-pin-40x55.png");
									});

									$("#contest-counter p").html("You have "+contest_counter+" entries this month.");
									make_autocompleting_date_fields("#nod_month","#nod_day","#nod_year");
								});
								</script>
