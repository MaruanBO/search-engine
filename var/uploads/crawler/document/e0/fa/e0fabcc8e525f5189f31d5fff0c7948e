								function sessionStorageAvailable() {
									try {
									  return 'sessionStorage' in window && window['sessionStorage'] !== null;
									} catch(e) {
									  return false;
									}
									return false;
								}
								var todays_date = (function(){
									var d    = new Date(),
										yyyy = d.getFullYear(),
										_m   = d.getMonth() + 1,
										mm   = _m < 10 ? '0' + _m : _m,
										_d   = d.getDate(),
										dd   = _d < 10 ? '0' + _d : _d,
										yyyymmdd = yyyy + '-' + mm + '-' + dd;
									return yyyymmdd;
								})();
								if (! sessionStorageAvailable() ) {
									window.sessionStorage = {};
								}
								if (typeof sessionStorage.userWidgetSlide === 'undefined') {
