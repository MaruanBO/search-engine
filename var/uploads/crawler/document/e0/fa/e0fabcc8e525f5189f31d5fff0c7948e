function bookmarksite(title,url){

    if (window.sidebar) {

        // firefox

        window.sidebar.addPanel(title, url, "");

    }

    else if (window.opera && window.print) { // opera

        var elem = document.createElement('a');

        elem.setAttribute('href',url);

        elem.setAttribute('title',title);

        elem.setAttribute('rel','sidebar');

        elem.click();

    } 

    else if (document.all) { // ie

        window.external.AddFavorite(url, title);

    }

    else { 

        var shortcut = (navigator.userAgent && navigator.userAgent.match(/(Mac OS X|\(Macintosh;)/)) ? 'Command-D' : 'Control-D';

        alert("Press "+ shortcut +" to bookmark this site.");

    }

}</script><script type="text/javascript">
function linkify(bodyText, searchTerm, href) {

  if ( window.location.href === href ) {
    return bodyText;
  }

  var startTag = "<a href=\"" + href + "\" class=\"dictionary-link\">";
  var endTag = "</a>";
  var found = false;

  var newText = "";
  var i = -1;
  var lcSearchTerm = searchTerm.toLowerCase();
  var lcBodyText = bodyText.toLowerCase();
    
  while (bodyText.length > 0) {
    i = lcBodyText.indexOf(lcSearchTerm, i+1);
    if (i < 0 || found) {
      newText += bodyText;
      bodyText = "";
    } else {
      if (
          // skip anything inside an HTML tag
          bodyText.lastIndexOf(">", i) >= bodyText.lastIndexOf("<", i) &&
          // skip anything inside a word by checking for spaces around the word
          i - 1 === lcBodyText.lastIndexOf(" ", i) &&
          i < lcBodyText.lastIndexOf(" ", i + searchTerm.length + 1) &&
          // tag filters
          lcBodyText.lastIndexOf("/script>", i) >= lcBodyText.lastIndexOf("<script", i) &&
          lcBodyText.lastIndexOf("/a>", i) >= lcBodyText.lastIndexOf("<a", i) &&
          lcBodyText.lastIndexOf("/h1>", i) >= lcBodyText.lastIndexOf("<h1", i) &&
          lcBodyText.lastIndexOf("/h2>", i) >= lcBodyText.lastIndexOf("<h2", i) &&
          lcBodyText.lastIndexOf("/h3>", i) >= lcBodyText.lastIndexOf("<h3", i) &&
          lcBodyText.lastIndexOf("/h4>", i) >= lcBodyText.lastIndexOf("<h4", i) &&
          lcBodyText.lastIndexOf("/style>", i) >= lcBodyText.lastIndexOf("<style", i) &&
          lcBodyText.lastIndexOf("/label>", i) >= lcBodyText.lastIndexOf("<label", i) &&
          !found
      ) {
        newText += bodyText.substring(0, i) + startTag + bodyText.substr(i, searchTerm.length) + endTag;
        bodyText = bodyText.substr(i + searchTerm.length);
        lcBodyText = bodyText.toLowerCase();
        i = -1;
        found = true;
      }
    }
  }
  
  return newText;
}

$(document).ready(function() {
  // define the block elements to parse
  var contents = $("#article-body, #blog-article"), i;
   
  for (i=0; i < contents.length; i++) {
    var text = $(contents[i]).html();
    for (var j in dictionary) {
      text = linkify(text, j, dictionary[j]);
    }
    $(contents[i]).html(text);
  }
});</script><script type="text/javascript">
