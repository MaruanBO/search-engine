<script src="/sci/static/wro/6.10.1/core-common-gpt.js" type="text/javascript"></script>
<script>
    /**
     * GPT multiple out of page ad positions override
     **/
    ibsys.ad.gpt.defineSlotsAndDisplayAds = function (gptAdSlots) {
        var self = this;
        if (gptAdSlots.length > 0) {
            /* define pageType, contextId, and contentId */
            var pgType = gptAdSlots[0].data.pageType,
                    cxId = gptAdSlots[0].data.contextId,
                    coId = gptAdSlots[0].data.contentId;

            googletag.cmd.push(function () {
                /* config */
                self.configureGPT(pgType, cxId, coId);
                /* loop through adLocations, setup GPT adSlot declarations adLocation found, match ad display divIds with defined adSlots */
                $ib.each(gptAdSlots, function () {
                    var adSizeData = this.data.size.toLowerCase(),
                        adPositionData = String(this.data.position),
                        adSlotOptions = {
                            ad : this,
                            adObjId : this.$el[0].id,
                            adSize : self.formatSlotSizes(adSizeData),
                            sizeMapping : this.sizeMapping,
                            adPosition : adPositionData,
                            isCompanionAdLocation : this.data.isCompanion,
                            isOop : adPositionData.lastIndexOf('oop', 0) != -1, /* detect if this is an out of page position */
                            targetedAdUnit : self.formatAdUnit(this.data)
                        };

                    /* adSlot declarations */
                    self.defineAdSlot(adSlotOptions);

                    /* match ad display div ids with defined adSlots */
                    googletag.cmd.push(function () {
                        googletag.display(adSlotOptions.adObjId);
                    });
                });
            });
        }
    };

    /**
     * GPT custom ad format overrides
     **/
    ibsys.ad.gpt.isMobile = function(data) {
        return data.topic.substring(0,2) === 'm_' ? '.mob' : '';
    };
    ibsys.ad.gpt.formatAdUnit = function (data) {
        return '/' + data.networkId /* NOTE: value of networkID can not begin with 'N' */
                + '/' + data.groupkey
                + this.isMobile(data)
                + '.' + data.sitekey
                + '/' + data.topic
                + '/' + data.section;
    };
    ibsys.ad.gpt.init({
        positionKeyword : 'pos'
    });
</script>
