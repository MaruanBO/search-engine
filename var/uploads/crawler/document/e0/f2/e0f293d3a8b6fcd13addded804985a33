var tablet = 'null';
window.onorientationchange = detecttabletOrientation;
function detecttabletOrientation() {
  if (typeof orientation == 'undefined') {
    // Do nothing.
  } else if (orientation == 0 || orientation == 180) {
    tablet = 'portrait';
  } else if (orientation == 90 || orientation == -90) {
    tablet = 'landscape';
  }
}
detecttabletOrientation();
