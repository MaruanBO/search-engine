<script type="text/javascript">
    // <![[CDATA


  function frmSubmit() {

  	var frm = document.newslettersignup;
  	var email = $.trim($("#email").val());

    var selected = false;
    for(var i=0;i<document.newslettersignup.offerId.length;i++) {
      if(document.newslettersignup.offerId[i].checked) {
        selected = true;
      }
    }

  	var error="";
    var pattern =/^([\w]+)(.[\w]+)*@([\w]+)(.[\w]{2,3}){1,3}$/;
  	if (email==null || email=="") {
  		error = "Please enter a valid email address to sign up.";
  	}
  	else if (!(email.match(pattern))) {
  		error = "Please enter a valid email address to sign up.";
  	}
    else if(!selected) {
      error = "Please select at least one newsletter";
    }

  	if (error != "") {
  		alert(error);
  		return false;
  	}

	cmCreateRegistrationTag ("WH Newsletter", email, null, null, null);
	return true;
  }

    // ]]>
  </script>
