	function AnvPlaylistSlide(divId,playlist,playerInstance,playlistHeight,numVideos) {
		console.log("@AnvPlaylist playlist:",playlist);
		var holderWidth = playlist.length * 211;
		var hiddenWidth = playlist.length * 206;
		var holderPx = holderWidth+"px";
		var hiddenPx = hiddenWidth+"px";		
		var container=document.getElementById(divId);
		container.className="storyplaylist";
		var containerInner0=document.createElement("div");
		containerInner0.id="playListCont";
		containerInner0.className="row-fluid content_wrap";
		var containerInner1=document.createElement("div");
		containerInner1.id="playListMaster";	
		var containerInner2=document.createElement("div");
		containerInner2.id="playListSlide";
		containerInner2.style.width = hiddenPx;
		var containerInner3=document.createElement("div");
		containerInner3.id="holder";	
		containerInner3.style.width = holderPx;
		for(var i=0,len=playlist.length; i<len;	i++) {		
			var entry=playlist[i];
			var itemSpan=document.createElement("div");	
			itemSpan.className="vidPLcont";
			var item=document.createElement("div");
			var thumb=document.createElement("img");
			var play=document.createElement("div");
			var title=document.createElement("div");
			var header=document.createElement("div");
			header.style.position="absolute";
			header.className="anv-item-header";
			header.style.top="0";
			header.style.height="auto";
			header.style.width="100%";
			header.style.padding="0px";
			item.className="imagesPLS anv-item";
			item.id="anv-item"+i;
			item.style.position="relative";	
			item.style.color="white";		
			item.onclick=onItemSelected;
			item.style.cursor="pointer";
			thumb.onload=onImageLoad;
			thumb.style.width="100%";
			thumb.style.height="auto";
			thumb.src=entry.src_image_url;
			play.style.background="url('http://media.graytvinc.com/designimages/anv_2_sprite32.png') 4px -18px";
			play.style.width="36px";
			play.style.height="36px";
			play.style.backgroundSize="400%";
			play.style.display="block";
			title.style.fontSize="13px";
			title.style.letterSpacing="0.02em";
			title.style.lineHeight="1.6em";
			title.style.position="relative";
			title.style.whiteSpace="wrap";
			title.style.fontFamily="Arial, sans-serif";
			title.style.padding="4px";
			title.innerHTML=entry.def_title;
			title.style.maxHeight="40px";
			title.style.overflow="hidden";
			item.appendChild(thumb);
			header.appendChild(title);
			header.appendChild(play);
			item.appendChild(header);
			containerInner3.appendChild(item);
		}
		containerInner2.appendChild(containerInner3);
		containerInner1.appendChild(containerInner2);
		containerInner0.appendChild(containerInner1);
		container.appendChild(containerInner0);
		var numPages = Math.ceil(playlist.length / 2);	
		if(numPages > 1 || (mobileFlag == true && playlist.length > 1)) {
			var leftPagerHolder = document.createElement("div");
			var leftPager = document.createElement("a");
			leftPager.id="plLEFT";
			leftPager.className="btn btn-large plLeft";						
			leftPager.innerHTML = "&nbsp&nbsp<img src=\"http://media.graytvinc.com/designimages/gdm_arrow_left.png\">";
			
			var rightPagerHolder = document.createElement("div");
			var rightPager = document.createElement("a");
			rightPager.id="plRIGHT";
			rightPager.className="btn btn-large plRight";
			rightPager.innerHTML = "<img src=\"http://media.graytvinc.com/designimages/gdm_arrow_right.png\">&nbsp&nbsp";
			
			container.appendChild(leftPager);
			container.appendChild(rightPager);
		}
				var videoSpace = 206;
		jQuery("#plLEFT").click(function(){
			var plWidth = jQuery("#playListMaster").width();
			var plMoveRemainder = plWidth % 206; 
			var plMove = plWidth - plMoveRemainder;	
			var leftStop = jQuery("#holder").css("margin-left");
			if(leftStop >= "0px"){
				jQuery("#holder").animate({marginLeft: '+=10px'}, 250 );
				jQuery("#holder").animate({marginLeft: '0px'}, 250 );
			}
			else
			{
				jQuery("#holder").animate({marginLeft: '+='+plMove}, 500 );
				if(leftStop >= "60px"){
					jQuery("#holder").animate({marginLeft: '0px'}, 250 );
				}
			}
		});
		jQuery("#plRIGHT").click(function(){
			var plWidth = jQuery("#playListMaster").width();
			var plMoveRemainder = plWidth % 206; 
			var plMove = plWidth - plMoveRemainder;	
			var plSlideWidth = jQuery("#playListSlide").width();
			var plRemainder = plWidth % videoSpace; 
			var plDisplayed = (plWidth - plRemainder) / videoSpace;
			var stopSpot = plSlideWidth - (plDisplayed*videoSpace);
			var rightStop = jQuery("#holder").css("margin-left");
			var rightStopReplace=rightStop.replace("px","");
			var rightStopInt=parseInt(rightStopReplace);
			var rightStopInvert=rightStopInt*-1;
				if(rightStopInvert >= stopSpot){
				jQuery("#holder").animate({marginLeft: '-=10px'}, 250 );
				jQuery("#holder").animate({marginLeft: '-'+stopSpot+'px'}, 250 );
			}
			else{
				jQuery("#holder").animate({marginLeft: '-='+plMove}, 500 );
				if(rightStopInt >= stopSpot){
					jQuery("#holder").animate({marginLeft: '0px'}, 250 );
				}
			}
		});
		function onImageLoad() {
			this.parentNode.style.display="inline-block";
		}		
		function onItemSelected() {
			pat=/anv-item(.+)/;
			var m=pat.exec(this.id);
			var index=parseInt(m[1]);
			playerInstance.play(playlist[index]);
			console.log(index);
		}	
	}
</script>

