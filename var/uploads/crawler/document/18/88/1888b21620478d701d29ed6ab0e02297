<script lang=javascript>
function validateForm()
{
var yourname=document.forms["qdbform"]["_fid_13"].value;
if (yourname==null || yourname=="" || yourname=="Your name")
  {
  alert("Your name must be filled out");
  return false;
  }
var finame=document.forms["qdbform"]["_fid_12"].value;
if (finame==null || finame=="" || finame=="Financial institution name")
  {
  alert("Your financial institution name must be filled out");
  return false;
  } 
var email=document.forms["qdbform"]["_fid_6"].value;
if (email==null || email=="" || email=="Email")
  {
  alert("Your email must be filled out");
  return false;
  }    
var phone=document.forms["qdbform"]["_fid_15"].value;
if (phone==null || phone=="" || phone=="Phone")
  {
  alert("Your phone must be filled out");
  return false;
  }  
var check=document.forms["qdbform"]["check"].value;    
if (check!=="")
  {
	  alert("filled field");
  //window.location = "http://www.digitalinsight.com/thankyou.html";
    return false;

  }        
  
  
}
</script>
