   $(function() {
        $('#dropdown').change(function(){ // DROP DOWN ON-OFF
            $('.hideblock').hide();
            $('.' + $(this).val()).show();
        });
		$('#moreinfo').change(function() { // OTHER CHECK BOX. If checked show div
        	if($(this).is(":checked")) {
			$('.notanexistingcustomer').show();
       		}
			else {
				$('.notanexistingcustomer').hide();
			}
    	});	
    });
		
function countChar(val) {
        var len = val.value.length;
        if (len >= 500) {
          val.value = val.value.substring(0, 180);
        } else {
          $('#textcount').text(180 - len);
        }
      };
</script>

