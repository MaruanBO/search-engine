<script type="text/javascript">

    function bookmark_us(url, title) {

        if (window.sidebar) // firefox
            window.sidebar.addPanel(title, url, "");
        else if (window.opera && window.print) { // opera
            var elem = document.createElement('a');
            elem.setAttribute('href', url);
            elem.setAttribute('title', title);
            elem.setAttribute('rel', 'sidebar');
            elem.click();
        }
        else if (document.all){ // ie
            window.external.AddFavorite(url, title);
		}
		else { // for other browsers which does not support
			alert('Your browser does not support this bookmark action.  Please add us to your bookmarks manually.');
			return false;
		}
    }
	
	$(document).ready(function(){$('#bookmarkLink').bind('click', function(){bookmark_us(window.location.href, document.title);});});
</script>

