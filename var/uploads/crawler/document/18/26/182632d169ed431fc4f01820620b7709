	PLATFORM.EventMan.registerToEvent(function() {

		var videoIcon = "http://ftpcontent.worldnow.com/wncustom/custom/cordillera/2015/video.png";
		var defaultImage = "http://KVOA.images.worldnow.com/images/6392363_G.jpg";
		var topTemplate = '<div class="csFeatureStory">' + '{if $graphicSrc}' + '<a href="{$headlineHref}"><img class="csFeatureImage" src="{$graphicSrc}"></a>' + '{/if}' + '</div>' + '<div class="csTopRight">' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}' + '<a href="{$headlineHref}"><img src="' + videoIcon + '" /></a>' + '{/if}' + '{if $custDate}<span class="badge new">NEW</span>{/if}' + '{$sectionText}' + '</h4>' + '<p>{$summaryHtml}</p>' + '<a class="wnSummaryLink" href="{$headlineHref}">Read Story&gt;</a>' + '</div>';

		var botTemplate = '<div class="csStory">' + '{if $graphicSrc}' + '<a href="{$headlineHref}"><img class="csImage" src="{$graphicSrc}"></a>' + '{/if}' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}' + '<a href="{$headlineHref}"><img src="' + videoIcon + '" /></a>' + '{/if}' + '{if $custDate}<span class="badge new">NEW</span>{/if}' + '{$sectionText}' + '</h4>' + '</div>';

		// Load story data
		var storyTop = $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var featureStory = storyTop[0];
		var storyBottom = $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var storyCol2 = $wn('#WNCol2 .wnDS-20 .wnDVSummary h4');

		function setBadge(stObj) {

			var badgeInsert = '';
			if (stObj.toLowerCase().indexOf("breaking") != -1) {
				badgeInsert = '<span class="badge breaking">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("developing") != -1) {
				badgeInsert = '<span class="badge developing">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("update") != -1) {
				badgeInsert = '<span class="badge update">' + stObj + '</span>';
			} else if (stObj.toLowerCase().indexOf("stream") != -1) {
				badgeInsert = '<span class="badge stream">' + stObj + '</span>';
			} else {
				badgeInsert = '<span class="badge">' + stObj + '</span>';
			}
			return badgeInsert;
		}


		featureStory.hasVideo = (this.isClip || this.isVideo);

		// Build top
		featureStory.storyId = (function(href) {
			var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
			return matches[1] || '';
		})(featureStory.headlineHref);

		var mDate = new Date(featureStory.dateText);
		mDate = (mDate.setHours(mDate.getHours()));
		featureStory.dateText = mDate;

		if (CSHELPERS.secondsSince(featureStory.dateText) < 3600) {
			if (featureStory.sectionText.length > 0) {
				featureStory.custDate = false;
			} else {
				featureStory.custDate = true;
			}
		}

		if (featureStory.sectionText.length > 0) {
			featureStory.sectionText = setBadge(featureStory.sectionText);
		}

		if (!featureStory.graphicSrc) {
			featureStory.graphicSrc = defaultImage;
		}

		$wn('#csTsr #csTop').csFormatterAppend(topTemplate, featureStory);

		// Build bottom
		$wn.each(storyBottom, function(i) {

			var mDate = new Date(this.dateText);
			timestamp = this.dateText
			mDate = (mDate.setHours(mDate.getHours()));
			this.dateText = mDate;
			if (CSHELPERS.secondsSince(this.dateText) < 3600) {
				var strTimeAgo = CSHELPERS.timeAgo(this.dateText);
				if (this.sectionText.length > 0) {
					this.custDate = false;
				} else {
					this.custDate = true;
					$wn('#WNCol2 .wnDS-20 .wnDVSummary h4:eq(' + i + ')').append('<span class="badge new">NEW</span>');
					$wn('#WNCol2 .wnDS-20 .wnDVSummary h4:eq(' + i + ')').before('<div class="timeAgo"><div id="timer"></div><span>' + strTimeAgo + '</span></div>');

				}
			} else {
				$wn('#WNCol2 .wnDS-20 .wnDVSummary h4:eq(' + i + ')').before('<div class="timeAgo"><div id="timer"></div><span>' + timestamp + '</span></div>');
			}

			if (this.sectionText.length > 0) {
				this.sectionText = setBadge(this.sectionText);
			}

			if (!this.graphicSrc) {
				this.graphicSrc = defaultImage;
			}
			this.hasVideo = (this.isClip || this.isVideo);
			var $bottom = $wn.$csFormatter(botTemplate, this);
			$wn('#csTsr #csBottom').append($bottom);

			$wn('#WNCol2 .wnDS-20 .wnDVSummary h4:eq(' + i + ')').before('<div class="sharingTools"><span>Share</span><div id="tools"><a id="facebook" href="https://www.facebook.com/sharer/sharer.php?u=' + document.domain + encodeURI(this.headlineHref) + '"></a><a id="twitter" href="https://twitter.com/intent/tweet?text=' + encodeURI(this.headlineText) + '&url=' + document.domain + encodeURI(this.headlineHref) + '>"></a><a id="googlePlus" href="https://plus.google.com/share?url=' + document.domain + encodeURI(this.headlineHref) + '"></a></div></div>');
		});
		$wn('#csBottom').find('.csStory').first().addClass('first');
		$wn('#csBottom').find('.csStory').last().addClass('last');
		// Hide any overflow

		$wn('#csBottom').find('.csStory:gt(3)').hide();
		$wn('#WNCol23Top #WNTopStoryWrap').hide();
		$wn('#WNCol2 #WNDS-20 .wnDSItems-standard .wnDVSummary:lt(4) ').hide();
		$wn('#WNCol2 #WNDS-20 .wnDSItems-standard .wnDVSummary:gt(13) ').hide();
		$wn('#WNCol2 #WNDS-20 .wnDSItems-standard .wnDVSummary').last().css('margin', '0px');

		$wn('#csTsr #csTop .csTopRight > p').dotdotdot();
		$wn('#csTsr #csBottom .csStory h4 a').dotdotdot();

	}, 'WNCol23done');
	function col23ARestyle() {
		$wn('<div class="header"><h2>CONTINUOUS NEWS <span class="breaking">/ Breaking News & Updates</span></h2></div>').insertBefore("#WNCol2 #WNDS-20 .wnDSItems-standard");
		//$wn("#WNCol2 #WNDS-20 > .header").append('<h3>Breaking News & Updates</h3>')

		var $stories = $wn("#WNCol2 #WNDS-20 .wnDSItems-standard");
		$stories.find(".wnDVSummary .wnSummaryLink").text('Read Story >');

		$stories.find('.wnDVSummary').each(function() {
			var image = $wn(this).find('.wnImage a');
			$wn(this).find('.wnImage').prependTo(this);
			$wn(this).find(".wnRole-STORY a .wnVideoIncluded").appendTo(image);
		});

		$wn("#WNCol2 #WNDS-20").append('<div id="readMore"><a href="/category/292667/top-stories"><span>More News >></span></a></div><div class="wnClear"></div>')
	}


	$(document).ready(function() {
		$('#WNCol2 #WNDS-20 .wnDSItems-standard .wnDVSummary p span').ellipsis({
			row : 3,
			onlyFullWords : true
		});
		$('#csTsr .csTopRight p').ellipsis({
			row : 4,
			onlyFullWords : true
		});
		col23ARestyle();
	}); 
</script>
<div id="csTsr" class="wnBlock displaySize">
	<div id="csTop"></div>
	<div id="csBottom"></div>
