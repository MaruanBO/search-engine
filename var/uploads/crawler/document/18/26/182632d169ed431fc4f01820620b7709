<script type="text/javascript">
	function col2Arestyle() {
		$wn("#WNCol2 #WNDS-20").insertAfter("#WNCol2 #DisplaySizeId80");
		var $stories = $wn("#WNCol2 #DisplaySizeId80 .wnGroup.contentGroup");
		$stories.find('.wnItem.feature ').each(function() {
			$wn(this).find('.summaryImage.abridged').prependTo(this);
		});
		$stories.find(".wnItem.header .more span").text('More Slideshows');
		$stories.find('.wnItem.feature:gt(3)').hide()
		$stories.find('.wnItem.feature .timestamps ').hide();
		$stories.find('.wnItem.feature .summary  ').hide();
		$stories.find('.wnItem.feature .sectionTitle').hide();
		$stories.find('.wnItem.feature .sharingTools').remove();
		$stories.find('.wnItem.feature .timeAgo').remove();
		$('#WNCol2 #DisplaySizeId80 .wnGroup.contentGroup .feature .headline a span').ellipsis({
			row : 2,
			onlyFullWords: true
		});
		var text = $('#WNCol2 #DisplaySizeId80 .wnGroup.contentGroup .feature .headline a span').text();
		//equal(text.lastIndexOf('...'), text.length - 4);
	}


	$(document).ready(function() {
		col2Arestyle();
	});
