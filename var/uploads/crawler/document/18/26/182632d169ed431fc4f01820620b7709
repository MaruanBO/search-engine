<script type="text/javascript" src="http://ftpcontent.worldnow.com/wncustom/js/wxmanager.js"></script>
<script type="text/javascript">
	function rotate() {
	}


	$wn(document).bind('csWxCol4Ready', function() {
		CSWxManager.setOption('defaultHandler', 'wsi');
		CSWxManager.setOption('iconSize', 45);

		var $CDEVcol4wx = $wn('#CDEV-cole4-weather');

		var wxZips = [{
			station : '85701',
			name : 'Tucson'
		}, {
			station : '85613',
			name : 'Sierra Vista'
		}, {
			station : '85621',
			name : 'Nogales'
		}, {
			station : '85607',
			name : 'Douglas'
		}, {
			station : '85546',
			name : 'Safford'
		}, {
			station : '85623',
			name : 'Oracle'
		}];

		CSWxManager.loadConfig(wxZips);

		CSWxManager.ready(function(wx) {
			if (!wx.hasData()) {
				return;
			}

			var cookieName = 'csWxCol4' + wng_pageInfo.affiliateName;
			var defaultStation = WNCookie.get(cookieName);

			// Populate the drop-down box
			$wn.each(wx.listNames(), function(i, name) {
				var $opt = $wn('<option />', {
					value : i,
					text : name
				});
				$CDEVcol4wx.find('.cdev-col4-weather-picker').append($opt);
			});

			// Wire up the drop-down 'change' event
			$CDEVcol4wx.find('.cdev-col4-weather-picker').change(function() {
				wx.index($wn(this).val());
				
				$CDEVcol4wx.find('.cdev-col4-weather-location-name').html($wn(this).find('option:eq(' + $wn(this).val() + ')').text());
				$CDEVcol4wx.find('.cdev-col4-weather-current-temp').html(wx.fetch('temperature'));
				$CDEVcol4wx.find('.cdev-col4-weather-current-high').html(wx.fetch('high') + '&deg;');
				$CDEVcol4wx.find('.cdev-col4-weather-current-low').html(wx.fetch('low') + '&deg;');
				$CDEVcol4wx.find('.cdev-col4-weather-feels-like').html('Feels like: ' + wx.fetch('feelslike') + '&deg;');
				$CDEVcol4wx.find('.cdev-col4-weather-icon').css('background-image', wx.fetch('iconcss'));
				$CDEVcol4wx.find('.cdev-col4-weather-sky-condition').html(wx.fetch('sky'));
				$CDEVcol4wx.find('.cdev-col4-weather-humidity').html(wx.fetch('humidity') + '%');
				$CDEVcol4wx.find('.cdev-col4-weather-picker')[0].checked = (wx.index() === parseInt(defaultStation, 10));
			});

			// Handle remembering and forgetting the default city
			$CDEVcol4wx.find('.cdev-col4-weather-saved-location').change(function() {
				if (this.checked) {
					// Expire one year from now
					var expDate = new Date();
					expDate.setFullYear(expDate.getFullYear() + 1);

					defaultStation = wx.index();
					WNCookie.set(cookieName, defaultStation, {
						expires : expDate
					});
				} else {
					defaultStation = null;
					WNCookie.set(cookieName, defaultStation, {
						expires : new Date(0)
					});
				}
			});

			// Load up the initial city
			if (defaultStation !== null) {
				$CDEVcol4wx.find('.cdev-col4-weather-picker').val(defaultStation).change().css({
					'opacity' : '0',
					'display' : 'block'
				});
			} else {
				$CDEVcol4wx.find('.cdev-col4-weather-picker').change().css({
					'opacity' : '0',
					'display' : 'block'
				});
			}

		});
	});

	$wn(document).trigger('csWxCol4Ready'); 
</script></div><script>Worldnow.Helpers.debug('UB col4a-weather-block(378546)', 'UB_378546');</script></div>
