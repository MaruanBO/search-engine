<script type="text/javascript">
	function col4Story() {
		var storyCol2 = $wn('#WNCol4 .displaySize .wnItem.feature h4');
		var col4Story = $wn('#WNCol4 .nestedCategory .wnItem.feature').wnDVSummaryExtract()
		if (wng_pageInfo.containerId='289038') {
			
			$wn('#WNCol4 .displaySize .header .more.enabled').hide();
		}
		$wn.each(col4Story, function(i) {

			//var timeAgo = $wn('#WNCol4 .nestedCategory .wnItem.feature .timestamps:eq(' + i + ') ').find('.timeago').text();
			//$wn('#WNCol4 .nestedCategory .wnItem.feature .headline.abridged:eq(' + i + ')').before('<div class="timeAgo"><div id="timer"></div><span>' + timeAgo + '</span></div>');
			var mDate = new Date(this.dateText);
			mDate = (mDate.setHours(mDate.getHours()));
			this.dateText = mDate;
			if (CSHELPERS.secondsSince(this.dateText) < 3600) {
				var strTimeAgo = CSHELPERS.timeAgo(this.dateText);
				if (this.sectionText.length > 0) {
					this.custDate = false;
				} else {
					this.custDate = true;
					$wn('#WNCol4 .displaySize .wnItem.feature .headline.abridged:eq(' + i + ')').before('<div class="timeAgo"><div id="timer"></div><span>' + strTimeAgo + '</span></div>');

				}
			}
		});
		$('#WNCol4 .displaySize .wnItem.feature .summary span').ellipsis({
			row : 2,
			onlyFullWords: true
		});
		var text = $('#WNCol4 .displaySize .wnItem.feature .summary span').text();
		//equal(text.lastIndexOf('...'), text.length - 4);

	}


	$(document).ready(function() {
		col4Story();
