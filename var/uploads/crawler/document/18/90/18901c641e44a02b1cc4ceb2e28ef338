<script type="text/javascript">

$(function() {
    zonex.featureSlider.atachEventHandler('slide.ss', function(e) {
        var $slide = $(e.relatedTarget);

        if ($slide.hasClass('item-slideshows')) {
            $(this).off('.ss');

            // If IE8 and less prevent unsupported Move plugin use.
            if (!($.browser.msie && parseInt($.browser.version) <= 8)) {
                setTimeout(function() {
                    move('#fs-slideshow-image').add('top', 170).then(move('#fs-slideshow-image').rotate(360)).end();
                }, 500);
            }
        }
    });
});

</script>
