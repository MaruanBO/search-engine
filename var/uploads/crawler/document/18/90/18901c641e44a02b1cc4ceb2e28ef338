<script type="text/javascript">

    var ua = navigator.userAgent.toLowerCase();
    var isAndroid = ua.indexOf("android") > -1;

    function showLogin() {
    	
    	
        if (isAndroid) {
            return;
        } else {
            return zonex.showLayer({
                id: 'loginlayer_dlg',
                content: '/index/login-layer/'
            });
        }
    }
    
    function showRegistration() {
        
        
        if (isAndroid) {
            return;
        } else {
            return zonex.showLayer({
                id: 'reglayer_dlg',
                content: '/access-manager/index/registration-layer/'
            });
        }
    }

</script>

            
        
	
        
            

