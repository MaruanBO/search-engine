<script type="text/javascript">
    
        $(function() {
            $('.accordion-body').on('shown', function() {
                $('html, body').animate({
                    scrollTop: $(this).parent().offset().top - 40
                }, 500);
            });
        });
    
</script>

<script type="text/javascript">
    
        $(function() {
            var $activeVisitors = $('#cs-active-visitors');
                   
            // get active visitors for particular platform
            function getPlatformVisitors(url, platform, refreshData) {
                // get desktop active vistors
                $.ajax({
                    dataType: "json",
                    url: url,
                    success: function(data) {
                        if (data && data['totalsForAllResults']) {
                            var activeVisitors = data['totalsForAllResults']['ga:activeVisitors'];
                            $activeVisitors.data(platform, activeVisitors ? activeVisitors : 0);
                        }
                            
                        if (refreshData) {
                            refreshActiveVisitors();
                        }
                    },
                    error: function() {
                        clearInterval($activeVisitors.data('refresh-interval-id'));
                    }
                });                
            }
            
            // get aactive visitors
            function getActiveVisitors() {
                getPlatformVisitors('http://sh-stats-proxy.appspot.com/query?id=ahBzfnNoLXN0YXRzLXByb3h5chULEghBcGlRdWVyeRiAgICAgICACgw', 'desktop-active-visitors', false);
                getPlatformVisitors('http://sh-stats-proxy.appspot.com/query?id=ahBzfnNoLXN0YXRzLXByb3h5chULEghBcGlRdWVyeRiAgICAwJyCCgw', 'mobile-active-visitors', true);
            }
                
            // highlight result if number of visitor has changed
            function refreshActiveVisitors() {
                var activeVisitors = parseInt($activeVisitors.data('desktop-active-visitors')) + parseInt($activeVisitors.data('mobile-active-visitors'));
                if (activeVisitors && ($activeVisitors.data('active-visitors') != activeVisitors)) {
                    $activeVisitors.data('active-visitors', activeVisitors).text(activeVisitors).addClass('highlight');
                    setTimeout(function() { $activeVisitors.removeClass('highlight'); }, 2000);
                }                
            }
                
            // start refreshing every 31 second
            function activateRefreshInterval() {
                var refreshIntervalId = $activeVisitors.data('refresh-interval-id');
                if (!refreshIntervalId) {
                    refreshIntervalId = setInterval(getActiveVisitors, 31000);
                    $activeVisitors.data('refresh-interval-id', refreshIntervalId);
                }                
            }
            activateRefreshInterval();
              
            // refresh only if tab is active
            $(window).focus(function() {
                activateRefreshInterval();
            });

            $(window).blur(function() {
                var refreshIntervalId = $activeVisitors.data('refresh-interval-id');
                if (refreshIntervalId) {
                    $activeVisitors.removeData('refresh-interval-id');
                    clearInterval(refreshIntervalId);
                }
            })
        });
    
</script>


