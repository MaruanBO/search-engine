<script>window.jQuery || document.write('<script type="text\/javascript" charset="utf-8" src="/static/js/jquery.js"><\/script>')</script>
<script type="text/javascript" charset="utf-8" src="/static/js/alljs.2.4.php"></script>
<script type="text/javascript">
jQuery(document).ready(function() {
    //safe mode anticache footer text
    var safe_mode_cookie_name = 'ir_safe_mode';
    var sf_cook_value = getCookie(safe_mode_cookie_name);
    if(typeof sf_cook_value == "undefined") {
        sf_cook_value = 1;
    }
    
    if( sf_cook_value == '-1' ) {
    	jQuery('#footer_safe_mode_indicator').text('OFF').css('color', '#DE0000');
    }else if( sf_cook_value == '1' ) {
    	jQuery('#footer_safe_mode_indicator').text('ON').css('color', '#9DCB3B');
    }


    //safe mode enable/disable
    jQuery("#ir_safe_mode_disable_btn").on('click', function(){
    	document.cookie= safe_mode_cookie_name + "=-1; expires=Thu, 18 Dec 2050 12:00:00 GMT";
    	$(this).text('Safe Mode Disabled!').attr('disabled','disabled');
    });

    jQuery("#ir_safe_mode_enable_btn").on('click', function(){
    	document.cookie= safe_mode_cookie_name + "=1; expires=Thu, 18 Dec 2050 12:00:00 GMT";
    	$(this).text('Safe Mode Enabled!').attr('disabled','disabled');
    });
});
</script>










    



