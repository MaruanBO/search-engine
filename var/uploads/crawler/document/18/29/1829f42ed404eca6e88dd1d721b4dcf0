<script type="text/javascript" src="/static/front/js/jquery.iframetracker.js"></script>
<script type="text/javascript">
    //tessss
    function irp_register_ad_click_event() {
    	_gaq.push(['_trackEvent', 'iRpBannerAd', 'iRpBannerAdClick', 'Click on ad Container']);
        _paq.push(['trackEvent', 'iRpBannerAd', 'iRpBannerAdClick', 'Click on ad Container']);

    }
    
    function irp_register_play_click_event() {
        _gaq.push(['_trackEvent', 'iRpPlayClicks', 'iRpPlayClick', 'Click on ad Play Container']);
        _paq.push(['trackEvent', 'iRpPlayClicks', 'iRpPlayClick', 'Click on ad Play Container']);
    }

    jQuery(document).ready(function($){
    	$('.track-ads-div div').on('click', function(){
    		irp_register_ad_click_event();
    	});

    	$('.track-ads-div iframe').iframeTracker({
    		blurCallback: function(){
                    irp_register_ad_click_event();
    		}
    	});
        
        
        $('.track-play-btn a').on('click', function(){
            irp_register_play_click_event();
    	});
        
    });
</script>







