  $(document).ready(function() {
        jQuery.ias({
            container : '.content',
            loaderDelay: 600,
            history: false,
            triggerPageThreshold: 30,
            item: '.deal',
            pagination: '.nav2',
            next: '.next-posts',
            loader: '<img src="/img/loader.gif" />',
            onRenderComplete: function(items) {
                $('div.timeRemaining').each(function() {
                    var timeLeft = $(this).attr('data-time-left');

                    var template;
                    switch( true ) {
                        case $(this).hasClass( 'miniTimer' ):
                            template = timerTemplates.mini;
                        break;
                        case $(this).hasClass( 'cartTimer' ):
                            template = timerTemplates.cart;
                        break;  
                        case $(this).hasClass( 'expired' ):
                            template = timerTemplates.expired;
                        break;
                        default:
                            template = timerTemplates.main;
                    }

                    $(this).countdown({
                        until: '+' + timeLeft + 's',
                        layout: template
                    });
                });
				
		        $('[data-toggle="tooltip"]').tooltip();

				            },
            onPageChange: function(pageNum, pageUrl, scrollOffset) {
								
                if (typeof(_gaq) !== 'undefined') {
                    // This will track a pageview every time the user
                    // scrolls up or down the screen to a different page.
                    path = jQuery('<a/>').attr('href',pageUrl)[0].pathname.replace(/^[^\/]/,'/');
                    _gaq.push(['_trackPageview', path]);
                }
            }
        });
    });
</script>
