    <script>
        $(function () {

            var currentCookie = window._data_.readCookieData("promopopupdata");

            if (currentCookie == "") {
                $('#newsletter-modal').delay(3000).modal();
                $('.modal-backdrop').addClass('transparent');
            }

            //$('#newsletter-modal-close').on('click', function() {
            //    $('#newsletter-modal .show').hide('medium', function () {
            //        $('#newsletter-modal .js-popup-closed').removeClass('hide').show();
            //        _gaq.push(['_trackEvent', 'Newsletter signup', 'popup', 'closed']);
            //        $('#newsletter-modal-close').off('click');
            //        $('#newsletter-modal-close').bind('click', function() {
            //            $('#newsletter-modal').modal('hide');
            //        });
            //    });
            //});

            $('#newsletter-modal').on('hide', function () {

                if (currentCookie != "") {
                    currentCookie = "," + currentCookie;
                }

                var d = new Date();
                var curr_date = d.getDate();
                var curr_month = d.getMonth() + 1; //Months are zero based
                var curr_year = d.getFullYear();
                window._data_.setCookieData('promopopupdata', 'Newsletter|' + curr_month + "-" + curr_date + "-" + curr_year + currentCookie, '1825');

            });
        });
    </script>



