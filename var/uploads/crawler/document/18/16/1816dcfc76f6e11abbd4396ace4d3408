<script>
$(document).ready(function(){
	$("#searchAccountantCTA span").on("click",function(){
		var zipVal = $("#zipField").val() || "";
		var distVal= $("#distField").val();
		if(zipVal.length > 0 && zipVal.length == 5 && (/^[0-9]*$/).test(zipVal)){
			// Creating iframe and appending the updated url to it, this creates required session for the search result page.
			$(this).parent().append("<iframe src='http://proadvisor.intuit.com/estore/apd/acm/SaveSBOInfo?saveCriteria=true&saveContact=true&zipCode="+zipVal+"&distance="+distVal+"&industry=&products=&services=&city=&state=&proAdvisorLastName=&suaFilter=false&geocode=true&_=1408400237497' id='searchAccountantIframe' width='1' height='1'></iframe>");
			window.open("http://proadvisor.intuit.com/find-a-proadvisor/search-results.jsp","_blank");
            // Removing the iframe as we are getting white background content on the page
            // Adding delay to make the iframe url request successful
            setTimeout(function(){$("#searchAccountantIframe").remove();},1000);
			$("#zipValErrMsg").remove();			
		}else{
			//Displaying error message
			if($("#zipValErrMsg").length == 0){
				$(".form-field-container").append("<div id='zipValErrMsg' class='zip-val-err-msg'>Please enter your 5-digit Zip Code</div>");
			}
		}
	});
	//Remove Zip Field error message once user start entering data again
	$("#zipField").on("keydown",function(){
		if($("#zipValErrMsg").length > 0) $("#zipValErrMsg").remove();	
	});
   
    $('#isearch').submit(function (e) {
        if ($('#searchTerm').val() == "" || $('#searchTerm').val() == "Search this site") {
            e.preventDefault();
        }
    });

    $('#searchTerm').focus(function () {
        if ($(this).val() == "Search this site") {
            $(this).val("");
            $(this).parent().toggleClass("with-data");
        }
    }).blur(function () {
         if ($(this).val() == "") {
            $(this).val("Search this site");
            $(this).parent().toggleClass("with-data");
        }
    });
   
    var geooverlay = setInterval(
        function()
        {
            if(!$('#geo-ip-redirect-popup').is(':hidden')) {
                $("body").css('overflow', 'hidden');
                clearInterval(geooverlay);
            }
        }, 1000);

    $('#no-thanks-geo-ip, #go-to-site-geo-ip').click(function(){
      $("body").css('overflow', 'auto');
    });
});

</script>
