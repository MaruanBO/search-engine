<script>$(document).ready(function(){
    /* Bubble script for product variant  */
    /* Intialize top value and roll area value */
    /* For Right Bubble */
    var topVal, oldTopVal, rollVal, oldRollVal,topVal2, oldTopVal2, rollVal2, oldRollVal2;

if(!$("section#promo_banner.promo-banner").length > 0){
$(".hero-product-content").css("padding-bottom","65px");
}
     topVal = ["-76","35","204"];
     oldTopVal = ["7","130","300"];
     rollVal = ["-21","90","260"];
     oldRollVal = ["-14","108","277"];
    /* For Left Bubble */
     topVal2 = ["-48","36","156"];
     oldTopVal2 = ["35","135","255"];
     rollVal2 = ["5","90","200"];
     oldRollVal2 = ["13","112","222"];

    /* Right side bubbles */
    $(".circle-img [class^='circle-img']:odd").each(function(i){

        $(this).css("z-index","10");
        $(this).hover(function(e){
            $(".bubble-text-left, .bubble-text").hide();
            $(this).next().stop().animate({"width":"250px", "top": topVal[i] + "px","left":"844px"},300, function(){
                $(this).find(":eq(0)").css("display","block");
            });
        },function(){
            $(this).next().find(":eq(0)").parent().stop().animate({"width":"0px", "top": oldTopVal[i]+"px","left":"914px"},"slow");
            $(this).next().find(":eq(0)").css("display","none");
            $(".bubble-text-left,.bubble-text").hide();
        });
    });

    /*Left side bubbles*/
    $(".circle-img [class^='circle-img']:even").each(function(i){
        $(this).css("z-index","10");
        $(this).hover(function(e){
            $(".bubble-text-left, .bubble-text").hide();
            $(this).next().stop().animate({"width":"250px", "top": topVal2[i] + "px","left":"-152px"},300, function(){
                $(this).find(":eq(0)").css("display","block");
            });
        },function(){
            $(this).next().find(":eq(0)").parent().stop().animate({"width":"0px", "top": oldTopVal2[i] + "px","left":"24px"},"slow");
            $(this).next().find(":eq(0)").css("display","none");
            $(".bubble-text-left,.bubble-text").hide();
        });
    });
$('html, body').css('overflow-y', 'auto');
  });</script>

    </Section>

