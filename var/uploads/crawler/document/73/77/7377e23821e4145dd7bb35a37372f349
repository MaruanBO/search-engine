	<div id="rightrail" class="cell">
		
	<div id="searchbox" class="innertube">
		Search: <input type="text" name="string" id="string" size="25" style="width:150px;"  />
		<a id="searchbutton" href="#">Go &rarr;</a>
	</div>

	<form id="searchform" action="/search" method="GET">
		<input type="hidden" name="query" id="query" />
		<input type="hidden" name="contenttypes" value="STORY" id="contenttypes" />
	</form>

	<script>
		$(document).ready(function() {
			$('#searchbox').keyup(function (event) {
				if (event.keyCode == "13") {
				$('#searchbutton').click();
				}
			});
		
			$('#searchbutton').click(function () {
				if ( $('#string').val() != "") { 
					$('#query').val($('#string').val()) 
				}
				

				/* This breaks on live for some reason.
				if ( $('#search_tb').is(':checked')) {	
					if ( $('#string').val() != "") { 
						var MyString = $('#string').val().replace(/\s/g,"* ") + '*';
						$('#query').val(MyString) 
					}
				}*/
						
				$('#searchform').submit();
			});
		});
	</script>






		
	





















		 




	<div id="div-gpt-ad-Micro" class="adtext mb25">
		<script type='text/javascript'>
			googletag.display('div-gpt-ad-Micro');
		</script>
