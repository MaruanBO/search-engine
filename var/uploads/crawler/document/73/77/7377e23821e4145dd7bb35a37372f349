		<div class="youneeq">
    <div id="youneeq_holder" class="">
        <div class="yq_header">
            <h3 class="upper center">Recommended for You</h3>
        </div>
    </div>
    <input type="image" class="recommended-modal" src="http://api.youneeq.ca/Content/images/powered-by-y.gif" role="button" />
    <script type="text/javascript">
        jQuery(document).ready(function($) {
            function on_yq_suggest(suggestions) {
                //check if there are suggested results, if so populate 'youneeq' div
                if (suggestions && suggestions.suggest && suggestions.suggest.node) {
                    var nodeids = suggestions.suggest.node;
                    var stories = "<ul class='slbull'>";

                    //add links, and apply sidebar styles - this could also be handled with a list
                    for (var i = 0; i < nodeids.length; i++) {
                        var articleTitle = nodeids[i].title;
                        var articleLink = nodeids[i].url;
						var trimmedTitle = Yq.titleTrim(nodeids[i].title);
                        
						stories += '<li class="railhed"><a id="'
                                + 'yq-rec-link-0' + i + '"'
                                + 'onclick="'
								+ "Yq.yq_panel_click('" + articleLink + "','" + trimmedTitle + "','" + nodeids[i].id + "')" + '" '
                                + 'class="yq_link" data-yq-id="' + nodeids[i].id + '" data-yq-title="' + articleTitle + '" href="' + articleLink + '">' + articleTitle + '</a></li>';
                    }
                    //opening and closing tags for widget + css class ref
                    $("#youneeq_holder").append(stories + '</ul>');
                }
            }

            function my_yq_init() {
				var canon_href = "http://www.toledoblade.com";
                Yq.observe({
					'domains': 'http://www.toledoblade.com',
                    //fetches 6 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
                    'suggest': [{'type': 'node', 'count': 6, 'categories': [], 'is_panel_detailed': 'true', 'isAllClientDomains': 'false'}],
					'alt_href': canon_href
                }, on_yq_suggest);
            }
            Yq.onready(my_yq_init);
        });
        
        var yq_profile_id = jQuery.get('http://api.youneeq.ca/app/sessionid', function(data){yq_profile_id = data;});

        function yq_panel_click(rec_link, rec_title, rec_id) {
            var api_url = "http://api.youneeq.ca/api/panelaction";
            var current_url = document.location.href;
            var panel_click_data = {domain_name: document.location.host, user_id: yq_profile_id, recommendation_url: rec_link, recommendation_title: rec_title, recommendation_content_id: rec_id, current_page: current_url};
            var json_data = {'json': JSON.stringify(panel_click_data)};
            var ajax_data = {url: api_url, crossDomain: true, dataType: 'jsonp', data: json_data};
            jQuery.ajax(ajax_data);
        }
    </script>
</div>

