<script type="text/javascript">
 (function($, window, document, undefined){
$(function() {
 
  var phoneControl = new PhoneControl();
  phoneControl.initCountryList("iSMSCountry");

  $('.selectpicker').selectpicker();

  $.validator.setDefaults({
    submitHandler: function() { 
      var country=$("#iSMSCountry");
      var phoneCountryCode = phoneControl.getPhoneCountryCode(country.val());
      var respMsg =  $('#respMsg'), 
            phoneNumber =  $("#phoneNumber");

      $("a.submit").unbind('click.sms');
      phoneNumber.addClass("process");
      respMsg.removeClass('servererror').removeClass('sent').empty();

      $.ajax({
                dataType: "json",
		 url:"https://www.real.com/cloud/smsapi/sendDownloadLink?test=1&jsoncallback=?",
                data:{
                    phoneNumber: phoneCountryCode + phoneNumber.val()
                }, 
                success:function(data) {
                   phoneNumber.removeClass("process");
                   if (data.status=="success") {
                     phoneNumber.removeClass("error");
                     respMsg.text(data.message).addClass('sent');
                  } else {
                    phoneNumber.addClass("error");
                    respMsg.text(data.message).addClass('servererror');
                  }
                  $("a.submit").bind('click.sms', sendSMS);
              }
        });
    }
  });
	// validate signup form on keyup and submit
      var getApp = $("#getApp");

      function sendSMS() {
           if(getApp.valid()){
                $(this).submit();
           }
      }

      $('#phoneNumber').bind('keyup', function() {
            $('#respMsg').removeClass('servererror').removeClass('sent').empty();
      });

       getApp.validate({
		rules: {
			phone: {
				required: true,
	                        phoneNB: true
			}
		},
		messages: {
			phone: "*Please enter a valid phone number."
		}
	});

        getApp.validate({
              showErrors: function(errorMap, errorList) {
                    $('#respMsg').removeClass('servererror').removeClass('sent').empty();
              }
        });

	$("a.submit").bind('click.sms', sendSMS);

});

})(jQuery, window, document);
</script>    
       
      <div class="push">
      </div>
    </div>
