<td valign="top" align="left" width="700">
<a href="/merge/tsnform.aspx?c=sportsnetwork&page=nfl/specialevents/superbowl/2015/index.aspx"><img src="http://images.sportsnetwork.com/snetwork/home700/events/nfl/superbowl2.jpg" border="0" width="700" height="60"></a>
</td>

<td valign="middle" colspan="1" width="303" bgcolor="#f36c22" class="countbg">
<table cellpadding="0" cellspacing="0" border="0">
<tr>
<td valign="middle" colspan="1" width="303">
<cite id="countdown"></span>
<script>
// set the date we're counting down to
var target_date = new Date("Feb 01, 6:30 pm 2015").getTime();

// variables for time units
var days, hours, minutes, seconds;

// get tag element
var countdown = document.getElementById("countdown");

// update the tag with id "countdown" every 1 second
setInterval(function () {

    // find the amount of "seconds" between now and target
    var current_date = new Date().getTime();
    var seconds_left = (target_date - current_date) / 1000;

    // do some time calculations
    days = parseInt(seconds_left / 86400);
    seconds_left = seconds_left % 86400;

    hours = parseInt(seconds_left / 3600);
    seconds_left = seconds_left % 3600;

    minutes = parseInt(seconds_left / 60);
    seconds = parseInt(seconds_left % 60);

    // format countdown string + set tag value
    countdown.innerHTML = days + " days " + hours + " hours " + minutes + " min. " + seconds + " sec.";

}, 1000);
</script>
</td>
</tr>
</table>
</td>

<td valign="top" colspan="1" width="2"></td>
</tr>
