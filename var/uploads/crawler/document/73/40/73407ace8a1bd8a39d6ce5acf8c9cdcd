        <script>
        (function(win, doc){
            // Set some vars on the window:
            win.JSON || doc.write('<script src="//d2pe20ur0h0p8p.cloudfront.net/libs/JSON-js/2011-02-23/json2.js"><\/script>')
            win.HTML_LOAD_TS=(new Date()).getTime();
            win.FBT = {};

            // MicroEvent modified from https://github.com/jeromeetienne/microevent.js/blob/master/microevent.js
            win.FBT.MicroEvent = MicroEvent = function(){};
            MicroEvent.prototype = {
                on  : function(event, fct){
                    this._events = this._events || {};
                    this._events[event] = this._events[event]   || [];
                    this._events[event].push(fct);
                },
                off : function(event, fct){
                    this._events = this._events || {};
                    if( event in this._events === false  ) return;
                    var index = this._events[event].indexOf(fct);
                    this._events[event].splice(index, 1);
                },
                trigger : function(event /* , args... */){
                    this._events = this._events || {};
                    if( event in this._events === false  ) return;
                    for(var i = 0; i < this._events[event].length; i++){
                        this._events[event][i].apply(this, Array.prototype.slice.call(arguments, 1))
                    }
                }
            };
            MicroEvent.mixin = function(destObject){
                var props = ['on', 'off', 'trigger'];
                for(var i = 0; i < props.length; i ++){
                    destObject.prototype[props[i]] = MicroEvent.prototype[props[i]];
                }
            };
            if (!Array.prototype.indexOf) {
                Array.prototype.indexOf = function(elem) {
                    for (var i = 0, length = this.length; i < length; i++) {
                        if (this[i] === elem)   return i;
                    }
                    return -1;
                };
            }
            // Simple State Emitter
            win.FBT.StateEmitter = StateEmitter = function(initialState){
                this._state = initialState;
                this.on('change', function(state){this._state = state })
            };
            StateEmitter.prototype.state = function(){ return this._state };
            MicroEvent.mixin(StateEmitter);
            // debounce from underscore.js (http://underscorejs.org/)
            var debounce = function(func, wait, immediate) {
                var timeout;
                return function() {
                    var context = this, args = arguments;
                    var later = function() {
                        timeout = null;
                        if (!immediate) func.apply(context, args);
                    };
                    if (immediate && !timeout) func.apply(context, args);
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                };
            },

            getViewState = function(){
                var width, view = "narrow";
                try {
                    width = doc.documentElement["clientWidth"];
                    if (width < 503) {
                        view = "narrow";
                    } else if (width < 803) {
                        view = "medium";
                    } else {
                        view = "wide";
                    }
                } catch (e) { }
                return view
            },

            currentView = getViewState(),
            ViewSizeEmitter = new StateEmitter(currentView),
            resizeThrottle = 300,
            resizeCallback = debounce(function(){
                var nextView = getViewState();
                if(ViewSizeEmitter.state() !== nextView){ ViewSizeEmitter.trigger('change', nextView) }
            }, resizeThrottle);

            if( win.addEventListener ){ win.addEventListener( "resize", resizeCallback, false ); }
            else if( win.attachEvent ){ win.attachEvent( "onresize", resizeCallback ); } // From Respond.js

            FBT.ViewSizeEmitter = ViewSizeEmitter;
        })(window, document);
        </script>
    


        

