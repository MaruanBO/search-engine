	}
    var isLandscape = function () {
        return ('undefined' !== typeof window.orientation && (90 === Math.abs(window.orientation) || 270 === Math.abs(window.orientation))) ? true : false;
    },
        addE = window.addEventListener || window.attachEvent,
        callB = function (e) {
