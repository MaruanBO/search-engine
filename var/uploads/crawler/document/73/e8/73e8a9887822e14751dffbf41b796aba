		$(document).ready(function() {
			$('#partners').carousel({
				framesContSel: 'ul', 
				framesSel: 'ul li',
				transition: 'slide'
			});
			$('#partners').data('carousel').auto(true);
			
			$('#partners-prev').click(function() {
				$('#partners').data('carousel').prev();
				return false;
			});
			$('#partners-next').click(function() {
				$('#partners').data('carousel').next();
				return false;
			});
			
			$('#quote').carousel({
				framesSel: 'li',
				transition: 'fade'
			});
			$('#quote').data('carousel').auto(true);
			
