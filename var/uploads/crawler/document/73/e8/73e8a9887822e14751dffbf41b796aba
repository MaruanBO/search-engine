	<script type="text/javascript">

		$(document).ready(function() {

			

			function selectTab(el) {

				var sectionNav = $(el).parents('.section-nav');

				var sectionName = sectionNav.attr('id').substr(0, sectionNav.attr('id').length - 4);

			

				var selected = $('#' + sectionName + ' .section-nav li.selected');

				selected.removeClass('selected');

				var selectedSubSectionName = selected.find('a').attr('href').substr(1);

				$('#' + selectedSubSectionName).css('display', 'none');

				

				

				$(el).parent().addClass('selected');

				var subSectionName = $(el).attr('href').substr(1);

				$('#' + subSectionName).css('display', 'block');

				

				var index = $(el).parent().index() + 1;

				

				sectionNav.css('background-image', 'url(images/home/tabs.' + sectionName + '.' + index + '.png)');

			}

		

			$('.section-nav li a').click(function() {

				selectTab(this);

				return false;

			});

			

			$('#updates-nav a').click(function() {

				document.cookie = 'active-updates-tab=' + $(this).attr('href') + '; expires=' + new Date(2020, 1, 1, 0, 0, 0) + '; path=/';

			});

			

			var matches =  /active-updates-tab=(.*?)(:?;|$)/.exec(document.cookie);

			if (matches && matches[1]) {

				var activeUpdatesTab = matches[1];

				if (activeUpdatesTab) {

					selectTab($('#updates-nav a[href=' + activeUpdatesTab + ']').get(0));

				}

			}

			

			setTimeout(function() {

				$('#spotlight').carousel({

					framesContSel: '.frames',

					framesSel: '.frame',

					navItemsSel: '.nav li',

					transition: 'fade'

				});

				

				$('#spotlight').data('carousel').auto(true);

			}, 500);

			

		});

	</script>

