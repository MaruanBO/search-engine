    ga('create', 'UA-4532215-2', 'auto', { 'allowLinker': 'False' });
    ga('require', 'displayfeatures');
    var pageTrackerUniversal = {
        _trackPageview: function (url) {
            ga('send', 'pageview', url || '');
        },
        _trackEvent: function (category, action, opt_label, opt_value) {
            ga('send', 'event', category, action, opt_label || '', (typeof opt_value === 'number') ? opt_value : null);
        },
        _addTrans: function (orderId, affiliation, total, tax, shipping, city, state, country) {
            ga('require', 'ecommerce', 'ecommerce.js');
            ga('ecommerce:addTransaction', {
                id: orderId || '',     // Transaction ID*
                affiliation: affiliation || '', // Store Name
                revenue: total || '',       // Total
                shipping: shipping || '',          // Shipping
                tax: tax || ''         // Tax
            });
        },
        _addItem: function (orderId, sku, name, category, price, quantity) {

            ga('ecommerce:addItem', {
                id: orderId || '',            // Transaction ID*
                sku: sku || '',         // Product SKU
                name: name || '',   // Product Name*
                category: category || '',      // Product Category
                price: price || '',              // Price
                quantity: quantity || ''                   // Quantity
            });
        },
        _initData: function () {
            // Deprecated. initData() now executes automatically in the ga.js tracking code.
        },
        _link: function (targetUrl, useHash) {
            ga('linker:autoLink', [targetUrl]);
            ga('send', 'pageview');
        },
        _trackTrans: function () {
            ga('ecommerce:send');
        },
        _clearTrans: function () {
            ga('ecommerce:clear');
        },
        _setCustomVar: function (index, name, value, opt_scope) {
            ga('set', 'dimension' + index, value);
        }
    };
</script>
