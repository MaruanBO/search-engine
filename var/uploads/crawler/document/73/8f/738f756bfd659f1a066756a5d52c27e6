		function checkFormValues() {
			var radio_checked = false;
			var r_vals = document.getElementsByName("answer");
			for( index = 0 ; index < r_vals.length ; index++ )	{
				if( r_vals[index].checked )	{
					radio_checked = true;
				}
			}
			if( document.getElementById("form_fname").value == '' 
				|| document.getElementById("form_email").value == ''
				|| !radio_checked ) {
				alert("Please fill out the form.");
				return false;
			}
			var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
			if(!re.test(document.getElementById("form_email").value)) {
				alert("Invalid Email.");
				return false;
			}
		
			return true;
		}
		
		function form_submit() {
			if( checkFormValues() ) {
				$('#form-msg').fadeIn();
				$('#email-form').fadeOut(3000);

				<!-- AE Conversion Tracker #52 -->
				aeConversionTracker = 52;
				var jsLink = $("<script type='text/javascript' src='http://code.adengage.com/track_conversion.js'>");
			    $("head").append(jsLink); 
				<!-- // End AE Conversion Tracker -->
				
				return true;
			} 
			return false; 
		}
		
