		{
			if (startCount == 1)
			{
				
				if (movie_time >= future_time) 
				{
					randseed = Math.ceil((Math.random()*4));
					theCount = theCount-randseed;
										
					if (theCount<=5) 
					{
						clearInterval(countDown);
						clearInterval(window.timer2);
						theCount = 5;
					}
					else 
					{
						movie_time = timer_seconds;
						seconds = (end_time - movie_time) / (theCount-5);
						future_time = movie_time + seconds;
