<script type="text/javascript">
$('div#col3 a.watch')
	.live('mouseover mouseout', function(event) {
		if (event.type == 'mouseover')
			$('div#col3 div.player img#video').attr('src', '/images/temp-home-video-on.jpg');
		else
			$('div#col3 div.player img#video').attr('src', '/images/temp-home-video-off.jpg');
	});
	
$(window).load(function(){
	CSSTable('div#home-footer', 'div.column', 3);
});
$(document).ready(function() {
	$.ajax ({
         type: "GET", 
         url: "twitter.json", 
         dataType: "json", 
         contentType: "application/json; charset=utf-8", 
         cache: "false",
         success: twitter_callback
    });
});
function twitter_callback (twitters) {
	var status = twitters[0].text.replace(/((https?|s?ftp|ssh)\:\/\/[^"\s\<\>]*[^.,;'">\:\s\<\>\)\]\!])/g, 	function(url) {
      return '<a href="'+url+'">'+url+'</a>';
    }).replace(/\B@([_a-z0-9]+)/ig, function(reply) {
      return  reply.charAt(0)+'<a href="http://twitter.com/'+reply.substring(1)+'">'+reply.substring(1)+'</a>';
    });

	var time = parseTwitterDate(twitters[0].created_at);
	var formated_time = time.strftime('Posted on Twitter %I:%M %p %b %d');
	$("#latest_tweet").html(status);
	$("#tweet_posted").html(formated_time);
	CSSTable('div#home-footer', 'div.column', 3);
}
function parseTwitterDate(str) {
    var v=str.split(' ');
	return new Date(Date.parse(v[1]+" "+v[2]+", "+v[5]+" "+v[3]+" UTC"));
}
getTextTout('home');
</script>

