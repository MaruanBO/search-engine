<script type="text/javascript" src="https://static.garmincdn.com/com.garmin.www/ui/src-js/jquery.cycle-v1.0.0.js"></script>
<script type="text/javascript" src="https://static.garmincdn.com/com.garmin.www/ui/src-js/jquery.touchwipe.min-v1.0.0.js"></script>
<script type="text/javascript">
jQuery(document).ready(function() {
   // Cycle Plugin
   jQuery('.slide-show').cycle({
      fx: 'scrollHorz',
      pager:  '.slide-show-nav',
      speed: 300,
      timeout: 5000,
      pause: 1
   });
   // Touchwipe Plugin
   jQuery(".slide-show").touchwipe({
            wipeLeft: function() {jQuery(".slide-show").cycle("next");},
            wipeRight: function() {jQuery(".slide-show").cycle("prev");}
   });
   //Promo Box Randomize
   var $li = jQuery(".random");
   if ($li.length > 0) {
      $li.hide();
   var len = $li.length;
   var counter = 1; // how many items to display
   var randomnumber;
   while (counter > 0) {
      randomnumber = Math.floor(Math.random() * (len + 1));
      if ($li.eq(randomnumber).is(":hidden")) {
         $li.eq(randomnumber).show().addClass('active');
         counter -= 1;
      }
   }  
  }
});
</script>
