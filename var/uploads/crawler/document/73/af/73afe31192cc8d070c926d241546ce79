<script type="text/javascript">
function showUP(success){
  if (!success) return;
  var custom       = false;
  var origin       = garminHost.env.device.location;
  var surveycookie = 'user-survey.2'; /* user panel (used to recruit users for testing) should always use 'user-survey.2' */
  var hidden       = garminGlobalStore.getGlobal(surveycookie);
  var internal     = origin.internal;
  var startDate    = new Date('2015-01-12T00:00:00');
  var endDate      = new Date('2015-01-28T00:00:00');
  var today        = new Date();
  var activeDate   = today >= startDate && today < endDate;
  var usa          = origin.country_code == 'US';
  var local        = (origin.region_code == 'KS'  || origin.region_code == 'MO') &&
                     (origin.areacode    == '913' || origin.areacode    == '816');
  var hashVal      = document.location.hash;
  var gexp         = hashVal && hashVal == '#up';
  var myloc        = window.location.href;
  var locale       = garminGlobalStore.getGlobal('locale');
  if (typeof(locale) == 'undefined' || locale == false) {
    var language = garminHost.env.user.language ? garminHost.env.user.language + '-' : '';
    locale       = language + origin.country_code;
  }
  var customVarsArray = [
    'custom1=user-panel-survey',
    'custom2=' + encodeURIComponent(garminHost.env.browser.name),
    'custom3=' + encodeURIComponent(garminHost.env.browser.version),
    'custom4=' + encodeURIComponent(garminHost.env.os.name),
    'custom5=' + encodeURIComponent(garminHost.env.os.version),
    'custom6=' + encodeURIComponent(window.location.href),
    'custom7=' + encodeURIComponent(locale),
    'custom8=' + garminHost.isMobile ? 'isMobile' : ''
  ];
  var myLink = jQuery('.feedback-container').find('.survey-link');
  myLink.attr('href',myLink.attr('href') + '?' + customVarsArray.join('&'));
  try {
    custom = true;
  } catch (e){}
  if (custom && local && !hidden && !internal && usa && activeDate){
    jQuery('body').addClass('show-up');
  }
}
jQuery(document).ready(function(){
  var hideUP = function(e){
    garminGlobalStore.setGlobal(surveycookie, 1);
    jQuery('body').removeClass('show-up');
  };
  jQuery('.feedback-container .close, .feedback-container a').click(hideUP);
});
window.garminHost.getRequestInfo(showUP);
</script>
