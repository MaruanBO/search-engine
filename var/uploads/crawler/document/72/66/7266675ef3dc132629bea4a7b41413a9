        <script type="text/javascript" src="mysite/javascript/jquery/jquery-1.3.2.min.js"></script>

        
        <script type="text/javascript" src="mysite/javascript/jquery/plugins/cookie/jquery.cookie.js"></script>

        <script language="javascript" type="text/javascript">
    
    var themeDir = "themes/lto";
    var countryCode = "US";
    var isOpenExternalLinksInNewWindow = "1";

    
    
    

    var isContactUsForm = 0;
    var isCommentsForm = 0;
    var isHelpfulForm = 0;
    var isAskUsForm = 0;
    var isAnalytePageWithSubarticle = 0;
    var isSubarticlePage = 0;

    var stakeholdersServerURL = "//stakeholders.labtestsonline.org";
    var isEnableSponsoredPages = 1;
    var isAdmin = 0;

    
    var isShowAds = getIsShowAds();


    /**
     * Determine whether or not to show ads by comparing referrer domain against
     * list of ad-free domains.
     *
     * NOTE: this function only compares domains!  Even if the function determines
     *       that ads should be displayed, there still could be no ads for the page
     *       if its keywords and title doesn't match up with the ad keywords; the
     *       latter logic is processed in Page_Controller::AdScript().
     *
     *       So if this function determines that ads should be displayed based on domains, but
     *       Page_Controller::AdScript() returns null, then no ads are displayed since the template
     *       variable  (see TopAd.ss and SideAd.ss) is null.
     *
     *       In summary, two factors determine whether ads are displayed:
     *       1. referrer domain compared to ad-free domain list (getIsShowAds())
     *       2. page keywords and title compared to Ad keywords (Page_Controller::AdScript(), TopAd.ss, SideAd.ss)
     *
     */
    function getIsShowAds() {

        /* initialize the return value */
        var isDisplayAds = true;

        var queryString = location.search;
        /* don't show ads if in print preview mode */
        if (queryString && queryString.match(/printpreview=1/i)) {
            isDisplayAds = false;
            return isDisplayAds;
        }

        /* get this website's domain (removing any possible port number */
        var thisSiteDomain = (location.host).replace(/^(.*):.*$/, "$1");

        /* get the referring URL, may or may not be available depending of response header */
        var referrerUrl = document.referrer;

        /* Admins only: for testing, can set 'referrer' query string in URL */
        if (isAdmin) {
            var queryString = location.search; // eg, ?a=b&c=d
            if (queryString) {
                queryString = queryString.replace('?','');
                var matches = queryString.match(/referrer=[^&]+/i);
                if (matches) {
                    referrerUrl = matches[0].replace('referrer=', '');
                }
            }
        }

        /* extract the domain from the referrer URL */
        var referrerDomain = referrerUrl.replace(/^https*:\/\/([^\/]+)\/.*$/i, "$1");

        var externalReferrerDomain = jQuery.cookie('ExternalReferrerDomain');

        /* if Ad cookie exists and is set to 'no', then don't show any ads for the rest of the session
         * unless:
         *     1. the referrer domain has changed and
         *     2. the referrer domain is not the current site.
         *
         * Use case: visitor comes to LTO from an ad-free domain, visitor should see no ads and ad cookie is set to 'no';
         *           if visitor then searches for a term from a search engine and clicks on a link to the LTO website,
         *           then ads should be displayed again.  (Session cookie is valid a acroos all browser tabs and is
         *           only auto cleared when entire browser is shut down but not when a tab is closed or re-used
         */
        var isCookieShowAds = jQuery.cookie('IsCookieShowAds');
        if (isCookieShowAds && isCookieShowAds == 'no') {
            if (externalReferrerDomain && (externalReferrerDomain != referrerDomain) && (referrerDomain != thisSiteDomain)) {
                //alert(['Ad cookie is NO and referrer domain is DIFFERENT than previous external domain'].join(' | '));

                /* on a different domain, so set a new external domain and delete Ad cookies;
                   the new external domain will undergo domain analysis below */
                jQuery.cookie('ExternalReferrerDomain', referrerDomain, {path: '/'});
                jQuery.cookie('IsCookieShowAds', null, {path: '/'});
            }
            else {
                //alert(['Ad cookie is NO and referrer domain is SAME as previous domain.  Exiting.'].join(' | '));
                /* haven't left the LTO site since coming from the ad-free domain, so continue to not display ads;
                   no domain analysis needed */
                isDisplayAds = false;
                return isDisplayAds;
            }
        }

        /* at this stage, go through the domain analysis and determine if Ads should be displayed on this page based on referrer domain;
           also set a session cookie if the referrer domain is in the ad-free list AND the ad cookie does not yet exist */

        /* JS array of ad-free domains and exclusion subdomains (generated by SS) */
        var adFreeDomains = [/nih\.gov/i];
        var adFreeExclusionSubDomains = "";

        var numAdFreeDomains = adFreeDomains.length;
        var numAdFreeExclusionSubDomains = adFreeExclusionSubDomains.length

        if (referrerDomain && ((adFreeDomains instanceof Array) && numAdFreeDomains)) {

            //alert(['referrer Url, referrer Domain, Ad Free Domains, Ad Free Exclusion Subdomains',referrerUrl, referrerDomain, adFreeDomains, adFreeExclusionSubDomains].join('|'));

            /* check if referrer domain is in the ad-free domains array */
            var isExclusionSubDomainMatch = false;
            for (var i = 0; i < numAdFreeDomains; i++) {
                //alert(['referrerDomain, adFreeDomains[i], match', referrerDomain, adFreeDomains[i], referrerDomain.match(adFreeDomains[i])].join(' | '));
                if (referrerDomain.match(adFreeDomains[i])) {
                    isDisplayAds = false;
                    if (adFreeExclusionSubDomains instanceof Array && numAdFreeExclusionSubDomains) {
                        isExclusionSubDomainMatch = false;
                        for (var j = 0; j < numAdFreeExclusionSubDomains; j++) {
                            //alert(['referrerDomain, adFreeExclusionSubDomains[j], match', referrerDomain, adFreeExclusionSubDomains[j], referrerDomain.match(adFreeExclusionSubDomains[j])].join(' | '));
                            if (referrerDomain.match(adFreeExclusionSubDomains[j])) {
                                isDisplayAds = true;
                                break;
                            }
                        }
                        break;
                    }
                }
            }
            /* if Ad cookie doesn't exist and Ads should not be displayed, then set
             * a session cookie to NOT show ads for the browsing session; also set
             * the external referral domain */
            if (!isCookieShowAds && !isDisplayAds) {
                /* this is a session cookie since there is no 'expires' property set */
                jQuery.cookie('IsCookieShowAds', 'no', {path: '/'});
                jQuery.cookie('ExternalReferrerDomain', referrerDomain, {path: '/'});
                //alert(['Ad cookie does not exist and not displaying ads  based on referrer url; setting no ad display cookie'].join(' | '));
            }
        }
        else {
            if (typeof console == "object") { console.log('WARNING: browser did not report any referrer URL or adFreeDomains is not valid' + [referrerUrl, adFreeDomains].join(' | ')); }
        }

        //alert(['isDisplayAds based on comparing domains only', isDisplayAds].join(' | '));
        return isDisplayAds;
    }

</script>
        
