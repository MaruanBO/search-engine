		// Resize background photo area
		setPhotoHeight();
		$(window).resize(function() {
  			setPhotoHeight();
		});

		// Set up xray effect
		var $xray = $('.xray');
		var $xrayMesh = $('.xray-mesh');
		var windowWidth = $(window).width();
		$xray.mousemove(function(e){
			newWidth = $xray.width() - e.pageX + $xray.offset().left;
			$xrayMesh.css('width', newWidth);
		}); 

		// Set up video play
		$("#screenshot").click(function(){
			$(this).hide();
			$('.video-wrapper').show();
			$f('video').api("play");
		});
	})

	function setPhotoHeight() {
		var bigPhoto = $('.big-photo');
		bigPhoto.css('height', bigPhoto.width()*0.5);

		// Set margin and height of xray effect handle
		$('.xray-mesh img').css({'margin-top': bigPhoto.height()*0.184, 'height': bigPhoto.height()*0.766});
	}
</script>

    <script src="owl.carousel/assets/js/jquery-1.9.1.min.js"></script> 
    <script src="owl.carousel/owl-carousel/owl.carousel.js"></script>
	<script src="js/jquery.prettyPhoto.js" type="text/javascript" charset="utf-8"></script>

	<script type="text/javascript" charset="utf-8">
  $(document).ready(function(){
    $("a[rel^='prettyPhoto']").prettyPhoto();
  });
</script>



