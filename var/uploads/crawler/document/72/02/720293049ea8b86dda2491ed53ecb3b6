$(function(){
	var ftcol = $('.footer_links').find('.linkblock').size()
	if(ftcol == 3)
	{
		$('.footer_links').find('.linkblock').css("width","31.5%");
	}
	else if(ftcol == 2)
	{
		$('.footer_links').find('.linkblock').css("width","48%");
	}
});
</script>

	




		







	
		
		
