<script type="text/javascript">
	function trackEventInTealium(payload){
		//TODO: Analyze if we need to check for badly formatted payload
		if (payload){
			for (var key in payload){
				//TODO: Analyze if we need to check for null values in data
				if (key && payload[key]){
					utag_link[key] = payload[key];
				}
			}			
		}
		utag.link(utag_link);
	}
	/*
	 * Creates a Conversion Event tag
	 *
	 * eventID			: required. Conversion event ID
	 * actionType		: required. 1=conversion initiation, 2=conversion completion
	 * categoryID		: optional. Category for the event
	 * points			: optional. Point value to assign to conversion.
	 */
	 function tlCreateConversionEventTag(eventID, actionType, categoryID, points, attributes) {
		 trackEventInTealium({"event_id" : eventID
 							 ,"event_action_type" : actionType
							 ,"event_type" : eventID
							 ,"event_category" : categoryID
							 ,"event_points" : points
							 ,"attributes": attributes
							 });
	 }
	 //Enhancement 8691 - Update Email Footer Sign-ups Coremetrics implementation- Samrat
	function invokeCoremetricsForNewsletterSignup(emailid){
		trackEventInTealium({"event_id" : "newsletter sign-up"
							,"event_action_type" : "2"
							,"event_type" : "newsletter signup"
							,"event_category" : "registration"
							,"event_points" : "5"
							,"customer_email":emailid
							});
	}
	function t1NewsletterSignupOrder(emailid){
		trackEventInTealium({"event_id" : "newsletter sign-up checkout"
							,"event_action_type" : "2"
							,"event_type" : "newsletter signup"
							,"event_category" : "registration"
							,"event_points" : "5"
							,"customer_email":emailid
							});
	}	 
	function invokeCoremetricsForNewsletterSignup(emailid){
		trackEventInTealium({"event_id" : "newsletter sign-up"
							,"event_action_type" : "2"
							,"event_type" : "newsletter signup"
							,"event_category" : "registration"
							,"event_points" : "5"
							,"customer_email":emailid
							});
	}	 
	function invokeCoremetricsForNewsletterSignupfooter(emailid){
		trackEventInTealium({"event_id" : "newsletter sign-up footer"
							,"event_action_type" : "2"
							,"event_type" : "newsletter signup"
							,"event_category" : "registration"
							,"event_points" : "5"
							,"customer_email":emailid
							});
	} 
	function invokeCoremetrics_directionStoreLocator(){
		trackEventInTealium({"event_type" : "get_directions"});
	}
	function invokeCoremetrics_GSkShopAction(pid1, pid2, pid3, pid4){
		var products = [];
		
		if (pid1){
			products[products.length] = pid1;
		}
		if (pid2){
			products[products.length] = pid2;
		}
		if (pid3){
			products[products.length] = pid3;
		}
		if (pid4){
			products[products.length] = pid4;
		}
		trackEventInTealium({ "event_type" : "add_to_bag_gsk"
							, "product_id" : products
							});
	}
	function invokeCoremetrics_AddToBag(pid,cgid){
		var payload = {"product_id" : [pid]};		
		if (cgid && cgid != "null"){
			payload.product_category = [cgid];
		}
		if ("HOME_PAGE" == "ORDER_HISTORY_PAGE"){
			payload.event_type = "add_to_bag_history";
		}
		trackEventInTealium(payload);
	}
	function invokeCoremetrics_ProductChanged_tealium(productID, productName, productCategory, productBrand){
		var payload = {"event_type" : "prodview"};
		if(productName){
			payload.product_name = [productName];  
		} 
		if (productCategory){
			payload.product_category = [productCategory];
		}
		if (productBrand){
			payload.product_brand = [productBrand];
		}
		if(productID){
			payload.product_id = [productID];
			trackEventInTealium(payload);
		}										
	}
	function invokeCoreMetrics_EasyCheckout(){
		//tlCreateConversionEventTag ('Easy Checkout on Shopping Bag','1','Manage Account','10')
		trackEventInTealium({"event_type" : "easy_checkout"});
	}
</script>







