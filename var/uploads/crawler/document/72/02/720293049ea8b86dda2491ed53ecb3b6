<script>
$('ul.shop_column a').each(function() {
	var url = $(this).attr('href');
	var decodedUrl = url.replace(/&amp;/g, '&');
	 $(this).attr('href',decodedUrl);
	 if( !$.trim($(this).text()).length ) 
	 {
		 $(this).parent().hide();
	 }
	
});
</script>
<script>
$(function() {
	$('#nav.newnav li.shopMenu').hover(function() {
	var newcolumncount = $(this).find('li.newcolumn').size();
	if (newcolumncount == 3) {
		$(this).find('li.newcolumn').eq(1).addClass('border');
	}
	else if (newcolumncount == 2) {
		$(this).find('li.newcolumn').addClass('columntwo');
		$(this).find('li.newcolumn').eq(0).addClass('borderleft');
		$(this).find('li.colmenu').addClass('twocolumn');
	}
	else if (newcolumncount == 1 || newcolumncount == 0) {
		$(this).find('li.colmenu').addClass('onecolumn');
	}
	var adslotscount = $(this).find('div.adslots').children().length;
	if (adslotscount == 0)
	{
		$(this).find('div.adslots').css('display','none');
	}	
	var minHeight = 0;
	$(this).find('li.newcolumn').each(function() {
		if (minHeight < $(this).height()) {
			minHeight = $(this).height();
		}
		if($(this).siblings().eq(1).hasClass("border")) {
			$(this).siblings().eq(1).css('min-height', minHeight);
		}
		else if($(this).siblings().eq(0).hasClass("borderleft")) {
			$(this).siblings().eq(0).css('min-height', minHeight);
		} 
		});
	}, function() {
	});
}); 
</script>
<script>
$(function() {
	$('ul.shop_column').find('li.appendbelowexistingcolumn').each(function() {
		if($(this).prev().hasClass('newcolumn'))
		{
			$(this).prev().append(this);
		}
	});

});
</script>


					
		
		
		




