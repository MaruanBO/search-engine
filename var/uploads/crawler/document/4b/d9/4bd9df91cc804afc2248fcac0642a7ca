<script>
function setupRotator(){
	var textRotation = 0;
	var rotationInterval = 5000;

	jQuery(".header-banner-2>*:first").addClass('previous');
	jQuery('.header-banner-2 .previous').animate({"top": "+=54px"},500);
	jQuery('.header-banner-2 .previous').next().addClass('current');
	textRotation = setInterval('textRotate()', rotationInterval);
	jQuery(".header-banner-2").hover(
		function(){
			clearInterval(textRotation);
		}, 
 		function(){
 			textRotation = setInterval('textRotate()', rotationInterval);
 		}
 	);
}
function textRotate(){
	var rotationSpeed = 500;
	var numOfItems = jQuery(".header-banner-2>*").length;
	var currentItem = jQuery(".header-banner-2 .current");
	
	jQuery('.header-banner-2 .previous').animate({"top": "+=54px"},rotationSpeed).delay(rotationSpeed).animate({"top": "0"},0);
	jQuery('.header-banner-2 .previous').removeClass("previous");
	jQuery('.header-banner-2 .current').animate({"top": "+=54px"},rotationSpeed);
	if(currentItem.index() + 1 < numOfItems) {
		currentItem.removeClass("current");
		currentItem.addClass("previous");
		currentItem.next().addClass("current");
	} else {
		jQuery(".header-banner-2>*").removeClass("current");
		currentItem.addClass("previous");
		jQuery(".header-banner-2>*:first").addClass('current');
	}
}
jQuery(document).ready(setupRotator);
</script>


