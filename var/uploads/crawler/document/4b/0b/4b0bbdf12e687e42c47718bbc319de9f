<script type="text/javascript">
	function showSimpleMessageDialog(title, message, reloadPage, callback)
	{
		$("#simpleMessageDialog  #simpleMessageDialogTitle")[0].innerHTML = title;
		$("#simpleMessageDialog  #simpleMessageDialogMessage")[0].innerHTML=message;
		$("#simpleMessageDialog  #simpleMessageDialogAction").unbind("click");
		$("#simpleMessageDialog  #simpleMessageDialogClose").unbind("click");
		if(callback)
		{
			var callbackFunc = new Function(callback);
			$("#simpleMessageDialog  #simpleMessageDialogAction").bind("click", function(){callbackFunc()});	
			$("#simpleMessageDialog  #simpleMessageDialogClose").bind("click", function(){callbackFunc()});	
		}
		else if(reloadPage)
		{
			$("#simpleMessageDialog  #simpleMessageDialogAction").bind("click", function(){location.reload(true)});	
			$("#simpleMessageDialog  #simpleMessageDialogClose").bind("click", function(){location.reload(true)});	
		}
		else
		{
			$("#simpleMessageDialog  #simpleMessageDialogAction").bind("click", function(){hidePrompt()});	
			$("#simpleMessageDialog  #simpleMessageDialogClose").bind("click", function(){hidePrompt()});	
		}
		
		lightBoxSpawn('simpleMessageDialog', false);
	}
</script><script type="text/javascript">

$(document).ready(function() {
	
	if('' == 'true')
	{
		showSimpleMessageDialog('','',false, false);
	}

});
</script>









