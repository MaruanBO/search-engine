			function setIsPWS() {
				var pageUrl = window.location.pathname;
				if(pageUrl != null && pageUrl != '' && pageUrl.indexOf('/pws/') > -1) {
					$.ajax({
						url: '/pws/setPWSToSession',
						type: 'GET',
						data: {browserUrl: pageUrl},
						dataType: "json",
						success: function(data) {},
						error: function(data) {}
					});
				}
	 		}

