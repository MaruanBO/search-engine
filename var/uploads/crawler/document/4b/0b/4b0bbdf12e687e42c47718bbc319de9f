<script type="text/javascript">
	function showConfirmDialog(title, message, callback)
	{
		$("#confirmationDialog  #confirmationDialogTitle")[0].innerHTML = title;
		$("#confirmationDialog  #confirmationDialogMessage")[0].innerHTML=message;
		var callbackFunc = new Function(callback);
		$("#confirmationDialog  #confirmDialogAction").unbind("click");	
		$("#confirmationDialog  #confirmDialogAction").bind("click", function(){callbackFunc()});	
		lightBoxSpawn('confirmationDialog', false);
	}
	
	function showConfirmDialogWithLabels(title, message, noLabel, yesLabel,  callback)
	{
		$("#confirmationDialog  #confirmationDialogTitle")[0].innerHTML = title;
		$("#confirmationDialog  #confirmationDialogMessage")[0].innerHTML=message;
		$("#confirmationDialog  #noButtonLabel").html(noLabel);
		$("#confirmationDialog  #yesButtonLabel").html(yesLabel);
		var callbackFunc = new Function(callback);
		$("#confirmationDialog  #confirmDialogAction").unbind("click");	
		$("#confirmationDialog  #confirmDialogAction").bind("click", function(){callbackFunc()});	
		lightBoxSpawn('confirmationDialog', false);
	}	
	
	function showConfirmDialogWithBothCallbacks(title, message, callback, noCallback)
	{
		$("#confirmationDialog  #confirmationDialogTitle")[0].innerHTML = title;
		$("#confirmationDialog  #confirmationDialogMessage")[0].innerHTML=message;
		var callbackFunc = new Function(callback);
		$("#confirmationDialog  #confirmDialogAction").unbind("click");	
		$("#confirmationDialog  #confirmDialogAction").bind("click", function(){callbackFunc()});	
		
		var noCallbackFunc = new Function(noCallback);
		$("#confirmationDialog  #confirmationModalTagNoButton").unbind("click");	
		$("#confirmationDialog  #confirmationModalTagNoButton").bind("click", function(){noCallbackFunc()});
		
		lightBoxSpawn('confirmationDialog', false);
	}	
	

</script> <script type="text/javascript" src="/js/global-header.js?1423864828000"></script><script type="text/javascript">

	function goToLink(url)
	{
		if(url == '/')
			showConfirmDialog("You’re about to leave this Personal Web Site", "By clicking &#34;OK&#34;, you will be entering The Pampered Chef&#39;s public site. Would you like to continue?", "window.location.href='"+url+"'");
		else
			window.location.href = url;
	}
	
</script>

