	<script type="text/javascript">
	$(document)
			.ready(
					function() {
						$('input[name=searchCategoryType]')
								.val(
										'Product');
						$('input[name=searchSecondaryCategoryType]')
								.val(
										'Recipe');

						$('input[name=searchItemType]').val(
								'TPCProduct');

						if ($(location).attr('href').indexOf(
								"/recipe-landing-page") !== -1) {
							$('input[name=searchCategoryType]')
									.val(
											'Recipe');
							$('input[name=searchSecondaryCategoryType]')
									.val(
											'Product');
							$('input[name=searchItemType]').val('Recipe');
						}

						if ($(location).attr('href').indexOf("/cc") !== -1) {
							$('input[name=searchCategoryType]')
									.val(
											'Resource');
							$('input[name=searchSecondaryCategoryType]')
									.val(
											'Product');
							$('input[name=searchItemType]').val('Resource');
						}

					});

	var isSubmitted = false;
	function runGlobalSearch() {
		if(!isSubmitted) {
			isSubmitted = true;
			searchTextVal = $('#globalSearchText').val();
			globalHeaderSearchVal = $('#searchGlobalHeaderSearch').val();
			itemTypeVal = $('#searchItemType').val();
			categoryTypeVal = $('#searchCategoryType').val();
			secondaryCategoryTypeVal = $('#searchSecondaryCategoryType').val();
			userSelectionVal = $('#searchUserSelection').val();
			qString = "?searchText=" + searchTextVal + "&GlobalHeaderSearch=" + globalHeaderSearchVal + "&itemType=" + itemTypeVal + "&categoryType=" + categoryTypeVal + "&secondaryCategoryType=" + secondaryCategoryTypeVal + "&userSelection=" + userSelectionVal;
			document.getElementById("SearchForm").action = document.getElementById("SearchForm").action + qString;
			document.getElementById("SearchForm").submit();
		}
	}

</script>

