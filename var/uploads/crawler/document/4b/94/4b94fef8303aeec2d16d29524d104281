var clogd = false;

function consoleLog(msg) {
    if (typeof(console) != "undefined" && clogd) {
        console.log(debugGetTimeDelta() + " " + msg);
    }
}

/* Leader Board iFrame Area - Hides default doubleclick ad if no other ad loaded. */
function collapseAdDisplay(iFrameArea,leaderBoardArea) {
    $("#"+iFrameArea).hide();
    if (typeof(leaderBoardArea) != 'undefined' && (leaderBoardArea != '')){
        $("#"+leaderBoardArea).hide();
    }
    showHideAdChoice(iFrameArea, false);
}


function showHideAdChoice(adName, showAd){
    /*if showAd is true means , ad is loded and adchoice should be visible.
     * If ad is not loded, hide the iframe for ad
     * */
    if(showAd){
        $("#"+adName).parent().find(".adChoice").show();
    }else{
        $("#"+adName).hide();
    }
}

/**
 * avoids adding iframe location to browser history and breaking the back button.
 * @param iframeId
 * @param untrackedUrl
 */
function replaceIframeUrl(iframeId, untrackedUrl) {
//   console.info(iframeId + ' --- ' + untrackedUrl);
   var w = document.getElementById(iframeId)
   if(w){
     w.contentWindow.location.replace(untrackedUrl);
   }
}

triggerParms["cpp_5"] = "LoginStatus:";
triggerParms["cpp_6"] = "TrafficSource:";
triggerParms["cpp_7"] = "PageType:";
triggerParms["cpp_8"] = "CategoryName:";

function trimString(str) { return str.replace(/^\s+/, '').replace(/\s+$/, ''); }

function submitSearch(searchValue){
    var trimmedSearchValue = trimString(searchValue);
    if ((trimmedSearchValue  == ""))  {
        $("#search_text").val("");
        $("#search_text").focus();
        $("#search_text").blur();
        return false;
    }
    return true;
}

