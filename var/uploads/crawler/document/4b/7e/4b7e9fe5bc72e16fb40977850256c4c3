<script type="text/javascript">

function redirectToMobilePage(box,value)
{  
	var language = 'en';
	var country = 'US';
	    
    var user = $('#userid').val();
    var mobilePagePath = box.findParentByType('dialog').find('name','./MobilePageLink')[0].getValue(); 
    var heroSpace=new Object(); 

    heroSpace.heroTitle = getHeroSpaceValue(box,'./HeroTitleMobile');   
    heroSpace.heroSubTitle = getHeroSpaceValue(box,'./HeroSubTitleMobile');
    heroSpace.ctaLinklabel = getHeroSpaceValue(box,'./CTALinklabelMobile');
    heroSpace.heroSpaceCTALink = getHeroSpaceValue(box,'./HeroSapceCTALinkMobile');
    heroSpace.heroSpaceDesc = getHeroSpaceValue(box,'./heroSpaceDescriptionMobile');
    heroSpace.relatedBackgroundCanvas = getHeroSpaceValue(box,'./relatedBackgroundCanvasMobile');   
    var requestHeader =  "{\"heroTitle\""+":"+heroSpace.heroTitle+","+"\"heroSubTitle\""+":"+heroSpace.heroSubTitle+","+
    "\"ctaLinklabel\""+":"+heroSpace.ctaLinklabel+","+"\"heroSpaceCTALink\""+":"+heroSpace.heroSpaceCTALink+","+
    "\"heroSpaceDesc\""+":"+heroSpace.heroSpaceDesc+","+"\"relatedBackgroundCanvas\""+":"+heroSpace.relatedBackgroundCanvas+"}";    
    var serviceurl = "/bin/HomepageCarouselServlet?user="+user+"&action=preview&language="+language+"&country="+country;
    $.ajax( {
        url : serviceurl,
        type : 'post',
        data : {
            "heroSpaceData" : requestHeader
        },       
        success: function(data){},
        complete: function(data) {
            var MobilePageLink=mobilePagePath+'.html';
            window.open(MobilePageLink,"_newtab");
            }
    });

    
}
function restoreMobileNode(){ 
	var language = 'en';
	var country = 'US';
    var pageName = 'home';
    var serviceurl = "/bin/HomepageCarouselServlet?action=restore&currentPage="+pageName+"&language="+language+"&country="+country;
    $.ajax({ url: serviceurl,
          type: 'post',           
          error: function(request,error) {},
          success: function(msg){},
          complete: function(data) {
                 
              }});  
}
function getHeroSpaceValue(box,name){
    var fieldValues = box.findParentByType('dialog').find('name',name); 
    var value="[";
    for(i=0;i<fieldValues.length;i++)
    {
        var temp = "\""+fieldValues[i].getValue()+"\"";         
        value+=temp+",";
    }
    if(value.length > 1) {
        return value.substring(0,value.length-1)+"]";
    } else {
    	return '[]';
    }
}
</script>
    
