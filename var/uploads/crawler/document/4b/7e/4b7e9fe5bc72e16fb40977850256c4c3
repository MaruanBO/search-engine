<script type="text/javascript">

function submitTopSearch()
{

    var topSearchQ = $("#input-search-top").val();
    //topSearchQ = topSearchQ.trim();
    topSearchQ = $.trim(topSearchQ);
    // Replace the Abnormal Single-Quote with the Normal Single-Quote
    // Replace the Trade-mark symbol with an empty space
    topSearchQ = escape(topSearchQ);
    topSearchQ = topSearchQ.replace(/%u2019/g, "%27");
    topSearchQ = topSearchQ.replace(/%u2122/g, "");
    topSearchQ = unescape(topSearchQ);
    var topSearchCollection;
    var searchResultsUrl = '/content/NorthAmerica/kelloggus/en_US/home/search-results';
    
    if(topSearchQ == 'Search' || topSearchQ == "")
    {   
        return;
    }
    
    //alert($("#input-search-top").val());
    var arrRadio = document.getElementsByName("searchwithin");

    for(i=0; i<arrRadio.length; i++)
    {
        //alert(arrRadio[i].checked);
        if(arrRadio[i].checked)
        {
            topSearchCollection = arrRadio[i].value;
        }
    }
    //alert(topSearchCollection);
    //alert(searchResultsUrl);

    if(topSearchQ != null && topSearchQ != undefined && topSearchQ != "" && topSearchCollection != null && topSearchCollection != undefined && topSearchCollection != "")
    {
     
        searchResultsUrl = searchResultsUrl +".q:"+encodeURIComponent(topSearchQ)+".site:"+topSearchCollection+".html";
        //searchResultsUrl = searchResultsUrl +".html/q="+topSearchQ+"/site="+topSearchCollection;

        //alert(searchResultsUrl);
        //window.location.href = searchResultsUrl;
        var recipecatSplit = searchResultsUrl.split("/");
       
       //alert(recipecatSplit);
       
        var catePageName = recipecatSplit[recipecatSplit.length - 1];
       
             
            pMethod(catePageName,'topSearchRedirect');
    }
    
}
</script></div>
 
                    
            
