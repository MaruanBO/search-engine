	     var currentPage = '/content/NorthAmerica/kelloggus/en_US/home';
	     
	    function newLocation(path){
			var search = document.getElementById("search-input-mm").value;
		     var redirect = path + ".reqmeta*@-.ii1*@-" + escape(convertToEntities(search)) +".ii2*@-.ei*@-.html";
		     var recipecatSplit = redirect.split("/");
		     var catePageName = recipecatSplit[recipecatSplit.length - 1];
			 catePageName = catePageName .replace(/%23/g,'123hash123').replace(/%3B/g,'123semi123');
	     
	     	pMethod(catePageName,'recipeCatRedirectMega');
		}
	    
		function convertToEntities(search) {
		    var inputString = search;
		    var queryString  = '';
		    for (var i = 0; i < inputString.length; i++) {
		        if (inputString.charCodeAt(i) > 127) {
		            queryString += '&#' + inputString.charCodeAt(i) + ';';          
		        } else {
		            queryString += inputString.charAt(i);
		        }
		    }
		    return queryString;
		}

