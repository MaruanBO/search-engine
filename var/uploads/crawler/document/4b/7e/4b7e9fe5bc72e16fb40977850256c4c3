  $(document).ready(function(){

        /***Ipad orientation changes for openModal2***/
        /***Live orientation starts**/ 
                var orientation=0;     
                window.onorientationchange = function(){
                orientation = window.orientation;
                // Look at the value of window.orientation:
                    if (orientation === 0||orientation === 180){
                                //If In portrait mode call function                    
                                changeCordPortrait();
                    }   
                    else
                    {
                                //If in Landscape mode change co-ordinates of maps to default
                                changeCordLandscape(); 
                    }
                }
        /***Live orientation ends***/
       
