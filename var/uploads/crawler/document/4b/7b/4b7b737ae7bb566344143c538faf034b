	// script for the home page that pauses videos when clicking on the accordian items
	// so that you don't have multiple videos playing at the same time.
	
	$j("h4.pgf_title a").click(function() {
						
		$j('iframe[src*="http://www.youtube.com/embed/"]').each(function(i) {
				var func = 'pauseVideo';
				this.contentWindow.postMessage('{"event":"command","func":"' + func + '","args":""}', '*');
		});		
	
	});
</script>		
				
