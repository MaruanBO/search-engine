	jQuery(function($) {
		var page = 1, l = function() {
			page++;
			var a = $(this),
				url = 'http://whatculture.com/wp-admin/admin-ajax.php',
				data = {
					'action': 'load_latest_posts',
					'offset': page,
					'category': 'Film',
					'tag': ''
				};
			$.post(url, data, function(response) { $(response).insertBefore(a); });
			return false;
		};
		$('.load-latest-news').on('click', l);
	});
</script>
