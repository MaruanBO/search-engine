	bam.loadSync(bam.homePath + "bam.cookies.js");
	var fprt = bam.cookies.get('fprt');
	var accountLinks;
	if (fprt!=null && fprt!="") {
		accountLinks = 	['<span class="register-link">',
					'<a target="_top" href="',
					sslServerURLGlobal,
					'/enterworkflow.do?flowId=registration.profile"',
					' title="MY ACCOUNT">Account</a></span>',
					' <span class="login-link">',
					'<a target="_top" href="',
					sslServerURLGlobal,
					'/enterworkflow.do?flowId=registration.logout&sid=milb"',
					' title="LOGOUT">Logout</a></span>'
					].join("")
	}else{
		accountLinks = 	['<span class="register-link">',
					'<a target="_top" href="',
					sslServerURLGlobal,
					'/enterworkflow.do?flowId=registration.profile"',
					' title="REGISTER">Register</a></span>',
