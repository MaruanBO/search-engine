<script type="text/javascript">
function selectedPollClass(){
	var selectedPollText = $('input.pollKeyword:checked').parent().text();
	$('.resultLineItem').removeClass('selectedPollItem');
	$('.pollResultText').each(function(){
		if ( selectedPollText == $(this).text() ) { $(this).parent().addClass('selectedPollItem'); }
	});	
};
</script>
		

		
