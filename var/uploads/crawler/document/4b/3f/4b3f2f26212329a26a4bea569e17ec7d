bam.loadSync("/shared/scripts/bam/bam.poll.js");
$(function() {
	var poll_yBjPk8nwnmnEIlP2KV86UKLjDco0 = new bam.poll.Poll({	
		key:				"yBjPk8nwnmnEIlP2KV86UKLjDco0", // campaign id
		showResults:		"yes", // yes | no
		customSuccess:		"", // URI	
		submitImg:			"/y2013/images/global/vote_now.gif",
		buttonText:			"Vote >",
		sortOrder:			"ascending", // ascending | descending
		sortType:			"none", // text | number | last-name
		sortResultsByCount:	"yes", // yes | no
		resultType:			"percent", // percent | count
		showTotalVotes:		"no", // yes | no
		columns:			"1", // 1 | 2
		resultsText:		"Results delayed up to 5 minutes.", // default results text	
		callback:			null, // function 	
		resultsCallback:	selectedPollClass // function		
	});
	poll_yBjPk8nwnmnEIlP2KV86UKLjDco0.init();
});
</script>



	</div>
