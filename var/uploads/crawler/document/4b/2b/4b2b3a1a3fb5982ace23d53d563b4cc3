	<script type="text/javascript">

        //bit of code to allow the esc key to close overlays.
	    $(document).keyup(function(e) {
	        if (e.keyCode == 27) {
	            window.location.reload();
	            closeOverlay();
	            
	        }   
	    });

	    var VisitorAttributes = '';
	    var IsCurrentProDayLimit = '60';
	    var VisitorType_Pro = '';
	    var VisitorType_Contributor = '';
	    var VisitorType_Registered = '';
	    var RatedResource = '';
	    var Commented = '';

	    if (VisitorType_Registered == "" && VisitorType_Contributor == "" && VisitorType_Pro == "") {
	        if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	        VisitorAttributes = VisitorAttributes + "anon"
	    }

	    if (VisitorType_Registered != "") {
	        if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	        VisitorAttributes = VisitorAttributes + VisitorType_Registered;
	    }

	    if (VisitorType_Contributor != "") {
	        if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	        VisitorAttributes = VisitorAttributes + VisitorType_Contributor;
	    }

	    if (VisitorType_Pro != "") {
	        var LastProLoginDate = new Date('');
	        var TodaysDate = new Date;
	        var daysSinceLastLogin = parseInt((TodaysDate - LastProLoginDate) / (24 * 3600 * 1000));
	        if (daysSinceLastLogin < IsCurrentProDayLimit) {
	            if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	            VisitorAttributes = VisitorAttributes + VisitorType_Pro;
	        }
	        else {
	            if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	            VisitorAttributes = VisitorAttributes + "inap";
	        }
	    }

	    if (RatedResource != "") {
	        if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	        VisitorAttributes = VisitorAttributes + "rate";
	    }

	    if (Commented != "") {
	        if (VisitorAttributes != "") { VisitorAttributes = VisitorAttributes + "," }
	        VisitorAttributes = VisitorAttributes + "com";
	    }
	    
        //track the IsLogged in custom variable.
	    var IsLoggedIn = "False";

	    var TrackViewEvent = "0";
	    //alert(TrackViewEvent);
	    //set the account, add variables and track pageview.
	    var _gaq = _gaq || [];
	    var pluginUrl = '//www.google-analytics.com/plugins/ga/inpage_linkid.js';
	    _gaq.push(['_require', 'inpage_linkid', pluginUrl]);
	    _gaq.push(['_setAccount', 'UA-293620-9']);
	    _gaq.push(['_setCustomVar', 4, 'IsLoggedIn', IsLoggedIn, 2]);
	    _gaq.push(['_setCustomVar', 1, 'VisAttrib', VisitorAttributes, 1]);

	    var EventResourcetitle = "";
	    EventResourcetitle = escape(EventResourcetitle);
	    if (TrackViewEvent==1) {
	        _gaq.push(['_trackEvent', '', 'view', EventResourcetitle, , 'true']);
	    }
	    _gaq.push(['_trackPageview']);
	    //_gaq.push(['_trackPageLoadTime']); //DEPRICATED
	    
	</script>
	
