<script type="text/javascript">
    function CSP_ButtonClick(section) { 
        if(section=="allsermonprep"){
            document.getElementById("CSP_Button_AllSermonPrep").className = "CSP-MediaSearchButton-Selected";
            document.getElementById("CSP_Button_AllMedia").className = "CSP-MediaSearchButton";
            document.getElementById("CSP-SermonPrepSearch").style.display = "";
            document.getElementById("CSP-MediaSearch").style.display = "none";
            document.getElementById("CSP_Sermon_Trending_Topics").style.display = "";
            document.getElementById("CSP_Media_Trending_Topics").style.display = "none";
        }

        if (section == "allmedia") {
            document.getElementById("CSP_Button_AllSermonPrep").className = "CSP-MediaSearchButton";
            document.getElementById("CSP_Button_AllMedia").className = "CSP-MediaSearchButton-Selected";
            document.getElementById("CSP-SermonPrepSearch").style.display = "none";
            document.getElementById("CSP-MediaSearch").style.display = "";
            document.getElementById("CSP_Sermon_Trending_Topics").style.display = "none";
            document.getElementById("CSP_Media_Trending_Topics").style.display = "";
        }
    }
</script>
