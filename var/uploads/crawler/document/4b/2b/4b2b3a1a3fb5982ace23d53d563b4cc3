            function DrawTextAd(RandomNumber) {
                var strAd = "";
                var ad;
                if (RandomNumber == 1) { ad = ad1 }
                if (RandomNumber == 2) { ad = ad2 }
                if (RandomNumber == 3) { ad = ad3 }
                if (RandomNumber == 4) { ad = ad4 }
                if (RandomNumber == 5) { ad = ad5 }
                if (RandomNumber == 6) { ad = ad6 }
                if (RandomNumber == 7) { ad = ad7 }
                if (RandomNumber == 8) { ad = ad8 }
                if (RandomNumber == 9) { ad = ad9 }
                if (RandomNumber == 10) { ad = ad10 }
                
                if (ad[0] != ""){
                    if (ad[2] == "yes") {
                        strAd = strAd + "<div><a target='_blank' class='external' style='font-weight:normal;' href=\"" + ad[0] + "\" onclick=\"_gaq.push(['_trackEvent', 'Textlink', 'Click', '" + ad[0] + "']);\">" + ad[1] + "</a></div>";
                    } else {
                        //_gaq.push(['_trackEvent', 'Textlink', 'Impression', ad[0], , true]);
                        strAd = strAd + "<div><a target='_blank' class='external' style='font-weight:normal;' href=\"/redirect.asp?url=" + ad[0] + "\" onclick=\"_gaq.push(['_trackEvent', 'Textlink', 'Click', '" + ad[0] + "']);\">" + ad[1] + "</a></div>";
                    }
                }
                return strAd; //document.write(strAd);
            }
