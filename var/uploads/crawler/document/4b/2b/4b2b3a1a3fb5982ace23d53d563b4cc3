		    <script type="text/javascript">
		        function Search() {
		            //if (document.getElementById("ROSSearchTextBox").value + "" != "" && document.getElementById("ROSSearchTextBox").value + "" != "Keyword, Scripture, etc.") {
		            if (document.getElementById("ROSSearchTextBox").value + "" != "Keyword, Scripture, etc.") {
		                var searchtype = document.getElementById("HeaderResourceDropdown").value;
		                switch(searchtype) {
		                    case "bundles":
		                        window.location = '/Preaching-Bundles/SearchResults.asp?sscat=bundles&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
		                        break; 
		                    case "sermons":
                                window.location = '/Sermons/SearchResults.asp?sscat=sermons&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "sermonseries":
                                window.location = '/Sermon-Series/SearchResults.asp?sscat=sermonseries&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "contributor":
                                window.location = '/Contributors/SearchResults.asp?sscat=contributors&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "illustrations":
                                window.location = '/Illustrations/SearchResults.asp?sscat=illustrations&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "articles":
                                window.location = '/Articles/SearchResults.asp?sscat=articles&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "bible":
                                window.location = '/bible.asp?sscat=bible&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&Keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "audiobook":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=13&sscat=audiobook&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "audiomusic":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=14&sscat=audiomusic&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "audiosermon":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=15&sscat=audiosermon&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "ebook":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=16&sscat=ebook&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "image":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=17&sscat=highresolutionimage&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "videocountdown":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=20&sscat=videocountdown&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "videoillustration":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=21&sscat=videoillustration&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "videomotionloop":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=22&sscat=videomotionloop&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "worshipvideo":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=23&sscat=worshipvideo&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "powerpointtemplate":
                                window.location = '/church-media-preaching-sermons/SearchResults.asp?multimediatypeid=24&sscat=powerpointtemplates&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            case "allchurchmedia":
                                window.location = '/church-media-preaching-sermons/SearchResults_AllMedia.asp?multimediatypeid=0&sscat=allmedia&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                                break;
                            default:
                                window.location = '/Search/SearchResults.asp?sscat=allresources&sitesearch=ros-' + document.getElementById('ROSSearchTextBox').value + '&rk=' + document.getElementById('ROSSearchTextBox').value + '&keyword=' + document.getElementById('ROSSearchTextBox').value;
                        }
		            }
		        }
		        function SearchFromEnterKey() {
		            if (event.keyCode == 13) {
		                Search();
		            }
		        }
		    </script>
		    
