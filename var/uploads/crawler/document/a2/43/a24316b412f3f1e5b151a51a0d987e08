<script type="text/javascript">
;(function($) {

    $.fn.extend({
        addPPSelector: function(options) {
            if (options && typeof (options) == 'object') {
                options = $.extend({}, $.addPPSelector.defaults, options);
            }
            this.each(function() {
                new $.addPPSelector(this, options);
            });
            
            return;
        }
    });
    $.addPPSelector = function(elem, options) {
            var ddV2 = $(elem);
            var ddV2txt = ddV2.find('.pp-box-dropdown-txt');

            ddV2.hover(function() {},
                function() {
                    $(this).removeClass('pp-box-dropdown-active');
                }
            );

            ddV2.click(function(){
                $(this).removeClass('pp-box-dropdown-active');
                $(this).toggleClass('pp-box-dropdown-active');
            });

            var mic = ddV2.find('.pp-itm-choise');
            mic.hover(function() {
                $(this).toggleClass('pp-itm-choise-hover');
            });
            
            mic.last().addClass('pp-dropdown-itm-last');
            
            mic.click(function(ee) {
                var $this = $(this);
                $this.siblings().removeClass('pp-itm-choise-selected');
                $this.addClass('pp-itm-choise-selected');
                
                ee.stopImmediatePropagation();
                ddV2.removeClass('pp-box-dropdown-active');
                
                if($this.text() != ddV2txt.text()){
                    ddV2txt.text($this.text());
                }
            });
        } 
        $.addPPSelector.defaults = {};
})(jQuery);

</script>

<script type="text/javascript">

    function updateFeatureContent(feature,animal){
        var kcontent = feature.replace(/ /g,'') + animal;
        var $feature = $("#" + kcontent);


        $('.tab-result').hide();
        $('.300x480Ad').hide();
        $feature.show();
        
        if($feature.is(':empty'))
        {
            $feature.html("<img src='i/progress.gif' alt='Please Wait..' style='position:absolute;top:50%;left:50%' />");
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: '/services/FeatureContentService.asmx/GetFeatureHtml',
                data: "{'featureid':'" + feature + "','animal':'" + animal + "'}",
                success:
                function(msg) {
                    $feature.removeClass('tab-empty');
                    $feature.html(msg.d);
                    if (($feature[0].id.indexOf("TopStories") >= 0)) {
                        $feature.find('#300X480Ad').before($('.300x480Ad').find('div')[0]);
                        $('.300x480Ad').show();
                    }
                },
                error:
                function(XMLHttpRequest, textStatus, errorThrown) {
                    //$feature.html('');
                }
            });
        }
    }
    
    $(document).ready(function() {
        $('.pp-box-dropdown-in').addPPSelector();
        $('.tab-box a').click(function(){
            if(!$(this).hasClass('selected')){
                $('.tab-box a').removeClass('selected');
                $(this).toggleClass('selected');
                updateFeatureContent($(this).text(),
                    $('.feature-content-5 .pp-box-dropdown-txt').text());
            }
        });
        
        $('.feature-content-5 .go').click(function(){
            updateFeatureContent($('.feature-content-5 .tab-box a.selected:first').text(),
                $('.feature-content-5 .pp-box-dropdown-txt').text());
        });
        
        updateFeatureContent($('.feature-content-5 .tab-box a.selected:first').text(),
                $('.feature-content-5 .pp-box-dropdown-txt').text());
                
    });
    
    function setTab(option){
        var mic = $('.pp-box-dropdown-in').find('.pp-itm-choise:contains("' + option + '")');
        mic.click();
        
        updateFeatureContent($('.feature-content-5 .tab-box a.selected:first').text(),
                $('.feature-content-5 .pp-box-dropdown-txt').text());

    }
</script>
	
