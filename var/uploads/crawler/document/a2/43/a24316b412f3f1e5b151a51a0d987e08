<script type="text/javascript">
    var myTimer = 0;
    var index = 0;
    var itemInfo = ["/v4/i/uploaded/dog_0037.jpg","Winter Care for the Outdoor Dog","/dogs/winter-care-for-the-outdoor-dog/page1.aspx","/v4/i/uploaded/cat_0009.jpg","The Perfect Cat Day: What’s an Ideal Cat Schedule?","/cats/ideal-cat-schedule/page1.aspx","/v4/i/uploaded/dog_0042.jpg","Training: Teaching your Dog to Stay","/dogs/training-teaching-your-dog-to-stay/page1.aspx"];
    
     // create object
     imageObj = new Image();
     // start preloading
     for(i=0; i<(itemInfo.length/3); i++) 
     {
          imageObj.src=itemInfo[i*3];
     }
    
    $('.todays-feature-pager .previous-icon').click(function(){
        showFAItem(-1);
    });
    
    $('.todays-feature-pager .next-icon').click(function(){
        showFAItem(-1);
    });

    function showFAItem(offset){
        index = index + offset;

        if (index < 0)
            index = (itemInfo.length/3) - 1;
        else if (index == (itemInfo.length/3))
            index = 0;
       
        $("#linkHeader").hide(1, function(){
            $("#linkHeader").attr("href", itemInfo[(index*3) + 2]);
            $("#imgHeader").attr("src", itemInfo[(index*3)]);
            $("#linkHeader").show();
            
            $("#feature-subtitle").text(itemInfo[(index*3) + 1].replace(/&quot;/g, '"'));
            $("#feature-subtitle").attr("href", itemInfo[(index*3) + 2]);
            $(".readMoreLnk").attr("href", itemInfo[(index*3) + 2]);            
         });
                        
        if(myTimer != 0)
            clearInterval(myTimer);
        
        myTimer = setInterval('showFAItem(1)', 8000);
    }         
</script>

