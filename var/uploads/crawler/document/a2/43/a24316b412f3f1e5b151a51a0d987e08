<script  type="text/javascript">
$(document).ready(function() {


    $('.main-search').keypress(function(event){ 
        var keycode = (event.keyCode ? event.keyCode : event.which);
        if(keycode == '13'){
           if(location.href.indexOf('aspx')>-1)
           {  
                __doPostBack('ctl00$ctl05$lkbSearch','lnkPPButton');
           }
           else
           {
                __doPostBack('ctl00$ctl04$lkbSearch','lnkPPButton');
           }
        }
	});


    var inSearch = $('.main-search');

    inSearch.addClass('google-watermark');
    inSearch.focus(function() {
        $(this).filter(function() {
            return $(this).val() == "";
    }).removeClass('google-watermark').val('');
    });
    
    inSearch.blur(function() {
        $(this).filter(function() {
            return $(this).val() == ''
        }).addClass('google-watermark').val('');
    });


function hideSumenu()
{
  pageTracker._trackEvent('main-menu-apr2014', 'Close Menu',$('#mainnav-contentbox h2').text());   
  $('#mainnav-contentbox').fadeOut();
}

function GetSubmenulHtml(submenuid,animal) {
    pageTracker._trackEvent('main-menu-mar2013', menutext + ' Views',menutext);
}

var currentMenu;
$("#close-menu").live("click", function() { 
    hideSumenu(); 
});   

$("#internallink").live("click", function() { 
    GetSubmenulHtml($("#internallink").attr('data-submenuid'),$("#internallink").attr('data-animal'));
});

var showSMenu = function(){

        var $this = $(this);
        $this.addClass('mnu-actSelected');
        $('.mnu-sbmnu').hide();
        
        $this.next().show();
        
        $this.parent().mouseleave(function(){
            $this.removeClass('mnu-actSelected');
            $this.next().hide();
        });
    
};


var hideSMenu = function(){
}

$('.mnu-act').hover( showSMenu, hideSMenu );



showFAItem(0);

});
</script>


        <script type="text/javascript">
            var _sf_async_config={uid:5361,domain:"petplace.com",useCanonical:true};
            (function(){
              function loadChartbeat() {
                window._sf_endpt=(new Date()).getTime();
                var e = document.createElement('script');
                e.setAttribute('language', 'javascript');
                e.setAttribute('type', 'text/javascript');
                e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
                document.body.appendChild(e);
              }
              var oldonload = window.onload;
              window.onload = (typeof window.onload != 'function') ?
                 loadChartbeat : function() { oldonload(); loadChartbeat(); };
            })();
            
            function validateEmail(email) {
                var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                return re.test(email);
            }
            $(document).ready(function() {
                // track my pet bar always because the my pet bar is showing always
                pageTracker._trackEvent('My Pet Sign-Up Bar', 'Shown', 'Sign-Up Button');
                pageTracker._trackEvent('My Pet Sign-Up Bar', 'Shown', 'Login Button');

                var mypetbutton = 'Login Button';

                $(".btn-signup").click(function() {
                    $("#mypetbarpopup").show();
                    mypetbutton = 'Sign-Up Button';
                    pageTracker._trackEvent('My Pet Sign-Up Bar', 'Clicked', 'Sign-Up Button');
                });
                $(".btn-login").click(function() {
                    $("#mypetbarpopup").show();
                    mypetbutton = 'Login Button';
                    pageTracker._trackEvent('My Pet Sign-Up Bar', 'Clicked', 'Login Button');
                });
                $("#close").click(function() {
                    $("#mypetbarpopup").hide();
                });

                $(".btn-notify-me").click(function() {
                    if (validateEmail($('.emailfield').val())) {
                        $(this).hide();
                        $('#mypetbarpopup label').hide();
                        $('.emailfield').hide();
                        $('#mpprogress').show();
                        pageTracker._trackEvent('My Pet Sign-Up Bar', 'Registered', mypetbutton);
                        $.ajax({
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            url: '/services/SubscriptionService.asmx/MyPetPreSubscription',
                            data: "{'email':'" + $('.emailfield').val() + "'}",
                            success:
                        function(msg) {
                            $('#mpprogress').hide();
                            $('#mypetbarpopup #thankyou').show();
                        },
                            error:
                        function(XMLHttpRequest, textStatus, errorThrown) {
                            //$feature.html('');
                        }
                        });
                    } else {
                        if ($('#mypetbarpopup label .mperror').length == 0)
                            $('#mypetbarpopup label').append('<span class="mperror">Invalid E-Mail.</span>');
                    }
                });
            });
        </script>

