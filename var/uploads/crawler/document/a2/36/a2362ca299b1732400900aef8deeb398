window.nova_init_hook_laifengnew = function(){
	$$("#qheader_nav .yk-nav-pills-sub li a").each(function(o){
		if(o.innerHTML=='直播' || o.innerHTML=='公益'){
			o.parentNode.innerHTML+='<div class="ico_new_top" title="新"></div>';
		}
	});
}

