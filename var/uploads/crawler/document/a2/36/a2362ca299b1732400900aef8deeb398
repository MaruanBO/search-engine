    <script>
        /**
         * @author zongkelong@youku.com
         * @time 2014-9-19
         * @des 游戏排行榜数据加载
         */
        var gameRanking = {
            // Dom
            currentRanking : null,
            lastRanking : null,
            latestInfo : null,
            TRYTIMES : 3,
            CURRENTRECORDSTATUS : 0,
            CURRENTREQUESTSTATUS : 0,
            //ranking data interface
            getRankingdataURL:'http://uo.youku.com/channelstore/rank/getRankData',

            init:function(){
                var me = this;
                me.currentRanking = $('subscribe');
                me.latestInfo = $('NB');
                me.getJson(me.getRankingdataURL);
            },

            getJson:function(url){
                var me = this;
                var _script = document.createElement("script");

                url+= '?rank_id=20&channel_id=0&user_detail=1&pz=6&format=jsonp&callback=gameRanking.getGameRankingData';
                _script.type = "text/javascript";
                _script.src = url;
                if (!/*@cc_on!@*/0) {
                    _script.onload = me.callBackScript;
                } else {
                    _script.onreadystatechange = function () {
                        if (_script.readyState == 'loaded' || _script.readyState == 'complete') {
                            me.callBackScript();
                        }
                    }
                }
                document.getElementsByTagName("head")[0].appendChild(_script);
            },

            callBackScript : function(){
                if(this.parentNode){
                    this.parentNode.removeChild(this);
                }
            },
            // 3 Times
            tryRequest : function(url) {
                this.getJson(url);
            },
            getGameRankingData: function (data) {
                var me = this,
                    erroCode = data.error_code;
                if(erroCode || !data){
                    if(this.CURRENTREQUESTSTATUS < this.TRYTIMES) {
                        this.tryRequest(this.getRankingdataURL);
                        this.CURRENTREQUESTSTATUS ++;
                    }else{
                        me.showMsg(0);
                    }
                }else{
                    me.initRankingDom(data);
                }
            },
            initRankingDom: function (data) {
                var me = this;
                data = data.result;
                var currentWeek;
                    currentWeek = data;
                    me.setRanking(currentWeek,me.currentRanking);
            },
            setRanking: function(data,distNode){
                var me = this,
                        len = data.length,
                        str = '',
                        num,
                        userName,
                        userNameShort,
                        userNameLength,
                        userImg,
                        userDec,
                        uid,
                        userZone,
                        ranking,
                        last_ranking,
                        ranking_change;
                if(len){
                    str += '<div class="yk-rank yk-rank-long">';
                    for(var i=0; i<len; i++){
                        num = i + 1;
                        userName = data[i].screen_name;
                        userNameLength = userName.length;
                        userImg = data[i].profile_image_url.middle;
                        userZone = data[i].home_page;
                        ranking = data[i].ranking;
                        last_ranking = data[i].last_ranking;
                        ranking_change = ranking - last_ranking;
                        userDec = data[i].description;
                        uid = data[i].id;

                        userNameShort = me.getSubString(userName,12);
                        str += '<div class="item">';
                        if(i<3){
                            str += '<label class="hot">'+ num +'</label>';
                        }else{
                            str += '<label>'+ num +'</label>';
                        }
                        str += '<img src="'+ userImg + '" alt="用户头像"/>';
                        str += '<div class = "userinfo">';
                        str += '<h3><a target="_blank" href=\"'+ userZone +'\">'+ userNameShort +'</a></h3>';
                        /*if(ranking_change < 0){
                            ranking_change = Math.abs(ranking_change);
                            str += '<span>上升<span>'+ ranking_change +'</span>名</span>';
                        }else if(ranking_change>0){
                            str += '<span>下降<span>'+ ranking_change +'</span>名</span>';
                        }else if(ranking_change===0){
                            str += '<span>排名稳定</span>';
                        };*/
                        str += '<p title = "'+ userDec +'">'+ userDec +'</p>';
                        str += '</div>';
                        str += '<a class="sub-btn" target="_blank" href=\"'+ userZone +'\">订阅</a>';
                        str += '</div>';
                    }
                    str += '<a href="http://guanghe.youku.com"> 查看更多频道 &gt&gt</a>';
                    str += '</div>';
                    distNode.update(str);
                }else{
                    me.showMsg(1);
                }
            },
            //TODO 完善提示信息展示方法 0 代表获取信息失败，1代表暂时没有排行信息，2代表正在加载，并带有动画
            showMsg: function (flag) {

            },
            /*
             * 该方法处理中英文字符混合的字符串的按长度截断问题
             * @param str 要处理的字符串
             * @param n 处理后的字符串的长度，以英文字符的个数为标准
             * @desc 先把字符串统一转换成英文字符，一个中文字符为两个英文字符，这里用**来替代
             * 如果转后的字符串长度仍然小于要求的长度，那么直接返回，若大于则进行处理，因为是中英混合的字符串
             * 所以处理后的字符个数介于n/2(假设n为偶数，最小字符个数为全为汉字的情况)与n之间
             * 所以从n/2开始进行判断直到长度达到要求
             * */
            getSubString: function(str,n){
                var strReg=/[^\x00-\xff]/g;
                var _str=str.replace(strReg,"**");
                var _len=_str.length;
                if(_len>n) {
                    var _newLen = Math.floor(n/2);
                    var _strLen = str.length;
                    for (var i = _newLen; i < _strLen; i++) {
                        var _newStr = str.substr(0, i).replace(strReg, "**");
                        if (_newStr.length >= n) {
                            return str.substr(0, i) + "...";
                            break;
                        }
                    }
                }else{
                    return str;
                }
            }

        };
        document.observe("dom:loaded",function(){
            gameRanking.init();
            $('subscribeTag').observe('mouseenter',function(){
                $('subscribeTag').addClassName('current');
                $('NBTag').removeClassName('current');
                $('subscribe').setStyle({'display':'block'});
                $('NB').setStyle({'display':'none'});
            });
            $('NBTag').observe('mouseenter',function(){
                $('subscribeTag').removeClassName('current');
                $('NBTag').addClassName('current');
                $('subscribe').setStyle({'display':'none'});
                $('NB').setStyle({'display':'block'});
            });
        });
    </script>




	</div>
</div>
</div>

            
        </div>
    </div>
</div>
</div>

