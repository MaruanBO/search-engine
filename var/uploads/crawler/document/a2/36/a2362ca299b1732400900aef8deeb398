(function(){
    OST = {};
    var osType = {
        isWin:'Win',
        isMac:'Mac',
        isSafari:'Safari',
        isChrome:'Chrome',
        isIPAD: 'iPad', 
        isIPHONE: 'iPhone', 
        isIPOD: 'iPod',
        isLEPAD: 'lepad_hls',
        isMIUI: 'MI-ONE',
        isAndroid:'Android',
        isAndroid4: 'Android 4.',
        isAndroid41: 'Android 4.1',
        isSonyDTV: "SonyDTV",
        isBlackBerry:"BlackBerry",
        isMQQBrowser:'MQQBrowser',
        isMobile:'Mobile'
    };
    for(var os in osType){
        if(navigator.userAgent.indexOf(osType[os]) !== -1){
            OST[os] = true;
        }else{
            OST[os] = false;
        }
    }
    OST.isIos = ((OST.isIPAD || OST.isIPHONE || OST.isIPOD) || OST.isMac );
    OST.isPhone = (OST.isIPHONE || OST.isIPOD || (OST.isAndroid&&OST.isMobile));
    OST.isPad = (OST.isIPAD || (OST.isAndroid && !OST.isMobile));
})();


function closeclick_pad()
{
document.getElementById('addDesktop_ipad').style.display='none';
cookiesave('closeclick_pc','closeclick_pc','','','');
 var h=document.getElementsByTagName('head')[0],
        s=document.createElement('script');
        s.type='text/javascript';
        s.charset='utf-8';
        s.src='http://hz.youku.com/red/click.php?tp=1&cp=4009132&cpp=1000607&url='+window.location.href;
        h.firstChild?h.insertBefore(s,h.firstChild):h.appendChild(s);
}
function is_iPad(){
    var ua = navigator.userAgent.toLowerCase();
    if(ua.match(/iPad/i)=="ipad") {
       return true;
    } else {
       return false;
    }
}
function cookiesave(n, v, mins, dn, path){
	if(n)
	{
		if(!mins) mins =  10;
		if(!path) path = "/";
		var date = new Date();
		date.setTime(date.getTime() + (mins * 60 * 1000));
		var expires = "; expires=" + date.toGMTString();
		if(dn) dn = "domain=" + dn + "; ";
		document.cookie = n + "=" + v + expires + "; " + dn + "path=" + path;
	}
}

function cookieget(n){
	var name = n + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i<ca.length;i++){
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	}
	return ;
};
(function(){
	if (document.readyState && document.readyState == 'complete') {
           var check_ipad=is_iPad();
           var $addDesktop_ipad = document.getElementById('addDesktop_ipad');
           if(!OST.isIPAD){
             $addDesktop_ipad && ($addDesktop_ipad.style.display='none');
	    }else{
		if(cookieget('closeclick_pc')=='closeclick_pc'){
			$addDesktop_ipad && (document.getElementById('addDesktop_ipad').style.display='none');
		}else{
                        if(check_ipad){
                          if(Boolean(navigator.userAgent.match(/OS [7-9]_\d[_\d]* like Mac OS X/i))){
                     document.getElementById( "ico_adddesktop_ios7_pad" ).className = "ico_adddesktop_ios7";   
                      }
                                 $addDesktop_ipad && (document.getElementById('addDesktop_ipad').style.display='block');
      var h=document.getElementsByTagName('head')[0],
        s=document.createElement('script');
        s.type='text/javascript';
        s.charset='utf-8';
        s.src='http://hz.youku.com/red/click.php?tp=1&cp=4009131&cpp=1000607&url='+window.location.href;
        h.firstChild?h.insertBefore(s,h.firstChild):h.appendChild(s);

                        }
			
		}
            }

	} else {
		setTimeout(arguments.callee, 10);
}
})();


