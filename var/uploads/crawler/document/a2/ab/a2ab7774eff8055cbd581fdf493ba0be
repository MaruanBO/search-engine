	<script src="/includes/js/jquery.swfobject.1-0-7.min.js"></script>
	<script src="/includes/js/jquery.jcoverflip.patch.js"></script>
	<script src="/includes/js/jquery.tools.min.js"></script>
	<script src="/includes/js/jquery.uuid.js"></script>
    <script src="/includes/js/jquery.timer.min.js"></script>
    <script src="/includes/js/jquery.banner.js"></script>
	<script>
	    $(document).ready(function() {
	        $("#banner").banner({
	            "timeout": 10000,
	            "infoContainer": ".section-stage",
	            "imageContainer": ".img-feature",
	            "controlsContainer": "#lst-stage",
	            "dataUrl": "/templates/json/rotate.ashx?id=[id]"
	        });

			// custom tooltip
			$("#lst-stage a.thumb").hover(function(e) {
				var uuid, el, galleryId;

				el = $(this);

				//galleryId = el.parent().attr("id").split("-")[1];
				galleryId = el.parent().attr("id");
				if (el.attr("id")) {
					uuid = el.attr("id").split("_")[2];
				} else {
					uuid = $.uuid();
					el.attr("id", "tooltip_link_" + uuid);
					$(document.body).append($("<div>").attr("id", "tooltip_" + uuid).addClass("tooltip")
					.append($("<div>").addClass("tooltip-body")
						.append($("<div>").addClass("bd")
							.append($("<img>").addClass("loading").attr("src", "/includes/img/loading.gif"))
						)
						.append($("<div>").addClass("ft"))
					)
				);
				}

				$(".tooltip:not(#tooltip_" + uuid + ")").fadeOut();

				var parent = el.parent();
				var parentOffset = parent.offset();

				var tooltip = $("#tooltip_" + uuid);

				tooltip.stopTime(uuid);

				tooltip.hover(function() {
					tooltip.stopTime(uuid);
				}, function() {
					tooltip.oneTime(300, uuid, function() {
						tooltip.fadeOut();
					});
				});


				if (tooltip.css("display") == "none") {
					var windowDiff = $(window).width() - parentOffset.left;
					if (windowDiff <= tooltip.outerWidth()) {
						tooltip.css("top", parentOffset.top - tooltip.outerHeight()).css("left", parentOffset.left - windowDiff);
						tooltip.addClass("tooltip_right");
					} else {
						tooltip.css("top", parentOffset.top - tooltip.outerHeight()).css("left", parentOffset.left - 292);
					}
				}
				tooltip.fadeIn();

				tooltip.oneTime(500, "tooltip-delay", function() {
					//var fetchUrl = "/includes/json/gallery-" + galleryId + ".json";
					var fetchUrl = "/templates/json/rotate.ashx?id=" + galleryId;
					$.getJSON(fetchUrl, function(data) {
						var tooltipBody = tooltip.find(".bd");
						tooltipBody.html("").append($("<img>").attr("src", data.excerptImageUrl));
						tooltipBody.append($("<h6>").html(data.excerptTitle));
						tooltipBody.append(data.excerptHTML);
						if (data.excerptLinks.length > 0) {
							var linksEl = $("<p>");
							for (var i = 0; i < data.excerptLinks.length; i++) {
								if (i > 0) {
									linksEl.append("<span class=\"url\">, </span>");
								}
								linksEl.append($("<a>").addClass("more").attr("href", data.excerptLinks[i].url).html(data.excerptLinks[i].text));
							}
							tooltipBody.append(linksEl);
						}
						tooltip.css("top", parentOffset.top - tooltip.outerHeight());
					});
				});

			}, function(e) {
				var uuid = $(this).attr("id").split("_")[2];
				var tooltip = $("#tooltip_" + uuid);
				tooltip.oneTime(300, uuid, function() {
					tooltip.fadeOut();
				});
			});

			// use ajax to call in featured programs on click
			$(".featured-programs a").click(function(e) {
				var fetchUrl = $(this).attr("href");
				$.getJSON(fetchUrl, function(data) {
					$(".fp-results img").attr("src", data.imgSrc);
					$(".fp-results h3").html(data.header);
					$(".fp-results .excerpt").html(data.excerpt);
					$(".fp-results .lnk-prog").attr("href", data.progUrl);
				});
				e.preventDefault();
				e.stopPropagation();
			});

			// tooltip for featured programs
			// hide for Apple Mobile
			if (!isAppleMobile()) {
				$(".featured-programs img[title]").tooltip({
					"delay": 0,
					"tipClass": "tooltipAlt"
				});
			}

			// coverflow effect for highlights
			$('#flip').jcoverflip({
				"current": 2,
				beforeCss: function(el, container, offset) {
					el.removeClass("current after").addClass("before");
					el.css("z-index", (900 - offset) + "");
					if (offset > 0) {
						el.hide();
					} else {
						el.show();
					}
					var anim = { "left": "0", "top": "15px" };
					var animImg = { "width": "211px", "height": "128px" };
					return [
					$.jcoverflip.animationElement(el, anim, {}),
					$.jcoverflip.animationElement(el.find("img"), animImg, {})
				];
				},
				afterCss: function(el, container, offset) {
					el.removeClass("current before").addClass("after");
					el.css("z-index", (900 - offset) + "");
					if (offset > 0) {
						el.hide();
					} else {
						el.show();
					}
					var anim = { "left": "209px", "top": "15px" };
					var animImg = { "width": "211px", "height": "128px" };
					return [
					$.jcoverflip.animationElement(el, anim, {}),
					$.jcoverflip.animationElement(el.find("img"), animImg, {})
				];
				},
				currentCss: function(el, container) {
					el.removeClass("before after").addClass("current");
					el.css("z-index", "999");
					el.show();
					var anim = { "left": "80px", "top": "0" };
					var animImg = { "width": "260px", "height": "157px" };
					return [
					$.jcoverflip.animationElement(el, anim, {}),
					$.jcoverflip.animationElement(el.find("img"), animImg, {})
				];
				},
				change: function(event, ui) {

					$("#count").html(($("#flip").jcoverflip("current") + 1));
					$("#flip-learnmore").fadeOut('100', function() {
						$("#flip-learnmore").css("margin-top", $("#flip .title:eq(" + ($("#flip").jcoverflip("length") - $("#flip").jcoverflip("current") - 1) + ")").outerHeight() + 16 + "px");
						$("#flip-learnmore").fadeIn('100');
					});
				}

			});
			$(".btn-flip-next").click(function() {
				$("#flip").jcoverflip("next");
				return false;
			});
			$(".btn-flip-prev").click(function() {
				$("#flip").jcoverflip("previous");
				return false;
			});
			$("#count").html(($("#flip").jcoverflip("current") + 1));
			$("#length").html($("#flip").jcoverflip("length"));
			$("#flip-learnmore").css("margin-top", $("#flip .title:eq(" + ($("#flip").jcoverflip("length") - $("#flip").jcoverflip("current") - 1) + ")").outerHeight() + 16 + "px");
		});
	</script>

	
	


