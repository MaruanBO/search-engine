	var nic_editor_ran_already = false;
}
$(document).ready(
	function()
	{
		if(!nic_editor_ran_already)
		{
			//this is one big workaround because IE8 doesn't do getElementsByClassName.
			var textareas = document.getElementsByTagName('textarea');
			var niceEdit_textareas = [];
			for(var i=0; i<textareas.length; i++)
			{
				var this_ta = textareas[i];
				if ($(this_ta).hasClass('nicEdit'))
				{
					niceEdit_textareas.push(this_ta);
					$(this_ta).addClass('isNicEdit');
				}
			}
			for(var i = 0; i < niceEdit_textareas.length; i++)
			{
				new nicEditor({buttonList : ['bold','italic','underline','strikethrough','subscript','superscript','ol','ul']}).panelInstance(niceEdit_textareas[i]);
			}

			$(document).on("js_infinite_new_content check_for_new_nicEdit", function() {
				//this is one big workaround because IE8 doesn't do getElementsByClassName.
				var textareas = document.getElementsByTagName('textarea');
				var niceEdit_textareas = [];
				for(var i=0; i<textareas.length; i++) {
					var this_ta = textareas[i];
					if(this_ta.className == 'nicEdit') {
						niceEdit_textareas.push(this_ta);
						$(this_ta).addClass('isNicEdit');
					}
				}
				for(var i = 0; i < niceEdit_textareas.length; i++) {
					new nicEditor({buttonList : ['bold','italic','underline','strikethrough','subscript','superscript','ol','ul']}).panelInstance(niceEdit_textareas[i]);
				}
			});
		}
		// hack to make this not run again, it is causing problems with infinite scroll
		nic_editor_ran_already = true;
	}
);


</script>



