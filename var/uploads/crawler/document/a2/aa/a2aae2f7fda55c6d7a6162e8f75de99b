<script>
var min=9;
var max=17;
function normalFontSize() {
   var p = document.getElementsByTagName('font');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      p[i].style.fontSize = s+"px"
   }
   var p = document.getElementsByTagName('p');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      p[i].style.fontSize = s+"px"
   }
   var d = document.getElementsByTagName('div');
   for(i=0;i<d.length;i++) {
      if(d[i].style.fontSize) {
         var s = parseInt(d[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      d[i].style.fontSize = s+"px"
   }
   var l = document.getElementsByTagName('label');
   for(i=0;i<l.length;i++) {
      if(l[i].style.fontSize) {
         var s = parseInt(l[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      l[i].style.fontSize = s+"px"
   }
   var l = document.getElementsByTagName('input');
   for(i=0;i<l.length;i++) {
      if(l[i].style.fontSize) {
         var s = parseInt(l[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      l[i].style.fontSize = s+"px"
   }
   var l = document.getElementsByTagName('textarea');
   for(i=0;i<l.length;i++) {
      if(l[i].style.fontSize) {
         var s = parseInt(l[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s -= 1;
      }
      l[i].style.fontSize = s+"px"
   }
   document.getElementById("ratingText").style.fontSize="16px";
   
    var frame = document.getElementById("comments_ifr");
    var content = (frame.contentDocument || frame.contentWindow);
    content.body.style.fontSize = "16px";
}
function increaseFontSize() {
   var p = document.getElementsByTagName('font');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s += 1;
      }
      p[i].style.fontSize = 19+"px"
   }
   var p = document.getElementsByTagName('p');
   for(i=0;i<p.length;i++) {
      if(p[i].style.fontSize) {
         var s = parseInt(p[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s += 1;
      }
      p[i].style.fontSize = 19+"px"
   }
   var d = document.getElementsByTagName('div');
   for(i=0;i<d.length;i++) {
      if(d[i].style.fontSize) {
         var s = parseInt(d[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s += 1;
      }
      d[i].style.fontSize = 19+"px"
   }
   var l = document.getElementsByTagName('label');
   for(i=0;i<l.length;i++) {
      if(l[i].style.fontSize) {
         var s = parseInt(l[i].style.fontSize.replace("px",""));
      } else {
         var s = 16;
      }
      if(s!=max) {
         s += 1;
      }
      l[i].style.fontSize = 19+"px"
   }
   var l = document.getElementsByTagName('input');
   for(i=0;i<l.length;i++) {
      if(l[i].style.fontSize) {
         var s = parseInt(l[i].style.fontSize.replace("px",""));
