<!-- Dock Menu :: Begin -->
<script type="text/javascript"><!--
    // Dock Navigation
    $(document).ready(function() {
        //TO DO: declare & set disableDockMenu js variable to true value before the below script executes
        if (typeof(disableDockMenu) != "undefined" && disableDockMenu){
            return;
        }

        var scrollLimit = 141 + $(".homepagealert").height();
        var viewHeight = $(document).height() - $(window).height();
        $(window).scroll(function(e) {
            $menu = $('#head_dock');
            $welcomeBar = $('#welcome_bar');
            if ($(this).scrollTop() > scrollLimit && viewHeight > scrollLimit) {
                if (jQuery.browser.mobile === false) {
                    $("#jewelscent-bar").hide();
                    $("#header-container").hide();
                    $(".homepagealert").hide();
                    //$(".wf-active").css("margin-top", "92px");
                } else {
                    $(".header-sponsor").hide();
                }
                $menu.addClass('top-docked');
            } else {
                //$(".wf-active").css("margin-top", "0");
                $menu.removeClass('top-docked');
                if (jQuery.browser.mobile === false) {
                    $("#jewelscent-bar").show();
                    $("#header-container").show();
                    $(".homepagealert").show();
                } else {
                    $(".header-sponsor").show();
                }
            }
        });
    });
    //--></script>
<!-- Dock Menu :: End -->

