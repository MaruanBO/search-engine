<script type="text/javascript"><!--
    // scroll smooth
    var $root = $('html, body');
    $('a.scrollto').click(function() {

        if(typeof($.attr(this, 'offset-top'))  === "undefined") {
            offsetCustom = 0;
        }
        else {
            offsetCustom = $.attr(this, 'offset-top');
        }
        $root.animate({
            scrollTop: $($.attr(this, 'href')).offset().top - offsetCustom
        }, 700);
        return false;
    });
    //--></script>



