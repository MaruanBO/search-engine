<script type="text/javascript"><!--
    window.fbAsyncInit = function() {
        // init the FB JS SDK
        FB.init({
            appId: '1375784615970208', // App ID from the app dashboard
            status: true, // Check Facebook Login status
            cookie: true, // enable cookies to allow the server to access the session
            channelUrl: '//www.jewelscent.com/channel.html',
            xfbml: true                                  // Look for social plugins on the page
        });

        // Additional initialization code such as adding Event Listeners goes here
        // BEGIN: Social discount extension
        try
        {
            if (FB != undefined) {
                FB.Event.subscribe('edge.create', function(href, widget) {
                    if (href == "http://www.facebook.com/jewelscent") {
                        applySocialDiscount(8);
                    }
                });

                FB.Event.subscribe('edge.remove', function(href, widget) {
                    if (href == "http://www.facebook.com/jewelscent") {
                        removeSocialDiscount(8);
                    }
                });
            }
        } catch (err) {
        }

        function applySocialDiscount(social_discount_id) {
            $.ajax({
                url: 'index.php?route=module/social_discount/applySocialDiscount',
                type: 'post',
                data: 'social_discount_id=' + social_discount_id,
                dataType: 'json',
                success: function(json) {

                    if (json['success']) {
                        $('#notification').html('<div class="success" style="display: none;">' + json['success'] + '<img src="http://d2bc1p6cja8oir.cloudfront.net/catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
                        $('.success').fadeIn('slow');
                        $('#cart').load('index.php?route=module/cart #cart &gt; *');
                        $('.cart-total').load('index.php?route=checkout/cart .cart-total &gt; *');
                        $('html, body').animate({scrollTop: 0}, 'slow');
                    } else if (json['error']) {
                        $('#notification').html('<div class="warning" style="display: none;">' + json['error'] + '<img src="http://d2bc1p6cja8oir.cloudfront.net/catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
                        $('.warning').fadeIn('slow');
                    }
                }
            });
        }

        function removeSocialDiscount(social_discount_id) {
            $.ajax({
                url: 'index.php?route=module/social_discount/removeSocialDiscount',
                type: 'post',
                data: 'social_discount_id=' + social_discount_id,
                dataType: 'json',
                success: function(json) {
                    if (json['remove']) {
                        $('#notification').html('<div class="warning" style="display: none;">' + json['remove'] + '<img src="http://d2bc1p6cja8oir.cloudfront.net/catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
                        $('.warning').fadeIn('slow');
                        $('#cart').load('index.php?route=module/cart #cart > *');
                        $('.cart-total').load('index.php?route=checkout/cart .cart-total > *');
                        $('html, body').animate({scrollTop: 0}, 'slow');
                    } else if (json['error']) {
                        $('#notification').html('<div class="warning" style="display: none;">' + json['error'] + '<img src="http://d2bc1p6cja8oir.cloudfront.net/catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
                        $('.warning').fadeIn('slow');
                    }
                }
            });
        }
// END: Social discount extension
    };

    // Load the SDK asynchronously
    (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/en_US/all.js";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
//--></script>
<!-- END: Load Facebook SDK asynchronously -->

