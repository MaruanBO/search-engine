    // Dock Navigation
    $(document).ready(function() {
        $('#gotop').hide();
        if (jQuery.browser.mobile === false) {
            $(window).scroll(function(e) {
                if ($(this).scrollTop() > 1700) {
                    $('#gotop').show();
                } else {
                    $('#gotop').hide();
                }
            });
        }
    });
    //--></script>
