        function switchPicture(switchTo) 
        {
            var videoImage1 = document.getElementById("videoImage1");
            var videoImage2 = document.getElementById("videoImage2");
            if (videoImage1 != null && videoImage2 != null)
            {
                videoImage1.style.display = (switchTo == 1 ? "" : "none");
                videoImage2.style.display = (switchTo == 1 ? "none" : "");
            }
