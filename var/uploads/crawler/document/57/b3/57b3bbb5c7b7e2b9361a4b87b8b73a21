<script>
// Fix error page home btn
dojo.addOnLoad(function() 
{	
	try
	{
		var errorNodes = dojo.query("h1[class^='error']");
		
		if(errorNodes.length > 0)
		{
			var errorElement = errorNodes[0].parentNode;
			
			if(errorElement && errorNodes[0].innerHTML.indexOf("re Sorry") != -1)
			{
				var errHomeBtnStore = "";
				var errHomeButton = dojo.query("a[class^='btn']",errorElement);
				
				switch(jsStoreIdent)
				{
					case "Hand":
						errHomeBtnStore = "h";
						break;
					case "Wolf":
						errHomeBtnStore = "w";
						break;
					case "Cush":
						errHomeBtnStore = "c";
						break;
					default:
						errHomeBtnStore = "h";
						break;
				}
				
				if(errHomeBtnStore != "")
				{
					errHomeButton[0].href = "/" + errHomeBtnStore + "/home";
				}
			}
		}
	}
	catch(e)
	{
		console.error("Unable to fix Exception home button: " + e);
	}
});
</script>

<script type="text/javascript">
dojo.addOnLoad(function() {    
    var footerLinks = dojo.query("a", dojo.byId("footer"));
    
    dojo.forEach(footerLinks, function(node,idx) {
        if(node.href.indexOf("allreviews.htm") != -1)
        {
            nodeParent = dojo.query(node)[0].parentElement;
            
            dojo.style(nodeParent, {"display":"none"});
        }
    }); 
});
</script>

