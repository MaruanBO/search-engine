<script type="text/javascript">
	
	function cmInitiateElementTag(elementID, category)
	{
		if(typeof cmPassCoremetrics !== "undefined" && cmPassCoremetrics != 'OFF'){cmCreatePageElementTag(elementID, cmCommon.appendESiteID(category));}
	}
	
	function cmInitiatePageViewTag(cmPageName,cmPageCat,cmSearchSLI,cmSearchSLIResults,cmPVAttributes)
	{
		if(typeof cmPassCoremetrics !== "undefined" && cmPassCoremetrics != 'OFF'){cmCreatePageviewTag(cmCommon.appendESiteID(cmPageName),cmPageCat,cmSearchSLI,cmSearchSLIResults,cmPVAttributes);}
	}
	
	function cmInitiateConversionEvent(cmEventID,cmActionType,cmCategoryID,cmPoints,cmEAttributes)
	{
		if(typeof cmPassCoremetrics !== "undefined" && cmPassCoremetrics != 'OFF'){cmCreateConversionEventTag(cmEventID, cmActionType, cmCategoryID, cmPoints,cmEAttributes)}
	}
	
	function cmInitiateManualImpressionTag(cmPageName,cmTrackSP,cmTrackRE,cmTrackCR,cmTrackME)
	{
		if(typeof cmPassCoremetrics !== "undefined" && cmPassCoremetrics != 'OFF'){cmCreateManualImpressionTag(cmPageName, cmTrackSP, cmTrackRE, cmTrackCR, cmTrackME)}
	}
	
	function cmInitiateManualLinkClickTag(cmHref,cmLinkName,cmPageName)
	{
		if(typeof cmPassCoremetrics !== "undefined" && cmPassCoremetrics != 'OFF'){cmCreateManualLinkClickTag(cmHref, cmLinkName, cmPageName);	}
	}
</script>
