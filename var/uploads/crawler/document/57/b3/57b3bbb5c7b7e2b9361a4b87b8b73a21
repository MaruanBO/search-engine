			dojo.subscribe("/bec/analytics/coremetrics/orderokview", function(params) {
				//Build product list
				var itemPriceArr = tmParams.prices.split("|");
				var quantityArr = tmParams.qtys.split("|");
				var adjustedItemPriceArr = tmParams.adjustedProductPrice.split("|");
				
				var productList = new Array();				
				for(idx in itemPriceArr)
				{
					var tempObj = {};
					
					tempObj.item = params.cart[idx].productId;
					tempObj.amount = itemPriceArr[idx];
					tempObj.quantity = quantityArr[idx];
					tempObj.discount = tempObj.amount - adjustedItemPriceArr[idx];
					
					productList.push(tempObj);
				}
				
				//Discount
				var discount = 0;
				for(idx in itemPriceArr)
				{
					discount = discount + parseFloat(itemPriceArr[idx]);
				}
				var orderDiscount = becDotomi.subTotal - Math.round(discount * 100)/100;
				
				if(MasterTmsUdo)
				{
					MasterTmsUdo.transaction.order_discount = orderDiscount;
					MasterTmsUdo.transaction.productlist = productList;
				}
				
