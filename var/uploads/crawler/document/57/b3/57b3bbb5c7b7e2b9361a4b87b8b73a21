<script type="text/javascript"><!--
	try 
	{	
		if(cmPassCoremetrics != 'OFF')
		{
			cmCommon.generateCategoryName(cmCategory);
			cmAttributes = cmCategory;
			
			if(typeof cmPageName == "undefined")
			{
			  	var cmPageName = "";
			}
			
			// Trim and append eSite ID.
			cmPageName = cmCommon.RTrim(cmCommon.appendESiteID(cmPageName));
			
			// Reviews Conversion Event Id
			cmReviewConvEventId = cmCommon.RTrim(cmCommon.appendESiteID("Add a Review"));
			
			// Homepage
			if(cmPageName == 'HomePage')
			{
				cmCreateTechPropsTag(cmPageName, cmCategory, cmAttributes);
				console.debug("Coremetrics: Main case statement - TechProps tag thrown");
			}
			// Product page
			else if(typeof cmProductPage != "undefined" && cmProductPage == 'Yes')
			{					
				//SLI Rich Auto Suggest tagging
				var isSuggestedProduct = cmCommon.getUrlParamVal("suggestedProduct");
				if(isSuggestedProduct && isSuggestedProduct == "true")
					cmCategory = "AUTOCOMPLETE-SUGGESTED PRODUCT";
				
				var form = document.OrderItemAddForm;				
				if (form) {
					form.field2_0.value = cmCategory + ";ProductStandard";
				}
	
				cmCreateProductviewTag(cmProductId, cmItemName, cmCategory, cmProductAttrs);
				console.debug("Coremetrics: Main case statement - ProductView tag thrown");
			}	
			// Shopping Cart page
			else if(cmPageName == 'ShoppingCart_Page')
			{
				cmCreatePageviewTag(cmPageName, cmCategory, cmSearch, null, cmAttributes);
				console.debug("Coremetrics: Main case statement - Shopping Cart page found");
			}
			// BV: Add a Review
			else if(cmPageName == "Add a Review")
			{
				cmCreatePageviewTag(cmPageName, cmCategory, cmSearch, null, cmAttributes);
				cmCreateConversionEventTag(cmReviewConvEventId, 1, cmCategory,100);
				console.debug("Coremetrics: Main case statement - Add a review page found");
			}
			// BV: Complete Review
			else if(cmPageName == "Completed Review")
			{
				cmCreatePageviewTag(cmPageName, cmCategory, cmSearch, null, cmAttributes);
				cmCreateConversionEventTag(cmReviewConvEventId, 2, cmCategory,100);
				console.debug("Coremetrics: Main case statement - Add a review (Complete) page found");
			}
			// Error page
			else if(cmPageName == 'error')
			{
				cmCreateErrorTag(cmPageName, cmErrMsg);
				console.debug("Coremetrics: Main case statement - Error page found");
			}
			// Category Page
			else if(cmPageName.indexOf("Cat:") >= 0)
			{
				// If no category filters have been selected
				if(window.location.hash == "")
				{
					cmCreatePageviewTag(cmPageName, cmCategory, cmSearch, null, cmAttributes);
				}
				console.debug("Coremetrics: Main case statement - Category page found");
			}
			// Search
			else if(cmPageName == "Search")
			{
				// Placeholder to prevent default pageView.  Handled by SLI.
			}
			// Default page
			else if(cmPageName != "")
			{
				cmCreatePageviewTag(cmPageName, cmCategory, cmSearch, null, cmAttributes);
				console.debug("Coremetrics: Main case statement - Default page w/ pageName");
			}
			else
			{
				cmCreatePageviewTag();
				console.debug("Coremetrics: Main case statement - Default page w/o pageName");
			}
			
			console.debug("Coremetrics: Main case statement - COMPLETE (name = " + cmPageName + ") ");
		}
	}
	catch(e)
	{
		console.error("bec.analytics.Coremetrics._processPageName() Exception: " + e);
	}
	
	<!-- BEGIN: cmCreateTagNC.jsp --><!-- END: cmCreateTagNC.jsp --> 		
//--></script>
