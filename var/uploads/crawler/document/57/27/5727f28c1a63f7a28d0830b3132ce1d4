    {
        if (typeof label == 'undefined' ) label = '';
        cleanValue = typeof value == 'undefined' ? 0 : Math.round(value);
        ga( 'send', 'event', pageName, action, label, cleanValue );
    }
	    // ]]>
    </script>        
	</body>
</html>

";i:2;s:4:"6311";}