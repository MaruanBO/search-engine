    <script type="text/javascript">
        $HA.callbacks.unshift(function () {
            $(document).on( 'click', '.browse-items-navigation .navigation-menu-options .expandable', function () {
                var el = $(this);                
                el.find($('.navigation-submenu-sub')).slideToggle();
                var toggles = el.find($('.expand-icon'));                
                if (toggles.hasClass('expand-down')) {
                    toggles.removeClass('expand-down').addClass('expand-up');
                } else {
                    toggles.removeClass('expand-up').addClass('expand-down');
                }
            });
        });
    </script>        
