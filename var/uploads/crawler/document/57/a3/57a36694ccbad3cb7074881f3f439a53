	window.addEvent('domready', function(){
		// load all the images for mobile devices since the effects while scrolling are not reliable.
		var loadAll = function() {
			window.fireEvent('loadAllImages');
			window.removeEvent('touchstart', loadAll);
		};
		window.addEvents({'touchstart': loadAll});
	});

