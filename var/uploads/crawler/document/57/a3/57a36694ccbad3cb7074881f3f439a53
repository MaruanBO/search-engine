<script type='text/javascript'>
(function(){
	var gradeSelector = $('gradeselect'),
		allGrades = gradeSelector.getElements('option');
	$('guidedformel').addEvents({
		'submit': function(e){
			if(e) e.stop();
			// show loading...

			var url = '/'+$('contentblock').get('value')+'/'+$('gradeselect').get('value')+'/';
			// tracking code?

			window.location = url;
		},
		'change': function(e){
			var topic = $('contentblock').get('value');
			var selectedGrade = gradeSelector.get('value');
			gradeSelector.empty();
			for(var i = 0; i < allGrades.length; i++ ) {
				if(!allGrades[i].hasClass('no-'+topic)) {
					var n = allGrades[i].clone();
					n.selected = (n.get('value') == selectedGrade);
					gradeSelector.adopt(n);
				}
			}
		}
	});
	$('go').addEvent('click', function(e){
		//e.stop();
	});
	var arrow = $$('.read-down')[0];

	arrow.addEvents({
		'mouseenter' : function(e) {

		},
		'mouseleave': function(e) {

		},
		'click': function(e) {
			e.stop();
			new Fx.Scroll(window).toElement($$('.ContentGrids')[0], 'y');

		}
	});
})();
</script>

