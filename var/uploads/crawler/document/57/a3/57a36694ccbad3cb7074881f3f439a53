<script type="text/javascript">
	$$('.add-hover').addEvents({
		'mouseenter': function(e){ this.addClass('hover');},
		'mouseleave': function(e){ this.removeClass('hover');}
	});

	// We don't have a page to link to for the "More" tab, so make sure it doesn't do anything when clicked
	$$('#neckup-new-more > a').addEvent('click', function(e){ e.stop(); });

	// Keep a tab open if it's search form has focus
	$$("#top .main-nav .menu-bar .menu-item").addEvent('mouseenter',function() {
		$$("#top .main-nav .menu-bar").removeClass('force-close');
		var id = this.get('id');
		$$("#top .main-nav .menu-bar .force-open").each(function(el) {
			var focus = el.getElements(":focus");

			// Current tab
			if(id===el.get('id')) {
				if(!focus.length) el.removeClass('force-open');
			}
			// Other tab
			else if(focus.length) {
				if(document.activeElement) document.activeElement.blur();
				el.removeClass('force-open');
			}

			if(id===el.get('id') && !el.getElements(":focus")) el.removeClass('force-open');
		});
	});
	$$("#top .main-nav .menu-bar .menu-item").each(function(item) {
		item.getElements('input').addEvents({
			focus: function(e) {
				item.addClass('force-open');
			},
			blur: function(e) {
				item.removeClass('force-open');
			}
		});
	});
	$$("#top .main-nav .menu-bar .dropdown-content .icon-cancel").addEvent('click',function(e) {
		$$("#top .main-nav .menu-bar").addClass('force-close');
	});
</script>

