			window.addEvent('domready', function(){
			var origWidth = $('cse-top-input').getStyle('width').toInt() || $('cse-top-input').getSize().x;
			var stickr = $('top').getElement('.neckup-seasonal');
			window.fireEvent('addNewMask', $('cse-top-input').addEvents({'focus': function(e){
				this.tween('width', (origWidth + 70));
