<script type='text/javascript'>

	var morecontroller = new Class({
		initialize: function(options) {
			this.sliders = {};
			$$('#footer-links .morelink').each( function(el, ind) { 
				el.addEvent('click', function(event) {
					event.stop();
					this.clickedit(el, ((el.id)+'-div'));
				}.bind(this) );
			}, this );
		},
		clickedit: function(link, id) {
			if(!$defined(this.sliders[id]))
			{
				this.sliders[id] = new Fx.Slide(id).hide(); 
				$(id).setStyle('display', 'block');
			}
			this.sliders[id].toggle();
			//swap out link text:
			var alt = link.getProperty('alt');
			link.setProperty('alt',link.get('text'));
			link.set('text', alt);
			
		}	
	});
	var mc = new morecontroller();
	
</script>
