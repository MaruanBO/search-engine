toggleSpoiler = function(spoiler){
	if (spoiler.parentNode.parentNode.getElementsByTagName('div')[1].getElementsByTagName('div')[0].style.display != '')
	{
		spoiler.parentNode.parentNode.getElementsByTagName('div')[1].getElementsByTagName('div')[0].style.display='';
