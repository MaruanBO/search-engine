<script type="text/javascript">
//<![CDATA[
//******************************************************** 
//* Note: This function is dynamically generated. 
//* DO NOT CALL THIS METHOD DIRECTLY AS IT MAY CHANGE IN THE FUTURE. 
//* USE 'CartNavigationControl.GenerateUpdateScript()' TO INJECT JAVASCRIPT WRAPPER METHOD 'RefreshCartItemCount()' AND CALL THAT INSTEAD. 
//********************************************************
function CartItemCountUpdate() 
{
    try
    {
        Amway.Core.Web.UI.Cart.CartNavigationControl.PopuplateCartItemCount(PopuplateCartItemCount_callback);
    }
    catch (ex)
    {
    }
}
//********************************************************
//* Note: This function is dynamically generated.
 //********************************************************
function PopuplateCartItemCount_callback(response)
{
    try
    {
        var lnkCart = null;
        var isDifferentIFrameDomain = false;
        try {
            isDifferentIFrameDomain = (window.document.domain != window.parent.document.domain) ? true : false;
        } catch (ex) {
            isDifferentIFrameDomain = true;
        }
        if (isDifferentIFrameDomain) {
            lnkCart = document.getElementById('ctl00_ctl07___ctl04___lnkItemsInCart'); 
        }
        else {
            lnkCart = top.document.getElementById('ctl00_ctl07___ctl04___lnkItemsInCart'); 
        }
        if (response != null && lnkCart != null)
        {
           lnkCart.innerHTML = response.value;
           lnkCart.title = response.value.replace('<br />','');
        }
    }
    catch (ex)
    {
    }
}

