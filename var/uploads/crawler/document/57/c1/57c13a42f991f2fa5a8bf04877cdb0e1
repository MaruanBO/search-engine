<script type="text/javascript">
	function getContent(select_year) {
		get_remote_content(select_year);
		//window.scrollTo(0, 2840);
	}

	function get_remote_content(select_year) {
		var url = "/resource/reviews/"+select_year+".htm";
		//$("#tm_div").load(url); 
	}

	var select_year = "pagination";
	get_remote_content(select_year);

	function reviews_load(year){
		var select_year = "reviews_"+year;
		get_remote_content(select_year);
	}

	function loadIframe(iframeName, url) {
		var $iframe = $('#' + iframeName);
		if ( $iframe.length ) {
			$iframe.attr('src',url);   
			return false;
		}
		return true;
	}
</script>

        
