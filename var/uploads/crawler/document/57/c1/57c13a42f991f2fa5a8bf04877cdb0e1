<script type="text/javascript" src="/common/PN22-24WK/js/zoombox.js"></script> 
    <script type="text/javascript">
      $(function() {
        var SECONDS_TILL_GO_TIME = 1;
        var initialDelay;
        var currentPlayer;
        var $players = $('iframe.vzaar-video-player'); //won't need to be changed unless you stray from the default iframe class

        $('a.zoombox').zoombox();

        $.each($players, function(index, value) {
          var id = $(this).attr('id');
          window['player_' + id] = new vzPlayer(id);
          window['player_' + id].ready(function() {
            window['player_' + id].addEventListener("playState", function(state) {
              dealWithState(id, state);
            });
          });
        });

        $('.zoombox').click( function(){
          $.each($players, function(index, value) {
            var id = $(this).attr('id');
            zoomboxChecker(currentPlayer);
            window['player_' + id].pause();
          });
        });

        function dealWithState(id, state) {
          if (state === "mediaStarted" || state === "mediaPlaying") {
            currentPlayer = window['player_' + id];
            $.each($players, function(index, value) {
              if ($(this).attr('id') !== id) {
                window['player_' + $(this).attr('id')].pause();
              }
            });
            window['player_' + id].play2();
          }
        }

        function autoplayFirstVideo() {
          window.clearTimeout(initialDelay);
          var id = $players.first().attr('id');
          window['player_' + id].play2();
        }

        initialDelay = window.setTimeout(autoplayFirstVideo, (SECONDS_TILL_GO_TIME * 1000));

        function zoomboxChecker(player){
          var checkInt = setInterval(function(){
            if ($('#zoombox').length == 0){
              player.play2();
              clearInterval(checkInt);
            }
          }, 1000);
        };
      });
    </script>
