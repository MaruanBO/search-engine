	function resizeIframe(dynheight,id)
        {
            var id;
			var ifram_height = parseInt(dynheight)+0;
			if (ifram_height==null) ifram_height = 1211;
			$("#testimonial").height(ifram_height);

			//alert(dynheight);
			if (id=="page")
			$("html, body").animate({ scrollTop: $('#s10').offset().top }, 1000);  /* page moving */
    }
	
        jQuery(window).load(function(){
			$(window).scroll(function(){ // 
				var d_top = $(document).scrollTop();

				var w_height = $(window).height();
				var d_height = $(document).height();
				var show_top = "3000";
				var show_bottom = "1300";
				var check_bottom = d_height - show_bottom;

				if (d_top<show_top || d_top > check_bottom)
					document.getElementById("fixlocation1").style.display = "none";
				else 
					document.getElementById("fixlocation1").style.display = "block";
        });
	});
