<script type="text/javascript">
	function popupWindow(url, w, h) {
		window.open(url, '', 'toolbar=no,location=no,directories=no status=no,menubar=no,scrollbars=yes,resizable=no,copyhistory=no,width=' + w + ',height=' + h + ',screenX=150,screenY=150,top=150,left=150')
	}

	function subscribe_action(){
		var subsEmail = $("#subscribe_email").val();
		//exacttarget();
		//$(".wrapper").show();
		$.ajax({
			type : "POST",
			url : '/cart/include/function/subscribe_action.asp',
			//contentType: "charset=utf-8",
			dataType : "html",
			data : { "subsEmail":subsEmail },
			success: ajaxSuccessSubscribe,
			error : ajaxErrorSubscribe
		});
	}

	function ajaxSuccessSubscribe(data){
		var windowWidth = document.documentElement.clientWidth;
		var windowHeight = document.documentElement.clientHeight;
		var popupHeight = 400;
		var popupWidth = 600;
		$("#subscribe_message").css({
			"position": "fixed",
			"top": windowHeight/2-popupHeight/2,
			"left": windowWidth/2-popupWidth/2
		});
		$("#subscribe_message").show();


		$(".show_message").html("<h4>"+data+"</h4>");
		//alert(data);
		window.setTimeout( subscribe_close, 4000 ); 

		//==============================//
	}
	function ajaxErrorSubscribe(data){
	}
	
	function subscribe_close(){
		$("#subscribe_message").hide();

		$("#subscribe_email").val("EMAIL");
	}

	function subscribe_check(id){
		var v = $(id).val();
		if (v=="EMAIL") $(id).val("");
	}

	
	function subscribe_blank(id){
		var v = $(id).val();
		var currentId = $(id).attr('id');
		//alert(currentId);
		if (v=="") {
			if (currentId=="subscribe_email") $(id).val("EMAIL");
		}
	}

	function exacttarget(){
		

		// extacttarget send data //
		var subsEmail = $("#subscribe_email").val();
		$.ajax({
			type:"GET",
			url:'http://www.olivelotion.com/test.php?email='+subsEmail,
			dataType:'text',
			success : function(data){alert(data);},
			error: function(data){}
		});
	}

