	<script type="text/javascript" src="http://admin.brightcove.com/js/BrightcoveExperiences.js"></script>
	<script type="text/javascript">
		var myTemplateLoaded, onTemplateReady;
		var myTemplateLoaded2, onTemplateReady2;
		var player0, modVP1, modExp1, modCon1;
		var player2, modVP2, modExp2, modCon2;
		var previousVideoID=0, currentVideo;
		var videoCount	= 0;
		var popupStatus1 = 0;

		myTemplateLoaded = function (experienceID) {
			player0 = brightcove.api.getExperience(experienceID);
			modVP1 = player0.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
			modExp1 = player0.getModule(brightcove.api.modules.APIModules.EXPERIENCE);
			modCon1 = player0.getModule(brightcove.api.modules.APIModules.CONTENT);

		}

		onTemplateReady = function (evt) {
			modVP1.addEventListener(brightcove.api.events.MediaEvent.BEGIN, onMediaEventFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.CHANGE, onMediaEventFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.COMPLETE, onMediaEventFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.ERROR, onMediaEventFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.PLAY, onMediaEventFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.PROGRESS, onMediaProgressFired);
			modVP1.addEventListener(brightcove.api.events.MediaEvent.STOP, onMediaEventFired);
		}

		function onMediaEventFired(evt) {
		   if (evt.type === "mediaComplete") {
			  if (videoCount<2)
			  {
					videoCount = videoCount + 1;
					startVideo('3203784222001');
			  }
			   //changeVideo();
		   }
		}

		function onMediaProgressFired(evt) {
		   //document.getElementById("positionLog").innerHTML = "CURRENT POSITION: " + evt.position;
		}

		
		function initial_video(){
			myTemplateLoaded2 = function (experienceID) {
				player2 = brightcove.api.getExperience(experienceID);
				modVP2 = player2.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
				modExp2 = player2.getModule(brightcove.api.modules.APIModules.EXPERIENCE);
				modCon2 = player2.getModule(brightcove.api.modules.APIModules.CONTENT);
			}

			onTemplateReady2 = function (evt) {

				modVP2.addEventListener(brightcove.api.events.MediaEvent.BEGIN, onMediaEventFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.CHANGE, onMediaEventFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.COMPLETE, onMediaEventFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.ERROR, onMediaEventFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.PLAY, onMediaEventFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.PROGRESS, onMediaProgressFired2);
				modVP2.addEventListener(brightcove.api.events.MediaEvent.STOP, onMediaEventFired2);
			}
			function onMediaEventFired2(evt) {
			   if (evt.type === "mediaComplete") {
				   //changeVideo();
				   setTimeout("disablePopup()", 5000);
				   setTimeout("disableTestimonial()", 5000);
				   setTimeout("playr1play()", 5000);
			   }
			}

			function onMediaProgressFired2(evt) {
			   //document.getElementById("positionLog").innerHTML = "CURRENT POSITION: " + evt.position;
			}

		}

		function changeVideo(id){
		  playr1Pause();
		  modVP2.getCurrentVideo(currentVideoCallback2(id));
		}

		function currentVideoCallback2(id)
		 { 
			 modVP2.loadVideoByID(id);
		}

		function startVideo(id){
		  modVP1.getCurrentVideo(currentVideoCallback1(id));
		}
		function currentVideoCallback1(id)
		 {  
			 modVP1.loadVideoByID(id);
		}
		function playr1Pause(e) {
			modVP1.pause();
		}
		function playr1play(e) {
			modVP1.play();
		}
		function playr1play2(e) {
			modVP2.play();
		}

		function loadVideo(){
			//setTimeout("embedPlayer()", 5000);
			embedPlayer()
		}
		
		function embedPlayer(){
			$("#player1").show();
			startVideo('3203784222001');
		}

		function BgloadPopup(){
			if(popupStatus1==0){
				popupStatus1 = 1;
				initial_video()
				$('#videoPopup').children().remove();
				centerPopup();
				$("#videoPopup").show();
				closeVdoBut();
				$("#closeVdo").show();
				$("#backgroundLayer").css({	"opacity": "0.6","background-color":"#ccc","top":"0","left":"0"});
				$("#backgroundLayer").show();

			}
		}

		function disablePopup(){
			if(popupStatus1==1){
				popupStatus1 = 0;

				$("#playr2").remove();
				$('#videoPopup').children().remove();
				$("#backgroundLayer").hide();
				$("#closeVdo").hide()
				$("#videoPopup").fadeOut(150);
				$("#videoTestimonial").hide();
				setTimeout("playr1play()", 500);

			}
		}


		function BgloadTestimonial(){
			if(popupStatus1==0){
				popupStatus1 = 1;
				initial_video()
				$('.videoTestimonialPlay').children().remove();
				centerTestimonial();
				$("#videoTestimonial").show();
				closeTestimonial();
				$("#closeTest").show();
				$("#backgroundLayer").css({	"opacity": "0.6","background-color":"#ccc","top":"0","left":"0"});
				$("#backgroundLayer").show();

			}
		}

		function disableTestimonial(){
			if(popupStatus1==1){
				popupStatus1 = 0;

				$("#playr2").remove();
				$('.videoTestimonialPlay').children().remove();
				$("#backgroundLayer").hide();
				$("#videoTestimonial").fadeOut(150);
				setTimeout("playr1play()", 500);

			}
		}

		function closeVdoBut() {
			var windowWidth1 = document.documentElement.clientWidth;
			var windowHeight1 = document.documentElement.clientHeight;
			var popupHeight1 = $("#closeVdo").height();
			var popupWidth1 = $("#closeVdo").width();
			$("#closeVdo").css({
				"position": "fixed",
				"top": windowHeight1/2-popupHeight1/2-175,
