	<script type="text/javascript">
		$(document).ready(function(){
			
			
			$('#nav>li>ul').removeAttr('style').hide();
			$('#nav li').addClass('activeLi');
			
			if($('#nav>li').hasClass('current')){
				$('.current').find('.top-triangle').show();
				$('.current>ul').show();
				if($('#nav>li>ul>li').hasClass('subcurrent')){
					$('.subcurrent').find('.sub-triangle').show();
				}
			}
		});
	</script>
<script type="text/javascript">

  //remove trail / from the location address

  var link = location.pathname.replace(/\/$/,'');

  //then search for a link in the #nav bar that matches

  var matchingLink = $('#nav .sub-menu a[href*="'+link+'"]');

  //if we find a matching link, add the "subcurrent" class to the parent "li"

  if(matchingLink.length > 0){

    matchingLink.parent('li').addClass('subcurrent');

  }

  </script>
  
   <script type="text/javascript">

$(document).ready(function(){
  
  
  $("#sidebar ul li.left-menu-close span").click(function()
  {
    if ($(this).parent().hasClass("current"))
    {
      return;
    }
     
    var parentIndex = $("*").index($(this).parent());
    $("#sidebar ul li.left-menu-close").each(function()
    {
      if (parentIndex != $("*").index($(this)) && !($(this).hasClass("current")))
      {
        $("ul", this).slideUp();
      }
    });

    $("ul", $(this).parent()).slideToggle();
    $(this).parent().toggleClass("opened");
  });
});
</script>


  
