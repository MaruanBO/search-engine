$(document).ready(function(){
  var cookieExist = jQuery.cookie('logo');
  if (cookieExist) {
    $('#logoAnimate').remove();
    $('#logo').show();
  }
  else {
    $.cookie('logo', 'value',{ expires: null });
    $('#logo').remove();
    $('#logoAnimate').show();
  }    
});


</script> 
