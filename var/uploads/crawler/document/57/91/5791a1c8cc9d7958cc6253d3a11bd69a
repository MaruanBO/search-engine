                                            <script type="text/javascript">
                                        
                                            google.load("feeds", "1");
                                            
                                            function initialize() {
                                              var feed = new google.feeds.Feed("http://www.molotteryblog.com/molotteryblog/rss.xml");
                                              feed.setNumEntries(2);
                                              feed.load(function(result) {
                                                if (!result.error) {
                                                  var container = document.getElementById("feed");
                                                  for (var i = 0; i < result.feed.entries.length; i++) {
                                                    
                                                    var entry = result.feed.entries[i];  
                                                    var li = document.createElement("li");  
                                                    li.innerHTML = '<h3><a target="_blank" href="' + entry.link + '">' + entry.title + '</a></h3>';  
                                                    li.innerHTML += '<p>' + entry.contentSnippet + '</p>';  
                                                    container.appendChild(li);  
                                                     
                                                  }
                                                }
                                              });
                                            }
                                            google.setOnLoadCallback(initialize);
                                            </script>
                                            
