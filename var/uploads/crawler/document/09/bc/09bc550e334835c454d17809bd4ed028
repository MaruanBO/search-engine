jq.fn.toggleButtonColorOnTouch = function() {
return this.each(function() {
var self = jq(this);
var background = self.css("background-color");
var color = self.css("color");
self.on("touchstart", function() {self.css({"background-color": color, color: background})});
self.on("touchend", function() {self.css({"background-color": background, color: color})});
});
}
