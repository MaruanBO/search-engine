<div class="contenitore_squadre">
	<div id="topcalcio">
		<div id="topcalcio-speciale">
			<div id="topcalcio-bg">
				<div class="topTitle" id="europei">Euro 2016 - Qualificazioni</div>
			</div>
			<div id="topcalcio-bg">
				<div class="topTitle" id="seriea">Serie A</div>
			</div>
			<div id="topcalcio-bg">
				<div class="topTitle" id="champions">Champions League</div>
			</div>
			<div id="topcalcio-bg">
				<div class="topTitle" id="europa">Europa League</div>
			</div>
			<div id="topcalcio-bg" style="float:right">
				<div class="topTitle2" onclick="document.location='/sport/calcio'" style="color:white;">Tutte le news</div>
			</div>
		</div>
	</div>
	<div id="blocco_wrapperContainer"><div id="blocco_wrapper"></div></div>
	<div id="blocco_ticker">
		<div id="blocco_ticker_bg"></div>
		<ul id="blocco_ticker_" class="newsticker"></ul>
	</div>
