    <script type="text/javascript">
		  window.___gcfg = {lang: 'it'};
		
		  (function() {
		    var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
		    po.src = 'https://apis.google.com/js/platform.js';
		    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
		  })();
		</script>
		
		<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
		
		<script>
		
			$j("#facebook_placeholder, #google_placeholder, #twitter_placeholder").unbind();
		
			$j("#facebook_placeholder").click(function(e){
				
				e.preventDefault();

				if($j(this).hasClass("clicked")){
					closeSocialItem(this);
				}else{
					closeSocialItem($j("#twitter_placeholder"));
					closeSocialItem($j("#google_placeholder"));
					openSocialItem(this);
				}
			});
			
			$j("#google_placeholder").click(function(e){
				
				e.preventDefault();

				if($j(this).hasClass("clicked")){
					closeSocialItem(this);
				}else{
					closeSocialItem($j("#twitter_placeholder"));
					closeSocialItem($j("#facebook_placeholder"));
					openSocialItem(this);
				}
			});
			
			$j("#twitter_placeholder").click(function(e){
				
				e.preventDefault();

				if($j(this).hasClass("clicked")){
					closeSocialItem(this);
				}else{
					closeSocialItem($j("#google_placeholder"));
					closeSocialItem($j("#facebook_placeholder"));
					openSocialItem(this);
				}
			});
			
			function openSocialItem(item){

				if(!$j(item).hasClass("clicked")){
					var imgWidth = 25;
					var share_container_width = $j(item).find(".share_container").width();
					
					$j(item).addClass("clicked");
						
					$j(item).animate({
						width: imgWidth + share_container_width + 15
					}, 300);
				}
			}
			
			function closeSocialItem(item){

				if($j(item).hasClass("clicked")){
					var imgWidth = 25;
					var share_container_width = $j(item).find(".share_container").width();
					
					$j(item).removeClass("clicked");
						
					$j(item).animate({
						width: imgWidth
					}, 200);
				}
			}
			
			//.ls-topBannerSearch
			
			$j(document).ready(function(){
				//$j(".ls-topBannerSearch").after($j("#top_share_link"));
				//$j("#top_share_link").fadeIn(200);
			});
			
			
		</script>
</div>


</div>
		
		<div class="fclear"></div>
	</div>
</div>

<script>
	$j( document ).ready(function() {
		var hManchette=$j(".ls-leftManchette").height()
		
		if(hManchette > 2){
			$j(".wrapper_ls-logo").css({
				"height": "90px"
			});
		}
	});
</script>


