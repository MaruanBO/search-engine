(function ( $ ) {
	$.fn.col1BHP = function() {
	   	this[0].orientationChanged = function() {
     		this.render();
     	}

		this[0].render = function () {
			var Col1B = $j(".ls-colonna1B").detach();
			if (device.landscape()) {
				Col1B.css("margin-top", "0rem");
				Col1B.insertAfter($j(".ls-colonna1A"));
			}
			else {
				Col1B.css("margin-top", "1rem");
				Col1B.insertAfter($j(".ls-colonna1A .ls-box-object:eq(2)"));
			}
		}
		
		registerOCL(this[0]);
	}
	if (device.mobile()) {
		var bCol1B = $j("#UUID-df458cbc-ffbe-11e0-9f7e-f40cb56bf8d7");
		bCol1B.swiper(
			{
				type:"swiper",
				content:bCol1B.find(".ls-boxcontent-body .ls-simple-list"),
				container:bCol1B.find(".ls-boxcontent-body")
			}
		);
		var Col1B = $j(".ls-colonna1B");
		Col1B.col1BHP();
		Col1B[0].orientationChanged();
	}
}( jQuery ));
