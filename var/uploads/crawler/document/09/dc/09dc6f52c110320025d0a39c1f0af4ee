	<script type="text/javascript">
if (device.mobile()) {
	$j(function() {
		$j.cachedScript(
			'http://www.lastampa.it/common/js/element.write.min.js',
			function () {
				$j.cachedScript(
					'http://www.lastampa.it/common/js/writeCapture2.min.js',
					function () {
						var adX25 = $j('<div id="ad_x25" style="text-align:center;width:300px;margin:auto;">');
						if (mm_section == "/forum" && titolo_articolo == "") {
							adX25.insertAfter($j("table"));
						}
						else if (mm_section == "/Blogs" && titolo_articolo == "") {
							adX25.insertAfter($j(".ls-lastBlog-list"));
						}
						else if (titolo_articolo == "") {
							// se in homepage
							if (mm_section == "/") {
		//						$j("#ad_frame1").replaceWith(adX25);
								adX25.insertAfter($j("#apertura"));
							}
							else if ($j(".ls-boxMM3col").length > 0) {
								adX25.insertAfter($j(".ls-boxMM3col").parent());
							}
							else if ($j(".ls-box-object").length > 4) {
//								adX25.css("margin-top", "1rem");
								adX25.insertAfter($j(".ls-box-object:eq(1)"));
//								$j("<hr class='ls-box-separator' />").insertAfter(adX25);
							}
						}
						else {
							if (media_type == 'articolo') {
								var spacer = $j('<div style="height:200px;float:left;width:1px;"></div>');
								var adX25Out = $j('<div style="text-align:center;width:100%;float:left;">');
								adX25Out.append(adX25);
								$j(".ls-articoloSuperIntestazione").after(spacer);
								adX25Out.insertAfter(spacer);
							}
							else {
								adX25.insertAfter($j("#ls-multimedia"));
							}
						}
						writeCapture.write(document.getElementById('ad_x25'),'<scr'+'ipt type="text/javascript">OAS_AD("x25")</scr'+'ipt>');
					}
				);
			}
		);
	});
}
