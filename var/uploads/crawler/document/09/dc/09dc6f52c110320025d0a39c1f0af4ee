<script>
	var ticker = "";
	var theTicker = "";
	var co = 1;
	var champ = "";
	var ctid = 13;
	var cpid = 19;

	function activate(){
		$j.ajax({
			dataType: "json",
			url: "/_stc/partite/activation.json",
			async: true,
			cache: false,
			contentType : 'application/json; charset=utf-8',
			success: function( data ) {
				var ce = false;
				for(var i in data) {
					if (data[i]["size"] < 100) {
						$j("#" + data[i]["champ"] + ".topTitle").parent().css("display", "none");
					}
					else {
						// lCss("http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,700italic,400,300,700", true);
						// lCss("/modulo/sport/calcio/box-risultati-partite/css/style.css", true);
						// lCss("/modulo/sport/calcio/box-risultati-partite/css/ticker-style.css", device.desktop());
						if (!ce) {
							setChamp(data[i]["champ"]);
							setI();
						} 
						ce = true;
					}
				}
			}
		});
	}
			
	function setChamp(champi) {
		switch(champi) {
			case "europei":
				ctid = -31;
				cpid = -31;
				break;
			case "seriea":
				ctid = 13;
				cpid = 19;
				break;

			case "champions":
				ctid = -1;
				cpid = -1;
				break;
			case "europa":
				ctid = -35;
				cpid = -35;
				break;
		}
		if (champi != champ) {
			$j("#" + champ).css("color", "black");
			$j("#" + champ).parent().css("cursor", "pointer");
			champ = champi;
			$j("#" + champ).css("color", "white");
			$j("#" + champ).parent().css("cursor", "default");
			if (device.mobile()) {
//				$j.getScript('/common/js/jquery/plugins/swiper/jquery.swiper.js?id=1', function() {
//					$j.getScript('/common/js/jquery/plugins/jgestures.min.js', function() {
						update_results();
//					});
//				});
			}
			else {
				update_results();
			}
		}
	}
		
	function setI() {
		setInterval(
			function() {
				update_results();
			},
			60000
		);
	}
	
	function update_results(){
		$j.ajax({
			dataType: "json",
			url: "/_stc/partite/" + champ + ".json",
			async: true,
			cache: false,
			contentType : 'application/json; charset=utf-8',
			beforeSend: function(xhr){
				if (xhr.overrideMimeType) {
					xhr.overrideMimeType("application/json");
				}
			},
			success: function( data ) {
				if (data.lista_partite) {
					var partite = new Object();
					$j("#blocco_wrapper").html("");
					var contatore_partite = 0;
					var now = new Date();
					//data.lista_partite.length
					for(var i = 0; i < data.lista_partite.length; i++) {
						var aX = data.lista_partite[i].giorno.split("-");
						for(var j = 0; j < data.lista_partite[i].data_array.length; j++){
							var oraX = data.lista_partite[i].data_array[j].orario_inizio.split(":");
							var giorno = new Date(aX[0].trim(), aX[1].trim()-1, aX[2].trim(), eval(oraX[0])+1, oraX[1], '00'); //ora reale italiana
							if ((new Date(new Date().getTime() + (3600000*4)).getTime() > giorno.getTime()) || mm_section == "/sport") {
								data.lista_partite[i].data_array[j].ddata = giorno;
								partite[contatore_partite] = data.lista_partite[i].data_array[j] 
								contatore_partite++;
							}
						}
					}
//					contatore_partite = 6
				}
				ticker = "";
				co = 1;
				
				var toBeSorted = new Object();
				for (var parti = 0; parti < contatore_partite; parti++) {
					toBeSorted[parti] = partite[parti]["ddata"];
				}
				var sortedListe = bySortedValue(toBeSorted);
				
				for (var parti = 0; parti < contatore_partite; parti++) {
					val = partite[sortedListe[parti][0]];
//					console.log(sortedListe[parti][0], val)
					update(contatore_partite, val, parti);
				}
				
				var clear_b = $j("<div class=\"clear_b\">");
				$j("#blocco_wrapper").append(clear_b);

//				$j("#blocco_ticker_").remove();
				$j("#blocco_ticker_").html(ticker);
				if (theTicker == "") {
					theTicker = $j('#blocco_ticker_').newsTicker({
						row_height: 18,
						max_rows: 1,
					    speed: 1000,
					    direction: 'up',
					    duration: 7500,
					    autostart: 0,
					    pauseOnHover: 1
					});
				}
				else {
					theTicker.newsTicker("stop");
				}
				if ($j("#blocco_ticker_ li").size() > 1) {
					theTicker.newsTicker("start");
				}
				if (contatore_partite > 0) {
					$j(".contenitore_squadre").css("display", "block");
					if (device.mobile()) {
						doMobile();
					}
				}
				else {
					$j(".contenitore_squadre").css("display", "none");
				}
			}
		});
	}

	function bySortedValue(obj) {
	    var tuples = [];
	    for (var key in obj) {
	    	tuples.push([key, obj[key]]);
	    }
	    tuples.sort(
	    	function(a, b) {
	    		return a[1] < b[1] ? -1 : a[1] > b[1] ? 1 : 0 
	    	}
	    );
	    return tuples; 
	}


	function update(numeroPartite, data_array, counter){
		if (numeroPartite > 11) {
			numeroPartite = 10;
		}
		$j(".contenitore_squadre").attr("class", "contenitore_squadre formato_" + numeroPartite);
		var stile = (counter % 2 == 0) ? "blocco_scuro" : "blocco_chiaro";
		var blocco = creaBlocco(stile, data_array, numeroPartite);
		if (blocco != null){
			blocco.click(function(){
				location.href = "http://calcio.lastampa.it/?maid=" + data_array.matchID + "&Cust=12002&lang=10&ctid=" + ctid + "&cpid=" + cpid + "&pSTR=match_details&inCup=Y&rid=30";
			});
			$j("#blocco_wrapper").append(blocco);
		}
	}
	
	function creaBlocco(stile, blocco_partita, numeroPartite){
		if (blocco_partita.ddata == undefined) {
			return;
		}
		blocco_partita.squadra_uno_nome = blocco_partita.squadra_uno_nome.replace("-", " ");
		blocco_partita.squadra_due_nome = blocco_partita.squadra_due_nome.replace("-", " ");
		var len = (blocco_partita.events.length > 4) ? 4 : blocco_partita.events.length;
		var start = (blocco_partita.events.length > 4) ? blocco_partita.events.length - 4 : 0;
		if (len > 0 || blocco_partita.status != "") {
			ticker += "<li class='news-item'><strong>" + blocco_partita.squadra_uno_nome + " - " + blocco_partita.squadra_due_nome + "</strong>";
			ticker += " : ";
		}
		for (var i = 0; i < len; i++) {
			var evento = blocco_partita.events[i+start];
			if (evento != null) {
				if (i > 0) {
					ticker += ", ";
				}
				team = (evento.team == "A") ? blocco_partita.squadra_uno_nome : blocco_partita.squadra_due_nome;
				ticker += evento.time + "' <span class='evt_" + evento.type + "'>&nbsp;</span> " + ((evento.type == "OG")?"Autogol ":"") + evento.player + " (" + team + ")";
			}
		}
		if (len > 0 || blocco_partita.status != "") {
			ticker += "</li>";
		}
		
		var blocco_squadre 	= $j("<div class=\"blocco_squadre\" >");
		var blocco_sfumino 	= $j("<div class=\"blocco_sfumino " + stile + "\" >");
		var blocco_squadre_	= $j("<div class=\"blocco_squadre_\">");
		blocco_squadre.append(blocco_sfumino, blocco_squadre_);

		//SQUADRA UNO
		var squadra_uno 	= $j("<div class=\"squadra_uno\">");
		var img_squadra 	= $j("<div class=\"img_squadra\">");
		if (blocco_partita.squadra_uno_img != "no") {
			var img_tag 		= $j("<img data-src=\"" + blocco_partita.squadra_uno_img + "\" alt=\"\">");
		}
		else {
			var img_tag 		= $j("<div style=\"float:left;\">");
		}	
		var nome_squadra 	= $j("<div class=\"nome_squadra\"><div>" + blocco_partita.squadra_uno_nome + "</div></div>");
		var goal_squadra 	= $j("<div class=\"goal_squadra\"><div>" + ((blocco_partita.status == "") ? "-" : blocco_partita.reti_squadra_a) + "</div></div>");
		
		var clear_b_uno 	= $j("<div class=\"clear_b\">");
		
		img_squadra.append(img_tag);
		squadra_uno.append(img_squadra, nome_squadra, goal_squadra, clear_b_uno);

		//SQUADRA DUE
		var squadra_due 		= $j("<div class=\"squadra_due\">");
		var img_squadra_due 	= $j("<div class=\"img_squadra\">");
		if (blocco_partita.squadra_due_img != "no") {
			var img_tag_due 		= $j("<img data-src=\"" + blocco_partita.squadra_due_img + "\" alt=\"\">");
		}
		else {
			var img_tag_due 		= $j("<div style=\"float:left;\">");
		}	
		
		var nome_squadra_due 	= $j("<div class=\"nome_squadra\"><div>" + blocco_partita.squadra_due_nome + "</div></div>");
		var goal_squadra_due 	= $j("<div class=\"goal_squadra\"><div>" + ((blocco_partita.status == "") ? "-" : blocco_partita.reti_squadra_b) + "</div></div>");
		
		var clear_b_due 	= $j("<div class=\"clear_b\">");
		
		img_squadra_due.append(img_tag_due);
		squadra_due.append(img_squadra_due, nome_squadra_due, goal_squadra_due, clear_b_due);

		//BLOCCO ORARIO
		var blocco_orario = $j("<div class=\"blocco_orario\">");
//		console.log("blocco_orario => " + blocco_partita.ddata)
		var hh = blocco_partita.ddata.getHours();
		hh = hh < 10 ? "0" + hh : hh;
		var mm = blocco_partita.ddata.getMinutes();
		mm = mm < 10 ? "0" + mm : mm;
		var dd = blocco_partita.ddata.getDate();
		dd = dd < 10 ? "0" + dd : dd;
		var MM = blocco_partita.ddata.getMonth();
		MM = MM < 10 ? "0" + (MM+1) : (MM+1);
//		console.log("MM => " + MM)
		blocco_orario.html(dd + "/" + MM + " - " + hh + ":" + mm);
		
		//BLOCCO STATO
		var blocco_stato = $j("<div class=\"blocco_stato\">");
		var stato = "";
		var color = "";
		switch (blocco_partita.status) {
			case "Pst" :
				stato = "rinviata";
				break;
			case "Ssp" :
				stato = "sospesa";
				break;
			case "1T" :
				stato = "1° tempo";
				break;
			case "HT" :
				stato = "intervallo";
				break;
			case "2T" :
				stato = "2° tempo";
				break;
			case "FT" :
				stato = "finale";
				color = "black";
				break;
			case "AET" :
				stato = "dopo t.s.";
				color = "black";
				break;
			case "ET" :
				stato = "supplementari";
				break;
			case "PEN" :
				stato = "c. di rigore";
				break;
			case "11M" :
				stato = "dopo c.d.r";
				color = "red";
				break;
			default :
				stato = "&nbsp;";
				break;
		}
		if (color != "") {
			blocco_stato.css("color", color);
		}
		blocco_stato.html(stato);
		
		//SEPARATORE RISULTATI
		if(numeroPartite < 4){
			var separatore = $j("<div class=\"separatore_goal\">");
			separatore.html("<div>&nbsp;-&nbsp;</div>");
			blocco_squadre_.append(blocco_stato, squadra_uno, separatore, squadra_due, blocco_orario);
		}else{
			blocco_squadre_.append(blocco_stato, squadra_uno, squadra_due, blocco_orario);
		}

		return blocco_squadre;
	}
	
	var mesi = {
		"gennaio":0,
		"febbraio":1,
		"marzo":2,
		"aprile":3,
		"maggio":4,
		"giugno":5,
		"luglio":6,
		"agosto":7,
		"settembre":8,
		"ottobre":9,
		"novembre":10,
		"dicembre":11
	};
	
	function doMobile() {
		var cBase = $j(".contenitore_squadre");
 		cBase.removeClass();
 		cBase.addClass("contenitore_squadre swiperL formato_M");

		if(cBase.find(".blocco_squadre").length > 0) {
			cBase.swiper({
					type:"swiper",
					content:$j(".contenitore_squadre #blocco_wrapper"),
					container:$j(".contenitore_squadre #blocco_wrapperContainer"),
					itemSelector:".blocco_squadre"
				}
			);
		}
	}

	activate();
//	console.log(mm_section);

	$j(".topTitle").click(
		function() {
			setChamp($j( this ).attr("id"));
		}
	);

	if (typeof mm_section === 'undefined') {
		$j("#topcalcio").css("display", "none");
	}
	else {
		if (mm_section != "/" && mm_section != "/homepage-elezioni" && mm_section != "/sport") {
			$j("#topcalcio").css("display", "none");
		}
	}

</script>
<!-- apertura_dueterzi_unterzo -->
