		<!-- dummy: Cronaca --><div class="ls-apertura">
	<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,700italic,400,300,700' rel='stylesheet' type='text/css'>
<link href="/modulo/sport/calcio/box-risultati-partite/css/ticker-style.css" rel="stylesheet" type="text/css" />
<script src="/common/js/jquery/plugins/newsTicker/js/jquery.newsTicker.min.js" type="text/javascript"></script>

