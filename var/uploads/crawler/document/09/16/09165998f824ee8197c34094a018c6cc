<script>
function judge_days(){
	//days for select
	$("#day>option[value='29']").hide();
	$("#day>option[value='30']").hide();
	$("#day>option[value='31']").hide();
	if (parseInt($('#month').val()) == 2) {
		if ((parseInt($('#year').val()) % 4 == 0) && (parseInt($('#year').val()) % 100 != 0)) {
			$("#day>option[value='29']").show();
			if ($("#day").val() > 29){
				$("#day").val('');
			}
		} else {
			$("#day>option[value='29']").hide();
			if ($("#day").val() >= 29){
				$("#day").val('');
			}
		}
	} else {
		$("#day>option[value='29']").show();
		var cur_month = $('#month').val();
		if (cur_month == 1 || cur_month == 3 || cur_month == 5 || cur_month == 7 || cur_month == 8 || cur_month == 10 || cur_month == 12) {
			$("#day>option[value='30']").show();
			$("#day>option[value='31']").show();
		} else {
			$("#day>option[value='30']").show();
			if ($("#day").val() > 30){
				$("#day").val('');
			}
		}
	}
}
</script>
<script>


	function refresh_captcha(img_id) {
	var img_ins = document.getElementById(img_id);
	// get a different num every time, use Date()
	var d = new Date();
	var pars = d.getMilliseconds();
	img_ins.src = '/captcha?when=' + pars;
	return true;
	}

	/* language */
	var domain_setting = 'en';
	var region_en2 = {
	'AU':'Australia',
	'CA':'Canada',
	'IE':'Ireland',
	'NZ':'New Zealand',
	'ZA':'South Africa',
	'UK':'United Kingdom',
	'US':'United States'
	};
	function sel_lang(region){
	if(region == domain_setting) return;
	var cur_url = window.location.href;
	var new_url;
	var real_domain_setting;
	if (domain_setting == 'en2'){
	domain_setting = 'en';
	real_domain_setting = 'en2';
	}
	if(region_en2[region]){
	new_url=cur_url.replace("http://"+domain_setting+".","http://en.");
	if(new_url == cur_url && !real_domain_setting){
	new_url = new_url.replace("http://","");
	new_url = new_url.replace(/^www\./,"");
	new_url = "http://en." + new_url;
	}
	}else if(region =='en'){
	new_url=cur_url.replace("http://"+domain_setting+".zorpia.com","http://www.zorpia.com");
	if(new_url == cur_url){
	new_url=new_url.replace("http://"+domain_setting+".","http://");
	new_url=new_url.replace(".zorpia.cn",".zorpia.com");
	}
	}else if(region =='cn'){
	new_url=cur_url.replace("http://"+domain_setting+".zorpia.com","http://www.zorpia.cn");
	if(new_url == cur_url){
	new_url=new_url.replace("http://"+domain_setting+".","http://");
	new_url=new_url.replace(".zorpia.com",".zorpia.cn");
	}
	}else{
	new_url=cur_url.replace("http://"+domain_setting+".","http://"+region+".");
	if(new_url == cur_url){
	new_url = new_url.replace("http://","");
	new_url = new_url.replace(/^www\./,"");
	new_url = "http://" + region + "." + new_url;
	}
	}
	if(region == 'cn'){
	new_url = new_url.replace("zorpia.com","zorpia.cn");
	}else{
	new_url = new_url.replace("zorpia.cn","zorpia.com");
	}
	if(new_url.indexOf('?')>-1){
	var start = new_url.indexOf('site_switch');
	if(start == -1){
	new_url = new_url + '&site_switch=' + region;
	}else{
	new_url = new_url.substr(0 ,start);
	new_url = new_url + 'site_switch=' + region;
	}
	}else{
	new_url = new_url + '?site_switch=' + region;
	}
	if(real_domain_setting){
	domain_setting = real_domain_setting ;
	}
	//alert(new_url);
	window.location.href=new_url;
	}

</script>
