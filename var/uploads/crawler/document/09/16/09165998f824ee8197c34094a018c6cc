<script type="text/javascript">
			//
			$(document).ready(function(){
				//alert("test");
				function dynanimate($object, $delay, $speed, $startX, $startY, $endX, $endY){
					$object.show().stop().css({
						"opacity":"0",
						"left":$startX+"px",
						"top":$startY+"px"
					});
					$object.delay($delay).animate({
						opacity:1,
						left:$endX,
						top:$endY
					},$speed);
				};
				var $note3 = $("#note3");
				var $iphone5 = $("#iphone5");
				var $macbook = $("#macbook");
				$(window).load(function(){
					dynanimate($note3,300,500,0,30,130,30);
					dynanimate($iphone5,500,500,-150,50,-40,50);
					dynanimate($macbook,800,700,520,80,220,80);
					dynanimate($("#rwd_slogan"),1000,500,0,400,0,350);
					dynanimate($("#rwd_slogan span"),1300,400,0,30,0,0);
				});
			});
		</script>
