       <!-- END: popup box -->

<script type="text/javascript">
var price_dlr_st = 0;
var flag_name_1 = 'no_popup_buy_delivery_status';
window[flag_name_1] = 0;
var dlr_st_no_popup = 0;

function no_pop_up(check_sta) {
    dlr_st_no_popup = check_sta ;
    var no_popup = 0;
    if (check_sta){
        no_popup = 1;
    } 
    jQuery.ajax({
        type: "get",
        url: "/whoviewedme/no_popup",
        data: {"no_popup":no_popup},
        cache: false,
        success: function(data, textStatus){
        },
        error: function(){
            alert(ZORPIA_JS_LANG.ENCOUTER_SOME_ERROR);
        }
    });

};

function buy_pp() {
    document.getElementById('pop_close2').onclick();
    var user_id = document.form_popup2.user_id.value;
    buy_delivery_status2(user_id);
};


function popup_buy_delivery_status2(user_id) {
    user_id |= cur_user_id;
    if (!user_id){
        alert('you need login first!');
        return;
    }
jQuery.get("/click_put_me_here");
    document.form_popup2.user_id.value=user_id;
	jQuery('#user_head_img2')[0].src = user_headpp[user_id].replace('tns','ths');
    jQuery('#ppdst_user_balance').html(jQuery('#box_credit_balance').html());
	jQuery('#ppdst_item_desc').html("Promote yourself at a prominent position on My Zorpia and Search Results. You will get more homepage views and friends! ");
    if(dlr_st_no_popup || window[flag_name_1] || parseInt( jQuery('#box_credit_balance').html(), 10) < 500)
        buy_delivery_status2(user_id);
    else 
        setTimeout("showPopupBox('popupWrapR2', 'popupBgR2')",500);
}

function buy_delivery_status2(user_id) {
//     window.location="/search/=&pre_url=http%3A%2F%2Fzorpia.com%2F";
//     return;
    var dlr_box = jQuery('#user' + user_id);    var url = '/search/buy_pp';
    var pars = {"user_id":user_id, "is_ajax":1,'country_id':'', 'pre_url': "http%3A%2F%2Fzorpia.com%2F"};
    jQuery.get(url, pars,
    function(data) {
        //alert(data);
        if (data){
            var msg ;
            switch(data) {
                case 'OK':
                    showPopupBox('buy_pp_ok', 'buy_pp_ok_Bg');
                    break;                
                case 'BAD_REQUEST':
                    msg = ZORPIA_JS_LANG.BAD_REQUEST;
                    break;
                case 'OBJECT_NOT_EXIST':
                    msg = ZORPIA_JS_LANG.OBJECT_NOT_EXIST;
                    break;
                case 'BOUGHT_BEFORE':
                    msg = 'Sorry, but you have bought it before.';
                    break;
                case 'NO_ENOUGH_CREDIT':
					var credit_balance = parseInt( jQuery('#box_credit_balance').html(), 10);
					document.pending_payment.pending_object_type.value = 'pp';
					document.pending_payment.pending_ojbect_id.value=user_id;
					document.pending_payment.buyer_user_id.value = '';
					document.pending_payment.buyer_username.value = '';
					document.pending_payment.price.value = '500';
					document.pending_payment.object_owner_id.value = '';
					jQuery('#pop_item_title').html('Featured Zorpian Slot');
					jQuery('#pop_user_balance').html(jQuery('#box_credit_balance').html());
					jQuery('#pop_item_price').html(500);
					jQuery('#pop_coin_need').html(credit_balance > 500 ? 500 : 500 - credit_balance);
					document.getElementById('pop_img_src').src=user_headpp[user_id].replace('tns','ths');
					jQuery('#pop_item_desc').html("Promote yourself at a prominent position on My Zorpia and Search Results. You will get more homepage views and friends! ");
					showPopupBox('popupWrap_pl', 'popupBg_pl');
                    return;
                    break;
                case 'SERVER_ERROR':
                    msg = ZORPIA_JS_LANG.ENCOUTER_SOME_ERROR;
                    break;
             }
             
            if(msg) {
                alert(msg);
                return;
            }
			window.location.reload(true);
			
        }
    }
    );

}
</script>

