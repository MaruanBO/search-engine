jQuery(document).ready(function($) {
jQuery.validator.addMethod("string", function(value, element) {
return this.optional(element) || /^[a-zA-Z]\w+$/.test(value);
}, "Invalid String.");
// fix default to validate once onblur
jQuery.validator.setDefaults( {
//debug:true,
onfocusout: function(element) {
if ( !this.checkable(element) ) {
this.element(element);
}
},
onkeyup: function(element) {
if ( element.name in this.submitted || element == this.lastElement ) {
if (element.type != 'textarea') {
this.element(element);
}
var tip_box = $('#tip_' + element.id);
if (tip_box && tip_box.text()) {
tip_box.hide();
//element.removeClass("intError");
}
}
},
nsubmit:true,
highlight: function(element, errorClass) {
	//alert(element.id);
	$(element).addClass("intError");
},
unhighlight: function(element, errorClass) {
	$(element).removeClass("intError");
},
errorLabelContainer: $("#errorInfo"),

submitHandler:function(form){
form.submit();
}
} );

function merger(a,b) {
for( i in a ) {
for( j in b ) {
if (i==j) {
for( k in b[j] ) {
a[i][k] = b[j][k];
}
} else {
if (! a[j]) {
a[j] = b[j];
}
}
}
}
}
// get all input validate stuff
var rules = {};
var messages2 = {};
$('*[validate]').each(function(i) {
var validate = $(this).attr('validate');
var name = $(this).attr('name');
rules[name] = {};
// split with space
var rules_array = validate.split("##");
if ( rules_array.length < 2 ) {
rules_array = validate.split("#");
}

var part_num = 0;
while (part_num < rules_array.length) {
var lastPart = rules_array[part_num];
part_num += 1;
//check if that's a JSON
if (lastPart.match('{')) {
var rule;
eval("rule = " + lastPart + ";");
for (i in rule) {
if (i == 'message' || i == 'custom_message') {
messages2[name] = {};
for (j in rule[i]) {
messages2[name][j] = rule[i][j];
}
} else {
rules[name][i] = rule[i];
}
}
} else {
rules[name][lastPart] = true;
}
}
} );
// get custom messages
if (typeof(form_id) == 'undefined') {
form_id = 'signupForm';
}
var messages = {
	first_name:{
		required:'First Name is Required.',
		maxlength:$.validator.format("First Name requires no more than {0} characters."),
		string:'First Name contains invalid characters.'
	},
	last_name:{
		required:'Last Name is Required.',
		maxlength:$.validator.format("Last Name requires no more than {0} characters."),
		string:'Last Name contains invalid characters.'
	},
	email:{
		required:'Email is Required.',
		email:'Invalid email address.',
		maxlength:$.validator.format("Email requires no more than {0} characters.")
	},
	password:{
		required:'Password is Required.',
		minlength:"Password requires at least 6 characters.",
	},
	country:{
		required:'Country is Required.'
	},
	city:{
		required:'City is Required.'
	},
	YYYY:{
		required:'Birthday (year) is Required.'
	},
	MM:{
		required:'Birthday (month) is Required.'
	},
	DD:{
		required:'Birthday (day) is Required.'
	},
	captcha:{
		required:'Verify Code is Required.',
		digits:'Verify code you entered was incorrect.'
	}
};
if (typeof(messages) != 'undefined') {
merger(messages2, messages);
}

var validate_params = {
rules: rules,
messages: messages,
errorClass: 'error_Info',
errorElement: 'div'
};
if (typeof(errorPlacement) != 'undefined') {
validate_params["errorPlacement"] = errorPlacement;
}
if (typeof(submitHandler) != 'undefined') {
validate_params["submitHandler"] = submitHandler;
}
// validate myForm form on keyup and submit
var validator = $("#" + form_id).validate( validate_params );
});
