$(document).ready(function() {
    /* $(".popup").colorbox(); */
    $(".popup").colorbox({width:550, height:350, iframe:true});
    $("#searchbox").keyup(function(event) {
        if (event.keyCode == 13) {
            var skey = $(this).val();
            window.location = 'http://karmatube.org/videos.php?op=filter&cat='+skey;
        }
    });
    $("#searchbutton").click(function(event) {
         var skey = $("#searchbox").val();
         window.location = 'http://karmatube.org/videos.php?op=filter&cat='+skey;
    });
    $("embed").attr("wmode", "transparent").wrap('<div>'); 
});
</script>

