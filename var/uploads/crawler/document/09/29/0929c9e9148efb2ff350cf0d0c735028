         var td0 = document.createElement('td');
         var td1 = document.createElement('td');

         tr0.appendChild(td0);
         tr0.appendChild(td1);

         table.appendChild(tr0);
         var searchChild = document.getElementById('searchChild');
         var SearchString = document.createElement('input');
         var button3 = document.createElement('input');
         var button4 = document.createElement('input');
         var button0 = document.createElement('input');

         SearchString.id = "SearchString";
         SearchString.type = "text";
         SearchString.placeholder = "Find furniture and decor for your home...";
         if (device.mobile()) {
             var searchHolder = document.getElementById('searchHolderM');
             table.style.background = "#fff";
             table.style.border = "1px solid #ccc";
             table.style.width = "100%";
