             $("#button4").click(
             function () {
                 $("#SearchString").val("");
                 $("#SearchString").focus();
                 return false;
             });
             $("#SearchString").on("input",
             function () {
                 if ($("#SearchString").val() == "") {
                     $("#button4").hide();
