<script type="text/jscript">
	function onBannerMouseOver(a) {
		var pos = $(a).css('backgroundPosition').split(' ');
		var xPos = pos[0];
		var yPos = '51px';

		$(a).css('background-position', xPos + ' ' + yPos);
	}

	function onBannerMouseOut(a) {
		var pos = $(a).css('backgroundPosition').split(' ');
		var xPos = pos[0];
		var yPos = '0px';

		$(a).css('background-position', xPos + ' ' + yPos);
	}
</script>
