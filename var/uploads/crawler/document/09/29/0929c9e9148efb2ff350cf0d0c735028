                 else {
                     $("#button4").show();
                 }
             });
             $("#button4").hide();
         }
     }

     function goCategorySearch() {

         var searchText = document.getElementById('SearchString').value;
         searchText = searchText.replace(/\./g, ' ')
         searchText = searchText.replace(/&/g, ' ')
         searchText = searchText.replace(/\+/g, ' ')
         searchText = searchText.replace(/\$/g, ' ')
         searchText = searchText.replace(/"/g, ' ')
         searchText = searchText.replace(/,/g, ' ')
         searchText = searchText.replace(/\(/g, ' ')
         searchText = searchText.replace(/\)/g, ' ')
         searchText = searchText.replace(/\'/g, ' ')
         searchText = searchText.replace(/ and /g, ' ')
         searchText = searchText.replace(/\>/g, ' ')
         searchText = searchText.replace(/\</g, ' ')
         searchText = searchText.replace(/\//g, ' ')

         while (searchText.indexOf('  ') >= 0) {
             searchText = searchText.replace('  ', ' ');
         }
         searchText = trim(searchText)
         searchText = searchText.replace(/ /g, '-')
         queryString = '?q=' + searchText; //Added to support indexing for search engines
         searchText = searchText + '--C0.htm'
         window.location.href = window.location.protocol + '//' + window.location.host + '/' + searchText + queryString;
         return false;
     }

     function trim(stringToTrim) {
         return stringToTrim.replace(/^\s+|\s+$/g, "");
     }



     function createQueryString() {

         currUrl = window.location.pathname.substring(1)
         if (currUrl.indexOf('--C') < 0) {
             if (device.mobile())
                 return 'Enter keyword, then tap on search image';
             else
                 return '';
         }

         currUrl = currUrl.split('--C')[0];
         currUrl = currUrl.replace(/-/g, ' ');
         currUrl = currUrl.replace(/%20/g, ' ');
         if (currUrl.indexOf('common/') == 0) {
             currUrl = currUrl.replace('common/', '');
         }
         if (currUrl.indexOf('NoBot/') == 0) {
             currUrl = currUrl.replace(/NoBot\//g, '');
         }

         return currUrl;
     }

     document.getElementById("SearchString").value = createQueryString();
</script>
