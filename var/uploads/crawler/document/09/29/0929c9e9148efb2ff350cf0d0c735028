<script type="text/javascript">

    function ReviewShow() {
        $('#review').fadeIn(1000);
        document.getElementById("rEmail").focus();
    }

    function ReviewHide() {
        $('#review').fadeOut(1000);
    }

    function txtDescChange() {
        document.getElementById("rNumWords").innerText = document.getElementById("rStory").value.replace('    ', ' ').replace('   ', ' ').replace('  ', ' ').split(' ').length;
        document.getElementById("rNumWords").textContent = document.getElementById("rStory").value.replace('    ', ' ').replace('   ', ' ').replace('  ', ' ').split(' ').length;
        //alert(document.getElementById("rStory").value.split(' ').length);
    }

    function formReviewValidate() {
        if (document.getElementById("rEmail").value == "") {
            document.getElementById("msgReview").innerHTML = "Email is required!";
            document.getElementById("rEmail").focus();
            return false;
        }
        if (document.getElementById("rStory").value.replace('    ', ' ').replace('   ', ' ').replace('  ', ' ').split(' ').length < 250) {
            document.getElementById("msgReview").innerHTML = "At least 250 words required!";
            document.getElementById("rStory").focus();
            return false;
        }
        return true;
    }

</script>
