<script type="text/javascript">
jQuery(document).ready(function(){
    if ("https:" == document.location.protocol) {
	/* secure */
    } else {
	jQuery(".input_search").autocomplete({
	    source: "/qsearch.phtml",
	    minLength: 2,
	    select: function( event, ui ) {
		window.location.href='/'+ui.item.url;
	     }
	});
    }
   
    jQuery('#slider1').bxSlider({
        auto: true,
        autoControls: false,
        controls: false,
        speed:1000,
        pause:10000
    });
    jQuery('#slider2').bxSlider({
        auto: true,
        autoControls: false,
        controls: false,
        speed:1000,
        pause:10000
    });

	jQuery('select#country').change(function () {
		var country = jQuery(this).val();
		if (country == 'US') {
			jQuery('input#international_states').css('display','none');
			jQuery('input#international_states').val('');	
			jQuery('select#usa_states').css('display','block');
		} else {
			jQuery('select#usa_states').css('display','none');
			jQuery('input#international_states').css('display','block');
		}
	}).trigger('change');
	
	jQuery('select#ship2country').change(function () {
		var country = jQuery(this).val();
		if (country == 'US') {
			jQuery('input#ship2international_states').css('display','none');
			jQuery('input#ship2international_states').val('');	
			jQuery('select#ship2usa_state').css('display','block');
		} else {
			jQuery('select#ship2usa_state').css('display','none');
			jQuery('input#ship2international_states').css('display','block');
		}
	}).trigger('change');
	
	jQuery('select#bill2country').change(function () {
		var country = jQuery(this).val();
		if (country == 'US') {
			jQuery('input#bill2international_states').css('display','none');
			jQuery('input#bill2international_states').val('');	
			jQuery('select#bill2usa_state').css('display','block');
		} else {
			jQuery('select#bill2usa_state').css('display','none');
			jQuery('input#bill2international_states').css('display','block');
		}
	}).trigger('change');
});
</script>
