  background: url("http://s3.golfballs.com/images/gbc2/widget-blog/lc-blog-masthead_3.png") 0 0 no-repeat;
  width: 121px;
  height: 72px;
  margin: 1em auto .8em auto;
  display: block;
}
#blog-box h2 {
  text-align: center;
  border: none;
  letter-spacing: 1px;
  text-transform: uppercase;
  margin: 0;
}
#blog-box #divRss {
  text-align: center;
}
#blog-box #divRss ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
}
