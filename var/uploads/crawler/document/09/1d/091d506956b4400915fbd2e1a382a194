  display: block;
  text-align: center;
  text-decoration: none;
  padding: 3px;
  margin: 0 20px;
  -webkit-border-radius: 30px;
  -webkit-background-clip: padding-box;
  -moz-border-radius: 30px;
  -moz-background-clip: padding;
