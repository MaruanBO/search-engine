<script type="text/javascript">
    function search()
    {
        location.href = baseURL + 'Golfballs/Page.aspx?key=search&search=' + $('mainSearchBox').value;
    }

    function searchBoxClicked()
    {
        if ($('mainSearchBox').value == 'Search')
        {
            $('mainSearchBox').value = '';
        }
    }

    function searchBoxBlur()
    {
        if ($('mainSearchBox').value == '')
        {
            $('mainSearchBox').value = 'Search';
        }
    }

    function searchAuto(input, obj) {
        if (input.keyCode == 13) { // enter
            input.returnValue = false;
            input.cancel = true;
            eval('search()');
        } else if (input.keyCode == 9) { // tab
            obj.value = obj.value + ' ';

            //alert('tab' + obj.value);
            //input.returnValue = false;
            // input.cancel = true;
        }
    }

    window.addEvent('domready', function () {
        SearchAutoInit();
        if ($$('.basictip').length > 0) {
            var toolTipsB = new Tips($$('.basictip'), {
                className: 'basic_tip'
            });
        }
    });
</script>
