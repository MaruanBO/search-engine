  border: 1px solid #ccc;
  color: #333;
  background-image: url(data:image/svg+xml;base64,);
  background-image: -webkit-linear-gradient(top, #f7f7f7 0%, #eeeeee 60%);
  background-image: -moz-linear-gradient(top, #f7f7f7 0%, #eeeeee 60%);
  background-image: -o-linear-gradient(top, #f7f7f7 0%, #eeeeee 60%);
