<script>
function setupModal(theproduct){

  var $som = $("#sold-out-modal");
  var prodimg =  theproduct.find('.imagebg').attr('data-img');
  var prodtitle = theproduct.find(".topsection h2 a").html();
  var price = theproduct.find('.inprice').text();
  var listprice =  theproduct.find('.thelistprice').text(); 
  var totalsavings = listprice - price;
  var savings = theproduct.find('.savings').text();
  var catlink = theproduct.find('.product-box').attr('data-catlink');
  $som.find('#som-product-title').text(prodtitle);
  $som.find('#som-product-image').attr('src', prodimg);
  $som.find('.price-target').text('$'+price);
  $som.find('.savings-target').text(savings);
  $som.find('.totalsavings-target').text('$'+parseInt(totalsavings).toFixed(2));
  $som.find('.moredealsbtn').attr('href',catlink);
}

jQuery(document).ready(function($){  
  // $(".sold-out-product a").click(function(e){
  //   e.preventDefault();
  //   setupModal($(this).parents('.sold-out-product'));
  //   $("#sold-out-modal").modal('show');
  // });
  $("#signupmodalcontainer").on('show.bs.modal',function(e){
    if(!$(this).find('iframe').attr('src')){
      $(this).find('iframe').attr('src',$(this).find('iframe').attr('data-src'));
    }
  });
});
</script>


