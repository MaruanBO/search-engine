     <script type="text/javascript">
         var isAdvanced = false;
         function FixVisibility() {
             isAdvanced = !isAdvanced;
             var i = 1;
             var o = document.getElementById("advancedFlip" + i);
             while (o != null) {
                 if (isAdvanced) {
                     o.className = o.className.replace(" invisible", "");

                 }
                 else {
                     o.className = o.className + " invisible";
                 }
                 i++;
                 o = document.getElementById("advancedFlip" + i);
             }
             var e = document.getElementById("advancedExtra");
             if (e != null) {
                 var advancedHeight = (!(document.getElementById("rdoSearchResources").checked)) ? "408px" : "308px";
                 e.style.height = (isAdvanced) ? advancedHeight : "269px";
             }
         }
         //FixVisibility();
         function FixText(city) {
             //var city = document.getElementById("keyword");
             if (city.style.color != 'black') {
                 city.style.color = 'black';
                 city.style.fontStyle = 'normal';
                 city.value = '';
             }
         }
         function ClearText(city, str) {
             //var city = document.getElementById("keyword");
             if (city != null) {
                 if (city.value == '') {
                     city.style.color = 'gray';
                     city.style.fontStyle = 'italic';
                     city.value = 'Enter ' + str;
                 }
             }
         }
         ClearText(document.getElementById("keywordTerm"), "Search Term");
         ClearText(document.getElementById("txtFname"), "First Name");
         ClearText(document.getElementById("txtLname"), "Last Name");
         ClearText(document.getElementById("txtBirth"), "Birth Year");
         ClearText(document.getElementById("txtDeath"), "Death Year");
         ClearText(document.getElementById("txtMarriage"), "Marriage Year");
         ClearText(document.getElementById("txtDivorce"), "Divorce Year");
         ClearText(document.getElementById("city"), "City");
        </script>

