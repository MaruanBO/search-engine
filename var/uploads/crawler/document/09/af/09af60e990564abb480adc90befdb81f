            function radioSearch(id) {
                switch (id) {
                    case 0:
                        {
                            show(document.getElementById("termSearchDiv"), null);
                            hide(document.getElementById("personSearchDiv"));
                            break;
                        }
                    case 1:
                        {
                            show(document.getElementById("personSearchDiv"), null);
                            hide(document.getElementById("termSearchDiv"));
                            break;
                        }
                }
            }
        </script>


