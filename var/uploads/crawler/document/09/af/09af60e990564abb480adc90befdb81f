        <script type="text/javascript">
            function FixSides()
            {
                var mainDiv = $("#mainBarSearch");//var mainDiv = document.getElementById("mainBarSearch");
                var sideDiv = $("#sideBarSearch");//document.getElementById("sideBarSearch");
                if ((mainDiv!=null)&&(sideDiv!=null)) {
                    var height = Math.max(mainDiv.height(), sideDiv.height()); //Math.max(mainDiv.offsetHeight, sideDiv.offsetHeight);
                    $("#mainBarSearch").css("height", null);
                    $("#sideBarSearch").css("height", null);
                    mainDiv.height(height + "px");
                    sideDiv.height(height + "px");
                    var h = 155;
                    var ie7 = (navigator.appVersion.indexOf("MSIE 7.") != -1);
                    if (ie7) {
                        h = 20;
                    }
                    setHeight("totalLength", height + h);
                    setHeight("whiteDiv", height + h);
                    setHeight("cover", height + h);
                }
                try
                {
                    FixLeft();  
                } catch(e)
                {

                } 
            }
            function FixPage()
            {
                var mainDiv = $("#mainBarSearch");//var mainDiv = document.getElementById("mainBarSearch");
                var sideDiv = $("#sideBarSearch");//document.getElementById("sideBarSearch");
                if ((mainDiv!=null)&&(sideDiv!=null))
                {
                    FixSides();
                    var tr = document.getElementById("totalResults");
                    if (tr!=null)
                    {
                        tr.innerHTML = " results found in  seconds";
                    }
                }
                    
                var tf = document.getElementById("txtFname");
                if (tf!=null)
                {
                    if (tf.className.indexOf("noFocus")==-1)
                    {
                        tf.focus();
                    }
                }
                
            }
            setTimeout(FixSides, 3000);
            setTimeout(FixSides, 6000);
            setTimeout(FixSides, 9000);
        </script>
