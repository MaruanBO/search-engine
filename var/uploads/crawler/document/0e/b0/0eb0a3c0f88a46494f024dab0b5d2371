(function(){
	var objcnt = 0;
	var flto = setInterval(function(){
	objcnt++;
	if (typeof FLPXobj == 'object') {
	clearInterval(flto);
	Initflf();
	}
	if(objcnt > 4){
	clearInterval(flto);
	}
	},100);
	function Initflf(){
	FLPXobj.procinit(['464','726']);
	var myto = setInterval(function(){
		clearInterval(myto);
		var status = FLPXobj.pxstatus;
	},400);
	}
})();
</script>

