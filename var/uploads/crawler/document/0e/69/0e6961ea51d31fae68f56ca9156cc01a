	function move_ads(banner_name)
	{
		banner_system=document.getElementById('banner_container_' + banner_name);
		banner_data=document.getElementById('banner_data_' + banner_name);
		banner_system.appendChild(banner_data);
		banner_data.style.visibility='visible';
		banner_system.style.display="block";
	}
</script>

