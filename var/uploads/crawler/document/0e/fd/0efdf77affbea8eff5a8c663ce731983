						<script type="text/javascript">
							var $searchForm = $('#searchForm');
							$searchForm.tooltip({
								html: true,
								placement: 'left',
								trigger: 'manual',
								title: '<span class="icon"> </span><span class="message">Please enter a search term and try your search again</span>',
								style: 'searchError'
							});
							
							//local json test version (requires test file test_json.jsp to function
							/*$("#keyword").autocomplete("/catalog/test_json.jsp", { */

							//test adobe call version
							//		    $("#keyword").autocomplete("http://content.atomz.com/autocomplete/sp10/04/05/03/?callback=?", {
							var spKey = 'sp1004eee9';

							function getACRequest(account){
								var g_staged = ($("#sp_staged") ? $("sp_staged").val() : 0);
								var protocol = (document.location.protocol == "https:" ? "https:" : "http:");
								var postfix = (g_staged ? "-stage/" : "/");
								var acct = account.split("");
								var spid = "";
								var j = 0;
								for (var i = 0; i < acct.length ; i++){
									if (i >= 2){
										j++;
										if (j == 2){
											j = 0;
											spid += (i != (acct.length - 1)) ? acct[i] + "/" : acct[i];
										} else {
											spid += acct[i];
										}
									} else {
										spid += acct[i];
									}
								}
								return protocol + "//content.atomz.com/autocomplete/" + spid + postfix + "?callback=?";
							}

							// Define AdobeAjax Settings
							var ax = {
								account: spKey,
								searchDomain: 'http://' + spKey + '.guided.ss-omtrdc.net/',
								inputElement: "input#keyword",
								inputFormElement: "form#searchForm",
								delay: 100,
								minLength: 1,
								maxResults: 8,
								browserAutocomplete: false,
								submitOnSelect: true,
								queryCaseSensitive: false,
								autoFocus:false,
								appendTo:"",
								startsWith: true,
								highlightWords : true,
								zindex : 997,
								highlightWordsBegin : true,
								focus: function(e, ui) {
									setTimeout(function() {
										var t = $('#ui-active-menuitem').text();
										t = t.replace(/\&amp;reg;/g,"&#174;");
										t = t.replace(/\&amp;trade;/g,"&#8482;");
										$("#keyword").val(t).html();
										$("#keyword").addClass('predictiveSearch');
									}, 25);
								},
								close: function(e, ui) {
									$("#keyword").removeClass('predictiveSearch');
								}
							};

