<script type="text/javascript">
	$( document ).ready(function(){
		// search show/hide
		$("#tNavSrch").mouseenter(function(){
			$("#searchWrapper").slideDown(500);
		});
		$("#tNavSrch").mouseleave(function(){
			setTimeout(function(){
				if($("#searchWrapper").is(":hover")) {
					
				} else {
					$("#searchWrapper").slideUp(500);
					$("#ui-id-1").hide();
				}
			}, 200);
		});
		$("#searchWrapper").mouseleave(function(){
			setTimeout(function(){
				if($("#tNavSrch").is(":hover") || $("#ui-id-1").is(":hover")) {
					
				} else {
					$("#searchWrapper").slideUp(500);
					$("#ui-id-1").hide();
				}
			}, 200);
		});
		$("#ui-id-1").mouseleave(function(){
			setTimeout(function(){
				if($("#searchWrapper").is(":hover")) {
					
				} else {
					$("#searchWrapper").slideUp(500);
					$("#ui-id-1").hide();
				}
			}, 200);
		});
		$("#cssdropdown li").mouseenter(function(){
			$("#searchWrapper").slideUp(500);
			$("#ui-id-1").hide();
		});
	});
