<script> 
	var custserv_url_value = String(window.location); 
	var patt2=/custserv/gi; 
	var custserv_category_value = custserv_url_value.match(patt2);
	if (custserv_category_value != null){
		if (custserv_category_value[0] == "custserv"){ 
			$("#forcustomerserviceonly").css("display","block"); 
			$(".universalNavBacking").css('background','url("/assets/lb/assets/images/cms/customer_service/LBBG_footer_CSV2.png") repeat-x scroll left bottom');
			$(".universalNavBacking").css('height','396px');				
		} 
	}
</script>

<script>
$(document).ready(function(){
	var usePersistentNav = 1;	
	var checkout_url_value = String(window.location); 
	var checkout_pattern_string=/checkout/gi; 
	var checkout_category_value = checkout_url_value.match(checkout_pattern_string);
	if (checkout_category_value != null){
		if (checkout_category_value[0] == "checkout"){ 
			usePersistentNav = 0;
		} 
	}
	if (usePersistentNav != 0 && navigator.platform.indexOf("iPad") == -1 && navigator.platform.indexOf("iPhone") == -1){
		var emailsignupscrolltop;
		var widthWindow = $(window).width();
		var getPersistentMargin = ((widthWindow - 974)/2);
		$("#persistentNavigation div.persistentUnivTabs").css("left",getPersistentMargin);
		//$("#persistentNavigation div.persistentNav").css("left",getPersistentMargin);	
		$("#persistentNavigation div.persistentNavigationMiniCArt").css("left",getPersistentMargin+745);		
	
		var getGlobalNavHTML = $("#cssdropdown").html();
		var getMiniCart = $("div#headerWrapperInner div.cart-info-container").html();
		var persistentNavigationShowing = 0;
		$("#persistentNavigation .persistentNav").html('<div id="globalNav2"><ul class="horiz" id="cssdropdown2">'+getGlobalNavHTML+'</ul></div>');
		$(".persistentNavigationMiniCArt").html(getMiniCart);
		
		var getPersistentHolder = $("#moveToBody").html();
		$("body").append(getPersistentHolder);
		$("#moveToBody").html("");
	
		$(window).scroll(function(){
			emailsignupscrolltop= $(window).scrollTop();
			if (emailsignupscrolltop > 140){			
				widthWindow = $(window).width();
				getPersistentMargin = ((widthWindow - 974)/2);
				$("#persistentNavigation div.persistentUnivTabs").css("left",getPersistentMargin);
				//$("#persistentNavigation div.persistentNav").css("left",getPersistentMargin);	
				$("#persistentNavigation div.persistentNavigationMiniCArt").css("left",getPersistentMargin+745);					
				$('#persistentNavigation').css("display","block");
				if (persistentNavigationShowing != 1){
					persistentNavigationShowing = 1;			
					$('#persistentNavigation').animate({
						top: '+=75',
					}, 500,function(){
						$('#persistentNavigation').css("top","0px");
					});
				}
			}else{
				if (persistentNavigationShowing != 0){
					persistentNavigationShowing = 0;
					$('#persistentNavigation').animate({
						top: '-=75',
					}, 100,function(){
						$('#persistentNavigation').css("top","-75px");
						$('#persistentNavigation').css("display","none");
					});
				}
			}
		});
		
		$(window).resize(function() {
			emailsignupscrolltop= $(window).scrollTop();
			if (emailsignupscrolltop > 140){			
				widthWindow = $(window).width();
				getPersistentMargin = ((widthWindow - 974)/2);
				$("#persistentNavigation div.persistentUnivTabs").css("left",getPersistentMargin);
				//$("#persistentNavigation div.persistentNav").css("left",getPersistentMargin);	
				$("#persistentNavigation div.persistentNavigationMiniCArt").css("left",getPersistentMargin+745);					
				$('#persistentNavigation').css("display","block");
				if (persistentNavigationShowing != 1){
					persistentNavigationShowing = 1;			
					$('#persistentNavigation').animate({
						top: '+=75',
					}, 500,function(){
						$('#persistentNavigation').css("top","0px");
					});
				}
			}else{
				if (persistentNavigationShowing != 0){
					persistentNavigationShowing = 0;
					$('#persistentNavigation').animate({
						top: '-=75',
					}, 100,function(){
						$('#persistentNavigation').css("top","-75px");
						$('#persistentNavigation').css("display","none");
					});
				}
			}
		});
		if($('#cssdropdown2 li.headlink').length > 0) {
			 $('#cssdropdown2 li.headlink').hoverIntent({
				sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)
				interval: 25,   // number = milliseconds for onMouseOver polling interval
				over: showPersistNavSubLink,     // function = onMouseOver callback (required)
				timeout: 100,   // number = milliseconds delay before onMouseOut
				out: hidePersistNavSubLink       // function = onMouseOut callback (required)
				});
		 }
	}
});
	
 function showPersistNavSubLink() {
	 $('#contflyzoom').remove();

	 var sublink = $(this).children(".sublink");
	 if( sublink.children().size() > 0 )
	 {
			sublink.slideDown();
	 }
 }

 function hidePersistNavSubLink() {
	 var menu = $(this);
	 menu.children(".sublink").slideUp();
 }
</script>

