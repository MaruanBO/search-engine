<script type="text/javascript">
	var	EmailSignUpTop = ($(window).height()-230)/2;	
	var	EmailSignUpleft = ($(window).width()-360)/2;		
	var fromsonsiemail = "NO";
	var secondarySourceID=56;
	
	$(document).ready(function(){
		$("body").append('<div id="emailSignUpPopupBackground" onClick="closeEmailSignUp()"></div>');
		$("body").append('<div id="internationalModalBackground" onClick="internationalModalClose()"></div>');
		$("#headerEmailSignUp").css("display","block");	
	});	
	
	function donothing (){
		var donothing = 0;
	}
	
	function callEmailLightBox() {
	   //Validate that email and reentered email match
	   var email = $("#emailLightBoxSubscribeForm input[name=emailLightBoxAddress]").val();
	   var reEnteredEmail = $("#emailLightBoxSubscribeForm input[name=emailLightBoxReAddress]").val().trim();
	   if(reEnteredEmail == '' || reEnteredEmail != email){
		  $("#emailLB_error").css("display","block");
	      $("#emailLB_error").html("<div class=\"error\">Email address entries do not match. Please try again.</div>");
	      return false;
	   } else {
			$("#emailLB_form").css("display","none");					 
			$("#emailLightBoxStatus").css("display","block");
	      	var formAction = $("#emailLightBoxSubscribeForm").attr("action");
      		$("#emailLightBoxStatus").html("Saving...").load(formAction,{"emailAddress":email,
	         "reEnterEmailAddress":reEnteredEmail,
	         "unSubscribe":false,
	         "fromFooter":false,
	         "secondarySourceCode":secondarySourceID,
	         "userEmailOptionFlags":10}
			  , function(){
			  if ($('#emailLB_error').text().length > 1) {
					//email validation failed - show input
					$('#emailLB_form').css("display","block");
					$("#emailLightBoxStatus").css("display","block");					
			  }else{
				  $("#emailLB_thanks").fadeIn('slow', function() {	
					$("#emailLB_thanks").delay(5000).fadeOut('fast', function() {
						closeEmailSignUp();
					});									 
				});
			  }
			  }
			  );
	   }
	   return false;
	}
	
	function checkEmail(email) {
	   if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email)){
	      return true;
	   }
	   return false;
	}		
			
	function closeEmailSignUp(){
		$("#emailSignUpPopupContainer").css("display","none");
		$("#emailSignUpPopupBackground").css("display","none");
	}
	
	function openEmailSignUp(passSecondarySourceID){
		secondarySourceID=passSecondarySourceID;		
		$('#emailLightBoxSubscribeForm').get(0).reset();  
		$('form[name=emailLightBoxSubscribeForm]').get(0).reset(); 
	    $("#emailLB_error").html("");       //clear off any old msgs
		$('#emailLB_form').css("display","block");
		$('#emailSignUpPopupContainer').css("display","block");		
		$('#emailLB').css("display","block");
		$("#emailLightBoxSubscribeForm").css("display", "block");
		$("#emailSignUpPopupBackground").css("height",$(window).height());
		$("#emailSignUpPopupBackground").css("width",$(window).width());		
		$("#emailSignUpPopupBackground").css("display","block");
		$("#emailLightBoxStatus").css("display","none");		
		
	} 

	$('[placeholder]').focus(function() {
	  var input = $(this);
	  if (input.val() == input.attr('placeholder')) {
	    input.val('');
	    input.removeClass('placeholder');
	  }
	}).blur(function() {
	  var input = $(this);
	  if (input.val() == '' || input.val() == input.attr('placeholder')) {
	    input.addClass('placeholder');
	    input.val(input.attr('placeholder'));
	  }
	}).blur();
	$('[placeholder]').parents('form').submit(function() {
	  $(this).find('[placeholder]').each(function() {
	    var input = $(this);
	    if (input.val() == input.attr('placeholder')) {
	      input.val('');
	    }
	  })
	});

	function internationalModalOpen(){
		$("#intl_modal_container").css("display","block");
		$("#internationalModalBackground").css("height",$(window).height());	
		$("#internationalModalBackground").css("width",$(window).width());		
		$("#internationalModalBackground").css("display","block");
	}
	
	function internationalModalClose(){
		$("#intl_modal_container").css("display","none");
		$("#internationalModalBackground").css("display","none");
	}
	// fix mboxDefault visibility
	var mboxTriggeredCtr = 0;
       
</script>
<div class="mboxDefault"></div>
<script>
function setGlobalCookie(cname,cvalue,exdays)
{
var d = new Date();
d.setTime(d.getTime()+(exdays*24*60*60*1000));
var expires = "expires="+d.toGMTString();
document.cookie = cname + "=" + cvalue + ";path=/; " + expires;
}

function getGlobalCookie(cname)
{
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i=0; i<ca.length; i++)
  {
  var c = ca[i].trim();
  if (c.indexOf(name)==0) return c.substring(name.length,c.length);
}
return "";
}

var LB_OverlayLightBox=getGlobalCookie("LB_OverlayLightBox2");
if ((LB_OverlayLightBox!="" && LB_OverlayLightBox!=null) || (document.URL.indexOf("LoyaltyEmail") != -1 || document.URL.indexOf("CreditSweeps") != -1 || document.URL.indexOf("D2SRGC") != -1 || document.URL.indexOf("D2SR10") != -1))
{
	donothing();
}else{
	setGlobalCookie("LB_OverlayLightBox2",1,60);
	mboxCreate('LB_OverlayLightBox');
}
</script></div><!-- /headerContentArea1 -->

