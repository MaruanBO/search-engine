        $(document).ready(function () {
            //open popup
            $("#ratingPopup").click(function () {
                $("#ratingPoptext").fadeIn(1000);
                positionPopup();
            });

            //close popup
            $("#close").click(function () {
                $("#ratingPoptext").fadeOut(500);
            });
        });
        //position the popup at the center of the page
        function positionPopup() {
            if (!$("#ratingPoptext").is(':visible')) {
                return;
            }
            $("#ratingPoptext").css({
                left: ($(window).width() - $('#ratingPoptext').width()) / 0,
                top: ($(window).width() - $('#ratingPoptext').width()) / 0,
                position: 'absolute'
            });
        }
        //maintain the popup at center of the page when browser resized
        $(window).bind('resize', positionPopup);
    </script>
    
