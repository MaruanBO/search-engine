            <script type="text/javascript">
                function RedirectToGSA() {
                    var myControlId = 'searchTextInput';
		    var inputInfo = document.getElementById(myControlId).value.replace("&", "%26");
		    var searchUrl = "http://search.pluggedin.com/search?q={0}&btnG=Search&filter=&ntqr=0&output=xml_no_dtd&sort=date%3AD%3AL%3Ad1&client=pluggedin_com&filter=&ud=1&oe=UTF-8&ie=UTF-8&proxystylesheet=pluggedin_com&site=pluggedin_com&getfields=*&filter=0";
			var url = searchUrl.replace("{0}", inputInfo);
			location.href = url;
        }

        $(function () {
            $('#searchTextInput').keypress(function (event) {
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if (keycode == '13') {
                    RedirectToGSA();
                    return false;
                }
            });
        });
            </script>
