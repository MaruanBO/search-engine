		        fadeOut();
		    },


		    onAfterResponse: function (data, params, request) {
		        //jQuery(".ThumbContainer1").fixPNG('crop');
		    },

		    onDrawArea: function (area, node) {

		        html = node.html();
		        return html;
		    },

		    onDOMComplete: function () {

		        _AX.scrollTop(45, 600, 0, "div#adab-viewport", fadeIn);

		    }

		}




	    var fadeOut = function (speed, opac) {
	        speed = (speed) ? speed : "slow";
	        opac = (opac) ? opac : .25;


	        //if (jQuery.browser.msie)
	        //{
	        //jQuery("#results").fadeTo(speed, opac);
