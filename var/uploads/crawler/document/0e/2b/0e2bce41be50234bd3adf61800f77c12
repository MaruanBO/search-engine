	        //}
	        //else
	        //{
	        jQuery("#results").animate({ opacity: opac }, speed);
	        //}

	    }

	    var fadeIn = function (speed, opac) {
	        speed = (speed) ? speed : "slow";
	        opac = (opac) ? opac : 1;

	        //if (jQuery.browser.msie)
	        //{
	        //jQuery("#results").fadeIn(speed);//IE
