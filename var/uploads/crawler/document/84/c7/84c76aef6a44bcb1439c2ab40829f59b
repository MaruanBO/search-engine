var devt;
var __protoinit = [];
__protoinit.push(function(){
function esc_close(e){
console.log("d");
e.stop();
if(e.keyCode == 27){
closeb();
}
}
devt = $(document).on('keyup', esc_close);
});
function closeb(){
$$('body').each(function(el){
el.removeClassName('oh');
});
devt.stop();
$('intro').makeClipping();
var add = videoOn? 110 : 10;
$('intro').setStyle({
top: ($('intro').measure('top')+$('intro').measure('margin-top') - add) + "px",
left: ($('intro').measure('left')+$('intro').measure('margin-left') + add) + "px",
width: $('intro').measure('padding-box-width')+"px",
height: $('intro').measure('padding-box-height')+"px",
marginLeft:0,
marginTop:0,
padding:0
}).update();
var lang = 'en-US';
if(lang == "en-US"){
$('intro').shift({
top: $('j2m').measure('cumulative-top'),
left: $('j2m').measure('cumulative-left'),
width: $('j2m').measure('margin-box-width'),
height: $('j2m').measure('margin-box-height'),
borderRadius:0,
MozBorderRadius:0,
WebkitBorderRadius:0, 
propertyEasings:{
left:     'sineOut',
top:    'linear'
/*opacity: 'sineInOut',
height:  'linear',
width:   'linear'*/
},
easing: 'linear',
opacity:0.4,
duration:1,
remove:true
});    
}else{
$('intro').remove();
}
$('intro-dim').remove();
}
</script>
