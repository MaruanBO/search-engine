(function () {
    var l = window.location,
        r = false;
    $.each(window.jp_ab, function (k, v) {
        if (l.search.indexOf(k+'=') >= 0) { return; }
        if (getCookie(k) === v) { return; }
        setCookie(k, v, '/', 7);
        r = true;
    });
    if (r) { l.reload(); }
})();

