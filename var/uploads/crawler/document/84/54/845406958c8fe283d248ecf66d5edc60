$(document).ready(function() {
    $(".livechat-button").click(function () {
        if (_gaq) {
            var rel = $(this).attr("rel");
            var eventLabel = (rel && rel !== 'nofollow') ? rel : !location.search ? location.href.toLowerCase() : location.href.substring(0, location.href.indexOf('?')).toLowerCase();
            _gaq.push(['_trackEvent',"LiveChat", "Initiate", eventLabel]);
        };
    });
    $("#signout").click(function() {
        if (_gaq) { _gaq.push(['_trackEvent',"SignOut", "Click", location.href.toLowerCase()]) };
    });
    $("#support-fly-out a, .support-ribbon").click(function () {
        var linkClicked = "Support-fly-" + $(this).attr("rel");
        if (_gaq) { _gaq.push(['_trackEvent', linkClicked, "Click", location.href.toLowerCase()]) };
    });
});
