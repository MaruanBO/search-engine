        <script type="text/javascript">
            var respState = false;
            var respVisibleAgentProspect = false;
            var respVisibleQuote = false;
            var normVisibleAgentProspect = false;
            var normVisibleQuote = false;
            var isUserLoggedIn = false;
            var agentCount = 0;
            var selectBoxCTA = null;

            isUserLoggedIn = eval(false);

            
            function setSubTabs(obj) {
                $('.sub-nav a').removeClass('on'); $('.home-agent .content').hide();
                $(obj).addClass('on'); $('.home-agent .content:eq(' + $(obj).parent().index() + ')').show();
            }

            function QuotePulldownVal() {
                $('#ctl05_ddInsuranceTypes option').filter("[selected='selected']").removeAttr('selected');
                $('#ctl05_ddInsuranceTypes option').filter(function(index) { if ($(this).text() == selectBoxCTA.getLabelText()) { $(this).prop('selected', 'selected'); } });                
                
            };
            
            function QuoteSubmitQuote()
            {
                
                Global.SetPageState('QuoteAgentCTA--'+ QuoteGetPulldownValDialog());
                var e = document.getElementById("ctl05_ddInsuranceTypes");
                var _prodType = e.options[e.selectedIndex].value;
                var _url = "";
                _url = 'https://www.allstate.com/Anon/Handlers/QuoteActivity.ashx' + '?product=' + _prodType + '&zip=&intcid=/home/home.aspx|quote_box|' + _prodType;
                window.location.href = _url;
            }
            
            function QuoteGetPulldownVal() {

                return typeof(selectBoxCTA) == 'undefined' ? '' : selectBoxCTA.getLabelText();
                //$('.selectBox-label').html();
            };

            function QuoteGetPulldownValDialog()
            {
                var e = document.getElementById("ctl05_ddInsuranceTypes");
                return e.options[e.selectedIndex].value;
                //return $('#ctl05_ddInsuranceTypes option').filter("[selected='selected']").val();
            }
                

            

            

            $(document).ready(function() {
            // set up tabs

                // check responsive during resize               

                var _customer = 0;
                
                var _children = $('.sub-nav').children();
                if (_children) {
                    if (_children.length == 2) {
                        _children.first().css('width', '55%');
                        _children.last().css('width', '44%');
                        _children.last().css('width', _children.last().width() - 1);
                    }
                    else if (_children.length == 1) {
                        _children.first().css('width', '100%')
                    }
                }

                // setup component
                $('.sub-nav a').click(function(e) {
                    e.preventDefault();
                    setSubTabs($(this));
                });

                $('.tab-agent-customer').remove();


                setSubTabs($('.sub-nav a:eq(0)'));

                if (_customer) {
                    $('.tab-agent-prospect .quote-cta-agent-pagination').show();
                    $('.quote-cta-info-customer .quote-cta-agent-name a').each(function() {
                        if ($(this).text().length > 40) $(this).html($.trim($(this).text().substr(0, 40)) + '&#8230;');
                    });

                    $('.tab-agent-prospect .quote-cta-info-customer').easyPaginate({
                        step: 1,
                        controls: 'ap-nav-prospect',
                        container: 'ap-prospect'
                    });
                }
                else {
                    $('.tab-agent-prospect .quote-cta-agent-pagination').show();
                    $('.quote-cta-info-prospect .quote-cta-agent-name a').each(function() {
                        if ($(this).text().length > 40) $(this).html($.trim($(this).text().substr(0, 40)) + '&#8230;');
                    });

                    $('.tab-agent-prospect .quote-cta-info-prospect').easyPaginate({
                        step: 3,
                        controls: 'ap-nav-prospect',
                        container: 'ap-prospect'
                    });
                }
                
                
                // restore pulldown selection.
                if (Global.RestorePageState() != null) {
                    if (Global.RestorePageState().indexOf('QuoteAgentCTA') >= 0) {
                        var data = Global.RestorePageState().split('--');
                        if (data.length == 2) {
                            $('#ctl05_ddInsuranceTypes option').filter("[selected='selected']").removeAttr('selected');
                            $('#ctl05_ddInsuranceTypes option').filter(function(index) { if ($(this).val() == data[1]) { $(this).attr('selected', 'selected'); } });
                        }
                    }
                }

                selectBoxCTA = new SelectBox($('#ctl05_ddInsuranceTypes'), settings = { mobile: true });
                

                // find and replace missing agent icons with default image
                for (var i = 0; i < $('.tab-agent-prospect').find('img').length; i++)
                {
                  if (!Global.IsImageOk($('.tab-agent-prospect').find('img')[i]))
                  {
                    $($('.tab-agent-prospect').find('img')[i]).attr('src', '/resources/images/agent-mia.jpg');
                  }
                }

                $('.selectBox-label').attr('style','width: auto');
                


                // responsive 

                $(".tab-quote").hide();
                $(".tab-agent-prospect").hide();

                

                if ($(window).width() < 768) {
                    $(".tab-quote").hide();
                    $(".tab-agent-prospect").hide();
                    $('.tab-agent-prospect .quote-cta-agent-pagination').hide();
                    respState = true;
                }
                else {
                    if(normVisibleAgentProspect) {
                        $(".tab-agent-prospect").show();
                        $('.tab-agent-prospect .quote-cta-agent-pagination').show();
                    } else {$(".tab-agent-prospect").hide();}
                    
                    if(normVisibleQuote) {$(".tab-quote").show();} else {$(".tab-quote").hide();}
                    respState = false;
                }

                // setup responsive agent pager
                var respQuoteAgentProspectPageDisplay = $('.resp-cell:first-child');
                respQuoteAgentProspectPageDisplay.data("data-quote-agent-prospect-page-display", "true");
                QuoteBoxDisplayResponsiveAgent();

                $('.resp-paging-cell').first().addClass('on'); // activate first pager icon

                if('tab-agent-prospect'==='tab-quote') {
                    $('.resp-sub-nav-quote').trigger('click');
                    normVisibleQuote = true;
                }
                if('tab-agent-prospect'==='tab-agent-prospect') {
                    $('.resp-sub-nav-agent').trigger('click');
                    normVisibleAgentProspect = true;
                }

                
                if (agentCount >= 1 && respState) {
                    $(".tab-agent-prospect > h2, .tab-agent-prospect > p ").hide();
                }
                else {
                    $(".tab-agent-prospect > h2, .tab-agent-prospect > p ").show();
                }
                

                if($('.resp-paging-cell').length<=1){
                    $('.resp-paging, .resp-left, .resp-right').css('visibility','hidden');
                }

             
            });

            

            
      </script>
      

</div>    </div></div></div></div><div class="ls-row-clr" ></div></div></div></div></div><div class="ls-row-clr" ></div></div></div><div class="ls-row LayoutBody" id="lowerBodyRow"><div class="ls-fxr" id="ls-gen96970340-ls-fxr"><div class="ls-area LayoutBodyArea" id="lowerBodyArea"><div class="ls-area-body" id="ls-gen96970341-ls-area-body"><div class="ls-cmp-wrap ls-1st" id="w1383846123953"><div class="iw_component" id="1383846123953"><div class="FreeTextScroller" style="padding-top:0px; padding-bottom:11px;">
