    $(function() { 
      $('#suggestions_dialog').dialog({
        width:400, height:315,
        autoOpen:false, resizable:false,
        modal:true,
        buttons: { 
          "Close": function(event) { 
            $(this).dialog("close"); 
            $('#suggestions_dialog :input[name=Comments]').val("");
            $("#suggestion_results").html("");
            $(event.target).next().show();
          },
          "Submit": function (event) {    
            var comments = $('#suggestions_dialog :input[name=Comments]').val();
            if (comments != "") {
              $('#suggestion_results').html("<span style='color:green;'><b>Please wait while we process your request.</b></span>");
              $(event.target).prop('disabled',true);
              $.post('/suggestion.php', $('#suggestions_form').serialize(), function(data) {
                $('#suggestion_results').html(data);
                $(event.target).prop('disabled', false).hide();
              });
            } else {
              $("#suggestion_results").html("<span style='color:red;'><b>Please enter a comment or question.</b></span>");
            }
          }
        } //end buttons
      });
    });
  </script>

