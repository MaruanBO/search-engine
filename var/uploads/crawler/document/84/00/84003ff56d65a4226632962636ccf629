  $('#viewing_date').attr('size',10).datepicker({dateFormat:'yy-mm-dd',buttonImage:'/static/images/calendar2.gif',buttonImageOnly:true,showOn:'both',minDate:'2010-01-1',maxDate:'2015-03-31'});
  $('#quarter_date_picker').dialog({
    autoOpen:false,
    modal:true,
    width:350,
    position:['center', 75],
    buttons : {
      'Save' : function(){ 
        $.post('/account/ajax_quarter_switcher.php', $('#quarter_date_picker_form').serialize(), function(data){
          if (data && data.success) {
            alert(data.info);
            window.location.reload();
          } else {
            alert(data.info);
          }
        }, 'json');
        
      },
      'Cancel' : function(){ $(this).dialog('close'); }
    }
  })
  $('#viewing_date').bind('change click', function() {
    $('#d_specify').prop('checked',true);
  });
  $(document).delegate('a.pick_date','click',function(){
    $('#quarter_date_picker').dialog('open');
    return false;
  });
  
  $('body').on('click', 'a:contains(Sign In)', function(){return signin_popopen();});  
  $('body').on('click', 'a:contains(SIGN IN)', function(){return signin_popopen();});  
    
  $('#password_dialog').dialog({'modal':true,'autoOpen':false});
});

function signin_popopen(no_animate) {
    header_height = '280px'; body_top_padding = '280px';
  $('#header_signin').show();
  $('#main_signin').focus();
  if (no_animate) {
    $('div.header').css({'height':header_height});
    $('div.header_shadow').css({'top':header_height});
    $('div.body_section').css({'padding-top':body_top_padding}); 
  } else {
    $('div.header').animate({'height':header_height});
    $('div.header_shadow').animate({'top':header_height});
    $('div.body_section').animate({'padding-top':body_top_padding});
  }
    return false;
}
function signin_popclose(){
  $('div.header').animate({'height':'87px'});
  $('div.header_shadow').animate({'top':'87px'});
  $('div.body_section').animate({'padding-top':'87px'});
  $('#header_signin').hide();
}
</script>

