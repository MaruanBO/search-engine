<script>
$( document ).ready(function() {  
  // fix tap issue on iPads
  $('.tools_btn_div a').live('click touchend', function(e) {
    var el = $(this);
    var link = el.attr('href');
    window.location = link;
  });
  // set up hover actions for each button/div
  $('#search_hp_form').hover(function(){hoverPurple('search')},   function(){hoverColored('search')});
  $('#tool_buttons_div').hover(function(){hoverPurple('tool_buttons')}, function(){hoverColored('tool_buttons')});
  $('#cad_div').hover(function(){showColored('cad_img_purple')},function(){showPurple('cad_img_purple')});
  
  $('#mac_div').hover(function(){showColored('mac_img_purple')},   function(){showPurple('mac_img_purple')});
  $('#nev_div').hover(function(){showColored('nev_img_purple')},   function(){showPurple('nev_img_purple')});
  $('#can_div').hover(function(){showColored('can_img_purple')},   function(){showPurple('can_img_purple')});
  $('#bac_div').hover(function(){showColored('bac_img_purple')},   function(){showPurple('bac_img_purple')});
  $('#brac_div').hover(function(){showColored('brac_img_purple')}, function(){showPurple('brac_img_purple')});
  $('#sac_div').hover(function(){showColored('sac_img_purple')},   function(){showPurple('sac_img_purple')});
  $('#fan_div').hover(function(){showColored('fan_img_purple')},   function(){showPurple('fan_img_purple')});
});

function showColored(icon)
{  // fade out the purple icon to show the colored icon
//alert('fade out purple');
  $('#'+icon).fadeOut('fast');
}

function showPurple(icon)
{  // fade in the purple icon
//alert('fade in purple');
  $('#'+icon).fadeIn('fast');
}

//stop(true,true).

function hoverPurple(icon)
{
  if(icon!='cad_img_purple')  $('#cad_img_purple').fadeIn('fast');
  if(icon!='mac_img_purple')  $('#mac_img_purple').fadeIn('fast');
  if(icon!='nev_img_purple')  $('#nev_img_purple').fadeIn('fast');
  if(icon!='can_img_purple')  $('#can_img_purple').fadeIn('fast');
  if(icon!='bac_img_purple')  $('#bac_img_purple').fadeIn('fast');
  if(icon!='brac_img_purple') $('#brac_img_purple').fadeIn('fast');
  if(icon!='sac_img_purple')  $('#sac_img_purple').fadeIn('fast');
  if(icon!='fan_img_purple')  $('#fan_img_purple').fadeIn('fast');
}

function hoverColored(icon)
{
  if(icon!='cad_img_purple')  $('#cad_img_purple').fadeOut('fast');
  if(icon!='mac_img_purple')  $('#mac_img_purple').fadeOut('fast');
  if(icon!='nev_img_purple')  $('#nev_img_purple').fadeOut('fast');
  if(icon!='can_img_purple')  $('#can_img_purple').fadeOut('fast');
  if(icon!='bac_img_purple')  $('#bac_img_purple').fadeOut('fast');
  if(icon!='brac_img_purple') $('#brac_img_purple').fadeOut('fast');
  if(icon!='sac_img_purple')  $('#sac_img_purple').fadeOut('fast');
  if(icon!='fan_img_purple')  $('#fan_img_purple').fadeOut('fast');
}
</script>

