<script type="text/javascript">
/**
 * @brief This block controls the static footer and ties the live chat button into olark.
 * @author Ray Peters <rpeters@ontraport.com>
 */
$(document).ready(function()
{
	var _ontraport = {
		toggleChat: function(){
			var olark = $("#habla_panel_div");
			// Toggle Chat
			return !olark.hasClass('opFooter--on') ? 
						olark.addClass('opFooter--on') : olark.removeClass('opFooter--on');
		}
	};

	// Wait three seconds then...
	setTimeout(function(){
		// Hook events into the DOM
		$("li.op-footer__button--olark").on( 'click.opFooter', _ontraport.toggleChat );

		// Chained timeout ties into olark close button once it's done loading
		var olarkCloseHook = function(){
			var olarkCloseButton = $("a#habla_closebutton_a");
			if ( olarkCloseButton.length === 0 ) {
				// olark still loading
				setTimeout( olarkCloseHook, 500 );
			} else {
				// hook into event
				olarkCloseButton.on( 'click.opFooter', _ontraport.toggleChat );
			}
		}();

		// Fade in static footer
		$('.op-footer').fadeToggle(1500);
	}, 3000);
});
</script>

