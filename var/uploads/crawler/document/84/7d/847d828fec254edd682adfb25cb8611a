						jQuery(document).ready(function($) {

							var $existing = '';

							var loginCheck = function(data,status)
							{
								//console.log('status' + data.code);
                                if(parseInt(data.ver) == 3) {
									switch(parseInt(data.code)) {
									case 0:
										loginform.unbind('submit').submit();
										break;
									case 1:
									case 2:
										// Bring back original, lets not waste DOM Ops
										if ($existing.length){
											$existing.fadeIn();
										} else {
											var $usernameInput = $(".ui-corner-all.user");
											$("body").append( 
												window.$loginErrorDialogueDOM.css({
													position:"absolute", 
													top: 20, "z-index": 99999, 
													left: $usernameInput.position().left + $usernameInput.width() + 15
												}).find("#login_error_close").bind("click", function(){
													var $existing = $("#"+ window.$loginErrorDialogueDOM.attr('id'));
													$existing.fadeOut();
												}).end().fadeIn()
											);
										}
											
										// alert("Your username or password is incorrect.\nIf you are trying to login to OfficeAutoPilot, please login through www.officeautopilot.com");
										break;
									default:
										alert("An unknown error occurred. Please contact your support team.");
										break;
									}
								}
								else if(parseInt(data.ver) == 2) {
                                   switch(parseInt(data.status)) {
										case 0:
											if ($existing.length){
											$existing.fadeIn();
										} else {
											var $usernameInput = $(".ui-corner-all.user");
											$("body").append( 
												window.$loginErrorDialogueDOM.css({
													position:"absolute", 
													top: 20, 
                                                    "z-index": 99999,
													left: $usernameInput.position().left + $usernameInput.width() + 15
												}).find("#login_error_close").bind("click", function(){
													var $existing = $("#"+ window.$loginErrorDialogueDOM.attr('id'));
													$existing.fadeOut();
												}).end().fadeIn()
											);
										}
											break;
										case 1:
											window.location.href = data.url;
											break;
										case 2:
											if ($existing.length){
											$existing.fadeIn();
										} else {
											var $usernameInput = $(".ui-corner-all.user");
											$("body").append( 
												window.$loginErrorDialogueDOM.css({
													position:"absolute", 
													top: 20, 
													left: $usernameInput.position().left + $usernameInput.width() + 15
												}).find("#login_error_close").bind("click", function(){
													var $existing = $("#"+ window.$loginErrorDialogueDOM.attr('id'));
													$existing.fadeOut();
												}).end().fadeIn()
											);
										}
											break;
									}
								}
								else if(parseInt(data.ver) == 1) {
									var $usernameInput = $(".ui-corner-all.user");
									$("body").append( 
										window.$loginChooseAccount.css({
											position:"absolute", 
											top: 20,
                                            "z-index": 99999, 
											left: $usernameInput.position().left + $usernameInput.width() + 15
										}).find("#login_error_close").bind("click", function(){
											var $existing = $("#"+ window.$loginChooseAccount.attr('id'));
											$existing.fadeOut();
										}).end().fadeIn()
									);

									$('.oap-log').click(function()
									{
										$('.sys-version').attr('name', 'version').val('2');
										$.ajax({
											type:'POST',
											url:'/proxy.php',
											data:loginform.serialize(),
											dataType:'json',
											success: loginCheck
										});
									});
									$('.ontra-log').click(function()
									{
										$('.sys-version').attr('name', 'version').val('3');
										$.ajax({
											type:'POST',
											url:'/proxy.php',
											data:loginform.serialize(),
											dataType:'json',
											success: loginCheck
										});
									});
								}
							}
						
							var loginform = $('#loginforma, #loginform1');
							loginform.unbind('submit').submit(function(event) {
							event.preventDefault();
							event.stopImmediatePropagation();

								$existing = $("#login_error");
								$existing.fadeOut();

								$.ajax({
									type:'POST',
									url:'/proxy.php',
									data:loginform.serialize(),
									dataType:'json',
									success: loginCheck
								});
							});        
						});
