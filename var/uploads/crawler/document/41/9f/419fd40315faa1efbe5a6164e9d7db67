        <script>
            Sunrise.loadListMode = function() {
                var newsList = $('#newsList');

                newsList.append('<div style="text-align: center; padding-top: 50px"><img src="/assets/img/interface/loading.gif"/></div>');
                $.ajax('/?overridetemplate=SUN_PAGE_ListMode', {
                    success: function(data) {
                        newsList
                            .html(innerShiv(data, false))
                            .find("#listing_home")
                                .delay('fast')
                                .fadeIn(Sunrise.initListMode);
                        Sunrise.initFacebook();
                    }
                });
            };
        </script>

