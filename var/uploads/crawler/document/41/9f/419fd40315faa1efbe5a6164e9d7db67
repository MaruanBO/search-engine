		<script type="text/javascript">
 var the_images = [];
  var rotatetime = 6000;
 var key = 0;

 window.onload = function(){
 the_images.push(["http://classifieds.torontosun.com/toronto/education/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470896480_ORIGINAL.jpg","Employment"]);
 the_images.push(["http://classifieds.torontosun.com/toronto/services/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470899200_ORIGINAL.jpg", "Services"]);
 the_images.push(["http://classifieds.torontosun.com/toronto/rentals/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470900007_ORIGINAL.jpg", "Rentals"]);
 the_images.push(["http://classifieds.torontosun.com/toronto/automotive/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470894563_ORIGINAL.jpg", "Autos"]);
 the_images.push(["http://classifieds.torontosun.com/toronto/real-estate/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470899849_ORIGINAL.jpg", "Real Estate"]);
 the_images.push(["http://classifieds.torontosun.com/toronto/home-services/search", "http://storage.torontosun.com/v1/suns-prod-images/1297470899009_ORIGINAL.jpg", "Home Services"]);
 image_rotate((Math.floor(Math.random() * the_images.length)));
 }
 function image_rotate(active){
    var image_container = document.getElementById("classified_ad");
	jQuery.preloadImage(
        the_images[active][1],
        function(){
            /*complete
            this.width!=0 == true
            */
        },
        function(){
            /*error*/
        }
    )
    image_container.innerHTML = "<a href=\""+the_images[active][0]+"\"><img src=\""+the_images[active][1]+"\" alt=\""+the_images[active][2]+"\" width=\"300\" height=\"250\" border=\"0\"  title=\""+the_images[active][2]+"\" /></a>";
     active++;
     if(active >= the_images.length){
        active = 0;
    }
    setTimeout("image_rotate("+active+")", rotatetime);
 }
  jQuery.preloadImage=function(src,onSuccess,onError)
    {
        var img = new Image()
        img.src=src;
        var error=false;
        img.onerror=function(){
            error=true;
            if(onError)onError.call(img);
        }
        if(error==false)    
        setTimeout(function(){
            if(img.height>0&&img.width>0){ 
                if(onSuccess)onSuccess.call(img);
                return img;
            }   else {
                setTimeout(arguments.callee,5);
            }   
        },0);
        return img; 
    }  
    
 </script>
  <div id="classified_ad"></div>
	 
		

            
        


    
        

















        
        

        
        

        
        
            
            
            
            
            
            
                
                
                    
                    
                    
                    
                









