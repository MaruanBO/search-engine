<script>
    $(function () {
        $('body').click(function(event) {
            var $tgt = $(event.target);
            if(!$tgt.parents('.menuLang').hasClass('menuLang') ) {
                $('#sw').hide();
            }
        });
            //it's hack especially for header menu for closing popup that is open
        var listMenyLink = ['a-products-panel', 'a-resources-panel', 'a-support-panel', 'a-company-panel', 'a-info-panel'];
        for (var link in listMenyLink) {
            $(document.getElementById(listMenyLink[link])).click(function(e) {
                if (document.getElementById('sw').style.display == 'block') {
                    $(document.getElementById('sw')).hide();
                }
            });
        }
    });
</script>

