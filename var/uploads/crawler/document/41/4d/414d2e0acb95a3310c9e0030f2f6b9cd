<script>
    //make position of preloading box in the center of the screen
    $(function () {
        var leftLenght = Math.ceil((screen.width / 2.2 - 350 / 2) * 100 /  screen.width);
        var topLenght = Math.ceil(screen.height / 2.2 - 145);
        $('#preloading').css('top', topLenght + 'px');
        $('#preloading').css('left', leftLenght + '%');

        $('#close_lang_bar').click(function(e){
            $('.close_lang_bar').hide();
        })
    });
</script>
