	$('#synWord2').html(html);
	$(document).ready( function(){
		$('#synWord2').innerfade({
			animationtype: 'slide',
			speed: 750,
			timeout: 5000,
			type: 'sequence',
			containerheight: '1em'
		});
	});
}
//]]>
</script>
