            }

            function _IEPlaceholderOn() {
            if(_$('ilnPopupEmail').value == "") {
            _$('ilnPopupEmail').style.color = "#999999";
            _$('ilnPopupEmail').value = "Enter Your Valid Email Address";
            }
            }
            function _IEPlaceholderOff() {
            if(_$('ilnPopupEmail').value.toLowerCase() == "enter your valid email address") {
            _$('ilnPopupEmail').style.color = "#555555";
            _$('ilnPopupEmail').value = "";
            }
            }
