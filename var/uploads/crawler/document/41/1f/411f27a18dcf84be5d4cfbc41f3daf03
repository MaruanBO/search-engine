                                                                    function subcat1_parsePagingData(data){
                                                                    var htmlOut = "";
                                                                    if (data.rows) {
                                                                    var rows = data.rows;
                                                                    htmlOut += "<ul class='article-links'>";
                                                                    $.each(data.rows, function(i, e) {
                                                                    htmlOut += '<li><a href="'+e.path+'">'+e.event_title+'<\/a><\/li>';
                                                                    });
                                                                    htmlOut += "<\/ul>";
                                                                    htmlOut += '<div class="read-more">';
                                                                    htmlOut += '<button onclick="';
                                                                    htmlOut += "window.location='health'";
                                                                    htmlOut += '" class="btn btn-warning">READ MORE<\/button>';
                                                                    htmlOut += '<\/div>';

                                                                    }
                                                                    return htmlOut;
                                                                    }

