            function _Popup() {
            if(_GetCookie("closedPopup-1027") == "") {
            if($popup_type == "email") {
            if(_$().addEventListener) { _$('ilnPopupEmail').value = "";
            } else { _$('ilnPopupEmail').attachEvent("onfocus", _IEPlaceholderOff); _$('ilnPopupEmail').attachEvent("onblur", _IEPlaceholderOn); }
