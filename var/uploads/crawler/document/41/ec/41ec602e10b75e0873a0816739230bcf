		function createRegistrationTagIfValidEmail(email){
	 		if (validate_email_without_add_error(email)) {
	 			cmCreateRegistrationTag('-1002',email.value, null, null, null, null,null, null, null, null, null, null,null,null,null,null,null);
	 		}
		}
		function emailSignUpSubmit(form)
		{
			validator_reset();
			
			clear_instructions_emailSignUp();
	
			emailSignUp_frmvalidator.validator_submit();
	
			if (validation_display_errors('') == true){	
				/**
				* @author hsawalhi
				* Per WCSBD-807
				*/
				document.getElementById("emailSignUp_label").removeAttribute("style");
				
				cmCreateRegistrationTag('-1002',$('emailSignUp').value, null, null, null, null,null, null, null, null, null, null,null,null,null,null,null);
				return true;		      
			}
			return false;
		}
		
