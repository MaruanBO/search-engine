function changeforumcol2(obj){
	//var obj = document.getElementById('forumselect');
	if(obj != null){
		var s = 'https://footballrecruiting.rivals.com/forum.asp?fid=' + obj.options[obj.selectedIndex].value.toString();
		document.location = s;
     }
}
</SCRIPT>
