<script>
var sProspectSearchSelected = 'prospectfootball';
var strOnMenuClr = '#0A4EA3';
var strOffMenuClr = '#BEC3C9';
var strHvrMenuClr = '#AAB1BA';
function ChangeProspectSearchBackGround(stype, obj){
	if (obj.id != sProspectSearchSelected){
		if (stype == 'on')
			obj.style.backgroundColor = strHvrMenuClr;
		else
			obj.style.backgroundColor = strOffMenuClr;
	}
}
function ChangeProspectSearchDisplay(stype){
	var obj = null;
	var obj2 = null;
   switch (stype) {
	    case 'basketball':
   		obj = document.getElementById('prospectsearchfootball');
	    	obj2 = document.getElementById('prospectfootball');
		    if (obj != null)
			    obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    obj = document.getElementById('prospectsearchbasketball');
   		obj2 = document.getElementById('prospectbasketball');
	    	if (obj != null)
		    	obj.style['display'] = 'inline';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOnMenuClr;
		    obj = document.getElementById('prospectsearchbaseball');
   		obj2 = document.getElementById('prospectbaseball');
	    	if (obj != null)
		    	obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    sProspectSearchSelected = 'prospectbasketball';
	    break;
	    case 'baseball':
   		obj = document.getElementById('prospectsearchfootball');
	    	obj2 = document.getElementById('prospectfootball');
		    if (obj != null)
			    obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    obj = document.getElementById('prospectsearchbasketball');
   		obj2 = document.getElementById('prospectbasketball');
	    	if (obj != null)
		    	obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    obj = document.getElementById('prospectsearchbaseball');
   		obj2 = document.getElementById('prospectbaseball');
	    	if (obj != null)
		    	obj.style['display'] = 'inline';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOnMenuClr;
		    sProspectSearchSelected = 'prospectbaseball';
   	break;
	    default:
   		obj = document.getElementById('prospectsearchfootball');
	    	obj2 = document.getElementById('prospectfootball');
		    if (obj != null)
			    obj.style['display'] = 'inline';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOnMenuClr;
		    obj = document.getElementById('prospectsearchbasketball');
   		obj2 = document.getElementById('prospectbasketball');
	    	if (obj != null)
		    	obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    obj = document.getElementById('prospectsearchbaseball');
   		obj2 = document.getElementById('prospectbaseball');
	    	if (obj != null)
		    	obj.style['display'] = 'none';
   		if (obj2 != null)
	    		obj2.style.backgroundColor = strOffMenuClr;
		    sProspectSearchSelected = 'prospectfootball';
	}
}
</script>
