// hide subcategories from left nav browse
jQuery.noConflict();
jQuery(document).ready(function(){
	jQuery('#left-1 ul.linkList li a').each(function(){
		var getCategoryLeftnav = jQuery(this).text().replace(/^\s*|\s*$/g,'');
		if( (getCategoryLeftnav=="Father's Day Gift Set") || (getCategoryLeftnav=="Premium Gun Safes") || (getCategoryLeftnav=="Personalized") || (getCategoryLeftnav=="Premium Meat Snacks") || (getCategoryLeftnav=="1911RAC BB Air Pistol") || (getCategoryLeftnav=="Truck Wheels") ){
		jQuery(this).parent().remove();
		}
	});
});
</script>


