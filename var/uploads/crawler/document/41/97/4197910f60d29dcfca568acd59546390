     function getCookieValue(name) {
         var value = "; " + document.cookie;
    	 var parts = value.split("; " + name + "="); // split cookie into 2 arrays. Second array will have the value of the cookie if present
    	 if (parts.length == 2){ 
    	     return parts.pop().split(";").shift(); // This will get the second array and split it with ; and return the first array
    	 }
    	 return null;
    	}
    
