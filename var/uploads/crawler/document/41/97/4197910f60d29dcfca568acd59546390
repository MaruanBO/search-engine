<script type="text/javascript">
var readyFunction = function(){
    
    jQuery(decodeURI("div.signInMenu%20li.signOut%20a")).each(function(){
        jQuery(this).find("*").add(this).contents().filter(function(){
            return (this.nodeType == 3) && (!/^\s*$/.test(this.nodeValue)) && (this.parentNode.tagName != "SCRIPT");
        }).each(function(){
            this.nodeValue = this.nodeValue.replace(decodeURI("Sign%20Out"), decodeURI("Sign%20In/Out"));
        });
    });
    
};
jQuery.readyList.unshift(readyFunction);

var textReplacementList = jQuery(document).data("textReplacementList");
if(!textReplacementList){ 
    textReplacementList = []; 
}
textReplacementList.unshift({searchClass: "signInMenu%20li.signOut%20a", readyFunction : readyFunction});
jQuery(document).data("textReplacementList", textReplacementList);
</script>

