    <script type="text/javascript">
// Add New icon after Model 700 Series and after Model 870 Series
jQuery('#header-8 ul li#globalNavItem5 li').each(function(){
var ktm = jQuery(this).text();
ktm = ktm.replace(/^\s+|\s+$/g,'');
//alert(ktm);
	// jQuery(this).css('padding-left','20px');
	if ( (ktm == "Model 700 Series")||(ktm == "Model 870 Series") ){
		jQuery(this).css({'background':'url("/media/Site Files/Merchandising Files/new.png") top right no-repeat', 'background-position':'120px 5px'});
		//jQuery(this).css({'background':'url("/media/Site Files/Merchandising Files/new.gif") no-repeat', 'background-position':'5px'});
	}
});

// Change link of Air Rifle under Shooting category
/*jQuery('#header-8 ul li#globalNavItem4 li').each(function(){
