    $(document).ready(function(){
    
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
    
    $("#HomeBoxes a").click(function(event){
    event.preventDefault();
    $(this).parent().addClass("Clicked");    
    });
    
    $("#HomeBoxes .Clicked a").click(function(event){
    event.preventDefault();
    $(this).unbind('click').click();    
    });
    
    }
    
    $("#SideCategoryList ul li ul li").each(function(){
    
    $(this).has("ul").addClass("HasSubMenu");
    
    });
    
    $("#SideCategoryList .HasSubMenu a").click(function(event){
    
    var parentCheck = $(this).parent().hasClass("HasSubMenu");
    
    if (parentCheck==true) {
    event.preventDefault();
    $(this).parent().toggleClass("Clicked");
    $(this).parent().find("ul:first").slideToggle('1000');
    }
    });
    
    });
</script>










