        <script type="text/javascript" src="http://assets.windowsphone.com/813d460c-c1db-4403-8cd9-a9de21fecd17/HP-Nokia-Lumia-630-carousel-track_InvariantCulture_Default.js"></script>
    <script type="text/javascript" src="http://cmsresources.windowsphone.com/windowsphone/en-US/home/PhonesCarousel/scripts/Icon-HP-family-phone-sitecat.js"></script>
    <script type="text/javascript" src="http://cmsresources.windowsphone.com/windowsphone/en-US/Home/PhonesCarousel/scripts/crsl.js"></script>
    <script type="text/javascript">
    	(function() { 

		var _crsl = function(el) {
			var self = this;


			this.$el = $(el);
			this.$vw = $(el).find(".crsl-view2");
			this.$list = $(el).find(".crsl-list");
			this.$items = $(el).find(".crsl-item");
			this.numItems = this.$items.length;
			this.itemWidth = $(this.$items[0]).width();
			this.itemHeight = $(this.$items[0]).height();
			this.viewOffsetX = -this.itemWidth;
			this.$win = $(window);

			// work around max-width issues
			if(this.itemWidth == 0) {
				this.viewOffsetX = this.itemWidth = this.$el.width();
			}


			// number the items
			this.$items.each(function(idx, d){
				$(d).attr("data-page-index", idx);
				$(d).css("width", self.itemWidth);
			});

			// initial variables
			this.curIdx = 0;

			// fill our view
			this.fillView();

			// attach handlers
			this.$el.bind("mousedown", this, this.onMouseDown);
			
			// IE10
			if (window.navigator.msPointerEnabled) {
				this.$el.bind("MSPointerDown", this, this.onPointerDown);
			} else {
				this.$el.bind("touchstart", this, this.onTouchStart);
			}

			$(document).bind("dragstart", function(ev) { ev.preventDefault(); return false; } );
			this.$el.find('img').bind('dragstart', function(ev) { ev.preventDefault(); });

			this.$el.find("a[data-crsl-seek]").bind("click", this, this.onSeek);

			this.$el.find(".crsl-next-btn").bind("click", this, this.moveToNext);
			this.$el.find(".crsl-prev-btn").bind("click", this, this.moveToPrev);


			this._oldWinWidth = this.$win.width();
			setInterval(function() { self.onTimer(); }, 50);


		};



		_crsl.prototype.onMouseDown = function(ev) {
			var self  = ev.data;

			if(ev.srcElement && ev.srcElement.tagName == "A") return;

			self.startDrag(ev, ev.pageX, ev.pageY);

			// stop default behavior
			ev.preventDefault();

			// bind mouse handlers
			$(document).bind("mouseup.crsl", self, self.onMouseUp);
			$(document).bind("mousemove.crsl", self, self.onMouseMove);
		}



		_crsl.prototype.onMouseMove = function(ev) {
			var self = ev.data;

			self.onDragMove(ev,ev.pageX);
		}		



		_crsl.prototype.onMouseUp = function(ev) {
			var self = ev.data;

			//console.log("mouseup");
			//console.dir(ev);

			self.onDragEnd(ev,ev.pageX);

			$(document).unbind("mousemove.crsl");
			$(document).unbind("mouseup.crsl");
		}



		_crsl.prototype.startDrag = function(ev,x,y) {

			// record initial data
			this.startTouch = this.lastTouch = { x: x, y: y };

			// reset view offset
			this.updateViewOffset(-this.itemWidth);
		}


		_crsl.prototype.onDragMove = function(ev, x, y) {
			var offX = x - this.startTouch.x,
				viewOffX = offX - this.itemWidth;

			this.lastTouch = { x: x, y: y };

			// if the x offset has changed, update the offset
			if(viewOffX != this.viewOffsetX) {
				this.updateViewOffset(viewOffX);
			}
		}		

		_crsl.prototype.onDragEnd = function(ev, x) {
			
			// make sure we respond to an event if we have previous data
			if(typeof this.startTouch !== "undefined") {
				var xdiff = x-this.startTouch.x;

				//console.log("> " + x + " - " + this.startTouch.x + " = " + xdiff);

				// if we scroll more than some amount, flip the pag
				if(Math.abs(xdiff) > 100) {

					if(xdiff > 0) {
						this.moveToPrev();
					} else {
						this.moveToNext();
					}
				} else {
					this.scrollReset();
				}
			}


			ev.preventDefault();

		}

		_crsl.prototype.scrollLeft = function(onComplete) {
			this.$vw.animate({ "margin-left": 0 }, 500, "linear", onComplete);
		}

		_crsl.prototype.scrollRight = function(onComplete) {
			this.$vw.animate({ "margin-left": -2*this.itemWidth }, 500, "linear", onComplete );
		}

		_crsl.prototype.scrollReset = function(onComplete) {
			this.$vw.animate({ "margin-left": -this.itemWidth }, 300, "linear", onComplete );
		}






		_crsl.prototype.onTouchStart = function(ev) {
			var self = ev.data,
				t0 = ev.originalEvent.touches[0],
				x = t0.pageX,
				y = t0.pageY;


			self.startDrag(ev, x, y );

			// bind touch handlers
			$(document).bind("touchend.crsl touchcancel.crsl", self, self.onTouchEnd);
			$(document).bind("touchmove.crsl", self, self.onTouchMove);
		}


		_crsl.prototype.onTouchEnd = function(ev) {
			var self = ev.data,
				x = self.lastTouch.x,
				dist = Math.sqrt(Math.pow(x - self.startTouch.x, 2) + Math.pow(self.lastTouch.y - self.startTouch.y, 2));

			self.onDragEnd(ev,x);

			// workaround for some chrome versions (~34)
			if(dist < 10) {
				self.tryFakeTouchClick(ev);
			}

			$(document).unbind("touchmove.crsl");
			$(document).unbind("touchend.crsl touchcancel.crsl");


		}


		_crsl.prototype.onTouchMove = function(ev) {
			var self = ev.data,
				t0 = ev.originalEvent.touches[0],
				x = t0.pageX,
				y = t0.pageY;


			self.onDragMove(ev, x, y);
		}





		_crsl.prototype.onPointerDown = function(ev) {
			var self = ev.data,
				t0 = ev.originalEvent,
				x = t0.pageX,
				y = t0.pageY;


			self.startDrag(ev, x, y );

			// bind touch handlers
			$(document).bind("MSPointerUp.crsl MSPointerCancel.crsl", self, self.onPointerEnd);
			$(document).bind("MSPointerMove.crsl", self, self.onPointerMove);

			ev.preventDefault();
		}


		_crsl.prototype.onPointerEnd = function(ev) {
			var self = ev.data,
				oe = ev.originalEvent,
				x = oe.pageX,
				dist = Math.sqrt(Math.pow(x - self.startTouch.x, 2) + Math.pow(oe.pageY - self.startTouch.y, 2));

			self.onDragEnd(ev,x);


			$(document).unbind("MSPointerMove.crsl");
			$(document).unbind("MSPointerUp.crsl MSPointerCancel.crsl");

			ev.preventDefault();
			ev.stopPropagation();
		}


		_crsl.prototype.onPointerMove = function(ev) {
			var self = ev.data,
				t0 = ev.originalEvent,
				x = t0.pageX,
				y = t0.pageY;


			self.onDragMove(ev, x, y);

		}



		_crsl.prototype.getItem = function(id) {

			if(typeof this.$list === "undefined" || this.$list.length == 0) {
				return null;
			}


			if(id > this.numItems) {
				id -= this.numItems;
			} 

			if(id < 0) {
				id += this.numItems;
			}

			if(id >= 0 && id < this.numItems) {
				return this.$items[id];
			} else {
				return this.$items[0];
			}


		}

		_crsl.prototype.addPageToView = function(idx, cls) {
			var sel = "[data-page-index='" + idx + "']",
				$pg = this.$list.find(sel);

			if($pg.length > 0) {
				this.$vw.append($pg);
			} else {
				$pg = this.$vw.find(sel);

			}

			if($pg.length > 0) {
				if(cls) {
					$pg.removeClass("cur prev next");
					$pg.addClass(cls);
				}
			}
		}

		_crsl.prototype.fillView = function() {
			// empty all children
			//this.$vw.empty();
			var self = this,
				cur = this.curIdx,
				prev = this.wrapNum(cur-1),
				next = this.wrapNum(cur+1);


			this.updateView(prev,cur,next);

			// reset offset
			this.updateViewOffset(-this.itemWidth);

			this.updateSelection();

		}


		_crsl.prototype.updateView = function(prev,cur,next) {
			var self = this;

			//console.log("removing from view");
			this.$vw.find("li").each(function(i,d){
				var pg_num = +($(d).data("page-index"));

				//if(pg_num < prev || pg_num > next ) {
					//console.log("moving " + pg_num);
					self.$list.append(d);
				//}
			});

			// add elements
			this.addPageToView(prev, "prev");
			this.addPageToView(cur, "cur");
			this.addPageToView(next, "next");			
		}


		_crsl.prototype.updateSelection = function() {
			var $pgbtns = this.$el.find(".crsl-page-btn");
			
			$pgbtns.removeClass("selected");

			$pgbtns.find("a[data-crsl-seek='" + this.curIdx + "']")
				.parents(".crsl-page-btn")
				.addClass("selected");


		}

		_crsl.prototype.onSeek = function(ev) {
			var self = ev.data,
				seekIdx = +($(this).data("crsl-seek"));

			self.seek(seekIdx);

			ev.preventDefault();
		}


		_crsl.prototype.seek = function(idx) {
			var self = this,
				cur = this.curIdx,
				prev = this.wrapNum(cur-1),
				next = this.wrapNum(idx);

			if(prev == next) {
				for( var k = 0; k < this.numItems; k++ ) {
					if( k != cur && k != next ) {
						prev = k;
						break;
					}
				}
			}

			this.updateView(prev,cur,next);
			self.scrollRight(function(){
				self.curIdx = idx;
				self.fillView();
			});
		}

		_crsl.prototype.wrapNum = function(idx) {
			if(idx < 0) {
				idx += this.numItems;
			} else if (idx >= this.numItems) {
				idx = idx % this.numItems;
			}

			return idx;
		}


		_crsl.prototype.moveToNext = function(ev) {
			var self = ((typeof ev !== "undefined") ? ev.data : this);

			self.scrollRight(function(){
				self.curIdx = self.wrapNum(self.curIdx +1);	
				self.fillView();
			});
		
			if(ev) {
				ev.preventDefault();
			}
		}


		_crsl.prototype.moveToPrev = function(ev) {
			var self = ((typeof ev !== "undefined") ? ev.data : this);

			self.scrollLeft(function(){
				self.curIdx = self.wrapNum(self.curIdx -1);	
				self.fillView();
			});
		
			if(ev) {
				ev.preventDefault();
			}
		}



		_crsl.prototype.onWinResize = function() {
			var self = this,
				w = this.$el.width();
			this.itemWidth = w;

			this.$items.each(function(idx, d){
				$(d).css("width", w);
			});

			this.updateViewOffset(-w);

		}

		_crsl.prototype.onTimer = function() {
			var w = this.$win.width();

			if( w != this._oldWinWidth) {
				this.onWinResize();

				this._oldWinWidth = w;
			}
		}


		_crsl.prototype.updateViewOffset = function(vox) {
			this.viewOffsetX = vox;
			this.$vw.css({"margin-left": (vox) + "px" });
		}





		_crsl.prototype.tryFakeTouchClick = function(ev) {
			var src = ev.originalEvent.srcElement;

			if(src.tagName == "A") {
				var $s = $(src);

				if($s.hasClass("crsl-btn")) {

					if(!!$s.attr("data-crsl-seek")) {
						this.onSeek.call(src,ev);
					} else {
						if($s.hasClass("crsl-prev-btn")) {
							this.moveToPrev.call(src,ev);
						} else if($s.hasClass("crsl-next-btn")) {
							this.moveToNext.call(src,ev);
						}
					}
				}
			}
		}



		$(document).ready(function(){
			var csel = ".carousel2",
				$crsl = $(".carousel2"),
				$panel = $crsl.parents(".panel");

			$crsl.detach();
			//$panel.empty();
			$panel.prepend($crsl);
			$panel.find(".altGroupBackgroundColor").remove();
			$panel.find(".altPanelBackgroundColor").remove();
			$panel.find(".innerContent").remove();

			var crsl = new _crsl(".carousel2");
		});


	})();
    </script>



    
