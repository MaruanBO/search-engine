    <script type="text/javascript">
        window.fbAsyncInit = function () {
            FB.init({
                appId: '210175465694104',
                channelUrl: '//www.windowsphone.com/channel.html',
                status: true,
                cookie: true,
                xfbml: true
            });

            // FB tracking
            FB.Event.subscribe('edge.create',
                function (response) {
