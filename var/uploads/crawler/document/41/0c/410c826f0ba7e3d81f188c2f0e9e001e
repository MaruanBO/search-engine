  <script src="/custom/domain_1/theme/restaurants2/js/chosen/chosen.jquery.min.js"></script>
  <script>
    Restaurants = {};
    
    Restaurants.onSearchBtnClick = function(e) {
      var stateSearch = $('#search-state'),
          citySearch = $('#search-city'),
          categorySearch = $('#search-category'),
          state,
          city,
          category;
    
      state = stateSearch.find(':selected').attr('data-slug');
      city = citySearch.find(':selected').attr('data-slug');
      category = categorySearch.find(':selected').val();
      
      state == "" ? null : state;
      city == "" ? null : city;
      category == "" ? null : category;
      
      if (!state && !city) {
        category = categorySearch.find(':selected').attr('data-slug');
        category == "" ? null : category;
      }
	  
      if (!state && !city && !category) {
        return;
      }
      else {
        Restaurants.search(state, city, category);
		e.preventDefault();
      }
    }
    
    Restaurants.onStateSearchChange = function() {
      var stateSearch = $('#search-state');
      
      if (stateSearch.length == 0) {
        console.error('Could not find the state search box.');
        return;
      }
      
      Restaurants.setCitySearchBox(stateSearch.val())
    }
  
    Restaurants.applyChosenStyles = function() {
      $('[data-chosen]').chosen({
        allow_single_deselect: true
      });
    }
    
    Restaurants.setCitySearchBox = function(id, callback) {
      $.getJSON('/searchcache/cities_' + id + '.json', function(data) {
        var i,
            option,
            citySearch = $('#search-city');
        
        if (!data || Object.prototype.toString.call(data) !== '[object Array]') {
          console.error('Invalid city data.');
          return;
        }
      
        if (citySearch.length == 0) {
          console.error('Could not find the city search box.');
          return;
        }
        
        citySearch.find('[value!=""]').remove();
        
        for (i = 0; i < data.length; i++) {
          option = $('<option>')
            .attr('value', data[i].id)
            .attr('data-slug', data[i].slug)
            .text(data[i].name);
            
          citySearch.append(option);
        }
        
        citySearch.trigger('chosen:updated');
        
        if (callback) {
          callback();
        }
      });
    }
    
    Restaurants.search = function(state, city, category) {
      var location;
    
      if (state && city && category) {
        location =
          '/' + encodeURIComponent(state) +
          '/' + encodeURIComponent(city) + 
          '?&category=' + category;
      }
      else if (state && city) {
        location =
          '/' + encodeURIComponent(state) +
          '/' + encodeURIComponent(city);
      }
      else if (state) {
        location =
          '/' + encodeURIComponent(state);
      }
      else if (category) {
        location = '/listing/search/' + encodeURIComponent(category) + '/where/empty/';
      }
      
      window.location.href = location;
	  
    }
    
    Restaurants.applyChosenStyles();
            
    $('#search-btn').click(Restaurants.onSearchBtnClick);
    $('#search-state').change(Restaurants.onStateSearchChange);
  
    window.changePageScreen = function(baseUrl, dropdownId, page, searchUrl) {
      var url = searchUrl,
          category = null;
      
      url = url + '/screen/' + page;
      
      if (category) {
        url = url + '?category=' + category;
      }
      
      window.location = url;
    }
  </script>
</div>
