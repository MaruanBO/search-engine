<script>
	/*
		*********  
		********* Navigation Roll Over
		*********
	*/
	$(".giant").bind( "mouseenter", function() {
		var debugFlag = 0;
		wID = $(this).attr('id');							   	
		if (debugFlag) console.log('wID:' + wID);
		if (theAllowNavTags.indexOf(wID) > -1) {
			wFID = '#F'+wID;
			wFIDSource = "http://nav.whec.com/"+wID+"X.shtml";
			$(wFID).attr('src', wFIDSource);
			if (debugFlag) console.log('Loading:' + wFIDSource); 		 
		}
		else if (debugFlag) console.log('Not Loaded'); 		
	});
	var debugFlag = 0;
	
</script>
