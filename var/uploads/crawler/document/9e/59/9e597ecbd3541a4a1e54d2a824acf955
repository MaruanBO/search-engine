$("#fi-edu-center-tabs").bind("click", function () {
    var imgSrc = $("#eduCenterImgSrc").val();
    var activeIdx = $("#fi-edu-center-tabs").tabs().tabs('option', 'active'); // selected for version before 1.9
    // with jquery 1.7
    //var visiblePaneId = $('div.tab-pane.active').attr('id');
    //var activeTabIndex = $('#fi-edu-center-tabs a[href="#' + visiblePaneId + '"]').parent().index();
    var thumbs = $("#fi-edu-center-tabs ul li a img");
    for (var i = 0; i < thumbs.length; i++) {
        if (i != activeIdx) {
            thumbs[i].src = imgSrc + "img/fi-edu-center/Tab_Button_0" + (i + 3) + ".png";
        }
        else {
            //$(thumbs[i]).fadeIn(1000)[0].src = "Content/images/fi-edu-center/Tab_Button-Active_0" + (activeIdx + 3) + ".png";
            thumbs[i].src = imgSrc + "img/fi-edu-center/Tab_Button-Active_0" + (activeIdx + 3) + ".png";
        }
    }
});
/*======================end of functions for Investment Education Center======================*/

