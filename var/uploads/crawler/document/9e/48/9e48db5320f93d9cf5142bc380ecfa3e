function getChannel() {
	var channels = {};
	if(rs.community == "France")
		channels = { "publicsector":{ "name":"Public Sector/Secteur Public"},"business":{ "name":"Business/Entreprises"},"personal":{"name":"Personal/Particuliers"},"education":{"name":"Education/Education"},"k12":{"name":"K-12/K-12"},"highereducation":{"name":"Higher Education/Enseignement sup."},"reviews":{"name":"Reviews/Avis"},"review":{"name":"Reviews/Avis"},"Cart":{"name":"Cart/Panier"}};
	else if(rs.community == "Italy")
		channels = { "publicsector":{ "name":"Public Sector/Settore pubblico"},"business":{"name":"Business/Organizzazioni"},"personal":{"name":"Personal/Personale"},"education":{"name":"Education/Educazione"},"k12":{"name":"K-12/K-12"},"highereducation":{"name":"Higher Education/Educazione superiore"},"reviews":{"name":"Reviews/Commenti"},"review":{"name":"Reviews/Commenti"},"Cart":{"name":"Cart/Carrello"}};
	else if(rs.community == "Spain")
		channels = { "publicsector":{ "name":"Public Sector/Sector p?blico"},"business":{ "name":"Business/Organizaciones"},"personal":{"name":"Personal/Personal"},"education":{"name":"Education/Educaci?n"},"k12":{"name":"K-12/K-12"},"highereducation":{"name":"Higher Education/Educaci?n superior"},"reviews":{"name":"Reviews/Comentarios"},"review":{"name":"Reviews/Comentarios"},"Cart":{"name":"Cart/Cesta"}};
	else if(rs.community == "Germany2")
		channels = { "publicsector":{ "name":"Public Sector/?ffentlicher Sektor"},"business":{ "name":"Business/Unternehmen"},"personal":{"name":"Personal/Privatkunden"},"education":{"name":"Education/Bildung"},"k12":{"name":"K-12/K-12"},"highereducation":{"name":"Higher Education/Hochschulen"},"reviews":{"name":"Reviews/Meinungen"},"review":{"name":"Reviews/Meinungen"},"Cart":{"name":"Cart/Einkaufskorb"}};
	else
		channels = { "publicsector":{ "name":"Public Sector"},"business":{ "name":"Business"},"personal":{"name":"Personal"},"education":{"name":"Education"},"k12":{"name":"K-12"},"highereducation":{"name":"Higher Education"},"reviews":{"name":"Reviews"},"review":{"name":"Reviews"}};
		
	this.pagespath = rs.pagename.split('/');

	var c = ltrim(rtrim(rtrim(this.pagespath[1]),'#'));
	if(channels[c] != undefined)
		return channels[c].name;
	else
		{
		c = "personal";
		return channels[c].name;
		}
}

function trackMenuEvents() {
	var omevent = null;
	
	if(document.referrer.length) {
		rs.referrerhost = document.referrer.getHostname();
		rs.thishost = document.location.href.getHostname();
		if(rs.thishost == rs.referrerhost) {
			if(jQuery.inArray(rs.pagename, demoPageNames) != -1)
				omevent = 'event1';
			/*else if(jQuery.inArray(rs.pagename, methodPageNames) != -1)
				omevent = 'event2';*/
			else if(jQuery.inArray(rs.pagename, featurePageNames) != -1)
				omevent = 'event3';
			else if(jQuery.inArray(rs.pagename, languagePageNames) != -1)
				omevent = 'event4';
			/*else if(jQuery.inArray(rs.pagename, reviewsPageNames) != -1)
				omevent = 'event5';
			else if(jQuery.inArray(rs.pagename, homePageNames) != -1) 
                omevent = 'event6'; 
			else if(jQuery.inArray(rs.pagename, businessPageNames) != -1)
				omevent = 'event11';*/
			else if(RSUI.util.getCookie("pricingClicked") == 1) {
				omevent = 'event16';
				RSUI.util.setCookie("pricingClicked","0");
				}
			else if(jQuery.inArray(rs.pagename, faqpage) != -1)
				omevent = 'event38';
			if((rs.community == "United Kingdom")||(rs.community == "France") ||(rs.community == "EU") ||(rs.community == "Italy")||(rs.community == "Spain")||(rs.community == "Germany2") || (rs.community == "USA-Hispanic"))
				{
				//if((jQuery.inArray(rs.pagename, languagePageNames) != -1) || (jQuery.inArray(rs.pagename, buynowPageNames) != -1))
					//omevent = 'event16';
				}

		}
	}
	if(rs.community == "USA" || rs.community == "USA-Hispanic" || typeof ThisIsProgressPage_ != "undefined")
	{
	  if((typeof dlactivation != "undefined" && jQuery.inArray(rs.pagename, dlactivation) != -1) || typeof ThisIsProgressPage_ != "undefined")
	    omevent = 'event40';
	}	
	return omevent;
}
function getOmniPageName() {
	if(jQuery.inArray(rs.pagename, homePageNames) != -1) {
		return('/');
	}
	return rs.pagename;
}
