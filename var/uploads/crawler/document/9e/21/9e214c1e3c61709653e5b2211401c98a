			<script type="text/javascript">
/* Open contact form 7 in a fancybox popup */ 
jQuery(function($){
	$(".contact7-btn a").fancybox({
            closeBtn: true,
            padding: 30,
            afterClose: function() {
                $('.response-success, .response-error').hide();
                $('form.signup').show();
            },
            beforeShow: function() {
                $('form.signup :input').not(':button, :submit, :reset, :hidden').val('').removeAttr('checked').removeAttr('selected');
            }
        });
});

jQuery(function($){
    $('.terms').on('change', function(){
        if ($(this).is(':checked')) {
            var termsDateTime = new Date().format();
            $(this).parent().find('.terms-timestamp').val(termsDateTime);
        } else {
            $(this).parent().find('.terms-timestamp').val('');
        }
    });
    $('form.signup').on('submit', function(e){
        e.preventDefault();
        var $container = $(this).parent();

        $.ajax({
            url: $(this).attr('action'),
            type: 'POST',
            data: {
                form: $(this).serialize()
            },
            dataType: 'json',
            success: function(json) {
                if (json && json.status == 200) {
                    $container.find('form').hide();
                    $container.find('.response-success').fadeIn();
                } else {
                    $container.find('.response-error').fadeIn();
                }
            }
        });
    });
});
Date.prototype.format = function() {
    var now = this;
    var utc = new Date(now.getTime() + now.getTimezoneOffset() * 60000);
    var est = new Date(utc.getTime() - (60000 * 300));

    var y = est.getFullYear(),
    m = 1 + est.getMonth(),
    d = est.getDate(),
    h = est.getHours(),
    i = est.getMinutes(),
    a = 'AM';

    if (h > 12) {
        h -= 12;
        a = 'PM';
    }

    return m + "/" + d + "/" + y + " " + h + ":" + i + " " + a;
};
</script>
