		<button class="btn btn-primary standard-button2 ladda-button" type="submit" id="submit" name="submit" data-style="expand-left">Send Message</button>
	</form>
</div>

<script>
	jQuery("#id-54e6566b8050a").submit(function(e) {
		e.preventDefault();
		var name = jQuery("#id-54e6566b8050a-name").val();
		var email = jQuery("#id-54e6566b8050a-email").val();
		var subject = jQuery("#id-54e6566b8050a-subject").val();
		var message = jQuery("#id-54e6566b8050a-message").val();
		var dataString = 'action=contact&name=' + name + '&email=' + email + '&subject=' + subject + '&message=' + message;
		function isValidEmail(emailAddress) {
			var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
			return pattern.test(emailAddress);
		};
		if (name && email && subject && message && isValidEmail(email)) {
			jQuery.ajax({
			    type: "POST",
			    url: "http://www.bidtellect.com/wp-admin/admin-ajax.php",
			    data: dataString,
			    success: function(data) {
			    	if (data) {
					    jQuery('#id-54e6566b8050a .alert-success').fadeIn(1000);
					    jQuery('#id-54e6566b8050a .alert-danger').fadeOut(500);
						/* Clear all fields after success */
						jQuery("#id-54e6566b8050a-name").val('');
						jQuery("#id-54e6566b8050a-email").val('');
						jQuery("#id-54e6566b8050a-subject").val('');
						jQuery("#id-54e6566b8050a-message").val('');
					}
					else {
						jQuery('#id-54e6566b8050a .alert-danger').find("span").html("The wp_mail() function failed.");
						jQuery('#id-54e6566b8050a .alert-danger').fadeIn(1000);
					}
			    }
			});
		}
		else {
			jQuery('#id-54e6566b8050a .alert-danger').find("span").html("Make sure all fields are filled out and your email address is valid.");
			jQuery('#id-54e6566b8050a .alert-danger').fadeIn(1000);
		}
		return false;
	});
</script>


