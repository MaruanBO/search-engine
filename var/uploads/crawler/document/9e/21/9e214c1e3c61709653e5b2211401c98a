			<script type="text/javascript">
/* Functionality of 5 rounded buttons */
jQuery(document).ready(function($){
	$('#section14 a.btn').append('<span class="arrow-down"></span>');
	$('#section14 a.btn:nth-child(1)').addClass('btn-current');
	
	$("#section14 a.btn:nth-child(1)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section14 .ad-image").slideUp();
		$("#section14 .ad-image1").slideDown();
		$('#section14 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
	$("#section14 a.btn:nth-child(2)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section14 .ad-image").slideUp();
		$("#section14 .ad-image2").slideDown();
		$('#section14 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
	$("#section14 a.btn:nth-child(3)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section14 .ad-image").slideUp();
		$("#section14 .ad-image3").slideDown();
		$('#section14 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
		
		
});
</script>
