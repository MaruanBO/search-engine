			<script type="text/javascript">
/* Functionality of 5 rounded buttons */
jQuery(document).ready(function($){
	$('#section11 a.btn').append('<span class="arrow-down"></span>');
	$('#section11 a.btn:nth-child(1)').addClass('btn-current');
	
	$("#section11 a.btn:nth-child(1)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section11 .ad-image").slideUp();
		$("#section11 .ad-image1").slideDown();
		$('#section11 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
	$("#section11 a.btn:nth-child(2)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section11 .ad-image").slideUp();
		$("#section11 .ad-image2").slideDown();
		$('#section11 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
	$("#section11 a.btn:nth-child(3)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section11 .ad-image").slideUp();
		$("#section11 .ad-image3").slideDown();
		$('#section11 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});
	$("#section11 a.btn:nth-child(4)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section11 .ad-image").slideUp();
		$("#section11 .ad-image4").slideDown();
		$('#section11 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});	
	$("#section11 a.btn:nth-child(5)").click(function(){
		if ( $(this).hasClass('btn-current') ) {
			return false;
		}
		$("#section11 .ad-image").slideUp();
		$("#section11 .ad-image5").slideDown();
		$('#section11 a.btn').removeClass('btn-current');
		$(this).addClass('btn-current');
		return false;
	});		
		
});
</script>
