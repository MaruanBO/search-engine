        <div class="titlebar">
            <h1>
                Click of the Day</h1>
        </div>
    <ul>
            <li class="first item-0">
                    <div class="teaser imageOnTop-true imageWidth-138" itemscope itemtype="http://schema.org/CreativeWork">
        <meta itemprop="url" content="http://www.krcrtv.com/living/editorial-calendar-template/want-35-feet-of-bacon-on-your-pizza/31342304"/>
<figure  class="ib-figure-image" itemscope itemtype="http://www.schema.org/ImageObject" itemprop="associatedMedia">
        <meta itemprop="url" content="http://www.krcrtv.com/Little-Caesars-bacon-wrapped-pizza/31345004"/>
<meta itemprop="dateCreated" content="2015-02-18T03:17:21"/>
    <meta itemprop="dateModified" content="2015-02-18T03:16:57"/>
<a href="/living/editorial-calendar-template/want-35-feet-of-bacon-on-your-pizza/31342304"><img 
                src="/image/view/-/31345004/medRes/2/-/maxh/92/maxw/138/-/8lfth6/-/Little-Caesars-bacon-wrapped-pizza-jpg.jpg"
                alt="Little Caesars bacon wrapped pizza"
                title="Little Caesars bacon wrapped pizza"
                
                />
    </a><figcaption>
                <meta itemprop="caption description" content="Little Caesars bacon wrapped pizza"/>
