	<script>
		//this file holds scripts that are important for the initial view of the header
//top bar cart ** chanegd to be included in html
(function(){
	var getCookie = function(cookie){
		if(document.cookieObj)return document.cookieObj[cookie];
		var parts = document.cookie.split(";"), i, parts, part, name, val;
		document.cookieObj = {}
		for(i=0;i<parts.length;i++){
			part = parts[i].split("=");
			name = part.shift().trim();
			val = part.join("=").trim();
			document.cookieObj[name] = val;
		}
		return getCookie(cookie);
	};
	var uui = getCookie("uui"),
		topBarCart = getCookie("TopBarCart"),
		ttldlr = document.getElementById("js-ttldlr"),
		ttlitm = document.getElementById("js-ttlitm"),
		jslogin = document.getElementById("js-login"),
		loginame = document.getElementById("js-loginname"),
		phonenum = document.getElementById("js-phonenum");
	
	if(topBarCart && ttldlr){
		topBarCart = topBarCart.split("|");
		ttlitm.textContent = ttlitm.innerText = topBarCart[0];
		ttldlr.textContent = ttldlr.innerText = topBarCart[1];
	}
	if(jslogin && uui){
		uui = uui.split("|");
		if(phonenum)phonenum.textContent = phonenum.innerText = decodeURIComponent(uui[0]);
		if(uui[1]){
			loginame.textContent = loginame.innerText = decodeURIComponent(uui[1]);
			jslogin.className +=" js-loggedIn";
		}
	}
}());
	</script>
	
