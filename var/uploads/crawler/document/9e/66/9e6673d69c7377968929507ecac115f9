    function ga_trackDimensions(typeLabel) {
        var viewportWidth = $(window).width();
        var viewportHeight = $(window).height();
        _gaq.push(
          ['_trackEvent', 'Viewport Dimensions', 'Viewport Width ' + typeLabel, viewportWidth + 'x' + viewportHeight, viewportWidth, true],
          ['_trackEvent', 'Viewport Dimensions', 'Viewport Height ' + typeLabel, viewportWidth + 'x' + viewportHeight, viewportHeight, true]
