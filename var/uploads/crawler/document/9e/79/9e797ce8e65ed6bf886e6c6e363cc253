<script language="javascript" type="text/javascript">

function findPos(obj)
{
	var curleft = curtop = curwidth = 0;
	if (obj.offsetParent) {
		curleft = obj.offsetLeft
		curtop = obj.offsetTop
		curwidth = obj.offsetWidth
		while (obj = obj.offsetParent) {
			curleft += obj.offsetLeft
			curtop += obj.offsetTop
		}
	}

	return [curleft,curtop];
}

function positionLogon()
{
	var posEl = document.getElementById("tdLogon");
	if ( posEl != null )
    {
		var pos = findPos(posEl);
        var logonEl = document.getElementById("top-container-myacct-logon");

        if ( logonEl != null )
        {
			logonEl.style.left = pos[0];
            logonEl.style.top = pos[1];
            logonEl.style.visibility = "visible";
        }
   }
}

function checkLogonNuggetKey(e)
{
	if (e.keyCode == 13)
	{document.LogonNuggetForm.submit();}
}

var enterSelect = null;
function turnOffSearchSelects() {              
    var hasDom = document.getElementById?1:0;
	if(enterSelect != null)
	{
		div = enterSelect;		
		obj=hasDom?document.getElementById(div).style:bw.ie4?document.all[div].style:bw.ns4; 
		if(obj.display=='block') 
		{
			obj.display='none';
			enterSelect = null;			
		}				
	}
}

window.onresize=function() { positionLogon(); };
window.onload=function(){javascriptIsEnabled();footerNav();positionLogon();}
//window.onfocus=function() { positionLogon(); };
</script><div id="top-container-myacct-logon" style="position:absolute;text-align:left;visibility:hidden;z-index:0" >
<h3>Account Log On</h3>
<form class="gh-form" action="https://www.cdw.com/shop/eaccount/logon/logonprocessor.aspx" name="LogonNuggetForm" id="LogonNuggetForm" method="post" autocomplete="off">
<input class="txtbox" tabindex="101" MaxLength="50" name="UserName" value=" Enter User Name" onFocus="if(this.value==' Enter User Name'){this.value=''}" onBlur="if(this.value==''){this.value=' Enter User Name'}" >
<span class="margin-bottom5 underline"><a href="https://www.cdw.com/shop/eaccount/logon/forgotusername1.aspx?Site=F95919EFD581468DA0C741A07BFD7E96&Target=default.aspx">Forgot User Name?</a></span>
<input class="txtbox" tabindex="102" MaxLength="50" name="UserPassword" type="password" onkeypress="script:checkLogonNuggetKey(event);">
<input type="hidden" name="Target" value="/default.aspx">
<span class="margin-bottom5 underline"><a href="https://www.cdw.com/shop/eaccount/logon/forgotpassword1.aspx?Site=F95919EFD581468DA0C741A07BFD7E96&Target=default.aspx">Forgot Password?</a></span>
