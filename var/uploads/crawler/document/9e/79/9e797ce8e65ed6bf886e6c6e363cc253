<script type="text/javascript">
    function SubmitSearch() {

        $.ajax({
            url: "/api/searchcookie?action=reset",
            type: "post",
            timeout: 200,
            success: function () { },
            error: function () {
                if (window.console) console.log('error when clearing search cookie');
            },
            complete: function () {
                var key = $("#searchbox2").val() == 'Search for...' ? '' : $("#searchbox2").val();
                var url = "/shop/search/result.aspx?key=" + key + "&wclsscat=" + $("#wclss").val() + "&b="
                    + $("#brandFilter").val() + "&p=" + $("#selectPriceRanges").val() + "&ctlgfilter=" + $("#ctlgfilter").val() + "&searchscope=all&sr=1";
                window.location.href = url;
            }
        });
    }
</script>
