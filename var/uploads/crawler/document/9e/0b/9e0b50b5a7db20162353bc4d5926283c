        var reviews = [];
    var reviewers = [];
    for (var r = 0; r < bizrateReviewsGroup.length; r++) {
        var htmlGroup = jQuery(bizrateReviewsGroup[r]);
        for (var rg = 0; rg < htmlGroup.length; rg++) {
            var reviewPart = jQuery(htmlGroup[rg]);
            if (reviewPart[0].className == 'review_text') {
                reviewPart[0].className = 'review-text';
                reviews.push(htmlGroup[rg]);
            } else if (reviewPart[0].className == 'review_name') {
                reviewPart[0].className = 'review-name';
                reviewers.push(htmlGroup[rg]);
            }
        }
    }
    for (var rs = 0; rs < reviews.length; rs++) {
        bizrateReviews.push({
            text: reviews[rs],
            name: reviewers[rs]
        });
    }

    var fader = jQuery('#reviews-fader');
    for (var i = 0; i < bizrateReviews.length; i++) {
        var thisReview = bizrateReviews[i].text;
        jQuery(thisReview).html(jQuery(thisReview).html() + '"');
        var thisReviewer = bizrateReviews[i].name;
        var thisContainer = jQuery('<div class=\'review-box\'></div>');
        thisContainer.append(thisReview).append(thisReviewer);
        fader.append(thisContainer);
    }

    (function startFader() {
        var review = fader.find('.review-box').filter(':first');
        if (review.html().length >= 250) {
            review.remove();
            startFader();
            return;
        }
        review.fadeIn('fast', function() {
            setTimeout(function() {
                review.fadeOut('slow', function() {
                    review.detach();
                    fader.append(review);
                    startFader();
                })
            }, 4000);
        });
    })();
</script>
