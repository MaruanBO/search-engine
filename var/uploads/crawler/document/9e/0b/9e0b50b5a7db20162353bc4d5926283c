<script type="text/javascript">
    $j(document).ready(function() {
        var nlRequest;

        $j('#signupForm4ink').on('submit', function(e) {
            if (nlRequest) {
                nlRequest.abort();
            }

			var $email = $j(this).find('input[name="email"]'),
				$data,
				$tag = $j(this).find('input[name="tag"]');

            if ($email.val().match(/^[^@]+@[^@]+\.[^@]+$/))
            {
                $data = {
                    email: $email.val(),
					tag: $tag.val(),
                    ajax: true
                }

                $email.prop('disabled', true).css({
                    color: '#cccccc',
                    fontStyle: 'italic'
                });

                nlRequest = $j.ajax({
                    url: "http://www.4inkjets.com/signup.php",
                    type: "get",
                    data: $data,
                    success: function(response, textStatus, jqXHR) {
                        var notification = $j('.tooltip-notification');
                        if (response == 'already subscribed') {
                            // runs if successful
                            notification
                                .css('display','none')
                                .removeClass('tooltip-error')
                                .html('This email is already subscribed')
                                .fadeIn(1000, function() {
                                    setTimeout(function() {
                                        if (!notification.hasClass('tooltip-error')) {
                                            notification.fadeOut(1000);
                                        }
                                    }, 3000);
                                });
                        } else {
                            // runs if successful
                            notification
                                .css('display','none')
                                .removeClass('tooltip-error')
                                .html('<b>Thank you!</b><br />You have been subscribed')
                                .fadeIn(1000, function() {
                                    setTimeout(function() {
                                        if (!notification.hasClass('tooltip-error')) {
                                            notification.fadeOut(1000);
                                        }
                                    }, 3000);
                                });
                        }
                        $email
                            .val('')
                            .prop('disabled', false)
                            .css({
                                color: '#000',
                                fontStyle: 'normal'
                            });
                    },
                    error: function(response, textStatus, errorThrown) {
                        // runs if error
                        $j('.tooltip-notification')
                            .html('<b>Sorry!</b><br />There was an error!')
                            .addClass('tooltip-error')
                            .fadeIn(1000);
                        $email
                            .prop('disabled', false)
                            .css({
                                color: '#000',
                                fontStyle: 'normal'
                            });
                    }
                });

            } else {

                $j('.tooltip-notification')
                    .html('<b>Oops!</b><br />Invalid Email Address')
                    .addClass('tooltip-error');
                $j('.tooltip-notification').fadeIn(1000);
            }

            e.preventDefault();
        });

        $j('#signupForm4ink #email')
            .keypress(function() {
                var notification = $j('.tooltip-notification');
                if (notification.hasClass('tooltip-error')) {
                    notification.fadeOut(1000);
                }
            })
    })

    if (window.XMLHttpRequest) {
        // code for IE7+, Firefox, Chrome, Opera, Safari
        xmlhttp=new XMLHttpRequest();
    } else {
        // code for IE6, IE5
        xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
    }

    xmlhttp.onreadystatechange=function()
    {
        if (xmlhttp.readyState==4 && xmlhttp.status==200)
        {
            document.getElementById("lastorder").innerHTML=xmlhttp.responseText;
        }
    }
    xmlhttp.open("GET","ajax_get_last_order.php",true);
    xmlhttp.send();
</script>
<!-- right column ends here -->
