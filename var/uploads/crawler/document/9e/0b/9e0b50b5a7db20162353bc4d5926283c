<script>

    jQuery(document).ready(function() {
        jQuery('.dropdown').not('.dropdown:eq(0)').addClass('unfocused');
    });

    /* Finder */
    function clear_options(id) {
        var el = document.getElementById(id);
        for(var i = el.length; i > 0; i--) {
            el[i] = null;
        }
    }

    function finder_pop(id, id_field, el_id, action) {
        if(id == '') return;
        var ajax = new sack("catalog.php");
        var sel = document.getElementById(el_id);
        ajax.setVar('_ajax', action);
        ajax.setVar(id_field, id);
        if (action == 'finder_get_printers_by_category' && el_id == 'finder_printer2') {
            var parent_id = document.getElementById('finder_manuf2').value;
            ajax.setVar('parent_id', parent_id);
        }
        ajax.method = 'GET';
        ajax.onCompletion = function() {
            clear_options(el_id);
            var p = ajax.response.split("\n");
            for(var i = 0; i < p.length; i++) {
                if(p[i] == '') continue;
                var p2 = p[i].split("|");
                if(p2.length != 2) continue;
                sel[sel.length] = new Option(p2[1], p2[0]);
            }
            jQuery('#'+el_id).easyDropDown('destroy');
            jQuery('#'+el_id).easyDropDown({cutOff: 12});
        };
        ajax.runAJAX();
    }

    jQuery('#finder_manuf2').on('change', function() {
        var id = jQuery(this).val();
        var id_field = 'manuf_id';
        finder_pop(id, id_field, 'finder_model2', 'finder_get_models');
        jQuery('.dropdown:eq(1)').removeClass('unfocused');
    });

    jQuery('#finder_model2').on('change', function() {
        var id = jQuery(this).val();
        finder_pop(id, 'printer_category', 'finder_printer2', 'finder_get_printers_by_category');
        jQuery('.dropdown:eq(2)').removeClass('unfocused');
    });

    jQuery('#finder_printer2').on('change', function() {
        if (jQuery('#finder_printer2').val() == "") return;
        window.location.href = 'http://www.4inkjets.com/catalog.php?path=printer&relation=HDD&internal=PrinterFinder&id='+jQuery(this).val();
    });
</script><script>
