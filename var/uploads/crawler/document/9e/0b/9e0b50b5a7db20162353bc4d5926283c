var ld_pixels = [], ld_noscripts = [];

(function (w) {
    var old = w.onload;
    w.onload = function () {
        if (typeof old === 'function') {
            old();
        }
        setTimeout(loadPixels, 50);
        function loadPixels() {
            var pixelHTML = '', noscriptHTML = '';
            for (var i = 0, len = ld_pixels.length; i < len ; i++) {
                pixelHTML += ld_pixels.join(' ');
            }
            
            for (var i = 0, len = ld_noscripts.length; i < len ; i++) {
                noscriptHTML += ld_noscripts.join(' ');
                pixelHTML += '<noscript>' + noscriptHTML + '</noscript>';
            }
            document.getElementById('pixels').innerHTML = pixelHTML;
        }
    }
}(window));

YUI().use('node', function(Y) {
    
    var body = Y.one(document.body);
    function init() {
try {
$LAB
.script("http://upfront.thefind.com/scripts/main/utils-init-ajaxlib/upfront-badgeinit.js")
.wait(function() { thefind.upfront.init('tf_upfront_badge', '00dd0313603d71235197d02ce3503a07');});
} catch (e) {
}

try {
YUI().use("node", "transition", "node-event-simulate", function (Y) {
    var mask = Y.one("#mask");
    var promo = Y.one("#news_signup");
    var input = Y.one("#email");
    var  btn = Y.one("#news_signup button");
    var  errorMsg = Y.one("#errorMsg");
    var re =  /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;

    if (mask) {
        mask.setStyle('opacity', '0', 'display', 'none');
    }
    if (promo) {
        promo.setStyle('opacity', '0', 'display', 'none');
    }

    function closePromo() {
        var mask = Y.one("#mask");
        var promo = Y.one("#news_signup");
        promo.transition({ opacity: '0' });
        mask.transition({ opacity: '0' });
	document.getElementById('mask').style.display = 'none';
	document.getElementById('news_signup').style.display = 'none';
    }
function checkInput(e) {
            var theKey = e.which,
                    theValue = input.get("value");
            switch (theKey) {
                case 13:
                    e.preventDefault();
                    btn.simulate("click");
                    break;
                default:
                    if (theValue == "Enter e-mail address") {
                        input.set("value", "");
                        input.setStyles({
                            color: "#000000",
                            fontStyle: "normal"
                        });
                    }
            }
        }

        function checkValid() {
            var theValue = input.get("value");
            if (re.test(theValue)) {
                input.setStyle("color", "#000000");
            }
        }

        function returnInputStyle() {
            if (input.get("value") == "")  {
                input.set("value","Enter e-mail address");
                input.setStyles({
                    color: "#949494",
                    fontStyle: "italic"
                });
            }
        }

        function validateSignupform() {
            var theEmail = Y.one("#email").get("value");
            if (re.test(theEmail)) {
                submitSignupform2();
            } else {
                input.setStyle("color","#FF0000");
                errorMsg.setStyle("display", "block");
            }
        }

    Y.on('load', function(){
        var mask = Y.one("#mask");
        var promo = Y.one("#news_signup");

        if (mask) {
            mask.transition({ opacity: '0.7', duration: 1, 
              on: {
                      end: function() {
                        promo.transition({ opacity: '1', duration: 0.7 })
                      }
                  }
             });
        }
    });
     input.on("blur", returnInputStyle);
        input.on("keydown", checkInput);
        input.on("keyup", checkValid);

        if (btn) {
            btn.on("click", validateSignupform);
        }

        Y.on('keydown', function(e) {
            if (e.which == 27) {
                closePromo();
            }
        });
    Y.all('.promo_overlay').on('click', closePromo);
});
} catch (e) {
}

