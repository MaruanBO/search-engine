updateLoginStatus(function(userStatus) {
    var menuItem = document.getElementById('loginlogout');
    if (userStatus.logged_in) {
      var updates = '<li class="menu-logoff"><a href="';
      if (userStatus.fb && userStatus.fb_logout)
        updates += userStatus.fb_logout;
      else
        updates += 'logoff.php';
      updates += '" class="pic2" title="Logoff" rel="nofollow"><span>Logoff</span></a></li>';
      menuItem.outerHTML = updates;
    }  
});
