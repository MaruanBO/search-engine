<script>
	var novicaSearch = {
		go: function(el) {
			var searchWord;
			if(el.keywordHeader.value) { 
				searchWord = el.keywordHeader.value.replace(/ /gi,"-");
				if(window.location.toString().indexOf('dev.')>=0) {
					location.href="http://dev.novica.com/" + encodeURI(searchWord) + "/s/";
				} else {
					location.href="http://www.novica.com/" + encodeURI(searchWord) + "/s/";	
				}
			}
			return false; 
		}
	}
</script>
