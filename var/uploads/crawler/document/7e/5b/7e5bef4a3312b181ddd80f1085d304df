	<script>				
function openPromoDialog( type ) {
	var displayDialog = 0;
	var divIcon = 'premiumgift';
	var divID, divTitle;
	switch(type) {
		case "earlyblackfriday":
			displayDialog = 1;
			divID = "dialog-box-earlyblackfriday";
			divTitle = "Early Black Friday Codes";
			break;
		case "blackfriday":
			displayDialog = 1;
			divID = "dialog-box-blackfriday";
			divTitle = "Black Friday Codes";
			break;
		case "artisanmonday":
			displayDialog = 1;
			divID = "dialog-box-artisanmonday";
			divTitle = "Artisan Monday Codes";
			break;
	}
	if(displayDialog) {
		if (jQ('.divDialogCommon').is(':data(dialog)')) {
	        jQ('.divDialogCommon').dialog('destroy');
	    }
	    jQ('.divDialogCommon').empty();
	    jQ('.divDialogCommon').html(jQ('#' + divID).html());
		
	    buildDialog(divTitle, divIcon, 500, 300);

		jQ('.divDialogCommon').dialog('open');
	}
}
</script>

