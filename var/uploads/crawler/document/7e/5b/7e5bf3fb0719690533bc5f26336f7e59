<script>
$(document).ready(function() {
        $.cookie.raw = true;
	var url = document.URL;
	var rootURL = location.protocol + '//' + location.host;
	var baseURL = document.URL.split('?')[0];
	baseURL = baseURL.replace('lucky', 'm.lucky');
	baseURL = baseURL.replace('Sites-LuckyBrand-Site', 'Sites-LuckyBrandMobile-Site');
	var param = document.URL.split('?')[1];
	if(param && (param != 'stop_mobi=true')){
		baseURL += "?" + param + '&stop_mobi=false';
	} else {
		baseURL += '?stop_mobi=false';
	}
	if(url.indexOf('stop_mobi=true') >= 0 ){
		$.cookie('viewDesktop', 1, { path: '/' });
	}
	if ($.cookie('viewDesktop')){
		$('.viewDesktop').append('<a href="' +baseURL +'">View Mobile</a>');
	}

});
</script>
