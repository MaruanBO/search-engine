<script type="text/javascript">
	$(window).resize(function(){
		var winheight = $(window).height();
		var winwidth = $(window).width();
		$("#signup-dropdown").css("width",winwidth+"px");
	});
function signup_dropdown(){
	$("#noclick").show().css("background-color","rgba(64,64,65,.8)");
	$("#bottom-bar-cont").stop().animate({bottom: "-120px"}, 800);
	if($("#signup-dropdown-form").html() == ""){
		$.get( "http://www.mommypage.com/wp-content/themes/wp-ellie/signup_form.php", function( data ) {
			$( "#signup-dropdown-form" ).html( data );
		});
	}
	var winheight = $(window).height();
	var winwidth = $(window).width();
	var time = 2000;
	$("#signup-dropdown").css("width",winwidth+"px").css("margin-top","-"+winheight+"px").show().animate({marginTop: "0px"},time);
	$("html, body").animate({ scrollTop: $("#signup-dropdown").offset().top},time);
}
$("#signup-dropdown .closediv").click(function(){
	var winheight = $(window).height();
	var time = 2000;
	$("#signup-dropdown").animate({marginTop: "-1000px"},time);
	$("html, body").animate({ scrollTop: $("body").offset().top},time);
	window.setTimeout(function(){
		$("#signup-dropdown").hide();
		$("#noclick").hide();
	},time);
});
</script><div id="page-contnr">
