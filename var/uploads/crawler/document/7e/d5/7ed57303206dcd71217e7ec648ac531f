<script type="text/javascript">
( function( $ , undef ) {
    var 
        testimonials,
        testimonialsList,
        midPost = 5,
        mostLeft = 0,
        carouselInterval,
        direction = -1,
        resetCarousel = function ()
        {
            if (undef !== carouselInterval)
                clearInterval(carouselInterval);
                
            carouselInterval = setInterval(function(){
                spinCarousel(1);
            }, 8000);
        },
        spinCarousel = function (diff){
            $(testimonialsList).animate({left: '-=115'}, 300);
            $(testimonials[(midPost + 1) % testimonials.length]).animate({width: '960px'}, 300);
            $(testimonials[midPost]).animate({
                width: '104px'
            }, 300, function(){
               // Relocate element.
                var shifted = testimonials[mostLeft];
                testimonialsList.append($(shifted).parents('li'));
                
                $(testimonialsList).css({left: '+=115'});
                
                mostLeft = (mostLeft + 1) % testimonials.length;
                midPost = (midPost + 1) % testimonials.length;
            });
        };
        
    $(document).ready(function(){
        testimonials = $('#testimonials article');
        testimonialsList = $('#testimonials ul');
        /*testimonials = $('#testimonials article .profilepic').click(function(){
            
        });*/
    });
    
    $(document).ready(function(){
        resetCarousel();
    });        
} )( jQuery );
</script>
