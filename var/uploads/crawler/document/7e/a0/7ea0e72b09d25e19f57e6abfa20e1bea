                if (window.addEventListener)
                    window.addEventListener('message', openDemoPopup, false);
                else if (window.attachEvent)
                    window.attachEvent('message', openDemoPopup, false);
                function openDemoPopup(event)
                {
                    if (event.origin !== "http://my.nanorep.com" && event.origin !== "http://cdn-my.nanorep.com") return;
                    if (event.data == "demoPopupShow")
                        $('#reqDemoMob').click();
                }

                if ($(window).width() < 641)
                {

                    var ele = $('#reqDemoMob');
                    ele.removeClass('colorbox cboxElement');
                    ele.unbind('click');
                    ele.bind('click', function ()
                    {
                        event.preventDefault();
                        var h = $(window).height() - 74;
                        $("#pageContents").css("display", 'none');
                        $("#generalFooter").css("display", 'none');
                        var iframe = document.createElement("iframe");
                        iframe.src = this.getAttribute("href");
                        iframe.style.width = "100%";
                        iframe.style.height = h + "px";
                        iframe.style.minHeight = "420px";
                        iframe.setAttribute("id", "contactIframeElement1");
                        iframe.setAttribute("frameborder", "0");
                        iframe.setAttribute("scrolling", "no");
                        iframe.style.position = "absolute";
                        iframe.style.top = "40px";
                        iframe.style.bottom = "0px";
                        iframe.style.left = "0px";
                        iframe.style.zIndex = "2";
                        iframe.style.backgroundColor = "#ffffff";
                        var closeDiv = document.createElement("div");
                        closeDiv.setAttribute("id", "CloseButtonIframe");
                        $(".Content").attr("style", "position:relative;z-index:100");

                        document.body.appendChild(iframe);
                        document.body.appendChild(closeDiv);
                        closeDiv.setAttribute("style", "cursor:pointer;width: 30px;padding-top: 40px;height: 29px;line-height:29px;text-align: center;font-weight: normal;font-size: 18px;position: absolute;top: 0px;right: 0px;color: #0099cc;z-index: 13;");
                        closeDiv.innerHTML = "X";


                        closeDiv.onclick = function ()
                        {
                            document.body.removeChild(iframe);
                            $("#pageContents").css("display", 'block');
                            $("#generalFooter").css("display", 'block');
                            document.body.removeChild(closeDiv);
                            $(".Content").attr("style", "position:relative;");
                        }


                    });
                }
                else
                {
                    if (document.getElementById("CloseButtonIframe"))
                    {
                        document.getElementById("aspnetForm").style.display = '';
                        document.body.removeChild(document.getElementById("CloseButtonIframe"));
                        document.body.removeChild(document.getElementById("contactIframeElement"));
                    }

                    var ele = $('#reqDemoMob');
                    ele.unbind('click');
                    ele.addClass('colorbox cboxElement');
                }

        </script>
        </div>
    </div>
</div>
