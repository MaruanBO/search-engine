<script>

$jq(document).ready(function() {
	securitize();
});


function securitize(){
	  var domain = window.location.host;
	  $jq('a[type]').each(function(){
		 var $this = $jq(this), 
		   anchorType = $this.attr('type'), 
		   anchorHref = $this.attr('href'), 
		   start = '';
		 if (anchorType == 'secure') { 
			  start = 'https://' + domain + '/'; 
		 } else if (anchorType == 'nonsecure') { 
			  start = 'http://' + domain + '/'; 
		 }
		 $this.attr('href', anchorHref.replace(/^(?:https?:\/\/.*?)?\//, start));
	  });
 }


</script>

