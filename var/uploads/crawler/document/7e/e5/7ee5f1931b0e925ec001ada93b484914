		var str='{cheetahMailFullDomainName=www.lastcall.com, cheetahMailDomainName=lastcall}';
		//Replace the { } from in the string
		str=str.replace(/^\{+/, "").replace(/\}$/,'');
		var splitStr = str.split(",");
		var jsGenericObject = {};
		for(i=0;i < splitStr.length ; i++) {
			//Trim any whitespces in the key
		  	var propName=(splitStr[i].split("="))[0].replace(/\s/g, "");
			var propValue=(splitStr[i].split("="))[1];
		  	jsGenericObject[propName]=propValue;
		}
	</script>


	
	
