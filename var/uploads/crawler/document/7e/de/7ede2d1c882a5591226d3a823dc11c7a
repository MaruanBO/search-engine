<script>
OnInit(function () {
	var selector, ac = [], included = [],
		search = function (tag) {
			tag = tag.toLowerCase();
			results = [];

			for(var k = 1; k <= 2; k++) {
				for (var i = 0; i < ac.length; i++) {
					var r = ac[i];

					if (included.indexOf(r[0]) == -1 && r[k].toLowerCase().indexOf(tag) != -1) {
						results.push({
							id: r[0],
							value: r[1],
							icon: '',
							imps: r[3],
							twitter: r[4],
						});
						included.push(r[0]);

						if (results.length == 5) return results;
					}
				}
			}

			return results;
		}, req, $ac = $('.filter .search form input[name="tagsearch"]');

	if ($ac.length)
		$ac.autocomplete({source: function (request, response) {
			var term = request.term;

			if (!ac.length)
			{
				if (req) req.abort();
				req = $.ajax({
					url: '/pub/nosvn/autocomplete/44c4e416dc0bbb79dbded02c73d2a4e0.json',
					dataType: 'json',
					cache: true,
					success: function (data) {
						for (var i = 0; i < data.length; i++) ac.push(data[i]);
						response(search(term));
					}
				});
			} else response(search(term));
		}, minLength: 3, appendTo: '.autocomplete', select: selector = function (event, ui) {
			event.preventDefault();
			if (typeof ui.item == 'undefined')
				$('.search form').submit();
			else
				window.location = "https://beaconads.com/buy/detail/" + ui.item.id;
		}, open: function (event, ui) {
			$('.ui-autocomplete').attr('style', '');
			if (!$('.autocomplete #viewmore').length)
			{
				$('.autocomplete ul').eq(0).append('<li class="moreresults ui-menu-item" id="viewmore"><a href="#" data-autocomplete="">View More Results</a></li>');
				$('#viewmore').unbind('click').click(function () { $('.search form').submit(); });
			}
		}, delay: 300 }).data('autocomplete')._renderItem = function(ul, item) {
			return $('<li></li>')
				.data('item.autocomplete', item)
				.append("<a class=\"gatrack\" rel=\"Autocomplete;Search;" +
					$('.search form input[name="tagsearch"]').val() + "\">" +
					"<span class=\"propname\">" +
					item.value + "</span><span class=\"impscount\">" +
					(item.imps ? item.imps + " Impressions" : item.twitter + " Followers") + "</span></a>")
				.appendTo(ul);
	    };
});
</script>
