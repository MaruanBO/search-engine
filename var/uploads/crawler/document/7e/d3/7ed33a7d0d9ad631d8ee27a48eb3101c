function writit(text,id)
{
 
 // alert('entering writit');
  if (document.getElementById)
   {
     x = document.getElementById(id);
     x.innerHTML = '';
     x.innerHTML = text;
   }
}

//alert("screen resolution = " + screen.width + " * " + screen.height);
function pwWindow(url, wname)
{ 
 
  // alert("screen resolution = " + screen.width + " * " + screen.height);

  restrictedFile=wname;
  var width = Math.round(.31*screen.width);
  var height = Math.round(.23*screen.height);
  var left = Math.round((.5*screen.width) - (.5*width)); 
  var top = Math.round((.5*screen.height) - (.5*height)); 

 //onclick="window.open('page.asp','QuotePuzzler','width='+width+',height='+height+',left='+((screen.width-width)/2)+',top='+((screen.height - height)/2)+',toolbar=no,menubar=no,scrollbars=yes, status=no,resizable=no');">
 
  	
        if (childWin) childWin.close()
   //childWin=window.open(url,'child1',"top="+top+",left="+left+",width="+width+",height="+height+",menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
 


     if (screen.width==1024) {
	childWin=window.open(url,'child1',"top=377,left=300,width=320,height=160,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width==1280) {
	childWin=window.open(url,'child1',"top=492,left=420,width=400,height=180,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width==800) {
	childWin=window.open(url,'child1',"top=290,left=240,width=256,height=126,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width==640) {
	childWin=window.open(url,'child1',"top=230,left=192,width=205,height=101,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }

else if (screen.width > 1600) {
	childWin=window.open(url,'child1',"top=600,left=510,width=544,height=268,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width > 1400) {
	childWin=window.open(url,'child1',"top=530,left=450,width=480,height=236,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }

else if (screen.width > 1280) {
	childWin=window.open(url,'child1',"top=470,left=402,width=429,height=211,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }

else if (screen.width > 1100) {
	childWin=window.open(url,'child1',"top=410,left=357,width=381,height=187,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width > 900) {
	childWin=window.open(url,'child1',"top=350,left=300,width=320,height=160,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }

else if (screen.width > 700) {
	childWin=window.open(url,'child1',"top=280,left=240,width=256,height=126,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else if (screen.width > 500) {
	childWin=window.open(url,'child1',"top=200,left=180,width=192,height=95,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }
else {
	childWin=window.open(url,'child1',"top=120,left=120,width=128,height=63,menubar=0,resizable=1,scrollbars=0,status=0,titlebar=0,toolbar=0");
     }

}


