 { 
   toggleMe('AboutThisSiteMsg');
 }

function BHS_IMG_display_Rtn(){
 var adjustment = .74 - ((1900 - screen.width)/876*.06);
 var table_screen_width = Math.round(adjustment * screen.width);
//alert("adjustment = " + adjustment);
 //alert("table_screen_width = " + table_screen_width);

// if (ie)
//     {table_screen_width = table_screen_width *1.01;}

       var IMG_str = '<IMG src="Images/BHS904.jpg"  alt="Bladensburg High School Class of 1959, Maryland MD" border="0" width="' + table_screen_width + '">';

    writit(IMG_str,'Display_BHS_Photo');
 //   toggleMe('Display_BHS_Photo');

}



