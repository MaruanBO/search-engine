 function Display_Forecast_Rtn(url){
 
   var Forecast_Str = '<iframe align="center" ' +
    'width="130px" height="100pc" scrolling="no"' + 
    ' src=' +
    '"' + url + '"' + 
    ' frameborder="0" allowfullscreen>' +
    '</iframe>';
    writit(Forecast_Str,'Display_Forecast_Inner');

  }

function getForecast(x)
{
//alert("x = " + x);
var url = "http://www.weatherreports.com/banners/125x125_white.html?location=" + x + "&units=f";
Display_Forecast_Rtn(url);
}
 
 function Initial_Forecast_Display_Rtn(){
var Display_Forecast_str = '<table style="vertical-align:top;" border="0"  width="130px" height="100" bgcolor="#ffffff">' +
'<tr align="center">' +
'<td class="noborder" width="130px" height="100" valign="middle">' +
'<center>' +
 '<div id="Display_Forecast_Inner" >' +
 '</div>' +
'</center>' +
'</td>' +
'</tr>' +
'<tr>' +
'<td width="130px" class="noborder"><B>Enter zip code</b></td>' +
'</tr>' +
'<tr>' +
'<td width="130px" class="noborder"> <input TYPE="TEXT" NAME="zipcode" size="6" value="" onchange="getForecast(this.value)"></td>' +
'</tr>' +
'</table>';

    writit(Display_Forecast_str,'Display_Forecast');
    toggleMe('Display_Forecast');
}

