 function getXY(event)
 {
  DisplayPromptMsg.style.left=event.clientX
  DisplayPromptMsg.style.top=event.clientY
  //alert("x,Y = " + event.clientX + "," + event.clientY);
 
 }



  var Xvalue;
  var Yvalue;

 function getXY2() {
	if (window.Event) {
	document.captureEvents(Event.MOUSEMOVE);
	}
	document.onclick = getCursorXY;
}

function getCursorXY(e) {
	Xvalue = (window.Event) ? e.pageX : event.clientX + (document.documentElement.scrollLeft ? document.documentElement.scrollLeft : document.body.scrollLeft);
	Yvalue = (window.Event) ? e.pageY : event.clientY + (document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop);
 //alert("x = " + Xvalue);
 //alert("y = " + Yvalue);
}


function displayform(url)
{ 
  
        if (newwindow1) newwindow1.close()
	//newwindow1=window.open(url,'abc',"menubar=1,resizable=1,scrollbars=1,status=0,titlebar=0,toolbar=0");
	newwindow1=window.open(url,'abc');
	if (window.focus) {newwindow1.focus()}
        window.open('','_self','');
        window.close();
}

