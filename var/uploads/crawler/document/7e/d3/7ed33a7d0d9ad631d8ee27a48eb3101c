|David Sedaris~12|26|1954|Ozzie Smith~12|26|1947|Carlton Fisk~12|26|1945|John Walsh~12|26|1940|Phil Spector~12|26|1933|Carroll Spinney~12|26|1927|Alan King~12|26|1921|Steve Allen~12|26|1914|Richard Widmark~12|26|1893|Mao Tse-tung~12|26|1891|Henry Miller~12|26|1791|Charles Babbage~12|27|1975|Heather O'Rourke~12|27|1966|Bill Goldberg~12|27|1948|G&eacute;rard Depardieu~12|27|1943|Cokie Roberts~12|27|1939|John Amos~12|27|1906|Oscar Levant~12|27|1901|Marlene Dietrich~12|27|1879|Sydney Greenstreet~12|27|1822|Louis Pasteur~12|27|1571|Johannes Kepler~12|28|1978|John Legend~12|28|1954|Denzel Washington~12|28|1954|Gayle King~12|28|1946|Edgar Winter~12|28|1940|Don Francisco~12|28|1934|Maggie Smith~12|28|1933|Nichelle Nichols~12|28|1931|Martin Milner~12|28|1921|Johnny Otis~12|28|1911|Sam Levenson~12|28|1908|Lew Ayres~12|28|1905|Cliff Arquette~12|28|1905|Earl `Fatha` Hines~12|28|1883|Hendrik Meijer~12|28|1856|Woodrow Wilson~12|29|1972|Jude Law~12|29|1966|Bryan `Dexter` Holland~12|29|1954|Ed Autry~12|29|1947|Ted Danson~12|29|1946|Marianne Faithfull~12|29|1938|Jon Voight~12|29|1936|Mary Tyler Moore~12|29|1879|Billy Mitchell~12|29|1808|Andrew Johnson~12|29|1800|Charles Goodyear~12|30|1980|Eliza Dushku~12|30|1977|Laila Ali~12|30|1975|Tiger Woods~12|30|1961|Sean Hannity~12|30|1959|Tracey Ullman~12|30|1957|Matt Lauer~12|30|1953|Meredith Vieira~12|30|1946|Patti Smith~12|30|1945|Davy Jones~12|30|1942|Michael Nesmith~12|30|1939|Del Shannon~12|30|1937|Noel Paul Stookey~12|30|1935|Sandy Koufax~12|30|1934|Russ Tamblyn~12|30|1928|Bo Diddley~12|30|1920|Jack Lord~12|30|1914|Bert Parks~12|30|1865|Rudyard Kipling~12|31|1972|Joe McIntyre~12|31|1959|Val Kilmer~12|31|1958|Bebe Neuwirth~12|31|1948|Donna Summer~12|31|1947|Tim Matheson~12|31|1946|Diane von Furstenburg~12|31|1945|Barbara Carrera~12|31|1943|Ben Kingsley~12|31|1943|John Denver~12|31|1942|Andy Summers~12|31|1941|Sarah Miles~12|31|1937|Anthony Hopkins~12|31|1930|Odetta~12|31|1908|Simon Wiesenthal~12|31|1880|George C. Marshall~12|31|1869|Henri Matisse";

  var BdayArray = birthdayStr.split("~");

  var subArray = new Array(3);
 
  for (i in BdayArray)
  {
   subArray = BdayArray[i].split("|");
   name = subArray[3];
   bMonth = subArray[0];
   bDay = subArray[1];
   bYear = subArray[2];

   if ((CurrMonth+1)==(bMonth) && CurrDay==bDay) 
    {
      //alert("bMonth = " + bMonth + ";bDay = " + bDay);
      if (banner == "") {banner = "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + name + "(" + bYear + ")";}
      else
      {banner = banner + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + name + "(" + bYear + ")";}
    }
  }

  

   banner_celeb = banner.replace(/`/gi,'"') + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;";
   //alert("banner = " + banner);
   return banner_celeb;
 
}

  function loaddataBHS(datafile)
  {

  if (document.getElementById) 
  {
   if (ie)
     {        //----Internet Explorer, Avant Browser
      objXml = new ActiveXObject("Microsoft.XMLHTTP");
      objXml.open("GET", datafile, true);
      objXml.onreadystatechange=function()
        {
         if (objXml.readyState==4) 
         {birthdayStr = objXml.responseText;
          CreateBannerBHS();
         }
     }
       objXml.send(null);}
   else {         //----Netscale, Mozilla, Firefox
       objXml = new XMLHttpRequest();
       objXml.open("GET",datafile,false);
       objXml.send(null);
       birthdayStr = objXml.responseText; 
       CreateBannerBHS();
        }
  }
//var name=prompt("Please enter your name","Harry Potter");

//window.document("banner_BHS = " + banner_BHS);

    //     alert("birthdayStr = " + birthdayStr);
   return banner_BHS;
        
 }


  //var birthdayStr="";
  var d = new Date();
  var CurrYear = d.getFullYear();
  var CurrMonth = d.getMonth();   // returns the month (from 0 to 11)
  var CurrDay = d.getDate();
  var JulianArray  =new Array("0","31","59","90","120","151","181","212","243","273","304","334");
  var JulianArrayLY=new Array("0","31","60","91","121","152","182","213","244","274","305","335"); // leap year
  var leap_year_adj = 0;
  var curr_julian_date = parseInt(JulianArray[CurrMonth]) + parseInt(CurrDay);

  if (CurrYear%4 == 0)
      {curr_julian_date = parseInt(JulianArrayLY[CurrMonth]) + parseInt(CurrDay);   // leap year
      leap_year_adj = 1;
      }                                                                        



  function CreateBannerBHS()
 { 
  var adjust = 0;
  var i=0;
  var bMonth=0;
  var bDay;
  var name;
  var banner="";
  var diff=0;

 var birthdayStr = "1|1|Evelyn Cogle~1|1|Joan Libby Johnson~1|1|Joan Zollinhofer~1|4|Eleanor Hitt Schlosser~1|8|Dorene Levy Patterson~1|8|Evelyn Davis Porter~1|10|Joyce Curtin Bowie~1|10|Paul Creelman~1|13|Dottie Coleman Chester~1|24|Christine Hayes Miller~1|25|Ronald Hall~1|26|Madaleine D`andrea Attick~1|28|Sharon Gantt Farley~1|28|Zelda Caruana Potts~1|29|James Maness~2|2|Carol Skuchko White~2|6|Ron Franks~2|7|Ron Compton~2|7|Joan CULVER Seabright~2|7|Sandy Slocombe Peter~2|9|Michael Beck~2|17|Judy Tayman Webber~2|19|Diann Hanback Guido~2|21|Copeland, C.h.pete~3|2|Jimmy Smith~3|3|Bobbie Clark Chaconas~3|4|Barbara Faasen Palmer~3|4|Carolyn White~3|7|Lee Keenan Whiteleather~3|9|Joella Delp Warner~3|9|Lee Ledford Carpenter~3|12|Punky Nash Anderson~3|24|Kitchin, Patricia Ford~3|29|Gail Swenson Foley~3|30|Joan Huff Shotland~3|31|Rick Maloof~4|2|Dale Hussong Grider~4|2|Jack Lassise~4|4|William Roberts~4|5|Dale Woolwine~4|6|Larry Hughes~4|10|Muriel Scott Miller~4|12|Dave Keefer~4|12|Diane Keefer Stith~4|13|Hank Reeves~4|13|Johnny Flippin~4|19|Nancy Stumph~4|19|Ron Mummert~4|22|Cecil Doppelheuer~4|24|Pat Kitchen Ford~4|25|Dale Devey Dunn~4|30|Bill Isherwood~4|30|Joe Maddox~5|3|Shirley Dough Sorenson~5|5|Peggy Yoho Hysan~5|6|Gloria Shinn Burch~5|6|Joyce Meredith~5|8|Patricia Gastrock Deener~5|13|Kenny Wright~5|26|Betty Allen Davis~5|26|Judy Ocker Diebold~5|26|Sue Allen Holmes~5|31|Artie Smith~6|2|Donnie Vazquez~6|3|Judy Wood Lindsay~6|4|Larry Trabucco~6|4|Peggy Niemann Zipf~6|4|Robert Burke~6|7|Loretta Marsico Kelly~6|12|Ron King~6|15|Jean Ward Waid~6|16|Chris Haney~6|16|William Schlichtig~6|18|Barbara Burdine Hardisty~6|24|Will~6|25|Katherine~6|28|Marty Murphy Ralston~7|3|Kathy Kline Barrett~7|4|Gloria Jenkins Rodill~7|8|Joan Jaynes Hogan~7|12|Joan Howell Knapp~7|14|Jackie Oldham Criscuolo~7|15|Danny Falk~7|21|Carol Sladki Paris~7|23|Elaine Montague Mailliard~7|25|Eleanor Bosch Compton~7|28|Nancy Tayman Conley~7|29|James Spicer~7|30|Dick Hayden~8|3|Paul Chester~8|5|Dottie Crumit Lanahan~8|6|Ruby Wissman Brenton~8|7|Val Smart Hopkins~8|9|Mel Thiel~8|10|Mitzi Selby Parker~8|11|Carolyn BIVENS Cote~8|11|Sue Cristofane Yatman~8|14|Vince Daly~8|15|James Christensen~8|16|Elna Stuart Adams~8|22|Jeffrey Lebeau~8|24|Richard Tracy~8|24|Ronnie Righter~8|26|Sharon Offutt Baxter~8|30|Bobbie Tayman~9|1|Janice Slaughter Lay~9|2|Nancy Coburn Lusby~9|6|Barbara Johansen Mark~9|6|Danny Dennis~9|6|Joan Walter Brittan~9|6|Judy Ingram Ingels~9|9|Donnie Webber~9|9|George Duvall~9|16|Maryan Daniel Formy-Duval~9|22|Bobby Might Wagner~9|25|Connie Eastman~9|25|Joseph Parrish~9|26|Thomas Beatty, teacher~9|28|Billy Butcher~9|29|Carolyn Gaines Dilda~9|30|Joanie Johnson Libby~10|2|Edwin Woolson~10|3|Tom Quarnstrom~10|13|Patricia Thomas Dove~10|19|Joan Thompson Fultz~10|21|Thomas Staples~10|21|William Staples~10|22|Jeri Jessel Heiligman~10|23|Judy Wallace Randall~10|25|Joan Miller Saslaw~10|29|Jerry Freedman~11|2|Alan Sorkin~11|6|Tony Digirolamo~11|7|Marla Gerster Allen~11|12|Marty Stanford Mann~11|13|Rowena Kern Russell~11|13|Sharleen Walker Bennett~11|13|Sharron Schuh Woolwine~11|14|Gary Woodruff~11|15|Jack Shriver~11|16|Dolores Avery Mason~11|17|Bob Edward Laramy~11|19|Anne Amberger Hall~11|19|Sammy Paladino~11|23|Doug Moreland~11|25|Dave Marcos~12|5|Jack Divers~12|6|Cara Pennington Morris~12|7|Gail Pace Crocker~12|8|Carol Taylor Prince~12|9|Bonnie Barrett~12|9|Carol Smith Sullivan~12|18|Bill Martin~12|20|Whitey Whiteleather~12|23|Jane Wilson McDuffie~12|30|Dallas Carter~12|31|Bill Jarboe~12|31|Harry Lieberman";
  var BdayArray = birthdayStr.split("~");
  var subArray = new Array(3);

  var end_banner="";
  var padding = "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;";
  var prev_bMonth = 0;
  //alert("BdayArray = " + BdayArray);

  for (i in BdayArray)
  {
   subArray = BdayArray[i].split("|");
   name = subArray[2];
   bMonth = subArray[0];
   bDay = subArray[1];
   bday_julian_date = parseInt(JulianArray[bMonth-1]) + parseInt(bDay);
   if (CurrYear%4 == 0) {bday_julian_date = parseInt(JulianArrayLY[bMonth-1]) + parseInt(bDay);}


   if (CurrMonth==11 && bMonth ==1) {
     bday_julian_date = bday_julian_date + 365 + leap_year_adj; 
   }


   if (CurrMonth==0 && bMonth==12) {
      adjust = 365; 
      if ((CurrYear-1)%4 == 0) {adjust = adjust + 1; }
   }

  //alert("bday_julian_date = " + bday_julian_date);
  diff = Math.abs(curr_julian_date + adjust - bday_julian_date); 
  //alert("diff = " + diff);


   if (diff < 12)
    {
      if (banner == "") {prev_bMonth = bMonth;}
      if (bMonth - prev_bMonth < 2)
       {banner = banner + padding + name + "(" + bMonth + "/" + bDay + ")";}
       else
       {end_banner=banner; banner =  padding + name + "(" + bMonth + "/" + bDay + ")";}  
       prev_bMonth = bMonth;                     
    }

 
   // alert("bday_julian_date = " + bday_julian_date + "; bMonth = " + bMonth + "; bDay = " + bDay + "; diff = " + diff);

  }
   banner_BHS = padding + banner + end_banner + padding + padding + "&nbsp;&nbsp;";
   //alert("banner_BHSyy = " + banner_BHS);

   return banner_BHS;
}


  //var d = new Date();
  //var CurrMonth = d.getMonth() + 1;   // returns the month (from 0 to 11)
  //var CurrDay = d.getDate();
  var historyfile="";

  function loadHistoryArchive(datafile,where)
  {

  if (document.getElementById) 
  {
   if (ie)
     {        //----Internet Explorer, Avant Browser
      objXml = new ActiveXObject("Microsoft.XMLHTTP");
      objXml.open("GET", datafile, true);
      objXml.onreadystatechange=function()
        {
         if (objXml.readyState==4) 
         {historyfile = objXml.responseText;
          if (where=="txt")
          {GetTodaysHistory();}
          else
          {GetTodaysHistoryVideo();}
         }
     }
       objXml.send(null);}
   else {         //----Netscale, Mozilla, Firefox
       objXml = new XMLHttpRequest();
       objXml.open("GET",datafile,false);
       objXml.send(null);
       historyfile = objXml.responseText; 
          if (where=="txt")
          {GetTodaysHistory();}
          else
          {GetTodaysHistoryVideo();}
        }
  }
        
 }

  function GetTodaysHistory()
 { 

  var TodaysHistory_str="";
  var Header_str="";
  var i=0;
  var histMonth=0;
  var histDay=0;
  var sourceCode;
  var HistoryArray = historyfile.split("\n");
  var subArray = new Array(2);
  var prefix = '<table align="center" width="100%" style="background-color:#eeeeee;border-color:#000000;border-width:4px;" border=3 bgcolor="#eeeeee">' +
  '<tr><TD width="25%"></td>' +
  '<td class="head" width="50%">This Day In History<span style="font-size:14px;"><br>';

  var suffix = '</span></td>' +
  '<TD class="head2" width="23%" valign="middle">' +
  '<input class="custombutton" type="button" value="Exit" onclick="toggleMe(' +
  "'DisplayThisDaynHistory')" +
  '"></td><TD width="2%"></td>' +
  '</tr></table>';

  for (i in HistoryArray)
  {
   subArray = HistoryArray[i].split("|");
   sourceCode = subArray[2];
   histMonth = subArray[0];
   histDay = subArray[1];
//document.write("histMonth = " + histMonth + "<br>");

   if ((CurrMonth+1)==(histMonth) && CurrDay==histDay) 
    {
      if (sourceCode.indexOf("<tr>")!= -1)
       {TodaysHistory_str = TodaysHistory_str + sourceCode;}
        else
       {
          Header_str = prefix + sourceCode + suffix;
       // alert("Header_str = " + Header_str);
       }
 
    }
  }

     TodaysHistory_str = '<table  align="center">' + TodaysHistory_str + '</table>';
//alert("TodaysHistory_str = " + TodaysHistory_str);



   writit(TodaysHistory_str,'DisplayThisDaynHistoryInner2');
   writit(Header_str,'DisplayThisDaynHistoryInner1');
   toggleMe('DisplayThisDaynHistory');
 
}
 
 
 
