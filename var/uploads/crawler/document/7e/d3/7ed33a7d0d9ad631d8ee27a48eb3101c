imagesPreloaded = new Array(4)
for (var i = 0; i < MyImages.length ; i++) 
{
    imagesPreloaded[i] = new Image(120,120)
imagesPreloaded[i].src= MyImages[i]
}

function txt_info(datafile,jpeg) {
 var tribute="";

 if (document.getElementById) {
 if (ie) {        //----Internet Explorer, Avant Browser
     objXml = new ActiveXObject("Microsoft.XMLHTTP");
     objXml.open("GET", datafile, true);
     objXml.onreadystatechange=function() {
                if (objXml.readyState==4) {
                    tribute = objXml.responseText; 
     writit(tribute,'DisplayTribute'); toggleMe("DisplayTribute");
                                          }
                                          }
      objXml.send(null);}
 else {         //----Netscale, Mozilla, Firefox
     objXml = new XMLHttpRequest();
     objXml.open("GET",datafile,false);
     objXml.send(null);
     tribute = objXml.responseText; 
     writit(tribute,'DisplayTribute'); toggleMe("DisplayTribute");
 }

 }
 if (!document.getElementById) {
 alert("Sorry ! your browser does'nt support this feature...");}
 else { 

 
      }

 }



function txt_info2(datafile,show,id) {
//alert("inside txt_info");
 var tribute="";
 // alert("datafile = " + datafile);
 
 if (document.getElementById) {
 if (ie) {        //----Internet Explorer, Avant Browser
     objXml = new ActiveXObject("Microsoft.XMLHTTP");
     objXml.open("GET", datafile, true);
     objXml.onreadystatechange=function() {
                if (objXml.readyState==4) {
                    tribute = objXml.responseText; 
     writit(tribute,"DisplayTribute2"); toggleMe2("DisplayTribute2",show);                                          }
                                          }
      objXml.send(null);}
 else {         //----Netscale, Mozilla, Firefox
     objXml = new XMLHttpRequest();
     objXml.open("GET",datafile,false);
     objXml.send(null);
     tribute = objXml.responseText; 
     writit(tribute,"DisplayTribute2"); toggleMe2("DisplayTribute2",show);
 }

 }
 if (!document.getElementById) {
 alert("Sorry ! your browser does'nt support this feature...");}
 else { 

 
      }

 }


function automaticly() {
 
if (currentIndx<imagesPreloaded.length){
currentIndx=currentIndx
}
else {
 currentIndx=0
}
document.theImage.src=imagesPreloaded[currentIndx].src
document.form1.text1.value= Messages[currentIndx];
currentIndx=currentIndx+1;
var delay = setTimeout("automaticly()",3500)
   
}


