     toggleMe('DisplayPromptMsg');
     //getXY(event);
  }
  else
  {displayform(url);}


}
 
function ValidatePrayerForm(formname)
 {
 var cmts = document.forms[formname]["comments"].value;
     cmts= cmts.toLowerCase();

     if (document.PrayerForm.personsname.value == "") 
 {alert("Please enter name of person for whom we are praying (where the cursor is blinking).");
 document.PrayerForm.personsname.focus();
 return false;}

else if(document.PrayerForm.sendersemail.value == "") 
{alert("Please enter your email address where the cursor is blinking.");
document.PrayerForm.sendersemail.focus();
return false;}

else if (document.PrayerForm.comments.value == "")
     
{alert("Please enter your comments, suggestions or special memories where the cursor is blinking.");
document.PrayerForm.comments.focus();
return false;}

else if (cmts.indexOf("url=http://")!= -1 ) 
 {toggleExit('PrayerRequestform');return false;}

else if ((document.PrayerForm.permission[0].checked == false) &
         (document.PrayerForm.permission[1].checked == false))
     
{alert("Please check either the 'YES' or 'NO' permission button.");
return false;}


else
{
 return true;}

}
 
function ValidateForm1(formname,DivName) 
 {
 var fname = document.forms[formname]["firstname"].value;
 var lname = document.forms[formname]["lastname"].value;
 var cmts = document.forms[formname]["comments"].value;

     cmts= cmts.toLowerCase();
//alert("cmts = " + cmts);

     if (document.forms[formname]["firstname"].value == "") 
 {alert("Please enter your first name where the cursor is blinking.");
 document.forms[formname]["firstname"].focus();
 return false;}
else if (document.forms[formname]["lastname"].value == "") 
 {alert("Please enter your last name where the cursor is blinking.");
 document.forms[formname]["lastname"].focus();
 return false;}

else if(document.forms[formname]["sendersemail"].value == "") 
{alert("Please enter your email address where the cursor is blinking.");
document.forms[formname]["sendersemail"].focus();
return false;}

else if (document.forms[formname]["comments"].value == "")
     
{alert("Please enter your comments, suggestions or special memories where the cursor is blinking.");
document.forms[formname]["comments"].focus();
return false;}

else if ((fname == lname) ||
         (cmts.indexOf("url=http://")!= -1 )) 
 {toggleExit(DivName);return false;}

else
{
 if (fname.substr(0,8) == lname.substr(0,8))
   {document.forms[formname]["comments"].value = cmts + "xxx";}

 return true;}

}

function toggleExit(a){

  var e=document.getElementById(a);
  if(!e)return true;
  if(e.style.display=="none"){
    e.style.display="block"
  } else {
    e.style.display="none"
  }

 
}

function CloseWindow(a){

  var e=document.getElementById(a);
  if(!e)return true;
  if(e.style.display=="block"){
    e.style.display="none"
  } 
 
  return true;
}

 function toggleMe2(a,show){
//alert("a = " + a);
//alert("show = " + show);
  if(show=="none") {
    CloseWindow("DisplayTribute");
    CloseWindow("DisplayTribute2"); }
 
  else {
  toggleMe('DisplayTribute');
  toggleMe('DisplayTribute2');
  }
 // return true;

 }

function toggleMe(a){

  var e=document.getElementById(a);
  if(!e)return true;
  if(e.style.display=="none"){
    e.style.display="block"
  } else {
    e.style.display="none"
  }

  if(a=="PrayerRequestform") {
    document.PrayerForm.personsname.focus();
   }
  
  return true;
}



 function DisplayClassOf1959()
 {
   var ms=0;
    if (ie)
      {    
       ms = 4300;
      }
    else 
      {         
       ms = 5000; 
      }
   var t=setTimeout("writClassOf1959();", ms); 
   toggleMe('Display_Class_of_1959');
 }

 
function writClassOf1959()
{
 var ClassOf1959Str = '<img src="Images/class-of-1959-test3.gif" alt="" border="0">';
 document.getElementById("Display_Class_of_1959").innerHTML=ClassOf1959Str;
}

  

function CloseWin(){

  var e=document.getElementById('DisplayBookMarkMsg');
  if(!e)return true;
  if(e.style.display=="none"){
    e.style.display="block"
  } else {
    e.style.display="none"
  }

   
  return true;
}

function blinkIt() {
 if (!document.all) return;
 else {
   for(i=0;i<document.all.tags('blink').length;i++){
      s=document.all.tags('blink')[i];
      s.style.visibility=(s.style.visibility=='visible')?'hidden':'visible';
   }
 }
}

window.moveTo(0,0);
window.resizeTo(screen.width,screen.height);


var newwindow1;
var childWin;
var url;
var wname;
var restrictedFile;


  now = new Date();
  eventDate = new Date("OCTOBER 5, 2014");
  msEachDay = 24 * 60 * 60 * 1000 ;
  
  function daysUntilEvent(){

  daysRemaining = (eventDate.getTime() - now.getTime()) / msEachDay;

  daysRemaining = Math.round(daysRemaining);

   var DisplayStr = '<a style="font-size: 13px;font-family: Arial Black;color: #660033;text-decoration: none;"' +
              ' href="javascript:myPopup(' + 
              "'About_the_55th_reunion_memory_book.htm', '55thClassOf1959Reunion','800','850','','0')" + '">' +
              '<b><span style="font-size: 16px;">55th BHS Class of 1959 Multi Year Reunion</span><br>In <font size="3" color="#663366"><blink>' + daysRemaining + "</blink></font> days.<br>Click For Details</b></a>"; 
//alert("DisplayStr = " + DisplayStr);
   writit(DisplayStr,'DisplaySenderList');

  }

