          jQuery('.piano-message').addClass('hidden');
        }
        else {
          jQuery('.piano-my-account').addClass('hidden');
          jQuery('.piano-logout').addClass('hidden');
          jQuery('.piano-login').removeClass('hidden');
          jQuery('.piano-register').removeClass('hidden');
          jQuery('.piano-message').removeClass('hidden');
          if (typeof trigger_multipurpose_popup == 'function') {
            trigger_multipurpose_popup();
          }
        }
        }]]
    };
    
    var pianoHostProtocol = (("https:" == document.location.protocol) ? "https://" : "http://"); 
    (function() {
        var s = document.createElement('script'); 
        s.type = 'text/javascript';
        s.async = true;
        s.src = pianoHostProtocol + 'bar.piano-media.com/newsweek/bar.js';
        var p = document.getElementsByTagName('script')[0]; p.parentNode.insertBefore(s, p);
      })();
</script>
    </div>

</div><!-- /.block ibtmedia_piano--piano_top_bar -->
