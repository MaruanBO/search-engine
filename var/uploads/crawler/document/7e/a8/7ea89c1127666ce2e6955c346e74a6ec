function gotoCO(tld)
{
    var sd = 'www';
    if ( tld == 'br' )
    {
        sd = 'gfkbrasil';
        tld= 'com';
    }
    if ( tld == 'ar' )
    {
        sd = 'ar';
        tld= 'com';
    }
    if ( tld == 'pt' )
    {
        tld= 'com.pt';
    }
    window.location='http://'+sd+'.askgfk.'+tld+'/index.php?id=7';
}
</script>
