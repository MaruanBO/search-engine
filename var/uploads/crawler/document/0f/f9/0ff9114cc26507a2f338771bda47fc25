<script type="text/javascript">
    var RegistrationModal = {
        PhoneNumberRegExPattern: /^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{4})$/,
        wireup: function () {
            $("a[href$=Register.aspx]").not(".selectMessage a").click(function (e) {
                e.preventDefault();
                RegistrationModal.show();
            });
            $("a[href$=register.aspx]").not(".selectMessage a").click(function (e) {
                e.preventDefault();
                RegistrationModal.show();
            });
        },
        show: function (section) {
            if (section && section == "apps") {
                $("#reg-modal-body-devices").hide();
                $("#reg-modal-body-apps").show();
                $("#reg-modal-body-phonenumber").hide();
                $("#registrationModal_btnBack").show();
            }
            else if (section && section == "number") {
                $("#reg-modal-body-devices").hide();
                $("#reg-modal-body-apps").hide();
                $("#reg-modal-body-phonenumber").show();
                $("#registrationModal_btnBack").hide();
            }
            else {
                $("#reg-modal-body-devices").show();
                $("#reg-modal-body-apps").hide();
                $("#reg-modal-body-phonenumber").hide();
                $("#registrationModal_btnBack").hide();
            }

            $(".selectParent").show();
        },
        hide: function () {
            $(".selectParent").hide();
        },
        showAppsSection: function () {
            $("#reg-modal-body-phonenumber").slideToggle("slow");
            $("#reg-modal-body-apps").slideToggle("slow");
            $("#registrationModal_btnBack").show();
        },
        showPhoneNumberSection: function () {
            $("#reg-modal-body-phonenumber").find(".loading").hide();
            $("#modal-phonenumber").val("");
            $("#reg-modal-body-devices").slideToggle("slow");
            $("#reg-modal-body-phonenumber").slideToggle("slow");
            $("#registrationModal_btnBack").show();
            $("#modal-phonenumber").focus();
        },
        showHomeSection: function () {
            $("#reg-modal-body-phonenumber").slideUp("slow");
            $("#reg-modal-body-apps").slideUp("slow");
            $("#registrationModal_btnBack").hide();
            $("#reg-modal-body-devices").slideDown("slow");
        },
        validatePhoneNumber: function (phoneNumber) {
            if (!this.PhoneNumberRegExPattern.test(phoneNumber)) {
                var validationError = $("#reg-modal-body-phonenumber").find(".validation-error");
                validationError.text("The phone number entered is invalid.");
                return false;
            }

            return true;
        },
        checkMDN: function () {
            var phoneNumberSection = $("#reg-modal-body-phonenumber");
            var loading = phoneNumberSection.find(".loading");
            var validationError = phoneNumberSection.find(".validation-error");
            validationError.text("");

            var mdn = $("#modal-phonenumber").val();

            var isPhoneNumberValid = RegistrationModal.validatePhoneNumber(mdn);

            if (isPhoneNumberValid) {
                loading.show();
                loading.spin();

                // Strip out all non-numeric characters.
                mdn = mdn.replace(RegistrationModal.PhoneNumberRegExPattern, "$1$2$3");

                $.get("/Registration/CheckMDN", { mdn: mdn }, function (redirectURL) {
                    if (redirectURL.length) {
                        location.href = redirectURL + "?mdn=" + mdn;
                    }
                    else {
                        // Pass the mdn to the app links.
                        var fivestar = $("#fivestar");
                        var fivestarUrl = fivestar.attr("href");
                        fivestarUrl += "?mdn=" + mdn;
                        fivestar.attr("href", fivestarUrl);

                        var medcoach = $("#medcoach");
                        var medcoachUrl = medcoach.attr("href");
                        medcoachUrl += "&mdn=" + mdn;
                        medcoach.attr("href", medcoachUrl);

                        RegistrationModal.showAppsSection();
                    }
                })
                .fail(function (jqXHR, status, message) {
                    validationError.text("An error has occurred while verifying your phone number. Please try again or contact customer service at 800-276-7266.");
                    loading.hide();
                });
            }
        }
    };

    $(function () {
        // Only load script once (some pages are already loading this script).
        $.loadScript("/Shared/Scripts/spin.min.js", true);

        $("#registrationModal_btnClose").click(function (e) {
            e.preventDefault();
            RegistrationModal.hide();
            $("#modal-phonenumber").val("");
            $(".spinner").css("display", "none");
        });

        $("#registrationModal_btnBack").click(function (e) {
            e.preventDefault();
            RegistrationModal.showHomeSection();
        });

        $(".greatcall-registration #apps").click(function (e) {
            e.preventDefault();
            RegistrationModal.showPhoneNumberSection();
        });

        $("#modal-phonenumber").keyup(function (e) {
            if (e.keyCode == 13) {
                RegistrationModal.checkMDN();
            }
        });

        $("#submit-phonenumber").click(function (e) {
            e.preventDefault();
            RegistrationModal.checkMDN();
        });
    });
</script>



