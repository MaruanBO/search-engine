<script type="text/javascript">

	// need to make the main nav buttons stay "active" until the new page loads
	$(document).ready(function () {
		$('.mainNavLI').click(function (event) {
			// clear them all out first
			$('[id$=mainNavUL] li a').removeClass('mainNavAActive');
			$('[id$=mainNavUL] li').removeClass('mainNavLiActive').removeClass('mainNavLeftLiActive');
			$('[id$=mainNavUL] li').removeClass('mainNavLiActive').removeClass('mainNavRightLiActive');

			var $target = $(event.target);
			//depending on which LI we hit, add the active class for the LI and the A
			if ($target.is('[id$=ProductsLink]')) {
				$target.toggleClass('mainNavAActive');
				$target.parent().toggleClass('mainNavLeftLiActive');
			}
            else if ($target.is('[id$=appsLink]')) {
 				$target.toggleClass('mainNavAActive');
				$target.parent().toggleClass('mainNavRightLiActive');           
            }
            else {
				$target.toggleClass('mainNavAActive');
				$target.parent().toggleClass('mainNavLiActive');
			}
		});

	});

</script>

