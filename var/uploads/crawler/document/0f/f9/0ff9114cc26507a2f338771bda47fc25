<script type="text/javascript">
    jQuery(document).ready(function() {
        overRideTextBoxStyling();
        jQuery(".search-field:first").bind("blur", function(){
            overRideTextBoxStyling();
        });
    });
    function overRideTextBoxStyling() {
        jQuery(".search-field:first").css("border", "0");
        jQuery(".search-field:first").css("background", "transparent");
    }


</script>
