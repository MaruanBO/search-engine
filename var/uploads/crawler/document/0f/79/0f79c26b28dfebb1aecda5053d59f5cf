	<script type="text/javascript">
		$(window).load(function() {
			// Date.now needs to be defined for < IE9 support
			Date.now = Date.now || function() { return new Date().getTime(); }
			var bmMinutes = -1;
			var bmPages = -1;
			var exceedsPageVisits = ((parseInt($.cookie('cPc')) >= bmPages) && bmPages != -1) ? true : false;
			var timeOnPage = (Date.now() - new Date(parseInt($.cookie("ClrSCD"))).getTime())
			var timeLeft = ((bmMinutes * 60000) - timeOnPage);
			function showEmailHopup() {
			// Only show the hop-up email overlay if cookies are enabled and it meets the time or page requirements
				var cookie = $.cookie('emailCapture');
				if(typeof cookie == 'string') cookie = (cookie == 'true');
				if (!cookie) {
					$('#showEmailCaptureOverlay').click();
					$.cookie('emailCapture', 'true', {path: '/'});
				}
				clearInterval(emailHopupInterval);
			}
			// if user has exceeded minimum page visits, show the email hopup
			if (exceedsPageVisits){ 
				showEmailHopup();
			// if the user hasn't yet exceeded the page count, set the hopup to show after however much time is left between now and time specified in BM
			} else if (bmMinutes != -1) {
				var emailHopupInterval = setInterval(showEmailHopup, timeLeft);
			// if nothing is specified, show the hopup the first time the page loads
			} else if (bmMinutes == -1 && bmPages == -1){
				showEmailHopup();
			}
		});
	</script>









	








	
	
	






	





	
		
