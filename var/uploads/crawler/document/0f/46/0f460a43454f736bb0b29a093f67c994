<script type="text/javascript">
	var navBar = j('#navBar2');
	
	j('li.DropDownParent', navBar).hover(setShowDropdownTimeout, dropdownInit);
	
	j('#DropDownContainerTeamChallenge').find('a.ReportAbuse').click(function() { openTeamReportAbuse(); return false; });
	
		j('#allGamesList').find('li').quicksearch({
		position: 'before',
		attached: '#allGamesList',
		labelText: '',
		loaderText: '',
		loaderImg: 'http://ak.imgfarm.com/images/iwon/global/ajax-loader.gif',
		delay: 300
	});
	
	j(function() {
		var defaultText = "Quick Game Search",
		allGamesList = j('#allGamesList'),
		DropDownContaineriWonGames = j('#DropDownContaineriWonGames');
		
		j('input.qs_input', '#quicksearch').addClass("qs_default").val(defaultText).click(function() {
			if(j(this).hasClass("qs_default")) {
				j(this).val("").removeClass("qs_default");
				allGamesList.removeClass('Hidden');
				DropDownContaineriWonGames.css({'display':'none'});
			}
		}).blur(function() {
			if(j(this).val() == "") {
				j(this).val(defaultText).addClass("qs_default");
				//allGamesList.addClass('Hidden');
				//DropDownContaineriWonGames.css({'display':'block'});
			} else {
				allGamesList.removeClass('Hidden');
				DropDownContaineriWonGames.css({'display':'none'});//.removeClass('Hidden');
			}
		}).keyup(function() {
			if(j(this).val() != "") {
				DropDownContaineriWonGames.css({'display':'none'});
			}
		});
		
		var NavBarItemiWonGames = j('#NavBarItemiWonGames');
		if(hasToolbar) {
			var coinIcon = j('div.coinIcon:first', NavBarItemiWonGames).clone();
			j('div.lockedIcon', NavBarItemiWonGames).replaceWith(coinIcon);
		} else {
			j('div.lockedIcon', NavBarItemiWonGames).css('visibility', 'visible');
		}
	});
	
	
</script>			
