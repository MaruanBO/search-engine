function awardedSpins(arg,f) {
	// add spins after user enters email addresses
	instantWinGamesData[currentInstantWinGame].ID.setInstantWinSpins(arg,f);
	instantWinGamesData[currentInstantWinGame].Score = arg;
	reloadSpinsLoadVar();
	Control.Modal.closeCurrentStackItem();
}

function reloadAwardedSpins(arg,f) {
	instantWinGamesData[currentInstantWinGame].ID.setInstantWinSpins(arg,f);
	instantWinGamesData[currentInstantWinGame].Score = arg;
	reloadSpinsLoadVar();
}

