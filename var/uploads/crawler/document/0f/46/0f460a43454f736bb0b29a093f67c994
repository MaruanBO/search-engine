<script type="text/javascript">
var cpmTimer;
var cpmAniDuration=600;
/* Initiates the cpm settings and javascript */
function cpm_init(){
	j("#cpm-container").css("position","relative");	
 
		var cpmArray = [		
					
			[ "CPM_715x240_teamchallenge_november_2012.swf", "cpm_game_tiles_teamchallenge_november.png"],
						[ "CPM_715x240_gardenscapes.swf", "cpm_game_tiles_gardenscapes.png", "9321"],
			[ "CPM_715x240_mydreamwedding_04b.swf", "cpm_game_tiles_mydreamwedding.png"],
			[ "CPM_715x240_pallisades_guardian_2a.swf", "cpm_game_tiles_pg2.png", "9291"]
		];	  
			
		if (hasToolbar)
			cpmArray.push([ "CPM_715x240_collpase2.swf", "cpm_game_tiles_collapse.png", "51"]);
		else
			cpmArray.push([ "CPM_715x240_toolbar.swf", "cpm_game_tiles_toolbar.png"]);
		
				
		
		var i = 1;
		    	
					var mashup = cpmArray.concat(instantWinGames);
	i=0;
		
	while(mashup.length != 0){
		
		var index = randRange(0,mashup.length-1);
		var item = mashup.splice(index,1)[0];

		if(item==null)
			continue;	//ignores code beneath and goes to next iteration of loop	
			
		if(item.push){ //is Array and is a CPM
			i++;
			j("#cpm-wrapper").append("<div id='CPM-" + i + "' class='large-cpm'></div>");
			var so = new SWFObject("http://ak.imgfarm.com/images/iwon/games/cpms/" + item[0], "swf" + i, "715", "240", "8", "#ffffff");
    		so.addParam('wmode', 'opaque');
    		so.addParam('AllowScriptAccess', 'Always');
    		so.addVariable("gameId", item[2]);
    		    		so.write("CPM-" + i);
    		
    		j("#thumbnails ul").append('<li><div class="cpm-thumb" rel="#CPM-' + i + '" style="background-image: url(http://ak.imgfarm.com/images/iwon/home/cpms/' + item[1] + ')"></div></li>');
		}else{
			if(item != null){
    			j("#cpm-wrapper").append("<div id='"+  item.getVariable("gameName") +"' class='large-cpm'></div>");
    			j("#thumbnails ul").append('<li><div class="cpm-thumb" rel="#' + item.getVariable("gameName") + '" style="background-image: url(http://ak.imgfarm.com/images/iwon/home/cpms/' + item.getVariable("thumbnail") + ')"></div></li>');
			}
		}
	}
	
	j("#thumbnails").find("ul").bind("click",function(e){
		cpm_pause();
		cpm_slide(e.target);
	})
	.find("li:first").remove()
	.appendTo("#thumbnails ul").css({left: 0}).hide()
	.prev().css("display","block");
	
	j("#thumbnails").jScrollPane({showArrows:true});
		
	cpm_show();		cpm_start();
}
function cpm_rotate(){
	cpm_slide("#thumbnails ul li:first div:first");
}

function cpm_start(){
	cpmTimer = setInterval(cpm_rotate,10000);
}

function cpm_pause(){
	clearInterval(cpmTimer)
}
function cpm_slide(slideElement){
	var bigCPMId= j(slideElement).attr("rel");
		if(bigCPMId != null){
			j(bigCPMId).insertAfter("#cpm-wrapper div:first");
			cpm_show();	//shows instant win games
			
			//sliding main graphic out
			var currentCPM = j("#cpm-wrapper div:first");
			currentCPM.animate({marginLeft: -(currentCPM.width()+20)},  cpmAniDuration, function(){
				j(this).remove().appendTo("#cpm-wrapper").css({marginLeft: 0});	//add it ot the end
				cpm_show();
			});
					
			//slide thumbnail out
			j(slideElement).parent().css({position: "relative"})
			.animate({left: -j(slideElement).width()},  cpmAniDuration+200,  function(){
				var ref= j(this);
				j(this).slideUp(200,function(){
					ref.remove().appendTo("#thumbnails ul").css({left: 0}).prev().css("display","block");	//add it ot the end
					
				});
			});
		}
}
 
function cpm_show() {
		var gameName = j("#cpm-wrapper div:first").attr("id");
				for (i = 1 ; i < instantWinGames.length ; i++) {
			if (gameName == instantWinGamesData[i].ID.gameName){
				currentInstantWinGame = i;
				//instantWinGamesData[currentInstantWinGame].ID.setInstantWinSpins(instantWinGamesData[currentInstantWinGame].Score);
				reloadSpinsLoadVar();
								
								if(cpm_getSelected().html()=='' || ((instantWinGamesData[i].ID.gameName=="PrizeMachine" || instantWinGamesData[i].ID.gameName=="Spin2Win") && document.all))
				instantWinGames[i].write(instantWinGamesData[i].ID.gameName);
			}
		}
}

/* returns a jQuery Object */
function cpm_getSelected(){
	return j("#cpm-wrapper div:first");
}

stopCPMRotation = cpm_pause;
startCPMRotation = cpm_start;

pauseCurrentCPM = function() {
	try {				thisMovie("swf" + cpm_getSelected().attr("id").search(/\d/g)).pauseCPM();
	} catch (e) {}
}
playCurrentCPM = function() {
   	try {     		thisMovie("swf" + cpm_getSelected().attr("id").search(/\d/g)).playCPM();
   	} catch (e) {} 
} 


	
							var instantWinGames = new Array();

var instantWinGamesID = new Array();
var instantWinGamesData = new Array();

var currentInstantWinGame = 0;
var oldInstantWinGame = 0;
var startingIWGFound = false;
var urlIWG = "";
startingIWG = 0; 

      


        	    		cookieIW = readSharedCookie("PC", "IW")
    		if (cookieIW != ""){
    			startingIWG = cookieIW;
    		}
    	    



for (i = 1 ; i < instantWinGames.length ; i++) {
	if (instantWinGamesData[i].ID.gameId == startingIWG){
		j('#instantWin ul').tabs('select', '#' + instantWinGamesData[i].ID.gameName);
		startingIWGFound = true;
	}
}

if ((!startingIWGFound)&&(instantWinGames.length > 0)){
	currentInstantWinGame = 1;
}


cpm_init();
</script>			
