<div id="progressBar">

<script type="text/javascript">
function ProgressBar(){
	this.init();
	this.draw();
}

ProgressBar.prototype = {	
	init: function(){
		this.status = [];
		this.status["registered"] =  false ;
		this.status["coins"] =  false ;
		this.status["FBconnect"] =  false; ;
		this.numStatuses = 3;		
				if(toolbarSupportedBrowser){
			this.status["toolbar"] = (hasToolbar || false);
			this.toolbarIncluded = true;
			this.numStatuses++;
		}
				if (this.numStatuses > 3) {
			j(".status_bar").show();
		}
		this.width = Math.floor((630 - (33 * this.numStatuses)) / this.numStatuses);
		if(hasToolbar && !false && (document.cookie.indexOf("toolbarInstalled") == -1 || Cookie.get("toolbarInstalled")=="ignore") && isSignedIn() && this.toolbarIncluded == true){
			setCookie("toolbarInstalled",hasToolbar,2, '/', '.iwon.com');
			if(isSignedIn()) window.location.reload();
		}else{
			if(this.toolbarIncluded==null)
				setCookie("toolbarInstalled","ignore",2, '/', '.iwon.com');
			else
				delete_cookie("toolbarInstalled");
		}				
	},
	draw: function(){
		var HTML = "<table width='100%'><tr>";
		
		var numComplete =0;
		for(var i in this.status){
			if(this.status[i]){
				HTML+="<td class='progress_module " + i + "_complete'></td>";
				numComplete++;
			}
		}		
		
		for(var i in this.status){
			if(!this.status[i])
				HTML+="<td class='progress_module " + i + "_pending'></td>";				
		}
		
		HTML+= "</tr></table>";
		j("#progressBar").append(HTML);
		
		if(this.numStatuses==4){
			j("div.status_bar").css({background: 'url(http://ak.imgfarm.com/images/iwon/home/progressbar/progressbar_short_sprite.png)  no-repeat'});
			
		}
		j("#progressBar").find("td.progress_module").css({width: this.width});
			
		j("#progressBar").find("td").not("td:last").each(function(){
			j("#progressBar").append("<div class='progress_module_divider' style='left: " + (j(this).next()[0].offsetLeft-12) + "px;'></div>");
		});

		//adding click events to items if the user has not completed them
		j("#progressBar").find("td.registered_pending").addClass("TrackClicks")
		.attr("clickpath","progressbar/register").click(function(){ window.location = "register.jhtml" });
		
		j("#progressBar").find("td.coins_pending").addClass("TrackClicks")
		.attr("clickpath","progressbar/coins").click(function(){
			tb_show('', '/modules/snippets/misc/whatAreCoinsModal.jhtml?height=570&width=455&modal=true');
		});       
       
		j("#progressBar").find("td.toolbar_pending").addClass("TrackClicks")
		.attr("clickpath","progressbar/toolbar")
		.click(function(){
			 //window.location = "/dl/index.jhtml?theme=IWONIS&partner=ZLxdm322";
			 toolbarInstall("ZLxdm322");
		});
		
		j("#progressBar").find("td.FBconnect_pending").addClass("TrackClicks")
		.attr("clickpath","progressbar/fb_connect").click(function(){
			IWON.Event.trigger(IWON.Event.FB_LOGIN);
		});
		
		j("#progressBar").find("td.teamChallenge_pending").addClass("TrackClicks")
		.attr("clickpath","progressbar/team_challenge").click(function(){
			window.location = "/team-challenge.jhtml";
		});
		
		//highlight progress bar
				if(numComplete > 0){
			var thisRef = this;
			j(function(){
				j("#bar_complete").animate({width : Math.floor (((thisRef.numStatuses == 5) ? 122 : 151.4) * numComplete) },2000);
			});
		}
			}
}

var Progress = new ProgressBar();

</script>
</div>

