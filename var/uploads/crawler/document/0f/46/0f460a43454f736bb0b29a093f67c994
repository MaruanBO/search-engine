		}
	}	
	showSigninWindow();
}

/* Prize Machine Diagnostics */
function debugIW(logLevel, gameId, action, debugInfo) {
	var img = new Image();
	var pixel = "";
	if (logLevel == "ERROR") {
		pixel = "e";
	} else {
		pixel = "a"
	}
	 var url = '/images/iw/' + pixel + '.gif?gameId=' + gameId + '&u=&action=' + action;
	 if (debugInfo != null & debugInfo != "") {
	 	url += '&' + debugInfo;
	 }
	 url += '&rand=' + Math.random();
	 img.src = url;
}




