function forceInstantWinStop(){
	jQuery('#InstantWinGames').children('.LeftColumnComponent:visible').children('*')[0].setInstantWinResult("ERROR",instantWinGamesData[currentInstantWinGame].Score,-21)
}

// Get the result of the instantwin via AJAX
InstantWinGame.prototype.getInstantWinResult = function() {
	
	
		toggleAnimationInterrupt(true);
	
	//quick check for old bot
	jackass247 = "&hack=false";	
	sUrl = "/winnerchk.jhtml?gameID=" + this.gameId + "&rand=" + Math.random() + jackass247;
	randomPrizeId = (Math.floor(Math.random() * 4) + 20) * -1;
	c = "game" + currentInstantWinGame;	
	IWGErrorTimer = setTimeout('forceInstantWinStop()',10000);
	//instantWinGamesData[currentInstantWinGame].Score --;
	//reloadSpinsLoadVar();
	var self = this;
	jQuery.ajax({
		url: sUrl,
		method: 'GET',
		dataType: 'xml',
		success:  function(xml) {
			clearTimeout(IWGErrorTimer);
			var responseXML = xml;   
			var status = responseXML.getElementsByTagName("status")[0].firstChild.data;
			var outcome = "LOSE";
			prizeId = 0;
			if (status == "OK") {
				var resultXML = responseXML.getElementsByTagName("result")[0];
				var bWinner = (resultXML.getElementsByTagName("status")[0].firstChild.data == "Win") ? true : false;
				var spins = resultXML.getElementsByTagName("plays-remaining")[0].firstChild.data;
				instantWinGamesData[currentInstantWinGame].Score = instantWinGamesData[currentInstantWinGame].Score - 1;
				reloadSpinsLoadVar();
				var prizeId = resultXML.getElementsByTagName("prize-id")[0].firstChild.data;
				/*
				if (bWinner) {				
					var claimCode = resultXML.getElementsByTagName("claim-code")[0].firstChild.data;
					outcome = "WIN";
					self.claimCode = claimCode;
					self.prizeId = prizeId;
					self.setInstantWinResult(outcome, spins, prizeId, claimCode);
				} else {
				*/
					var claimCode = "";
					if (prizeId != 0) {
						if (resultXML.getElementsByTagName("prize-type")[0]) {
							var prizeType = resultXML.getElementsByTagName("prize-type")[0].firstChild.data;
						}
						else {
							var prizeType = 'nothing';
						}					
						if (prizeType == 'coins'){
							//Player gets coins
							lastWinType = "coins";
							var prizeCoins = resultXML.getElementsByTagName("coins")[0].firstChild.data;
							NumberOf['TotalMedallions'] = prizeCoins;
							outcome = "WIN";
							claimCode = "consolation";
							
						}
						else if (prizeType == 'spins'){
							//Player gets spins
							lastWinType = "spins";
							//awardedSpins(instantWinGamesData[currentInstantWinGame].Score);
							oldSpins = instantWinGamesData[currentInstantWinGame].Score;
							outcome = "WIN";
							claimCode = "consolation";
							////alert('spins: ' + spins + ' oldspins: ' + oldSpins );	
						}
						else {
							//Player really loses
							lastWinType = "nothing";
						}					
					}
					else {
						////alert('loss');
						prizeId = randomPrizeId;	
						spins = instantWinGamesData[currentInstantWinGame].Score;
					}		
					//window.setTimeout(function() {  self.setInstantWinResult(outcome); }, 2 * 1000);
					self.setInstantWinResult(outcome, instantWinGamesData[currentInstantWinGame].Score, prizeId, claimCode);
					//awardedSpins(instantWinGamesData[currentInstantWinGame].Score);
					instantWinGamesData[currentInstantWinGame].Score = spins;
					reloadSpinsLoadVar();				
				//}			
			} else {
				outcome = "ERROR";
				error = responseXML.getElementsByTagName("error")[0].firstChild.data;
				if (error == "STATUS_AT_MAX") {
					self.setInstantWinResult(outcome, 0, randomPrizeId);
				} else {
					self.setInstantWinResult(outcome, 1, randomPrizeId);
				}
			}
		},
		error: function() {
			clearTimeout(IWGErrorTimer); 
			outcome = "ERROR"; 
			self.setInstantWinResult(outcome, instantWinGamesData[currentInstantWinGame].Score, randomPrizeId); 
		},
		complete:function(jqXHR, textStatus){
			//nothing	
		}
	});

	//clearInterval(changeIWGTimer);
}

// Send the instantwin result to flash
InstantWinGame.prototype.setInstantWinResult = function(outcome, spins, prizeId, claimCode) {
	////alert('setInstantWinResult :' + outcome + ',' + spins + ',' + prizeId + ',' + claimCode);
	thisMovie(this.id).setInstantWinResult(outcome, spins, prizeId, claimCode);	
	this.logEvent(outcome, spins, prizeId, claimCode);
	
};

/*  REPORTING */
/*  Log InstantWin Events to Unified */
InstantWinGame.prototype.logEvent = function(outcome, spins, prizeId, claimCode){
	_Anemone.logEvent("InstantWinGameEntry", {anxui: '', anxut: 'IWON', PrizeId: prizeId, Outcome: outcome, SpinsRemaining:spins, gameId:this.gameId, GameName:this.gameName, GameLocation:"HomePage"});
	//alert("logged to Unified....");
};

 
// Tell the flash movie to add spins
InstantWinGame.prototype.setInstantWinSpins = function(spinsToSend,forceDisplay) {
	// Only update spins when there is no fading, otherwise try again later
	////alert('setInstantWinSpins :' + spinsToSend + ',' + forceDisplay);
	if (!currentlyFading) {
		try {thisMovie(this.id).setInstantWinSpins(spinsToSend,forceDisplay);}
		catch (e){}
	}
	else {
		tryAgain = setTimeout("reloadAwardedSpins(instantWinGamesData[currentInstantWinGame].Score)",150);
	}
}
function instantWinUIBeforeOpen() {
	jQuery.getScript("../js/taf.js");
}
function instantWinUIAfterOpen() {
	var clickPath = "/clicks/TAF/open/user_initiated";
	if (typeof(urchinTracker) == "function") {
		urchinTracker(clickPath);
	}
	if(typeof _iWonUnified != 'undefined'){
			_iWonUnified.logClickEvent(clickPath);
	}
}
InstantWinGame.prototype.instantWinOutOfSpins = function() {
	if (jQuery('#signedIn').html() == "1") {
		var tafURL = '/modules/pages/taf/tafv2_form.jhtml?type=play&gameID=' + this.gameId ;
		//window.open(tafURL,"taf",'resizable=0,scrollbars=1,width=750,height=635');

		popupWindow(tafURL, "taf", 'resizable=0,scrollbars=1,width=750,height=635');
		
		instantWinUIAfterOpen();
	} else {
		signInWithMsg(2);
	}
}
InstantWinGame.prototype.showResult = function(result, claimCode) {
	/*if ((result == "WIN")&&(!(claimCode == "consolation"))) {		
		this.showInstantWinWin(result, claimCode);
	}
	else
	{
	*/
		if (lastWinType == "coins"){
			setBalance('TotalMedallions',NumberOf['TotalMedallions']);
		}
		else if (lastWinType == "spins"){
			////alert('showresult: spins');
			if (instantWinGamesData[currentInstantWinGame].ID.gameId == 2004)
			{
			////alert ('trifecta')
			reloadAwardedSpins(instantWinGamesData[currentInstantWinGame].Score)
			}
			else
				IWGSpinAnimation(oldSpins,instantWinGamesData[currentInstantWinGame].Score);
		}
		lastWinType = "nothing";
		//refresh ads after every 3rd instantwin play
		if(++instantWinClickCounter == 3){
			refreshNonModalAds();
			instantWinClickCounter = 0;
		}	
	//}
	//startCPMRotation();
		toggleAnimationInterrupt(false);
}
function IWGSpinAnimation(start,stop)
{
	if (start < stop){
		reloadAwardedSpins(start)
		start = start + 1;
		setTimeout('IWGSpinAnimation('+ start + ',' + stop + ')',150);
	}
	else
		reloadAwardedSpins(stop);
}


