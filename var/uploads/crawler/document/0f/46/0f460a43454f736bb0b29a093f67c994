function getPrizeDetails(thisClaimCode, prizeID) {
	j.post('/modules/prizeDetail/misc/getPrizeDetail.jhtml', {"prizeId":prizeID}, 
		function(prize) {
			signInAndClaim(thisClaimCode, prize);
		},
	"json");
}
