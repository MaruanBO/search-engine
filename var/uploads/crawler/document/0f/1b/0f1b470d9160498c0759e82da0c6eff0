          <script type="text/javascript">

  function catalog_pop() {
  
 
    // get all the inputs into an array.
    var $inputs = $('#homecatalog :input');

   
    var values = {};
    $inputs.each(function() {
        values[this.name] = $(this).val();
    });


    
    
  if(values['email'].length > 0){
      
      var aEmail = values['email'];
   
    
      var promoCode = "0534656";
      
      var emailValid = false;
      if( /(.+)@(.+){2,}\.(.+){2,}/.test(aEmail) ){
        emailValid = true;
      }
    
      if((aEmail.length <= 0) || (aEmail=="Enter Your Email Address Here") || (aEmail=="Enter Your Email for Instant Savings")) {
        $("#promomsgnewsltrerr").show();
        alert("Email address is required");
        return false;
      }
      else if (emailValid == false) {
        $("#promomsgnewsltrerr").show();
        alert("A valid email address is required");
        return false;
      }
      else {
        $("#promomsgnewsltrerr").hide();
        $.post("/promo_message_apply", { p: promoCode }, function(response){ });
        $.post("/catalog_request", { method:values['method'], title:values['title'], first_name: values['first_name'], last_name: values['last_name'], address1: values['address1'], address2: values['address2'], city: values['city'], state: values['state'], czip: values['czip'], email: values['email'], category: values['category']  } );

        UCX.LogContact("33D72149", "", aEmail, "", "email_offer::banner", promoCode);
        GaCommon.SetCookie('emailCollected','true',15);
        GaCommon.SetCookie('email',aEmail,15);
        $('#promomsgnewsltrcont').hide();
        $('#promomsgnewsltrthanks').show();   
        $('#catalog_thanks').show();
        $('#catalog_form').hide();
        
        return false;
      }
      return false;
      
    }
    
    
    return false;
  
  }
  //windowshade_setCookie('emailPopDisplayed','true',1);

</script>
