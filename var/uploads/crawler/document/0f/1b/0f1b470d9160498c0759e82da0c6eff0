   <section id="homegrid-veg-favorites" class="homerows" class="clearfix">
  <header><h2 class="homegridhead">Vegetable Flavor Favorites</h2></header>
  <div id="homegrid-veg-favorites-items"></div>
  <div style="text-align:right;"><a href="/category/vegetable-flavor-favorites" style="color:#00659c;text-decoration:underline;">View All Vegetable Flavor Favorites&nbsp;&#187;</a></div>
</section>

<script>
jQuery(function() {
  $.getJSON('/category/vegetable-flavor-favorites?jsonviewprods=true', function(data) {
    var items = [];
    var i = 0;
    $.each(data.data, function(key, val) {
      if(i < 5) {
      var frm = '<form method="post" action="index.php" id="veg-favorites-buynow-'+val.sku+'_form" style="margin:0;padding:0;">'
        + '<input type="hidden" name="action" value="AddCart" />'
        + '<input type="hidden" name="prod_name" value="'+val.prod_name+'" />'
        + '<input type="hidden" name="prod_id" value="'+val.prod_id+'" />'
        + '<input type="hidden" name="cat_id" value="'+val.categories[0]+'" />'
        + '<input type="hidden" name="sku" value="'+val.sku+'" />'
        + '<input type="hidden" name="price" value="'+val.price+'" />'
        + '<input type="hidden" name="qty" id="qty_'+val.prod_id+'" value="1"  class="formtext" maxlength="3" />'
        + '<input type="image" src="http://cdn.gurneys.com/images/btn-addtocart.jpg" alt="Add to Cart" name="submit" />'
        + '</form>';
      if((val.is_parent=='y')||(val.stock_status=='Out of Stock'))
        frm = '<div class="more-link"><a href="/product/'+val.url_name+'"><img src="http://cdn.gurneys.com/images/btn-addtocart.jpg" alt="Add to Cart" name="submit" /></a></div>';
      var prc = '<div class="price">$'+parseFloat(val.price).toFixed(2)+'</div>';
      if((val.special_price > 0) && (val.special_price < val.standard_price))
        prc = '<div class="price"><del>'+parseFloat(val.standard_price).toFixed(2)+'</del> $'+parseFloat(val.special_price).toFixed(2)+'</div>';
      items.push('<li class=" iefix" id="veg-favorites-' + key + '">'
        + '<div class="thumb-product">'
        + '<div class="image"><a href="/product/'+val.url_name+'"><img src="http://cdn.gurneys.com'+val.web_image+'" alt="'+val.prod_name+'" title="'+val.prod_name+'" style="width:110px;height:110px;" /></a></div>'
        + '<div class="info">'
        + '  <div class="name"><a href="/product/'+val.url_name+'">'+val.prod_name+'</a></div>'
        + '  <div id="vfStars'+val.sku+'" />'
        + '  ' + prc
        + '</div>'
        + frm
        + '</div>'
          + '</li>');
        i++;
      }
    });
    $('<ul/>', {
      'class': 'homegrid',
      html: items.join('')
    }).appendTo('#homegrid-veg-favorites-items');
    
    i = 0;
    $.each(data.data, function(key, val) {
      if(i < 5) {
        GaCommon.ShowReviewStars(val.reviews.rating, '#vfStars'+val.sku);
        if((val.is_parent!='y')&&(val.stock_status!='Out of Stock'))
          GaCommonWs.AddToCart('#veg-favorites-buynow-'+val.sku+'_form',val.sku);
        i++;
      }
    });    
  });
});
</script>
