<script>

function promomsgnewsltr_set_email(form){
  var aEmail = $('#promomsgnewsltr').find('input[name="add_email"]').val();
  var fEmail = $('#promomsgnewsltr').find('input[name="from_email"]').val();
  var tEmail = $('#promomsgnewsltr').find('input[name="to_email"]').val();

  var promoCode = "0534656";
  
  var emailValid = false;
  if( /(.+)@(.+){2,}\.(.+){2,}/.test(aEmail) ){
    emailValid = true;
  }

  if((aEmail.length <= 0) || (aEmail=="Enter Your Email Address Here") || (aEmail=="Enter Your Email for Instant Savings")) {
    $("#promomsgnewsltrerr").show();
    alert("Email address is required");
    return false;
  }
  else if (emailValid == false) {
    $("#promomsgnewsltrerr").show();
    alert("A valid email address is required");
    return false;
  }
  else {
    $("#promomsgnewsltrerr").hide();
    $.post("/promo_message_apply", { p: promoCode }, function(response){ });
    $.post("index.php", { action: "NewsletterRequest", to_email: tEmail, from_email: fEmail, add_email: aEmail, p: promoCode } );
    $.post("/newsletter_signup", { method: "emailsignup", to_email: tEmail, from_email: fEmail, add_email: aEmail, location: "email_offer::banner", p: promoCode } );
    UCX.LogContact("33D72149", "", aEmail, "", "email_offer:homepage:banner", promoCode);
    GaCommon.SetCookie('emailCollected','true',15);
    GaCommon.SetCookie('email',aEmail,15);
    $('#promomsgnewsltrcont').hide();
    $('#promomsgnewsltrthanks').show();    
    return false;
  }
  return false;
}  

</script><!--
<promo_code></promo_code>--></section>    
    	                                                                                      
                  
                  
                  
                  
                  
                  
