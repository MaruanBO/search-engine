<script>
function homepage_updateQuickOrderTile() {
  var shcr = false;
  var shcrv = '<a href="/catalog_request"><img src="http://cdn.gurneys.com/images/GU-home_catrequest.jpg" alt="Request a Catalog" /></a>';
  var utmz = GaCommon.GetCookie('__utmz');
  var creq = GaCommon.GetCookie('catalogRequested');
  if (typeof utmz !== "undefined" && utmz !== null) {
    if (creq == 'yes')
    shcr = false;
  else if (((utmz.indexOf("utmcmd=cpc") > -1)&&(utmz.indexOf("utmctr=gurneys") == -1))||(utmz.indexOf("utmcmd=organic") > -1))
    shcr = true;
  else
    shcr = false;
  if (shcr==true)
    $('#qoTile').html(shcrv);
  }
  
}

</script>

