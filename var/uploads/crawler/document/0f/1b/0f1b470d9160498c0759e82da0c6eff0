<div style="padding-top:110px;text-align:center;">

<script>
 var zip = "";
function handleShopZone(){
  zip = jQuery("#shopZoneZipcode").val();
  if(zip.length!=5)
    alert("Invalid zipcode");
  else
    GaCommon.GetZoneFromZip(zip,"handleShopZoneCallback");
  return false;
}
function handleShopZoneCallback(result) {
  if(result.Zone!=null){
  	var keywordId = "";
    if(result.Zone.replace(/\D/g,'') == 10)
    	keywordId = 12;
  	else if(result.Zone.replace(/\D/g,'') == 2)
    	keywordId = 18;
    else if(result.Zone.replace(/\D/g,'') == 3)
    	keywordId = 23;
    else if(result.Zone.replace(/\D/g,'') == 4)
    	keywordId = 29;
    else if(result.Zone.replace(/\D/g,'') == 5)
    	keywordId = 35;
    else if(result.Zone.replace(/\D/g,'') == 6)
    	keywordId = 41;
    else if(result.Zone.replace(/\D/g,'') == 7)
    	keywordId = 47;
    else if(result.Zone.replace(/\D/g,'') == 8)
    	keywordId = 53;
    else if(result.Zone.replace(/\D/g,'') == 8)
    	keywordId = 59;
      
   
    document.location.href='/category/f/'+keywordId+'|a?zip='+zip;
    //jQuery('#shopZoneFormSubmit').attr('action','/category_sort/best_seller/f--'+keywordId+'?zip='+zip);
    //jQuery('#shopZoneFormSubmit').submit();
  }
  else {
    alert('Zone not found');
  }
}
</script>

  <form method="get" action="/" id="shopZoneFormSubmit">
  </form>
  <form method="post" action="/" name="shopZoneForm" id="shopZoneForm" onsubmit="return handleShopZone();">
  <input type="text" name="shopZoneZipcode" id="shopZoneZipcode" value="Enter your zip code" style="height:22px;width:190px;font-size:12px;vertical-align:middle;" onfocus="if (this.value=='Enter your zip code') this.value='';" onblur="if (this.value=='') this.value='Enter your zip code';" />
  <input type="image" name="submit" src="http://cdn.gurneys.com/images/GU-home-btn-submit.png" />
  </form>
</div>
</section>                
