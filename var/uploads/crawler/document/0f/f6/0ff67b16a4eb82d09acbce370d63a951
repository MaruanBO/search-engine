var months = new Array('jan', 'feb', 'mar', 'apr', 'may', 'jun', 'jul', 'aug', 'sep', 'oct', 'nov', 'dec');
function go_to_event_date(y, m, d) {
    if (d < 10) { d = '0' + d.toString(); }
    window.location = 'http://www.independent.com/events/' + y + '/' + months[m-1] + '/' + d + '/';
}
var eventcal;
addEvent(window, 'load', function() {
    if (navigator.userAgent.indexOf('MSIE 5') != -1
	&&
	navigator.userAgent.indexOf('Mac') != -1) return; /* Unfortunate browser detect for IE5/Mac */
    eventcal = new Calendar("eventcal", "go_to_event_date");
    eventcal.drawDate(02, 2015);
    document.getElementById('c1922015').style.backgroundColor = '#f4b371';
});
</script>

