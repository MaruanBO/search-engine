	$(window).bind("load", function(){
		$("a[rel^='prettyPhoto']").prettyPhoto( {hideflash: true, theme: 'light_square', showTitle: false, iframe_markup: '<iframe src ="{path}" width="{width}" height="{height}" frameborder="no" scrolling="no"></iframe>'});
