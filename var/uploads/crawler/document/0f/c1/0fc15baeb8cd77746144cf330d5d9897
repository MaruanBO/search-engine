			$("body").addClass("signed-in").removeClass("not-signed-in");
		}

		//get current nav
		var myString = new String(document.location); 
		var myArray = myString.split('/'); 
		if (myArray[myArray.length-2] == 'www.wjla.com') {
			$('.nav-news').addClass('current');
		} else {
			$('.nav-news').removeClass('current');
			$('.nav-'+myArray[myArray.length-2]).addClass('current');
		}

	});
</script>

