	<div class="modalButtonContainer">
		<button type="button" class="deLocaleBtn" id="DeutschLocaleSelector" name="DeutschLocaleSelector">Deutsch</button>
		<button type="button" class="englishLocaleBtn" id="EnglishLocaleSelector" name="EnglishLocaleSelector">English</button>
	</div>
</div>
