	<script type="text/javascript">
	$(document).ready(function() {

		// setup querystring object
		var _querystring = {};
		var qs = window.location.search.substring(1).split("&");
		for (var i=0; i < qs.length; i++) {
			var singleQS = qs[i].split("=");
			for (var j = 0; j < singleQS.length; j++) {
				_querystring[singleQS[0]] = singleQS[1];
			}
		}

		switch (_querystring.lang) {
			case "de":
				$(".deLocaleBtn").click(function() {
					window.location='/de';
				});
				$(".englishLocaleBtn").click(function() {
					$.modal.close();
				});
				$(".localeModalDE").modal();
				break;
			case "ptb":
				$(".ptbLocaleBtn").click(function() {
					window.location='/br';
				});
				$(".englishLocaleBtn").click(function() {
					$.modal.close();
				});
				$(".localeModalPTB").modal();
				break;
		}

	});
</script>
<div class="localeModal localeModalDE hide">
	<div class="modalLogoWrapper"><img src="/images/redesign2009/logo.png"/></div>
