<script language="javascript"> 
function toggleme(showdiv) {
	var chart = document.getElementById("trend");
	var samples = document.getElementById("sampbox");
	var comments = document.getElementById("about");
	var chartbut = document.getElementById("trendbutton");
	var samplebut = document.getElementById("samplebutton");
	var commentbut = document.getElementById("comments");
	if(showdiv=="samples") {
//		alert ("elestyle display"+ele.style.display);
    		chart.style.display = "none";
			chartbut.style.backgroundColor="#EDF6D7";
			commentbut.style.backgroundColor="#EDF6D7";
			samplebut.style.backgroundColor="#AECA76";
			comments.style.display = "none";
			samples.style.display = "block";
  	}
	if (showdiv=="trend"){
//	alert ("elestyle display"+ele.style.display);
		chart.style.display = "block";
		comments.style.display = "none";
		samples.style.display = "none";
		chartbut.style.backgroundColor="#AECA76";
		commentbut.style.backgroundColor="#EDF6D7";
		samplebut.style.backgroundColor="#EDF6D7";
	}
	if (showdiv=="comment"){
//	alert ("elestyle display"+ele.style.display);
		comments.style.display = "block";
		chart.style.display = "none";
		samples.style.display = "none";
		chartbut.style.backgroundColor="#EDF6D7";
		commentbut.style.backgroundColor="#AECA76";
		samplebut.style.backgroundColor="#EDF6D7";
	}
} 
	</script>
	
