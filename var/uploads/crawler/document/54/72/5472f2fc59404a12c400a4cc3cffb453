		   for (i=0;i<chartnums.length;i++)
		   {
		   
				switch(trim(String(chartnums[i].Description))) 
				{
					case "Good MD5":returnData[0].data.push(chartnums[i].theCount);break;
					case "PC Pitstop Bad":returnData[1].data.push(chartnums[i].theCount);break;
