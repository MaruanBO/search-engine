(function () {
	'use_strict';
	var
	store = {},
	load  = function (ele, type, id, param, selected) {
		var url = "/nc_util/utils/fetch_" + [type, id, param].join("/") + ".json";
		if (typeof store[url] === "undefined") {
			$.ajax({
				"url": url,
				"success": function (data) {
					var lbl = (type === "cities") ? "Any City" : "All Categories";
					store[url] = data;
					if (data.length > 0) {
						store[url].unshift({"id": "0", "title": lbl});
					}
					update(store[url], ele, selected);
				}
			});
		} else {
			update(store[url], ele, selected);
		}
	},
	update = function (data, ele, selected) {
		$(ele).empty();
		if (data.length == 0) {
			$(ele).hide();
		} else {
			$(ele).show();
		}
		$.each(data, function(key, obj) {
			var ele2 = $("<option></option>").attr("value", obj.id).text(obj.title);
			if (obj.id == selected) {
				$(ele2).attr("selected", "selected");
			}
			$(ele).append(ele2);
		});
	};
	$("#SearchCp").on("change", function () {
		load("#SearchCc", "categories", this[this.selectedIndex].value, "false", null);
	});
	$("#SearchA").on("change", function () {
		load("#SearchC", "cities", this[this.selectedIndex].value, null, null);
	});
	load("#SearchC", "cities", 'all', null, null);
	load("#SearchCp", "categories", 977, "true", null);
	load("#SearchCc", "categories", null, "true", null);
	$('#event_search').on('submit', function (evt) {
		nc.interaction('searchEvent', this, evt);
	});
}());
</script></div><!-- .grid6 --></div><!-- .grid8 --></section><!-- #events --></div><!-- .col- -main --><section id="featured" class="col--main__col1"><section class="offers full tier"><header class="wf_wisdom_alternate"><h1><span>Travel</span> Deals</h1><img src="//www.visitnc.com/files/img/icon-deals.png" alt="Travel Deals icon"></header><div class="full list"><article class="g-1of2 first hasimage"><div class="list__details"><h1><a href="/travel-deal/do-more-in-four-special">FREE Night - Do More in Four Special</a></h1><p class="list__details__provider">Bed & Breakfast on Tiffany Hill</p><p class="list__details__place">Mills River</p><p class="list__details__valid_thru">			Valid: Dec 31 - Feb 28</p></div><!-- .list_details --><a href="/travel-deal/do-more-in-four-special" alt="Bed & Breakfast on Tiffany Hill - Do More in Four Special" title="Click to See Travel Deal"><img src="/contents/imgcrop/53542/100/100" id="53542Image" alt="Do More in Four Special" title="Do More in Four Special"/></a></article><article class="g-2of2  hasimage"><div class="list__details"><h1><a href="/travel-deal/beacon-house-romance-packages">$125 & Up - Beacon House Romance Packages</a></h1><p class="list__details__provider">Beacon House Inn Bed & Breakfast</p><p class="list__details__place">Carolina Beach</p><p class="list__details__valid_thru">			Valid: Jan 7 - Feb 28</p></div><!-- .list_details --><a href="/travel-deal/beacon-house-romance-packages" alt="Beacon House Inn Bed & Breakfast - Beacon House Romance Packages" title="Click to See Travel Deal"><img src="/contents/imgcrop/54632/100/100" id="54632Image" alt="Beacon House Romance Packages" title="Beacon House Romance Packages"/></a></article><article class="g-1of2  hasimage"><div class="list__details"><h1><a href="/travel-deal/the-world-s-largest-dinosaurs-exhibition">$4 Off - The World’s Largest Dinosaurs Exhibition</a></h1><p class="list__details__provider">North Carolina Museum of Natural Sciences</p><p class="list__details__plac