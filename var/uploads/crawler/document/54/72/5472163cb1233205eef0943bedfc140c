			$(this).removeClass('is-open').addClass('is-closed');
		}
	});
	$("a.site_tools__search__close").on("click", function (evt) {
		evt.preventDefault();
		$('.site_tools__search').removeClass('open');
		$("a.site_tools__search__trigger").removeClass('is-open').addClass('is-closed');
	});
	$("a.site_tools__account__close").on("click", function (evt) {
		evt.preventDefault();
