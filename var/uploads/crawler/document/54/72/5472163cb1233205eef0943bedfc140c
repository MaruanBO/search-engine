(function () {
	'use strict';
	$("a.site_tools__search__trigger").on("click", function (evt) {
		evt.preventDefault();
		if($(this).hasClass('is-closed')) {
			$('.nav__ul .hover').removeClass('hover');
			$('.subnav__bg').removeClass('is-active').css('height','0');
			$('#global_header').addClass('search_open');
			$('.site_tools .open').removeClass('open');
			$('.site_tools__triggers .is-open').removeClass('is-open').addClass('is-closed');
			$('.site_tools__search').addClass('open');
			$(this).removeClass('is-closed').addClass('is-open');
		} else {
			$('.site_tools__search').removeClass('open');
			$('#global_header').removeClass('search_open');
			$(this).removeClass('is-open').addClass('is-closed');
		}
	});
	$("a.site_tools__account__trigger").on("click", function (evt) {
		evt.preventDefault();
		if($(this).hasClass('is-closed')) {
			$('.nav__ul .hover').removeClass('hover');
			$('.subnav__bg').removeClass('is-active').css('height','0');
			$('.site_tools .open').removeClass('open');
			$('.site_tools__triggers .is-open').removeClass('is-open').addClass('is-closed');
			$('.site_tools__account').addClass('open');
			$(this).removeClass('is-closed').addClass('is-open');
		} else {
			$('.site_tools__account').removeClass('open');
			$(this).removeClass('is-open').addClass('is-closed');
		}
	});
