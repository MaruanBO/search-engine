        <script type="text/javascript" src="http://www.heartlandamerica.com/js/prototype/window.js"></script>
    <link rel="stylesheet" type="text/css" href="http://www.heartlandamerica.com/js/prototype/windows/themes/default.css" />
    <link rel="stylesheet" type="text/css" href="http://www.heartlandamerica.com/skin/frontend/enterprise/hla/css/hla_email_modal.css" />
    <script>

        document.observe("dom:loaded", function() {

            var request_running = false;

            var el = $('hla_sw_email_modal');
            var html = el.innerHTML;
            win = new Window({ title: "Sign up and Save $10 at the Heartland!", zIndex:3000, destroyOnClose: true, recenterAuto:false, resizable: false, minimizable: false, maximizable: false, draggable: false});
            win.setHTMLContent(html);
            win.setSize(el.getWidth(), el.getHeight());
            //hack for ie8 since focus cannot be set on a hidden element
            $(win.element.id).setStyle({opacity: '0'});
            //remove status bar
            $(win.element.id + '_row3').hide();
            $(win.element.id).show();
            win.showCenter(true);

            //ajax to collect email address
            Event.observe('hla_email_modal_btn', 'click',
                function(e){

                    if(request_running){
                        e.preventDefault();
                        return;
                    }

                    request_running = true;
                    setModalMsg('Processing...', 'success');

                    var email = $('hla_sw_modal_email').getValue();
                    var form_key = $('hla_sw_modal_form_key').getValue();

                    if(email.length != 0){

                        new Ajax.Request('/emails/email/add/email/' + email + '/preference/1d/location/MODAL', {
                            method: 'post',
                            postBody: 'pref_email_addr=' + email + '&pref_location=MODAL&email_pref=1d&form_key=' + form_key,
                            onSuccess: function(response){
                                if(response.responseText == ''){

                                    setModalMsg('Your email is signed up. Thank you!', 'success');

                                    setTimeout(function(){
                                        //have to use window.event for ie8
                                        Windows.close(win.element.id, window.event);
                                    }, 2000);
                                }
                                else{

                                    setModalMsg(response.responseText, 'error');
                                }
                            },
                            onFailure: function(response){

                                setModalMsg('There was a problem saving your email. Please try again.', 'error');
                            }
                        });
                    }
                    else{

                        setModalMsg('Please enter an email address.', 'error');
                    }
                });

            //close the window if they click outside of it
            Event.observe(document, 'click', function (event) {

                if (event.element().id == 'overlay_modal') {
                    Windows.close(win.element.id, event);
                }
            });

            //determines which message box to show and populate
            function setModalMsg(msg, type){

                $('hla_modal_msg').update(msg);

                if(type == 'error'){

                    $('hla_modal_msg').hide();
                    $('hla_modal_msg_err').show();
                    $('hla_modal_msg_err').update(msg);
                    request_running = false;
                }
                else{
                    $('hla_modal_msg_err').hide();
                    $('hla_modal_msg').show();
                    $('hla_modal_msg').update(msg);
                }
            }
        });

    </script>
    <div id='hla_sw_email_modal' style="display: none;"><div style="background-image: url('http://c426092.r92.cf2.rackcdn.com/site_images/modal_10_bg.jpg');  width: 544px; height: 387px;">
    <div style="height: 262px;">&nbsp;</div>
    <div id="hla_modal_msg_wrap">
        <div id="hla_modal_msg" style="display: none;"></div>
