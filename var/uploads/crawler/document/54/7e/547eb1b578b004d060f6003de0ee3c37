<script type="text/javascript">

var timeoutHandle = window.setInterval(function(){next_slide(1)},20000);


function next_slide(increment,stop){

   if (stop){
      window.clearInterval(timeoutHandle);
   }

   if (! increment){
      increment = 1;
   }

   var slides = new Array("/stats/demo_streaming.pl?oc=1","/stats/demo_map.pl","/stats/demo_map2.pl","/stats/demo_referrer_tracker.pl?loginID=demo");
   var slides_text = new Array("Live activity on sample site","Live map of visitors to sample site","Cumulative traffic map for sample site","Weekly referrer activity on sample site");
   var current_slide = parseInt(document.getElementById("slide_number").innerHTML);
   var next_slide = current_slide + increment;
   if (next_slide >= slides.length){
     next_slide = 0;
   }
   else if (next_slide < 0){
     next_slide = slides.length-1;
   }
   document.getElementById("slide_number").innerHTML = next_slide;
   document.getElementById("sample_text").innerHTML = slides_text[next_slide];
   document.getElementById("demo").src = slides[next_slide];

}


</script>



