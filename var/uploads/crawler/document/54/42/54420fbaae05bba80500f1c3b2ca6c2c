	<script>
	// Geoplugin
	// The following anonymous function loads the geoPlugin script and runs geoTest when the script is ready.
	// This asynchronous loading of the script has the advantage that the page can start with layout while
	// the script loads. When using a script tag the page layout would be delayed until after the script has loaded.
	showNyOffer = false; // var used elsewhere to determine if client was identified as local; may be overridden elsewhere
	(function() {
		function geoTest() {
			try {
				if (geoplugin_areaCode() == 518) {
					showNyOffer = true;
				}
	
				// Some code on the page may depend on this fetch and cannot be executed
				// until after the geolocation is complete, so trigger a custom event now.
				$(document).trigger("geoplugin_success");
	
			} catch (e) {}
		}
	
		var url = "http://www.geoplugin.net/javascript.gp";
		
		// Only do this once the rest of the page has loaded.
		$(document).ready(function() {
			$.ajaxSetup({ cache: true });
			$.getScript(url, geoTest);
		});
	}());
	</script>
</body>
