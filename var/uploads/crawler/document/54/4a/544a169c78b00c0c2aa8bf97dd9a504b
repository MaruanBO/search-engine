		function () { jQuery(this).addClass('highlight'); },
		function () { jQuery(this).removeClass('highlight'); }
	);
	
	jQuery('#primary').hover(
		function () { jQuery('.current').removeClass('active'); },
		function () { jQuery('.current').addClass('active'); }
