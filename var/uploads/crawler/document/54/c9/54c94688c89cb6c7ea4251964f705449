    var theLeaderboard = document.getElementById('adcode1');
    if (theLeaderboard.clientHeight > 0) {
        document.getElementById('adposition1').style.height = theLeaderboard.clientHeight + "px";
        if (theLeaderboard.clientWidth > 730) {
            document.getElementById('adposition1').style.width = theLeaderboard.clientWidth + "px";
        }
        DS.Desktop.leaderboardInit();
        clearInterval(theLeaderboardInterval);
    }
    else if (theLeaderboardCount == 5) {
        clearInterval(theLeaderboardInterval);
    }
    theLeaderboardCount++;
}, 50);
