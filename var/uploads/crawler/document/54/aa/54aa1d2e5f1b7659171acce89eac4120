<div class="dailyChoice">

	<div class="dailyChoice-label"></div>

<script>
var currentDailyChoice=1, movingDailyChoicePic, rotateDailyChoice;

function SelectDailyChoice(c)
{
var title = document.getElementById('dailyChoice-title'+c);
currentDailyChoice = c;

	if(title.style.zIndex == 3)
	return false;

	for(var i=1; i <= 5; i++)
	{
	var t = document.getElementById('dailyChoice-title'+i), g = document.getElementById('dailyChoice-title-genre'+i);
	t.style.zIndex = 1;
	t.style.opacity = "0.5";
	t.style.filter  = 'alpha(opacity=50)';
	g.style.display = 'none';
	}

title.style.zIndex = 3;
title.style.opacity = "1";
title.style.filter  = 'alpha(opacity=100)';
document.getElementById('dailyChoice-title-genre'+c).style.display = 'block';
clearInterval(movingDailyChoicePic);
movingDailyChoicePic = setInterval('MoveDailyChoicePic(-'+(290*(c-1))+')', 10);
}

function MoveDailyChoicePic(to)
{
var div = document.getElementById('dailyChoice-images');
var tmp = div.style.marginTop;
tmp = parseInt(tmp.replace('px', ''))
	if(tmp != to)
	{
	var left = Math.abs(to-tmp), by=30;
		if(left < 20) by = 1;
		else if(left < 50) by = 2;
		else if(left < 100) by = 5;
		else if(left < 300) by = 10;
	tmp = (to > tmp) ? tmp+by : tmp-by;
	div.style.marginTop = tmp+'px';
	}
	else
	clearInterval(movingDailyChoicePic);
}

function RotateDailyChoice()
{
currentDailyChoice = (currentDailyChoice >= 5) ? 1 : currentDailyChoice+1;
SelectDailyChoice(currentDailyChoice);
}
</script>

