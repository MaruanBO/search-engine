window.fbAsyncInit = function()
{
FB.init({appId: '133194620036541', status: true, cookie: true, xfbml: true});

	FB.getLoginStatus(function(response)
	{
		if(response.status == 'connected')
		_FBUID = response.authResponse.userID || -1;
		if(!_FBUID)
		{
		$("#SocialSwitch").attr("class", "switch off");
		$("#SocialSwitch .switch-text").text("OFF");
		}
	ShowLayer('SocialSwitch');
		else if(document.getElementById("page") == null)
		{
		var d = document;
			if(d.getElementById('infobar') == null && d.getElementById('add_search') == null)
			{
			var i = d.createElement('div');
			i.id = 'infobar';
			i.className = 'infobar login-with-facebook';
			d.body.insertBefore(i, d.body.firstChild);
			d.getElementById('infobar').innerHTML = '<fb:login-button size="large" onlogin="document.location.replace(\'/browse?fbLogin=3e92e3b4dbec27c4f28fd4f313feec9e\');" scope="email,user_birthday,publish_actions">Login with facebook</fb:login-button><span class="infobar-fb-login-text">&nbsp;&nbsp;and kick-start your MusicPlayOn experience!</span>';
			FB.XFBML.parse(document.getElementById('infobar'));
			}
		}
	});

	FB.Event.subscribe('edge.create', function(targetUrl)
	{
	_gaq.push(['_trackSocial', 'Facebook', 'Like', targetUrl]);
	});
	
	FB.Event.subscribe('edge.remove', function(targetUrl)
	{
	_gaq.push(['_trackSocial', 'Facebook', 'Unlike', targetUrl]);
	});

	FB.Event.subscribe('comment.create', function(response)
	{
	$(window).trigger('resize');
	var targetUrl = response.href;
		FB.api(
		{
		method: 'fql.query',
		query: "Select fromid, text From comment Where object_id IN (Select comments_fbid From link_stat Where url ='"+response.href+"') Order By time DESC Limit 1"
		},
		function(response)
		{
		var fbComment = response[0];
		var radio = (_RID > 0) ? "radio-" : "";
			if(fbComment.text)
			{
			Ajax(radio+"addopinion",(radio?_RID:_VID),"1&comment="+encodeURIComponent(fbComment.text)+"&fbUID="+fbComment.fromid);
			_gaq.push(['_trackSocial', 'Facebook', 'Comment', targetUrl]);
			}
		});
	});

	if(document.getElementById('page') != null)
	{
	FB.Event.subscribe("xfbml.render", function(response) { $(window).trigger('resize'); } );
	FB.Event.subscribe("comment.remove", function(response) { $(window).trigger('resize'); } );
	}

};

