<script type="text/javascript" src="/JavaScripts/jquery.cycle.min.js"></script>
<script type="text/javascript">
    // Ride the carousel...
    jQuery(document).ready(function () {
        jQuery("#mycarousel").cycle({
            timeout: 5000,
            after: onAfter,
            pager: '#homeImgSlideShowNav',
            slideExpr: 'a',
            pagerAnchorBuilder: function (index, el) {
                var num = index + 1;
                return '<a href="#">&#183;</a>';
            }
        });
    });

    function onAfter() {
        var href = this.href; var clink = this.attributes.getNamedItem("clink").nodeValue;
        var div = document.getElementById("homeImgSlideShow");
        div.title = this.attributes.getNamedItem("title").nodeValue;
        var onclick = this.attributes.getNamedItem("onclick").nodeValue;
        var $obj = $("#homeImgSlideShow");
        $obj.css({ opacity: 0.2, background: "url(" + href + ") no-repeat", height: 292, width: 724 })
        .animate({ opacity: 1.0 }, 3000)
        .click(function () { urchinTracker("/" + onclick ); window.location = window.location + "/" + clink; });
    }
</script>
