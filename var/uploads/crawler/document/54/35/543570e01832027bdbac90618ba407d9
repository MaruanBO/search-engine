$("#en_promo_tracker_form").attr("action","http://www.ti.com/foldercomponents/docs/comp/getRegionalCookie.tsp");
$("#cn_promo_tracker_form").attr("action","http://www.ti.com.cn/foldercomponents/cn/docs/comp/getRegionalCookie.tsp");
$("#jp_promo_tracker_form").attr("action","http://www.tij.co.jp/foldercomponents/jp/docs/comp/getRegionalCookie.tsp");
</script>

<script type="text/javascript">		
com.TI.promo.constructActivityBar();
</script>

<script type="text/javascript">
$(document).ready(function(){
	var $page = $('.page'),
	
		$header = $('header'),
		$nav = $('.nav',$header),
		$navLis = $('li',$nav),
		$navExpandLinks = $('a.nav_expand',$nav),
		$navExpandContent = $('div.sub',$header);

	var defaultShow = '_history';
	
	$('#nav'+defaultShow).addClass('active');
	var $subHistory = $('#sub' + defaultShow);
	$subHistory. addClass('active');

	
	$navExpandLinks.click(function(e){
		e.preventDefault();
		var $this = $(this),
			$parent = $this.parent(),
			subId = $parent.attr('id').replace('nav_','sub_'),
			isActive = $parent.hasClass('active');
		$navLis.removeClass("active");
		$navExpandContent.removeClass('active');
		if (!isActive) {
			$parent.addClass("active");
			$('#'+subId).addClass('active');
		}
			
	});
	
		$subHistory.find('.pin').click(function(e){
		$(".pinMenu").toggleClass('active');
	});
	
	if (navigator.userAgent.match(/mobi/i) || navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/webOS/i) || navigator.userAgent.match(/iPhone/i)
            || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPod/i) || navigator.userAgent.match(/BlackBerry/i)
            || navigator.userAgent.match(/Windows Phone/i) || navigator.userAgent.match(/Opera Mini/i) || navigator.userAgent.match(/IEMobile/i))
		$('html').addClass('mobile');


});
</script>



<script type="text/javascript">
	function setCookie(cname, cvalue, exdays,domain) {
		var d = new Date();
		d.setTime(d.getTime() + (exdays*24*60*60*1000));
		var expires = "expires="+d.toUTCString();
		document.cookie = cname + "=" + cvalue + "; " + expires+"; path=/; domain="+domain;
	}
	setCookie('ST_TOGGLE_PREFERENCE','EN',365,"ti.com");
</script>

</body>
</html>
        
    

