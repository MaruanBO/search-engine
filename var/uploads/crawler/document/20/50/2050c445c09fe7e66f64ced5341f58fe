              function showAds(cpmStar) {
                 var adContainer= document.getElementById('cpmstargames');
                 while (cpmStar.adsLeft()) {
                    var ad = document.createElement('div');
                    ad.setAttribute("class", "favorite_item");
                    ad.className = "favorite_item";
                    ad.innerHTML = "" +
