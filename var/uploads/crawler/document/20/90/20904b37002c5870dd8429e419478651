                l = navigator.language ? navigator.language.toLowerCase().replace(/-[a-z]+$/, "") : navigator.userLanguage ? navigator.userLanguage.toLowerCase().replace(/-[a-z]+$/, "") : "en",
                c = EN.cookie.readCookie("EN_lang"),
                d = "en",
                s = window.location.hostname || "www.euronews.com/",
                p = s.match(/^([^\.]+)/)[0],
                m = /^(en|fr|de|it|es|pt|ru|ar|tr|ua|pl|gr)$/;
            
            if(c && c.match(m)) {                        
                if(c != d) { window.location = "http://" + (t[c] || "www") + ".euronews.com/"; };                 
            } else if(l.match(m)) {                        
                EN.cookie.createCookie("EN_lang", l, "365");            
                if(l != d) { window.location = "http://" + (t[l] || "www") + ".euronews.com/"; };
            };
        }; 
    } catch(err) {};          
})();
