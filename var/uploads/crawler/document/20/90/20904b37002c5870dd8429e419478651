<script type="text/javascript">
// <![CDATA[  
         
        function showVotingForm() {                 
                var content   = document.getElementById("poll-js-1460"),
                    dl        = content.getElementsByTagName("dl")[0],
                    fm        = content.getElementsByTagName("form")[0],
                    pt        = document.getElementById("pHook_1460");
                    
                pt.parentNode.removeChild(pt);
                dl.style.display = "none";
                fm.style.display = "block";                 
                return false;        
        };
   
(function() {
        if(!EN.cookie.cookieEnabled()) { return; };          
        var wrapper   = document.getElementById("poll_1460"),
            content   = document.getElementById("poll-js-1460"),
 
            warning   = content.getElementsByTagName("div")[0],
 
            pollHTML  = "",
            pollTS    = '1424381715|41|89|28|15|44'.split("|"),
            cookie    = EN.cookie.readCookie('en_poll_1460'),
            cValid    = false,
            votes     = 0,
            output    = [],
            questions = ["Marion Cotillard (Two Days, One Night)","Rosamund Pike (Gone Girl)","Felicity Jones (The Theory of Everything)","Reese Witherspoon (Wild)","Julianne Moore (Still Alice)"],
            per, formHTML, resultsHTML;              

        if(cookie) {                    
                cookie = decodeURI(cookie).split("|");                
                if(cookie && cookie.length && pollTS && pollTS.length && cookie.length == pollTS.length && cookie[0] > pollTS[0]) {
                        pollTS = cookie; 
                        cValid = true;                             
                };
        };
        
 
        warning.parentNode.removeChild(warning);
 
        if(cookie && cValid) {                    
                for(var i = 1; i < pollTS.length; i++) {                
                        votes += parseInt(pollTS[i], 10) || 0;        
                };             
               
                var total = 0;
                
                for(var i = 1; i < pollTS.length; i++) {
                        per = pollTS[i] > 0 ? Math.round(pollTS[i] / (votes / 100)) : 0;                        
                        if((i == pollTS.length - 1) || (total + per > 100)) {
                                per = 100 - total;
                                total = 100;                                 
                        } else {
                                total += per;
                        }; 
                        output[output.length] = '<dt dir="ltr">' + questions[i - 1] + ' <span class="votePercentage" dir="ltr">(' + per + '% - ' + (pollTS[i]) + ' votes)</span></dt><dd><span title="" style="width:'+(per == 0 ? '1px' : per+'%')+';">&#160;</span></dd>';
                };         
                resultsHTML = '<dl>' + output.join("") + '</dl><span id="pHook_1460"></span><div id="pForm_1460"></div>';
                content.innerHTML = resultsHTML;                 
        };                                       

                            
        if(!cookie) {                
                document.getElementById("pHook_1460").innerHTML = '<a title="Participate in this poll" class="participateButton" href="#" onclick="showVotingForm(); return false;">Participate in this poll</a>';        
                formHTML = '<form style="display:none" class="poll_form" action="/news/you/#poll_1460" method="post"><input type="hidden" name="poll_id" value="1460" /><ol dir="ltr"><li dir="ltr"><input class="pollChoice" type="radio" name="poll_choice" id="choice_5568" value="5568" /> <label class="pollChoiceLabel" for="choice_5568">Marion Cotillard (Two Days, One Night)</label></li><li dir="ltr"><input class="pollChoice" type="radio" name="poll_choice" id="choice_5570" value="5570" /> <label class="pollChoiceLabel" for="choice_5570">Rosamund Pike (Gone Girl)</label></li><li dir="ltr"><input class="pollChoice" type="radio" name="poll_choice" id="choice_5572" value="5572" /> <label class="pollChoiceLabel" for="choice_5572">Felicity Jones (The Theory of Everything)</label></li><li dir="ltr"><input class="pollChoice" type="radio" name="poll_choice" id="choice_5574" value="5574" /> <label class="pollChoiceLabel" for="choice_5574">Reese Witherspoon (Wild)</label></li><li dir="ltr"><input class="pollChoice" type="radio" name="poll_choice" id="choice_5576" value="5576" /> <label class="pollChoiceLabel" for="choice_5576">Julianne Moore (Still Alice)</label></li></ol><input type="submit" class="poll-submit" name="poll_submit" value="Vote" /></form>';                document.getElementById("pForm_1460").innerHTML = formHTML;
        };
           
})();
