        <div class="contentArea noComment ">
        
	        <h2> 
            	<a href="/nocomment/" title="no comment">no comment</a>
            </h2>
            
            <div class="block" dir="ltr">
	            
	
<script type="text/javascript" language="javascript">
var leftMargin = 0,frameWidth,carrouselWidth;

function slideCarousel(e) {
	
	var direction = $(this).hasClass("next") ? -1 : 1;
	
	leftMargin += direction * frameWidth ;
	
	
	if (leftMargin >=0 && leftMargin < frameWidth) {
		leftMargin = 0 ;	
	} else if (leftMargin >=0) {
		leftMargin = -1*(carrouselWidth - frameWidth) ;
	} else if ((-1)*leftMargin >= carrouselWidth ) {
		leftMargin = 0 ;
	} else if ((-1)*leftMargin >= carrouselWidth - frameWidth) {
		leftMargin = -1*(carrouselWidth - frameWidth) ;
	}
	
	$("#noCommentSlider ul").animate({'left' : leftMargin}, {queue:false, duration:500});
	
	return false; 
};
    
$(document).ready(function() {
	frameWidth = $("#noCommentSlider").width() + parseInt($("#noCommentSlider ul li").css("marginRight"));
	carrouselWidth = ($("#noCommentSlider ul li").length / 6) * frameWidth;
	$(".carrousel .prevNextButton").click(slideCarousel);
});
	  
</script>
	
