		<script>
			$( document ).ready(function() {
				jQuery('#promoSliderControl').click(function(){
					var slider = jQuery("#promoSlider");
					if(slider.hasClass('expanded')){
						jQuery("#promoSliderControl span").text("+ EXPAND");
						slider.removeClass('expanded', 500);
						jQuery.cookie('promoSliderCollapse', 'true', {path:'/'});
					}else{
						jQuery("#promoSliderControl span").text("- COLLAPSE");
						slider.addClass('expanded', 500);
						jQuery.cookie('promoSliderCollapse', 'false', {path:'/'});
					}
				});
			});
		</script>
	

