    <script type="text/javascript">
    //<![CDATA[


    jQuery(function($) {

        $.fn.swipeTabs = function(config) {

            config = config || {};

            return this.each(function() {

                var slider;
                slider = $(this).sliderTabs(config);

                slider.swipe({
                    allowPageScroll: 'vertical',
                    threshold:  100,
                    swipeLeft: function(event, direction, distance, duration, fingerCount) {
                        slider.data('sliderTabs').next();
                    },
                    swipeRight: function() {
                        slider.data('sliderTabs').prev();
                    },
                    tap:function(event,target){
                        if(!$(target).parent().hasClass('bc-hero-js')){
                           $(target).click();
                        }
                    }
                });

            });

        };

        if ( isTouchable !== true ) {
            $("#home-hero-slider-tab").sliderTabs({
                tabHeight: 'auto',
                height: 300,
                position: "bottom",
                tabArrowWidth: 0,
                transition: 'fade',
                transitionSpeed: 500,
                mousewheel: false,
            });

        } else {
            $("#home-hero-slider-tab").swipeTabs({
                tabHeight: 'auto',
                position: "bottom",
                tabArrowWidth: 0,
                indicators: true,
                transitionSpeed: 500,
                mousewheel: false
            });
        }
        var intervalForResize;

        
        intervalForResize = setInterval(function() {
            if (typeof modExp == "object") {
                changeVideoSize();
                clearInterval(intervalForResize);
                $(window).resize(function() {changeVideoSize()});
            }
        }, 500);

        function changeVideoSize() {
            modExp.setSize($('.ui-slider-tabs-content-container').width(), $('.ui-slider-tabs-content-container').height());
            $('#brightCove-player object').css({'width' : $('.ui-slider-tabs-content-container').width(), 'height': $('.ui-slider-tabs-content-container').height()});
        }
        $(document).ready(function() {
            $('#home-hero-slider-tab .ui-slider-tabs-list li').css({'width' : '25%', 'max-width' : 'inherit'});

            // set height for tabs all hero tabs to be equal to the tallest tab
            maxHeight = 0;
            tabs = $('#home-hero-slider-tab .ui-slider-tabs-list li a');
            $.each(tabs, function(){
                if (parseInt($(this).css('height'))>maxHeight){
                    maxHeight = parseInt($(this).css('height'));
                }
            });

            $.each(tabs, function(){
                $(this).css('height',maxHeight);
            });

            $('#home-hero-slider-tab .ui-slider-tabs-list li a').on(customClickEvent, function (e) {
                e.preventDefault();
            });

        });

        //Home hero content block height

        $(document).ready(function() {
            setContentBlockHeight();
        });

        var resizeTimer;
        $(window).resize(function() {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(setContentBlockHeight, 1500);
        });

        function setContentBlockHeight() {
            var sliderContentBlock =  $('#home-hero-slider-tab .ui-slider-tabs-content-container'),
                windowWidth = $(window).width();

            if (windowWidth >= 960) {
                sliderContentBlock.css('height', 300 + 'px');

            } else if (windowWidth > 767 && windowWidth < 960){
                sliderContentBlock.css('height', windowWidth/3.2 + 'px');
            }
        }

    });

