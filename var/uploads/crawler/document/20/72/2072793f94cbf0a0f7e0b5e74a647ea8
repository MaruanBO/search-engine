function SetCol(SubCol){
var $j = jQuery.noConflict();
$j(".holder").html("");
for ( var i = SubCol.length-1 ; i >= 0; i-- ) {
    $j(".holder").append( SubCol[ i ]);
  }
  $j(".col").css("margin-right", "");
  $j(".col:nth-child(3n)").css("margin-right", "0px");
}

$j("#tf_pro").click(function(){$j(".TypeFilter").removeClass("blue").addClass("lightgray");$j(this).addClass("blue").removeClass("lightgray");$j(".writtemPost").parents(".col").hide();$j(".videoPost").parents(".col").hide();$j(".promoPost").parents(".col").show();});
$j("#tf_vid").click(function(){$j(".TypeFilter").removeClass("blue").addClass("lightgray");$j(this).addClass("blue").removeClass("lightgray");SetCol(VideoCol);});
$j("#tf_wri").click(function(){$j(".TypeFilter").removeClass("blue").addClass("lightgray");$j(this).addClass("blue").removeClass("lightgray");SetCol(WrittenCol);});
$j("#tf_all").click(function(){$j(".TypeFilter").removeClass("blue").addClass("lightgray");$j(this).addClass("blue").removeClass("lightgray");SetCol(AllCol);});

