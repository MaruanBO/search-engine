    // new set basket $ amount
    function setSLICookieAmount(amount) {
        if (readCookie('SLI_AMOUNT') == null ? false : true) {
            eraseCookie('SLI_AMOUNT');
            createCookie('SLI_AMOUNT', amount,'.dollartree.com'  );
        } else {
            createCookie('SLI_AMOUNT', amount,'.dollartree.com'  );
            //console.log ('==> in set amount, :' + amount);
        }
    }
    //==> now set SLI Cookie for amount $ in basket
    function setSLICookie() {
        var newAmount = $("a.total_in_cart").text();
        if (newAmount) {
            setSLICookieAmount(newAmount);
        }
    }
</script>

