<script type="text/javascript">
    
    createCookie('userKey', '1424383671', '.dollartree.com');
    

    function doSearchFocus(component) 	{
        var searchVal = component.value;
        if(searchVal == "Search by Keyword or item #") component.value = "";
    }

    function doSearchBlur(component)  	{
        var searchVal = component.value;
        if(strTrim(searchVal) == '') component.value = "Search by Keyword or item #";
    }

    function doSearchValidation(theForm)	{
        var searchVal = theForm.keyword.value;
        if(searchVal == "" || searchVal == "Search by Keyword or item #")	{
            alert ("Please enter a search term and try your search again") ;
            return false ;
        }
        return true ;
    }

    function strTrim(s) 	{
        // Remove leading spaces and carriage returns
        while(s.substring(0,1) == ' ') s = s.substring(1, s.length);
        // Remove trailing spaces and carriage returns
        while(s.substring(s.length-1, s.length) == ' ') s = s.substring(0, s.length-1);
        return s;
    }

    function openBasketPage() {
        window.location = 'https://www.dollartree.com/checkout/basket.jsp';
    }

    $(document).ready(function() {
        $(".showUCart").click(function(e) {
            e.preventDefault();
            showBasket('show','');
        });
    });

    //page count session cookie for the zip code modal
    function pageCounter() {
        if (readCookie('page_count') == null ? false : true) {
            var count = parseInt(readCookie('page_count'));
            count = count + 1;
            eraseCookie('page_count');
            createCookie('page_count', count);
        } else {
            createCookie('page_count', 1);
        }
    };
    pageCounter();
    //==> to set cookie for SLI
    setSLICookie();

</script>

