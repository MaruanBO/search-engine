		<script type="text/javascript">
			$(document).ready(function(){
				$("[countdown=true]").countdown().each(function(){this.init()});
				$('[multipleWidth=true]').multipleWidthPromo();
				
			});
			$.fn.multipleWidthPromo = function(){
				return this.each(function(){
					var _self = this;
					this.init = function(){
						this.countdown = $(this).attr('countdown')
						if(this.countdown=="true"){
							this.countdown=true;
						}
						this.options=$(this).attr("imageOptions");
						this.options=$.parseJSON(this.options);
						this.current_time = new Date(); 
						this.current_time = this.current_time.setMilliseconds(0); 
						this.current_time = this.current_time/1000; 
						this.reskin = false;
						this.setUpPromo();
						this.firstPass();												
						this.attachEvents();
						this.current=-1;
					}
					this.attachEvents = function () {
						$(window).resize(function(){
							_self.firstPass();
						});
						$(window).bind('DartReskinApplied', function(){
							_self.reskin = true;
							_self.firstPass();
						});
					}
					this.firstPass = function(){
						this.currentWidth = $(window).width();
						var half_window = this.currentWidth/2;
						if(this.reskin){
							$(this).find('.left-side, .right-side').css({'width':'50%'});
						}
						else{
							$(this).find('.left-side').css({'width':half_window+'px'});
							$(this).find('.right-side').css({'width':half_window+'px'});
						}
					}
					this.setUpPromo = function(){
						var days = -2;
						if(this.countdown){
							days = parseInt($(this).attr("days"));
						}
						var i = 0;
						
						var found = -1;
						while (i<this.options.length&&found==-1){
							if(this.currentWidth>=this.options[i].width){
								found=i;
							}
							i++;
						}
						if(found==-1){
							found = this.options.length-1;
						}
						if(this.current!=found){
							var remove = false;
							var url = this.options[found].url;	
							//url = url.replace('~w~', this.options[found].width);							
							if(days>10){days=-1;}
							if(days>=1){
								url = url.replace('~x~', days);
							}
							else if(days==0){		 						
								if(this.current_time<1424743200){

								}
								else if(this.current_time<1424750400){
									days = 'airing';
								}
								else{
									$(this).css('display', 'none');
								}
							}
							else if(days==-1){
								remove=true;
							}
							if(!remove){
								this.addAllSizes(days);
								$(this).addClass('case'+found).removeClass('case'+this.current);
								this.current=found;
							}
						}
					}
					this.addAllSizes = function(x){			
						var html = '<div class="sizes large" style="background-image:url('+this.options[0].url.replace('~x~', x)+')"></div>';
						html += '<div class="sizes medium" style="background-image:url('+this.options[1].url.replace('~x~', x)+')"></div>';
						html += '<div class="sizes small" style="background-image:url('+this.options[2].url.replace('~x~', x)+')"></div>';
						$(this).find('.placeholder').append(html);
					}
					this.init();
				})
			};
			var special = {"helpers": {}};
			special.helpers.processCountdown = function(){
				$("[countdown=true]").countdown().each(function(){this.iterate()});
			}
			special.helpers.compareTimes=function(a,b)
			{
				var c=b-a;
				var time_apart = {"days":0, "hours":0, "minutes":0, "seconds":0}
				var days = 60*60*24;
				var hours = 60*60; 
				var minutes = 60;
				var seconds = 1;
				time_apart.days=Math.floor(c/days);
				c=c-(time_apart.days*days);
				time_apart.hours=Math.floor(c/hours);
				c=c-(time_apart.hours*hours);
				time_apart.minutes=Math.floor(c/minutes);
				c=c-(time_apart.minutes*minutes);
				time_apart.seconds=Math.floor(c/seconds);
				return time_apart;
			}
			$.fn.countdown = function(){
				return this.each(function(){
					this.init = function(){
						this.deadline = new Date();this.deadline.setFullYear(2015, 1, 23);this.deadline.setHours(0, 0, 0, 0);this.findState();
					}
					this.findState = function(){
						var today = new Date();
						today.setHours(0, 0, 0, 0);
						var state = '';
						var time_diff = this.deadline-today;
						var days = 0;
						if(time_diff>=0){
							days = Math.floor(time_diff/(1000*60*60*24));
							$(this).attr('days',days);
						}
						else {
							$(this).attr('days',-1);
						}
					}
				})
			}
				
		</script>
</div>
