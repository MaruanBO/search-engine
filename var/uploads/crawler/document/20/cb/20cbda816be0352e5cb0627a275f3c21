<!-- Sailthru Horizon --> 
<script type="text/javascript">
    (function() {
        function loadHorizon() { 
            var s = document.createElement('script'); 
            s.type = 'text/javascript'; 
            s.async = true;
            s.src = ('https:' === location.protocol?'https://d1gp8joe0evc8s.cloudfront.net':'http://cdn.sail-horizon.com')+'/horizon/v1.js';
            var x = document.getElementsByTagName('script')[0]; 
            x.parentNode.insertBefore(s, x);
        }
        loadHorizon();
        var oldOnLoad = window.onload;
        window.onload = function() {
            if (typeof oldOnLoad === 'function') {
                oldOnLoad();
            }
            Sailthru.setup({
                domain: 'horizon.bet.com'
            });
        };
    })();
</script>


<!-- Add White Ops tag -->
<script type="text/javascript">
    (function() {
        var id = 'mtvnservices-js';
        if (document.getElementById(id)) {
            return;
        }
        var location = document.URL;
        var matches = location.match(/cid=([^&]+)/i);
        var provider = "null";
        var section = "null";
        
        if (matches) provider = decodeURIComponent(matches[1]);
        if (s.channel) section = s.channel;

        var js = document.createElement('script');
        var jsSrc = "http://s.update.media.mtvnservices.com/2/255265/pagespeed.js?pp=[Provider_ID]&pc=[Section_ID]&dt=2552651421251501513000";
        js.id = id;
        js.async = true;
        jsSrc = jsSrc.replace("[Provider_ID]", provider);
        jsSrc = jsSrc.replace("[Section_ID]", section);
        js.src = jsSrc;
        
        var image = document.createElement('img');
        var imageSrc = "http://post.update.media.mtvnservices.com/2/255265/one.gif?pp=[Provider_ID]&pc=[Section_ID]&dt=2552651421251501513000";
        imageSrc = imageSrc.replace("[Provider_ID]", provider);
        imageSrc = imageSrc.replace("[Section_ID]", section);
        image.src = imageSrc;
        image.onerror = function(){this.parentNode.removeChild(this);};
        
        if (document.body != null){
            document.body.appendChild(js);
            document.body.appendChild(image);
        }
    }());
</script>

