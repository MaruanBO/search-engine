<!-- START Bet Experience Newsletter Subscriptions Popup -->
<script type="text/javascript">
        function displayNewsletters(selector){
            $('body').append('<div id="bq_betxnewsletterDiv"></div>');
            if(!selector){
                selector = "";
            } else {
                selector += ".";
            }
            $('#bq_betxnewsletterDiv').load("/home/_jcr_content.betx-newsletter-modal." + selector + "html",
                function() {
                    showNewslettersNoUl();
                });
        }

        function showNewslettersNoUl(){
        	if($('#bq_betxnewsletterDiv ul').length > 0){
                $('#bq_betxnewsletterDiv ul').hide();
            }
            $('#bq_betxnewsletterDiv').show();
        }
</script>
<!-- END Bet Experience Newsletter Subscriptions Popup -->









