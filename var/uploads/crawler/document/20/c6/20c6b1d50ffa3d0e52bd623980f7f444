<script type="text/javascript">
    (function() {
        /**
         * Select top-promo link from a randomized list.
         * These are presently being exempted from '/subscription/...' redirects,
         * and will be later handled via a currated group.
         */
        var image_div = $('.subscribe-now');
        var link_element = image_div.find("a");

        var choices = 4;
        var chooser = 1 + Math.floor(Math.random()*choices);
        switch (chooser) {
            case 1:
                image_div.css("background", "url(/static/front/images/header/left_corner1.png?v3)");
                link_element.attr("href", "https://w1.buysub.com/servlet/OrdersGateway?cds_mag_code=ATL&cds_page_id=172711&cds_response_key=I15BA2C");
                break;
            case 2:
                image_div.css("background", "url(/static/front/images/circ/big-save-subscribe.png)");
                link_element.attr("href", "https://w1.buysub.com/servlet/OrdersGateway?cds_mag_code=ATL&cds_page_id=171695&cds_response_key=I15BA1B");
                break;
            case 3:
                image_div.css("background", "url(/static/front/images/circ/2freeissue-violet.png)");
                link_element.attr("href", "https://w1.buysub.com/servlet/OrdersGateway?cds_mag_code=ATL&cds_page_id=172711&cds_response_key=I15BA2X");
                break;
            case 4:
                image_div.css("background", "url(/static/front/images/circ/savebig-turquoise.png)");
                link_element.attr("href", "https://w1.buysub.com/servlet/OrdersGateway?cds_mag_code=ATL&cds_page_id=171695&cds_response_key=I15BA1Z");
                break;
        }
    })();


</script>

