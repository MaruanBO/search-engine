    (function(){
        var sonobiSizes = {
            "300x250" : "5f2d6bce9df694672d0c",
            "300x600" : "4449390ce636c2bba09c",
            "728x90" : "0848bd941032b63fc7c7",
            "970x250" : "7d140ce0d19eb6068341"
        };
        var defs = Atlantic.Ad.manager.defs;
        var sizes;
        for (var key in defs){

            sizes = defs[key]['sizes'];
            for (var i = 0; i < sizes.length; i++) {
                formatted_size = sizes[i].join('x');
                var coded_size = sonobiSizes[formatted_size];
                if (coded_size !== undefined) {
                    sbi_morpheus.register(
                        key, coded_size);
                }
            };
        }

    }());
</script>

