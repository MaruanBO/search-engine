	<h2 class="title_arrow" id="find_tickets_header">Find Tickets</h2>
    <script type="text/javascript">
    $(document).ready(function(){
        //advanced search
        var advSearchObject = new SearchObject(
        {
            searchGoButton: $('#adv_search_submit'),
            useAdvSearch: true,
            trackingValue: 'advanced search',
            dateStartElement: $('#adv_search_start_date'),
            dateEndElement: $('#adv_search_end_date'),
            locationSelects:
            {
                stateSelectElement: $('#adv_search_state_select'),
                citySelectElement: $('#adv_search_city_select')
            },
            zipInputElement: $('#adv_search_zip'),
            categoryCheckboxes:
            {
                checkboxClass: 'adv_search_cat_check',
                allCheckboxID: 'adv_search_cat_check_all'
            }
        });

        $('#adv_search_start_date_btn').click({ aso: advSearchObject },
            function(event){
                event.data.aso.OpenStartDate();
            }
        );
        $('#adv_search_end_date_btn').click({ aso: advSearchObject },
            function(event){
                event.data.aso.OpenEndDate();
            }
        );
    });
</script>


