<script>
jQ(document).ready(function(){
 
	jQ(".basicSearch input")
		.bind("focus.labelFx", function(){
			jQ(this).prev().hide();
		})
		.bind("blur.labelFx", function(){
			jQ(this).prev()[!this.value ? "show" : "hide"]();
		})
		.trigger("blur.labelFx");
 
}); 
</script> 

