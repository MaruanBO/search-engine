jQ(document).ready(function(){
	jQ( ".toggleSearch" ).click(function() {
		jQ(".searchWrap").toggleClass("open");
		jQ(".searchBox").toggleClass("advSearchBox");
		jQ( ".advSearch" ).toggle( "slide", function() {
			if (jQ(".toggleSearch").text() == "advanced search [+]")
			jQ(".toggleSearch").text("basic search [-]")
			else
			jQ(".toggleSearch").text("advanced search [+]");
		});
	});
});
</script>

