<script>
$(document).ready(
    function ()
    {
    	$("#rotator_links a").hover(
            function ()
            {
            	// since the mouse is over the button, go ahead and select its image and capt
            	//          clear the current selected image
            	$('#rotator_links a.selected').removeClass('selected');
            	$('#home_page_img_rotator .rotator_img').removeClass('selected_img');
            	$(this).addClass('selected');
            	$('#home_page_img_rotator #' + $(this).attr('imgid')).addClass('selected_img');
            	// now select the corresponding image                
            }
        );
    }
);
</script>
