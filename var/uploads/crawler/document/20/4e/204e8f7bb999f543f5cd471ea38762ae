<script type="text/javascript">

var plainTNUrl = 'http://' + document.location.hostname;
var isAffiliatePage = false;
var catUrl = '';
$(document).ready(function () {

//business peeps want only parent categories or tn.com in general to get plus oned
//check the parent cat, if anything but sports, concerts, theater, just use plain TN url to plus one
switch(parentCat)
{
    case 'Concerts':
        catUrl = '/concert-tickets.aspx'
        break;
    case 'Sports':
        catUrl = '/sports-tickets.aspx'
        break;
    case 'Theater':
        catUrl = '/theater-tickets.aspx'
        break;
    default:
        break;
}

if(isAffiliatePage)
catUrl = '/affiliates/program.aspx';

var plusOneUrl = plainTNUrl + catUrl;



});
</script>


