function doSearch(){
var country = '';
var country2 = '';
            var strCountryURL = new String(document.location);
            var arrCountryURL = strCountryURL.split('/');
            if ((arrCountryURL[3] != 'homeusers') && (arrCountryURL[3] != 'enterprise') && (arrCountryURL[3] != 'partners') && (arrCountryURL[3] != 'search') && (arrCountryURL[3] != '') && (arrCountryURL[3].indexOf("default") < 0))
            {
                        country = '/'+arrCountryURL[3]+'/';
                country2 ='%2f'+arrCountryURL[3];
            }
            else
            {
                        country = '/';
                country2 = '%2f'
            }
            if(arguments.length == 0)
            {
                        if(document.getElementById('queryTerms').value!='')
                        {
                                   doSearch(document.getElementById('queryTerms').value);
                        }
            }else{
location.href='http://www.pandasecurity.com'+country+'search/?hl=en&lr=lang_en&nq=site%3awww.pandasecurity.com'+country2+'&term='+encodeURIComponent(arguments[0].replace(" ","+")); 
            }
}
</script>

 
