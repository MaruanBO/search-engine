<script type="text/javascript">
	function windowResize() {
		var width = $(window).width();

		if (width <= 581) {
			$("#hero_mobile").show();
			$("#hero_tablet").hide();
			$("#hero_large").hide();
		}


		if (width > 582){
			$("#hero_tablet").hide();
			$("#hero_large").show();
			$("#hero_mobile").hide();
		}
	}

	$(document).ready(function() {
		//Choose layout based on initial viewport size
		windowResize();

		//Choose layout after viewport resize
		$(window).resize(function() {
			 windowResize();
		});
	});
</script>

