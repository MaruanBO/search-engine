statetopop.innerHTML=usstatesform;
document.getElementById('sttodel').style.visibility="visible";

}else{

statetopop.innerHTML='';
document.getElementById('sttodel').style.visibility="hidden";


}

if(thiscountry != "US"){
//statetopop.innerHTML='';
//document.getElementById('sttodel').style.visibility="hidden";
}else{
//statetopop.innerHTML=usstatesform;

//document.getElementById('sttodel').style.visibility="visible";

}

if(thiscountry == "US" || thiscountry == "CA" || thiscountry == "PR" || thiscountry == "GU"){
