	function set_s_acct() {
		var domainName = document.domain.replace('www.','');
		var suite = "epsonglobalprod,epsonworldmapprod";	
		if (domainName.indexOf("dev") != -1)
			suite = "epsonglobaldev,epsonworldmapdev";
		else if (domainName.indexOf("qa") != -1)
			suite = "epsonglobalqa,epsonworldmapqa";
		return suite;
	}
