// 								PASDetermineAttribute("NULL","NULL");
// 						}			
// //console.info("final evar value: " + this.PasEvarValue);
	
// //console.info("PAS function sending evar!");
// 				PaSValuePassedToEvar = this.PasEvarValue;
		
// 			} else {
