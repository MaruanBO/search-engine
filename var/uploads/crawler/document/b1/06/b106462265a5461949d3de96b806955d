<script type="text/javascript">

	// Fix the browser iPad variances

	jQuery(function($) {
		var isOpera = !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;
		// Opera 8.0+ (UA detection to detect Blink/v8-powered Opera)
		var isFirefox = typeof InstallTrigger !== 'undefined';   // Firefox 1.0+
		var isSafari = Object.prototype.toString.call(window.HTMLElement).indexOf('Constructor') > 0;
		// At least Safari 3+: "[object HTMLElementConstructor]"
		var isChrome = !!window.chrome && !isOpera;              // Chrome 1+
		var isIE = /*@cc_on!@*/false || !!document.documentMode;   // At least IE6
		// var isiPad = navigator.userAgent.match(/iPad/i) !== null; // on iPad

		if (isFirefox) {
		    $(".hpMainImage").css("margin-top", "5px");
		    $('#HPBg3').css({"top":"498px", "height":"501px"});
		    $('#HPBg5').css({'top': '1497px','height': '500px'});
		    $('#HPBgNA').css({'top': '-3px','height': '503px'});
		    $('#HPBg8').css('height','504px');
		    $('#HPBg7').css({'top':'-1px','height':'180px'});
		   }
		});

		if (navigator.userAgent.match(/iPad/i) !== null) {
			$(".hpMainImage").css("margin-top", "-3px");
		}

	//Fix issue on iPad pixels off
	//create a meta tag and dynamically insert into head
	
	var meta = document.createElement('meta');
		meta.name = 'viewport';
		meta.content = "initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no, width=device-width";
		document.getElementsByTagName('head')[0].appendChild(meta);

	</script>

	<script type="text/javascript">
	jQuery(function($) {

//=============== Image Fade in Main 1 ==============//
var twoImageFade = function(){
	$('.fadeImage').css('opacity', 0);
	setTimeout(function(){
		$('.fadeImage').css('opacity', 1);
	},4500);
};

twoImageFade();

setInterval(function() {
	twoImageFade();
	},9000);

//============== Override logo ===============//

	//add other logos to fade in and out
	if($('#globalLogo').length) {
		$('#globalLogo a img').attr('src','/Asset_Archive/GPWeb/content/0009/420/313/assets/021115_all_HP_US_M3logo.png').addClass('gapLogoSwap gapLogo1');
		// $('#globalLogo a').prepend('<img class="gapLogo2 gapLogoSwap" src="/Asset_Archive/GPWeb/content/0009/420/313/assets/021115_all_HP_US_M2logo.png" alt="gap">');
		// $('#globalLogo a').prepend('<img class="gapLogo3 gapLogoSwap" src="/Asset_Archive/GPWeb/content/0009/420/313/assets/021115_all_HP_US_M3logo.png" alt="gap">');
	}

	if($('#mainNavHP').length && $('.hpcontent_2').length) {

		//Make logo fixed when it is 10px from top of browser
		$(window).scroll(function() {

			//get position of logo from top of window
			var main1Top = $('.hpcontent_2').offset().top;
			//check to see if promo in container 1 exists, if so add .hpcontent_1 for logo sticky
			if ($('.hpcontent_1').length) {
				main1Top = $('.hpcontent_1').offset().top; //.hpcontent_1 is reserved for promos
			}
			var windowTop = $(window).scrollTop();
			var logoFromWindow = (main1Top - windowTop);
				//console.info(logoFromWindow);
				// console.info("scroll from top " +windowTop);

			var gapLogo1 = $('#globalLogo a .gapLogo1');
			// var gapLogo2 = $('#globalLogo a .gapLogo2');
			// var gapLogo3 = $('#globalLogo a .gapLogo3');
			// var gapLogos = $('.gapLogoSwap');

			//fix logo when it is 10px from top
			if(logoFromWindow > 10) {
				$('#mainNavHP').css({'position':'absolute','top':'0'});
			} else {
				$('#mainNavHP').css({'position':'fixed','top':'10px'});
			}


			//get position of bottom of logo
			var bottomOfLogo = (windowTop + 152 + 10);

			//position of logo from top
			var logoToTop = $('#mainNavHP').offset().top;
			
			//get position of footer from bottom of logo
			var footerToTop = $('#footer_BrandContainer').offset().top;
			var footerToLogo = (footerToTop - bottomOfLogo);
				// console.info('footer to logo < ' +footerToLogo -1);
				// console.info('set top position to this: ' +(logoToTop - 332)+ 'px');
		
