<script>

function adjustHPHPBg1Width(BDelementsArray) {
// get the width of the browser window viewport
var BDviewport = document.viewport.getDimensions();
var BDwidth = BDviewport.width;
var BDelementsArrayLength = BDelementsArray.length;


	if (BDwidth >=1000) {

// loop through the different elements
// and set their width to the viewport width
// then we offset the left position
		for(var i=0; i < BDelementsArrayLength; i++) {
			$(BDelementsArray[i]).setStyle({
				width: BDwidth + "px",
				/*height: '900px',*/ /*(width*1.5) + "px",*/
				// position: "relative",
				marginLeft: - (((BDwidth - 988)/2) + 161 ) + "px",
				overflow: "hidden"
			});
		}
	}
}

//span the full width of the window
var elementsToAdjustBannerWidthArrayBD = [/*"HPBg2","HPBg3","HPBg4","HPBg5"*/];


Event.observe(window, 'resize', function() {
	adjustHPHPBg1Width(elementsToAdjustBannerWidthArrayBD);
});

jQuery(document).ready(function($) {

	//check to see if first container with promo exists, if so append to elementsToAdjustBannerWidthArrayBD array
	// if (jQuery('#HPBg1').length) {
	// 	// console.info('promo exists');
	// 	elementsToAdjustBannerWidthArrayBD.push('HPBg1');
	// }
	// //check for New Arrivals bar, append background HPBgNA to array above
	// if(jQuery('#HPBgNA').length) {
	// 	elementsToAdjustBannerWidthArrayBD.push('HPBgNA');
	// }

	// for (var i = 0; i < elementsToAdjustBannerWidthArrayBD.length; i++) {
	//     console.log(elementsToAdjustBannerWidthArrayBD[i]);
	// }
	var elementsToAdjustBannerWidthArrayBD = $('.stretchedBackgrounds');

	adjustHPHPBg1Width(elementsToAdjustBannerWidthArrayBD);
});


// Fix the issue for Big Image doesn't work for IOS
var isMobile = ( navigator.userAgent.match(/(iPad|iPhone|iPod)/g)||navigator.userAgent.match(/Android/i) ? true : false );
if( isMobile ) {
		jQuery(document).ready(function($) {
		jQuery('body').addClass('isIOSDevice');
	});
}

