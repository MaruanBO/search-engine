<!-- begin CNOV-109: Order in Store Self Serve / look for css above -->
<script type="text/javascript">
jQuery(function($) {
    var storeId;
    var kioskBrandPath = 'http://oldnavy.gap.com';
    var BASE_PATH = '/Asset_Archive/ONWeb/content/ois/images/';
    
    var scanDivTemplate = '<div id="onKioskDiv">' +
							'<div id="scanDiv">' +
								'<div id="roboDiv">' +
									'<img src="' + kioskBrandPath + '/Asset_Archive/ONWeb/content/ois/images/Robot.png" />' +
								'</div>' +
								'<div id="contentDiv">' +
									'<div id="cloudDiv">' +
										'<img src="' + kioskBrandPath + '/Asset_Archive/ONWeb/content/ois/images/Cloud.png" />' +
									'</div>' +
									'<div id="optionsDiv">' +
										'<img src="' + kioskBrandPath + '/Asset_Archive/ONWeb/content/ois/images/Steps.png" />' +
									'</div>' +
									'<div id="divider" />' +
										'<div id="scanButtonDiv">' + 
											'Scanner Not Working?' +
											'<div><input type="text" id="scanInput" placeholder="Enter Barcode Number"/></div>' +
										'</div>' +
									'</div>' +
							'</div>' +
							'<div id="bottomBarDiv">' +
								'<button id="browseProducts" class="toBrowse clickable">Browse Online</button>' +
							'</div>' +
						'</div>';

    var bindCarriageReturn = function() {
      $('#scanInput').bind('keypress', function(key){
        var carriageReturn = 13;
        if(key.keyCode == carriageReturn) {
          var upc = $('#scanInput').val();
          onScan(upc);
        }
      });
    };
          
    var autoResetFocus = function() {
      $('#scanbar, #scanDiv').click(function(event) {
        if ($.inArray(event.target, $('.clickable')) === -1) {
          $('#scanInput').focus();
        }
      });
    };
  
    var getCookieDomain = function() {
      var hostname = window.location.host;
      var splitArray = hostname.split('.');
      splitArray[0] = '';
      var domainName = splitArray.join('.');
      return domainName;
    }
    
    var onBrowse = function() {
      window.top.location.replace('/?tid=onvu000009');
    };
	
	var onScanButton = function() {
      if ($('#scanDiv').length == 0) {
        $('#universalBar').before(scanDivTemplate);
        bindCarriageReturn();
        $('body').css('overflow', 'hidden');
      }

      $('#scanInput').focus();
      autoResetFocus();
    };
    
    var searchStringMatches = window.location.search.match(/[?&]kioskAtStoreId=(\d+)/);
    if (searchStringMatches) {
      storeId = searchStringMatches[1];
      gidLib.setCookieVar("mktGlobalSession", "kioskStoreId", storeId);
      onScanButton();
    } else {
      storeId = gidLib.getCookieVar("mktGlobalSession", "kioskStoreId");
    }
    
    var postalCodeMatches = window.location.search.match(/[?&]pcode=(\d+)/);
    if (postalCodeMatches) {
      gidLib.setCookieVar("globalSession", "pcode", postalCodeMatches[1]);
    }
  
    if ( $('div#universalBar').length  && storeId ) {
      var onScan = function(upc) {
        var pidPromise = lookupStyle(upc);

        pidPromise.then(function(styleId) {
          gidLib.setCookieVar('globalSession', 'openFIS', 'true');
          window.top.location.replace(kioskBrandPath + '/browse/search.do?tid=onvu000009&searchText=' + styleId);
        });
      };
            
      var showError = function($field, message) {
        $field.after('<div class="errorBubble">' + message + '</div>');
      };
      
      var hideError = function($field) {
        $field.parent().find('#scanDiv .errorBubble').remove();
      };

      var lookupStyle = function(upc) {
        var callPromise = $.ajax('/resources/upcLookup/v1/lookup?upcCode=' + upc + '&storeId=' + storeId);

        var styleNumberDeferred = $.Deferred();
        
        callPromise.then(function(response) {
          styleNumberDeferred.resolve(response.styleId);
          hideError($('#scanInput'));
        }).fail(function(response, textStatus, errorThrown) {
          console.log('Something went wrong. Response: ' + response.responseText + ' Status: ' + textStatus + ' Error: ' + errorThrown);
          showError($('#scanInput'), 'Sorry. The item could not be found on OldNavy.com.');
          $('#scanInput').val('');
        });

        return styleNumberDeferred;
      };
      
      var scanAnywhere = function() {
        var keyPresses = [];
        $(document).bind('keypress', function(key) {
          if (keyPresses.length > 14) {
            keyPresses.shift();
          }
          
          if (!$(':focus').is('input')) {
            keyPresses.push(String.fromCharCode(key.which));
          }
        });
      
        $(document).bind('keypress', function(key){
          if (!$(':focus').is('input')) {
            var functionKey4 = 115;
            if(key.keyCode == functionKey4) {
              var barCodeMatches = keyPresses.join('').match(/(\d{12})\r/);
              if (barCodeMatches) {
                onScan(barCodeMatches[1]);
              }
            }
          }
        });
      };
      
      var popupFindInStoreOnProductPage = function() {
        var openFindInStore = gidLib.getCookieVar('globalSession', 'openFIS');
        gidLib.removeCookieVar('globalSession', 'openFIS');
        if (window.location.pathname.endsWith('product.do') && openFindInStore === 'true') {
          Event.observe(document, "productPage:ready", function() {
            if (merchLocationsManager.controller.isMerchLocatorActive()) {
              merchLocationsManager.view.storeLocationsListOverlay.open();
            }
          });
        }
      };
      
      $('button.toBrowse').click(onBrowse);
      autoResetFocus();
      scanAnywhere();
      popupFindInStoreOnProductPage();
    }
  });
</script>
