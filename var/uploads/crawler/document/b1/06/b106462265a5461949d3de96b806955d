    return contentItemLink(this,'',dataToPush.tid);
  });

    // traverse through the object
  jQuery.each(dataToPush, function(key, value) {
    if(typeof value !== 'object') return; // check to make sure we got an object before moving forward

    // check if element has the class to know which object we need to pull data from
    if(this.hasClass.length > 0) {
      dataToPush = this;
    }
  });

  jQuery('.new-arrivals-outfits').outfitPuller({
      categoryId: dataToPush.CID,
      facetStyleId: dataToPush.facetID,
