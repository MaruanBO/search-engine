<script type="text/javascript">

jQuery(window).on("PaSAttribute:ready", function() { 

  //console.info("PaS EVENT FIRED ******************** ->"); 
  // console.info("PaSValueForContent: " + PaSValueForContent); //Div Pref (A-H)
  // console.info("PaSValueForContent2: " + PaSValueForContent2);  //Segment:  Control, Test1, Test2 (A-C)

  var CID;
  var facetID;
  var tid='';

  var cases = {
    'A':{
      //Women
      CID:8792,
      facetID:94954,
      url:'/browse/category.do?cid=8792',
      tid:'HP_NewArrivals_W'
    },
     'B':{
      //Mens
        CID:11900,
        facetID:92113,
        url:'/browse/category.do?cid=11900',
        tid:'HP_NewArrivals_M'
    },
    'C':{
      //baby
      CID:1027204,
      facetID:1027717,
