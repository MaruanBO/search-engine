// 				PaSValuePassedToEvar = this.PasEvarValue;
// 				PASDetermineAttribute("NULL","NULL");
// 			}
// 		}
		

// 		var PASDetermineAttribute = function (idtype,value) {
// //console.info("--PASDetermineAttribute() called--");
