      url:'/browse/category.do?cid=14249',
      tid:'HP_NewArrivals_MAT'
    }
    
  };

  // We will get the PaSValueForContent

  // if (PaSValueForContent2 == "A") {

  //   var setDefaultContent = "Control";
  //   var dataToPush = cases[setDefaultContent];

  // } else {

    var dataToPush = cases[PaSValueForContent];

  // }
  // element we will apply new href and click event to
