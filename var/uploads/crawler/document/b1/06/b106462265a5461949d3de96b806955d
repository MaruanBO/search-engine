		$c.carouFredSel({
			align: false,
			items: 2,
			scroll: {
				items: 1,
				duration: 5000,
				timeoutDuration: 0,
				easing: 'linear',
				pauseOnHover: 'immediate',
			}
		});
	 
		
		$w.bind('resize.example', function() {
			var nw = $w.width();
			if (nw < 990) {
				nw = 990;
			}
	 
			$c.width(nw * 3);
			$c.parent().width(nw);
	 
		}).trigger('resize.example');
 
	});
});		
</script>


