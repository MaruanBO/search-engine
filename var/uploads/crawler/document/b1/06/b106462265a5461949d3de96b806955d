      tid:'HP_NewArrivals_Baby'
    },
    'D':{
      //TG
      CID:63863,
      facetID:97706,
      url:'/browse/category.do?cid=63863',
      tid:'HP_NewArrivals_TG'
    },
    'E':{
      //TB
      CID:1016138,
      facetID:1016139,
      url:'/browse/category.do?cid=1016138',
      tid:'HP_NewArrivals_TB'
