<script>
function stopRKey(evt) { 
  var evt = (evt) ? evt : ((event) ? event : null); 
  var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null); 
  if ((evt.keyCode == 13) && (node.type=="text"))  {return false;} 
} 
function MvalidateGearSearch() {
        var match = -1;
        var s = document.getElementById('dnn_GSearch_gearsearchtext').value;
        //s = s.replace(/[^a-zA-Z 0-9 + "]+/g, '');
        // alert(s);
        if (s == '') {
            alert('Please type search keyword before hitting search button');
            return false;
        }
        else {
			s = s.split('"').join("_");
			if ((s.indexOf('_', 0) > -1) && (s.indexOf('_', ((s.length) - 1)) > 0)) {
				match = 1;
			}
			else {
				match = 0;
			}
			s = s.replace(/[^a-zA-Z 0-9 +]+/g, '');
			window.location = '/gearsearch/' + s.toLowerCase() + '/-1/-1/';
			return false;
		}
    }
</script>

<div class="gearsearch clearfix">
    <div class="header pull-left">SEARCH GEAR</div>
    <div class="form clearfix pull-left">
        
        <input name="dnn$GSearch$gearsearchtext" type="text" id="dnn_GSearch_gearsearchtext" class="border-box" onkeypress="return stopRKey();" />  
	&nbsp;		
        <input type="submit" id="msearch-button" value="" onclick="return MvalidateGearSearch();" />	
		
    </div>
	   
</div>

