<script type="text/javascript">
    function validateSearch() {
        var match = -1;
        var s = document.getElementById('searchtext').value;
        //s = s.replace(/[^a-zA-Z 0-9 + "]+/g, '');
       // alert(s);
        if (s == '') {
            alert('Please type search keyword before hitting search button');
            return false;
        }
		else {
			s = s.split('"').join("_");
			if ((s.indexOf('_', 0) > -1) && (s.indexOf('_', ((s.length) - 1)) > 0)) {
				match = 1;
			}
			else {
				match = 0;
			}
			s = s.replace(/[^a-zA-Z 0-9 +]+/g, '');
			window.location = '/search/' + s + '/match/' + match;
			return false;
		}
    }

    function MvalidateSearch() {
        var match = -1;
        var s = document.getElementById('msearchtext').value;
        //s = s.replace(/[^a-zA-Z 0-9 + "]+/g, '');
        // alert(s);
        if (s == '') {
            alert('Please type search keyword before hitting search button');
            return false;
        }
        else {
			s = s.split('"').join("_");
			if ((s.indexOf('_', 0) > -1) && (s.indexOf('_', ((s.length) - 1)) > 0)) {
				match = 1;
			}
			else {
				match = 0;
			}
			s = s.replace(/[^a-zA-Z 0-9 +]+/g, '');
			window.location = '/search/' + s + '/match/' + match;
			return false;
		}
    }
	
	

    function searchKeyPress(e) {
        // look for window.event in case event isn't passed in
        if (window.event) { e = window.event; }
        if (e.keyCode == 13) {
            validateSearch();
        }
    }

    function Search(e) {
        var keynum
        var keychar
        var numcheck

        if (window.event) // IE
        {
            keynum = e.keyCode
        }
        else if (e.which) // Netscape/Firefox/Opera
        {
            keynum = e.which
        }
        var s = document.getElementById('searchtext').value;
        if (keynum == 13) {
            if (s == '') {
                alert('Please type search keyword before hitting search button');
                return false;
            }
            else {
                s = s.split('"').join("_");
                if ((s.indexOf('_', 0) > -1) && (s.indexOf('_', ((s.length) - 1)) > 0)) {
                    match = 1;
                }
                else {
                    match = 0;
                }
                s = s.replace(/[^a-zA-Z 0-9 +]+/g, '');
                //alert(s);
                window.location = '/search/' + s + '/match/' + match;
                return true;
            }
        }
    }

    function MSearch(e) {
        var keynum
        var keychar
        var numcheck

        if (window.event) // IE
        {
            keynum = e.keyCode
        }
        else if (e.which) // Netscape/Firefox/Opera
        {
            keynum = e.which
        }
        var s = document.getElementById('msearchtext').value;
        if (keynum == 13) {
			if (s == '') {
				alert('Please type search keyword before hitting search button');
				return false;
			}
			else {
				s = s.split('"').join("_");
				if ((s.indexOf('_', 0) > -1) && (s.indexOf('_', ((s.length) - 1)) > 0)) {
					match = 1;
				}
				else {
					match = 0;
				}
				s = s.replace(/[^a-zA-Z 0-9 +]+/g, '');
				window.location = '/search/' + s + '/match/' + match;
				return false;
			}
		}
    }

</script>
						
                 
