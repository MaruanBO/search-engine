        $('#Lessonselector').change(function () {
            var cat = $('#Lessonselector option:selected').text().toLowerCase().trim().replace(/["~!@#$%^&*\(\)_+=`{}\[\]\|\\:;'<>,.\/?"\- \t\r\n]+/g, '-');
