       $(document).ready(function () {

        $('a#toggle-menu').mouseover(function (e) {
            e.preventDefault();
            $('#test').slideToggle(200);
   
        });

        $(document).mouseover(function (e) {
            if ($(e.target).closest('#test').length > 0 || $(e.target).closest('#toggle-menu').length > 0) return;
