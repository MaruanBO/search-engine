<section id="block-views-ti-amg-tl-home-page-lists-block" class="block block-views clearfix">

        <h2 class="block-title">Home Page Lists</h2>
    
  <div class="view view-ti-amg-tl-home-page-lists view-id-ti_amg_tl_home_page_lists view-display-id-block home-page-lists-view view-dom-id-43b5545cc96b93f8596c4ccb6ef2ddb4">
            <div class="view-header">
       <h2 class="black-box">Lists</h2>
     </div>
  
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
      
