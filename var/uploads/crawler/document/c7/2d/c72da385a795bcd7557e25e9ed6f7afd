  var getCookie = function(name) { 
    var value = "; " + document.cookie; 
    var parts = value.split("; " + name + "="); 
    if (parts.length == 2) { return parts.pop().split(";").shift(); } 
  }; 
  var value = getCookie("paywall-gtm-variables"); 
  var returnObj; 
  if (value) { try { value = decodeURIComponent(value); var obj = JSON.parse(value); if (typeof obj === "object") { returnObj = obj; } else if (typeof obj === "string") { returnObj = JSON.parse(obj); } } catch(err) {} } 
  if (typeof returnObj !== "object") { returnObj = {}; } 
  if (returnObj.LoggedIn === undefined) { returnObj.LoggedIn = false } 
  if (!returnObj.SubscriberID) { returnObj.SubscriberID = '' } 
