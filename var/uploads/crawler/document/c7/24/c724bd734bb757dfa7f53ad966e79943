	$('#invite1').ketchup().submit(function() {
		if ($(this).ketchup('isValid')) {
			var action = $(this).attr('action');
			$.ajax({
				url: action,
				type: 'POST',
				data: {
					email: $('#MCE-email1').val()
				},
				success: function(data){
					$('#result1').html(data).css('color', 'white', 'padding', '10px 0px 0px 0px');
				},
				error: function() {
					$('#result1').html('Sorry, an error occurred.').css('color', 'red');
