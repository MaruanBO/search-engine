<script type="text/javascript">
var api;
$(function() {
	// STEPS NAVIGATION
	$("#rates-steps a").click(function() {
        return false;
    });
	$("ul#rates-steps").tabs("#rates-sections > .rates-section");
	
	api = $("ul#rates-steps").data("tabs");
	$("#rates-next1").click(function() {
        if($("#amrequestform").valid()){$("#rates-section1").hide(); $("#rates-section2").show();}
    });
    $("#rates-prev2").click(function() {
        $("#rates-section1").show(); $("#rates-section2").hide();
    });
    $("#rates-next2").click(function() {
        if($("#amrequestform").valid()){api.next(); $("#rates-section2").hide(); $("#rates-section3").show();}
    });
    
    $("#rates-next3").click(function() {
        var mdob = $('#dobMo option:selected').val();
        var ddob = $('#dobDay option:selected').val();
        var ydob = $('#dobYr option:selected').val();
        document.getElementById('BirthDate').value = mdob+"/"+ddob+"/"+ydob;     
        
               
        if($("#amrequestform").valid()){$("#rates-section4").show(); $("#rates-section3").hide();}
    });
    $("#rates-prev3").click(function() {
        api.prev(); $("#rates-section1").hide(); $("#rates-section2").show(); $("#rates-section3").hide();
    });
    $("#rates-prev4").click(function() {
        $("#rates-section4").hide(); $("#rates-section3").show();
    });
	
});
</script>
<script type="text/javascript">
//<![CDATA[
        $(function() {
                
                jQuery.validator.addMethod("lettersonly", function(value, element) {
                return this.optional(element) || /^[a-z]+$/i.test(value);
                }, "Letters only please");
                
                jQuery.validator.addMethod("digitonly", function(value, element) {
                return this.optional(element) || /^[0-9]+$/i.test(value);
                }, "Digit only please");  
                
                jQuery.validator.addMethod("postalcode", function(postalcode, element) {
                        return this.optional(element) || postalcode.match(/(^\d{5}(-\d{4})?$)|(^[ABCEGHJKLMNPRSTVXYabceghjklmnpstvxy]{1}\d{1}[A-Za-z]{1} ?\d{1}[A-Za-z]{1}\d{1})$/);
                }, "Please specify a valid zip code");
                
                jQuery.validator.addMethod("phoneUS", function(phone_number, element) {
                        phone_number = phone_number.replace(/\s+/g, ""); 
                        return this.optional(element) || phone_number.length > 9 &&
                                phone_number.match(/^(1-?)?(\([2-9]\d{2}\)|[2-9]\d{2})-?[2-9]\d{2}-?\d{4}$/);
                }, "Please specify a valid phone number");
                
                jQuery.validator.addMethod("checkemail", function(value, element) {
                return this.optional(element) || /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(value);
                }, "Email not valid");
				
                
                var options = {
                        datatype: 'script',
                        success: function(){
                                                $('div#rates-respond').append('Thank You!');
                                                $('div#rates-respond').fadeIn().delay(2000).fadeOut();
                                        },
                        error: function(){
                                                $('div#rates-respond').append('Something goes wrong...');
                                                $('div#rates-respond').fadeIn().delay(2000).fadeOut();
                                        }
                        }; 

                $('#amrequestform').validate({
                        submitHandler: function(form) {
                                $(form).ajaxSubmit(options);
                        },
                        focusInvalid: false,
                                focusCleanup: true,
                                rules: {
                              
										Gender: 'required',
										dobMo: 'required',                                        
										VehicleYear: {
											required: true
										},                   
										VehicleMake: {
											required: true
										},
										VehicleModel: {
											required: true
										}, 
										DailyMileage: {
                                            required: true,
                                            digitonly : true
                                        },
                                        DateOfBirth: {
                                                required: true
                                        },             
										DesiredCoverage: {
											required: true
										}, 
										State: {
											required: true
										},
										LevelOfEducation: {
											required: true
										},
										Occupation: {
											required: true
										},
										CreditRating: {
											required: true
										},
										LicenseStatus: {
											required: true
										},
										LicenseState: {
											required: true
										},
                                        FirstName: {
                                                required: true,
                                                minlength: 1,
                                                maxlength: 14
                                               
                                        },
                                        LastName: {
                                                required: true,
                                                minlength: 1,
                                                maxlength: 14
                                                
                                        },
                                        Address: {
                                                required: true,    
                                                minlength: 3,
                                                maxlength: 50,
                                        },
                                        Zip: {
                                                required: true,
                                                postalcode: true
                                        },
                                        City: {
                                                required: true,
                                                minlength: 3,
                                                maxlength: 14
                                        },
                                        LicenseAge:
                                        {
                                           required: true,
                                           digitonly : true 
                                        },
                                        phone1:
                                        {
                                           required: true,     
