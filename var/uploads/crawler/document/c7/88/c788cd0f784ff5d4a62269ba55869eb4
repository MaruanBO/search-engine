<script language="JavaScript">
//<!--Hide the script from old browsers-->
// var ip = new java.net.InetAddress.getLocalHost();
// var ipStr = new java.lang.String(ip);
// <!-- echo var="REMOTE_ADDR"-->
function ValidateWebAddress(url) {
//console.log('url is : ' + url);
if(url == null) {
url = "";
}
if(url.indexOf("DIRECT")>-1){
urlLength = url.length;
url = url.substr(7,urlLength);
url = url.trim();
//console.log("get actual url: " + url.substr(7,88));
}
url = url.replace(/[\[\]']+/g,'');
var webSiteUrlExp = /^(([\w]+:)?\/\/)?(([\d\w]|%[a-fA-F\d]{2,2})+(:([\d\w]|%[a-fA-F\d]{2,2})+)?@)?([\d\w][-\d\w]{0,253}[\d\w]\.)+[\w]{2,4}(:[\d]+)?(\/([-+_~.\d\w]|%[a-fA-F\d]{2,2})*)*(\?(&?([-+_~.\d\w]|%[a-fA-F\d]{2,2})=?)*)?(#([-+_~.\d\w]|%[a-fA-f\d]{2,2})*)?$/;
if (webSiteUrlExp.test(url)) {
return true;
}
else {
return false;
}
}
function isValidDate(d) {
if ( Object.prototype.toString.call(d) !== "[object Date]" )
return false;
return !isNaN(d.getTime());
}
function invokeForm(path) {
var ipStr = '';
var from = '';
var FullRef = getCookieValue('FullRef');
var ClickDate = getCookieValue('ClickDate');
var LandingURL = getCookieValue('LandingURL');
var ClickDate1 = new Date(ClickDate);
if(ValidateWebAddress(FullRef)){
FullRef = FullRef;
}else {
FullRef = "";
}
if(ValidateWebAddress(LandingURL)){
LandingURL = LandingURL;
}else {
LandingURL = "";
}
if(isValidDate(ClickDate1)){
ClickDate = ClickDate;
} else {
ClickDate = "";
}
var cookieVars = {"ipaddress":ipStr,"referrer":from,"adReferrer":FullRef,"adClick":ClickDate,"adLandingURL":LandingURL};
post_to_url(path, cookieVars, "post");
}
function post_to_url(path, params, method) {
method = method || "post"; // Set method to post by default if not specified.
// The rest of this code assumes you are not using a library.
// It can be made less wordy if you use one.
var form = document.createElement("form");
form.setAttribute("method", method);
form.setAttribute("action", path);
for(var key in params) {
if(params.hasOwnProperty(key)) {
var hiddenField = document.createElement("input");
hiddenField.setAttribute("type", "hidden");
hiddenField.setAttribute("name", key);
hiddenField.setAttribute("value", params[key]);
form.appendChild(hiddenField);
}
}
document.body.appendChild(form);
form.submit();
}
// End -->
</script>


