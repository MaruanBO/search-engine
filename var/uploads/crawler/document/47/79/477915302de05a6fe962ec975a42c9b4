function connatix_video_callback(args)
{
var connatix_interval = setInterval(function(){
if (typeof(ga) != 'undefined')
{
var message = '';
switch (args.eventCode)
{
	case 2:
		message = 'Video Data Loaded';
		break;
case 4:
		message = 'Video Played';
		break;
case 1:
		message = 'Video Ready';
		break;
case 8:
		message = 'Video Paused';
		break;
}
ga('send', 'event', 'connatix_video_' + args.videoId, message);
console.log('event sent to GA: ' + message);
clearInterval(connatix_interval);
}
},100);
}
