      $('.area.clients .slide-area').mouseover(function() {
        var gallery = $(this).data('gallery');
        if (gallery) {
          gallery.stop();
        }
        return true;
      });
      $('.area.clients .slide-area').mouseout(function() {
        var gallery = $(this).data('gallery');
        if (gallery) {
          gallery.play();
