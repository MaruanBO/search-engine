<script type="text/javascript" src="/Scripts/eg/jquery.cycle.all.js"></script>
<script type="text/javascript" src="/Scripts/eg/jquery.simplyscroll.js"></script>
<script type="text/javascript">

    $$$('#tablet_wrapper ul').cycle({
        fx: 'scrollLeft',
        speed: 650,
        timeout: 8000,
        before: function (i) {
            $$$('.activeslide').removeClass('activeslide');
            $$$(this).addClass('activeslide');
            $$$('#tablet_progress').css('background-position', ($$$(this).attr('_pos')*30 + 4) + 'px 0px');
        }
    });

    $$$('#tablet_progress div').click(function () {
        var newpos = $$$(this).attr('_pos');
        var curpos = $$$('.activeslide').attr('_pos');
        var tomove = newpos - curpos;

        if (tomove < 0) {
            while (tomove++)
                $$$('#tablet_wrapper ul').cycle('prev');
        } else {
            while (tomove--)
                $$$('#tablet_wrapper ul').cycle('next');
        }

        if ($$$('#splashvid').hasClass('splashvideo')) {
            $$$('#tablet_wrapper ul').cycle('resume');
            $$$('#splashvid').removeClass('splashvideo').attr('src', '');
            $$$('#vidslide img').show();
            $$$('#vidslide').css('background', 'none');
            $$$('#tablet_click').css('pointer-events', 'auto');
        }
    });

    function tabletCenter() {
        if ($$$(window).width() > 750 && $$$(window).width() < 1078) //767-1095
            $$$('#banner #tablet_wrapper').css('left', ($$$(window).width() - 1095) / 2);
        else
            $$$('#banner #tablet_wrapper').css('left', 'auto');
    }

    var brands = ["aero|aeropostale", "ameagle|american-eagle-outfitters", "banana|banana-republic", "barnes|barnes-and-noble-booksellers", "bathbody|bath-and-body-works", "chilis|chilis", "cvs|cvs-pharmacy", "dominos|dominos-pizza", "fandango|fandango", "footlocker|foot-locker", "homedepot|the-home-depot", "hottopic|hot-topic", "jcpenney|jcpenney", "landrys|landrys", "lowes|lowes", "oldnavy|old-navy", "overstock|overstock", "regal|regal-entertainment-group", "rei|rei", "sportsauthority|sports-authority", "tgifridays|tgi-fridays", "zappos|zappos"];
    $$$.each(brands, function () {
        $$$('#brands ul').append('<li><a href="/giftcards/' + this.split("|")[1] + '-gift-card"><img class="brand_gray" src="/Images/themes/ggc/brands/' + this.split("|")[0] + '_gray.png" /><img class="brand_gray" src="/Images/themes/ggc/brands/' + this.split("|")[0] + '_gray.png" /></a></li>');
    });

    $$$(window).load(function () {
        $$$('#brands ul li .brand_color').each(function () {
            var imgwidth = $$$(this).width();
            if (imgwidth < 100)
                $$$(this).closest('li').width(imgwidth);
        });

        $$$('#brands ul').simplyScroll({
            frameRate: 60,
            speed: 1
        });

        $$$('#brands ul').animate({ 'opacity': 1 }, 1000);

        tabletCenter();
    });

    $$$(window).resize(tabletCenter);

    $$$('.loginclick').click(function () {
        if($$$(this).hasClass('splashbottombut')) {
            _kmq.push(['record', 'splash bottom login clicked', { 'km_note': 'User clicked splash page bottom login button.'}]);
        } else if($$$(this).hasClass('splashlogin')) {
            _kmq.push(['record', 'splash header login clicked', { 'km_note': 'User clicked splash page header login button.'}]);
        } else if ($$$(this).hasClass('getstartedtop')) {
            _kmq.push(['record', 'splash get started button clicked', { 'km_note': 'User clicked splash page get started button.' }]);
        } else if ($$$(this).hasClass('inlinelink')) {
            _kmq.push(['record', 'splash inline link login clicked', { 'km_note': 'User clicked inline get started login link.' }]);
        }
    
        $$$('.loginbox').lightbox_me({
            centered: true
        });
    
    });

    $$$('.forretailers').click(function () {
        _kmq.push(['record', 'splash For Retailers button clicked', { 'km_note': 'User clicked For Retailers button on splash.' }]);
        $$$('.splashheaderswitch.active').removeClass('active');
        $$$(this).addClass('active');
        window.location = 'http://corporate.egifter.com/';
    });

    $$$('.splashlearnhow, .splashlearnhow_link').click(function () {
        if ($$$(this).hasClass('learnpts')) {
            top.location.href = '/points/';
        } else {
            
            $$$('.learnhow_lb').lightbox_me({
                centered: true,
                onLoad: function () {
                    $$$('#learnhow_vid').html('<iframe src="//player.vimeo.com/video/104512591" width="500" height="281" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>');
                },
                onClose: function () {
                    $$$('#learnhow_vid').html('');
                }
            });
            
        }
    });

    $$$(document).on('click', '#tablet_click', function () {
        if ($$$('#vidslide').hasClass('activeslide')) {
            if (!$$$('#splashvid').hasClass('splashvideo')) {
                $$$('#tablet_wrapper ul').cycle('pause');
                $$$('#tablet_click').css('pointer-events', 'none');
                $$$('#vidslide').css('background', '#000');
                $$$('#vidslide img').hide();
                $$$('#splashvid').addClass('splashvideo').attr('src', '//www.youtube.com/embed/sDn9ImA2qrk?autoplay=1&amp;showinfo=0&amp;rel=0&amp;autohide=1&amp;vq=large&amp;wmode=opaque&amp;enablejsapi=1');
                new YT.Player('splashvid', {
                    events: {
                        'onStateChange': onPlayerStateChange
                    }
                });
            }
        } else if ($$$('#bitcoin').hasClass('activeslide')) {
            window.location = '/bitcoin/';
        } else if ($$$('#points').hasClass('activeslide')) {
            window.location = '/points/';
        } else {
            _kmq.push(['record', 'tablet entrance clicked', { 'km_note': 'User clicked splash page tablet to enter.'}]);
        
            $$$('.loginbox').lightbox_me({
                centered: true
            });
           
        }
    });

    function onPlayerStateChange(event) {
        switch (event.data) {
            case YT.PlayerState.ENDED:
                $$$('#tablet_wrapper ul').cycle('resume');
                $$$('#splashvid').removeClass('splashvideo').attr('src', '');
                $$$('#vidslide img').show();
                $$$('#vidslide').css('background', 'none');
                $$$('#tablet_click').css('pointer-events', 'auto');
                break;              
        }
    }

    $$$(window).scroll(function () {
        var scroll = $$$(window).scrollTop();
        if (scroll == 0)
            $$$('#splashheader').addClass('ontop');
        else
            $$$('#splashheader').removeClass('ontop');
    });

    //if ($$$.browser.chrome && screen.height <= 800)
    //    $$$('#banner').css({ 'zoom': '0.8' });

