        function eg_register() {
            _kmq.push(['record', 'Button in nonsocial login popup clicked', { 'Button': 'Register' }]);
            
            if (!$$$('#recaptcha_response_field').hasClass('.validate[required]'))
                $$$('#recaptcha_response_field').addClass('.validate[required]');
            
            if ($$$('#register_fieldset').validationEngine('validate')) {
                $$$.ajax({
                    type: "POST",
                    url: '/blocks/AJAXSelfAuth.aspx',
                    data: extractFieldset('#register_fieldset'),
                    dataType: "json",
                    beforeSend: function (xml) {
                        Fringistics.loadersShow();
                    },
                    success: function (data) {
                        // Ensure the audience field in the response exactly matches your client_id
                        if (!(data.audience == 'SelfCalls')) {
                            console.log('problem, wrong audience');
                        }

                        if (data.error) {
                            if (data.errorType == "CAPTCHA") {
                                $$$('#recaptcha_response_field').validationEngine('showPrompt', data.errorMessage, 'fail', 'topLeft', true);
                                Recaptcha.reload();
                            }
                            else
                                addAlert('error', data.errorMessage, 'minidrawer');
                            //alert('Error: ' + data.errorMessage);
                            Fringistics.loadersHide();
                        }
                        else {
                        
                            if(location.href.indexOf('/event/') != -1)
                                location.href = location.href.replace("event","contrib") + "?ev=1";
                            else
                                location.href = '/friends/';
                        
                        }
                    },
                    // handle !success
                    error: function (data) {
                        console.log('ajax error');
                        Fringistics.loadersHide();
                    }
                });
            }
        }

        $$$("#firstname, #lastname, #email_address, #password, #confirmation, #recaptcha_response_field").keypress(function (e) {
            if (e.which == 13) {
                eg_register();
                return false;
            }
        });
        $$$('#btn_register').click(function() {
            eg_register();
        });

        function eg_signin() {
            _kmq.push(['record', 'Button in nonsocial login popup clicked', { 'Button': 'Login' }]);
            if ($$$('#login_fieldset').validationEngine('validate')) {
                $$$.ajax({
                    type: "POST",
                    url: '/blocks/AJAXSelfAuth.aspx',
                    data: extractFieldset('#login_fieldset'),
                    dataType: "json",
                    beforeSend: function (xml) {
                        Fringistics.loadersShow();
                    },
                    success: function (data) {
                        // Ensure the audience field in the response exactly matches your client_id
                        if (!(data.audience == 'SelfCalls')) {
                            console.log('problem, wrong audience');
                        }

                        if (data.error) {
                            addAlert('error', data.errorMessage, 'minidrawer');
                            //alert('Error: ' + data.errorMessage);
                            Fringistics.loadersHide();
                        }
                        else {
                        
                            if(location.href.indexOf('/event/') != -1)
                                location.href = location.href.replace("event","contrib") + "?ev=1";
                            else
                                location.href = '/friends/';
                        
                        }
                    },
                    // handle !success
                    error: function (data) {
                        console.log('ajax error');
                        Fringistics.loadersHide();
                    }
                });
            }
        }

        $$$("#self_username, #self_password").keypress(function (e) {
            if (e.which == 13) {
                eg_signin();
                return false;
            }
        });
        $$$('#btn_loginEmail').click(function() {
            eg_signin();
        });
        
        function eg_forgotpass() {
            _kmq.push(['record', 'Button in nonsocial login popup clicked', { 'Button': 'Forgot password submit' }]);
            if ($$$('#login_forgot_fieldset').validationEngine('validate')) {
                $$$.ajax({
                    type: "POST",
                    url: '/blocks/SubmitForgotPassword.aspx',
                    data: extractFieldset('#login_forgot_fieldset'),
                    dataType: "xml",
                    beforeSend: function (xml) {
                        Fringistics.loadersShow();
                    },
                    success: function (xml) {
                        var result = $$$('success', xml).text();
                        var eC = $$$('errorCode', xml).text();
                        var eM = $$$('errorMsg', xml).text();

                        if (result == "true") {
                            
                            $$$('#btn_forgotRequest:not(.disabled)').html('Email Sent').addClass('disabled');
                            
                            Fringistics.loadersHide();
                        }
                        else {
                            addAlert('error', eM, 'minidrawer');
                            Fringistics.loadersHide();
                        }
                    },
                    // handle !success
                    error: function (data) {
                        console.log('ajax error');
                        Fringistics.loadersHide();
                    }
                });
            }
        }

        $$$("#forgot_username").keypress(function (e) {
            if (e.which == 13) {
                eg_forgotpass();
                return false;
            }
        });
        $$$('#login_forgot_fieldset').on('click', '#btn_forgotRequest:not(.disabled)', function () {
            eg_forgotpass();
        });

    

        $$$('.logingrid').off().on('click', '.logingridrow:not(.on) .sociallight, .logingridrow:not(.on) .sociallogin', function () {
            $$$('#new_subscription').validationEngine('hideAll');

            if ($$$(this).closest('.logingridrow').hasClass('fb')) {
                _kmq.push(['record', 'Social login in popup clicked', { 'Path': window.location.pathname, 'SocialNetwork': 'Facebook' }]);
                Fringistics.loadersShow();
                Fringistics.Facebook.gifterlogin('user_birthday,user_relationships,user_relationship_details,user_hometown,user_location,email,read_stream,friends_birthday,friends_relationships,friends_relationship_details,friends_hometown,friends_location,offline_access', function (response) {
                    _kmq.push(['record', 'fb_connect success']);
                    if (response) {

                        Fringistics.Facebook.getMe(function (resp) {
                            if (!resp) {                
                                return;
                            }
            
            
                            // Logic cahanged from cookie to Facebook helper as real website was not storing it in cookie
                            var tmpName = resp.name; //Cookie.get('FB_UserName');
                            var tmpUID = resp.id; // Cookie.get('FB_UID');
                            var fbEmailId = resp.email;// Cookie.get('FB_EM');
                            var isValidSocInvitee = false;
                   
                            if(typeof CheckForfacebookInvitee == 'function')
                                isValidSocInvitee = CheckForfacebookInvitee(tmpName,tmpUID, fbEmailId);
                            else
                                isValidSocInvitee = true;

                            if(isValidSocInvitee){
                           
                         
                                if(location.href.indexOf('/event/') != -1)
                                    location.href = location.href.replace("event","contrib") + "?ev=1";
                                else
                                    location.href = '/home/';
                         
                            }
                            else{
                                $$$("div.loginbox.ga_lightbox").hide();
                                Fringistics.loadersHide();
                            }
                
                         
                        });

                
                     
                  
                    }
                    else {
                        _kmq.push(['record', 'fb_connect error']);
                        Fringistics.loadersHide();
                        //addAlert('error fberror', '<img src="/Images/themes/eGifter/fb-favicon.png" alt=""/>Oops! You need to log in before we can continue.');
                    }
                });
            } else if ($$$(this).closest('.logingridrow').hasClass('gp')) {
                _kmq.push(['record', 'Social login in popup clicked', { 'Path': window.location.pathname, 'SocialNetwork': 'Google+' }]);

                var clientId = '69948307128-s3t9ai7t6psrk2i2kt3dvjre8glfoolq.apps.googleusercontent.com';
                var apiKey = 'AIzaSyDF_qYuqU7C-sIu579AlqA1_Rpxu3VMwOE';
                var scopes = ["https://www.googleapis.com/auth/plus.login", "https://www.googleapis.com/auth/userinfo.email", "https://www.googleapis.com/auth/plus.me", "https://www.google.com/m8/feeds", "https://www.googleapis.com/auth/userinfo.profile"];
                gapi.client.setApiKey(apiKey);
                gapi.auth.authorize({ client_id: clientId, scope: scopes, immediate: false }, handleAuthResultLoginPopup);
            } else if ($$$(this).closest('.logingridrow').hasClass('li')) {
                _kmq.push(['record', 'Social login in popup clicked', { 'Path': window.location.pathname, 'SocialNetwork': 'LinkedIn' }]);
            
                IN.User.authorize(OnLICallLoginPopup)
            }
        });

    if($$$('.logingridrow.on').length < 2)
    {
        $$$('.logingridrow.on .socialdisconnect').html('unlink');
    }

    $$$('.logingridrow').on('click', '.socialdisconnect', function () {
        var logoutnetwork;
        
        if($$$('.logingridrow.on').length < 2)
        {
            window.location = "/Logout.aspx?ReturnUrl=/Default.aspx"; //logout if only one connected network
            return true;
        }

        if ($$$(this).closest('.logingridrow').hasClass('fb')) {
            logoutnetwork = "fb";
            $$$('.logoutnet').html('Facebook');
        } else if ($$$(this).closest('.logingridrow').hasClass('gp')) {            
            logoutnetwork = "gp";
            $$$('.logoutnet').html('Google+');
        } else if ($$$(this).closest('.logingridrow').hasClass('li')) {
            logoutnetwork = "li";
            $$$('.logoutnet').html('LinkedIn');
        }

        $$$('.logoutconfirm').attr('_net',logoutnetwork);

        $$$('.loginbox').fadeOut(500);

        $$$('.logoutconfirm').lightbox_me({
            centered: true,
            onLoad: function() { 
            },            
            onClose: function() { 
            },
            closeClick: false,
            closeEsc: false
        });
    });

    $$$('#disconnect_cancel').click(function() {
        $$$('.logoutconfirm').trigger('close');
        $$$('.loginbox').fadeIn(500);
    });

    $$$('#disconnect_confirm').click(function() {
        var logoutnetwork = $$$('.logoutconfirm').attr('_net');

        Fringistics.loadersShow();

        $$$.ajax({
            url: '/blocks/SubmitSocialDisconnect.aspx',
            data: 'social=' + logoutnetwork,
            dataType: "json",
            success: function (data) {
                $$$('.logingridrow.' + logoutnetwork).removeClass('on');
                $$$('.logingridrow.' + logoutnetwork + ' .socialuser').addClass('sociallogin').removeClass('socialuser')
                        .html('Sign In');
                $$$('.logoutconfirm').trigger('close');
                $$$('.loginbox').fadeIn(500);
                if($$$('.logingridrow.on').length < 2)
                {
                    $$$('.logingridrow.on .socialdisconnect').html('log out');
                }
            },
            error: function() {
                window.location.reload();
            }
        });
    });

    function handleAuthResultLoginPopup(authResult) {
        if (authResult && !authResult.error) {
            $$$.ajax({
                url: 'https://www.googleapis.com/oauth2/v1/tokeninfo',
                data: 'access_token=' + authResult.access_token,
                dataType: "jsonp",
                success: function (data) {
                    // handle {"error":"invalid_token"}
                    if (data.error) {
                        console.log('problem, error response.');
                    }

                    $$$.ajax({
                        url: '/blocks/AJAXGoogleAuth.aspx',
                        data: 'access_token=' + authResult.access_token,
                        dataType: "json",
                        success: function (data) {
                            // handle {"error":"invalid_token"}
                            if (data.error) {
                                console.log('problem, error response.');
                            }
                            // Ensure the audience field in the response exactly matches your client_id
                            if (!(data.audience == 'GoogleCalls')) {
                                console.log('problem, wrong audience');
                            }

                            // Process the token in the platform..
                        
                        Fringistics.loadersShow();
                        if(location.href.indexOf('/event/') != -1)
                            location.href = location.href.replace("event","contrib") + "?ev=1";
                        else
                            location.href = '/friends/';
                        
                    },
                    // handle !success
                    error: function (data) {
                        console.log('ajax error');
                    }
                });

                //reload with the contacts..

            },
            // handle !success
            error: function (data) {
                console.log('ajax error');
            }
        });
    } else {
        //alert('2');
    }
}
    
function LICallForwardLoginPopup() {
    $$$.ajax({
        url: '/blocks/AJAXLinkedInAuth.aspx?stage=2',
        dataType: "json",
        success: function (data) {
            
            Fringistics.loadersShow();
            if(location.href.indexOf('/event/') != -1)
                location.href = location.href.replace("event","contrib") + "?ev=1";
            else
                location.href = '/friends/';
            
        },
        // handle !success
        error: function (xhr, ajaxOptions, thrownError) {
            console.log("error");
        }
    });
}
    
function OnLICallLoginPopup(result) {
    $$$.ajax({
        url: 'https://www.egifter.com/blocks/AJAXLinkedInAuth.aspx?callback=?',
            dataType: "json",
            success: function (response) {
                LICallForwardLoginPopup();
            },
            // handle !success
            error: function (xhr, ajaxOptions, thrownError) {
                if (xhr.status == 200) {
                    LICallForwardLoginPopup();
                }
                else
                    window.location.href = window.location.href;
            }
        });
    }
    </script>
