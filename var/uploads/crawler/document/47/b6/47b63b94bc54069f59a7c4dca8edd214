      //<![CDATA[
        // Ensure that extractFieldset exists
        if (typeof extractFieldset != 'function') {
            function extractFieldset(fieldsetName) {
                jQuery('body').append('<form id="form-to-submit" style="visibility:hidden;"></form>');
                jQuery('#form-to-submit').html(jQuery(fieldsetName).clone());
                //seems cloning will remove the selectDD. we know we have giftgoal in there, lets get it
                $$$('#form-to-submit select').val($$$(fieldsetName + ' select').val());
                var data = $$$('#form-to-submit').serialize();
                jQuery('#form-to-submit').remove();
                return data;
            }
        }
