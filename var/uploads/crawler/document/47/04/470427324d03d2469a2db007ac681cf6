            var loadUrl = "/livefyre-hottest.json";
            var LFHottestData;
            $.getJSON(loadUrl, function(response){
                var LFHottestData = response.data;
                var LFlength = LFHottestData.length - 1;
                if (LFlength > 9) {
                    LFlength = 9;
                }
                for (var i = 0; i <= LFlength; i++) {
