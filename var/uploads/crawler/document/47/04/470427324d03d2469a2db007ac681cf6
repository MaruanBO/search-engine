                });
                function fixSideTrendingList(evt){
                    var attrs = { };
                    $.each($("#addthis_trendingcontent div.addthis-content-row")[0].attributes, function(idx, attr) {
                        attrs[attr.nodeName] = attr.value;
                    });
                    $("#addthis_trendingcontent div.addthis-content-row").replaceWith(function () {
                        return $("<li />", attrs).append($(this).contents());
                    });
                    $("#addthis_trendingcontent").addClass('MostReadList');
                }
                addthis.addEventListener('addthis.trending.render', fixSideTrendingList);
