<script type="text/javascript">
        // <![CDATA[
        $("#TopNews").ready(
        function()
        {
            jQuery("#TopNews").nivoSlider(
            {"design": "basic_with_thumbs", "effect": "fade", "pauseTime": 3000, "controlNavThumbs": true, "controlNavThumbsFromRel": true}
        );

            /* following adds text to thumbnails */

            /* Please note that rels with a counter were added to the thumbnails
             * It will copy the alt attribute of the image links to the title and alt tags of the thumbnail link
             */

            // variable that holds the alt values
            var alts = {};

            // for each rel item, we create an array item containing the alt value from the original image
            $('a.nivo-imageLink[rel]').each(function() {
                var r = $(this).attr('rel');
                alts[r] = $(this).attr('alt');
            });

            // now we apply these alt values to the thumbnails which have the class nivo-control
            $.each(alts, function(index,value) {
                $('a.nivo-control[rel=' + index + ']').attr('alt',alts[index]);
                $('a.nivo-control[rel=' + index + ']').attr('title',alts[index]);
            });

        }
    );
        // ]]>
    </script>
