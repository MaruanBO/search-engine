<script type="text/javascript"> 
    $(document).ready(function() {
        // form submit
        $("#teaserform").submit(function() {
            var txt = $("input#textfeld").val();
            window.location.href = "http://www.spreadshirt.com/design-your-own-t-shirt-C59/product/109301291/tx1/" + encodeURIComponent(txt) + "/";
            return false;
        });
    });
