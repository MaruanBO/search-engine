<script type="text/javascript">

    jQuery(document).ready(function() {
        jQuery('.popoverInfo').hide();
        // init
        infoIcon();
    });

    function infoIcon() {

        // define var
        var infoClick = 0;
        var popverClass = '.popoverInfo';
        var iconInfoClass = '.icon-info';
        var mouse_is_inside = false;
        var custom = 12;
        // set position of popup over content box
        var infoIconWidth = jQuery(iconInfoClass).width() + parseInt(jQuery(iconInfoClass).parent().css('padding-left'));
        var infoIconHeight = jQuery(iconInfoClass).height() + custom;
        var popOverHeight = jQuery(iconInfoClass).parent().children(popverClass).height();
        var popOverWidth = jQuery(iconInfoClass).parent().children(popverClass).width();

        // set position of the box
        if (jQuery(popverClass + ', left')) {
            jQuery(iconInfoClass).parent().children(popverClass).css({'top': -(popOverHeight-infoIconHeight)/2,'left' : infoIconWidth + 1 });
        }

        if (jQuery(popverClass + ', top')) {
            jQuery(iconInfoClass).parent().children(popverClass).css({'top': infoIconHeight + (custom/2),'left' : -(popOverWidth/2 + 2)});
        }

        // check if mouse hover the info icon or the popover info box
        jQuery(iconInfoClass + ', ' + popverClass).hover(function() {
            mouse_is_inside = true;
        }, function(){
            mouse_is_inside = false;
        });

        // close popover info box on click if mouse is not inside the box or the icon
        jQuery("body").mouseup(function() {

            if(! mouse_is_inside && infoClick === 1) {

                jQuery(iconInfoClass).parent().children(popverClass).animate({
                    opacity: 0
                }, 100).hide();
                infoClick = 0;
            }

        });

        // click event for info btn
        jQuery(iconInfoClass).click(function() {

            if(infoClick === 0) {
                jQuery(this).parent().children(popverClass).animate({
                    opacity: 1
                }, 100).show();
                infoClick = 1;
            } else if(infoClick === 1) {
                jQuery(this).parent().children(popverClass).animate({
                    opacity: 0
                }, 100).hide();
                infoClick = 0;
            }

        });

    }

</script><a name="109860"></a>
