    /* form effekt */
    function formEffect() {
        jQuery('#textfeld').click(function() {
            if (this.value === this.defaultValue) {
                this.value = '';
                jQuery(".createme_submit").show();
                jQuery(".csr").hide();
            } else {
                jQuery(".createme_submit").show();
                jQuery(".csr").hide();
            }
        });
        jQuery('#textfeld').blur(function() {
            if (this.value === '') {
                this.value = this.defaultValue;
                jQuery(".createme_submit").hide();
                jQuery(".csr").show();
            }
        });
    }
    //-->
