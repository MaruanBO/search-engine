(function () {
var isSmartphone = function () {
try {
return (navigator.userAgent.search(/mobile/i) >= 0
&& (window.matchMedia && window.matchMedia('(max-width: 640px)').matches
|| window.screen.width < 766));
} catch (e) {
return false;
}
},
