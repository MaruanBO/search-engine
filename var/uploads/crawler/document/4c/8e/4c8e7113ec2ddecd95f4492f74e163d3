jQuery( document ).ready(function() {
  //check if side rails loaded
   if(jQuery("#backgroundSideRails").length){
      ga('send', 'event', 'Background Side Rails', 'Impression', jQuery("#backgroundSideRails").attr('bannername'), 1);  
      jQuery("#backgroundSideRails").live('click', function(){
      ga('send', 'event', 'Background Side Rails', 'Click', jQuery(this).attr('bannername'), 1); 
    });
   }
   //check if footer banner loaded
   if(jQuery(".footerBlock a").length){
      jQuery(".footerBlock a").each(function(index){  
         ga('send', 'event', 'Footer', 'Impression', jQuery(this).attr('bannername'), 1);  
      });

      jQuery(".footerBlock a").live('click', function(){
          ga('send', 'event', 'Footer', 'Click', jQuery(this).attr('bannername'), 1); 
      });
   }
   //check if right rails  loaded
   if(jQuery(".rightRailBlock a").length){
      jQuery(".rightRailBlock a").each(function(index){  
         ga('send', 'event', 'Right Rail', 'Impression', jQuery(this).attr('bannername'), 1);  
      });

      jQuery(".rightRailBlock a").live('click', function(){
          ga('send', 'event', 'Right Rail', 'Click', jQuery(this).attr('bannername'), 1); 
      });
   }
});

