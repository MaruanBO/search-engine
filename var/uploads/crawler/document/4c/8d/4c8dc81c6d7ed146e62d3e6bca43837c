});
flowplayer(function (api, root) {
  api.bind("ready", function () {
	api.waVideoName = api.video.src.substring(api.video.src.lastIndexOf("/")+1,api.video.src.indexOf(api.video.suffix)-1);
	api.waPageName = location.pathname.toLowerCase() + ' - video event';
	dcsMultiTrack('WT.dl','200','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','impression','WT.cg_n','event','WT.cg_s','video impression','DCS.dcsuri',api.waPageName);
	}).bind("cuepoint", function (e, api, cuepoint) {
	if (cuepoint.index == 0)
	dcsMultiTrack('WT.dl','202','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','25% Viewed','WT.si_n','pb_video','WT.si_x','2','WT.cg_n','event','WT.cg_s','video 25% viewed','DCS.dcsuri',api.waPageName);
	if (cuepoint.index == 1)
	dcsMultiTrack('WT.dl','203','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','50% Viewed','WT.si_n','pb_video','WT.si_x','3','WT.cg_n','event','WT.cg_s','video 50% viewed','DCS.dcsuri',api.waPageName);
	if (cuepoint.index == 2)
	dcsMultiTrack('WT.dl','204','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','75% Viewed','WT.si_n','pb_video','WT.si_x','4','WT.cg_n','event','WT.cg_s','video 75% viewed','DCS.dcsuri',api.waPageName);
  }).bind("resume", function () {
	if(api.video.time > .2 && api.video.time < (api.video.duration -1))
		dcsMultiTrack('WT.dl','207','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','resume','WT.cg_n','event','WT.cg_s','video resume','DCS.dcsuri',api.waPageName);
	else
		dcsMultiTrack('WT.dl','201','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','start','WT.si_n','pb_video','WT.si_x','1','WT.cg_n','event','WT.cg_s','video start','DCS.dcsuri',api.waPageName);
  }).bind("finish", function () {
    dcsMultiTrack('WT.dl','205','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','finish','WT.si_n','pb_video','WT.si_x','5','WT.si_cs','1','WT.cg_n','event','WT.cg_s','video finish','DCS.dcsuri',api.waPageName);
  }).bind("pause", function () {
	if(api.video.time <= (api.video.duration -1))
		dcsMultiTrack('WT.dl','206','DCSext.videonm',api.waVideoName,'DCSext.videotype',api.video.suffix,'DCSext.videoaction','pause','WT.cg_n','event','WT.cg_s','video pause','DCS.dcsuri',api.waPageName);
  });
});
//Establish web analytics object and base variables
wa = new Object;
//Identify the sub-domain if applicable
wa.subDomain = "";
if((location.hostname.split('.').length - 1) == 2)
	wa.subDomain = location.hostname.substr(0,location.hostname.indexOf("."));
/*If the sub-domain is a non-production server, set a different file path...*/
if(wa.subDomain.match(/beta/) || wa.subDomain.match(/test/) || wa.subDomain.match(/dev/) || wa.subDomain.match(/prod/)) {
	wa.environment = 'bcbst ' + wa.subDomain;
	wa.filePath = '//' + wa.subDomain + '.bcbst.com/js/webAnalytics/';
/*...otherwise, use the production file path*/
} else {
	wa.environment = 'bcbst prod';
	wa.filePath = '//www.bcbst.com/js/webAnalytics/';
}
