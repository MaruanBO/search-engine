//Flowplayer Video Tracking
var api = $('.flowplayer').data('flowplayer');
$(document).ready(function(){
	$('.thumbnail').click(function (event) { 
		event.preventDefault();
		api = $('.flowplayer').data('flowplayer');
		api.play($(this).data('videoIndex'));
