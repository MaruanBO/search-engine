<script type="text/javascript">
        //Affiliate-specific access token:
        var accesstoken = '20a4d8cc-f3bc-4571-906d-55145f419f7b';
        var primaryactivity = [];
        //Enables cross-domain ajax for IE
        (function (jQuery) {

            if (window.XDomainRequest) {
                jQuery.ajaxTransport(function (s) {
                    if (s.crossDomain && s.async) {
                        if (s.timeout) {
                            s.xdrTimeout = s.timeout;
                            delete s.timeout;
                        }
                        var xdr;
                        return {
                            send: function (_, complete) {
                                function callback(status, statusText, responses, responseHeaders) {
                                    xdr.onload = xdr.onerror = xdr.ontimeout = jQuery.noop;
                                    xdr = undefined;
                                    complete(status, statusText, responses, responseHeaders);
                                }
                                xdr = new XDomainRequest();
                                xdr.open(s.type, s.url);
                                xdr.onload = function () {
                                    callback(200, "OK", {
                                        text: xdr.responseText
                                    }, "Content-Type: " + xdr.contentType);
                                };
                                xdr.onerror = function () {
                                    callback(404, "Not Found");
                                };
                                if (s.xdrTimeout) {
                                    xdr.ontimeout = function () {
                                        callback(0, "timeout");
                                    };
                                    xdr.timeout = s.xdrTimeout;
                                }
                                xdr.send((s.hasContent && s.data) || null);
                            },
                            abort: function () {
                                if (xdr) {
                                    xdr.onerror = jQuery.noop();
                                    xdr.abort();
                                }
                            }
                        };
                    }
                });
            }
        })(jQuery);

        function getSearchResult() {
            var url = 'http://api2.adventurelink.com/CuratedClientCategoryTrips?accessToken='+accesstoken+'&type=jsonp&callback=ProcessFeaturedTrip';
            $.ajax({
                url: url,
                type: "GET",
                dataType: "jsonp",
                success: function(data)
                {
                    ProcessFeaturedTrip(data);
                }
            });
        }

        function ProcessFeaturedTrip(data)
        {
            $('#divFeaturedTrip').html('');
            $('#divTrip').html('');
            var divTrip = '<ul id="packagesBig" >';
            var divFeatured = '';
            var cnt=1;
            for (var j = 0; j < data.results.length; j++)
            {
                var shortdescription='';
                if(data.results[j].ShortDescription.length>0)
                {
                    if(data.results[j].ShortDescription.length>170)
                    {
                        shortdescription = data.results[j].ShortDescription.substring(0,170)+'...';
                    }
                    else
                    {
                        shortdescription = data.results[j].ShortDescription;
                    }
                }

                var tripname=''
                if(data.results[j].Name.length>0)
                {
                    if(data.results[j].Name.length>35)
                    {
                        tripname = data.results[j].Name.substring(0,35)+'...';
                    }
                    else
                    {
                        tripname = data.results[j].Name;
                    }
                }
                var totalduration = parseInt(data.results[j].Duration/1440);

                var durationtext='Days';
                if(totalduration == 1)
                {
                    durationtext='Day';
                }
                else
                {
                    durationtext='Days';
                }

                var lowestprice = 0;
                if(data.results[j].PromotionPrice!=null && data.results[j].PromotionPrice>0)
                {
                    lowestprice = data.results[j].PromotionPrice;
                }
                else
                {
                    lowestprice = data.results[j].LowestPrice;
                }
                var priceperday = parseFloat(lowestprice)/totalduration;

                if(j==0)
                {
                    ///////////////////////featured trip//////////////////
                    divFeatured += '<div class="imagePackage">';
                    divFeatured += '<a href="http://luxurylink.adventurelink.com/detailPricing.html?tripid=' + data.results[j].TripID + '&cid='+data.results[j].Category+'&lid=">';
                    divFeatured += '<img itemprop="contentUrl" src="http://photos.luxurylink.us/images/home/970x250-uganda.jpg" alt="'+data.results[j].Name+'"></a>';
                    divFeatured += '<div class="pricingAndSavingsDisplay">';
                    divFeatured += '<div class="openingBid">';
                    divFeatured += '<span class="priceItem">$'+priceperday.toFixed(0)+'</span><span class="itemDivider">/</span><span class="dayItem">DAY</span></div>';
                    divFeatured += '</div>';
                    divFeatured += '<div class="day_block">';
                    divFeatured += '<span class="day_Count">'+totalduration+'</span><span class="day_text">'+durationtext+'</span></div>';
                    divFeatured += '</div>';
                    divFeatured += '<div class="visualDesc">';
                    divFeatured += '<div class="linkURL">';
                    divFeatured += '<a href="http://luxurylink.adventurelink.com/detailPricing.html?tripid=' + data.results[j].TripID + '&cid='+data.results[j].Category+'&lid=">'+tripname+'</a>';
                    divFeatured += '</div>';
                    divFeatured += '<div class="location">'+data.results[j].City+', '+data.results[j].Country;
                    //            divFeatured += '<span class="saveTxt">(SAVE 25%)</span>';

                    if (data.results[j].IsPromotionActive == true)
                    {
                        if(data.results[j].PromotionTypeID == 1)
                        {
                            var discount = parseFloat(parseFloat(data.results[j].PromotionDiscount) / parseFloat(data.results[j].LowestPrice)) * 100;
                            divFeatured += '    <span class="saveTxt">(SAVE '+parseFloat(discount).toFixed(0)+'%)</span>';
                        }
                        else
                        {
                            var discount = parseFloat(parseFloat(data.results[j].PromotionDiscount));
                            divFeatured += '    <span class="saveTxt">(SAVE $'+parseFloat(discount).toFixed(0)+')</span>';
                        }
                    }
                    divFeatured += '</div>';
                    divFeatured += '<div class="packageBlurb">'+data.results[j].ShortDescription+'</div>';
                    divFeatured += '</div>';
                    ///////////////////////////////////////////////
                    continue;
                }

                if(cnt==3)
                {
                    divTrip += '<div class="displayPackage thirdPackage">';
                }
                else
                {
                    divTrip += '<div class="displayPackage">';
                }
                divTrip += '<div class="imagePackage">';
                divTrip += '    <a href="http://luxurylink.adventurelink.com/detailPricing.html?tripid=' + data.results[j].TripID + '&cid='+data.results[j].Category+'&lid=">';
                divTrip += '        <img src="http://res.cloudinary.com/adventurelink/image/fetch/w_360,h_236,c_fill/https://s3.amazonaws.com/al-photos/'+data.results[j].Photo.FileName+'" alt="'+data.results[j].Name+'">';
                divTrip += '    </a>';
                divTrip += '   <div class="pricingAndSavingsDisplay">';
                divTrip += '        <div class="openingBid">';
                divTrip += '            <span class="priceItem">$'+priceperday.toFixed(0)+'</span>';
                divTrip += '            <span class="itemDivider">/</span>';
                divTrip += '            <span class="dayItem">DAY</span>';
                divTrip += '        </div>';
                divTrip += '    </div>';
                divTrip += '    <div class="day_block">';
                divTrip += '        <span class="day_Count">'+totalduration+'</span>';
                divTrip += '        <span class="day_text">'+durationtext+'</span>';
                divTrip += '    </div>';
                divTrip += '</div>';
                divTrip += '<div class="clientDescription">';
                if (data.results[j].IsPromotionActive == true)
                {
                    if(data.results[j].FlashSale > 1)
                    {
                        divTrip += '<div class="flash-img-banner"><span> Flash Sale: '+data.results[j].FlashSale+' days left</span></div>';
                    }
                    else
                    {
                        if(data.results[j].FlashSale <=1)
                        {
                            divTrip += '<div class="flash-img-banner"><span> Flash Sale: Ending Soon</span></div>';
                        }
                    }
                }
                divTrip += '    <div class="linkURL">';
                divTrip += '        <a href="http://luxurylink.adventurelink.com/detailPricing.html?tripid=' + data.results[j].TripID + '&cid='+data.results[j].Category+'&lid=">'+tripname+'</a>';
                divTrip += '    </div>'
                divTrip += '    <div class="location">'+data.results[j].City+', '+data.results[j].Country;
                if (data.results[j].IsPromotionActive == true)
                {
                    if(data.results[j].PromotionTypeID == 1)
                    {
                        var discount = parseFloat(parseFloat(data.results[j].PromotionDiscount) / parseFloat(data.results[j].LowestPrice)) * 100;
                        divTrip += '    <span class="saveTxt">(SAVE '+parseFloat(discount).toFixed(0)+'%)</span>';
                    }
                    else
                    {
                        var discount = parseFloat(parseFloat(data.results[j].PromotionDiscount));
                        divTrip += '    <span class="saveTxt">(SAVE $'+parseFloat(discount).toFixed(0)+')</span>';
                    }
                }
                divTrip += '    </div>';
                divTrip += '    <div class="packageBlurb">'+shortdescription+'</div>';
                divTrip += '</div>';

                if(data.results[j].isExclusiveOffer == true){
                    divTrip += '<div class="exclusive"></div>';
                }

                divTrip += '</div>';
                if(cnt % 3 == 0)
                {
                    divTrip += '<div style="clear:both;"></div>';
                    cnt=0;
                }
                cnt++;
                if(j==6){
                    break;
                }
            }
            divTrip += '</ul>';

            $('#divTrip').html(divTrip);
            $('#divFeaturedTrip').html(divFeatured);
        }

        $(document).ready(function () {
            getSearchResult();
        });
    </script>
<style>#packagesBig{margin:0;padding:0;overflow:hidden;}#packagesBig .linkURL,.visual-holder .linkURL{margin-bottom:3px;color:#008ec5;text-decoration:none;font-size:20px;font-weight:300;line-height:1.2;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}.location{position:relative;}.location{display:block;font-family:Raleway,sans-serif;font-style:normal;font-size:12px;color:#222;font-weight:300;margin:0 0 10px;}.visual-holder .day_block{left:175px;bottom:-8px;}.visual-holder .pricingAndSavingsDisplay{padding:14px 0 9px 0;left:24px;bottom:0px;}.visual-holder{float:left;margin:0 0 30px 0;}.visualDesc{width:50%;padding:2.5em 2em 0em 1.3em;float:left;}#packagesBig .packageBlurb,.visual-holder .packageBlurb{color:#222;font:12px/1.25 Raleway,sans-serif;margin:0;line-height:17px;}.displayPackage{float:left;margin:0 30px 70px 0;width:303px;position:relative;overflow:hidden;}.displayPackage.thirdPackage{margin-right:0;}.imagePackage{position:relative;}.displayPackage img{max-width:100%;margin-bottom:10px;}.saveTxt{font-weight:600;}.saveTxt{color:#4e012d;font-size:11px;margin-left:30px;}#packagesBig .openingBid{margin-top:5px;}.openingBid{opacity:1;}.exclusive{background:url('data:image/png;base64,') no-repeat scroll 0 0 rgba(0,0,0,0);height:78px;position:absolute;left:0px;top:0px;width:108px;}.displayPackage .flash-img-banner{background:linear-gradient(to bottom,#008ec5  0px,#008ec5  100%) repeat scroll 0 0 rgba(0,0,0,0);color:#fff;font-size:15px;position:absolute;top:-100px;left:0px;transition:top 200ms ease 0s;width:100%;z-index:10;visibility:hidden;}.displayPackage .flash-img-banner>span{background:url("../images/flash-clock.png") no-repeat scroll 0 10px rgba(0,0,0,0);display:block;margin:0 20px;padding:9px 0 12px 30px;}.displayPackage:hover .flash-img-banner{top:0px;visibility:visible;}.pricingAndSavingsDisplay{background-color:rgba(210,206,205,0.8);padding:6px 0;position:absolute;bottom:9px;left:0;float:left;color:#00223c;font-weight:300;line-height:1.2em;width:120px;text-align:center;}.priceItem{font-size:18px;}.itemDivider{font-size:18px;margin-left:4px;}.dayItem{font-size:10px;}.day_block{background-image:url('data:image/png;base64,');width:47px;height:47px;position:absolute;bottom:0px;right:10px;float:left;color:#fff;text-align:center;}.day_Count{font-size:26px;font-weight:300;line-height:1em;}.day_text{line-height:1.7em;display:inline-block;margin-left:8px;text-transform:uppercase;font-size:12px;float:left;}</style>
