<script type="text/javascript">
        $(window).load(function(){
            var ad = $('#pfa-ad');
            var adLength = $('#LLTG_Ad_Slot_0 div iframe').contents().find('a').length;
            if(adLength === 0){
                ad.remove();
                startSlideshow();
            }else{
                ad.show();
                startSlideshow();
            }
        });

        function startSlideshow() {
            window.revapi = $('.tp-banner').revolution(
                    {
                        delay: 9000,
                        startwidth: 1170,
                        startheight: 500,
                        hideThumbs: 10,
                        thumbWidth: 90,
                        thumbHeight: 47,
                        thumbAmount: 5,
                        navigationType: 'none',
                        navigationArrows: "solo",
                        hideTimerBar: 'on',
                        lazyLoad: 'on',
                        navigationStyle: 'll-custom-arrows'
                    });
        }; //ready
    </script>
