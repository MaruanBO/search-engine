    <script type="text/javascript">
    var isRetina = (
    window.devicePixelRatio > 1 ||
    (window.matchMedia && window.matchMedia("(-webkit-min-device-pixel-ratio: 1.5),(-moz-min-device-pixel-ratio: 1.5),(min-device-pixel-ratio: 1.5)").matches)
    );
    if( isRetina ){
      $('#logo-rotunda').attr( 'src', 'images/homepage/rotunda-logo-2x.png');
    }
    </script>
    <script type="text/javascript">
    /* ==================================================
    Mobile Navigation
    ================================================== */
    var RotundaHome = window.RotundaHome || {};
    /* Clone Menu for use later */
    var mobileMenuClone = $('#menu').clone().attr('id', 'navigation-mobile');

    RotundaHome.mobileNav = function(){
      var windowWidth = $(window).width();
      
      // Show Menu or Hide the Menu
      if( windowWidth >= 979 ) {
        $('#navigation-mobile').css('display', 'none');
        if ($('#mobile-nav').hasClass('open')) {
          $('#mobile-nav').removeClass('open'); 
        }
      }
    };

    // Call the Event for Menu 
    RotundaHome.listenerMenu = function(){
      
      $('#mobile-nav').on('click', function(e){
        $(this).toggleClass('open');
	if( $('.hero').hasClass('hero-margintop') ) {
	$('#navigation-mobile').stop().slideToggle( { duration: 350, 
                                        easing: 'easeOutExpo',
                                        start: function() { setTimeout( function() { $('.hero').toggleClass('hero-margintop'); }, 200 ) }
                                        } );
	}
	else {
		$('.hero').toggleClass('hero-margintop');
		$('#navigation-mobile').stop().slideToggle( { duration: 350, 
                                        easing: 'easeOutExpo'
                                        } );

	}
        e.preventDefault();
      });
    };


  RotundaHome.mobileNav();
  RotundaHome.listenerMenu();


$(window).bind('resize',function(){
  RotundaHome.mobileNav();
  
});

    </script>    

    
