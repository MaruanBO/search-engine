	<script type="text/javascript">
	function deleteConfirm(url) { if ( confirm("Are you sure you want to delete this?") ) {
			window.open(url, "_top");	}	}
	function ignoreConfirm(url) { if ( confirm("Are you sure you want to ignore this user?") ) {
			window.open(url, "_top");	}	}
	function miscConfirm(url) { if (confirm("Are you sure you want to do this?")) {
	        window.open(url, "_top");   }	}
	function actionConfirm(url, myMSG) {
	    if (confirm( myMSG )) {
	        window.open(url, "_top");
	    }
	}
	function hideWarning() {
	    document.getElementById('warningID').style.visibility = 'hidden';
		document.getElementById( 'warningID' ).style.display = 'none';	}
	function hideUpdate() {
	    document.getElementById('updateID').style.visibility = 'hidden';
	    document.getElementById('updateID').style.display = 'none';    }
	function openLogin() {
	    document.getElementById('loginID').style.visibility = 'visible';
		document.logFrm.login.focus();	}
	function hideLogin( ) {	document.getElementById( 'loginID' ).style.visibility = 'hidden';	}
	function menuToggle( targetID ) { if ( document.getElementById( targetID ).style.display == 'none' ) {
//			document.getElementById( 'MainMenuID1' ).style.display = 'none';
			document.getElementById( 'MainMenuID2' ).style.display = 'none';
			document.getElementById( targetID ).style.display = 'inline'; } else {
			document.getElementById( targetID ).style.display = 'none';	}	}
	function openActiveBox( targetID ) { if ( document.getElementById( targetID ).style.display == 'none' ) {
			document.getElementById( targetID ).style.display = 'inline'; } else {
			document.getElementById( targetID ).style.display = 'none';	}	}
	</script>
