    <script type="text/javascript">
      $(document).ready(function(){
        TimetoastApp.addRegions({
          timelineViewControlsRegion: "#timeline-view-controls-region"
        });
        if(Timetoast.utils.userAgent.isIOSDevice()){
          // don't bother showing the Flash timeline
          $("#timeline-player-container").hide();
        }else{
          TimetoastApp.addRegions({
            timelineViewControlsRegion: "#timeline-view-controls-region"
          });
          // show the classic timeline
          TimetoastApp.addInitializer(function(){
            var viewMode = "timeline"
            TimetoastApp.module("ClassicTimelineViewerApp").start({
              timelineId: '115783',
              environment: "null",
              viewMode: viewMode,
              size: "small"
            });
            TimetoastApp.module("TimelineViewControlsApp").start({ 
              region: TimetoastApp.timelineViewControlsRegion, 
              viewMode: viewMode,
              currentSize: "small",
              showSizeControls: true
            });
          });
          TimetoastApp.on("initialize:after", function(){
            if(Backbone.history){
              return Backbone.history.start();
            }
          });
          TimetoastApp.on("start", function(options){
            var timelineLoader = TimetoastApp.request("component:timelineLoader");
            timelineLoader.loadTimeline(115783);
          });
        }
      });
    </script>



