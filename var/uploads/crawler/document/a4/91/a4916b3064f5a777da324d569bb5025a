<script type="text/javascript">
	(function () {
		$('#WhySelectStateOpen').click(function () {
			$('#StateSelectDialog').addClass('noBottomRadius');
			$('.stateSelectImportance').slideDown();
			$('.close').show();
			$('.open').hide();
		});

		$('#WhySelectStateClose').click(function () {
			$('.stateSelectImportance').slideUp(function () {
				$('#StateSelectDialog').removeClass('noBottomRadius');
			});
			$('.close').hide();
			$('.open').show();
		});

		function loadStateSelectDialog() {
			$("#StateSelectDialog").show();
			if (document.cookie == '') {
				$("#StateSelectDialog .cookieRequiredError").show();
			}
		}

		$('#StateSelectShopButton').click(function () {
			var popUp = document.getElementById("stateSelectPopUp");
			if (popUp) {
				var selectedString = popUp.options[popUp.selectedIndex].value;
				if (selectedString == "--") {
					$("#StateSelectDialog .stateSelectError").show();
				} else {
					location.href = "/?state=" + selectedString;
				}
			}
		});

		var stateFromIpAddress = 'CA';
		if (stateFromIpAddress !== '') {
			$('#stateSelectPopUp').val(stateFromIpAddress);
		}

		loadStateSelectDialog();
	})();
</script>    <header class="pageHeader"><div class="topRow"><a href="/" class="logo"><img src="http://cache1.wine.com/assets/logo.png" alt="Wine.com logo" /><img src="http://cache1.wine.com/assets/logoStewardShip.png" alt="Wine.com Stewardship logo" class="stewardshipLogo" /></a><div class="customerSection"><a href="/v6/account/orderstatus.aspx">Track Order</a><a href="/customercare/"><em>Customer Care</em></a><a href="tel:18005925870"><em>1.800.592.5870</em></a><a href="http://www.wineshopper.com/?s=winecom_topnavbanner&amp;cid=winecom_topnavbanner" class="wineShopperUpSell" data-analytics-click="{&quot;id&quot;: &quot;wineShopperLink&quot;}"><img src="http://cache1.wine.com/assets/wineshopperUpsell.png" /></a></div><div class="stateSection"><label>Ship To<select class="stateSelect checkZip"><option value="AK">AK</option><option value="AL">AL</option><option value="AR">AR</option><option value="AZ">AZ</option><option value="CA" selected="true">CA</option><option value="CO">CO</option><option value="CT">CT</option><option value="DC">DC</option><option value="DE">DE</option><option value="FL">FL</option><option value="GA">GA</option><option value="HI">HI</option><option value="IA">IA</option><option value="ID">ID</option><option value="IL">IL</option><option value="IN">IN</option><option value="KS">KS</option><option value="KY">KY</option><option value="LA">LA</option><option value="MA">MA</option><option value="MD">MD</option><option value="ME">ME</option><option value="MI">MI</option><option value="MN">MN</option><option value="MO">MO</option><option value="MS">MS</option><option value="MT">MT</option><option value="NC">NC</option><option value="ND">ND</option><option value="NE">NE</option><option value="NH">NH</option><option value="NJ">NJ</option><option value="NM">NM</option><option value="NV">NV</option><option value="NY">NY</option><option value="OH">OH</option><option value="OK">OK</option><option value="OR">OR</option><option value="PA">PA</option><option va