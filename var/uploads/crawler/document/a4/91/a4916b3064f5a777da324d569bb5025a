        <script type="text/javascript">
        var Facebook = {
            Status: '',
            UserID: '',
            WineComFacebookUserId: '',
            AccessToken: '',
            FirstName: '',
            IsConnected: false,
            IsAuthorized: false,
            $LoginButton: '',
            $LoginListItem: '',

            init: function(response, serverSideFacebookId, nextUrl) {
                FB.Event.subscribe('auth.authResponseChange', Facebook.SetLoginStatus);
                Facebook.$LoginButton = $('#fbLoginLink');
                Facebook.$LoginListItem = $('#fbLoginListItem');
                Facebook.$LoginButton.click(function() {
                    FB.login();
                });

                Facebook.WineComFacebookUserId = serverSideFacebookId + '';
                Facebook.SetLoginStatus(response, nextUrl);
            },

            SetLoginStatus: function(response, nextUrl) {
                Facebook.Status = response.status;
                if (response.status === 'connected') {
                    Facebook.IsConnected = true;
                    Facebook.IsAuthorized = true;
                    Facebook.UserID = response.authResponse.userID + '';
                    Facebook.AccessToken = response.authResponse.accessToken;
                    FB.api('/me', function(apiResponse) {
                        Facebook.FirstName = apiResponse.first_name;
                    });
                    Facebook.$LoginListItem.hide();
                } else if (response.status === 'not_authorized') {
                    Facebook.IsConnected = true;
                    Facebook.IsAuthorized = false;
                    Facebook.$LoginListItem.show();
                } else {
                    Facebook.IsConnected = false;
                    Facebook.IsAuthorized = false;
                    Facebook.$LoginListItem.show();
                }

                if (Facebook.IsConnected && Facebook.IsAuthorized) {
                    //Do some account correction
                    if (Facebook.WineComFacebookUserId === '' || Facebook.WineComFacebookUserId === 'unknown') {
                        //We can't know for sure if the FB user own's this wine.com account.
                    } else if (Facebook.WineComFacebookUserId === Facebook.UserID) {
                        //same person.
                    } else {
                    	//Not the same person.
                        var redirectUrl = '/v6/account/login.aspx?mode=signout';
                        if (nextUrl !== '') {
                            redirectUrl += '&return=' + nextUrl;
                        }
                        window.location = redirectUrl;
                    }
                }
            }
        };

        function facebookOnScrollLeft(event) {
            if (typeof FB != 'undefined') {
                try {
                    var fbLeft = $('.facebookLikeLeft');
                    if (IsInViewport(fbLeft)) {
                        fbLeft.append('<fb:like href="http://www.facebook.com/winecom" width="185" show_faces="false" layout="button_count" action="like" font="verdana" colorscheme="light"></fb:like>');
                        $(window).unbind('scroll', facebookOnScrollLeft);
                        FB.XFBML.parse(fbLeft.get(0));
                    }
                } catch (e) {
                    $(window).unbind('scroll', facebookOnScrollLeft);
                    return;
                }
            }
        }

        function facebookOnScrollFooter(event) {
            if (typeof FB != 'undefined') {
                try {
                    var fbkLike = $('.facebookLike');
                    if (IsInViewport(fbkLike)) {
                        fbkLike.append('<fb:like href="http://www.facebook.com/winecom" width="360" show_faces="true" layout="standard" action="like" font="verdana" colorscheme="light"></fb:like>');
                        $(window).unbind('scroll', facebookOnScrollFooter);
                        FB.XFBML.parse(fbkLike.get(0));
                    }
                } catch (e) {
                    $(window).unbind('scroll', facebookOnScrollFooter);
                    return;
                }
            }
        }

        window.fbAsyncInit = function() {
            FB.init({
                appId: '108763387276', // App ID
                status: true, // check login status
                cookie: true, // enable cookies to allow the server to access the session
                oauth: true, // enable OAuth 2.0
                xfbml: true,
                channelUrl: '//' + window.location.hostname + '/facebook/fb_xd_channel.html'
            });

            FB.getLoginStatus(function(response) {
                if (typeof Facebook === 'undefined') {
                    console.log('Facebook Obj Literal not created yet.');
                } else {
                    Facebook.init(response, 'unknown', 'http://www.wine.com/');
                }
            });

            if ($('.facebookLikeProduct').length) {
                $(window).bind('scroll', facebookOnScrollProduct);
                facebookOnScrollProduct();
            }

            if ($('.facebookLikeLeft').length) {
                $(window).bind('scroll', facebookOnScrollLeft);
                facebookOnScrollLeft();
            }

            if ($('.facebookLike').length) {
                $(window).bind('scroll', facebookOnScrollFooter);
                facebookOnScrollFooter();
            }
        };

        // Load the SDK Asynchronously
        (function(d) {
            var js, id = 'facebook-jssdk', ref = d.getElementById('fb-root');
            if (d.getElementById(id)) {
                return;
            }
            js = d.createElement('script');
            js.id = id;
            js.async = true;
            js.src = "//connect.facebook.net/en_US/all.js";
            if (ref) {
                ref.parentNode.insertBefore(js, ref);
            }
        }(document));
    </script>

    
    
