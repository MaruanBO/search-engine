<script language="JavaScript" type="text/javascript">
    var domainName = "safeway";
    var environment = "www";
    var sURL = "http://www.safeway.com/";
    var prodURL = "http://www.safeway.com/";
    us = window.location.href;
    //alert (us);
    usa = us.split(".");
    for (i=0; i<usa.length; i++) {
        //alert (i + " = " + usa[i]);
        domainName = usa[1];
        environment = usa[0];
    }
    switch(domainName) {
        case "carrsqc":
            sURL = environment + "." + "carrsqc.com/";
            prodURL = "http://www.carrsqc.com/";
            break;
        case "dominicks":
            sURL = environment + "." + "dominicks.com/";
            prodURL = "http://www.dominicks.com/";
            break;
        case "genuardis":
            sURL = environment + "." + "genuardis.com/";
            prodURL = "http://www.genuardis.com/";
            break;
        case "pavilions":
            sURL = environment + "." + "pavilions.com/";
            prodURL = "http://www.pavilions.com/";
            break;
        case "randalls":
            sURL = environment + "." + "randalls.com/";
            prodURL = "http://www.randalls.com/";
            break;
        case "tomthumb":
            sURL = environment + "." + "tomthumb.com/";
            prodURL = "http://www.tomthumb.com/";
            break;
        case "vons":
            sURL = environment + "." + "vons.com/";
            prodURL = "http://www.vons.com/";
            break;
        case "safeway":
            sURL = environment + "." + "safeway.com/";
            prodURL = "http://www.safeway.com/";
            break;
        default:
            sURL = environment + "." + "safeway.com/";
            prodURL = "http://www.safeway.com/";
    }
</script>
