<script type="text/javascript">
function fire_omni_tag(click_event) {
	var banner="safeway";
	parsedLocation=window.location.href;
	parsedLocationSplit=parsedLocation.split(".");
	banner=parsedLocationSplit[1];
	var s=s_gi(s_account);
	s.linkTrackVars='eVar74';
	s.eVar74=banner+click_event;
	s.tl(this,'o',banner+click_event);
}
</script>
