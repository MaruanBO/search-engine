    function regionMatch(img, division, state) {
        var L = (loggedOn!=='');
        if (L && !img.loggedIn || !L && !img.loggedOut) return;
        if (state && Ext.Array.contains(img.state, state)) return 'state';
        if (division && Ext.Array.contains(img.division, division)) {
            for (var i=0,_i=img.state.length; i<_i; i++) if (0===img.state[i].indexOf(division)) return;
            return 'division';
        }
        return;
    }
	
	function selectDefaultImage(img) {
       var L = (loggedOn!=='');
	   if (L && !img.loggedIn || !L && !img.loggedOut) return; //If there are no images for signedin or signedout state, return
       if (L && img.loggedIn || !L && img.loggedOut) return img;
	   return;
    }

    function getImageContainers(division, state) {
        var images = [],
            containers = imageData.ImageContainers,
            ct, region, regional, static, img, i,_i, j,_j, cfg;

        // iterate each container
        for (i=0, _i=containers.length; i < _i; i++) {
            ct = containers[i];
            regional = {division:'',state:''};
			static = '';

            // iterate each container image
            for (j=0, _j=ct.data.length; j < _j; j++) {
			    img = ct.data[j];
                cfg = { loggedIn:  img.loggedIn,   imageSrc:  img.imageSrc,   imageMap:   img.imageMap,
                        loggedOut: img.loggedOut,  targetUrl: img.targetUrl,  imageTitle: img.imageTitle };
                if ('static'===ct.type || img.defaultImage) { 
					if (!static && selectDefaultImage(cfg)){static = cfg};
				}
                else if (region=regionMatch(img, division, state)) { if (!regional[region]) regional[region]=cfg; }
            }
            images.push(regional.state || regional.division || static);
        }
		return images;
    }
    
    sliderCfg.images = getImageContainers();
	console.log("Images supplied to Slider: ");
	console.log(sliderCfg.images);
    Ext.create('Ext.ux.ImageSlider', sliderCfg);

    GeoAPI.getStoreData(function(geo) {
        var division = (geo.brandId && geo.divNumber) ?
	geo.brandId+':'+geo.divNumber : null,
            state = (division && geo.state) ? division+':'+geo.state : null;
        if (location.search.match(/^\?((\d+:\d+):..$)/)) { state = RegExp.$1; division = RegExp.$2; }
        GeoAPI.state = state;
		sliderCfg.images = getImageContainers(division, state);
		console.log("Images supplied to Slider: ");
		console.log(sliderCfg.images);
		Ext.destroy(Ext.ComponentQuery.query('component[renderTo=imageSlider1]'), Ext.get('imageSlider1').down('.image-slider'));
        Ext.create('Ext.ux.ImageSlider', sliderCfg);
    });
    
}); /* Ext.onReady */

