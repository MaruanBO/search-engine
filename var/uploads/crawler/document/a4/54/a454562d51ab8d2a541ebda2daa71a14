<script type="text/javascript">

function geoCodePodContent() {

    var division = 'nothing-returned';
    var state = 'nothing-returned';
    var city = 'nothing-returned';
    var postalcode = 'nothing-returned';

    GeoAPI.getStoreData(function(store) {
      if(store === null) return;
      division = store.divName;
      state = store.state;
      city = store.city;
      postalcode = store.postCode;
    });




    if (city == "Sacramento") {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_fastforward_ad").style.visibility="visible";
        document.getElementById("pod_fastforward_ad").style.display="inline";
    } else if (division == "Norcal" && state == "HI") {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_hawaiiWidget").style.visibility="visible";
        document.getElementById("pod_hawaiiWidget").style.display="inline";
    } else if ( state == "MT") {
        // leave default visible 
    } else if( division == "Eastern" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_easternWidget").style.visibility="visible";
        document.getElementById("pod_easternWidget").style.display="inline";
    } else if( division == "Norcal" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_norcalWidget").style.visibility="visible";
        document.getElementById("pod_norcalWidget").style.display="inline";
    } else if( division == "Seattle" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_seattleWidget").style.visibility="visible";
        document.getElementById("pod_seattleWidget").style.display="inline";
    } else if( division == "Portland" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_portlandWidget").style.visibility="visible";
        document.getElementById("pod_portlandWidget").style.display="inline";
    } else if( division == "Phoenix" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_phoenixWidget").style.visibility="visible";
        document.getElementById("pod_phoenixWidget").style.display="inline";
    } else if( division == "Denver" ) {
        document.getElementById("pod_defaultWidget").style.visibility="hidden";
        document.getElementById("pod_defaultWidget").style.display="none";
        document.getElementById("pod_denverWidget").style.visibility="visible";
        document.getElementById("pod_denverWidget").style.display="inline";
    }
}

function addLoadEvent(func) {
  var oldonload = window.onload;
  if (typeof window.onload != 'function') {
    window.onload = func;
  } else {
    window.onload = function() {
      if (oldonload) {
        oldonload();
      }
      func();
    }
  }
}
addLoadEvent(geoCodePodContent);
</script></div><!--CILF code ends-->
