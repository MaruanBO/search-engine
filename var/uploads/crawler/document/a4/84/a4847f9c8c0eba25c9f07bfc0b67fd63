		function checkSubmit(form) {
			if(form.login.value=="") {
				alert("Please enter your username.");
				form.login.focus();
				return false;
			}
			if(form.password.value=="") {
				alert("Please enter your password.");
				form.password.focus();
				return false;
			}
			
			return true;
		}
	</script>
	
