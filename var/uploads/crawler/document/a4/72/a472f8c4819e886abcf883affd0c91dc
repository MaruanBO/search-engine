$(function() {
	var featuredStories = $('#widget-featured-stories .widget-content');
	featuredStories.find('.featured-story').sort(function (a, b) {
        var priority_a = a.getAttribute('data-priority'),
            priority_b = b.getAttribute('data-priority');
    	return +priority_b - +priority_a;
	})
	.appendTo( featuredStories );
});
</script>

