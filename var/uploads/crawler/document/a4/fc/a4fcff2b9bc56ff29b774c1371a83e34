        <script type="text/javascript">
            $(function () {

                if ($("#featuredArticlesList").find("#lastElement").length == 0)
                    $("#showMoreFeaturedBtn").show();

                function showFeaturedArcticlesResult(result, append) {
                    if (append) {
						var $newItems = $(result);
						$newItems.imagesLoaded(function(){
							$("#featuredArticlesList").isotope('insert', $newItems);
						});
					} else {
						$("#featuredArticlesList").html(result);
					}
                    if ($("#featuredArticlesList").find("#lastElement").length > 0) {
                        $("#showMoreFeaturedBtn").hide();
                    }
                    else $("#showMoreFeaturedBtn").show();

                    if (append)
                        $('input#recordsCount[rel="featuredArticlesList"]').val(parseInt($('input#recordsCount[rel="featuredArticlesList"]').val()) + 20);
                    else
                        $('input#recordsCount[rel="featuredArticlesList"]').val(20);
                }

                $('#showMoreFeaturedBtn').click(function (e) {
                    var recordsCount = $('input#recordsCount[rel="featuredArticlesList"]').val();
                    AcdbCommon.ajaxRequest('/action/Article/GetFeaturedArticlesListChunk?skip=' + recordsCount, null, showFeaturedArcticlesResult, true);
                });
            });
        </script>		
