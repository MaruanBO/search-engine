	<script type="text/javascript">


		window.lauoutSubscriptionFormMrg = window.lauoutSubscriptionFormMrg || {};
		window.lauoutSubscriptionFormMrg = {
			formHolder: '',
			emailInput: '',
			collapsedArea: '',
			errorArea: '',
			newsletterCheckboxes: '',
			validate: function () {
				var hasError, checked$, email;

				lauoutSubscriptionFormMrg.collapsedArea.show();

				lauoutSubscriptionFormMrg.errorArea.empty().hide();

				hasError = false;

				checked$ = lauoutSubscriptionFormMrg.newsletterCheckboxes.filter(":checked");
				if (checked$.size() <= 0) {
					hasError = true;
					lauoutSubscriptionFormMrg.errorArea.append("<h3><div class='error_img'></div>Please select a newsletter preference.</h3>");
				}

				email = $.trim(lauoutSubscriptionFormMrg.emailInput.val());
				var pattern = new RegExp(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/);
				if (email === "enter email address" || !pattern.test(email)) {
					hasError = true;
					lauoutSubscriptionFormMrg.errorArea.append("<h3><div class='error_img'></div>Please enter a valid e-mail address.</h3>");
				}

				if (hasError) {
					lauoutSubscriptionFormMrg.errorArea.slideDown();
				}
				return !hasError;
			},
			subscribe: function () {

				lauoutSubscriptionFormMrg.errorArea.hide();
				if (NRecoCommon.isValidForm(lauoutSubscriptionFormMrg.formHolder) && lauoutSubscriptionFormMrg.validate()) {
					var subsTypes = [];
					lauoutSubscriptionFormMrg.newsletterCheckboxes.filter(":checked").each(function () {
						subsTypes.push($(this).val());
					});
					var fields = NRecoCommon.readForm(lauoutSubscriptionFormMrg.formHolder);
					fields["name"] = fields["email"];
					fields["subscription_types"] = subsTypes;
					$.ajax({
						type: "POST",
						async: true,
						url: '/action/Page/SubscribeNewsletter',
						data: {
							data: NRecoCommon.jsonSerialize(fields)
						},
						success: function (res) {
							var $dialogRes = $('#newsletterResultForm').processTemplate().find('.modal');
							$dialogRes.find("span.resultHolder").html(res.message);
							var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
						},
						error: function (XMLHttpRequest, textStatus, errorThrown) {
							lauoutSubscriptionFormMrg.errorArea.html(XMLHttpRequest.responseText).show('fast');
						}
					});
				}
			},
			init: function () {

				lauoutSubscriptionFormMrg.formHolder = $('#' + 'lauoutSubscriptionForm');
				lauoutSubscriptionFormMrg.emailInput = lauoutSubscriptionFormMrg.formHolder.find('input[name="email"]');
				lauoutSubscriptionFormMrg.collapsedArea = lauoutSubscriptionFormMrg.formHolder.find('.options');
				lauoutSubscriptionFormMrg.errorArea = lauoutSubscriptionFormMrg.formHolder.find('.error');
				lauoutSubscriptionFormMrg.newsletterCheckboxes = lauoutSubscriptionFormMrg.formHolder.find("input[name=\"newsletter\"]");
				var $emailInput = this.emailInput;
				var emailEnter = $emailInput.attr("data-val-mouseenter") || "enter email address";
				var emailLeave = $emailInput.attr("data-val-mouseleave") || "NEWSLETTER SIGN UP";
				var emailDefault = emailLeave;

				lauoutSubscriptionFormMrg.formHolder.on('click', function (e) {
					e.stopPropagation();
				});


				// init
				NRecoCommon.initForm(lauoutSubscriptionFormMrg.formHolder);
				// validation
				NRecoCommon.validateForm(lauoutSubscriptionFormMrg.formHolder);

				lauoutSubscriptionFormMrg.formHolder.find('button.submit,a.submit').click(function () {
					lauoutSubscriptionFormMrg.subscribe();
				});

				$emailInput
				.val(emailDefault)
				.hover(
					function () {
						var val = $emailInput.val();
						emailDefault = emailEnter;
						if (val === emailLeave) {
							$emailInput.val(emailEnter);
						}
					},
					function () {
						var val = $emailInput.val();
						emailDefault = emailLeave;
						if (val === emailEnter) {
							$emailInput.val(emailLeave);
						}
					}
				)
				.focus(function (e) {
					$emailInput.addClass("focused");
					var val = $emailInput.val();
					if (val === emailEnter || val === emailLeave) {
						$emailInput.val("");
					}

					var $main = $('main');
					var $options = lauoutSubscriptionFormMrg.formHolder.find('.options');
					var $body = $('body');

					$options.css({
						'visibility': 'hidden',
						'display': 'block'
					});

					$options.addClass('open-upwards');
					$body.one('click', function () {
						$options.slideUp(function () {
							$options.removeClass('open-upwards');
						});
					});

					$options.css({
						'visibility': 'visible',
						'display': 'none'
					});

					$options.slideDown();

				})
				.on('click', function (e) {
					e.stopPropagation();
				})
				.blur(function () {
					$emailInput.removeClass("focused");
					if (!Boolean($.trim($emailInput.val()))) {
						$emailInput.val(emailDefault);
					}
				});
			}
		};

		$(function () {
			lauoutSubscriptionFormMrg.init();
		});
	</script>
</div>


