<script type="text/javascript">
	window.newsletterManager = {
		newlettersub: '',
		subsid: '',
		newslid: '',

		newsletterResultTypes: {
			0: 'Your email was not found in the list!',
			1: 'You\'ve been unsubscribed successfully!'
		},

		checkResult: function () {
			if (window.newsletterManager.newlettersub != '') {
				var $dialogRes = $('#newsletterResultForm').processTemplate().find('.modal');
				$dialogRes.find("span.resultHolder").html(window.newsletterManager.newsletterResultTypes[window.newsletterManager.newlettersub]);
				var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
			}
		},

		showSendToFriendForm: function () {
			if (window.newsletterManager.newslid != '' && window.newsletterManager.subsid != '') {
				var $dialogInv = $('#newsletterInviteForm').processTemplate().find('.modal');
				var $dialogInvOverlay = NRecoCommon.showModal($dialogInv);

				// init
				NRecoCommon.initForm($dialogInv);
				// validation
				NRecoCommon.validateForm($dialogInv);

				$dialogInv.find('button.submit,a.submit').click(function () {
						$dialogInv.find('.error').hide();
						if (NRecoCommon.isValidForm($dialogInv)) {
							var fields = NRecoCommon.readForm($dialogInv);
							fields["newslid"] = window.newsletterManager.newslid;
							$.ajax({
								type: "POST",
								async: true,
								url: '/action/Page/NewsletterInviteFriends',
								data: {
									data: NRecoCommon.jsonSerialize(fields)
								},
								success: function (res) {
									$dialogInvOverlay.close();
									var $dialogRes = $('#newsletterResultForm').processTemplate().find('.modal');
									$dialogRes.find("span.resultHolder").html(res.message);
									var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
								},
								error: function (XMLHttpRequest, textStatus, errorThrown) {
									$dialogInv.find('.error').html(XMLHttpRequest.responseText).show('fast');
							}
						});
					}
				});
			}
		}

	};

	$(function () {
		$('#newsletterResultForm').setTemplate($('#newsletterResultFormTemplate').html());
		$('#newsletterInviteForm').setTemplate($('#newsletterInviteFormTemplate').html());

		window.newsletterManager.checkResult();
		window.newsletterManager.showSendToFriendForm();
	});
</script>
		
