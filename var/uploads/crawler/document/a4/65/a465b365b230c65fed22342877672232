(function($) {
    if ($ === undefined || $ === null) {
        return;
    }

    $.fn.clickMonitor = function(callback) {
        if (typeof callback !== "function") {
            callback = function() {
            };
        }

        var _this = this;
        var elements_ready_to_click = [];

        _this.filter("[data-iframe-wrapper-id]").mouseenter(function() {
            elements_ready_to_click[$(this).attr("data-iframe-wrapper-id")] = true;
        }).mouseleave(function() {
            elements_ready_to_click[$(this).attr("data-iframe-wrapper-id")] = false;
        });

        $(window).blur(function() {
            for (i = 0; i < elements_ready_to_click.length; i++) {
                if (elements_ready_to_click[i] === true) {
                    var currentElement = _this.filter("[data-iframe-wrapper-id='" + i + "']");
                    currentElement.isIFrame = true;
                    currentElement.isLink = false;
                    callback.call(currentElement, $(currentElement).data());
                }
            }
        });

        _this.filter(":not([data-iframe-wrapper-id])").mouseup(function(e) {
            if (e.button === 0 || e.button === 1) {
                var currentElement = $(this);
                currentElement.isIFrame = false;

                if (this.tagName.toLowerCase() === 'a') {
                    currentElement.isLink = true;
                } else {
                    currentElement.isLink = false;
                }

                callback.call(currentElement);
            }
        });

        return _this;
    };
})($);
