	<script language="javascript">
		function onMouseStyle(elem)
		{
			for(i=1; i<=7; i++)
			{
					elem2 = String(i);			
					{

					}
			}
			
			document.getElementById("menuButtonLeft" + elem).style.backgroundImage="url('http://static1.blackfridaydiscount.net/images/menuLeftGrey.jpg')";
			document.getElementById("menuButtonMid" + elem).style.backgroundImage="url('http://static2.blackfridaydiscount.net/images/menuMidGrey.jpg')";
			document.getElementById("menuButtonRight" + elem).style.backgroundImage="url('http://static2.blackfridaydiscount.net/images/menuRightGrey.jpg')";
			document.getElementById("menuButtonMid" + elem).style.color="#ffffff";	
			
		}////onMouseStyle

		function outMouseStyle(elem,menus)
		{
			if(menus[Number(elem)]!=1)
			{
				document.getElementById("menuButtonLeft" + elem).style.backgroundImage="url('http://static1.blackfridaydiscount.net/images/menuLeft.jpg')";
				document.getElementById("menuButtonMid" + elem).style.backgroundImage="url('http://static2.blackfridaydiscount.net/images/menuMid.jpg')";
				document.getElementById("menuButtonRight" + elem).style.backgroundImage="url('http://static1.blackfridaydiscount.net/images/menuRight.jpg')";
				document.getElementById("menuButtonMid" + elem).style.color="#666666";
			}
			
			for(i=0; i<=7; i++){
				if(menus[i] == 1){
					onMouseStyle(i);
				}
			}
		}///////outMouseStyle
		
		var menus = ['0','0','0','0','0','0','0','0'];
	
		function clicked(elem){
		
			menus = ['0','0','0','0','0','0','0','0'];
			menus[elem] = 1;
		
			for(i = 0; i <= 7; i++){
				if(i!=elem){
					elem2 = String(i);
					outMouseStyle(i,menus);
				}
			
			}////for loop
		
		}///////clicked function
	
		onMouseStyle('0');
		clicked(0);	
	</script>
