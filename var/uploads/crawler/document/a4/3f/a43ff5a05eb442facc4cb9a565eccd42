    var ga_glb_brand = "fender";

    // Use to track event and delay form submit.
    function ga_trackSubmitClick(button, validate, category, action, label, value, fobj) {
        var $button = $(button);
        var $form = $button.closest("form");

        if (validate)
            $form.validate();

        if (!validate || $form.valid()) {
            ga_trackSimpleClick(category, action, label, value, fobj);

            setTimeout(function(){
                $("<input>").attr({
                    type: "hidden",
                    name: $button.attr("name"),
                    value: $button.attr("value")
                }).appendTo($form);

                $form.submit();
            }, 100);
        }

        return false;
    }

    // Use to track event and delay link navigation.
    function ga_trackOutboundLink(link, category, action, label, value, fobj) {
        ga_trackSimpleClick(category, action, label, value, fobj);

        setTimeout(function() {
            if (link.target.length > 0) {
                window.open(link.href, link.target)
            } else {
                document.location.href = link.href;
            }
        }, 100);

        return false;
    }

    // Use to track social event
    function ga_trackSocialClick(network, action, target, fobj) {
        try {
            ga(function() {
                var trackers = ga.getAll();
                for (var i=0; i<trackers.length; ++i) {
                    var tracker = trackers[i];
                    tracker.send('social', network, action, target, fobj);
                }
            });
        } catch(err){}
    }

    // Use to track generic event
    function ga_trackSimpleClick(category, action, label, value, fobj) {
        try {
            ga(function() {
                var trackers = ga.getAll();
                for (var i=0; i<trackers.length; ++i) {
                    var tracker = trackers[i];
                    tracker.send('event', category, action, label, value, fobj);
                }
            });
        } catch(err){}
    }

