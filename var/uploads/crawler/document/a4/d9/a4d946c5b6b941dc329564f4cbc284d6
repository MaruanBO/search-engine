<script language="javascript" type="text/javascript">//<![CDATA[
    jQuery(document).ready(function () {
        var prm = Sys.WebForms.PageRequestManager.getInstance();
        prm.add_beginRequest(initWaiting);
    });

    function initWaiting() {
        var h = jQuery(window).height();
        var relH = (h - 200) / 2 + $(document).scrollTop();
        relH = relH < 0 ? 0 : relH;
        jQuery('#wt').css('top', String(relH) + 'px');
    }

    function fnShowSplashScreen() {
        jQuery('#ctl00_PleaseWaitControl1_updateProcessContact').show();
        return true;
    }

    function fnHideSplashScreen() {
        jQuery('#ctl00_PleaseWaitControl1_updateProcessContact').hide();
        return true;
    }
    //]]>
</script>

    

<script type="text/javascript">//<![CDATA[
    var Browser = {
        Version: function () {
            var version = 999; // we assume a sane browser    
            if (navigator.appVersion.indexOf("MSIE") != -1) // bah, IE again, lets downgrade version number
                version = parseFloat(navigator.appVersion.split("MSIE")[1]);
            return version;
        }
    }
    function updateFooter() {

        var scnWid, scnHei;

        if (self.innerHeight) // all except Explorer
        {
            scnWid = self.innerWidth;
            scnHei = self.innerHeight;
        }
        else if (document.documentElement && document.documentElement.clientHeight)
            // Explorer 6 Strict Mode
        {
            scnWid = document.documentElement.clientWidth;
            scnHei = document.documentElement.clientHeight;
        }
        else if (document.body) // other Explorers
        {
            scnWid = document.body.clientWidth;
            scnHei = document.body.clientHeight;
        }

        var i = 0;
        var popupCorrectionX = 40;
        var popupCorrectionY = 40;
        var innerPopupDivCorrectionY = 140;
        var isIE6 = (Browser.Version() < 7);


        for (i; i < POPUPS.length; i++) {
            var popup = document.getElementById(POPUPS[i].id);
            if (popup == null) continue;

            var innerPopupDiv = GetChildDivWithClass(popup, "innerPopupDiv");
            if (isIE6) {

                if (innerPopupDiv != undefined)
                    innerPopupDiv.style.height = scnHei - innerPopupDivCorrectionY + "px";
                else
                    popup.style.height = scnHei - popupCorrectionY + "px";
            }
            else {
                if (innerPopupDiv != undefined)
                    innerPopupDiv.style.maxHeight = scnHei - innerPopupDivCorrectionY + "px";
                else
                    popup.style.maxHeight = scnHei - popupCorrectionY + "px";


            }

            var popupWidth = POPUPS[i].width != undefined ? POPUPS[i].width : 846;
            if (scnWid < popupWidth) {
                popup.style.width = scnWid - popupCorrectionX + "px";
                if (isIE6) {
                    innerPopupDiv.style.width = (scnWid - (popupCorrectionX + 20)) + "px";
                }
            }
            else {
                popup.style.width = popupWidth + "px";
                if (isIE6) {
                    innerPopupDiv.style.width = (popupWidth - 10) + "px";
                }
            }
        }
    }

    function GetChildDivWithClass(parent, className, level) {
        for (var i = 0; i < parent.childNodes.length; i++) {
            var childClassName = parent.childNodes[i].className;
            if (childClassName != undefined && childClassName.indexOf("innerPopupDiv") >= 0) {
                return parent.childNodes[i];
            }
        }

        if (level == undefined)
            level = 3
        else
            level -= 1;

        if (level <= 0)
            return null;

        for (var i = 0; i < parent.childNodes.length; i++) {
            var divNode = GetChildDivWithClass(parent.childNodes[i], className, level);
            if (divNode != undefined)
                return divNode;
        }

        return null;
    }

    jQuery(document).ready(function () {
        updateFooter();
    });

    jQuery(window).resize(function () {
        updateFooter();
    });

    if (typeof Sys !== "undefined")
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () { updateFooter(); });

    //]]>
</script>

    

