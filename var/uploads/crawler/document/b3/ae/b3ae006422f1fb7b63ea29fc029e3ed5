		$('#id_weather_currentconditions').html(data['weathertext'])
		$('#id_weather_temperature').html(data['temperature'])
		$('#id_weather_hightemperature').html(data['hightemperature'])
		$('#id_weather_lowtemperature').html(data['lowtemperature'])
		$('#id_div_weather_header').css({visibility: "visible"});
		$('#id_table_weather_data').css({visibility: "visible"});
	}
	);
})
</script><a href="/weather/"><span class="section-label"></span><span class="stat"><span id='id_weather_currentconditions'></span>, <span id='id_weather_temperature'></span>&deg;</span><span class="separator">/</span><span class="stat">High: <span id='id_weather_hightemperature'></span>&deg;</span><span class="separator">/</span><span class="stat">Low: <span id='id_weather_lowtemperature'></span>&deg;</span></a></div><div class="stocks col-sm-15 col-lg-8 popover-content"><script type="text/javascript" charset="utf-8">
$(document).ready(function() {
	$.get('/tu/ajax_current_financial/', 
	function(data) {
		dow = data['Dow']
		nasdaq = data['NASDAQ']
		sp500 = data['SP500']

		// alert(JSON.stringify(nasdaq))
		price = dow['price']
		pricenasdaq = nasdaq['price']
		pricesp = sp500['price']

		change = dow['change']
		changenasdaq = nasdaq['change']
		changesp = sp500['change']

		dow_is_down = change.charAt(0)=='-'
		nasdaq_is_down = changenasdaq.charAt(0)=='-'
		sp_is_down = changesp.charAt(0)=='-'

		$('#dow').html(price)
		if(dow_is_down)
			$('#dow').addClass('stockDown')
		else
			$('#dow').addClass('stockUp')

		$('#nasdaq').html(pricenasdaq)
		if(nasdaq_is_down)
			$('#nasdaq').addClass('stockDown')
		else
			$('#nasdaq').addClass('stockUp')

		$('#sp').html(pricesp)
		if(sp_is_down)
			$('#sp').addClass('stockDown')
		else
			$('#sp').addClass('stockUp')

	}
	);
})
</script><style type="text/css">
.stockDown { color: #c00; }
.stockUp { color: #329232; }
</style><strong>Dow: </strong><span id="dow"></span><strong> / NASDAQ: </strong><span id="nasdaq"></span><strong> / S&amp;P 500: </strong><span id="sp"></span><span class="section-label ticker-name"></span><span class="idx idx-down ticker-price"></span></div><nav class="utility-nav col-sm-15 col-lg-10" role="navigation"><ul class="micro-menu"><li><a class="btn btn-xs btn-primary btn-red" href="/store/" title="Store">Store</a></li><li class="first"><a href="https://travel.washingtontimes.com/" title="Book Travel" target="_blank">Book Travel</a></li><li><a href="/subscribe/" title="Subscribe">Subscribe</a></li><li><a href="http://www.print2webcorp.com/marketplace/washingtontimes/main/" title="Classifieds">Classifieds</a></li><li><a href="/weekly-ads/" class="Weekly Ads">Weekly Ads</a></li></ul></nav><div class="search col-sm-15 col-lg-5"><form id="frm_search" action="/search/"><fieldset><legend class="sr-only">Search</legend><label for="search-keyword" class="sr-only" title="Search Keyword">Search Keyword:</label><input type="hidden" name="cx" value="015385541671335030271:nfb7f1nj88q" /><input type="hidden" name="cof" value="FORID:11" /><input type="hidden" name="ie" value="UTF-8" /><input id="search-keyword" name="q" placeholder="Search" type="search" value="" /><input type="submit" name="sa" value="GO" class="btn btn-default btn-xs" title="Search" /></fieldset></form><script type="text/javascript" src="http://www.google.com/coop/cse/brand?form=cse-search-box&lang=en"></script></div></div></div><!-- //.dom-move --></div></div></header><!-- //.top-bar --><header id="masthead"><div class="masthead-inner container"><div class="row"><div id="twt-logo" class="col-sm-15 col-lg-8"><a href="/" title="The Washington Times | Home"><img src="http://twt-static.washtimes.com/v4/images/logo-twt.svg" alt="Logo: The Washington Times" /></a></div><div id="social-media-preferences" class="col-sm-15 col-lg-5 col-lg-push-17 hidden-xs"><div class="dom-move social" data-target="social" da