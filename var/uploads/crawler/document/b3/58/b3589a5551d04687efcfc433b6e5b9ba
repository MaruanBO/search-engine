<script type='text/javascript'>
  $('#contentbar a').click(function() {
    // Check if the clicked tab is already active
    if(this == $('#contentbar li.active a').get(0)) return false;
    
    var section = $(this).attr('href');
    $('#contentbar li.active').removeClass('active').removeClass('loading');
    $(this).parent().addClass('active').addClass('loading');
      
    // Check if the results were previously loaded
    var regex = /[^\/]+$/
    var sectionid = regex.exec(section);
    
    if(sectionid == "articles") {
      var section = $('#stories-filter li.active a').attr('href');
      sectionid = "articles-" + regex.exec(section);
    }
    else sectionid = "discussions-all";
    
    var previous = $("#content-results-" + sectionid);   
    
    if(previous.length>0) {
      $('#contentbar li.loading').removeClass('loading');
      $('.view-frontpage2-wrap','#content-inner').hide();
      previous.fadeIn(500);
    }    
    else {     
      $.get(section,{ajax:3},function(data) {
      	$('#contentbar li.loading').removeClass('loading');
      	$('.view-frontpage2-wrap','#content-inner').parent().append(data);
      	var previous = $("#content-results-" + sectionid);
      	
      	$('.view-frontpage2-wrap','#content-inner').hide();
      	previous.fadeIn(500);
      	
      	previous.spe_render_ads();
        if(typeof spe_render_likes == 'function') spe_render_likes(previous.get(0));
      	Drupal.attachBehaviors(previous.get(0));      	
      },'html');
    }
    
    if(sectionid == 'discussions-all') $('#stories-filter-wrap').hide();
    else $('#stories-filter-wrap').show();
    
    history.pushState({}, "", section);
    
    return false;
  });
  
  $('#stories-filter a').click(function() {
    if( $(this).parent('.active').length == 1 ) return false;
    
    var url = $(this).attr('href');
    $('#stories-filter li.active').removeClass('active').removeClass('loading');
    $(this).parent().addClass('active').addClass('loading');
    
    var regex = /[^\/]+$/
    var sectionid = "articles-" + regex.exec(url);   
    var previous = $("#content-results-" + sectionid);   
    
    if(previous.length>0) {
      $('#stories-filter li.loading').removeClass('loading');
      $('.view-frontpage2-wrap','#content-inner').hide();
      previous.fadeIn(500);
    }    
    else {     
      $.get(url,{ajax:3},function(data) {
        $('#stories-filter li.loading').removeClass('loading');
        $('.view-frontpage2-wrap','#content-inner').parent().append(data);
        var previous = $("#content-results-" + sectionid);
        
        $('.view-frontpage2-wrap','#content-inner').hide();
        previous.fadeIn(500);
        
        previous.spe_render_ads();
        if(typeof spe_render_likes == 'function') spe_render_likes(previous.get(0));
        Drupal.attachBehaviors(previous.get(0));        
      },'html');
    }
    history.pushState({}, "", url);
    
    return false;         
  });  
  
  $(window).bind("popstate", function(event) {
    if(document.location.pathname.indexOf("/content/discussions") !=-1) $('#content-section-discussions a').click();
    if(document.location.pathname.indexOf("/content/articles") !=-1 ) {
        var regex = /[^\/]+$/
        var filter = regex.exec(document.location.pathname);
        if(filter == "articles") filter = "all";
               
        if(filter == "all" || filter == "phones" || filter == "tablets" || filter == "apps" || filter == "accessories" || filter == "help") {
           $('#content-filter-' + filter + ' a').click();
        }
    }    
  });
  
  // Set the state of content view based on the cookie
  var list_view = $.cookie('spe_list_view');
  var v = $("#content-thread-view");
  v.find(".active").removeClass('active');
    
  if(list_view == "1") {
    $("#content-list-view",v).addClass('active');
    $('body').removeClass('expanded');
    $('body').addClass('collapsed-list');
  }
  else {
    $("#content-expand-view",v).addClass('active');
    $('body').addClass('expanded');
    $('body').removeClass('collapsed-list');    
  }
</script>
