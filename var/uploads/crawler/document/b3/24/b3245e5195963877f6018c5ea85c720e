<!--END RIGHT PROMOTIONAL ITEM --><script>
var rand = function(min, max) {
    return Math.floor(Math.random() * (max - min + 1)) + min;
};
 
var generateWeighedList = function(list, weight) {
    var weighed_list = [];
     
    // Loop over weights
    for (var i = 0; i < weight.length; i++) {
        var multiples = weight[i] * 100;
         
        // Loop over the list of items
        for (var j = 0; j < multiples; j++) {
            weighed_list.push(list[i]);
        }
    }
     
    return weighed_list;
};
 
var list = [];
$("#spotlight-ar").find("div").each(function(){ list.push(this); });
var weight = [.7, 0.3];
var weighed_list = generateWeighedList(list, weight);
 
var random_num = rand(0, weighed_list.length-1);
 
 
 
$('#spotlight-ar .spotlight').hide();
$('#spotlight-ar').html(weighed_list[random_num]); 
$('#spotlight-ar .spotlight').toggle(); 
</script><script>
var rand = function(min, max) {
    return Math.floor(Math.random() * (max - min + 1)) + min;
};
 
var generateWeighedList = function(list, weight) {
    var weighed_list = [];
     
    // Loop over weights
    for (var i = 0; i < weight.length; i++) {
        var multiples = weight[i] * 100;
         
        // Loop over the list of items
        for (var j = 0; j < multiples; j++) {
            weighed_list.push(list[i]);
        }
    }
     
    return weighed_list;
};
 
var list = [];
$("#random-video").find("div.spotlight").each(function(){ list.push(this); });
var weight = [.2, 0.2, .2, .2, .2];
var weighed_list = generateWeighedList(list, weight);
 
var random_num = rand(0, weighed_list.length-1);
 
 
 
$('#random-video .spotlight').hide();
$('#random-video').html(weighed_list[random_num]); 
$('#random-video .spotlight').toggle(); 
</script>




