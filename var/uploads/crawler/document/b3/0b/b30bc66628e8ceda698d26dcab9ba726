<script language="javascript">
jq183(document).ready(function(e) {
        jq183('#donate-menu-loaded').val('false');
	jq183('#donate-menu-go').click(function() {
		var ddData = jq183('#donate-menu').data('ddslick');
		donateMenuNav(ddData);
	});
	jq183('#donate-menu').ddslick({
		width:147,
		background: 'none repeat scroll 0 0 #fff',
		onSelected: function(data){
			if (jq183('#donate-menu-loaded').val() == 'true') {
				donateMenuNav(data);
			}
			jq183('#donate-menu-loaded').val('true');
		}   
	});
	function donateMenuNav(data) {
		top.document.location = data.selectedData.value;
	}
	
	donateMenuMouseOut($('ul.dd-options'));
	function donateMenuMouseOver(elm) {
		$('a.dd-option-selected').removeClass('dd-option-selected').addClass('dd-option-first');
		$(elm).unbind('mouseover');
		$(elm).mouseout(function() {
			donateMenuMouseOut(this);
		});

	}
	function donateMenuMouseOut(elm) {
		$('a.dd-option-first').removeClass('dd-option-first').removeClass('dd-option-selected').addClass('dd-option-selected');
		$(elm).unbind('mouseout');
		$(elm).mouseover(function() {
			donateMenuMouseOver(this);
		});

	}
});
</script>
