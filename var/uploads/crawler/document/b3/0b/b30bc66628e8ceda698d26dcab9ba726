<script type="text/javascript">

    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regexS = "[\\?&]" + name + "=([^&#]*)";
        var regex = new RegExp(regexS);
        var results = regex.exec(window.location.search);
        if(results == null) {
            return "";
        }
        else {
            return decodeURIComponent(results[1].replace(/\+/g, " "));
        }
    } 

    function setCookie(c_name,value,expiredays)
    {
        var exdate=new Date();
        exdate.setDate(exdate.getDate()+expiredays);
        document.cookie=c_name+ "=" +escape(value)+((expiredays==null) ? "" : ";expires="+exdate.toGMTString() ) + ";path=/;";
    }

    function NoThankYou()
    {
        HideDialog();
    }

    function Donate()
    {
        HideDialog();
        location.href = url;
    }

    function HideDialog()
    {
        setCookie("drtv_2014","true",10950);
        dialog.hide();
    }

    function fadeDialog() {
        setCookie("drtv_2014","true",10950);
        $('.yui-dialog').fadeOut(1000);
        $('#splashbox_mask').fadeOut(1000);
    }


    function preloadWSImage() {
        var wsImageUrl = $(".splash").css('background-image');
        wsImageUrl = /^url\((['"]?)(.*)\1\)$/.exec(wsImageUrl);
        wsImageUrl = wsImageUrl ? wsImageUrl[2] : ""; // If matched, retrieve url, otherwise ""
        if (wsImageUrl !== "") {
            var preloadedImage = new Image();
            preloadedImage.src = wsImageUrl;
        }
    }

     // We can hide the windowshade by adding "hidews=1" to the query string    
    if (getParameterByName("hidews") === "1") {
        console.log("Query string is telling us to hide the windowshade.");
        window.hideWindowShade = true;
    }   

     // Show window shade if there's no cookie and there's no "hideWindowShade" variable set
     // But we can force the windowshade to appear anytime by adding "forcews=1" to the query string

    if (((document.cookie.indexOf("drtv_2014") == -1) && (typeof window.hideWindowShade === "undefined" || window.hideWindowShade !== true)) || (getParameterByName("forcews") === "1")) {
        var dialog;
        //We can cancel preloading by adding preload=0 to url. Also, never preload on mobile.
        if (getParameterByName("preload") !== "0" && window.location.host !== "m.humanesociety.org") {
            console.log("WS Image preloaded");
            preloadWSImage(); //preload the windowshade image
        }
        YAHOO.util.Event.addListener(window, "load", function() {
            dialog = new YAHOO.widget.Dialog("splashbox", {
                fixedcenter: true,
                visible: false,
                modal: true,
                close: false,
                constraintoviewport: true
            });
            dialog.render(document.body);
            dialog.show();
            $(document).keyup(function(e) {
                if (e.keyCode == 27) { HideDialog(); }   // esc
            });
          //  setTimeout(fadeDialog, 20000); set the windowshade to disappear after 20 seconds. Cookie is set.
        });
    } else {
        document.getElementById("splashbox").style.display = "none";
    }
</script>
<!-- END 2014 ONE-CLICK WINDOWSHADE -->
