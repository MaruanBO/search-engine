<script>
$(document).ready(function(e) {
   setTimeout(function() {	//CMK09152014: Added delay to allow Optimizely test to take effect first
       // Make the entire billboard clickable, and respect target attribute in A tag
       $('#billboard li').click(function(event) {
            if($('a:first', this).attr("target") == "_blank") { 
                event.preventDefault();
                event.stopPropagation();
                window.open($('a:first', this).attr("href"), "_blank");
            } else {
                window.location = $('a:first', this).attr("href");       
            }
        });
    }, 2000);        
});
</script>


