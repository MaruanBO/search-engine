left: 0;
width: 100%;
height: 100%;
}
li.media {border-bottom:1px solid #dcdcdc; padding-bottom:5px;margin-bottom:5px;}
#videoContent li.media:first-child{display:none;}
#featuredheadline{padding-bottom:20px;}
</style>
<script type="text/javascript">
var playListId = 'PLlNB9Y9T3c_XoGB_eOfmMxRNEBIwxWsAr';
var playListURL = 'https://gdata.youtube.com/feeds/api/playlists/'+playListId+'?v=2&alt=json&max-results=5&callback=?';
var videoURL= 'http://www.youtube.com/watch?v=';
var videofeatured = '';
var videolist = '';
var vid = '';
var thumb = '';
var title = '';
var count = 0;
jQuery.getJSON(playListURL, function(data) {
jQuery.each(data.feed.entry, function(i, item) {
//vids.push( item["media$group"]["yt$videoid"]["$t"] );
vid = item["media$group"]["yt$videoid"]["$t"];
thumb = (item['media$group']['media$thumbnail']) ? item["media$group"]["media$thumbnail"][0]["url"] : '';
title = item["media$group"]["media$title"]["$t"];
if (i < 1 ) {
//videofeatured += '<iframe class="youtube-player" type="text/html" width="316" height="257" src="http://www.youtube.com/embed/'+vid+'" allowfullscreen frameborder="0">';
//videofeatured += '</iframe>';
jQuery('div#featured').empty().append('<div id="ytplayer"></div>');
loadPlayer(vid, 'ytplayer', '316', '257');
videofeaturedheadline = '<h4 class="title">'+title+'</h4>';
} if (i < 3) {
videolist += '<li class="media" id="vid'+vid+'">';
if (thumb) {
videolist += '<div class="video pull-left element_spacing_small">';
videolist += '<a class="ytvideo" href="#'+vid+'">';
videolist += '<img src="'+thumb+'" class="img-responsive">';
videolist += '</a>';
videolist += '</div>';
}
videolist += '<div class="headline element_spacing_small">';
videolist += '<h4 class="title"><a class="ytvideo" href="#'+vid+'">'+title+'</a></h4>';
videolist += '</div>';
videolist += '</li>';
}
});
//$("#featured").html(videofeatured);
jQuery("#featuredheadline").html(videofeaturedheadline);
jQuery("#videoContent").html(videolist);
});
jQuery(document).ready(function(){
jQuery('#videoContent').on("click", "a.ytvideo", function() {
var vid = jQuery(this).attr('href').replace("#","");
var vidtitle = jQuery('li#vid'+vid+' h4.title a').text();
var loadingImage = '<img alt="Loading..." src="http://media.miamiherald.com/static/images/escenic-images/ajax-loader_media-assets.gif"/>';
jQuery('div#featured').html(loadingImage);
jQuery('div#featured').empty().append('<div id="ytplayer"></div>');
loadPlayer(vid, 'ytplayer', '316', '257');
videohead = '<h4 class="title">'+vidtitle+'</h4>';
//jQuery('div#featured').html(videoembed);
jQuery("#featuredheadline").html(videohead);
jQuery('#videoContent li.media').show();
jQuery('#videoContent li#vid'+vid).hide();
jQuery("html, body").scrollTop(jQuery('div#featured').offset().top);
return false;
});
});
function loadPlayer(vid, elemId, width, height) {
if(document.getElementById('youtube_js')===null){
var tag=document.createElement('script');
tag.setAttribute('id','youtube_js');
tag.src="https://www.youtube.com/iframe_api";
var firstScriptTag=document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag,firstScriptTag);
}
// create youtube player if API is loaded
try {
player = new YT.Player(elemId, {
height: height,
width: width,
videoId: vid,
});
} catch (e) {
console.log('API not loaded, will create player using onYouTubeIframeAPIReady');
}
window.onYouTubeIframeAPIReady=function(){
player = new YT.Player(elemId, {
height: height,
width: width,
videoId: vid,
});
};
}
</script>
<div id="market-mh-video-vertical">
