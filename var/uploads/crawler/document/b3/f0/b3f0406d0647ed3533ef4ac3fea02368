<script>
// add/remove toggle-by-click functionality for mobile screen sizes
function setMenus() {
if (jQuery(window).width() > 1200) {
jQuery(".navbar-nav > .dropdown > a").attr("data-toggle", "");
} else {
jQuery(".navbar-nav > .dropdown > a").attr("data-toggle", "dropdown");
}
}
jQuery(setMenus);
jQuery(window).resize(setMenus);
// navbar top-level links need data-target attribute or we get ugly jquery syntax errors
jQuery('.dropdown > a').attr('data-target','#');
if (location.hostname.split('.')[1] == 'elnuevoherald') {
jQuery('#site-search .input-sm').attr('placeholder','Búsqueda');
jQuery('#site-search .btn-submit').text('Búsqueda');
jQuery('#header-nav .navbar-header .navbar-toggle').prepend('<span class="collapse-label visible-sm">Secciones</span>');
} else {
// add Sections label to nav toggle for small breakpoint
jQuery('#header-nav .navbar-header .navbar-toggle').prepend('<span class="collapse-label visible-sm">Sections</span>');
}
// delay hover effect on lg breakpoint
jQuery("#header-nav .megadropdown").mouseenter(function(){
if ($(window).width() > 1200) {
clearTimeout(jQuery("#header-nav").data('navCollapseTimeout'));
currentNav = jQuery(this);
navExpandTimeout = setTimeout(function(){
jQuery("#header-nav .megadropdown").removeClass('open');
currentNav.addClass('open');
}, 100);
jQuery("#header-nav").data('navExpandTimeout', navExpandTimeout);
}
}).mouseleave(function() {
if ($(window).width() > 1200) {
clearTimeout(jQuery("#header-nav").data('navExpandTimeout'));
navCollapseTimeout = setTimeout(function(){
jQuery("#header-nav .megadropdown").removeClass('open');
}, 100);
jQuery("#header-nav").data('navCollapseTimeout', navCollapseTimeout);
}
});
</script>
