<script type="text/javascript">
        $(document).ready(function(){
                $('.hometoggle').click(function (){
                        $(this).scrollTo();
                        var toggle = $(this).attr('toggle');
                        $('.hometoggle, .hometoggle span').removeClass('on');
                        $(this).addClass('on').children("span").addClass("on");

                        FetchHomepageFeed(toggle);

                        return false;
                });

                $('.fundedbf_paging').live('click', function(){
                        var params = "&id="+$(this).attr('id')+"&i="+$(this).attr('i')+"&sid="+$(this).attr('data-sid');
                        FetchHomepageFeed("fundedbf", params);
                        $(".c").scrollTo();
                        return false;
                });

                $('.nearme_paging').live('click', function(){
                        var params = "&id="+$(this).attr('id')+"&i="+$(this).attr('i')+"&zip="+$(this).attr('zip')+"&country="+$(this).attr('country');
                        FetchHomepageFeed("nearme", params);
                        $(".c").scrollTo();
                        return false;
                });

                if($().on !== undefined){

                        $(".togglediv").on("click",".refreshToggle",function(){
                                $(".hometoggle.on").click();
                                return false;
                        });
                }
        });
        function FetchHomepageFeed(toggle,additional_params){
                if (additional_params == null){
                        additional_params = "";
                }

                $('.togglediv').hide();
                $('.toggle_loading').fadeIn();
                var html = $.ajax({
                        url:'//www.gofundme.com/mvc.php?route=index/hometoggle&toggle='+toggle+additional_params,
                        async:true,
                        type:"GET",
                        success: function(togglehtml) {
                                $(".togglediv").fadeOut(400,function(){
                                        $(".togglediv").html(togglehtml);
                                        $('.toggle_loading').hide();
                                        $(".togglediv").fadeIn();
                                });;
                        }

                });
        }

        function show_editnear(){
                //$('#div_neartitle').hide();
                $('#div_editnearme').slideToggle(400, function(){
                        $('#nearme_input').val($('#nearme_input').attr('title'));
                });
        }
