<script type="text/javascript">
        $(document).ready(function(){
                $('.video-link').click(function ()
                {
                        if($(this).attr("data-videourl") !== undefined){
                                showHowItWorks($(this).attr("data-videourl"));

                        }else{
                                showHowItWorks();
                        }
                        return false;
                });

                $('.close-video-link').click(function ()
                {
                        hideHowItWorks();
                        return false;
                });

                $(".gfm_lb").click(function(){
                        hideHowItWorks();
                        return false;
                });

                $(".gfm_lb_main").click(false);

                        });

        function showHowItWorks(url){
                url = url || "//www.youtube.com/embed/ADrc7JSs_5s?showinfo=0&autoplay=1&modestbranding=1&rel=0";
                $("html body").scrollTo();
                $("#videoLightbox").fadeIn(400,function(){
                        $('#lightboxedVideo').attr('src',url);
                });

                $(".lb_overlay").fadeIn();
        }

        function hideHowItWorks(){
                $('#lightboxedVideo').attr('src',"");
                $('#videoLightbox').fadeOut();
                $(".lb_overlay").fadeOut();
        }
</script>

<script>
        $(".forceFbToggle").click(function(){
                $("#fbToggle").click();
                return false;
        });
</script>

