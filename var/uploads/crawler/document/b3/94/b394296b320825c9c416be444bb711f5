					$(document).ready(function()
					{
						var postbackElement;

						Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(function(sender, args)
						{
							try { postbackElement = args.get_postBackElement(); }
							catch (err) { }
						});

						var quickViewDialog = new WebLinc.quickViewDialog({ autoOpen: false}, $('#quickviewDialog'));
						Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function(sender, args){
							if(args._response._aborted) {
								return;
							}
							if (typeof (postbackElement) === "undefined")
								return;
							else if (postbackElement != null && (postbackElement.id.indexOf("lbQuickView") != -1 || postbackElement.id.indexOf("btnChooseOptions") != -1))
								quickViewDialog.dialog.dialog('open');
						});
					});
				
