		$(document).ready(function(){
			$("#breadcrumbs").hide();
        
			$('.row, .row .col1').equalHeightColumns();     
			
			WEBLINC.initOptionMenus($('.optionmenus .optionmenu-skintype > ul'), 'optionmenu-skinconcern1', true);
			WEBLINC.initOptionMenus($('.optionmenus #skincarecategory > ul'), 'optionmenu-category', true);
			WEBLINC.initOptionMenus($('.optionmenus #makeupcategory > ul'), 'makeupCategory', true);
			WEBLINC.initOptionMenus($('.optionmenus .optionmenu-skinconcern > ul'), 'optionmenu-skinconcern2', true);
			WEBLINC.toggleProductFinderOptions();	
        

            $('.quiz .optionmenus .options a').click(function(val) {
            
                if($(this).closest('ul.options').attr('class').indexOf('options-open') > -1)
                {
                    //css is open and clicking to close
                    
                    // Standard GA Event
                    _gaq.push(['_trackEvent', 'HomePageProductFinder', 'RefinementSelected', $(this).html()]);

                    // Universal GA Event
                    ga('send', 'event', 'HomePageProductFinder', 'RefinementSelected', $(this).html());
                }
                
            });
		});

        //Added Google Analytics for products
        $(document).mouseup(function(e) {
            var t = $(e.target);
            if(t.is("a") || t.is("img")) {
            var pLI = t.closest('li[class*="jcarousel-item"]');
            if(pLI.length != 0) {
                var productTag = pLI.find('a[id*="_hlName"]');
                if(productTag != null) {
                    var productName = productTag.html();
                        
                        // Standard GA Event
                        _gaq.push(['_trackEvent', 'HomePageProductFinder', 'ProductSelected', productName]);

                        // Universal GA Event
                        ga('send', 'event', 'HomePageProductFinder', 'ProductSelected', productName);            
                    }
                }
            }
        });
	
</script>
