    function test() {
        document.frmLogon.viewOnlyLogon.value = "";

        if (Trim(document.frmLogon.UserName.value) == "") {
            alert('Please enter your user name.');
            document.frmLogon.UserName.focus();
            return false;
        }
        if (Trim(document.frmLogon.Password.value) == "") {
            alert('Please enter your password.');
            document.frmLogon.Password.focus();
            return false;
        }

        return true;
    }
