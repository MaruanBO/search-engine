<!-- News is the homepage -->
<script type="text/javascript">
var searchCategoryTerms = "";
var fromDate = "", toDate = "", filter="", dateOut = "", containsOut = "";
try{
var hashValue = window.location.hash;
var authorStr = "", pubStr = "", sectionStr = "";
if (hashValue.indexOf("navigators") > -1){
var startIndex = hashValue.indexOf("navigators");
var endIndex = hashValue.indexOf('&', startIndex + 1);
searchCategoryTerms = hashValue.substring(startIndex, endIndex).replace("navigators=", "");
var sctItems = searchCategoryTerms.split("|");
for(var i = 0; i < sctItems.length; i++) {
if (sctItems[i].indexOf("publication_id:tto") > -1 || sctItems[i].indexOf("publication_id:sto") > -1){
pubStr = sctItems[i].replace("publication_id:", "publication:");
}
if (sctItems[i].indexOf("sectionname") > -1){
sectionStr += (sctItems[i].split(":"))[1] + ",";
}
if (sctItems[i].indexOf("article_authors:") > -1){
authorStr = (sctItems[i].split(":"))[1];
}
}
if (pubStr != "") { pubStr += "|"; }
if (authorStr != "") { authorStr = "journalist:" + authorStr.replace("@", ",") + "|"; }
if (sectionStr != "") { sectionStr = "section:" + sectionStr + "|"; }
}
if (hashValue.indexOf("from=") > -1){
var startIndex = hashValue.indexOf("from=");
var endIndex = hashValue.indexOf('&', startIndex + 1);
fromDate = hashValue.substring(startIndex, endIndex);
dateOut = "date:" + fromDate + "|";
}
if (hashValue.indexOf("to=") > -1){
var startIndex = hashValue.indexOf("to=");
var endIndex = hashValue.indexOf('&', startIndex + 1);
toDate = hashValue.substring(startIndex, endIndex);
dateOut += "date:" + toDate + "|";
}
if (hashValue.indexOf("filters=") > -1){
var startIndex = hashValue.indexOf("filters=");
var endIndex = hashValue.indexOf('&', startIndex + 1);
filters = hashValue.substring(startIndex, endIndex);
if (filters.indexOf("date_published_today") > -1){ dateOut = "date:today|"; }
if (filters.indexOf("date_published_7days") > -1){ dateOut += "date:last 7 days|"; }
if (filters.indexOf("date_published_30days") > -1){ dateOut += "date:last 30 days|"; }
if (filters.indexOf("contains_video") > -1){ containsOut = "contains:video|"; }
if (filters.indexOf("contains_photogallery") > -1){ containsOut += "contains:photogallery|"; }
if (filters.indexOf("contains_graphic") > -1){ containsOut += "contains:graphic|"; }
}
searchCategoryTerms = (pubStr + sectionStr + authorStr + dateOut + containsOut).replace(",|", "|");
}catch(err){}
</script>
