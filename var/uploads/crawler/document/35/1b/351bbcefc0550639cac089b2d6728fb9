	<script>
			function htmlEscape(str) {
				return String(str)
					.replace(/</g, '')
					.replace(/>/g, '');
			}
			
		$( "form[name=srchFrm]" ).submit(function( event ) {
			var submitVal = $("input[name=keyword]").val();
			var encodedVal = encodeURIComponent(htmlEscape(submitVal));
			$( "input[name=keyword]" ).hide();
			$( "input[name=keyword]" ).val(encodedVal);
			return;
		});
	
	</script>
