        //    $("#waitloader").hide();
        //});
        $(document).ready(function () {

            //add processing handlers
            $(document).ajaxStart(function () {
                $("#waitloader").show();
            });


            $(document).ajaxStop(function () {
                $("#waitloader").hide();
            });
            $(document).ajaxComplete(function () {
                $("#waitloader").hide();
            });



            if ($("#HdIsAuthenticatedUser").val() == "True") {
                $("#SessionPopupDelayValue").val(1140000)
                SessionTimeout.schedulePopup(1140000);

            }
            var url = window.location.href;

            //            if (url.indexOf("AE_AboutECP") > -1 || url.indexOf("FAQ") > -1 || url.indexOf("AE_Feedback") > -1 || url.indexOf("AE_PrivacyPolicy") > -1 || url.indexOf("AE_SitePolicy") > -1) {
            if (url.indexOf("AE_AboutECP") > -1 || url.indexOf("FAQ") > -1 ) {
                ShowSelectedMenu('mnAboutUs', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("AccountSummary") > -1 || url.indexOf("AccountHome") > -1) {
                ShowSelectedMenu('mnAccountSummary', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("CardApply") > -1 || url.indexOf("CP_FeaturesBenefits") > -1
                || url.indexOf("CP_MilStartRewardsMaster") > -1
                || url.indexOf("CP_MilStartCard") > -1 || url.indexOf("CP_TakeItHomeCard") > -1
                || url.indexOf("CP_CreditCardComparison") > -1) {
                ShowSelectedMenu('mnCreditCards', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("CustomerAds") > -1) {
                ShowSelectedMenu('mnPromotion', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("MR_DPolicy") > -1 || url.indexOf("MR_CBTips") > -1) {
                ShowSelectedMenu('mnResources', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("ContactUs") > -1) {
                ShowSelectedMenu('mnContactUs', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("AE_PrivacyPolicy") > -1) {
                ShowSelectedMenu('mnContactUs', 'active', 'dropdown-toggle');
            }
            else if (url.indexOf("AE_SitePolicy") > -1) {
                ShowSelectedMenu('mnContactUs', 'active', 'dropdown-toggle');
            }

            $("[data-toggle='tooltip']").tooltip({ html: true });
            $("#waitloader").hide();

            //            debugger;
            
            if ('False' == "True") {
                $('#chatwindow').load('/Chat/LaunchChatWindow');
            }
        });
