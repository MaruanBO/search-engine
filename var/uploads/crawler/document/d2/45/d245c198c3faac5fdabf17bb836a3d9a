$(function(){
	$('#slides2').slides({
		preload: false,
		hoverPause: true,
		play:5000,
		pause:5000,
		effect:'fade',
		generatePagination:false,
		crossfade:true
	});
	$('.newhpnav > ul > li').mouseenter(function(e){
		var hsa = $('.has-sub');
		for (i=0;i<hsa.length;i++) {
			if (hsa[i]!=this) {
				$(hsa[i]).removeClass('hactive');
			} else {
				$(hsa[i]).addClass('hactive');
				var cp=$(hsa[i]).position().top; var sh=$(hsa[i]).find('ul').eq(0).height(); var ph=$(hsa[i]).parent().height();
				if (sh>=ph) {pos=-(cp);} else if (cp+sh>ph) {pos=(ph-cp-sh);} else {pos=0;}; $(hsa[i]).find('ul').eq(0).css({'top':pos});
			}
		}
	});	
	$('.newhpnav').mouseleave(function(){$('.has-sub.hactive').removeClass('hactive');});
});
