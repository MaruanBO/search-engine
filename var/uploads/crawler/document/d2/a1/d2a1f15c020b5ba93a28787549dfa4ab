                        <script>function ccsNewsletter()
{if(document.getElementById('ccs-newsletter').checkValidity())
{var button=jQuery('button[type="submit"]','#ccs-newsletter');button.attr('disabled','disabled').text('Processing...');jQuery.ajax({type:'POST',url:'/ccs-newsletter.php',data:{'email':jQuery('input[name="email"]').val()},success:function(data)
{var res=JSON.parse(data);if(!res.error){button.text('You have been signed up');}else{var message=res.error;if('-100'==res.code)message='You must add a valid email address';button.addClass('btn-ccs-red').removeClass('btn-ccs-blue').text(message);}}}).error(function(err){button.addClass('btn-ccs-red').removeClass('btn-ccs-blue').attr('disabled',true).text('There was an error sending your request');});return false;}}</script>
                    </div>
                </div>
            </div>
