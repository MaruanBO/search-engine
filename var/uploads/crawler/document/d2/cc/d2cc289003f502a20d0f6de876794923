function ISOKForMe()
{
	if (navigator.appName== "Netscape" && parseInt(navigator.appVersion) >= 3 )
	{
		return true;;
	}
	if (navigator.appName == "Microsoft Internet Explorer" && parseFloat( navigator.appVersion ) >= 3.01 )
	{
		return true;;
	}
	 
	return false;;
}
 
 
if ( ISOKForMe() )
{
	var standard = new MakeArray(15);
	standard[1].src = "Images/TOCBtns/home.gif";
	standard[2].src = "Images/TOCBtns/homeOn.gif";
	standard[3].src = "Images/TOCBtns/signup.gif";
	standard[4].src = "Images/TOCBtns/signupOn.gif";
	standard[5].src = "Images/TOCBtns/help.gif";
	standard[6].src = "Images/TOCBtns/helpOn.gif";
	standard[7].src = "Images/TOCBtns/qStart.gif";
	standard[8].src = "Images/TOCBtns/qStartOn.gif";
	standard[9].src = "Images/TOCBtns/faq.gif";
	standard[10].src = "Images/TOCBtns/faqOn.gif";
	standard[11].src = "Images/TOCBtns/privacy.gif";
	standard[12].src = "Images/TOCBtns/privacyOn.gif";
	standard[13].src = "Images/TOCBtns/contact.gif";
	standard[14].src = "Images/TOCBtns/contactOn.gif";
}

function mouseOverTTOC(newImag)
{
	if ( ISOKForMe() )
	{
		var iMid = "Lmm1" + newImag;
		document.images[ iMid ].src = standard[((newImag-1) * 2) + 2].src;
	}
	 
}
 
function mouseOutTOC(newImag)
{
	if ( ISOKForMe() )
	{
		var iMid = "Lmm1" + newImag;
		document.images[ iMid ].src = standard[((newImag-1) * 2) + 1].src;
	}
	 
}
 
function MakeArray(n)
{
	if ( ISOKForMe() )
	{
		this.length = n;
		for (var i = 1; i<=n; i++)
		{
			this[i] = new Image();
		}
		 
		return this;
	}
	 
}
 
function validateChangePW(theForm)
{
	if (theForm.password1.value.length < 6 )
	{
		alert("The password must be 6 characters or more.");
		theForm.password1.focus();
		return (false);
	}
	if ( escape( theForm.password1.value ) != theForm.password1.value )
	{
		alert("Please only use numbers, letters, and the underscore character inside passwords.");
		theForm.password1.focus();
		return (false);
	}
	if (theForm.password2.value != theForm.password1.value )
	{
		alert("Your requested password does not match with the confirmation password. Please retype them both to insure correctness.");
		theForm.password1.focus();
		return (false);
	}
	 
	return (true);
}

function validateSignup(theForm)
{
	if (theForm.first.value == "")
	{
		alert("Please enter your first name into the text field so we can address you casually.");
		theForm.first.focus();
		return (false);
	}
	if (theForm.last.value == "")
	{
		alert("Please enter your last name into the text field so we can address you politely.");
		theForm.last.focus();
		return (false);
	}
	 
	if (theForm.uName.value == "")
	{
		alert("Your account cannot be created without a login name. Please enter one into the text field.");
		theForm.uName.focus();
		return (false);
	}
	 
	if ( theForm.uName.value.indexOf( " ", 0 ) != -1 )
	{
		alert("Sorry, but spaces are not permitted in Login Names. Please choose a different name and try again.");
		theForm.uName.focus();
		return (false);
	}
	 
	if (theForm.password.value == "")
	{
		alert("Your account cannot be created without a password, please enter one now.");
		theForm.password.focus();
		return (false);
	}
	 
	if (theForm.cpassword.value == "")
	{
		alert("Please retype the password into the 'confirm password' text field. This minimizes the chances of a typographical error.");
		theForm.cpassword.focus();
		return (false);
	}
	 
	if (theForm.password.value != theForm.cpassword.value)
	{
		alert("Your requested password does not match with the confirmation password. Please retype them both to insure correctness.");
		theForm.cpassword.focus();
		return (false);
	}
	 
	if (theForm.emailAddr.value == "")
	{
		alert("Please enter your email address, we will not give this out. We only use it to help with forgotten passwords.");
		theForm.emailAddr.focus();
		return (false);
	}
	if (theForm.emailAddr2.value == "")
	{
		alert("Please enter your email address into the 'Confirm Email' text field, we only make you confirm to minimize typo's.");
		theForm.emailAddr2.focus();
		return (false);
	}
	 
	if ( theForm.emailAddr.value.indexOf( "@", 0 ) == -1 )
	{
		alert("This does not seem like a valid email address. Please try again. Your email address will not be given out, we only use it to help with forgotten passwords.");
		theForm.emailAddr.focus();
		return (false);
	}
	if (theForm.emailAddr.value != theForm.emailAddr2.value)
	{
		alert("Your Email Address does not match with the confirmation Email Address. Please retype them both to insure correctness.");
		theForm.emailAddr2.focus();
		return (false);
	}
	 
	 
	return (true);
}


//-->
