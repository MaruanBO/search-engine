                    <script>
                        $(document).ready(function () {
                            $('#srchForm #Ntt').keydown(function (event) {
                                if (event.keyCode == 13) {
                                    validateSearchKeyword($(this).val());
                                    this.form.submit();
                                    return false;
                                }
                            });
                        });
                        function validateSearchKeyword(inputText) {
                            if (inputText.search(/[A-Za-z0-9]/) != -1 && inputText != 'Search') {
                                inputText.replace("\n", " ", "g");
                                inputText.replace("\r", " ", "g");
                                try {
                                    s = s_gi(s_account);
                                    s.linkTrackVars = "eVar9,prop9,events";
                                    s.linkTrackEvents = "event2";
                                    s.prop9 = s.eVar9 = inputText;
                                    s.tl(this, 'o', "Search");
                                    s.prop9 = s.eVar9 = "";
                                } catch (e) { }
                                $("#srchForm").attr("action", "/us/searchresults-tai?Ntt=" + inputText);
                                return true;
                            }
                            alert("Please enter a keyword or phrase to search.");
                            return false;
                        }
                    </script>
