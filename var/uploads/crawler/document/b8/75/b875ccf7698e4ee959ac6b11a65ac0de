<script type="text/javascript">
function staticUpdates(){
	var osphone=document.cookie.replace(/.*OSPhone=([^;]+).*/,"$1").trim();
	if((/^\d{3}.?\d{3}.?\d{4}$/).test(osphone) && jQuery("#Contact-Us-Btn").length>0) 
		jQuery("#Contact-Us-Btn").html(
			jQuery("#Contact-Us-Btn").html().replace(
				jQuery("#Contact-Us-Btn").html().match(/(\d{3}.?\d{3}.?\d{4})/)[0],osphone));
	var isLoggedIn=(/LoggedIn=true;/).test(document.cookie);
	if(isLoggedIn){
		var fullname=document.cookie.replace(/.*tracking.fullname=([^;]+).*/,"$1");
		if(fullname!=document.cookie){
			fullname = fullname.trim().replace(/^\"/,"").replace(/\"$/, "");
			if(jQuery("#Cust-Links a[href*=myaccount]").length > 0){
				jQuery("#Cust-Links a[href*=myaccount]").attr("href","/us/myaccount.to").html("Welcome, "+fullname);
			}else{
				var welcome="<a href=\"/us/myaccount.to\">Welcome, "+fullname+"</a><span class=\"link-sep\"></span>";
				jQuery(welcome).insertBefore(jQuery("#Cust-Links").children()[0]);
			}
		} else {
			jQuery("#Cust-Links a[href*=myaccount]").next(".link-sep").remove();
			jQuery("#Cust-Links a[href*=myaccount]").remove();
		}
		jQuery("#Cust-Links a[href*=log]").attr("href","/us/logout.to").html("Log Out");
	} else {
		jQuery("#Cust-Links a[href*=myaccount]").next(".link-sep").remove();
		jQuery("#Cust-Links a[href*=myaccount]").remove();
		jQuery("#Cust-Links a[href*=log]").attr("href","/us/showlogin.to").html("Log In");
	}
	var dend=new Date();dend.setDate(dend.getDate()-1);
	var dnew=new Date();dnew.setDate(dnew.getDate()+60); 
	if((/^.*\/us\/?(\?.*)?$/).test(document.location.href)) {
		document.cookie="segmentValue=;expires="+dend.toGMTString()+";path=/";
		document.cookie="segmentValue=homepage;expires="+dnew.toGMTString()+";path=/";
	}
	segRegex = /^.*\/us\/(business|enterprise|education|healthcare)\/?(\?.*)?$/;
	if(segRegex.test(document.location.href)){
		sValue = document.location.href.toLowerCase().replace(segRegex, "$1");
		document.cookie="segmentValue=;expires="+dend.toGMTString()+";path=/";
		document.cookie="segmentValue=" + sValue + ";expires="+dnew.toGMTString()+";path=/";
	}
	if(!(/segmentValue=[^;]+?;?/).test(document.cookie)) {
		document.cookie="segmentValue=homepage;expires="+dnew.toGMTString()+";path=/";
	}
}
staticUpdates();
if(location.search!="") jQuery.ajax({cache:false,url:"/us/catchParms"+location.search,success:function(data,textStatus){staticUpdates();}});
var dend=new Date();dend.setDate(dend.getDate()-1);
var dnew=new Date();dnew.setDate(dnew.getDate()+60); 
jQuery("#Segment-Nav a").each(function(){
	jQuery(this).click(function(){
		sValue = this.id.replace("-Seg","").replace("-Office","page").toLowerCase();
		document.cookie="segmentValue=;expires="+dend.toGMTString()+";path=/";
		document.cookie="segmentValue=" + sValue + ";expires="+dnew.toGMTString()+";path=/";
	})
});	
</script>
</html>
  
