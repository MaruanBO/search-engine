    <script>
        $(window).load(function () {
            $("#Featured-Categories #Laptops").hover(function () {
                if (!$(this).find('img').hasClass('animated')) {
                    $(this).find('img').dequeue().stop().animate({
                        'width': 340,
                        'height': 280,
                        'top': -85,
                        'left': -125
                    }, 100, "linear");
                }
            }, function () {
                $(this).find('img').addClass('animated').animate({
                    'width': 315,
                    'height': 259,
                    'top': -70,
                    'left': -113
                }, 100, "linear", function () {
                    $(this).removeClass('animated').dequeue();
                });
            });

            $("#Featured-Categories #Business-Laptops").hover(function () {
                if (!$(this).find('img').hasClass('animated')) {
                    $(this).find('img').dequeue().stop().animate({
                        'width': 340,
                        'height': 280,
                        'top': -85,
                        'left': -54
                    }, 100, "linear");
                }
            }, function () {
                $(this).find('img').addClass('animated').animate({
                    'width': 315,
                    'height': 259,
                    'top': -73,
                    'left': -44
                }, 100, "linear", function () {
                    $(this).removeClass('animated').dequeue();
                });
            });

            $("#Featured-Categories #Two-In-One").hover(function () {
                if (!$(this).find('img').hasClass('animated')) {
                    $(this).find('img').dequeue().stop().animate({
                        'width': 242,
                        'height': 179,
                        'top': -9,
                        'left': -31
                    }, 100, "linear");
                }
            }, function () {
                $(this).find('img').addClass('animated').animate({
                    'width': 205,
                    'height': 152,
                    'top': 10,
                    'left': -2
                }, 100, "linear", function () {
                    $(this).removeClass('animated').dequeue();
                });
            });

            $("#Featured-Categories #Tablets").hover(function () {
                if (!$(this).find('img').hasClass('animated')) {
                    $(this).find('img').dequeue().stop().animate({
                        'width': 320,
                        'height': 264,
                        'top': -80,
                        'left': -48
                    }, 100, "linear");
                }
            }, function () {
                $(this).find('img').addClass('animated').animate({
                    'width': 290,
                    'height': 239,
                    'top': -65,
                    'left': -39
                }, 100, "linear", function () {
                    $(this).removeClass('animated').dequeue();
                });
            });
        });

    </script>
       
