    <script type="text/javascript">
        var slideCounter = new Array();
        var slideNames = new Array();
        var s_account = "toshibadirectprod,toshibaglobal";

        function slideClicked(slideName) {
            try {
                omtr.linkTrackVars = "pageName,eVar28,events";
                omtr.linkTrackEvents = "event39";
                omtr.prop28 = omtr.eVar28 = slideName;
                omtr.events = "event39";
                omtr.tl(this, "o", "Ad Impressions");
            } catch (e) { }
        }
        function slideViewed(slideName) {
            if (typeof (slideCounter[slideName]) == "undefined") {
                slideCounter[slideName] = 1;
                slideNames.push(slideName);
            } else {
                slideCounter[slideName]++;
            }
        }

        jQuery(function ($) {
            // if omniture code exists
            if (!(typeof (omtr) == "undefined")) {
                // add unload function to capture slide impressions
                $(window).unload(function () {
                    try {
                        while (slideNames.length < 0) {
                            continue;
                        }
                        if (slideNames.length > 0) {
                            omtr.linkTrackVars = "pageName,products,events";
                            omtr.linkTrackEvents = "event38";
                            if ((typeof (omtr.products) == "undefined")) omtr.products = "";
                            for (i = 0; i < slideNames.length; i++) {
                                if (omtr.products != "") omtr.products += ",";
                                omtr.products += ";"
                  + slideNames[i]
                  + ";;;event38="
                  + slideCounter[slideNames[i]];
                            }
                            omtr.events = "event38";
                            omtr.tl(this, "o", "Ad Impressions");
                        }
                    } catch (e) { }
                });
            }

            /* start rotator */

            var rotator_events = {
                _createCode: function (name, id) {
                    return 'Homepage_Billboard_' + name;
                },
                viewed: [],
                onChange: function (data) {
                    if (!this.viewed[data.slides.current.name]) {
                        slideViewed(this._createCode(data.slides.current.name, data.slides.current.id));
                        this.viewed[data.slides.current.name] = true;
                    }
                },
                onClick: function (data) {
                    data.event.preventDefault();
                    slideClicked(this._createCode(data.slides.current.name, data.slides.current.id));
                    if (data.event.target.target == '_blank') {
                        window.open(data.event.target.href, '_blank');
                    }
                    else {
                        location.href = data.event.target.href;
                    }
                    data.rotator.stop();
                },
                onStart: function (data) {
                    this.onChange(data);
                }
            };

            // start the rotators

            if (checkVersion()) {
                var rotator = new $.Rotator('#Rotator', {
                    interval: 10000, slideNameAttr: 'id', zIndex: 'auto', random: false, transition: 'none', events: rotator_events,
                    nav: { numbers: true, next: true, prev: true, position: 'inside' }
                });
                var rotator = new $.Rotator('#Rotating-Ads', {
                    interval: 10000, slideNameAttr: 'id', zIndex: 'auto', random: false, transition: 'none', events: rotator_events, nav: { numbers: false }
                });
            }
            else {
                var rotator = new $.Rotator('#Rotator', {
                    interval: 10000, slideNameAttr: 'id', zIndex: 'auto', random: false, transition: 'fade', events: rotator_events,
                    nav: { numbers: true, next: true, prev: true, position: 'inside' }
                });
                var rotator = new $.Rotator('#Rotating-Ads', {
                    interval: 10000, slideNameAttr: 'id', zIndex: 'auto', random: false, transition: 'fade', events: rotator_events, nav: { numbers: false }
                });
            }
            // end rotators

        });

        function getInternetExplorerVersion() {
            var rv = -1; // Return value assumes failure.
            if (navigator.appName == 'Microsoft Internet Explorer') {
                var ua = navigator.userAgent;
                var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
                if (re.exec(ua) != null)
                    rv = parseFloat(RegExp.$1);
            }
            return rv;
        }

        function checkVersion() {
            var msg = "You're not using Windows Internet Explorer.";
            var ver = getInternetExplorerVersion();
            if (ver > -1) {
                if (ver >= 8 || ver <= 11)
                    return true;
                else
                    return false;
            }
        }
    </script>
