<script>

    function loginSignupUpdate(clickedElement, newUser) {
        var parentForm = $(clickedElement).parents('form');

        $('.login_section', parentForm).toggle(!newUser);
        $('.register_section', parentForm).toggle(newUser);
        $('input[id=ExistingUserRadio]').val(!newUser);


        var headerText = newUser ? "Sign Up" : "Welcome Back";
        $('.full-width-header').html("<h2>" + headerText + "</h2>");
        if (newUser) {
            $("#recaptcha").hide();
            $(".register_section select.state").val(" ");
        } else {
            if ($.cookie("cpt") != null) {
                $("#recaptcha").show();
                Recaptcha.create(vs.recaptchaPublicKey, "recaptcha", { callback: Recaptcha.focus_response_field });
            }
            $(".register_section select.state").val("AL");
        }
        headerText = newUser ? "Sign Up Now" : "Please Sign In";
        $('#signup-popup header').html(headerText);

        
        try
        {
            if (existingUserLoginWarning !=null && existingUserLoginWarning.length > 0)
            {
                if (!newUser){
                    var WarningBox=existingUserLoginWarning;
                }
                else
                    WarningBox='';
                $('#LoginWarningMessage').html(WarningBox);
            }
        }
        catch(e)
        {}
    }


</script>

