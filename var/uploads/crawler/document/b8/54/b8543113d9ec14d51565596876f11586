<script language="javascript" type="text/javascript">
    var armedForcesAbbrs = ['AA', 'AE', 'AP'];
    var stateLookup = {"1":["AA","AE","AK","AL","AP","AR","CA","CO","CT","DC","DE","FL","GA","HI","ID","IL","IN","KS","KY","MA","MD","ME","MI","MN","MO","MS","NC","ND","NE","NH","NJ","NM","NV","NY","OH","OK","OR","PA","RI","SC","SD","TN","TX","UT","VA","VT","WI","WV","WY"],"2":["AB","BC","MB","NB","NL","NS","NT","NU","ON","PE","QC","SK","YT"]};
    function loadStateDropdown(countryId) {
        $('.register_section select.state').
            html($.map(stateLookup[countryId],
                function(e, i) {
                    var selected = '';
                    if (countryId == 1) {
                        if (e === 'AL') {
                            selected = ' selected="selected"';
                        }
                        if ($.inArray(e, armedForcesAbbrs) < 0) {
                            return '<option value="' + e + '"' + selected + '>' + e + '</option>';
                        }
                        return '';
                    }
                    return '<option value="' + e + '">' + e + '</option>';
                }).join(''));
        if (countryId == 1) {
            $('.register_section select.state').append('<option value="--" disabled="true">--</option>');
            $.each(armedForcesAbbrs, function(index, value) {
                $('.register_section select.state').append('<option value="' + value + '">' + value + '</option>');
            });
        }
        $('.register_section select.state').prepend('<option value="  ">  </option>');
        $('.register_section select.state').val(" ");
    }
    
    loadStateDropdown(1);
    $(document).ready(function() {
        loginSignupUpdate($('.register_section select.state'), true);
    });


</script>
