      require(['jquery', 'sp-content/tooltip', 'sp-content/popover'], function() {
          var bindTooltipsAndPopovers = function() {
              var $tooltipActivators = jQuery('img.tooltip-activator, span.tooltip-activator, a.tooltip-activator');
              if($tooltipActivators.length > 0) {
                  $tooltipActivators.tooltip('mouseenter');
              }

