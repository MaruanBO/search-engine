      <script language="javascript" >
            function disableEnterKey(e) {
                var key;
                if (window.event)
                    key = window.event.keyCode; //IE
                else
                    key = e.which; //firefox      

                return (key != 13);
            }
        

            function doButtonPostBack() {

                var myText = document.getElementById('ctl00_textsearch').value;
                var RB1 = document.getElementById("ctl00_rbtnsearchtype");
                var radio = RB1.getElementsByTagName("input");
                var isChecked = false;
                var val = '';

                for (var i = 0; i < radio.length; i++) {
                    if (radio[i].checked) {
                        isChecked = true;
                        val = radio[i].value;
                        break;
                    }
                }
                                                                  
                var myurl = "searchresult.aspx?item=" + myText + "&type=" + val ;        
                location.href = myurl;
            }      
     
        </script>                
