        function ToUpper(ctrl) {
            var t = ctrl.value;
            ctrl.value = t.toUpperCase();
        }

        function ToLower(ctrl) {
            var t = ctrl.value;
            ctrl.value = t.toLowerCase();
        }

        function closePanel2() {
            $find("collapsed2")._doClose();
        }

        function openPanel2() {
            $find("collapsed2")._doOpen();
        }

        function closePanel() {
            $find("collapsed")._doClose();
        }

        function openPanel() {
            $find("collapsed")._doOpen();
        }

        function pageLoad() {
            $find("BehaviorID")._doOpen();
