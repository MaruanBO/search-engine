$(".tooltip-homepage").tooltip({ position: "top center" ,effect: "fade", offset:[0,50]}).dynamic({ bottom: { direction: 'down' } });
});

function random_rotate(_min,_max,id,rel_url,stm){
	var TOTAL = (_max + 1) - _min;
	var f = Math.floor(Math.random()*(TOTAL))+ _min;
	f = f < 10 ? "00" + f : f < 100 ? "0" + f : f;
	f = rel_url+stm+f+".stm";
	if(window.ActiveXObject){
	xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}
	else if (window.XMLHttpRequest)
	 {
	 xmlhttp=new XMLHttpRequest();
	 }
	xmlhttp.open("GET",f,false);
	xmlhttp.send();
		if(xmlhttp.status==200){
 	 document.getElementById(id).innerHTML=xmlhttp.responseText;
		}
}
</script>

