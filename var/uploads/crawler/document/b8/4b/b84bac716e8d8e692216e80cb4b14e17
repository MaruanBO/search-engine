<script type="text/javascript">
(function(jQuery){
      (function( $, undefined ) {
    $(function() {
        var ea_server_url = document.location.protocol + '//'+ document.location.host;
        var product_url =   document.location.protocol+ '//' + document.location.host ;
        var acOptions = {
            dict: 'TrueValue',
            id: 'keyword',
            prompt: 'Enter product name, number, brand, etc.',
            server: ea_server_url,
            serverSearch: ea_server_url,
            url: '/catalog/easyask_proxy.cmd',
            urlSearch: '/catalog/easyask_proxy.cmd?indexed=1&oneshot=1&ie=UTF-8&disp=json&RequestAction=advisor&RequestData=CA_Search&CatPath=All Products&defarrangeby=////NONE////',
            search:{
            	size:10
            },
            submitFctn: function(type,val){
                if (type == 'nav'){
                    // code that is equivalent to a breadcrumbclick on val
                    //easd.executeBreadcrumbClick(val);
	                var brand = '';  //alert('val=  '+ val);
	        		var searchType = 'k';
	        		var refinementType = '';
	        		var refinementvalue = '';
	                
	                var re = new RegExp('-', 'g');
	                var arr = val.split('/');
	                
	                if(arr.length ==4){
	                	var bc_keyword = arr[2];
	                	var bc_refinement = arr[3];	
	                }else{
	                	var bc_keyword = arr[1];
	                	var bc_refinement = arr[2];	                	
	                }
	                
	                bc_keyword = unescape(bc_keyword);
	                bc_keyword = bc_keyword .replace(re, ' ').trim();
	
	                
	                if(bc_refinement.indexOf(':') != -1){
	        			var arr2 = bc_refinement.split(':')
	                    brand = arr2[0].replace(re, ' ');
	                    bc_refinement =  arr2[1];
	        			searchType = 'R';
	        			refinementType = 'refinementNameIds';
	        			refinementvalue = brand.replace(re, ' ').trim() +'_'+bc_refinement.replace(re, ' ').trim();
	        			//refinementvalue = bc_refinement.replace(re, ' ');
	                }else{
	        			searchType = 'C';
	        			refinementType = 'refineByCategoryName';
	        			refinementvalue = bc_refinement.replace(re, ' ').trim();
	        		}
	        		
	        		var product_path = escape("All Products////AttribSelect=Product Type ='G'////UserSearch="+bc_keyword);
	        		var project_path = escape("All Products////AttribSelect=Product Type ='P'////UserSearch="+bc_keyword);
	        		
	        		$('<input>').attr({
	        		    type: 'hidden',
	        		    id: 'searchType',
	        		    name: 'searchType',
	        		    value:searchType
	        		}).appendTo('#searchForm');
	        		
	        		$('<input>').attr({
	        		    type: 'hidden',
	        		    id: 'currentProductSearchPath',
	        		    name: 'currentProductSearchPath',
	        		    value: product_path
	        		}).appendTo('#searchForm');
	        		
	        		$('<input>').attr({
	        		    type: 'hidden',
	        		    id: 'currentProjectSearchPath',
	        		    name: 'currentProjectSearchPath',
	        		    value: project_path
	        		}).appendTo('#searchForm');
	        		
	        		$('<input>').attr({
	        		    type: 'hidden',
	        		    id: 'refinementType',
	        		    name: refinementType,
	        		    value: refinementvalue
	        		}).appendTo('#searchForm');	
	        		
				
	        		$('#keyword').val(bc_keyword);
	                $('#searchBtnGo').click();
                }
                else {
                    // 'search' is the id of the button
                    $('#searchBtnGo').click();
                }
            },
            hover:true ,
            fixedWidth: 600,
            leftWidth:'60%',
            horizAlign:'right',
            products: {
            	size: 1,
                fields: {
                    thumbnail: 'Product_Thumbnail',
                    name: 'Product Name',
                    description: 'Description',
                    price: 'minPrice'
                },
                sizes: {
                    description: 115
               }
            },
            value: function(item,field){
                if (field == 'link'){
                    return product_url +
                            '/catalog/product.jsp?productId=' +
                            item['Product_Id'];
                }
                else {
                    return item[field];
                }
            },
            navigation: {
                size: 2,
                sections: [{
                    size: 4,
                    title: 'Categories',
                    type: 'category'
                },{
                    size: 6,
                    title: 'Brands',
                    type: 'brand'
                }]
            }
        };
        new EAAutoComplete().init(acOptions);
    });
    
    }(jQuery));
    }(window.eaj$183 || jQuery));
</script>
