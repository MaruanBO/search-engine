     function setResetForm(id, val){
        if($(id).attr('value') != val ||  $(id).attr('value') == "");{
            $(id).attr('value', val);
        }
        $(id).focus(function(){
            if(this.value== val){
                this.value="";
            }
        });
        $(id).blur(function(){
            if(this.value==""){
                this.value= val;
            }
        });
     }
