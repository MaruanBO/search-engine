    <script type="text/javascript">
    var locale = CTC.getCookie("CTC.locale");
    if (!locale) {
        locale = 'en';
        CTC.setCookie("CTC.locale", locale, {
            expires: 180,
            path : '/'
        });
    }
    var currentLocation = window.location.pathname;
    if (currentLocation.indexOf('/' + locale) == -1) {
        var cqLocale = 'en';
        if (cqLocale != locale) {
            CTC.setCookie('CTC.locale', cqLocale, {
                expires: 180,
                path : '/'
            });
        }
    }

</script>


    
    
