    <script type="text/javascript">
        (function () {
            var input = document.getElementById('sr-main-search__input'),
                submit = input.parentNode.children[2],
                disabledClass = "sr-main-search__submit-disabled",
                defaultValue = input.getAttribute('data-placeholder'),
                disabled = true;
            input.onfocus = function () {
                this.value = "";
                if (!disabled) { submit.classList.add(disabledClass); }
                disabled = true;
            };
            input.onkeyup = function () {
                var disabledNew = this.value.length <= 1 || this.value === defaultValue;
                if (disabled === disabledNew) { return; }
                if (disabled = disabledNew) {
                    submit.classList.add(disabledClass);
                    submit.disabled = disabled;
                } else {
                    submit.classList.remove(disabledClass);
                    submit.disabled = false;
                }
            }
        }());
    </script>
    <div class="sr-main-search__autocomplete-wrapper">
    </div>
</form>
