<script>
    var CTC = CTC || {};
    CTC.getCookie = function (name) {
        var m = document.cookie.match(new RegExp(
            "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\$1') + "=([^;]*)"
        ));
        return m && decodeURIComponent(m[1]);
    };
    CTC.setCookie = function (name, value, options) {
        options = options || {};
        var exp = options.expires;
        if (typeof exp == "number" && exp) {
            var d = new Date();
            d.setTime(d.getTime() + exp*86400000)
            exp = options.expires = d;
        }
        if (exp && exp.toUTCString) { options.expires = exp.toUTCString() }
        var cookie = name + "=" + encodeURIComponent(value);
        []
        for (var propName in options) {
            cookie += "; " + propName;
            var propValue = options[propName];
            if (propValue !== true) { cookie += "=" + propValue; }
        }
        document.cookie = cookie;
    }
</script>

