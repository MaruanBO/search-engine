<script type="text/javascript">
$(document).foundation();
var $container = $("#stack");

function triggerGrid() {
	if ( !$container ) {
		return;
	}
	$container.masonry({
		columnWidth: ".grid-item",
		itemSelector: ".grid-item",
		isFitWidth: true,
		transitionDuration: 0,
		stamp: ".grid-stamp"
	});
}

$(window).resize(function(){
	triggerGrid();
});
</script>

