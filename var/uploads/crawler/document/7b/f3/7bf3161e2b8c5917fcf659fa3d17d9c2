<script type="text/javascript">
$('.blockContent .jScrollNavigation').removeClass('active');
$('.blockContent .jScrollNavigation:last').addClass('active');

$('.blockContent .jScrollNavigation:last').click(function(event){

	event.preventDefault();
	$('.blockContent .jScrollNavigation').removeClass('active');

	$('.blockContent').jscroll({
	    padding: 0,
	    nextSelector: '.jScrollNavigation a',
	    contentSelector: '.blockContent',
	    autoTrigger: true,
	    loadingHtml: '<div class="loadingStatus"><img src="https://static.avforums.com/styles/avf/xenforo/widgets/ajaxload.info_000000_facebook.gif" border="0" /></div>',
	    callback: function(){
	    	$('.masonry_layer').packery({
			itemSelector : '.homeModule',
			"columnWidth": '.grid-sizer',
			"gutter": 8
		    });
	    }
	});
	
	$(window).trigger('scroll');

});


$(document).ready(function(){
  $('.masonry_layer').packery({
    itemSelector : '.homeModule',
    "columnWidth": '.grid-sizer',
    "gutter": 8,
    "transitionDuration": "0.0s"
  });
});

</script>
						
						
						
						
