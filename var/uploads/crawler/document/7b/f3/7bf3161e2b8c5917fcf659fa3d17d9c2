<script type="text/javascript">

function playYoutube(videoID)
{
     $("#mainVideo").fadeOut(500);
     var videoSrc = "//www.youtube.com/embed/" + videoID + '?autoplay=1&wmode=opaque'
     $("#mainVideo").attr('src', videoSrc);
     $("#mainVideo").fadeIn(500);
     $('#mediaModule .hubType').css({opacity: 0});
}

$(document).ready(function(){
  //display the first video in the list on page load
  var videoPageload = "//www.youtube.com/embed/" + $(".videoSlider .thumbnail").first().attr('id') + '?rel=0&autohide=1&showinfo=0&wmode=opaque';
  $("#mainVideo").attr('src', videoPageload);

  //click thumbnail to change the iframe src
  $(".playIcon").click(function (){
     playYoutube($(this).prev().attr('id'));
  });

  $("#videoOverlay").click(function(){
      videoPageload = "//www.youtube.com/embed/" + $(".videoSlider .thumbnail").first().attr('id') + '?rel=0&autoplay=';
      $("#mainVideo").attr('src', videoPageload);
      $('#mediaModule .hubType').css({opacity: 0});
  });



});
</script>


	
	




	
