function showBuyLease(obj){
	
	if(obj.options[obj.selectedIndex].value == 1){
		
	    document.getElementById('BuyDiv').style.display='';	
		document.getElementById('LeaseDiv').style.display='none';
		//$("#BuyDiv").show();
		//$("#LeaseDiv").hide();
	}else{
		document.getElementById('BuyDiv').style.display='none';	
		document.getElementById('LeaseDiv').style.display='';
		//$("#BuyDiv").hide();
		//$("#LeaseDiv").show();
	}
}

</script>




<script type="text/javascript" language="javascript">
	function validateForm2(frm) {
		
		val1 = sanitize(frm.LISTING_PRICE_MIN);
		val2 = sanitize(frm.LISTING_PRICE_MAX);
		if (parseInt(val1) > parseInt(val2) ) {
			alert("please check your price range");
			return false;
		}
		if (frm.quickSearch.value == 'Enter Address or Street Name or MLS Number or Zip Code') {
			frm.quickSearch.value = '';
		}

		return true;
	}
	
	function sanitize(obj) {
		if(obj.value != "") {
			obj.value =	obj.value.replace(/\D/g, '');
		}
		return obj.value;
	}
	
</script>
