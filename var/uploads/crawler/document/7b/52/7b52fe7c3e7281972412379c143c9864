<script type="text/javascript">

function submitItinerarySearchForm()
{
  var lForm = document.itinerarySearchForm;
  var lAction = lForm.action;
  lForm.action=lAction.replace("http:","https:");
  lForm.submit();
}

function handleSearchBy(searchOption)
{ 
  document.getElementById('searchByText').name = searchOption.value;
}

</script>
</form>

