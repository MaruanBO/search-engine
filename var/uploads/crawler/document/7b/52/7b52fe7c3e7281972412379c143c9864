  <script> 
   if(typeof String.prototype.trim !== 'function') 
   {
     String.prototype.trim = function() 
     {
     return this.replace(/^\s+|\s+$/g, ''); 
     }
   }
  
   function modifySearchQuery(pQuery)
   {
    var lSpecificKeywords = "parking";
    var lSplitKeywords = lSpecificKeywords.split(',');
    for(var i = 0; i < lSplitKeywords.length; i++)
    {
      var lKw = lSplitKeywords[i].trim().toLowerCase();
      pQuery = pQuery.replace(new RegExp("\\b" + lKw + "\\b","ig"), '"' + lKw + '"');
    }
    return pQuery;
   }
   </script>
