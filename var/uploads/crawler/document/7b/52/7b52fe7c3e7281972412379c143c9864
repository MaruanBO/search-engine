    btnNext: "#arrowRight",
    btnPrev: "#arrowLeft",
    easing:  "swing",
    speed: 400
  });
  
	function handleArrowHover(pHoverOn, pArrow)
	{

		Prototype.Browser.IE6 = Prototype.Browser.IE && parseInt(navigator.userAgent.substring(navigator.userAgent.indexOf("MSIE")+5)) == 6;

		if(!Prototype.Browser.IE6)
		{
			if(pHoverOn)
			{
				$(pArrow + "_off").style.display = "none";
				$(pArrow + "_on").style.display = "";
			}
			else
			{
				$(pArrow + "_off").style.display = "";
				$(pArrow + "_on").style.display = "none";
			}	
		}		
	}
</script>

