<!--  SiteCatalyst code version: H.20.3.
      Copyright 1996-2010 Adobe, Inc. All Rights Reserved More info available 
      at http://www.omniture.com -->

<script type="text/javascript">
/* Specify the Report Suite ID(s) to track here */
var s_account="carlsoncountryinn";
var s_linkInternalFilters="javascript:,countryinns.com";
</script>

<!-- The JS files referenced here are uploaded through the File Browser in CMS. We must load different files based on where the request comes from. -->

 
 
  <script  type="text/javascript" src="//cache.carlsonhotels.com/ow-cms/glb/js/omniture-s_code.js?version=5.5.1184.3"></script>
  <script  type="text/javascript" src="//cache.carlsonhotels.com/ow-cms/glb/js/omniture-custom.js?version=5.5.1184.3"></script>
 


<script type="text/javascript">
var Omniture = {
  tag : function(pProps, pUseAltFunction) {
    this.extend(s, pProps);
    var lCode;
    if (pUseAltFunction && typeof s.t != 'undefined') {
      lCode=s.t();
    } else if (!pUseAltFunction && typeof s.tl != 'undefined') {
      lCode=s.tl();
    } else {
      lCode = null;
    }
    if(lCode)document.getElementByTagName("body")[0].appendChild(lCode);
  },
  
  
  
  /** This is a simplified version of $.extend that also lowercases all string values. */
  extend: function() {
      var lSrc, lSrcProp, lDest = arguments[0] || {}, lSrcIndex = 1;
      for (; lSrcIndex < arguments.length; lSrcIndex++) if ((lSrc = arguments[lSrcIndex]) != null) for (lSrcProp in lSrc) {
        lDest[lSrcProp] = (typeof lSrc[lSrcProp] == "string") ? lSrc[lSrcProp].toLowerCase() : lSrc[lSrcProp];
      }
      return lDest;
  },
  
  

  /** Used to ensure the page name is fully qualified with the correct prefix. */
  getFullPageName: function(pPageName) {
	    if(!pPageName)
	    {
	    	return s.pageName;
	    }
      var lPageName;
      var lPrefixIndex = (s.pageName) ? s.pageName.indexOf(":") : -1;
      if(lPrefixIndex > 0)
      {
          var lPrefix = s.pageName.substring(0, lPrefixIndex+1);
          lPageName = ((new RegExp("^" + lPrefix)).test(pPageName)) ? pPageName : lPrefix + pPageName;
      }
      else
      {
          lPageName = pPageName;
      }
      return lPageName;
  }
};
/** Handles legacy calls */
function invokeOmniture(pPageName, pEvents, pEvar42, pEvar44, pEvar45, pUseAltFunction)
{
  var lPageName = Omniture.getFullPageName(pPageName);
  var lProps = {pageName:lPageName, events:pEvents};
  if(typeof pEvar42 != 'undefined')
  {
    lProps.eVar42 = pEvar42;
  }
  if(typeof pEvar44 != 'undefined')
  {
	
	  
	  
	  		lProps.eVar44 = pEvar44;
	  
  	
  }

  if(typeof pEvar45 != 'undefined')
  {
    lProps.eVar45 = pEvar45;
  }
  
  s = omniture_s; // s gets easily clobbered in the global namespace, so it is saved and reset
  
  Omniture.tag(lProps, pUseAltFunction);
}

function invokeOmnitureWithProps(pPageName, pEvents, pProps, pUseAltFunction)
{
  var lPageName = Omniture.getFullPageName(pPageName);
  var lProps = {pageName:lPageName, events:pEvents};
  jQuery.extend(lProps, pProps);
  Omniture.tag(lProps, pUseAltFunction);
};

	/** Execute the PageView tag **/
	var lOmniture = {pageName:"chi:home",pageType:"",channel:"chi:home",prop1:"Country Inns &amp; Suites",prop2:"unregistered",prop10:"chi:home",prop11:"en",prop37:"chi:home",prop38:"chi:home",prop39:"chi:home",hier1:"",currencyCode:"",events:"",products:"",purchaseID:"",linkInternalFilters:"javascript:,countryinns.com",eVar1:"Country Inns &amp; Suites",eVar2:"",eVar3:"unregistered",eVar49:"chi:home",eVar50:"chi:home",eVar51:"chi:home",state:"",zip:"",server:"www.countryinns.com"};
	if (typeof checkOmniturePageError != 'undefined')
	{
	    lOmniturePageError = checkOmniturePageError();
	    if (lOmniturePageError != null)
	    {
	      lOmniture.events += ((lOmniture.events == undefined)? "" : "," )+lOmniturePageError.events;
	      
	      
	      
	      lOmniture.eVar26 = lOmniturePageError.error;
	      lOmniture.prop25 = lOmniturePageError.error;
	      
	      
	    }     
	}
	Omniture.tag(lOmniture, true);

	omniture_s = s;  // s gets easily clobbered in the global namespace, so it is saved and reset
	
</script>


