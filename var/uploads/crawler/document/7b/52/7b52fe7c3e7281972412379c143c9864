<script src="http://maps.googleapis.com/maps/api/js?&v=3.14&sensor=false&callback=loadGeoCode"></script>

<script type="text/javascript">
var lEnterCodeMsg = jQuery("<div/>").html('Enter Code or ID').text();
var lMinSearchSuggest = 2;

function loadGeoCode()
{ 
    var geoCode = new google.maps.Geocoder();
}

 
  
   lSuggests = true;
   var lSuggestionCounter = 0;
   var lSuggestionCategoryCounter = 0;
   var lCategoryCountBeforeMaxListNumber = 0;
   var ajaxURL = "//" + (((typeof serverAddr === 'undefined') ? window.location.hostname
				: serverAddr) + "/lcrExpSearch");
   
   var A1 = new jQuery("#autocomplete").autocomplete({
				source: function(request, response) {
					jQuery.ajax({
						url: ajaxURL,
						dataType: "jsonp",
						"contentType": "application/x-www-form-urlencoded; charset=UTF-8",
						data: {
							suggest: "true",
							singleSearch: "true",
							jsonp: "true",
							city: request.term
						},
						success: function(data){
							response( jQuery.map( data, function(item) {
								item.city = decodeURIComponent(item.city);
								item.display = decodeURIComponent(item.display);
								if (item.isCategoryLabel == "true")
								{
									return { 
										isCategoryLabel: item.isCategoryLabel,
										label: item.categoryLabel,
									};
								}
								else
								{
									if (item.searchType == "CITY_STATE_COUNTRY" || item.searchType == "AIRPORT" 
											|| item.searchType == "ATTRACTION")
									{
										 return {
											 value: item.display,
											 searchType: item.searchType,
											 cityId: item.cityId,
											 city: item.city,
											 countryCode: item.countryCode,
											 stateCode: item.stateCode,
											 attractionId: item.attractionId
											};
									}
									else
									{
										  return {
											  value: item.display,
											  searchType: item.searchType,
											  hotelCode: item.hotelCode
											};
									}
								}
							}));
						}
					});
				},
				minLength: 2,
				autoFocus: true,
				select: function(event, ui) {
					jQuery("#destinationSearchType").val(ui.item.searchType);
					if (ui.item.searchType == "CITY_STATE_COUNTRY" || ui.item.searchType == "AIRPORT" 
						|| ui.item.searchType == "ATTRACTION")
					{
						setDestinationId(ui.item.cityId, ui.item.attractionId);
			    }
					else
					{
						setDestinationHotelCode(ui.item.hotelCode);
					}
				},
				search: function() {
					resetAutocompleteValues();
					jQuery("#indicator1").removeAttr("style");
				},
				response: function() {
					jQuery("#indicator1").css("display","none");
				},
				open: function() {
					var lMaxHeight = (20 * jQuery(".ui-menu-item").height()) 
							+ (lCategoryCountBeforeMaxListNumber * jQuery(".expandedSearchCategoryType").parent().height());
					
					openAutocomplete(lMaxHeight,'autocomplete');

					lSuggestionCounter = 0;
					lSuggestionCategoryCounter = 0;
					lCategoryCountBeforeMaxListNumber = 0;
				},
				close: function() {
					if(jQuery("#refineSearchWrapper").length > 0)
					{
						jQuery("#refineSearchWrapper").css("display","none");
					}
				},
				error: function (xhr, textStatus, errorThrown){
					jQuery("#indicator1").css("display","none");
				}
			}).data("ui-autocomplete")._renderItem = function(ul, item) {
				var lSuggestion ="";
				if (item.isCategoryLabel == "true")
				{
					lSuggestion = jQuery("<span class='expandedSearchCategoryType' style='font-style:italic;'></span>").text(item.label);
					lSuggestionCategoryCounter = lSuggestionCategoryCounter + 1;
				}
				else
				{
					lSuggestion = jQuery("<a></a>").text(item.value);
					highlightSearchedTerm(this.term, lSuggestion);
					if (lSuggestionCounter <= 20){
						lCategoryCountBeforeMaxListNumber = lSuggestionCategoryCounter;
					}
					lSuggestionCounter = lSuggestionCounter + 1;
				}
				return jQuery("<li></li>").append(lSuggestion).appendTo(ul);
			};
	
	 jQuery(document).ready(function() {
		jQuery("#autocomplete").focus();
		
		if (jQuery.browser.msie)
		{
			jQuery("#autocomplete_choices").css("width", jQuery("#autocomplete").width() + 2 +'px');
		}
		
		hideSearchHelp('autocomplete');
		
		jQuery("#autocomplete").focusout(function() {
			hideSearchHelp('autocomplete');
		});
		
		jQuery(document).mouseup(function(e)
		{
			hideSearchHelpContent(e,'autocomplete');
		});
	});

  
  
 
 
 function submitCitySearchForm()
 {
   submitCityRateSearchForm(true);
 }

function onFocusInForm(pEvent) 
 {
	var lElement = Event.element(pEvent);
	if (lElement.value == lEnterCodeMsg)
	{
		lElement.value = "";
	}
 }
 
</script>
</div>
