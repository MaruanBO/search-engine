		$(document).ready(function () {
		    $('#film_menu a.item').click(function(){
		            var sub_menu = $(this).parent().children('ul');
		            $('#film_menu li').children('ul').slideUp('fast');
		            if ($(sub_menu).is(":hidden")){
		                $(sub_menu).slideDown('fast');
		                $(this).addClass('Over');
		            }else{
		                $(sub_menu).slideUp('fast');
		                $(this).removeClass('Over');
		             }
		    });

            // what happens when a user clicks on a genre link
            $('.radio-genre-link').live('click', function(event) {
                $('.radio-results').show().html('<p>Loading...</p>');
                   var query = jQuery(this).html();
                    jQuery.ajax({
                            data: {'query': query, 'qt': 'genre' },
                            url: '/api/upge/cheetah-photo-search/radio',
                            success: function(text) {
                                $('.radio-results').html(text);
                                $('#RadioSearchTable').tablesorter();
                                $('#RadioSearchTable').trigger("update");
                                updateHeight();
                            },
                       });
            });

           $('.submenu li a').click(function(){
                $('.submenu li').removeClass('active');
