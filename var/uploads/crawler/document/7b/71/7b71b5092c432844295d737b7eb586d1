        function triggerFullscreen(){
                triggerFullscreenFront();
                return false;
        }

        function triggerFullscreenFront(){
            jQuery('#playercontainer').css('position', 'fixed');
            jQuery('#playercontainer').css('top', '0');
            jQuery('#playercontainer').css('left', '0');
            jQuery('#playercontainer').css('z-index', '113');
            jQuery('#playercontainer').css('width', '100%');
            jQuery('#playercontainer').css('height', $(window).height() - 16);
            jQuery('#playercontainer').css('background-color', '#000000');
            jQuery('#playercontainer').find('div.active').css('position', 'fixed');
            jQuery('#playercontainer').find('div.active').css('width', '100%');
            jQuery('#playercontainer').find('div.active').css('height', $(window).height() - 16);
            jQuery('#playercontainer').find('div.active').css('top', '0');
            jQuery('#playercontainer').find('div.active').css('left', '0');
            jQuery('#playercontainer').find('object').attr('width', '100%');
            jQuery('#playercontainer').find('object').attr('height', $(window).height() - 16);
            jQuery('#playercontainer').find('embed').attr('width', '100%');
            jQuery('#playercontainer').find('embed').attr('height', $(window).height() - 16);
            jQuery('#home').show();
            jQuery('.tools').removeClass('player').addClass('fc');
            jQuery('.tools li').hide();
            jQuery('.tools.fc li.replay-button').show();

            jQuery('#playlistformating').addClass('fsmode').css('height', $(window).height() - 90).css('position', 'fixed').css('right', '-365px');
            jQuery('.playlist-box').hide();
            jQuery('#main-menu').removeClass('body_bgr');
            jQuery('#nav').addClass('fsmode').css('position', 'fixed').css('height', $(window).height() - 120).css('left', '-180px');
            jQuery('#panelButtons').show();
            jQuery('#panelSlider').show();
            jQuery('body').css('overflow', 'hidden');
            load_suggestions();
		    jQuery('.footer_links').hide();
            return false;
        }
        function doneFullscreenFront(){
                jQuery('#playercontainer').css('position', 'relative');
                jQuery('#playercontainer').css('width', '640px');
                jQuery('#playercontainer').css('height', '390px');
                jQuery('#playercontainer').css('z-index', '102');
                jQuery('#playercontainer').find('div.active').css('position', 'absolute');
                jQuery('#playercontainer').find('div.active').css('width', '640px');
                jQuery('#playercontainer').find('div.active').css('height', '390px');
                jQuery('#playercontainer').find('object').attr('width', '640px');
                jQuery('#playercontainer').find('object').attr('height', '390px');
                jQuery('#playercontainer').find('embed').attr('width', '640px');
                jQuery('#playercontainer').find('embed').attr('height', '390px ');
                jQuery('#home').hide();

                jQuery('#playlistformating').removeClass('fsmode').css('height', '390px').css('position', 'relative').css('right', '0px').css("background-image", "none").show();
                jQuery('.playlist-box').show();

                 // hide player tools
                jQuery('.tools').removeClass('fc').addClass('player');
                jQuery('.tools li').show();
                jQuery('body').css('overflow', 'auto');
                jQuery('#playlistcontainer_fscontainer').hide();
                jQuery('#panelSlider').hide();

                jQuery('#nav').removeClass('fsmode').addClass('body_bgr').css('position', 'relative').css('left', '0px').css('top', '1px').css("background-image", "none").css('height', 'auto');
                jQuery('#panelButtons').hide();
                jQuery('#main-menu').addClass('body_bgr');
				jQuery('.videoplayer-control-pause').click();
		    	jQuery('.footer_links').show();
                return false;
        }
        jQuery(document).keyup(function(e) {
            if (e.keyCode == 27) {
                jQuery('#home').click();
                return false;
            }
        });
