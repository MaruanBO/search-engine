            window.onPlayVideo = function(event) {
                videoplayer.log('onPlayVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                videoplayer.log(videoData);
                videoplayer.playFromPlaylist(videoData, this.AUTOSTART);
                };

            window.onAddVideo = function(event) {
                videoplayer.log('onAddVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                var playlist = videoplayer.getPlaylist();
                playlist.push(videoData);
                videoplayer.setPlaylist(playlist);
                videoplayer.log('onAddVideo done.');
            };

                    $('#video-details .playlistitemremove').live('click', function(e) {
                        var toRemove = videoplayer.extractVideoData(this);
                        var playlist = videoplayer.getPlaylist();
                        for (var i = 0; i < playlist.length; i++) {
                            var playlistItem = playlist[i];
                            if(videoplayer.isSameVideo(playlistItem, toRemove)){
                                playlist.splice(i--, 1);
                            }
                        }
                        videoplayer.setPlaylist(playlist);
                        $(this).parent('div.video-desc').detach();
                    });
     // getting the video details delete button to work
                    var pl_len = $('#video-details .video-desc').length;
                    var init_vlist_value = false;
                    var new_vlist_value = false;
                    $('#video-details .details-manual-order').live('hover', function(e) {
                        $(this).select();
                        if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                    });
                    $('#video-details .details-manual-order').live('focus', function(e) {
                        if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                    });
                        //$('#video-details .details-manual-order').blur(function(e) {
                        //  $(this).parent().find('.details-manual-order-confirm').hide();
                        //})
                        $('#video-details .details-manual-order').live('keyup',function(e) {
                            $(this).parent().find('.details-manual-order-confirm').hide();
                            new_vlist_value = parseInt($(this).val());
                            if (isNaN(new_vlist_value)) { return false; }
                            if (new_vlist_value == undefined) { return false; }
                            if (new_vlist_value == parseInt(init_vlist_value)) { return false; }
                            if (new_vlist_value < 0) { return false; }
                            if (new_vlist_value > pl_len) { return false; }
                            if (new_vlist_value == 0) { new_vlist_value = 1; }
                            $(this).parent().find('.details-manual-order-confirm').show();
                        });
                        // add the keypress event
                        $('#video-details .details-manual-order').live('keypress', function(e) {
                            if(e.keyCode == 13) {
                                var tmp = $('#video-details .video-desc').eq(init_vlist_value-1);
                                var target = $('#video-details .video-desc').eq(new_vlist_value-1);
                                var tmpc = tmp.clone(true);

                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                var place = $('#video-details .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);

                                var items = $('#video-details .video-desc .details-manual-order');
                                for (var i = 0; i < items.length; i++) {
                                    $(items[i]).val(i+1);
                                }

                                videoplayer.switchPlaylistItems(init_vlist_value, new_vlist_value);
                                init_vlist_value = false;
                                new_vlist_value = false;
                                $('#video-details .details-manual-order-confirm').hide();
                            }
                        });

                        $('#details-items .details-manual-order-confirm').live('click', function(e) {
                                tmp = $('#video-details .video-desc').eq(init_vlist_value-1);
                                target = $('#video-details .video-desc').eq(new_vlist_value-1);
                                tmpc = tmp.clone(true);

                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                place = $('#video-details .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);

                                var items = $('#video-details .video-desc .details-manual-order');
