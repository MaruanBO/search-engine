                        jQuery(function() {
                            $('#radio_form').submit(function(event) {
                                        jQuery('.radio-search-results').show().html("Loading...");
                                        var query = jQuery('.radio_query').val();
                                        jQuery.ajax({
                                                data: {'query': query },
