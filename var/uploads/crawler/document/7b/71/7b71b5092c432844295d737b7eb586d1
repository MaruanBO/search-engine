                            });
                        });
                        function load_user_radios(){
                                    jQuery.ajax({
                                        url: '/api/upge/cheetah-photo-search/radio',
                                        data: {'query': geoip_country_name()},
                                        success: function(text) {
                                        if(!text) { text = 'No results'; }
                                            jQuery('.radio-user-results').show().html(jQuery(text));
