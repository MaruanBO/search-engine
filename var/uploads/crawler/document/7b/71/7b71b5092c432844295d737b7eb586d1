        function load_results(query){
            jQuery.ajax({
                url: '/api/upge/worldnews/results',
                data: { 'query': query },
                success: function(text) {
                    if(!text) { text = i18n("No results"); }
                    jQuery('#loaded-results-block').find('.results').html(text);
                    updateHeight();
                }
            });
        }

        function load_topstories(){
            jQuery.ajax({
                url: '/api/upge/worldnews/topstories',
                success: function(text) {
                    if(!text) { text = i18n("No results"); }
                    jQuery('#most-viewed').find('.center-content').html(text);
                    updateHeight();
                }
            });
        }

        function loadWeather() {
            jQuery.ajax({
                data: {
                    'query': 'world',
                 },
                url: '/api/upge/cheetah-weather/continents',
                success: function(text) {
                    if(text){
                    $('#weather').find('.cities_data').html(jQuery(text));
                    updateHeight();
                    }
                },
            });

        }


        function loadUserForecast() {
            jQuery.ajax({
                data: {
                    'country': geoip_country_name(),
                    'city' : geoip_city(),
                    'lat' : geoip_latitude(),
                    'lng' : geoip_longitude(),
                 },
                url: '/api/upge/cheetah-weather/user_time_forecast',
                success: function(text) {
                    if(text){
                    $('#weather').find('.user_forecast').html(jQuery(text));
                    $('.user_forecast').css('height', '290');
                    updateHeight();
                    }
                },
            });

        }
        function load_suggestions() {
            jQuery.ajax({
                url: '/api/upge/worldnews/videos',
                data: {'query': 'latest news'},
                dataType: 'jsonp',
                success: function(text) {
                if(!text) { text = i18n("No related videos"); }
                    jQuery('#suggestions_videos').html(text);
                }
            });
        }



        function find_video_andplayfs(title){
               // search youtube for first match
               jQuery.ajax({
                   data: {
                       t: 'cheetah-photo-search/mvs_videosearch.txt',
                       query: title,
                     },
                   dataType: 'jsonp',
                   url: 'http://upge.wn.com/',
                   success: function(text) {
                       video_to_add = $(text).find('.video-info').first();
                       if (video_to_add.length > 0) {
                                videoplayer.addVideo(video_to_add);
                                videoplayer.playlistContainer.find('li').last().find('.playlistitem').click();
                                triggerFullscreenFront();
                       }
                       else {
                           $('#noresults').html('<span data-trans="Could not find a suitable video!">Could not find a suitable video!</span>');
                       }
                   }
               });
               return false;
           }



