        $(function(){

            // bind change event to select
            $('#dynamic_select').bind('change', function () {
                var url = $(this).val(); // get selected value
                if (url) { // require a URL
                 $('#liveradio_iframe').attr('src', $(this).val());
                 return false;
            }
            return false;
            });

            $('#dynamic_select_url').bind('change', function () {
                var url = $(this).val(); // get selected value
                if (url) { // require a URL
                 $('#liveradio_iframe').attr('src', $(this).val());
                 $('.radio-presenters').show();
                  $('.radio-user-results').hide();
                  $('.radio-search-results').hide();
                 return false;
            }
            return false;
            });
        });
