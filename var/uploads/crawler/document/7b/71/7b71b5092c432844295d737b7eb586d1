		function getCookie(name) {
		    var nameEQ = name + "=";
		    var ca = document.cookie.split(';');
		    for(var i=0;i < ca.length;i++) {
		        var c = ca[i];
		        while (c.charAt(0)==' ') c = c.substring(1,c.length);
		        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		    }
		    return null;
		}

	    function load_more_videos() {
			jQuery('.more-videos').html("Loading...");
	        jQuery.ajax({
	            url: '/api/upge/cheetah-photo-search/videoresults_fs',
	            data: {'query': 'Breaking News Videos'},
	            success: function(text) {
	            if(!text) { text = ('No related videos'); }
	                jQuery('.more-videos').html(text);
					jQuery('.left_panel').addClass('smaller');
					jQuery('.playlistformating').animate({scrollTop:$('.main-playlistcontainer').height()}, 'slow');
					updateHeight();
	            }
					
	        });
	    }
		
	
$(document).ready(function(){  
            
		// show player tools onhover     
		         	
		 // show player tools onhover     
		$('#tools.player').animate({ opacity: 'hide' }, "slow")           
		$('.tools_container').hover(function(){
			$(this).find('#tools.player').animate({ opacity: 'show' }, "fast");
			$('#counter-obj').hide();							
		    }, function(){
			$(this).find('#tools.player').animate({ opacity: 'hide' }, "slow")
			$('#counter-obj').show();		
		});  
		

		$('ul.tabs li').click(function(){
			var tab_id = $(this).attr('data-tab');			
			$('ul.tabs li').removeClass('current');
			$('.tab-content').removeClass('current');
			$(this).addClass('current');					
			$('.tab-content').removeClass('current');
			$("."+tab_id).addClass('current');			
		});
		

        $('a.list-preview').click(function(){
            $('.list-preview').hide('fast');
            $('.thumb-preview').show('fast');
        });
        $('a.thumb-preview').click(function(){
            $('.list-preview').show('fast');
            $('.thumb-preview').hide('fast');
        });
        $('#playlist_clear').click(function(){
            $(this).hide('fast');
            $('#playlist_restore').show('fast');
         });
        $('#playlist_restore').click(function(){
            $('.playlist_clear').show('fast');
            $(this).hide('fast');
        });
						
                // determine what display the video gallery uses
                if (getCookie("wn-videoplayer-playlist-format") && getCookie("wn-videoplayer-playlist-format").indexOf("playlist-template-with-images") > 0) {
                    $('.list-preview').show();
                    $('.thumb-preview').hide();
                }
});
            // -->    
