var videoplayers = [null];

function remove_dynamic_playlist(num){
    var target_li = $('#playlist_menu');
    target_li.find('.remove-'+num).parent().remove();
    var first_playlist = target_li.find('a').attr('id');
    if(first_playlist){
        $("#main-menu li[id='#playlist_menu'] a").attr('id', first_playlist);
    }

    var target_li = $('#details_menu');
    target_li.find('.remove-'+num).parent().remove();
    var first_details = target_li.find('a').attr('id');
    if(first_details){
        $("#main-menu li[id='#details_menu'] a").attr('id', first_details);
    }
    var target_pl = $('#tabcontainer');
    target_pl.find('#playlist'+num).remove();
    target_pl.find('#details'+num).remove();
    var last = $('#playlist_menu li a').last().attr('id').substring(1);
    activateTab(last);
}
function remove_audio_playlist(){
    var target_li = $('#playlist_menu');
    target_li.find('.remove-100').parent().remove();
    var target_pl = $('#tabcontainer');
    target_pl.find('#playlist100').remove();
}

//-->
</script> <script type="text/javascript" nopack="1"><!--
function show_loading(status){
    if(status){
        // add class "loading" to loading button
    }else{
        // remove class "loading" to loading button
    }
}

var found_playlists = {};

function add_cached_videos(query, count, source){
    if(!count) { count = 1; }
    if(!found_playlists[query]){ return; }
    if(count>found_playlists[query].length){
        count = found_playlists[query].length;
    }
    if(!found_playlists[query].length){
        if(source){
            $(source).hide();
        }
        // _add_dynamic_playlist(query, 'no more playlists');
        return;
    }
    for(var i=0; i<+count; i++){
        var p = found_playlists[query].pop();
        _add_dynamic_playlist('playlist:'+p.playlist_id, p.title, i==count-1, query);
    }
}

function add_more_videos(query, count, source){
    if(!count) { count = 1; }
    if(found_playlists[query]){
        add_cached_videos(query, count, source);
    }else{
        found_playlists[query] = [];
        show_loading(true);
        $.ajax({
            url: 'http://wn.com/api/upge/edit/find_playlists',
            data: {query: query},
            dataType: 'jsonp',
            success: function(response){
                show_loading(false);
                found_playlists[query] = response['results'];
                if(found_playlists[query]){
                    add_cached_videos(query, count, source);
                }
            },
            failure: function() {
                show_loading(false);
            }
        });
    }
}

var tab_num = null;

function add_audio_playlist(query, title){
    if(!title){
        title = query;
    }
    _add_audio_playlist(query, title);
}
function add_dynamic_playlist(query, title){
    if(!title){
        title = query;
    }
    _add_dynamic_playlist(query, title, true, query);
}

function _add_dynamic_playlist(query, title, activate, base_query){
    show_loading(true);
    if(!tab_num){
        tab_num = $('#playlist_menu li').length;
    }
    var num = ++tab_num;

//add videos
    var target_li = $('#playlist_menu');
    target_li.append(code_li1({num: num, title: title}));

    var target_pl = $('#tabcontainer');
    target_pl.append(code_pl1({num: num}));

// add video details
    var target_li = $('#details_menu');
    target_li.append(code_li2({num: num, title: title}));

    var target_pl = $('#tabcontainer');
    target_pl.append(code_pl2({num: num}));

    jQuery.ajax({
        url: 'http://wn.com/api/upge/cheetah-photo-search/dynamic_playlist_news',
        data: {
           language_id: '1',
           vsid: num,
           query: query,
           base_query: base_query
        },
        dataType: 'jsonp',
        success: function(response) {
            jQuery('#playlist'+num).html(response['videos']);
            jQuery('#details'+num).html(response['video_details']);
            bind_videos(num);
            bind_video_details(num);
            $('#playlist'+num).css('display', 'block');
            $('.tabtrigger li a[id=#playlist'+num+']').first().click();
            // activateTab('playlist'+num);
           show_loading(false);
           updateHeight();
        },
        failure: function() {
            show_loading(false);
        }
    });

    if(activate){
        activateTab('playlist'+num);
        $('#id_new_playlist').attr('value', '');
    }
    $('.single-playlist-fullscreen').hide();

    return false;
}

function _add_audio_playlist(query, title){

    if(!tab_num){
        tab_num = '100';
    }
    var num = '100';

//add videos
    var target_li = $('#playlist_menu');
    target_li.append(code_li1({num: num, title: title}));

    var target_pl = $('#tabcontainer');
    target_pl.append(code_pl1({num: num}));

    jQuery.ajax({
        url: 'http://wn.com/api/upge/cheetah-photo-search/dynamic_playlist_news',
        data: {
           language_id: '1',
           vsid: num,
           query: query,
           base_query: query
        },
        dataType: 'jsonp',
        success: function(response) {
            jQuery('#playlist'+num).html(response['videos']);
            bind_videos(num);
            $('#playlist'+num).css('display', 'block');
        },
    });
    var fullscreen_interval_caller = false;
    fullscreen_interval_caller = setInterval(function(){
        jQuery('#playlistcontainer'+num).find('a.playlistitem').first().click();
    }, 1000);
    return false;
}
//-->
