        </script> <script type="text/javascript"><!--
        jQuery(function(){

                     $('a.chat').click(function(e) {
					  	$('.tab-link.chat').click();
                      });
                     $('a.load_twitter_iframe').click(function(e) {
                            $('#twitter_iframe').attr('src', $(this).attr('href'));
                            e.preventDefault();
                      });
                      $('a.load_facebook_iframe').click(function(e) {
                            $('#facebook_iframe').attr('src', $(this).attr('href'));
                            e.preventDefault();
                      });
                   $('a.load_radio_iframe').click(function(e) {
                       $('#liveradio_iframe').attr('src', $(this).attr('href'));
                       e.preventDefault();
                   });
                   $('.film-news').click(function(e) {
                       $('#film').find('.center-content').show();
                       $('#film').find('#film-content').hide();
                   });


        function showMenu(activeMenu, activeItem, shown){
            if(window.console) console.log("Activating menu: "+activeMenu);
            // <ul id='menu-name' class="submenu tabtrigger"><li id='#child-menu-id' class="active"><a id='#opened-tab-id'>
            // or
            // <ul id='main_menu' class="tabtrigger"><li id='#child-menu-id' class="current"><a id='#opened-tab-id'>

            var activeMenuElement = $('ul'+activeMenu);
            var parentMenuItem = $('ul.tabtrigger li[id='+activeMenu+']');
            var parentMenuElement = parentMenuItem.parents('ul');
            var parentMenuName = '#'+parentMenuElement.attr('id');
            if(parentMenuItem.length){
                if(activeMenu != '#main-menu'){
                    activeItem.addClass('active');
                    activeMenuElement.css('z-index', '100');
                    // $('.submenu-container').slideDown('fast');
                    if(parentMenuName != '#undefined' && parentMenuName != activeMenu){
                        showMenu(parentMenuName, parentMenuItem, shown);
                    }
                    // slide down in reverse order, from the topmost menu
                    activeMenuElement.slideDown('fast');
                    shown.push(activeMenu);
                }else{
                    activeItem.addClass('current');
                }
            }else{
                activeItem.addClass('current');
            }
        }

        window.updateMenus = function(tab){
            if(window.console) console.log("updateMenus for: "+tab);
            // find active menu
            var menu_item = $('.submenu li:not([id]) a[id='+tab+']').parents('li'); // item with no submenu
            if(!menu_item.length){
                menu_item = $('.submenu li a[id='+tab+']').parents('li'); // any submenu item
            }
            if(!menu_item.length){
                menu_item = $('.tabtrigger li a[id='+tab+']').parents('li'); // main menu item
            }
            // reset all submenus selection
            $('.submenu.tabtrigger').children('li').removeClass('active');
            // reset main menu selection
            $('.tabtrigger').children('li').removeClass('current');
            // move all submenus back
            $('.submenu').css('z-index', '10');
            var shown = [];
            //menu_item.parents('ul.collapse').removeClass('collapse').addClass('expand');
            menu_item.addClass('active');
            var menu_ul = menu_item.parents('ul').filter('[id]');
            var menu_name = '#'+(menu_ul.length ? menu_ul.attr('id') : 'main-menu');
            if(menu_item && menu_item.length && menu_name != '#undefined'){
                showMenu(menu_name, menu_item, shown);
            }
            // slide all other submenus up
            $('.submenu').each(function(n,el){
                if($.inArray('#'+el.id, shown) == -1){
                    $(el).slideUp('fast');
                }
            });
        };

        window.updateHeight = function(content_div){
            var active_id = content_div;
            if(!content_div){
                active_id = jQuery('li.current a').get(-1).id;
            }
            jQuery('#tabcontainer').height(jQuery('#tabcontainer '+active_id).height());
        }

        var matchNumber = function(x){
            return x && x.match(/^\d+$/);
        }

        var matchPosition = function(x){
            return x && x.match(/^(\d+:)?\d+:\d+$/);
        }

        var matchAction = function(x){
            return x && x.match(/^(\d+:)?\d+:\d+(\-|\.\.)(\d+:)?\d+:\d+$/);
        }

        window.lastHistory = null;
        window.activeArea = '';
        window.fsonload = null;

        window.triggerLoadFilm = function(file){
            if (file.length == 0) {
                return;
            }
            jQuery('.filmInfo').show();
            jQuery('#film-content').html('loading...');
            var file_name = file;
            jQuery('#film .center-content').hide();
            if (file_name.length > 0) {
                jQuery('#film-content').show().load('/api/upge/worldnews/films/' + file_name);
            }
            jQuery('#film_menu').show();
            updateHeight();
            addHistory('film/'+file);
        }

        window.triggerLoadSeries = function(file){
            var file_name = file;
            jQuery('.tvshow-content').load('/api/upge/worldnews/shows/' + file_name);
            updateHeight();
            return true;
        }

        window.activateTabArea = function(tab, areas){
            var parts = (areas || '').split('/');
            fsonload = jQuery.inArray('fs', parts) >= 0;
            var area = parts[0];

            if(matchPosition(area) || matchAction(area)){
                area = '1';
                parts.unshift('1');
            }

            //if(tab == 'film') {
            //    $('[id="#film--'+area+'"]').parent().addClass('active');
            //    triggerLoadFilm(area);
            //    return true;
            //}
            if(tab == 'playlist1'){
                var vp = videoplayer;
                var sources = {'yt': 'youtube', 'vm': 'vimeo', 'dm': 'dailymotion'};
                var findMatchingVideo = function(vid){
                    vid = vid.replace(/\/+$/,'');
                    if(vid.match(/^\d+$/)){
                        var number = parseInt(vid);
                        var pl = vp.getActualPlaylist();
                        return pl[number-1];
                    }else if(vid.match(/^\w\w-/)){
                        var pl = vp.getActualPlaylist();
                        var s = sources[vid.substr(0, 2)];
                        for(v in pl){
                            if(pl[v].source == s && pl[v].video_id == vid.substr(3)){
                                return pl[v];
                            }
                        }
                    }else{
                        if(window.console)
                            console.log('Warning: Bad video id format:' + vid);
                        return null;
                    }
                };
                var playQueue = [];
                var playQueuePosition = null;
                var playShouldStart = null;
                var playShouldStop = null;
                var parsePosition = function(x){
                    var parts = x.split(/:/g);
                    var pos = 0;
                    for(var p in parts){
                        pos = pos*60 + parseInt(parts[p].trim() || '0', 10);
                    }
                    return pos;
                };
                var parseAction = function(x){
                    var video = null;
                    var start = 0;
                    var stop = null; // at the end of video
                    if(x.match(/\//)){
                        var vid_pos = x.split(/\//, 2);
                        x = vid_pos[0];
                        var positions = vid_pos[1].split(/\-|\.\./, 2);
                        start = parsePosition(positions[0]) || 0;
                        if(positions[1] && positions[1].trim()){
                            stop = parsePosition(positions[1]) || null;
                        }
                    }
                    video = findMatchingVideo(x);
                    return {
                        'video': video,
                        'start': start,
                        'stop': stop
                    };
                };

                var parseList = function(x){
                    var items = x.split(/;|,/g);
                    var results = [];
                    for (i in items){
                        try{
                            var action = parseAction(items[i]);
                            if(!action.video){
                                if(window.console) console.log("Warning: No video for queued entry: " + items[i]);
                            }else{
                                results.push(action);
                            }
                        }catch(e){
                            if(window.console) console.log("Watning: Can't parse queue entry: " + items[i]);
                        }
                    }
                    return results;
                };

                var playQueueUpdate = function(){
                    var playPosition = playQueue[playQueuePosition];
                    vp.playFromPlaylist(playPosition.video);
                    playShouldStart = playPosition.start;
                    playShouldStop = playPosition.stop;
                };

                var playQueueAdvancePosition = function(){
                    if(window.console) console.log("Advancing play position...");
                    playQueuePosition ++;
                    if(playQueuePosition < playQueue.length){
                        playQueueUpdate();
                    }else{
                        vp.pauseVideo();
                        playShouldStop = null;
                        playShouldStart = null;
                    }
                };

                if(area){
                    var sel = findMatchingVideo(area);
                    if(sel){
                        vp.playFromPlaylist(sel);
                        playShouldStart = 0;
                    }
                }

                if(fsonload){
                    playShouldStart = 0;
                }

                if(matchPosition(parts[1])){
                        playShouldStart = parsePosition(parts[1]);
                }

                if(matchAction(parts[1])){
                        var action = parseAction(area+'/'+parts[1]);
                        playShouldStart = action.start;
                        playShouldStop = action.stop;
                }

                if(playShouldStart != null || fsonload){
                    setInterval(function(){
                        if(playShouldStop && vp.currentPlayer && vp.currentPlayer.getCurrentTime() > playShouldStop){
                            playShouldStop = null;
                            playQueueAdvancePosition();
                        }
                    }, 500);
                    vp.playerContainer.bind('videoplayer.player.statechange', function(e, state){
                        if(state == 'ended'){ // advance to the next video
                            playQueueAdvancePosition();
                        }
                    });
                    vp.playerContainer.bind('videoplayer.player.readychange', function(e, state){
                        if(fsonload) {
                            triggerFullscreenFront(); fsonload = false;
                        }
                        if(playShouldStart >= 0 && state){
                            vp.seekTo(playShouldStart);
                            playShouldStart = null;
                        }
                    });
                }
                return true;
            }
            return false;
        };

        window.activateTab = function(tab, area){
            window.activeArea = null;
            if(tab == 'video') { tab = 'playlist1';}
            if(tab == 'radio--bbc1'){ tab = 'radio'; }
            if(tab == 'radio--bbc2'){ tab = 'radio'; }
            if(tab == 'radio--bbc3'){ tab = 'radio'; }
            if(tab == 'radio--bbc4'){ tab = 'radio'; }
            if(tab == 'radio--bbc5'){ tab = 'radio'; }
            var sup = $('ul li a[id=#'+tab+']');
            if(sup && sup.length>0){
                window.activeArea = area;
                sup.first().click();
                tabClick('#'+tab);
                if(!window.activateTabArea(tab, area)){
                    window.activeArea = null;
                }
                return true;
            }
            return false;
        };
        window.addHistory = function(path){
            if(window.console) console.log("Adding to history: "+path);
            if(path == 'highlights') {
                path = '/';
            } else if(path.match(/playlist1/)) {
                path = '/'+path.replace('playlist1', 'video');
            } else if(path.match(/video\/chat/)) {
                path = path.replace('video/chat', 'chat');
            } else {
                path = '/' + path.replace('--','/');
            }
            if(window.history && history.replaceState && document.location.hostname.match(/^(\w{2,3}\.)?wn\.com$/)){
                if(window.lastHistory)
                    history.pushState(null, null, path);
                else if(window.lastHistory != path){
                    history.replaceState(null, null, path);
                    window.lastHistory = path;
                }
            }else if(window.lastHistory != path){
                window.location.hash = path;
                window.lastHistory = path;
            }
        };

        window.lastClick = '#highlights';

        $('.tabtrigger li a').live('click', function() {
            var tab = $(this).attr('id');
            tabClick(tab);
        });

        window.tabClick = function(tab){
            if(window.lastClick == tab) return;
            window.lastClick = tab;
            if(tab.substring(0,1) == '#'){
                var name = tab.substring(1);
                if(window.console) console.log("Triggering tab: "+name);
                var path = name;
                if(window.activeArea){
                    path = path + '/' + window.activeArea;
                }if(tab.match(/#playlist\d+/)){
                        $('.multiple-playlists').show();
                        $('.youtube-playlists').show();
                        $('#video-search').show();
                }else {
                        $('.multiple-playlists').hide();
                        $('.youtube-playlists').hide();
                        $('#video-search').hide();
                }
                addHistory(path);
                setTimeout(function(){
                    if(tab.match(/business--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'loaded-results-block');
                    }else if(tab.match(/regions--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'loaded-regions-block');
                    }else if(tab.match(/sport--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'loaded-results-block');
                    }else if(tab.match(/radio-/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-uk--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-us--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-india--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-france--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-tamil--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/radio-arabic--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'radio');
                    }else if(tab.match(/tvshows--/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'tvshows');
                    }else if(tab.match(/film--chat/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'chat_film');
                    }else if(tab.match(/^#?\/?film/)){
                        jQuery('.tabtrigger').offscreentabs('activateTab', 'film');

                    }
					if(tab.match(/playlist1/)){
                            update_chat_position($(tab).find('ul.tabs .current'));
                    }else {
                           $('#chat-box').hide();
                    }
					if(tab.match(/^#?\/?film/)){
						jQuery('.wn-chat').hide();
						jQuery('.film-chat').show();
                    }else {
						jQuery('.wn-chat').show();
						jQuery('.film-chat').hide();
                    }


                    updateMenus(tab);
                    updateHeight();
                }, 10);
            }
            return false;
        }

        jQuery('.tabtrigger').offscreentabs({container: '#tabcontainer'});

        var all_parts = document.location.hash.replace(/[#\/]*/,'');
        if(document.location.host == 'wn.com' && !all_parts){
            //e.g "/test"
            all_parts = document.location.pathname.replace('/', '');
        }
        var parts = all_parts.split(/\//g);
        var switches = parts.slice(1).join('/');
        all_parts = all_parts.replace(/\//g,'--'); // e.g. "test/2" -> "test--2"
        if(matchNumber(parts[0]) || matchAction(parts[0]) || matchPosition(parts[0]) || parts[0] == 'fs'){
            activateTab('video', all_parts);
        }else
          if(!activateTab(all_parts)){ // <all_parts>
            if(!activateTab(all_parts+'-block')){ // <all_parts>-block
              if(!parts[1] || !activateTab(parts[0], switches)){ // <all_parts:0> with args
                    if(!parts[0] || !activateTab(parts[0])){ // <all_parts:0>
                        activateTab('highlights'); // default
                    }
                }
            }
        }
    });
