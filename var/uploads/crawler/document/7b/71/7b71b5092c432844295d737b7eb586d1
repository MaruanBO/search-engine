            });



            $('.slink').click(function(){
                if ($("div.sms_page").is(":hidden")) {
                    if(!$("div.sms_page iframe").length){
                    $("div.sms_page").prepend('<iframe scrolling="Auto" frameborder="0" height="300" width="350" marginheight="0"'
                   +' marginwidth="0" src="http://upge.wn.com/?t=cheetah-photo-search/smsCaptcha.txt&title=WorldNews&url=http://wn.com/"></iframe>')
                                                }
                    $("div.sms_page").animate({ height: '310px', opacity: 'show' }, "slow");
                } else {
                    $("div.sms_page").animate({ height: '0%', opacity: 'hide' }, "slow");
                    }
            })
            $('.mlink').click(function(){
                if ($("div.email_page").is(":hidden")) {
                    if(!$("div.email_page iframe").length){
                    $("div.email_page").prepend('<iframe scrolling="Auto" frameborder="0" height="300" width="350" marginheight="0"'
                   +' marginwidth="0" src="http://upge.wn.com/?t=cheetah-photo-search/mailCaptcha.txt&title=WorldNews&url=http://wn.com/"></iframe>')
                                                }
                    $("div.email_page").animate({ height: '310px', opacity: 'show' }, "slow");
                } else {
                    $("div.email_page").animate({ height: '0%', opacity: 'hide' }, "slow");
                    }
            })
