<script type="text/javascript">

  function doSearch()
  { si= "/CitySearchResults.aspx?q="+encodeURIComponent(document.getElementById('BannerFactsSections_txtq').value);window.location = si }

  q = document.getElementById('BannerFactsSections_txtq');
  if (q.value == '')
    q.style.background = '#FFFFFF url(/Portals/_default/Skins/COSA0//images/COSA/search_watermark.gif) left no-repeat';
  var b = function()
  { if (q.value == '') { q.style.background = '#FFFFFF url(/Portals/_default/Skins/COSA0//images/COSA/search_watermark.gif) left no-repeat'} }
  var f = function() { q.style.background = '#ffffff'; };
  q.onfocus = f; q.onblur = b;

</script>
