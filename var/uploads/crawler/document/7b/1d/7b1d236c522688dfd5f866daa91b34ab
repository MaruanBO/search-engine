window.addEvent('domready', function() {
	if (_gaq) {
	$$('a').each(function(el) {
		el.addEvent('click', function() {
				var port = -1 == el.host.lastIndexOf(":") ? el.host.length : el.host.lastIndexOf(":");
				var host = el.host.slice(0, port)
				if (el.protocol == 'mailto:') {
					var label = (el.title) ? el.title : el.href.replace(/mailto:/i, '');
					_gaq.push(['_trackEvent', 'Email', 'Contact', label]);
				} else if (host != document.location.host) {
					var label = (el.title) ? el.title : el.href;
					_gaq.push(['_trackEvent', 'External', 'Exit', label]);
				}

				var ext   = el.pathname.substr(el.pathname.lastIndexOf(".")+1, el.pathname.length);
				var label = (el.title) ? el.title : el.pathname.replace(/^\//,'');
				if (ext.match(/(bmp|csv|doc|epg|gif|ico|jpg|odg|odp|ods|odt|pdf|png|ppt|swf|txt|xcf|zip|gz|tar|exe)/i)) {
					_gaq.push(['_trackEvent', 'Download', ext, label]);
				}
			}.bind(this));
		});
	}
});

