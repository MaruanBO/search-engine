    <!-- BEGIN RKG Attribution Management Tracking Tag -->
    <script type="text/javascript">
        try {
            function rkg_track_sid(mid) {
                //Figure out if work needs to be done
                if (!(document.referrer && document.referrer.match('://([^/]+)')[1].toLowerCase().match(document.domain.match('[^.]*\.[^.]*$')[0].toLowerCase()))) {
         
                    var cookie, sid, value;

                    //Determine whether a cookie exists
                    var cookieArray = document.cookie.split("; ");
                    var i;
                    for (i=0; i < cookieArray.length; i++){
                        cookie = cookieArray[i].substring(0,cookieArray[i].indexOf("="));
                        value  = cookieArray[i].substring(cookieArray[i].indexOf("=")+1);
                        if ( cookie == "rkglsid" ){
                            value = value.substring(value.indexOf("h-")+2,value.indexOf("_t-"));
                            sid = value;
                            break;
                        }
                    }
        
                    if ( !sid ){
                        //No cookie, create one
                        sid = rkg_generate_sid( mid );
                    }

                    //Place cookie
                    var expDate = new Date();
                    var theTime = Math.round( expDate.getTime() / 1000 );
                    expDate.setDate( expDate.getDate() + 365 );
                    value = "h-" + escape( sid ) + "_t-" + theTime + ";expires=" + expDate.toUTCString() + ";path=/;domain=" + window.location.hostname;
                    document.cookie = "rkglsid=" + value;
                    
                     
                    //Send to RKG
                    var href = document.location.protocol + "//mct.rkdms.com/sid.gif?mid=" + mid + "&ref=" + encodeURIComponent( document.referrer ) + "&rkglsid=" + sid;
                    document.write( '<img height="1" width="1" src="'+href+'"/>' );
                    
      
                }
            }

            function rkg_generate_sid( mid ) {
                var tsid;
                var v = 0;
                var hexmid = '';
                var chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"
                mid = mid.substring(0,4);
                var midArray = mid.split('');
                for (i=0; i < midArray; i++){
                    v = v * 64 + chars.search( midArray[i] );
                }
                hexmid = hexmid + v.toString(16);   
      
                var today  = new Date();
                var hexsec = today.getTime().toString(16);
                tsid = Math.floor( Math.random() * 65536 ).toString(16)
                    + Math.floor( Math.random() * 65536 ).toString(16)  
                    + hexmid 
                    + navigator.userAgent.replace(/[^0-9a-f]/g,'').split("").reverse().join("").substring(0,10)
                    + hexsec;
      
                while ( tsid.length < 32 ){
                    hexran = Math.floor( Math.random() * 16 ).toString(16);
                    tsid = tsid + hexran;
                }
                if ( tsid.length > 32 ){
                    tsid = tsid.substring(0,32);
                }
      
                return tsid;

            }    

            rkg_track_sid( "venus"); 
        }
        catch( e ){}
</script>
