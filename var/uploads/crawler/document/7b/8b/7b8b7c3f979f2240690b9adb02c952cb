		<script type="text/javascript">
/**
*
*  Javascript cookies
*  http://www.webtoolkit.info/
*
**/
 
function setCookie(cookieName,cookieValue,nDays) {
	var today = new Date();
	var expire = new Date();
	if (nDays==null || nDays==0) nDays=1;
	expire.setTime(today.getTime() + 3600000*24*nDays);
	document.cookie = cookieName+"="+escape(cookieValue)
				 + ";expires="+expire.toGMTString();
}
 
function getCookie (name) {
		name = name + "=";
		var carray = document.cookie.split(';');
 
		for(var i=0;i < carray.length;i++) {
			var c = carray[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
		}
 
		return null;
}
 
function deleteCookie (name) {
	this.setCookie(name, "", -1);
}



var closingTime = 10;
function secondsToClose() {
	jQuery('#cBoxClosing #cBoxSeconds').text(closingTime);
	if (closingTime == 0) {
		setCookie('nosurvey','1',0);
		jQuery.fn.colorbox.close();
	}
	closingTime--;
	setTimeout(secondsToClose, 1000); 
}
function closeForever() {
	setCookie('nosurvey','1',0);
	jQuery.fn.colorbox.close();
}
jQuery(function() {
	if ( jQuery('#modal-view').is(':empty') )	{ 
	} else {			
		if ( (!(getCookie('nosurvey'))) || (getCookie('nosurvey')  != '1')  ) {
			jQuery.fn.colorbox({ inline: true, href:'#modal-view', open: true,
				onComplete: function() {
					jQuery('#cboxTitle').html('<div id="cBoxClosing"><a id="cBoxCloseLink" onclick="closeForever()" href="javascript: void(0);">Click to Close</a>');
					jQuery.colorbox.resize();
				},
				onClosed: function() {
					setCookie('nosurvey','1',0);
				}
			});
		}
	
		jQuery('iframe #frmS #NextButton').click( function() {
			closeForever();
		});
	}
});
</script>
        
