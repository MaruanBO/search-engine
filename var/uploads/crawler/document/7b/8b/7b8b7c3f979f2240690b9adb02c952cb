					<script language="javascript" type="text/javascript">
						function settime () {
							var curtime = new Date();
							var unixtime = curtime.getTime(); 
							var curhour = curtime.getHours();
							var curmin = curtime.getMinutes();
							var time = "";
						 
							if(curhour == 0) curhour = 12;
							time = 	(curhour > 12 ? curhour - 12 : curhour) + ":" +
								(curmin < 10 ? "0" : "") + curmin + " " +
								(curhour >= 12 ? "pm" : "am");
						 
							jQuery("#showTime").html(time);
							//console.log(time);
						}
						
						var updateTime = setInterval("settime()", 30000);
						jQuery( function() {
							jQuery('#manage').click( function(e) {
								e.preventDefault();
								if ( jQuery('#msinput').css('display') == "none" )
								{
									jQuery('#msinput').css('display',"block");
								} else {
									jQuery('#msinput').css('display',"none");
								}
									
								
							});
							jQuery('#idform').submit( function(evt) {
								evt.preventDefault();
								var i = jQuery('#idform #id').val();
								//console.log(i);
								window.location = "http://link.federalnewsradio.com/manage/3j8/manage?email=" + i;
							});
						});
