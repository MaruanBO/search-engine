					if ( mainSize > rightSize) {
						var spotSize = jQuery('#spotlight-300-homepage').height();
						var plusSize = mainSize - rightSize;
						var newSize = spotSize + plusSize;
						//if (newSize > 385) { newSize = 385 }
						jQuery('#spotlight-300-homepage').css('min-height', (newSize + 7) + 'px');
						//console.log(newSize);
					} else if ( mainSize < rightSize) {
						
						var plusSize = rightSize - mainSize +2;
						//console.log(rightSize + ", " + mainSize + ", " + plusSize);
						jQuery('#picturesoftheweek').css('margin-top',plusSize + 'px');
					}
			
			});

