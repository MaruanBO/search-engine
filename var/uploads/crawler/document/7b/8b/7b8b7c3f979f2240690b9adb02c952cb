					//console.log("Size: " + fourSize);
					//var mainSize = jQuery('#Article-675-left').height();
					var adSize = jQuery('.rightsidebarOne .picturethis-ad').height();
					var pjobs = jQuery('.rightsidebarOne #powerjobs-widget').height();
					if ( jQuery('.rightsidebarOne #popular').css('display') == 'block' )
					{
						var popSize = jQuery('.rightsidebarOne #popular').height();
