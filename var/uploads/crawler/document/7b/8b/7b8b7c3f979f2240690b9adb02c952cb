<script type="text/javascript">
function moveTicker(base) {
	//console.log(base);
	base++;
	var move = -767 * base;
	//console.log("Base: " + base + "; Move: " + move);
	if ( move <= -(162604) ) 
	{
		//console.log("exceeded");
		move = 0;
		base = 0;
	}
	jQuery('#govevents-box').animate(
		{ left : move }
	);	
	Timer = setTimeout("moveTicker(" + base + ")", 10000);
}
var Timer;
var base = 0;
jQuery( function() {
	var gvh = jQuery('#govevents-box').height();
	//console.log(gvh);
	gvh -= 10;
	//console.log(gvh);
	jQuery('#leftbox').css('height',gvh + 'px');

	jQuery('.govevents-next').click( function() {
		clearTimeout(Timer);
		base++;
		var move = -767 * base;
		//console.log("Base: " + base + "; Move: " + move);
		if ( move <= -(162604) ) 
		{
			//console.log("exceeded");
			move = 0;
			base = 0;
		}
		jQuery('#govevents-box').animate(
			{ left : move }
		);
		
	});
	jQuery('.govevents-prev').click( function() {
		clearTimeout(Timer);
		base --;
		var move = -767 * base;
		//console.log("Base: " + base + "; Move: " + move);
		if ( move > 0 ) 
		{
			//console.log("below");
			move = -(162604) + 767;
			base = 211;
			//console.log("Base: " + base + "; Move: " + move);
		}
		jQuery('#govevents-box').animate(
			{ left : move }
		);
	});
	Timer = setTimeout("moveTicker(" + base + ")", 10000);
});
</script>
