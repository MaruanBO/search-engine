<script src="/JavaScripts/jquery-1.7.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
// Ref: http://snook.ca/archives/javascript/simplest-jquery-slideshow
$(function(){
	var imin = 0, imax = 5, icur = 1;
	var timerId = 0;
	timerId = setInterval(function(){
		if ( icur == imax ) icur = imin; else icur += 1;
		$('#slide_nav li').removeClass("selected");
		$('#home-ftr-media div').fadeOut("fast");
		$('#slide_nav li#slide_'+icur).addClass("selected");
		$('#home-ftr-media div.#feature_'+icur).fadeIn("slow");
		//alert(icur);
	},7000);
	$('#slide_nav li').click(function() {
		clearInterval(timerId);
		var theID = $(this).attr('id');
		var ftrnum = theID.substr(6,1);
		$(this).siblings().removeClass("selected");
		$(this).addClass("selected");
		$('#home-ftr-media div').fadeOut("fast");
		$('#home-ftr-media div.#feature_'+ftrnum).fadeIn("slow");
	});
});
</script>
