<script>
    jQuery( document ).ready(function($) {
        $("input[alt]", 'form#wForm').each(function () {
            var $input = $(this);
            $input.val($input.attr('alt'));
        });
        $('input[alt]', 'form#wForm').focus(function () {
            var $input = $(this);
            if ($input.val() === $input.attr('alt')) {
                $input.val('');
            }
        }).blur(function () {
            var $input = $(this);
            if ($input.val() === '') {
                $input.val($input.attr('alt'));
            }
        });
    });
</script></div><div id="tncms-block-993161" class="tncms-block">
