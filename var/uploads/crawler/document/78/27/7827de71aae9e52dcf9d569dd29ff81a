	var ua = navigator.userAgent;
	var checker = {
		iphone: ua.match(/iPhone/),
		ipod: ua.match(/iPod/),
		android: ua.match(/Android/),
		blackberry: ua.match(/BlackBerry/),
		webos: ua.match(/webOS/)
	};
	
	/* Set the mobile preference cookie */
	if(readCookie("mobile") == null) {
		//if(checker.iphone || checker.ipod || (checker.android && screen.width < 500) || (checker.blackberry && screen.width < 500) || (checker.webos && screen.width < 500)) {
		// Updated 6/18/2013 by GMW to remove screen size restriction.
		if(checker.iphone || checker.ipod || checker.android || checker.blackberry || checker.webos) {
			createCookie("mobile","true",7);
		}
		else {
			createCookie("mobile","false",7);
		}
	}
	
	
		if(readCookie("mobile") != "false") {
			window.location.replace("http://m.thecatholicdirectory.com/");
		}
	
</script>
