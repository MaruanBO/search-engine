<script type="text/javascript">
				var latitude;
				var longitude;
				var cityName = "";
				var stateAbbr = "";
				var stateName = "";
				var countryAbbr = "";
				var countryName = "";
				var postalcode = "";
				
				$(document).ready(function() {
					if (navigator.geolocation) {
						navigator.geolocation.getCurrentPosition(successFunction, errorFunction, {enableHighAccuracy: true});
					}
					else
					{
						console.log("HTML5 geolocation unsupported.");
					}
				});
				
				function successFunction(position) {
					latitude = position.coords.latitude;
					longitude = position.coords.longitude;
					
					codeLatLng(latitude, longitude)
				}
				
				function errorFunction() {
					console.log("Geocoder failed");
				}
				
				function codeLatLng(lat, lng) {
					var geocoder = new google.maps.Geocoder();
					var latlng = new google.maps.LatLng(lat, lng);
					
					geocoder.geocode({'latLng': latlng}, function(results, status) {
						if (status == google.maps.GeocoderStatus.OK) {
							var result = results[0];
							
							for(var i=0, len=result.address_components.length; i<len; i++) {
								var ac = result.address_components[i];
								
								if(ac.types.indexOf("locality") >= 0) cityName = ac.long_name;
								if(ac.types.indexOf("administrative_area_level_1") >= 0) stateName = ac.long_name;
								if(ac.types.indexOf("administrative_area_level_1") >= 0) stateAbbr = ac.short_name;
								if(ac.types.indexOf("country") >= 0) countryName = ac.long_name;
								if(ac.types.indexOf("country") >= 0) countryAbbr = ac.short_name;
								if(ac.types.indexOf("postal_code") >= 0) postalcode = ac.long_name;
							}
							
							var querystring = "lat=" + latitude + "&lon=" + longitude + "&cc=" + countryAbbr + "&cn=" + countryName + "&rc=" + stateAbbr + "&rn=" + stateName + "&ci=" + cityName + "&zc=" + postalcode;
							
							//console.log("Query string: " + querystring);
							
							$.ajax({
								type: "get",
								url: "act_set_location.cfm",
								data: querystring,
								dataType: "html",
								async: false,
								success: function(response){document.getElementById("locationStringDiv").innerHTML = $.trim(response); /*console.log("Server response: " + response);*/}
							});
							
							location.href = 'http://www.thecatholicdirectory.com/';
							
						} else {
							console.log("Reverse geocoder failed due to: " + status);
						}
					});
				}
				
				/*$(document).ready(function() {
					if(latitude != undefined && longitude != undefined) {
						
					}
				});*/
			</script>
<script type="text/javascript">
	// Initialize default variables
	var geocoder;
	var lat;
	var lon;
	var latlng;
	var maxAge = 300000;
	var forceRefresh = false;
	var addr = "";
	var destination = "";
	
	function goToLocationPage(enableForcedRefresh, pageType)
	{	
		destination = pageType;
		
		if (navigator.geolocation)
		{
			if (enableForcedRefresh == true) {
				maxAge = 0;
				forceRefresh = true;
			}
			
			navigator.geolocation.getCurrentPosition(success, error, {enableHighAccuracy: true, maximumAge: maxAge});
		}
		else
		{
			alert("HTML5 geolocation unsupported.");
		}
	}
	
	function success(position)
	{
		lat = position.coords.latitude;
		lon = position.coords.longitude;
		
		if (forceRefresh == true | addr == "") {
			// Google reverse geocoder
			geocoder = new google.maps.Geocoder();
			latlng = new google.maps.LatLng(lat,lon);
			
			geocoder.geocode({'latLng': latlng}, function(results, status) {
				if (status == google.maps.GeocoderStatus.OK) {
					if (results[1]) {
						updateLocation(results[1].formatted_address, true);
					} else {
						updateLocation("No results found", true);
					}
				} else {
					updateLocation("Geocoder failed due to: " + status, true);
				}
			});
		}
		else {
			updateLocation(addr, false);
		}
	}
	
	function error(err)
	{
		switch (err.code) {
			case 1:
				// PERMISSION_DENIED
				alert("Permission to access your location was denied. Check your settings to allow access.");
				break;
			case 2:
				// POSITION_UNAVAILABLE
				alert("Unable to determine your location.");
				break;
			case 3:
				// TIMEOUT
				alert("Timed out while trying to determine your location.");
				break;
			default:
				// UNKNOWN_ERROR
				alert("An unknown error occurred while trying to determine your location.");
		}
	}
	
	function updateLocation(locationStr, updateAddr) {
		if (updateAddr) {
			addr = locationStr;
		}
		
		if(destination == "list")
		{
			location.href = "directory.cfm?fuseaction=search_location&lat=" + lat + "&lon=" + lon + "&addr=" + encodeURI(locationStr) + "&radius=50&measurement=mi";
		}
		else if(destination == "map")
		{
			location.href = "map/dsp_map_results_home.cfm?lat=" + lat + "&lon=" + lon + "&addr=" + encodeURI(locationStr);
		}
		
		// Reset variables to defaults
		maxAge = 300000;
		forceRefresh = false;
	}
</script>
