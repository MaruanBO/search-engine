<script language="javascript" type="text/javascript">
	var W3C = (document.getElementById) ? true : false;
	var NS4 = (document.layers) ? true : false;
	
	function ToggleDisplay(areaID, visible, erase) {
		//alert(areaID);
		if(W3C) {
			if (visible) {
				document.getElementById(areaID).style.display='';
				document.getElementById(areaID).style.visibility = 'visible';
			} else {
				document.getElementById(areaID).style.display='none';
				document.getElementById(areaID).style.visibility = 'hidden';
			}
		} else if(NS4) {
			if(visible) { 
				document.layers[areaID].visibility = 'visible';
				document.layers[areaID].display = '';
			} else {
				document.layers[areaID].visibility = 'hidden';
				document.layers[areaID].display = 'none';
			}
		}
		if(erase)
			if(W3C) {
				document.getElementById(areaID).value = '';
			} else if(NS4) {
				document.layers[areaID].value = '';
			}
		//This opens the page for the dynamic div opening and bringing the gray down the page.
		//setTall();
		
	} 
	
	function ToggleArea(){
		if (document.getElementById('map_area')){
		document.getElementById('map_area').className='smallmap';
		ToggleDisplay('map_area',true);
		ToggleDisplay('nearby_listings',true);	
		}		
	}

	function getDataAdClose(id,typecount){
		if (typecount == 'Banner')
		{
			$(".couponbanner_"+id).fadeOut();			
		}
		else{
		  $(".coupon_"+id).fadeOut();			
		}
	}

	//Uses ajax to get the next select and the options.
	function getDataAd(id,target,typecount){
		var current_showing = $(".current_coupon_showing").val();
		
		var datastring = "qu="+ id + "&typecount=" + typecount;
		
		
			$(document).ready(function() {
				$.ajax({
				type: "get",
				url: "js/act_getcurrentad.cfm?returnformat=plain",
				data: datastring,
				dataType: "html",
				success: function(response){
					var resp = jQuery.trim(response);//getting alot of whitespace in my return CFC method
						
					if (resp != ""){
						
						document.getElementById(target).innerHTML = resp;
						if (typecount == 'Banner')
						{
							if(id!=current_showing)
							  $(".couponbanner_"+current_showing).fadeOut();
							
							$(".couponbanner_"+id).toggle('fade');
							$(".current_coupon_showing").val(id);
						}
						else{
							if(id!=current_showing)
							  $(".coupon_"+current_showing).fadeOut();
							
							$(".coupon_"+id).toggle('fade');
							$(".current_coupon_showing").val(id);
						}
					}
					else {
						document.getElementById(target).innerHTML = "";
					}
					
					//$("#SMS_Coupon").fancybox({
					//	'width'				: 455,
					//	'height'			: 440,
					//	'autoScale'			: true,
