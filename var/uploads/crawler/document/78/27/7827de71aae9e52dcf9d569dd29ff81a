					//	'type'				: 'iframe',
					//	'overlayColor'      : '#336699',
					//	'overlayOpacity'    : 0.5													
					//});
				
					$("#Email_Coupon").fancybox({
						'width'			: 380,
						'height'		: 250,
						'openEffect'    : 'elastic',
						'closeEffect'   : 'elastic',
						'fitToView' 	: false,
						'autoSize' 		: false,
						'type'         	: 'iframe',
						'helpers' : {
							'overlay' : {
								'css' : {
									'background':'transparent',
									'filter':'progid:DXImageTransform.Microsoft.gradient(startColorstr=##7f336699,endColorstr=##7f336699)', 
									'zoom': '1',
									'background' : 'rgba(51, 102, 153, 0.5)'
								}					
							}
						}   										
					});
				}
				});
			});
			
	}
		
	
					
	
	
	
		
</script>
