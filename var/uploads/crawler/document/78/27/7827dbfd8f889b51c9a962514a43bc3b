    <script type="text/javascript">
    	
    	//document ready
    	$(function() {
    		
    		//count the number of dynamic events so we know 
    		//what placeholder images to use
    		var events = $('#events .cimg.dynamic').length;
    		
    		//decide on when to hide them
    		if (events == 4) {
	    		$('#fill_giftcards,#fill_mvp,#fill_newsletter,#fill_twitter').hide();
    		}
			if (events == 3) {
	    		$('#fill_giftcards,#fill_mvp,#fill_newsletter,#fill_twitter').hide();
    		}
    		if (events == 2) {
	    		$('#fill_mvp,#fill_newsletter,#fill_twitter').hide();
    		}
    		if (events == 1) {
	    		$('#fill_newsletter,#fill_twitter').hide();
    		}
			if (events == 0) {
	    		$('#fill_newsletter').hide();
    		}
			
    		
	    	//hide the coming soon carousel
			$('#car_comingsoon').hide();
			$('#car_nowplaying').hide().fadeIn('slow');
			
			//start the now playing carousel
			$("#nowplaying").carouFredSel({
				circular: true,
				infinite: true,
				items: 6,
				scroll: { 
					items:1,
					duration: 500,
				},
				auto: true,
				prev: {
					button: "#nowplaying_prev",
					key: "left"
				},
				next: {
					button: "#nowplaying_next",
					key: "right"
				} 
			});
			
			//toggle the carousel when NP or CS buttons are clicked
			$('#but_nowplaying').click(function() {
				$('#car_comingsoon').fadeOut();
				$('#car_nowplaying').fadeIn();
				$(this).removeClass("carousel_button").addClass("carousel_button_active");
				$('#but_comingsoon').removeClass("carousel_button_active").addClass("carousel_button");
			});
			
			$('#but_comingsoon').click(function() {
				$('#car_nowplaying').fadeOut();
				$('#car_comingsoon').fadeIn();
				
				$("#comingsoon").carouFredSel({
					circular: true,
					infinite: true,
					scroll: { 
						items:1,
						duration: 500,
					},
					auto: true,				
					prev: {
						button: "#comingsoon_prev",
						key: "left"
					},
					next: {
						button: "#comingsoon_next",
						key: "right"
					}   
				});
			
				$(this).removeClass("carousel_button").addClass("carousel_button_active");
				$('#but_nowplaying').removeClass("carousel_button_active").addClass("carousel_button");
			});
			
			//start the special events carousel
			$("#events").carouFredSel({
				circular: true,
				infinite: true,
				scroll: { 
					items:1,
					duration: 500,
				},
				auto: true,
				prev: {
					button: "#events_prev",
					key: "left"
				},
				next: {
					button: "#events_next",
					key: "right"
				} 
			});
			
			//start the camera slider
			$('.camera_wrap').camera({
				height: '333px',
				pagination: true,
				thumbnails: true,
				playPause: false,
				time: 5000,
				transPeriod: 700,
				fx: 'random',
				bypassCache: false
			});
		});
    </script>
