<script>
    function validate(form) {
        if (!form)
            return false;

        var query = form.elements['s'].value;

        if (query && query.length > 0) {
            query = query.trim();

            if (query.length > 0)
                return true;

            form.elements['s'].value = "";
        }

        return false;
    }
</script>
