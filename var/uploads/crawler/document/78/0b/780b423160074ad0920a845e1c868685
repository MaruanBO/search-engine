        var code = e.keyCode || e.which;
        if (code === 13) {
            var emailField = $("#email-lite-text");
            var email = $(emailField).val();
            configureEmailLite(email, false, false, "GBLH");
            _gaq.push(['_trackEvent', 'MyMet', 'email lite']); _gaq.push(['_trackEvent', 'Home', 'Email']);
            return false;
        };
    });

