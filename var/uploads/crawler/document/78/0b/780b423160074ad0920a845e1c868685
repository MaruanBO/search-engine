    <script>
    
        function doSignOut() {
            $.ajax({
                type: "POST",
                url: location.toString(),
                data: { headSignOut: "Sign out" },
                success: function () { location.reload() },
                error: function () { window.location.href = "https://metmuseum.org/mymet/sign-up" }
            });
        }

    </script>
