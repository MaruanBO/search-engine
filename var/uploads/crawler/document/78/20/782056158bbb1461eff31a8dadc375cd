	$('#newsletterSignUpErrorAnime').hide();
	
	// show the form again
			$('#newsletterSignUpDivAnime .newsletterSignUpDivAnimeFormUI').show();
			// fade it up again
			$('#newsletterSignUpDivAnime .newsletterSignUpDivAnimeFormUI').fadeTo("slow", 1);	
}

