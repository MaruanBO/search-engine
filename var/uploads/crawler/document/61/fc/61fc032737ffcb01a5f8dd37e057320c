jQuery(document).ready(function($) {
	$("input[type=text]").focus(function () {
		var defVal = $(this)[0].defaultValue;
		if ($(this).val() == defVal) {
			$(this).val(""); 
		}
	});

	$("input[type=text]").blur(function () {
		var defVal = $(this)[0].defaultValue;
		if ($(this).val() == "") {
			$(this).val(defVal);
		}			
	});

	if($.cookie('socialBar') == 'closed'){
		$('#top_bar').hide();
	}

	//$('#top_bar').delay(1900).slideDown('slow');

		
	var cookieValueCTA = $.cookie("cta");
	
	if (cookieValueCTA != 1){
		$("#cta_block").show();
	} else {
		$("#cta_block2").show();
	}

});

function closeBar(){
	jQuery('#top_bar').slideUp('fast', function(){
		jQuery('#wrapper').css('margin-top', '1px'); 
	});
	jQuery.cookie('socialBar', 'closed', { expires: 1, path: '/' });
}

function recordOutboundLink(link, category, action) {
	try {
		var myTracker=_gat._getTrackerByName();
		_gaq.push(['myTracker._trackEvent', category , action ]);
		setTimeout('document.location = "' + link.href + '"', 100)
	}catch(err){}
}

function closeCTA() {
	jQuery.cookie("cta", 1, { expires : 2 });
	jQuery("#cta_block").slideUp();
	jQuery("#cta_block2").show();
	
}
