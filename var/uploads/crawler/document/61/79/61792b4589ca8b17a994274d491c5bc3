    <script type="text/javascript" charset="utf-8">
      jQuery(document).ready(function($) {
        $('.header input[name="email"]').click(function(e) {
          e.stopPropagation();
          $('#more_signup_fields').addClass('open');
        });
        
        $('body').click(function(e) {
          if (!$(e.target).closest('form').length) {
              $('#more_signup_fields').removeClass('open');
          }
        });
        
        $(".header .inner.head form.action_form").submit(function()
        {
        	// first do the error checks
        	var no_errors = (check_email(document.getElementById('id_email')) && valid('id_zip', translate('header_action_form_zip_error', 'Please enter a valid ZIP Code.'), translate('header_action_form_ZIP_label','ZIP Code'), 5))
        	
        	// if all is okay, lets remove the hidden element for main list signup if the checkbox is checked.
        	if (no_errors)
        	{
        		if ($(".header .inner.head form.action_form input.only_espanol").prop('checked'))
        		{
        			$(".header .inner.head form.action_form input.mainlist").remove();
        		}
        	}
        	return no_errors;
        });
      });
    </script>
