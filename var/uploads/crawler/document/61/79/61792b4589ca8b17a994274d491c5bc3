        var currentImageId = 0;
        var t = setTimeout("switchImage(true)", 5000);
        function switchImage(is_next)
        {
            clearTimeout(t);
            if( !is_next ) {
                currentImageId--;
            } else {
                currentImageId++;
            }

            if (currentImageId < 0 ) { currentImageId = images.length - 1;}
            else
            if (currentImageId > images.length - 1) { currentImageId = 0; }
            
            document.getElementById('flickr_link').href = links[currentImageId];
            document.getElementById('flickr_image').src = images[currentImageId]; 
            var txt = titles[currentImageId];
            if (txt.length > 100) {
              txt = txt.substring(0,100) + '<a href="'+links[currentImageId]+'" target="_blank">...</a>';
            }           
            document.getElementById('flickr_title').innerHTML = txt;
            t = setTimeout("switchImage(true)", 5000);
        }        
    </script>
