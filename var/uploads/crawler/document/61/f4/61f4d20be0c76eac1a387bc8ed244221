    <script type="text/javascript">
    (function(){
        var images = [
			"IMAGE_0000_1.jpg",
			"IMAGE_0001_2.jpg",
			"IMAGE_0002_3.jpg",
			"IMAGE_0003_4.jpg",
			"IMAGE_0004_5.jpg",
			"IMAGE_0005_6.jpg",
			"IMAGE_0006_7.jpg",
			"IMAGE_0007_8.jpg",
			"IMAGE_0008_9.jpg",
			"IMAGE_0009_10.jpg",
			"IMAGE_0010_11.jpg",
			"IMAGE_0011_12.jpg",
			"IMAGE_0012_13.jpg",
			"IMAGE_0013_14.jpg",
			"IMAGE_0015_16.jpg",
			"IMAGE_0016_17.jpg",
			"IMAGE_0017_18.jpg",
			"IMAGE_0018_19.jpg",
			"IMAGE_0019_20.jpg",
			"IMAGE_0020_21.jpg",
			"IMAGE_0021_22.jpg",
			"IMAGE_0022_23.jpg",
			"IMAGE_0023_24.jpg",
			"IMAGE_0024_25.jpg",
			"IMAGE_0025_26.jpg",
			"IMAGE_0026_27.jpg",
			"IMAGE_0027_28.jpg"
        ];

        var $goodsImages = $('.pattern-bg .goods-img');

        var arr = []
        while(arr.length <= 4){
          var randomnumber=getRandomInt(0, images.length-1);
          var found=false;
          for(var i=0;i<arr.length;i++){
            if(arr[i]==randomnumber){found=true;break}
          }
          if(!found)arr[arr.length]=randomnumber;
        }

        $goodsImages.each(function(index){
            var newSrc = '/wp-content/themes/hg-twentyten/hg-images/goods/' + images[arr[index]];
            $(this).attr('src', newSrc);
        });

        function getRandomInt(min, max) {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        }

    })();
    </script>

