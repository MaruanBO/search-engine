        //alert("channel:"+s.pageName);

        // This is to prevent duplicate tracking in the store locator
        function isLocatorQueryEmpty(){
            if ((window.location.href.toString().indexOf('searchLocStr',0) < 0) && 
                (window.location.href.toString().indexOf('sessAddress',0) < 0) && 
                (window.location.href.toString().indexOf('directions',0) < 0))
            {
                return true;
            }
        }
        // This is to prevent duplicate tracking in search results and store locator
        if(s.pageName != "search:results" && s.channel != "store-locator" ){
            var s_code=s.t();if(s_code)document.write(s_code);
        }else if (s.channel == "store-locator" && isLocatorQueryEmpty()){
            s.pageName = 'store-locator:splash';
            var s_code=s.t();if(s_code)document.write(s_code);
        }
        //alert(s.channel+"\n"+s.pageName);
