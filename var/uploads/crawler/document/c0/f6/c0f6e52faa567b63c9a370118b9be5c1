<script type="text/javascript" >
    function CallPage() {
    	
    	var DDllist = document.getElementById('ddllogin');
        var exitToHSA="";
    	
    	if(DDllist.value=="AccountHolder")
        {
            if (window.location.toString().toLowerCase().indexOf("edit") != -1)
            {
                exitToHSA="https://capp-stage-e-secure.optumhealthfinancial.com/portal/server.pt?OFSP_login=consumer";

               window.open(exitToHSA); 
                              }
            else
            {
                exitToHSA="https://secure.optumhealthfinancial.com/portal/server.pt?OFSP_login=consumer"; 
               window.open(exitToHSA);
            }
        }
        if(DDllist.value=="Employer")
        {
            if (window.location.toString().toLowerCase().indexOf("edit") != -1)
            {
        	  exitToHSA="https://capp-stage-e-secure.optumhealthfinancial.com/portal/server.pt?OFSP_login=employer";
                 window.open(exitToHSA); 
            }
            else
            {
    
        exitToHSA="https://secure.optumhealthfinancial.com/portal/server.pt?OFSP_login=employer";
              
window.open(exitToHSA); 
            }
        }
        if(DDllist.value=="HCP")
        {
            exitToHSA="";
            window.open("https://myservices.optumhealthpaymentservices.com/") ; 
        }
        
        //omniture tracking
        var templinkvars = s.linkTrackVars
        var templinkEvents = s.linktrackEvents

        s.linkTrackVars = "events,eVar46,";
        s.linkTrackEvents = "event21,event14";
        s.eVar46 = DDllist.options[DDllist.selectedIndex].text;
        s.events = "event21";
        if(exitToHSA!="")
        {
           s.linkTrackVars = "events,eVar46,eVar10,prop10,prop11";
           s.events = "event21,event14";
           s.eVar10=s.prop10=exitToHSA;
           s.prop11=s.pageName;
        }
        try {
            s.tl(this, 'o', 'Account Login');
        }
        catch (err) {
            /* do nothing */
        }
        s.linkTrackVars = templinkvars
        s.linkTrackEvents = templinkevents
        }
