<script type="text/javascript" src="http://www.onestopplus.com/scripts/brandscripts/jquery.cycle.js?WEB-REL-2015-02-10-150212-164112"></script>


 <script type="text/javascript" src="http://www.brylanehome.com/scripts/brandscripts/jquery.cycle.js?WEB-REL-2015-02-10-150212-164112"></script>

<script language="javascript" type="text/javascript">
	$slideshow = {
		context: false,
		tabs: false,
		timeout: 5500,      // time before next slide appears (in ms)
		slideSpeed: 1000,   // time it takes to slide in each slide (in ms)
		tabSpeed: 300,      // time it takes to slide in each slide (in ms) when clicking through tabs
		fx: 'scrollHorz',   // the slide effect to use
		
		init: function() {
			// set the context to help speed up selectors/improve performance
			this.context = $('#slideshow');
			// set tabs to current hard coded navigation items
			this.tabs = $('ul.slides-nav li', this.context);
			// remove hard coded navigation items from DOM 
			// because they aren't hooked up to jQuery cycle
			this.tabs.remove();
			// prepare slideshow and jQuery cycle tabs
			this.prepareSlideshow();
		},
		
		prepareSlideshow: function() {
			// initialise the jquery cycle plugin -
			// for information on the options set below go to: 
			// http://malsup.com/jquery/cycle/options.html
			$('div.slides > ul', $slideshow.context).cycle({
				fx: $slideshow.fx,
				timeout: $slideshow.timeout,
				speed: $slideshow.slideSpeed,
				fastOnEvent: $slideshow.tabSpeed,
				pager: $('ul.slides-nav', $slideshow.context),
				pagerAnchorBuilder: $slideshow.prepareTabs,
				before: $slideshow.activateTab,
				pauseOnPagerHover: true,
				pause: false
			});            
		},
		
		prepareTabs: function(i, slide) {
			// return markup from hardcoded tabs for use as jQuery cycle tabs
			// (attaches necessary jQuery cycle events to tabs)
			return $slideshow.tabs.eq(i);
		},

		activateTab: function(currentSlide, nextSlide) {
			// get the active tab
			var activeTab = $('a[href="#' + nextSlide.id + '"]', $slideshow.context);
			// if there is an active tab
			if(activeTab.length) {
				// remove active styling from all other tabs
				$slideshow.tabs.removeClass('on');
				// add active styling to active button
				activeTab.parent().addClass('on');
			}            
		}            
	};

	$(function() {
		// add a 'js' class to the body
		$('body').addClass('js');
		// initialise the slideshow when the DOM is ready
		$slideshow.init();
	});  
</script>




<script language="javascript" type="text/javascript">
	$smallslideshow = {
		context: false,
		tabs: false,
		timeout: 56500,      // time before next slide appears (in ms)
		slideSpeed: 1000,   // time it takes to slide in each slide (in ms)
		tabSpeed: 300,      // time it takes to slide in each slide (in ms) when clicking through tabs
		fx: 'scrollHorz',   // the slide effect to use
		
		init: function() {
			// set the context to help speed up selectors/improve performance
			this.context = $('#smallslideshow');
			// set tabs to current hard coded navigation items
			this.tabs = $('ul.slidessm-nav li', this.context);
			// remove hard coded navigation items from DOM 
			// because they aren't hooked up to jQuery cycle
			this.tabs.remove();
			// prepare slideshow and jQuery cycle tabs
			this.preparesmallSlideshow();
		},
		
		preparesmallSlideshow: function() {
			// initialise the jquery cycle plugin -
			// for information on the options set below go to: 
			// http://malsup.com/jquery/cycle/options.html
			$('div.slidessm > ul', $smallslideshow.context).cycle({
				fx: $smallslideshow.fx,
				timeout: $smallslideshow.timeout,
				speed: $smallslideshow.slideSpeed,
				fastOnEvent: $smallslideshow.tabSpeed,
				pager: $('ul.slidessm-nav', $smallslideshow.context),
				pagerAnchorBuilder: $smallslideshow.prepareTabs,
				before: $smallslideshow.activateTab,
				pauseOnPagerHover: true,
				pause: false
			});            
		},
		
		prepareTabs: function(i, slide) {
			// return markup from hardcoded tabs for use as jQuery cycle tabs
			// (attaches necessary jQuery cycle events to tabs)
			return $smallslideshow.tabs.eq(i);
		},

		activateTab: function(currentSlide, nextSlide) {
			// get the active tab
			var activeTab = $('a[href="#' + nextSlide.id + '"]', $smallslideshow.context);
			// if there is an active tab
			if(activeTab.length) {
				// remove active styling from all other tabs
				$smallslideshow.tabs.removeClass('on');
				// add active styling to active button
				activeTab.parent().addClass('on');
			}            
		}            
	};
	
	$(function() { 
	     // add a 'js' class to the body 
	     $('body').addClass('js-sm'); 
	     // initialise the slideshow when the DOM is ready 
	     $smallslideshow.init(); 
	 }); 

	
</script>


<script language="javascript" type="text/javascript">
$NA_slideshow = {
    context: false,
    tabs: false,
    timeout: 7500,      // time before next slide appears (in ms)
    slideSpeed: 1650,   // time it takes to slide in each slide (in ms)
    tabSpeed: 300,      // time it takes to slide in each slide (in ms) when clicking through tabs
    fx: 'scrollLeft',   // the slide effect to use
    
    init: function() {
        // set the context to help speed up selectors/improve performance
        this.context = $('#NA_slideshow');
        
        // set tabs to current hard coded navigation items
        this.tabs = $('ul.NA_slides-nav li', this.context);
        
        // remove hard coded navigation items from DOM 
        // because they aren't hooked up to jQuery cycle
        this.tabs.remove();
        
        // prepare NA_slideshow and jQuery cycle tabs
        this.prepareNA_slideshow();
    },
    
    prepareNA_slideshow: function() {
        // initialise the jquery cycle plugin -
        // for information on the options set below go to: 
        // http://malsup.com/jquery/cycle/options.html
        $('div.NA_slides > ul', $NA_slideshow.context).cycle({
            fx: $NA_slideshow.fx,
            timeout: $NA_slideshow.timeout,
            speed: $NA_slideshow.slideSpeed,
            fastOnEvent: $NA_slideshow.tabSpeed,
            pager: $('ul.NA_slides-nav', $NA_slideshow.context),
            pagerAnchorBuilder: $NA_slideshow.prepareTabs,
            before: $NA_slideshow.activateTab,
            pauseOnPagerHover: true,
            pause: true
        });            
    },
    
    prepareTabs: function(i, slide) {
        // return markup from hardcoded tabs for use as jQuery cycle tabs
        // (attaches necessary jQuery cycle events to tabs)
        return $NA_slideshow.tabs.eq(i);
    },

    activateTab: function(currentSlide, nextSlide) {
        // get the active tab
        var activeTab = $('a[href="#' + nextSlide.id + '"]', $NA_slideshow.context);
        
        // if there is an active tab
        if(activeTab.length) {
            // remove active styling from all other tabs
            $NA_slideshow.tabs.removeClass('on');
            
            // add active styling to active button
            activeTab.parent().addClass('on');
        }            
    }            
};


$(function() {
    // add a 'js' class to the body
    $('body').addClass('js');
    
    // initialise the NA_slideshow when the DOM is ready
    $NA_slideshow.init();
});    
</script>

<script language="javascript" type="text/javascript">
$smallslideshow = {
context: false,
tabs: false,
timeout: 7500,      // time before next slide appears (in ms)
slideSpeed: 350,   // time it takes to slide in each slide (in ms)
tabSpeed: 300,      // time it takes to slide in each slide (in ms) when clicking through tabs
fx: 'scrollHorz',   // the slide effect to use
init: function() {
// set the context to help speed up selectors/improve performance
this.context = $('#smallslideshow');
// set tabs to current hard coded navigation items
this.tabs = $('ul.slidessm-nav li', this.context);
// remove hard coded navigation items from DOM 
// because they aren't hooked up to jQuery cycle
this.tabs.remove();
// prepare slideshow and jQuery cycle tabs
this.preparesmallSlideshow();
},
preparesmallSlideshow: function() {
// initialise the jquery cycle plugin -
// for information on the options set below go to: 
// http://malsup.com/jquery/cycle/options.html
$('div.slidessm > ul', $smallslideshow.context).cycle({
fx: $smallslideshow.fx,
timeout: $smallslideshow.timeout,
speed: $smallslideshow.slideSpeed,
fastOnEvent: $smallslideshow.tabSpeed,
pager: $('ul.slidessm-nav', $smallslideshow.context),
pagerAnchorBuilder: $smallslideshow.prepareTabs,
before: $smallslideshow.activateTab,
pauseOnPagerHover: true,
pause: false
});            
},
prepareTabs: function(i, slide) {
// return markup from hardcoded tabs for use as jQuery cycle tabs
// (attaches necessary jQuery cycle events to tabs)
return $smallslideshow.tabs.eq(i);
},

activateTab: function(currentSlide, nextSlide) {
// get the active tab
var activeTab = $('a[href="#' + nextSlide.id + '"]', $smallslideshow.context);
// if there is an active tab
if(activeTab.length) {
// remove active styling from all other tabs
$smallslideshow.tabs.removeClass('on');
// add active styling to active button
activeTab.parent().addClass('on');
}            
}            
};
$(function() { 
    // add a 'js' class to the body 
    $('body').addClass('js-sm'); 
    // initialise the slideshow when the DOM is ready 
    $smallslideshow.init(); 
});
 
</script>



