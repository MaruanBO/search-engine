var log = document.getElementById("log");
var html = log.innerHTML;

jwplayer().onReady(function () {
    var playlist = jwplayer().getPlaylist();
    for (var index = 0; index < playlist.length; index++) {
        var playindex = index + 1;
        html += "<li><span class='dropt' title='" + playlist[index].title + "'><a href='#container1' onclick='jwplayer().playlistItem(" + index + ");'><img height='100' width='180' src='" + playlist[index].image + "'</img></br>" + "</a></br></span></li>";
        log.innerHTML = html;
    }

});

jwplayer().onPlaylistItem(function (e) {
    if (e.index > 0) {
        setTimeout(function () {
            jwplayer().stop();
        }, 10);
    }
});

