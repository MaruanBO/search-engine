      <script type="text/javascript">
        function getActions()
        {
             var fb_div = "";
             var i;
             FB.api("/me/news.reads", function(myactions)
              {
                   for(i=0;i<5;i++)
                   {
                   console.log(myactions["data"].length);
                   console.log(myactions["data"]);
                        var actionId = myactions["data"][i]["id"];
                        fb_div = fb_div + "<br/><div class='activity-table-cnt'><table cellpadding='0' cellspacing='0' style='border-top: 1px solid #CCCCCC;'><tr><td width='260' style='padding-top:15px;'>"+ myactions['data'][i]['data']['article']['title'] +"</td><td style='padding-top:15px;'><a  style='background:url(http://www.pinkvilla.com/sites/all/modules/social_reader/btn-cross_07.png) no-repeat; width:13px; height:12px; display:block; margin:0 0 0 5px;' href='#' onclick='deleteAction("+actionId+"); return false;'></a></td></tr></table></div>";    
                   }
                   if(fb_div == "")
                   {
                        fb_div = "No action found";
                   }  
                   document.getElementById("fb-activity").innerHTML = fb_div;
              }
            );
        }

        function newCookie(currentElement)
        {
                var btn_state;
                var fb_user = FB.getAuthResponse(); 
                btn_state = currentElement.value;
                    
                if(btn_state == "1")
                    { 
                        set_cookie(fb_user["userID"], btn_state);
                    }
                else
                    {
                        set_cookie(fb_user["userID"], btn_state);
                    }                    
        }        
         
         function set_cookie ( name, value )
         {
            var cookie_string = name + "=" + escape ( value );
            document.cookie = cookie_string;
         }
         function get_cookie ( cookie_name )
         {
             var results = document.cookie.match ( "(^|;) ?" + cookie_name + "=([^;]*)(;|$)" );
    
             if ( results ){
                return ( unescape ( results[2] ) );
    			}
             else
                return null;   
         }
         </script>
