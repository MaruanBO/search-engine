			{ "id": "empuLn", "defaultValue": "last name" },
			{ "id": "empuZc", "defaultValue": "zip/postal code" },
			{ "id": "empuCntry", "defaultValue": "country" }
		];
	$(document).ready(function () {
		for (var i = 0; i < empuFields.length; i++) {
			$('#' + empuFields[i].id).val(empuFields[i].defaultValue); }
	});
</script>
