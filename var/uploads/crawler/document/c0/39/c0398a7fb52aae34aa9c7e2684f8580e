<script type="text/javascript">
	var thisPage, emlSbscrbDlg, emlThnkYouDlg; thisPage = "home.aspx";
	$.ajax({ url: 'miscSrvrRtns.aspx?actn=emailSignupCheck&thspg=' + thisPage + '&uid=' + makeUid(), async: true,
		error: function (jqXHR, textStatus, errorThrown) { },
		success: function (response) {
			if (response != 'No') {
				var countryControlSettings = {
					"inputId": "empuCntry", "idPrefix": "cntry_", "countries": eval("(" + response + ")"), "textColor": "#555555", "listWidth": 260, "listHeight": 240,
					"listZIndex": 9999993, "listItemHeight": 16, "initialValue": "country", "defaultValue": "USA" };
				var emlCntryCtrl = new CountryControl(countryControlSettings);
				var settings = { "id": "empu", "contentDivId": "empuContent" };
				settings.onEnterPressed = function () { $('#empuSubmitImg').click(); };
				emlSbscrbDlg = new ModalDialog(settings);
				emlThnkYouDlg = new ModalDialog({ "id": "emputy", "contentDivId": "emputyContent" });
				$(document).ready(function () { emlSbscrbDlg.open(); }); } }
	});
	function empuSubmitClicked() {
		var emailAddress, firstName, lastName, zipCode, country, url, fieldErrors, focusField, fieldName;
		emailAddress = inputTxtGet(0); firstName = inputTxtGet(1); lastName = inputTxtGet(2); zipCode = inputTxtGet(3); country = inputTxtGet(4);
		url = 'miscSrvrRtns.aspx?actn=emailSignUp&ema=' + emailAddress + '&fn=' + firstName + '&ln=' + lastName + '&zc=' + zipCode + '&cntry=' + country + "&uid=" + makeUid();
		$.get(url, function (data) {
			if (data == "Ok") { emlSbscrbDlg.close(); emlThnkYouDlg.open(); }
			else {
				fieldErrors = eval("(" + data + ")"); focusField = "";
				$('.empuErr').each(function (i, obj) {
					fieldName = obj.id.substr(4, (obj.id.length - 7)).toLowerCase();
					if (fieldErrors[fieldName]) { $(this).html(fieldErrors[fieldName]); if (focusField == "") focusField = obj.id.substr(0, (obj.id.length - 3)); }
					else $(this).html('&nbsp;');
				});
				if (focusField != "") $('#' + focusField).focus();
			}
		});
	}
	function inputTxtGet(fieldNumber) {
		var $inputObj, fieldTxt;
		$inputObj = $('#' + empuFields[fieldNumber].id); fieldTxt = $inputObj.val(); if (fieldTxt == empuFields[fieldNumber].defaultValue) fieldTxt = "";
		return encodeURIComponent(fieldTxt);
	}
	function inputTxtFocus(fieldNumber) {
		var $inputObj = $('#' + empuFields[fieldNumber].id); if ($inputObj.val() == empuFields[fieldNumber].defaultValue) $inputObj.val("");
	}
	function inputTxtBlur(fieldNumber) {
		var $inputObj = $('#' + empuFields[fieldNumber].id); if ($inputObj.val() == "") $inputObj.val(empuFields[fieldNumber].defaultValue);
	}
</script>

