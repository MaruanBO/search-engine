<script charset="UTF-8">
window.fbAsyncInit = function() {
    FB.init({
        appId      : fbAppId,
        status     : true, // check login status
        cookie     : true, // enable cookies to allow the server to access the session
        xfbml      : true,  // parse XFBML
        version    : 'v2.0'
    });
    FB.Event.subscribe('auth.authResponseChange', function(response) {
        if (response.status === 'connected') {
            PizapLogin.handleFBLoginResponse(response);
        } else if (response.status === 'not_authorized') {
            if(isPizapBook && !displayPublic){
                top.location.href='https://www.facebook.com/appcenter/' + fbNamespace + "?redirect_uri=" + encodeURI(window.location);
            }
        } else {
            if(isPizapBook && !displayPublic){
                top.location.href='https://www.facebook.com/appcenter/' + fbNamespace + "?redirect_uri=" + encodeURI(window.location);
            }
        }
        if(isPizapBook)
            FB.Canvas.scrollTo(0,0);

        if ( window.self !== window.top )
            FB.Canvas.setAutoGrow();
        try{
            page_defer();
        }catch(e) {

        }
    });

    if(isPizapBook && !displayPublic){
        var container = document.getElementById("container");
        if(container){
            FB.Canvas.scrollTo(0,0);
            FB.Canvas.setAutoGrow();
            var interval = setInterval(function(){
                FB.Canvas.getPageInfo(
                        function(info) {
                            var frameHeight = document.documentElement.clientHeight;
                            var offsetTop = info.offsetTop;
                            var scrollTop = info.scrollTop;
                            var fbTollernce = 1500;

                            if ((scrollTop + offsetTop >= frameHeight - fbTollernce)) {
                                infiniteScroll();
                            }
                        });

            }, 100);
        }
    }
};



var isMobile = {
    Android: function() {
        return navigator.userAgent.match(/Android/i);
    },
    BlackBerry: function() {
        return navigator.userAgent.match(/BlackBerry/i);
    },
    iOS: function() {
        return navigator.userAgent.match(/iPhone|iPad|iPod/i);
    },
    Opera: function() {
        return navigator.userAgent.match(/Opera Mini/i);
    },
    Windows: function() {
        return navigator.userAgent.match(/IEMobile/i);
    },
    any: function() {
        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
    }
};


if(document.getElementById('login-username')){
    document.getElementById('login-username').onkeyup = function(e){
        if(e.which == 13) {
            PizapLogin.pizapLogin();
        }
    };
    document.getElementById('login-password').onkeyup = function(e){
        if(e.which == 13) {
            PizapLogin.pizapLogin();
        }
    };
}

if(isMobile.iOS())
{
    var diviphone = document.getElementById("diviphone");
    if(diviphone)
        diviphone.style.visibility = "visible";
}
if(isMobile.Android())
{
    var divandroid = document.getElementById("divandroid");
    if(divandroid)
        divandroid.style.visibility = "visible";
}
if( isMobile.any() )
{
    var tdInviteFriends = document.getElementById("tdInviteFriends");
    var tdFBShare = document.getElementById("tdFBShare");
    if(tdFBShare)
        tdFBShare.innerHTML = "";
    if(tdInviteFriends)
        tdInviteFriends.innerHTML = "";
}

if(document.getElementById("searchheader")){
    document.getElementById("searchheader").addEventListener('keyup', function(e) {
        if(e.which == 13) {
            _gaq.push(['_trackEvent', 'search', 'search', document.getElementById("searchheader").value]);
            if(isPizapBook){
                top.location.href='//apps.facebook.com/' + fbNamespace + "/search/" + encodeURI(document.getElementById("searchheader").value);
            }else{
                window.location.href = "/search/" + encodeURI(document.getElementById("searchheader").value);
            }
        }
    });
}
