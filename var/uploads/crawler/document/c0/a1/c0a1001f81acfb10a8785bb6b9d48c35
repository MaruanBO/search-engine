<script charset="UTF-8">
function isVisible(obj)
{
    if (obj == document) return true

    if (!obj) return false
    if (!obj.parentNode) return false
    if (obj.style) {
        if (obj.style.display == 'none') return false
        if (obj.style.visibility == 'hidden') return false
    }

    //Try the computed style in a standard way
    if (window.getComputedStyle) {
        var style = window.getComputedStyle(obj, "")
        if (style.display == 'none') return false
        if (style.visibility == 'hidden') return false
    }

    //Or get the computed style using IE's silly proprietary way
    var style = obj.currentStyle
    if (style) {
        if (style['display'] == 'none') return false
        if (style['visibility'] == 'hidden') return false
    }

    return isVisible(obj.parentNode)
}


var getJSON = function(url, methodType, myData, successHandler, errorHandler) {
    var xhr = typeof XMLHttpRequest != 'undefined'
            ? new XMLHttpRequest()
            : new ActiveXObject('Microsoft.XMLHTTP');
    xhr.open(methodType, url, true);
    xhr.onreadystatechange = function() {
        var status;
        var data;
        // http://xhr.spec.whatwg.org/#dom-xmlhttprequest-readystate
        if (xhr.readyState == 4) { // `DONE`
            status = xhr.status;
            if (status == 200) {
                try{
                    data = JSON.parse(xhr.responseText);
                    successHandler && successHandler(data);
                }catch(e){
                    successHandler && successHandler(xhr.responseText);
                }
            } else {
                errorHandler && errorHandler(status);
            }
        }
    };
    xhr.setRequestHeader("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
    if(myData != null)
        xhr.send(myData);
    else
        xhr.send();
};
    function openIframeDiv(url){

    }
</script>
    <script charset="UTF-8">
        function getFlashMovieObject2(movieName){
            if (window.document[movieName]){
                return window.document[movieName];
            }
            if (navigator.appName.indexOf("Microsoft Internet")==-1){
                if (document.embeds && document.embeds[movieName])
                    return document.embeds[movieName];
            }
            else{
                return document.getElementById(movieName);
            }
        }
    </script>
<script charset="UTF-8">
var PizapLogin = {
    showLoginDialog : function(network, permission){
        var sel = document.getElementById("loginDialog");
        if(sel)
            sel.style.display = 'block'
        var sel = document.getElementById("greybackground");
        if(sel)
            sel.style.display = "block";
        if(network){
            if(network == "facebook"){
                if(permission){
                    PizapLogin.showFacebook(permission);
                }else{
                    PizapLogin.showFacebook();
                }
            }
            if(network == "twitter"){
                PizapLogin.showTwitter();
            }
            if(network == "google"){
                PizapLogin.showGoogle();
            }
        }
    },
    showRegisterDialog : function(){
        var sel = document.getElementById("loginDialog");
        if(sel)
            sel.style.display = 'block'
        var sel = document.getElementById("greybackground");
        if(sel)
            sel.style.display = "block";
        PizapLogin.showRegister();
    },
    hideLoginDialog : function(){
        var sel = document.getElementById("loginDialog");
        if(sel)
            sel.style.display = 'none'
        var sel = document.getElementById("greybackground");
        if(sel)
            sel.style.display = "none";

        PizapLogin.resetLogin();
    },
    resetLogin: function(){
        var register = document.getElementById("login-link-register");
        if(register)
            register.style.display = "block";

        var email = document.getElementById("email");
        if(email)
            email.style.display = "none";

        var emailconfirm = document.getElementById("emailconfirm");
        if(emailconfirm)
            emailconfirm.style.display = "none";

        var resetText = document.getElementById("resetPasswordText");
        if(resetText)
            resetText.style.display = "none";

        var password = document.getElementById("login-password");
        if(password)
            password.style.display = "block";

        var button = document.getElementById("login-button-login-submit");
        if(button){
            button.innerHTML = "Log in";
            button.onclick = PizapLogin.pizapLogin;
        }

        var fbLogin = document.getElementById("fbLogin");
        if(fbLogin)
            fbLogin.style.display = "block";

        var socialLogins = document.getElementById("socialLogins");
        if(socialLogins)
            socialLogins.style.display = "block";

        var loginOr = document.getElementById("loginOr");
        if(loginOr)
            loginOr.style.display = "block";

        var loginForm = document.getElementById("loginForm");
        if(loginForm)
            loginForm.style.top = "230px";

        var passswordChange = document.getElementById("login-link-password");
        if(passswordChange){
            passswordChange.innerHTML = "Forgot your password?";
            passswordChange.onclick = PizapLogin.passwordReset;
        }

    },
    setTwitterData: function(username, name){
        PizapLogin.hideLoginDialog();
        if(editorPage){
            PizapLogin.getUser(PizapLogin.updateHeader);

            var flashMovie=getFlashMovieObject("flashContent");
            if(flashMovie){
                try{
                    flashMovie.twitterAuthenticated(username);
                }catch(e){}
            }
        }else{
            if(isPizapBook){
                top.location.href='//apps.facebook.com/' + fbNamespace + "/gallery/following";
            }else{
                window.location.href = "/gallery/following";
            }
        }

    },
    setGPlusData: function(name){
        PizapLogin.hideLoginDialog();
        if(editorPage){
            PizapLogin.getUser(PizapLogin.updateHeader);
        }else{
            if(isPizapBook){
                top.location.href='//apps.facebook.com/' + fbNamespace + "/gallery/following";
            }else{
                window.location.href = "/gallery/following";
            }
        }
    },
    updateHeader: function(data){
        var userMenu = document.getElementById("userMenu");
        if(userMenu){
            var name = data.Name ? data.Name : data.UserName;
            var c = "<table style='cursor: pointer;' onclick='showHeaderMenu()' id='accountMenu'><tr><td><span style='font-size: 14px;color:#FFF;padding-right:15px;'>"+name+"</span></td></tr></table>";
            userMenu.innerHTML = c;
        }
        var flashMovie=getFlashMovieObject2("flashContent");

        if(flashMovie){
            try{
                FB.api('/me/permissions', function(response){
                    if(flashMovie){
                        try{
                            data.permissions = response;
                            flashMovie.userLoggedIn(data);
                        }catch(e){}
                    }
                });

            }catch(e){
                try{
                    flashMovie.userLoggedIn(data);
                }catch(e){}
            }
        }
    },
    getUser: function(callback){
        getJSON('/me', 'GET', null, callback);
    },
    showTwitter: function(){
        var left = (screen.width/2)-(400);
        var top = (screen.height/2)-(300);
        var win = window.open("/twitter/login?dialog=true",'','width=800,height=500,top='+top+',left='+left);
    },
    showFacebook: function(permission){
        if(permission){
            FB.login(function(response) {
            }, {scope: 'email,' + permission});
        }else{
            FB.login(function(response) {
            }, {scope: 'email'});
        }
    },
    showGoogle: function(){
        var left = (screen.width/2)-(400);
        var top = (screen.height/2)-(300);
        var win = window.location.href = "/gpluslogin?dialog=true";
    },
    handleFBLoginResponse: function(response){
        if (response.status === "connected")
        {
            FB.api('/'+response.authResponse.userID, function(data) {
                var email = "";
                if(data.email)
                    email = data.email;

                PizapLogin.hideLoginDialog();
                var myData = 'name=' + encodeURIComponent(data.name) + '&id='+encodeURIComponent(data.id)+'&email='+encodeURIComponent(email)+'&gender='+encodeURIComponent(data.gender)+'&token='+response.authResponse.accessToken;
                xmlhttp=new XMLHttpRequest();
                xmlhttp.onload = function (){
                    if(!homePage){
                        PizapLogin.getUser(PizapLogin.updateHeader);
                    }else{
                        if(username == ''){
                            if(isPizapBook){
                                top.location.href='//apps.facebook.com/' + fbNamespace + "/gallery/following";
                            }else{
                                window.location.href = "/gallery/following";
                            }
                        }
                    }
                };
                xmlhttp.open("POST","/process_facebook",true);
                xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
                xmlhttp.send(myData);
            });
        }
    },
    pizapLogin: function(){
        var form = document.getElementById('loginForm');
        if(form.checkValidity()){
            var username = document.getElementById('login-username');
            var password = document.getElementById('login-password');

            var myData = 'isajax=1&username=' + encodeURIComponent(username.value) + '&password=' + encodeURIComponent(password.value);
            getJSON('/login', 'POST', myData, function(data){
                if(data.Error){
                    alert(data.Error);
                }else{
                    PizapLogin.hideLoginDialog();
                    if(editorPage == true){
                        PizapLogin.updateHeader(data);
                    }else{
                        if(isPizapBook){
                            top.location.href='//apps.facebook.com/' + fbNamespace + "/gallery/following";
                        }else{
                            window.location.href = "/gallery/following";
                        }
                    }
                }
            });
        }
        return false;
    },
    passwordReset: function(){
        var register = document.getElementById("login-link-register");
        if(register)
            register.style.display = "none";

        var resetText = document.getElementById("resetPasswordText");
        if(resetText)
            resetText.style.display = "block";

        var password = document.getElementById("login-password");
        if(password)
            password.style.display = "none";

        var fbLogin = document.getElementById("fbLogin");
        if(fbLogin)
            fbLogin.style.display = "none";

        var socialLogins = document.getElementById("socialLogins");
        if(socialLogins)
            socialLogins.style.display = "none";

        var loginOr = document.getElementById("loginOr");
        if(loginOr)
            loginOr.style.display = "none";

        var loginForm = document.getElementById("loginForm");
        if(loginForm)
            loginForm.style.top = "100px";

        var passswordChange = document.getElementById("login-link-password");
        if(passswordChange){
            passswordChange.innerHTML = "Go back";
            passswordChange.onclick = PizapLogin.resetLogin;
        }

        var button = document.getElementById("login-button-login-submit");
        if(button){
            button.innerHTML = "Change Password";
            button.onclick = function(){
                var username = document.getElementById('login-username');

                var myData = 'isajax=1&username=' + encodeURIComponent(username.value);
                getJSON('/change_password', 'POST', myData, function(data){
                    if(data.Error){
                        alert(data.Error);
                    }else{
                        alert(data.Success);
                        PizapLogin.hideLoginDialog();
                    }
                });
            }
        }
        return false;
    },
    showRegister: function(){
        var register = document.getElementById("login-link-register");
        if(register)
            register.style.display = "none";

        var email = document.getElementById("email");
        if(email)
            email.style.display = "block";

        var emailconfirm = document.getElementById("emailconfirm");
        if(emailconfirm)
            emailconfirm.style.display = "block";

        var fbLogin = document.getElementById("fbLogin");
        if(fbLogin)
            fbLogin.style.display = "none";

        var socialLogins = document.getElementById("socialLogins");
        if(socialLogins)
            socialLogins.style.display = "none";

        var loginOr = document.getElementById("loginOr");
        if(loginOr)
            loginOr.style.display = "none";

        var loginForm = document.getElementById("loginForm");
        if(loginForm)
            loginForm.style.top = "100px";

        var passswordChange = document.getElementById("login-link-password");
        if(passswordChange){
            passswordChange.innerHTML = "Go back";
            passswordChange.onclick = PizapLogin.resetLogin;
        }

        var button = document.getElementById("login-button-login-submit");
        if(button){
            button.innerHTML = "Register";
            button.onclick = function(){
                var username = document.getElementById('login-username');
                var password = document.getElementById('login-password');
                var email = document.getElementById('email');
                var emailconfirm = document.getElementById('emailconfirm');

                var myData = 'isajax=1&username=' + encodeURIComponent(username.value) + '&password=' + encodeURIComponent(password.value) + '&email=' + encodeURIComponent(email.value) + '&email_c=' + encodeURIComponent(emailconfirm.value) + '&password_c=' + encodeURIComponent(password.value);
                getJSON('/register', 'POST', myData, function(data){
                    if(data.Error){
                        alert(data.Error);
                    }else{
                        PizapLogin.hideLoginDialog();
                        if(editorPage == true){
                            PizapLogin.updateHeader(data);
                        }else{
                            if(isPizapBook){
                                top.location.href='//apps.facebook.com/' + fbNamespace + "/gallery/following";
                            }else{
                                window.location.href = "/gallery/following";
                            }
                        }
                    }
                });
            }
        }
        return false;
    },
    facebookPermission:function(){
        var flashMovie=getFlashMovieObject("flashContent");

        FB.getLoginStatus(function(response) {
            if (response.status === 'connected') {
                FB.api('/me/permissions', function(response){
                    if(flashMovie){
                        try{
                            flashMovie.facebookPermissionResponse(response);
                        }catch(e){}
                    }
                });
            } else if (response.status === 'not_authorized') {
                if(flashMovie){
                    try{
                        flashMovie.facebookPermissionResponse(response);
                    }catch(e){}
                }
            } else {
                if(flashMovie){
                    try{
                        flashMovie.facebookPermissionResponse(response);
                    }catch(e){}
                }
            }
        });
    }
};
