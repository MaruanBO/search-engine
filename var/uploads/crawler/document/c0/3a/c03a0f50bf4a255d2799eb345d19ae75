function getRenderedAds(type){
    var rendered_ads = null;
    var list = { 'search' : {'default_seo' : [[null, 'googlecsa', 'mid-1'], [null, 'googlecsa', 'mid-2']],
		                      'default_sem' : [[null, 'googlecsa', 'mid-1'], [null, 'googlecsa', 'mid-2']]}, 
				'category' : { 'miva' : null,
    	    				'default' : [[null, 'googleafc', 'mid-1'], [null, 'googleafc', 'bot-1']]}, 
				'article' : { 'miva' : null,
    	    				'default' : [[null, 'googleafc', 'mid-1'], [null, 'googleafc', 'bot-1']]}};
	switch(type){
		case "search":
		    if('/' == '/search.php') rendered_ads = list[type]['default_seo'];
            else rendered_ads = list[type]['default_sem'];
	    	break;
	    case "category":
	    case "article":
		    if('' == 'miva'){
		    	rendered_ads = list[type]['miva'];
	    	}else{
	    		rendered_ads = list[type]['default'];
	    	}
        	break;
    }
	return rendered_ads;
}
