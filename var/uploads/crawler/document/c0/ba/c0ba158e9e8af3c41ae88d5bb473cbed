<script type="text/javascript">

function ValidateEmail() {

    var emailAddress = document.forms['emailSignUpForm'].userEmail.value;
    var atpos = emailAddress.indexOf("@");
    var dotpos = emailAddress.lastIndexOf(".");
    var hastings = document.getElementById("hastings").checked;
    var deal_of_day = document.getElementById("deal_of_day").checked;

    if (atpos < 1 || dotpos<atpos+2 || dotpos+2>=emailAddress.length) {
        alert("Please enter valid email");
        return false;
    } 

    if (hastings == false && deal_of_day == false) {
        alert("Please check a list");
        return false;
    }
            
            if (emailAddress.indexOf(' ') >= 0) {
        alert("Please remove leading or trailing white space from email");
        return false;
    }
    return true;
}
    
</script>
</form>

<p><font style="float:left;">You can <a href="/custserv/custserv.jsp?pageName=Email_Unsubscribe">unsubscribe</a> at any time.</font></p>

 <!-- this script tag needs to go ABOVE all of the Listrak JavaScript script tags, like it is on this page -->
    <script type="text/javascript">
        
        //gets variables out of the url by name.  So in a url like this: http://www.example.com/?pageName=blah&foo=bar,
        //you would pass it "pageName" to get the value "blah", and "foo" to get the value "bar"
        function getURLParameter(name) {
            return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
        }
        
        //get the page name.  This will determine where to go from here.
        var pageName = getURLParameter("pageName");
        //this is the name of the page that we want to go to after submitting an email with the form.
        var confirmSignupPageName = "Email_Signup_Confirmation";
        var email = null;
        
                
        //if the page name is the email confirmation page's name, then set the email variable to be the url parameter.
        if (pageName == confirmSignupPageName) {
            email = getURLParameter("email");
        }
        
    </script>
    
<!-- This script segment is intended to be added to production.  It must be added AFTER the Listrak Analytics Script Source, 
         and it would be a good idea to leave it in its own script segment.  It may not work otherwise. -->

