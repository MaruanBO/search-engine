                     <script>
              
               //re-alignment the size between each category from top nav 
                  function reAlignmentTopNavigation() {
                  
                     var navWidth = 0;
                     var navColumns = 0;
                     var eachSide = 0;
                     var rigth = 0;
                     var navMargin = 3;
                     var navMaxSize = $(".navContainer").width();
                     var roundRemaining = 0;
                     var roundIncrement = 0;
                     
                     var is_chrome = navigator.userAgent.indexOf('Chrome') > -1;
                     var is_safari = navigator.userAgent.indexOf("Safari") > -1;
                     if ((is_chrome)&&(is_safari)) {is_safari=false;}
                     
                     $(".normalFW").each(function( index ) {
                        navColumns = navColumns + 1;
                        navWidth += $(this).width();
                     });
                  
                     var remaining = navMaxSize - (navMargin + navWidth + navMargin);
                     
                     eachSide = (remaining/((navColumns-1)*2));
                        eachSide = (remaining/((navColumns-1)*2)).toFixed(0);
                     roundRemaining = remaining - (eachSide*2*(navColumns-1));
                     var decimals = (roundRemaining * 100) % 100;
                     if (decimals != 0) {
                        if (0 > roundRemaining ) {
                           roundRemaining = roundRemaining + (decimals*0.01);
                        }
                        else {
                           roundRemaining = roundRemaining - (decimals*0.01);
                        }
                     }
                     
                     $(".normalFW").each(function( index ) {
                        if (index == 0) {
                           $(this).css('padding', '5px '+ eachSide + 'px 5px ' + navMargin + 'px' );
                        }
                        else if (index == (navColumns-1)) {
                           $(this).css('padding', '5px '+ navMargin + 'px 5px ' + eachSide + 'px' );
                        }
                        else {
                           var right = eachSide;
                           var left = eachSide;
                           $(this).css('padding', '5px '+ eachSide + 'px 5px ' + eachSide + 'px' );
                        } 
                     });
                     
                     //sharing the remaining pixels in each category if the remaining pixeles are negatives
                     if (0 > roundRemaining ) {
                     
                        while (0 > roundRemaining) {
                           $(".normalFW").each(function( index ) {
                              if (index == 0 && 0 > roundRemaining) {
                                 var rightPx = $(this).css('padding-right');
                                 var right = parseInt(rightPx.substring(0,rightPx.indexOf("px")));
                                 --right;
                                 $(this).css('padding', '5px '+ right + 'px 5px ' + navMargin + 'px' );
                                 roundRemaining = roundRemaining + 1;
                              }
                              else if (index == (navColumns-1) && 0 > roundRemaining) {
                                 var leftPx = $(this).css('padding-left');
                                 var left = parseInt(leftPx.substring(0,leftPx.indexOf("px")));
                                 --left;
                                 $(this).css('padding', '5px '+ navMargin + 'px 5px ' + left + 'px' );
                                 roundRemaining = roundRemaining + 1;
                              }
                              else if (0 > roundRemaining) {
                                 var leftPx = $(this).css('padding-left');
                                 var left = parseInt(leftPx.substring(0,leftPx.indexOf("px")));
                                 --left;
                                 roundRemaining = roundRemaining + 1;
                                 var rightPx = $(this).css('padding-right');
                                 var right = parseInt(rightPx.substring(0,rightPx.indexOf("px")));
                                 if (0 > roundRemaining) {
                                    --right;
                                    roundRemaining = roundRemaining + 1;
                                 }
                                 $(this).css('padding', '5px '+ right + 'px 5px ' + left + 'px' );
                              }
                           });
                        }
                     }
                   //sharing the remaining pixels in each category if the remaining pixeles are positives
                     else {
                        while (roundRemaining > 0) {
                           $(".normalFW").each(function( index ) {
                              if (index == 0 && roundRemaining > 0) {
                                 var rightPx = $(this).css('padding-right');
                                 var right = parseInt(rightPx.substring(0,rightPx.indexOf("px")));
                                 ++right;
                                 $(this).css('padding', '5px '+ right + 'px 5px ' + navMargin + 'px' );
                                 roundRemaining = roundRemaining - 1;
                              }
                              else if (index == (navColumns-1) && roundRemaining > 0) {
                                 var leftPx = $(this).css('padding-left');
                                 var left = parseInt(leftPx.substring(0,leftPx.indexOf("px")));
                                 ++left;
                                 $(this).css('padding', '5px '+ navMargin + 'px 5px ' + left + 'px' );
                                 roundRemaining = roundRemaining - 1;
                              }
                              else if (roundRemaining > 0) {
                                 var leftPx = $(this).css('padding-left');
                                 var left = parseInt(leftPx.substring(0,leftPx.indexOf("px")));
                                 ++left;
                                 roundRemaining = roundRemaining - 1;
                                 var rightPx = $(this).css('padding-right');
                                 var right = parseInt(rightPx.substring(0,rightPx.indexOf("px")));
                                 if (roundRemaining > 0) {
                                    ++right;
                                    roundRemaining = roundRemaining - 1;
                                 }
                                 $(this).css('padding', '5px '+ right + 'px 5px ' + left + 'px' );
                              }
                           });
                        }
                     }
                  
                  }
                  
                  reAlignmentTopNavigation();
                     
                  var zoom = document.documentElement.clientWidth / window.innerWidth;
                  $(window).resize(function() {
                     var zoomNew = document.documentElement.clientWidth / window.innerWidth;
                     if (zoom != zoomNew) {
                        // zoom has changed
                        reAlignmentTopNavigation();
                        zoom = zoomNew
                     }
                  });
                  
               </script>
            </div>
