<script language=javascript>
var the_topimages = new Array();
preLoadCatImages(the_topimages);

function checkFieldsSearch() 
{
  var searchtxt=Trim(document.searchForm.keyword.value);
  if (searchtxt == '') {
    alert("Please enter keyword or item#.");
    return false;
  } else if (searchtxt == 'Enter Keyword or Item No.') {
    alert("Please enter keyword or item#.");
    return false;
  } else if (searchtxt.length <= 2) {
    alert("We are sorry, the information you have requested is not available.");
    return false;
  } else {
    
      location.replace('http://search.russellstover.com/results.php?siteID=1&keywords=' + searchtxt);
      return false;
    
  }
}

function Trim(inString){
  return LTrim(RTrim(inString));
}

function LTrim(inString){
  return inString.replace(/^\s+/,'');
}

function RTrim(inString){
  return inString.replace(/\s+$/,'');
}

</script>
