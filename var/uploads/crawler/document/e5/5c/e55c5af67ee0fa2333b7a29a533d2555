    document.writeln('<scri' + 'pt type="text/javascript" src="http://ad.doubleclick.net/adj/rodale/home;article=/;kw=null;contextual=null;topic=home;sbtpc=home;slot=728x90;tile='+tile++ +';' + ';sz=728x90;page='+ page_no + ';gwd=' + as_param +';ord=' + ord +'?" ><\/scr' + 'ipt>')</script>
<noscript><a href="http://ad.doubleclick.net/adj/rodale/home;article=/;kw=null;contextual=null;topic=home;sbtpc=home;slot=728x90;tile=4;' + 'sz=728x90;ord=123456789?" target="_blank"><img src="http://ad.doubleclick.net/ad/rodale/home;article=/;kw=null;contextual=null;topic=home;sbtpc=home;slot=728x90;tile=4;' + 'sz=728x90;page='+ page_no + ';gwd=' + as_param +';ord=123456789?" border="" alt="Advertiser" /></a></noscript>
</div>

