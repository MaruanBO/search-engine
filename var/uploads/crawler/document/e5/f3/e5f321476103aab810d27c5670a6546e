                                                    function doEmailFocus(component) {
                                                        var term = $(component).addClass("selected");
                                                        var searchVal = term.val();
                                                        if (searchVal == "Email Address") {
                                                            term.val("");
                                                        }
                                                    }

                                                    function doEmailBlur(component) {
                                                        var term = $(component).removeClass("selected");
                                                        var searchVal = term.val();
                                                        if ($.trim(searchVal) == '' || $.trim(searchVal) == 'Email Address') {
                                                            term.val("Email Address");
                                                        }
