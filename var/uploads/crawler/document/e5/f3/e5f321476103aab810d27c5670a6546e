<script type="text/javascript">
		function openBasketPage() {
			window.location = 'http://www.kirklands.com/checkout/basket.jsp';
		}

		$(function(){
			$("#js-ucart").click(openBasketPage);

			$(".cart-search-keyword-container .search-button").click(function(){
				submitSearchForm(document.searchForm);
			});

			if ($(".keyword-text").height() < 16) {
				$(".keyword-text").height("16px");
			}
		});
	</script>
<script type="text/javascript">
		$(function(){
			var ucartTimer;

			$("#js-ucart").hover(
				function() {
				  ucartTimer = setTimeout(function() { showBasket('show',''); }, 500);
			},
				function(ev) {
					if( typeof ucartTimer != undefined ) { clearTimeout(ucartTimer); }
			});
		});

		// When the Universal Cart layer opens
		// it will close if the user clicks on anything in the main browser window
		$(document).click(function(event) {
			var posX = event.clientX;
			var posY = event.clientY;
			var miniCart = $('#ucart');
			var miniCartPos = miniCart.offset();
			if(isShowingBasket()) {
				// If user click to background, close the mini cart
				if(!(posX >= miniCartPos.left && posX <= (miniCartPos.left + miniCart.width())
					&& posY >= miniCartPos.top && posY <= (miniCartPos.top + miniCart.height()))) {
					hideBasket();
				}
			}
		});
		</script>
	<script src="/js/dyn/store_static.js?jsv=201502s5" type="text/javascript"></script><script type="text/javascript">

     
         preparePage();
     </script>



<script type="text/javascript">
	function loadHeaderCartItemCount() {
		$.ajax({
			type: "POST",
			url: '/includes/ajax/header_cart_item_count.jsp',
			data: { time: (new Date().getTime()) },
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
					var  totalCount;
					 if(data == '1'){
						 totalCount = "("+ data + ")";
						$(".js-headerItemCount").html(totalCount);						
					 }else{
						totalCount = "("+ data + ")";
						$(".js-headerItemCount").html(totalCount);						
					 }  
				}
			}
		});
	}
	
	function loadHeaderUserMessage() {
		$.ajax({
			type: "POST",
			url: '/includes/ajax/header_user_message.jsp',
			cache: 'false',
			data: { time: (new Date().getTime()) },
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
					$("#hdLinks ul.headerLink").prepend(data);
				}
			}
		});
	}
	
	
	$(function() {
		// load shopping cart item count
		loadHeaderCartItemCount();
		
		// load user message
		loadHeaderUserMessage();		
	});


</script>
