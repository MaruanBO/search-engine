   <script src="/js/dyn/store_locator_static.js?jsv=201502s5" type="text/javascript"></script><script type="text/javascript">
   var htmls = new Hash();
   var markers = new Hash();
   var toHtmls = new Hash();
   var fromHtmls = new Hash();   

   function getBalloonHtml(store) {
      var html = '<strong>' + store.STORE_NAME + '</strong>' +
                 '<br>' + store.ADDRESS_LINE_1;
       if (store.ADDRESS_LINE_2 != null && store.ADDRESS_LINE_2.length > 0) {
          html = html + '<br>' + store.ADDRESS_LINE_2;
       }
      html = html + '<br>' + store.CITY + ', ' + store.STATE + ' ' + store.ZIP_CODE;
       return html;
   }

   function getAddressForDirections(store) {
      var address = store.ADDRESS_LINE_1 + ' ';
       if (store.ADDRESS_LINE_2 != null && store.ADDRESS_LINE_2.length > 0) {
          address = address + store.ADDRESS_LINE_2 + ' ';
       }
      address = address + store.CITY + ', ' + store.STATE + ', ' + store.ZIP_CODE;
       return address;
   }

   function createStoreMarkers() {
       // create a marker for each store
       for (var i = 0; i < allStores.length; i++) {
           var store = allStores[i];
           if (store.LATITUDE != undefined && store.LONGITUDE != undefined  &&
              store.LATITUDE != 0.0 && store.LONGITUDE != 0.0 )
           {
              var html = getBalloonHtml(store);
              // The info window version with the "to here" form open
             toHtmls.setItem(store.STORE_ID, '<div class="direction" id="to-here" style="padding:2px;font-family:Arial,Helvetica,sans-serif; font-size:12px; width:200px;">' + html +
                     '<div><br>Get Direction: <b>To here</b> - <a href="javascript:fromHere(' + store.STORE_ID + ')">From here</a>' +
                           '<br>Start address:<form action="http://maps.google.com/maps" method="get" target="_blank" name="frmStart">' +
                           '<input type="text" SIZE=40 MAXLENGTH=40 name="saddr" id="saddr" value="" style="width: 190px" /><br>' +
                           '<div class="get-direction-button button" onclick="document.frmStart.submit();">' +
                           '<INPUT value="Get Direction" TYPE="SUBMIT" class="common-form-btn" />' +
                           '</div>' +
                           '<input type="hidden" name="daddr" value="'  + "(" + getAddressForDirections(store) + ")" +
                           '"/></form></div></div>');
             // The info window version with the "to here" form open
            fromHtmls.setItem(store.STORE_ID, '<div class="direction" style="padding:2px;font-family:Arial,Helvetica,sans-serif; font-size:12px; width:200px;">' + html +
                       '<div><br>Get Direction: <a href="javascript:toHere(' + store.STORE_ID + ')">To here</a> - <b>From here</b>' +
                             '<br>End address:<form action="http://maps.google.com/maps" method="get"" target="_blank" name="frmEnd">' +
                             '<input type="text" SIZE=40 MAXLENGTH=40 name="daddr" id="daddr" value="" style="width: 190px" /><br>' +
                             '<div class="get-direction-button button" onclick="document.frmEnd.submit();">' +
                             '<INPUT value="Get Direction" TYPE="SUBMIT" class="common-form-btn" />' +
                             '</div>' +
                             '<input type="hidden" name="saddr" value="' + "(" + getAddressForDirections(store) + ")" +
                             '"/></form></div></div>');

             //The inactive version of the direction info
            html = '<div style="padding:2px;font-family:Arial,Helvetica,sans-serif; font-size:12px; width:200px;">' +
                  html + '<br><br>Get Direction: <a href="javascript:toHere(' + store.STORE_ID +
                  ')">To here</a> - <a href="javascript:fromHere(' + store.STORE_ID + ')">From here</a></div>';

              // create marker
            var marker = MyMapAddMarker(store.LATITUDE, store.LONGITUDE, MYMARKER_TYPE1, html);
            markers.setItem(store.STORE_ID, marker);
             htmls.setItem(store.STORE_ID, html);
         }
       }
   }
</script>
