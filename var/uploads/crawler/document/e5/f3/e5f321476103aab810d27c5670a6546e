                function lC(nam, pg) {
                    var val = rC(nam);
                    if (val.indexOf('~' + pg + '~') != -1)
                        return false;
                    val += pg + '~';
                    wC(nam, val);
                    return true;
                }
                function firstTime(cN) {
                    // TSK00129: setting the cookie expire value -- START
                    //return lC('pWrD4jBo', cN);
                    return lC('FIRST_TIME_POPUP', cN);
                    // TSK00129: setting the cookie expire value -- END
                }
                function thisPage() {
                    var page = location.href.substring(location.href.lastIndexOf('\/') + 1);
                    pos = page.indexOf('.');
                    if (pos > -1) {
                        page = page.substr(0, pos);
                    }
                    return page;
                }

                //example code to call it - you may modify this as required
                function start() {
                    if (firstTime(thisPage())) {
                        // this code only runs for first visit
                        $("#emailSignUpPopUp").show();
                        $("#emailSignUpPopUp").appendTo('body').css({'position':'absolute', 'z-index':'999', 'width' : 'auto', 'height' : 'auto' });
                        lib.layer.center( "#emailSignUpPopUp" );
                        site.func.globalOverlay.create();
                    }
                    // other code to run every time once page is loaded goes here
                }*/

                /*$(document).ready(function() {
                    start();
                });*/

                $(".js-closeLayer").click(function(e){
                    e.preventDefault();
                    site.func.globalOverlay.remove();
                    $("#emailSignUpPopUp").hide();
                });

               

               /*
               binder.commonJSON.create(
                   "tcArea",
                   {}
               ) */

            </script>
