				<div class="no-cookies">This site requires browser cookies &amp; JavaScript to be turned on.</div>
				
				<script type="text/javascript">
					//Function to get the cookie data.
					function getCookie(cname) {
					    var name = cname + "=";
					    var ca = document.cookie.split(';');
					    for(var i=0; i<ca.length; i++) {
					        var c = ca[i];
					        while (c.charAt(0)==' ') c = c.substring(1);
					        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
					    }
					    return "";
					}
					//Default to hide the message so that it doesn't flicker on page load.
					$('.no-cookies').hide();
					
					//Wait for page load, then check if cookie is set, if true leave it hidden, if false show the message.
					$(document).ready(function(){
						if (getCookie('cookiecheck') != 'ok'){
						 $('.no-cookies').show();
						}
					});
				</script>
					
								
