<script type="text/javascript">
	$(document).ready(function() {
		
	        var vidDinnerFresh = $("#vid-dinner-fresh .vid-embed").clone(true);
		    $("#vid-dinner-fresh .vid-embed").remove();
		    $('.vid-dinner-fresh').click(
		    	function(e){
		    		if($(e.target).is('.vid-trigger-area')){
			            e.preventDefault();
			            showOverlayVideo(vidDinnerFresh, '#vid-dinner-fresh .vid-embed', e);
			            return;
			        }
			    }
			);
	            
	            
	        var vidAlaska = $("#vid-alaska-pollock .vid-embed").clone(true);
		    $("#vid-alaska-pollock .vid-embed").remove();
		    $('.vid-alaska-pollock').click(
		    	function(e){
		    		if($(e.target).is('.vid-trigger-area')){
						e.preventDefault();
						showOverlayVideo(vidAlaska, '#vid-alaska-pollock .vid-embed', e);
						return;
					}
		    	}
			);
	            
		
			$('.close-vid-overlay').bind('click', function() {
				closeOverlayVideo();
			});

			$('body').click(
				function() {
					closeOverlayVideo();
				}
			);
	});

	function showOverlayVideo(html, obj, e) {
			//prevent click from bubbling up to the body element
			e.stopPropagation();
			e.preventDefault();


			//clear video overlay contents
			if(!$('.vid-overlay').is(':hidden')) {
				closeOverlayVideo();
			}

			//set overlay contents
			$('.close-vid-overlay').after(html);
			$('.vid-overlay').show();
			$('.vid-embed').show();
			$(obj).show();

	}

	function closeOverlayVideo () {
		$('.vid-overlay .vid-embed').remove();
		$('.vid-overlay').hide();
	}
	
</script>

