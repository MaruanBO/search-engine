			<script type="text/javascript">
	function BCSignUp() {
		var valid = validateEmail();
		if(valid){
			document.signup.submit();
		}else{
			return false;
		}
	}
	function validateEmail(){
		var reg = '^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,4}$';
		if(document.signup.email.value.search(reg) == -1){
			return false;
		}
		return true;
	}
</script>
