	<script>
		$(document).ready(function() {
			$(".btnGoSignup").click(function(e) {
				var eml = $("#eml").val();

				e.preventDefault();

				if (!belk.util.isValidEmail(eml)) {
					belk.util.alert("Please enter a valid email address.", "Invalid Email Address");
					return false;
				} else {
					window.location.href = "/AST/Featured/Promo_Details/Email/FooterSignup.jsp?eml=" + eml;
				}

			});

			// Interactive Catalog
			if ('referrer' in document) {
				var catalogReferrer = 0;

				if (document.referrer.match(/belkonlinead/) != null) {
					catalogReferrer+= document.referrer.match(/belkonlinead/);
				}

				if (document.referrer.match(/dirxion/) != null) {
					catalogReferrer+= document.referrer.match(/dirxion/);
				}

				if (catalogReferrer > 0) {
					cmCreateConversionEventTag("Look Book","2","Interactive Catalog","1");
					if (console) {console.log("*** Welcome, Interactive Catalog customer! ***");}
				}
			}

		});
	</script>

