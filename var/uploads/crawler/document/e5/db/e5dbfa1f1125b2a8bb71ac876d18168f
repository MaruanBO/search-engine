<script>
 $(document).ready(function () {

$('body').children().first().before($('#modalFreeShipping').detach());
$('body').children().first().before('<div id="mask"></div>');

    $('.footermodalTrigger').click(function(e) {

     e.preventDefault();

     $('#mask').fadeTo("slow", 0.8);

     var id = $($(this).attr('href'));
     id.center();
     id.fadeIn(2000);

    });

    $('.footerwindow .footerclose, #mask').live("click", function (e) {
     e.preventDefault();
    $('#mask, .footerwindow').fadeOut("fast");
    });

    jQuery.fn.center = function () {
    this.css("top", Math.floor($(window).height()/2 - $(this).height()/2) - $(this).parent().offset().top + "px");
    this.css("left", Math.floor($(this).parent().width()/2 - $(this).width()/2) + "px");
    return this;
   }
 });

</script>


