<script type="text/javascript">

function parseCertonaResp(input){
    if(input.Resonance.Response[0].display == 'yes') {
		var output = input.Resonance.Response[0].output;
		var scheme = input.Resonance.Response[0].scheme;
		if(scheme == 'addtobag_rr') {
			var jsonOutput = eval('([' + output + '])');
			var commaDelimitedProductIDS = '';
			for(var i=0;i<jsonOutput.length;i++) {
				commaDelimitedProductIDS = commaDelimitedProductIDS+(jsonOutput[i].productCode);
				if(i<jsonOutput.length-1) {
					commaDelimitedProductIDS = commaDelimitedProductIDS+',';					
				}
			}
			$.ajax({
				cache: false,
				type: 		"get",
				url: 		"/ajax/getCertonaProductRecommendations.jsp?scheme="+scheme,
				data: 		"commaDelimitedProductIDS=" + commaDelimitedProductIDS,
				success:	function(msg) {
					msg = jQuery.trim(msg);
					if(msg.indexOf("<li>") > 0)
					    $('#certonaPlaceHolderProducts').removeClass("hidden");
					$('#certonaPlaceHolderProducts').html(msg);
				}
			});
		} 
		else if(scheme == 'wishlist_rr') {
			var jsonOutput = eval('([' + output + '])');
			var commaDelimitedProductIDS = '';
			for(var i=0;i<jsonOutput.length;i++) {
				commaDelimitedProductIDS = commaDelimitedProductIDS+(jsonOutput[i].productCode);
				if(i<jsonOutput.length-1) {
					commaDelimitedProductIDS = commaDelimitedProductIDS+',';					
				}
			}
			$.ajax({
				cache: false,
				type: 		"get",
				url: 		"/ajax/getCertonaProductRecommendations.jsp?scheme="+scheme,
				data: 		"commaDelimitedProductIDS=" + commaDelimitedProductIDS,
				success:	function(msg) {
					msg = jQuery.trim(msg);
					if(msg.indexOf("<li>") > 0)
					    $('#certonaPlaceHolderProducts').removeClass("hidden");
					$('#certonaPlaceHolderProducts').html(msg);
				}
			});
		} 
		else if(scheme == 'wishlistland_rr') {
			var commaDelimitedProductIDS = '';
			var splitString = output.split("</p>");
			var imgSplitString;
			var imgSubString = '';
			var altSubString = '';
			for (var i=0;i<splitString.length-1;i++)
			{				
				imgSplitString = splitString[i].split("<img");
				imgSubString = imgSplitString[1].substring(0,imgSplitString[1].indexOf(">"));
				altSubString = imgSubString.substring(imgSubString.indexOf("alt=")+5);
				commaDelimitedProductIDS = commaDelimitedProductIDS +   altSubString.substring(0,altSubString.length-1);
				if (i < splitString.length-2)
				{
					commaDelimitedProductIDS = commaDelimitedProductIDS + ',';
				}
			}
			$.ajax({
				cache: false,
				type: 		"get",
				url: 		"/ajax/getCertonaProductRecommendations.jsp?scheme="+scheme,
				data: 		"commaDelimitedProductIDS=" + commaDelimitedProductIDS,
				success:	function(msg) {
					msg = jQuery.trim(msg);
					if(msg.indexOf("<li>") > 0)
					    $('#wishlistland_rr').removeClass("hidden");
					$('#wishlistland_rr').html(msg);
				}
			});
		} 
	}
}

var resx = new Object();
var resxTempHolder = ''; // variable to hold value from scriplet.

TMAN.addParam('tm_appid','BELK01');  //for resx.appid
TMAN.addParam('tm_top1',100000);  //for resx.top1
TMAN.addParam('tm_top2',100000);  //for resx.top2

resxTempHolder='';
TMAN.addParam('tm_resx_links',resxTempHolder);  //for resx.links

var belkInfoCookie =  jQuery.parseJSON(belk.util.readCookie("info"));
if(belkInfoCookie){
	resxTempHolder=belkInfoCookie.email;
}

TMAN.addParam('tm_customerid',resxTempHolder);  //for resx.customerid




//certonaResx.run();
</script>
