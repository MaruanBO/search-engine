 <script type="text/javascript">
	var mydata =  jQuery.parseJSON(belk.util.readCookie("info")) || "",
			strCart = belk.util.readCookie("BelkCart"),
			intCartCount = 0,
			userStatusClass = "loggedOut",
			strCartToken;
	if(strCart){
		strCartToken = strCart.split( "-" );
		if(strCartToken){
			if(strCartToken.length>1){
				intCartCount = strCartToken[0];
			}else{
				if(mydata.noOfItems){
					intCartCount = mydata.noOfItems;
				}
			}
		}
	}
	if(mydata.userStatus){
		userStatusClass = mydata.userStatus;
	}
	$("#userStatus").attr('class', userStatusClass);
	$("#num_bag_items").html(intCartCount);
	if(parseInt(intCartCount, 10)===1){
		$("#shoppingBagPlaceHolder").html($("#shoppingBagPlaceHolder").html().replace("items ","item "));
	}
 </script>	
