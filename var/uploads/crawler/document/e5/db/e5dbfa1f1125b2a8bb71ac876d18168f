<script type="text/javascript">
function clickSwatchLink() {
	var idx = document.getElementById("sel_color").selectedIndex;
	var selected_text = document.getElementById("sel_color").options[idx].text;
	var links = document.getElementsByTagName("a");
	for(var i = 0; i < links.length; i++) {
		if (links[i].title == selected_text) {
			var swatchLink = links[i];
			if(document.dispatchEvent) { // W3C
					var clickEvent = document.createEvent("MouseEvents");
					clickEvent.initMouseEvent("click", true, true, window, 1, 1, 1, 1, 1, false, false, false, false, 0, swatchLink);
					swatchLink.dispatchEvent(clickEvent);
			} else if(document.fireEvent) { // IE
					swatchLink.fireEvent("onclick");
			}
		}
	}
}
	</script>

