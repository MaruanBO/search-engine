  <script>
   var bOverCartMenu = false;
   var bOverCartButton = false;
  
   function showCartMenu() {
    $("#shoppingBagMenu").css("display", "block");
    $("#shoppingBagMenu").stop().animate({opacity: 1}, "fast");
    $("#shoppingBag").addClass("active");
    bOverCartMenu = false;
   }
   
   function hideCartMenu() {
    $("#shoppingBagMenu").stop().animate({opacity: 0}, "slow", function() {bOverCartButton = false; $(this).css("display", "none");});
    $("#shoppingBag").removeClass("active");
   }
     
   
   function menuHighLight(){
	  
	 var value = $('#whatpage').val();
	 //the value is empty when using browser back button.
	 if(value == ""){
		var tmpValue = window.location.href; //get the current page url. appending empty string to convert into string type.
		var splitUrl = [];
		splitUrl = tmpValue.split("/");
		if (splitUrl.length > 6) {
			if (tmpValue.indexOf("/AST/Main/Belk_Primary/") > -1) {
				value = splitUrl[6];
	    		}
		}
	 }
	
	 var start = value.split(" ");
	
	 var lis = [];
	 lis.push( $('#Women') );
	lis.push( $('#Men') );
	lis.push( $('#Juniors') );
	lis.push( $('#Kids') );
	lis.push( $('#Beauty_And_Fragrance') );
	lis.push( $('#Shoes') );
	lis.push( $('#Handbags_And_Accessories') );
	lis.push( $('#Jewelry_And_Watches') );
	lis.push( $('#BedBath') );
	lis.push( $('#For_the_Home') );
	lis.push( $('#Sale_and_Clearance') );
	 
