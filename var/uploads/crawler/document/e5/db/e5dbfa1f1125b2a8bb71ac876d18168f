<!--[if lte IE 7]>
<script type="text/javascript">
$(function() {
	if ($.support.inlineBlockNeedsLayout){
		_setupSnifferModal();
	}

	function centerModal(sel) {
		// Center the modal
		var w = $(window),
			viewportHeight = w.height(),
			viewportWidth = w.width(),
			$modal = $(sel),
			modalHeight = $modal.outerHeight(true),
			modalWidth = $modal.outerWidth(true);

		$modal.css({
			top: ((viewportHeight / 2) - (modalHeight / 2)),
			left: ((viewportWidth / 2) - (modalWidth / 2))
		});
	}

	function _setupSnifferModal() {
		$("#snifferModal .close").click(function() {
			$.modal.close();
		});
		$('#snifferModal').modal({
			close: true,
			closeClass: 'close',
			persist: true,
			containerId: 'snifferModalContainer',
			onShow: function(){
				centerModal('#snifferModal');
			},
			onClose:function() {
				$.modal.close();
			}
		});
	}
});
</script>
<![endif]-->

	

