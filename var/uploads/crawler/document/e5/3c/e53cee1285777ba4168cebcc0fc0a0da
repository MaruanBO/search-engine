<script type="text/javascript">
    $(window).bind("load", function() {
       $.getScript('http://www.nrcc.org/wp-content/themes/NRCC2013/js/social.js', function() {});
    });
	$(document).ready(function() {
		$('input.gform_hidden[value^="http"]').val(window.location.href);

		var sidebar = $('section.sidebar.sticky');

		var sidebar_offset = $('section.sidebar.sticky').offset();
		var single_share = $('section.single-post > div.share');

					$('header.main').backstretch('http://www.nrcc.org/wp-content/uploads/2014/04/background-home.jpg');
			$('section.sidebar > div.gform_widget').backstretch('http://www.nrcc.org/wp-content/uploads/2014/04/background-home.jpg');
						
        
        $(window).scroll(function (){
        	// Single Pages only
        	
            if ($(window).width() > 767) {
            	if (sidebar.length > 0) {
            		if (sidebar.height() < $('section.span8.blog, section.single-post.span8').height()) {
		            	nrcc_sticky_sidebar(true,sidebar,sidebar_offset);
		            } else {
		            	nrcc_sticky_sidebar(false,sidebar,sidebar_offset);
		            }
	            }
            } else {
            	if (sidebar.length > 0) {
	            	nrcc_sticky_sidebar(false,sidebar,sidebar_offset);
	            }
            }
        });

        $(window).smartresize(function(){
            if ($(window).width() > 767) {
            	$('header.main ul.menu').show();
            	if (sidebar.length > 0) {
	            	nrcc_sticky_sidebar(false,sidebar,sidebar_offset);
					sidebar_offset = $('section.sidebar.sticky').offset();
            		if (sidebar.height() > $('section.span8.blog').height()) {
		            	nrcc_sticky_sidebar(true,sidebar,sidebar_offset);
		            } else {
		            	nrcc_sticky_sidebar(false,sidebar,sidebar_offset);
		            }
	            }
            } else {
            	if (sidebar.length > 0) {
	            	nrcc_sticky_sidebar(false,sidebar,sidebar_offset);
	            }
            	$('header.main ul.menu').hide({
            		complete: function() {
            			$('header.main').data('backstretch').resize();
            		}
            	});
            	$('div.showmenu').removeClass('active');
            }
        });

        $('div.showmenu').click(function(){
        	$('header.main ul.menu').slideToggle({
        		duration: 400,
        		complete: function(){
        			$('header.main').data('backstretch').resize();
        		}
        	});
        	$(this).toggleClass('active');
        });

        $('.single-wrap .single-post > .share > a').on('click',function(event){
        	event.preventDefault();
        	window.open($(this).attr('href'),'_blank','menubar=0,resizable=1,width=500,height=350');
        });

	});

    $('body').trigger('nrcc_scripts_finished');

</script>

<script type='text/javascript'>
    $(document).ready(function() {
        var nrcc_ga_cookie = $.cookie('nrcc_google_analytics');

        if (typeof(nrcc_ga_cookie) !== "undefined" && nrcc_ga_cookie) {
            nrcc_ga_cookie = $.parseJSON(nrcc_ga_cookie);
            console.log("Sending to Google Analytics:",nrcc_ga_cookie);
            var contribution_amount = nrcc_ga_cookie.contribution_total;
            var contribution_city = nrcc_ga_cookie.city;
            var contribution_state = nrcc_ga_cookie.state;
            var contribution_id = nrcc_ga_cookie.transactionid;
            var contribution_cat = nrcc_ga_cookie.contributioncat;

            if (contribution_amount !== null) {
                if (typeof(contribution_amount) === "string") {
                    contribution_amount = contribution_amount.replace(/[^0-9.]/g, "");
                }
                contribution_amount = parseInt(contribution_amount,10);
            } else {
                contribution_amount = 0;
            }
            if (isNaN(contribution_amount)) {
              contribution_amount = 0;
            }

            _gaq.push(['_addTrans',
                contribution_id,           // transaction ID - required
                'NRCC Contribution',  // affiliation or store name
                contribution_amount,          // total - required
                '',           // tax
                '',              // shipping
                contribution_city,       // city
                contribution_state,     // state or province
                'USA'             // country
            ]);

            _gaq.push(['_addItem',
                contribution_id,           // transaction ID - required
                '00001',           // SKU/code - required
                'Contribution',        // product name
                contribution_cat,   // category or variation
                contribution_amount,          // unit price - required
                '1'               // quantity - required
            ]);

                        _gaq.push(['_trackTrans']); //submits transaction to the Analytics servers
            window.optimizely = window.optimizely || [];
            window.optimizely.push(['trackEvent', 'contribution', {'revenue': contribution_amount * 100}]);
            
            (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
        }
        $.removeCookie('nrcc_google_analytics', { path: '/' });
    });
</script>

