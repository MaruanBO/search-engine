    var wiland_vars = ['_wds_im','_wds_ae','_wds_oi','_wds_oa','_wds_af','_wds_al','_wds_aa','_wds_ac','_wds_as','_wds_az'];
    wiland_data_json = $.cookie('wiland_integration');
    if (typeof(wiland_data_json) !== "undefined") {
        wiland_data = $.parseJSON(wiland_data_json);
        for (var i = 0; i < wiland_vars.length; i++) {
            if ( typeof(wiland_data[wiland_vars[i]]) !== "undefined" && wiland_data[wiland_vars[i]] !=+ "" ) {
                window[wiland_vars[i]] = wiland_data[wiland_vars[i]];
            }
        }
    }
    $.removeCookie('wiland_integration', { path: '/' });
</script>
