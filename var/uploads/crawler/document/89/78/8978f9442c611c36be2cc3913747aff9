$(document).ready(function($) {
    var ezIgnoreTags = ['SCRIPT','IFRAME','STYLE','LINK','BODY','HEAD'];
    $(document).bind('DOMNodeInserted', function(e) {  
        var element = e.target;
        
        //move our before and after spans to be first or last sibling elements
        var after_span = $(element).prev('.ezoic-after-el');
        if(after_span != null)
        {
             after_span.insertAfter($(element));
        }
        var before_span = $(element).next('.ezoic-before-el');
        if(before_span != null)
        {
             before_span.insertBefore($(element));
        }
        

        if(ezIgnoreTags.indexOf(element.tagName) == -1)
        {
            if($(element).hasClass('ezoic-wrapper') == false && $(element).hasClass('ezoic-no-wrapper') == false)
            {
                if($(element).closest('.ezoic-no-wrapper').length < 1 && $(element).closest('.ezoic-ad').length < 1 && $(element).closest('.ezo_ad').length < 1 && $(element).closest('.ezoic-wrapper').length < 1  && $(element).css('position') != 'fixed' && $(element).css('position') != 'absolute')
                {
                    $(element).addClass('ezoic-wrapper');
                    ezo_elements_to_check.push(element);
                    ezo_resize_interval_count=0; //reset interval timer

                    if($(element).hasClass('ez_wrap_ins') == false && $(element).parent().is('body'))
                    {
                        $(element).wrap("<ins class='ezoic-wrapper ez_wrap_ins'></ins>");
                        $(element).css('max-width','none');
