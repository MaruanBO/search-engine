// overrides for page-over-page prop, evar, event & listvar tracking
(function() {
  if (window.localStorage['Cars.omniture.props']) {
    var o = JSON.parse(window.localStorage['Cars.omniture.props']);  
    for (var k in o) {
      s_clven[k] = o[k];
    }
    // prevent them from firing on more than one page
    window.localStorage.removeItem('Cars.omniture.props');
  }
  if (window.localStorage['Cars.omniture.evars']) {
    var o = JSON.parse(window.localStorage['Cars.omniture.evars']);  
    for (var k in o) {
      s_clven[k] = o[k];
    }
    // prevent them from firing on more than one page
    window.localStorage.removeItem('Cars.omniture.evars');
  }
  if (window.localStorage['Cars.omniture.events']) {
    var o = window.localStorage['Cars.omniture.events'];
    // events is a comma-delimited string, so be careful about additions
    if (s_clven.events) {
      s_clven.events = s_clven.events + "," + o;  
    } else {
      s_clven.events = o;
    }
    // prevent them from firing on more than one page
    window.localStorage.removeItem('Cars.omniture.events');
  }
  if (window.localStorage['Cars.omniture.listVars']) {
    var o = JSON.parse(window.localStorage['Cars.omniture.listVars']);
    for (var k in o) {
      s_clven['list' + k] = o[k];
    }
    // prevent them from firing on more than one page
    window.localStorage.removeItem('Cars.omniture.listVars');
  }
})();

