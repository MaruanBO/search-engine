<script type="text/javascript">
var theGNtracking, theGNBC, getPseudoBCValues = function (theObject) {
  var arr = [], key;
  for (key in theObject) {
    if ( (key.indexOf("pseudo") > -1) && theObject.hasOwnProperty(key) ) {
      if ( key.indexOf("Price") > -1 ) {
        arr.push(28437);
      } else if ( key.indexOf("Mileage") > -1 ) {
        arr.push(28438);
      } else if ( key.indexOf("Year") > -1 ) {
        arr.push(28423);
      }
    }
  }
  return arr;
};

eVarArrayString = "";
propArrayString = "";
s_clven.pageName="index.jsp";

s_clven.pageName = "go/core/jsp/"+ s_clven.pageName;

// maintain index page tracking

  
    s_clven.pageName = 'go/homepage';
    
    
    
    
  



s_clven.server="";

s_clven.channel="";
if(s_clven.pageName.indexOf("/Search/") > -1) {
  if (s_clven.pageName.indexOf("Blended") > -1) { s_clven.channel="Blended Cars/Search"; }
  else if(s_clven.pageName.indexOf("New") > -1 && s_clven.pageName.indexOf("Used") < 0 ){s_clven.channel="New Cars/Search";} else {s_clven.channel="Used Cars/Search";}
}

if(s_clven.pageName.indexOf("dealers/") === 0) {
  s_clven.channel="dealers";
}
/* service and repair site sections */
if(s_clven.pageName.indexOf("service/repair") === 0){
  s_clven.channel="service";
}
if(s_clven.pageName.indexOf("repair_list") === 0 || s_clven.pageName.indexOf("repair_estimate") === 0){
  s_clven.channel="service";
}
else if(s_clven.pageName.indexOf('go/repair') === 0){
  s_clven.channel="go";
}

if (window.location.pathname == '/') { s_clven.channel = 'go'; } // force homepage channel
if(s_clven.channel.indexOf('/') == 0) { /* remove starting slash */
  s_clven.channel = s_clven.channel.slice(1);
}
/* Brand Pages site section */
if(s_clven.pageName === "research/brand/make"){
 s_clven.channel="Research";
}



var QueryString = function () {
  // This function is anonymous, is executed immediately and
  // the return value is assigned to QueryString!
  var query_string = {};
  var query = window.location.search.substring(1);
  /*if(localStorage.getItem('url') !== null){
	   query = localStorage.url;
     localStorage.removeItem('url');
  }
  else{
	   query = window.location.search.substring(1);
  }*/
  var vars = query.split("&");
  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
    	// If first entry with this name
    if (typeof query_string[pair[0]] === "undefined") {
      query_string[pair[0]] = pair[1];
    	// If second entry with this name
    } else if (typeof query_string[pair[0]] === "string") {
      var arr = [ query_string[pair[0]], pair[1] ];
      query_string[pair[0]] = arr;
    	// If third or later entry with this name
    } else {
      query_string[pair[0]].push(pair[1]);
    }
  }
    return query_string;
} ();

s_clven.pageType="";
s_clven.prop1="";
s_clven.prop2="";
s_clven.prop3="";
s_clven.prop4="";
s_clven.prop5="";
s_clven.prop6="national";
s_clven.prop7="";
s_clven.prop8="";
s_clven.prop9="";
if (QueryString.prMx) { s_clven.prop9 = QueryString.prMx;}
s_clven.prop10="";
s_clven.prop11="";
