<script>
show_yhd = function() {
	var youHaveDownloadsDialog = $('#dialog-yhd');
	if (youHaveDownloadsDialog.length > 0) {
		youHaveDownloadsDialog.dialog({
			width: 500,
            height: 200,
            modal: true,
            autoOpen: true,
            draggable: false,
            resizable: false,
            show: 'fade',
            hide: 'fade',
            title: ''
        });
		$('#btn-yhd-no-thanks').on('click', function () { youHaveDownloadsDialog.dialog('close'); });
		$.ajax({ type: 'GET', url: '/api/1.2/widget/seenPendingDownload' });
	}	
}

$(document).ready(function() {
	$.getJSON('/api/1.2/widget/getNavigationJson', function( data ) {
		$('#nav li').each(function(){
			if(data[$(this).attr('id')] !== undefined)
				$(data[$(this).attr('id')]).appendTo($(this));
			$(this).mouseout(function(){drop_down_hide_delay_nav();}).mouseover(function(){drop_down_show_nav($(this).attr('id'));});
		});
		if(selectedNav)do_drop_down_show_nav(selectedNav);
	});
});

$(window).load(function(){
	if(navigator.userAgent.match(/Android/i)) return false;
	
	show_yhd();
	
	var promotionDialog20ps = $('#dialog-20ps');
	if ($.cookie('2ps') != 1 && promotionDialog20ps.length > 0) {
		promotionDialog20ps.dialog({
			width: 500,
            height: 460,
            modal: true,
            autoOpen: true,
            draggable: false,
            resizable: false,
            show: 'fade',
            hide: 'fade',
            title: ''
        });
		$.cookie('2ps', 1, {domain: domain, expires: 90, path: '/'});
		$('#btn-20ps-continue-shopping').on('click', function () { $('#dialog-20ps').dialog('close'); });
	}
});
</script>


