<script language="JavaScript" type="text/javascript">
    
    String.prototype.trim = function() {
       // skip leading and trailing whitespace and return everything in between
       var x = this;
       x = x.replace(/^\s*(.*)/, "$1");
       x = x.replace(/(.*?)\s*$/, "$1");
       // strip single and double quotes, percent signs
       x = x.replace(/'/g, "");
       x = x.replace(/"/g, "");
       x = x.replace(/%/g, "");
       return x;
    }

    //---------------------------------------------------------------------------
    // Function used by search form to submit form.
    //---------------------------------------------------------------------------
    function submit_search_form(formName) {
       var terms = document[formName].Ntt.value.trim();
       var searchURL = "/search?q=";
       terms = stripStrangeCharacters(terms);
       terms = terms.replace('\'', ' ');
       terms = terms.replace('%', ' ');
       terms = encodeURIComponent(terms);
       //terms = terms.replace(/%20/g,'+');
       if (terms.length == 0) {
         alert("Please enter a search term");
       }
       else {
    	  searchURL += terms;
          document.location.href = searchURL;
       }
       return false;
    }

      //---------------------------------------------------------------------------
      // Function used to strip out funky characters in search term.
      //---------------------------------------------------------------------------
     function stripStrangeCharacters (searchTerm) {
         var processedTerm = searchTerm;
         for (i=0; i<processedTerm.length; i++) {
             if (processedTerm.charAt(i) > '\177') {
                 processedTerm = processedTerm.replace(processedTerm.charAt(i), '');
                 i = i-1;
             }
         }
         return processedTerm;
     }

    //-->
</script>
	
	  
  
