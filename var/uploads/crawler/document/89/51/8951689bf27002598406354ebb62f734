<script>
//B&C Cross Promotion Widget
(function() {
  var carouselDiv = $('#bc-carousel-source');  
  var carouselLiCollection = $(carouselDiv).find('li');
  
  var carouselHtmlToAdd = [];  
  for(var i =0; i < carouselLiCollection.length; i++) {
    carouselHtmlToAdd.push('<div class="bc-product-container">');
    addLiToCarousel($(carouselLiCollection[i]), 'left', Math.floor(i/2));
	i++;
	if(carouselLiCollection.length > i) {
		addLiToCarousel($(carouselLiCollection[i]), 'right', Math.floor(i/2));
	}
	carouselHtmlToAdd.push('</div>');
  } 
  
  $('#bc-carousel-source').html(carouselHtmlToAdd.join(''));
  $('#bc-carousel-source').show();
  $('#bc-carousel-source').cycle({ 
    fx:    'scrollLeft',
    pause:1,
	pager: '#bc-carousel-pager',
    speed:3000,
	timeout:  3000
});
  
  
  function addLiToCarousel(liCollectionObject, className, containerNumber) {
	var collectionName = $(liCollectionObject).find('.collectionName').text();
    var collectionURL = $(liCollectionObject).find('.collectionURL').text();
    var endDate = $(liCollectionObject).find('.endDate').text();
    var colletionImageURL = $(liCollectionObject).find('.collectionImageURL').text();
    var colletionDescription = $(liCollectionObject).find('.collectionDescription').text();
    
	var classNameToSet = '';
	if(className === 'left') {
		classNameToSet = 'bc-product-left';
	} else {
		classNameToSet = 'bc-product-right';
	}
	
	var endDate1 = endDate.split(/\-|\s/);
	var endDate2 = new Date(endDate1.slice(0,3).reverse().join('/')+' '+endDate1[3]);
	
	
	
	var bcEventCountdownTimer = countdown(function(ts) {
											var countDownOutput = '';
											if(ts.value >0 ) {
												if(ts.days > 0) {
													countDownOutput = ts.days + ' days : ' + ts.hours + ' hours';
												} else if(ts.hours > 0) {
													countDownOutput = ts.hours + ' hours : ' + ts.minutes + ' minutes';
												} else if(ts.minutes > 0) {
													countDownOutput = ts.minutes + ' minutes : ' + ts.seconds + ' seconds';
												} else {
													countDownOutput = '0 minutes : ' + ts.seconds + ' seconds';
												}											
											} else {
												countDownOutput = '0 minutes : 0 seconds';
											}
											$($('.bc-product-container')[containerNumber]).find('.' + classNameToSet).find('.bc-prod-collection-timer').html(countDownOutput);											
										}
											, endDate2, countdown.DAYS|countdown.HOURS|countdown.MINUTES|countdown.SECONDS);	
	
	carouselHtmlToAdd.push('<div href="" class="' + classNameToSet + '">');
	carouselHtmlToAdd.push('<img class="bc-event-image" src="' + colletionImageURL + '"/>');
    carouselHtmlToAdd.push('<div class="bc-prod-info-container"><div class="bc-prod-collection-container"><div class="bc-prod-collection-label">collection:</div><div class="bc-prod-collection-name">');
	carouselHtmlToAdd.push(collectionName);
	carouselHtmlToAdd.push('</div><div class="bc-prod-collection-description">' + colletionDescription + '</div></div><div class="bc-prod-collection-timer">');
	carouselHtmlToAdd.push('');
	carouselHtmlToAdd.push('</div><div class="bc-prod-collection-cta"><a href="'+collectionURL+'">shop event</a></div></div></div>');
  }
	
  $('.bc-product-left, .bc-product-right').live("click", function(e) {
	window.open($(this).find('.bc-prod-collection-cta a').attr('href'));
  });
})();

</script>



<!-- end global footer non-secure m2630131 -->
        
        
      
    
    


    
    

    
    







    
    
    
        
        
    
        
        
            

