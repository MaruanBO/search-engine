			$('#menu a').smoothScroll();
			$('#footernavi a').smoothScroll();
			$('#arrow a').smoothScroll();
			$('.goldbuttons').smoothScroll();
			
			$("input").change(function() {
				if(($(this).val()).length > 2) {
					$(this).addClass("small-textfield-done");
				} else {
					$(this).removeClass("small-textfield-done");
				}
			});
		
			function userSignIn() {
				$("#login_wrapper").fadeIn(300);
			}
			
			$("#loginoverlay").click(function() {
				$("#login_wrapper").fadeOut(300);
