		<script>
		
         var scroll = false;
         var menu = $('#menu').html();
		 
         $(document).scroll(function() {
         
         if( $(this).scrollTop() > 900 ) {
         	if( !scroll ) {
         		scroll = true;
				
         		$('#menu-scroll').fadeIn(500);
				$('#menu_1').html('<div id="menu">'+menu+'</div>');
				
         		
         	}
         } else {
         	if( scroll ) {
         		scroll = false;
         		$('#menu-scroll').hide();
         		
         	}
         }
         });
         
      </script>
