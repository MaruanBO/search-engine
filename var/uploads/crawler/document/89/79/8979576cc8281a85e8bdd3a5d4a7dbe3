	<div id="main-video-content" data-vr-contentbox="">
		<div id="VideoPlayerMain" class="main-video-clickable" data-id="42044" data-title="Road to DARPA's Robotics Finals: Episode 4"></div>
		<div id="main-video-playlist"></div>
	</div>
	
	
<script type="text/javascript" src="http://admin.brightcove.com/js/BrightcoveExperiences.js"></script>
<script type="text/javascript" src="/www.idge/js/video/bright-vid.js"></script>
<script>
idgeBV.fpVideo = {
	HTML : "<div id=\"BCLcontainingBlock\"><div class=\"BCLvideoWrapper\"><div id=\"mediaContainer42044target\"><div class=\"playerplaybutton ss-play\"></div><div id=\"main-video-player-title\">Road to DARPA's Robotics Finals: Episode 4</div><img class=\"posterImage\" src=\"http://images.techhive.com/images/article/2014/10/robotepisode4poster-100510009-poster.jpg\" /></div><div></div>",
	playlist : {
		HTML : "<div data-vr-contentbox=\"\" class=\"main-video-playlist-item clearfix main-video-clickable\" data-id=\"42044\" data-title=\"Road to DARPA's Robotics Finals: Episode 4\" data-plur=\"/video/raw.do?id=42044\"><div id='playbutton42044' class=\"playbutton\"><img class=\"posterImage\" src=\"http://images.techhive.com/images/article/2014/10/robotepisode4poster-100510009-poster.jpg\" /><span>Road to DARPA's Robotics Finals: Episode 4</span></div></div>"
			 + "<div data-vr-contentbox=\"\" class=\"main-video-playlist-item clearfix main-video-clickable\" data-id=\"42068\" data-title=\"Road to DARPA's Robotics Finals: Episode 5\" data-plur=\"/video/raw.do?id=42068\"><div id='playbutton42068' class=\"playbutton\"><img class=\"posterImage\" src=\"http://images.techhive.com/images/article/2014/10/robotepisode5poster-100510008-poster.jpg\" /><span>Road to DARPA's Robotics Finals: Episode 5</span></div></div><div data-vr-contentbox=\"\" class=\"main-video-playlist-item clearfix main-video-clickable\" data-id=\"47965\" data-title=\"Road to DARPA's Robotics Finals, Episode 6\" data-plur=\"/video/raw.do?id=47965\"><div id='playbutton47965' class=\"playbutton\"><img class=\"posterImage\" src=\"http://images.techhive.com/images/article/2015/02/ep6darparoadfinals-100568207-poster.jpg\" /><span>Road to DARPA's Robotics Finals, Episode 6</span></div></div>",
		
		ids : new Array(42044, 42068, 47965)
	},
	init : function() {
		idgeBV.fpVideo.droidVids = new Object();
		idgeBV.fpVideo.droidVids[42044] = {
			poster : "http://images.techhive.com/images/article/2014/10/robotepisode4poster-100510009-poster.jpg",
			plur : "/video/raw.do?id=42044"
		};
		idgeBV.fpVideo.droidVids[42068] = {
			poster : "http://images.techhive.com/images/article/2014/10/robotepisode5poster-100510008-poster.jpg",
			plur : "/video/raw.do?id=42068"
		};
		idgeBV.fpVideo.droidVids[47965] = {
			poster : "http://images.techhive.com/images/article/2015/02/ep6darparoadfinals-100568207-poster.jpg",
			plur : "/video/raw.do?id=47965"
		};
		
		$("#VideoPlayerMain").html(idgeBV.fpVideo.HTML);
		$("#main-video-playlist").html(idgeBV.fpVideo.playlist.HTML);
		$('.main-video-clickable').click(idgeBV.fpVideo.doClick);
		
	},
	doClick : function (e) {
		
			if (idgeBV.isMobile && !$('#VideoPlayerMain').is(':visible')){
				var plur = $(this).attr('data-plur');
				window.location.href = plur;
				return;
			}
				
		var id = $(this).attr('data-id') + "";
		$("#VideoPlayerMain").unbind();
		/* FF on OSX ignores the unbind command, so just nuke the whole DOM element ... */
		$("#VideoPlayerMain").replaceWith("<div id=\"VideoPlayerMain\"></div>");
		$("#VideoPlayerMain").html(idgeBV.fpVideo.HTML);
		$("div#eVideoName span").remove(); // removes title above video that was added in bright-vid.js
		idgeBV.fpVideo.insertPlayer(id);
		return false;
	},
	insertPlayer : function(id, volume) {
		$('.playbutton-now').attr({'class' : 'playbutton'});
		$('#playbutton'+id).attr({'class' : 'playbutton-now'});
		var title = $(this).attr('data-title') + "";
		idgeBV.debug("Playing video #" + id);
		
		
		if ("" != "") {
			idgeBV.prerolls[id] = "http://ad4.liverail.com/?LR_PUBLISHER_ID=&ingestCuePoints=true&LR_SCHEMA=vast2-vpaid&LR_AUTOPLAY=0&LR_MUTED=1&LR_CONTENT=6&LR_TITLE="+escape("")+"&LR_VIDEO_ID=&LR_ENVIRONMENT=flash,html5&LR_URL=" + escape(escape(window.location.href));
		} else {
			idgeBV.prerolls[id] = "";
		}
		
		$("#VideoPlayerMain").html(idgeBV.fpVideo.HTML);
		$('#mediaContainer').html('');
		idgeBV.fpVideo.currentPlaylist = new Array();

		/* Make sure the current video is last on the current playlist */
		for (var i = 0; i < idgeBV.fpVideo.playlist.ids.length; i++) {
			if (idgeBV.fpVideo.playlist.ids[i] != id) {
				idgeBV.fpVideo.currentPlaylist.push(idgeBV.fpVideo.playlist.ids[i]);
			}
		}
		idgeBV.fpVideo.currentPlaylist.push(id);
		idgeBV.initPlayer(480, 270, id, "ctw", "mediaContainer42044", idgeBV.fpVideo.currentPlaylist, true, idgeBV.prerolls[id], idgeBV.fpVideo.droidVids[id].plur, idgeBV.fpVideo.droidVids[id].poster, idgeBV.playerConfigs.big, "advancePlayIcon", true, volume);
	}
};

idgeBV.fpVideo.init();

function advancePlayIcon(){
	var getNextItem = false;
	var firstId = "";
	var prevId = "";
	var nextId = "";
	$( ".main-video-playlist-item" ).each(function( index ) {
		if (index == 0){
			firstId = $(this).attr('data-id') + "";
		}
		
	  	if (getNextItem){
	  		nextId = $(this).attr('data-id') + "";
	  		getNextItem = false
	  	}
	  	else if (prevId == "" && $(this).find(".playbutton-now").length) {
	  		prevId = $(this).attr('data-id') + "";
	  		getNextItem = true;
	  	}
	});	
	if (nextId != ""){
		$('#playbutton'+prevId).attr({'class' : 'playbutton'});
		$('#playbutton'+nextId).attr({'class' : 'playbutton-now'});
	}
	else if (prevId != null){
		$('#playbutton'+prevId).attr({'class' : 'playbutton'});
		$('#playbutton'+firstId).attr({'class' : 'playbutton-now'});
	}
}

</script>
				</div>
				<div id="main-video-footer-link"><a href="/video/">More videos</a></div>
			</div>
