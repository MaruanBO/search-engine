<script type="text/javascript">
	function showCorrectForm(myform) {
	    $(document).ready(function () {
		    if (myform=="capture") {
		    			    	//emailCapture = "/tools/basbleu_email_capture2.hzml";
		    	//$(this).modal({width:538, height:278, src:emailCapture}).open();return false;
	        	$(".main").append('<div class="loadmask2"></div>');      		
	        	$('#addcap').fadeIn("fast");		    	
		    }
		    else {
		    	   
		    }  	    
	    });
	}  
	function pop_close() {
		$(document).ready(function () {
			$(".capwrapper").fadeOut();
			$(".loadmask2").fadeOut();
			return false;
		});
	}	
	
	$(function() {
	    $(".submitbutton").click(function() {
	      var email = $("input#email").val();
	      var zip = $("input#zip").val();
	      if (!isEmailAddr(email)) {
				alert("Please enter a complete email address in the form: yourname@yourdomain.com");
				return false;
			} else {
				var dataString = 'bill_email=' + email + '&action=SERVE&item=tools/basbleu_email_capture2.hzml&request=optinrequest&optin=yes&capture=Y';
				//alert (dataString);return false;
				$.ajax({
					type: "POST",
					url: "http://www.basbleu.com/cgi-bin/hazel.cgi",
					data: dataString,
					dataType: "html",
					success: function() {
						$('.popform').hide();
						$('.popsuccess').show();
					}
				});
				return false;		
			} // end if/else for form checking
	    }); // end if for submit button clicked
  	});
</script>


<script type="text/javascript">
$(document).ready(function () {
	var checkval = $.cookie('capturewin');
	if ( (checkval == 'capture') || (checkval == 'nocapture') )  {
			} else {
		var WIN = "capture";
		showCorrectForm(WIN);
				var wasset = $.cookie('capturewin', WIN, { 
			expires: 400, 
			path: '/',
			domain: '.basbleu.com'
		});
			} // end if/else for existence of cookie
});
</script>


        
				  
		
				
   		
   		   		   		   			
   		   		        												
		
