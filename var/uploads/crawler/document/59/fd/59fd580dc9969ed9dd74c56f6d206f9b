<script type="text/javascript">
  jQuery(window).load(function(){
    //centerText();
  });
  
  jQuery(window).resize(function(){
    //centerText();
  });
  
  function centerText(){
    if(parseInt(jQuery(window).width()) > 756){
      var textWidth = parseInt(jQuery('#busyDinnerBottom .bottomText').width());
      var billboardWidth = parseInt(jQuery('#busyDinnerBottom').width());
      console.log(textWidth + ' and ' + billboardWidth);
      var marginLeft = (billboardWidth/2) - (textWidth/2);
      console.log(marginLeft);
      jQuery('#busyDinnerBottom .bottomText').css('margin-left', marginLeft + 'px');
    }
  }
</script>
