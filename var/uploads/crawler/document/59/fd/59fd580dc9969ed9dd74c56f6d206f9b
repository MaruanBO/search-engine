  function printRecipe(){
    var WinPrint = window.open('', '', 'letf=0,top=0,width=800,height=900,toolbar=0,scrollbars=0,status=0');
    WinPrint.document.write('<style> body { font-family:arial; } .recipeShare { display:none; } .recipeInfo .recipeDetail .recipeLeftCol img { max-width:240px; }</style>');
    WinPrint.document.write(jQuery('.recipeDetail').remove('.recipeShare').html()); 
    WinPrint.document.close();
    WinPrint.focus();
    WinPrint.print();
    WinPrint.close();
  }
  
