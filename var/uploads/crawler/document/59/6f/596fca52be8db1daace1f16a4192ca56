<script type="text/javascript"> 
// IIFE - Immediately Invoked Function Expression

function getArticle(type,from,filter) {
	var dynamicData = {};
	dynamicData["headlinesNb"] = 25;
	dynamicData["type"] = type;
	dynamicData["filter"] = filter;
	dynamicData["limit"] = $("form#select_filter > input[name$='limit']").val();
	
	if ( from == 0 ){
		dynamicData["ajxp_joblo"] 	= 0;
		dynamicData["ajxp_arrow"] 	= 0;
		dynamicData["ajxp_hotties"] = 0;
		dynamicData["ajxp_video"] 	= 0;
	} else {
		dynamicData["ajxp_joblo"] 	= $("form#pagination > input[name$='ajxp_joblo']").val();
		dynamicData["ajxp_arrow"] 	= $("form#pagination > input[name$='ajxp_arrow']").val();
		dynamicData["ajxp_hotties"] = $("form#pagination > input[name$='ajxp_hotties']").val();
		dynamicData["ajxp_video"] 	= $("form#pagination > input[name$='ajxp_video']").val();
	}
	
	return $.ajax({
		url: "headlines.json.2.0.2.php",
		type: "get",
		data: dynamicData,
		dataType: 'json',
		beforeSend: function() {
			$('#loading').show();
		},
		success: function(data) {
			$('#loading').hide();
			$("form#pagination > input[name$='ajxp_joblo']").val(data.ajxp_joblo);
			$("form#pagination > input[name$='ajxp_arrow']").val(data.ajxp_arrow);
			$("form#pagination > input[name$='ajxp_hotties']").val(data.ajxp_hotties);
			$("form#pagination > input[name$='ajxp_video']").val(data.ajxp_video);
		}
	});
}

function getActiveType() {
	var type = [];
	$('.filter > ul > li.active').each(function() {
		var active = $(this).find('a').text().toLowerCase();
		type.push(active);
	});
	return type;
}

var previousColumnSize = -1;

function hideNotCompletedLinesFeedArticles() {

	curWidth = jQuery(window).width(); //store the window's current width

	
	// All comparations widths reference to the with of responsive.css size changes.
	if ( curWidth > 975 ) {
		columnSize = 5;
	} else if ( curWidth > 725 ) {
		columnSize = 4;
	} else if ( curWidth > 670 ) {
		columnSize = 3;
	} else {
		columnSize = 1;
	}
	
	if ( columnSize == 1 ) {

		if ( previousColumnSize != 1 )  {

			$('#div-gpt-ad-1358966055166-1').html(''); //slot1
			$('#div-gpt-ad-1358966055166-0').html(''); //slot2
			
			if(typeof googletag !== 'undefined'){
			
			googletag.cmd.push(function() {
				slot4 = googletag.defineSlot('/25961103/mobile_joblo_middle_300x250', [300, 250], 'div-gpt-ad-1392217463771-0').addService(googletag.pubads());
				slot5 = googletag.defineSlot('/25961103/mobile_joblo_bottom_300x250', [300, 250], 'div-gpt-ad-1362519122897-0').addService(googletag.pubads());
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
				googletag.display('div-gpt-ad-1392217463771-0');
				googletag.display('div-gpt-ad-1362519122897-0');
			});

			googletag.cmd.push(function() {
				slot3 = googletag.defineSlot('/25961103/mobile_joblo_top_320x50', [320, 50], 'div-gpt-ad-1362519122897-1').addService(googletag.pubads());
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
				googletag.display('div-gpt-ad-1362519122897-1');
			});
			
			
			
			if ( previousColumnSize != -1 ) { googletag.pubads().refresh([slot3,slot4,slot5]); }
			
			}
			
			previousColumnSize = 1;
		}
	}
	else if ( columnSize == 3 ) 
	{

		if ( previousColumnSize != 3 ) {
	
			$('#div-gpt-ad-1358966055166-1').html(''); //slot1
			$('#div-gpt-ad-1358966055166-0').html(''); //slot2
			$('#div-gpt-ad-1392217463771-0').html(''); //slot4
			$('#div-gpt-ad-1362519122897-0').html(''); //slot5
			
			if(typeof googletag !== 'undefined'){

			googletag.cmd.push(function() {
				slot3 = googletag.defineSlot('/25961103/mobile_joblo_top_320x50', [320, 50], 'div-gpt-ad-1362519122897-1').addService(googletag.pubads());
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
				googletag.display('div-gpt-ad-1362519122897-1');
			});

			if ( previousColumnSize != -1 ) { googletag.pubads().refresh([slot3]); }
			
			}
			
			previousColumnSize = 3;
		}
	}
	else if ( columnSize == 4 ) 
	{

		if ( previousColumnSize != 4 ) {
	
			$('#div-gpt-ad-1358966055166-1').html(''); //slot1
			$('#div-gpt-ad-1358966055166-0').html(''); //slot2
			$('#div-gpt-ad-1392217463771-0').html(''); //slot4
			$('#div-gpt-ad-1362519122897-0').html(''); //slot5

			if(typeof googletag !== 'undefined'){
			
			googletag.cmd.push(function() {
				slot3 = googletag.defineSlot('/25961103/mobile_joblo_top_320x50', [320, 50], 'div-gpt-ad-1362519122897-1').addService(googletag.pubads());
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
				googletag.display('div-gpt-ad-1362519122897-1');
			});

			if ( previousColumnSize != -1 ) { googletag.pubads().refresh([slot3]); }
			
			}
			
			previousColumnSize = 4;
		}
	}
	else 
	{

		if ( previousColumnSize != 5 ) {

			$('#div-gpt-ad-1362519122897-1').html(''); //slot3
			$('#div-gpt-ad-1392217463771-0').html(''); //slot4
			$('#div-gpt-ad-1362519122897-0').html(''); //slot5
			
			if(typeof googletag !== 'undefined'){
			
			googletag.cmd.push(function() {
				slot1 = googletag.defineSlot('/25961103/portal_top_728x90', [728, 90], 'div-gpt-ad-1358966055166-1').addService(googletag.pubads());
				slot2 = googletag.defineSlot('/25961103/portal_side_300x250', [300, 250], 'div-gpt-ad-1358966055166-0').addService(googletag.pubads());
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
				googletag.display('div-gpt-ad-1358966055166-1');
				googletag.display('div-gpt-ad-1358966055166-0');
			});

			if ( previousColumnSize != -1 ) { googletag.pubads().refresh([slot1,slot2]); }
			
			}
			
			previousColumnSize = 5;
		}
	}
	

	elementNumber = $("#feed").children('article').length;
	
	
	lastLineCount = elementNumber % columnSize;
	if ( lastLineCount > 0) {
		// Hide last line
		for (i=1; i<=lastLineCount; i++)
		{ 
			$('#feed article:nth-last-child('+i+')').css('display','none');
		} 
	} else {
		// Show last line
		$('#feed article').css('display','inline-block');
	}
}

(function($, window, document) { // The $ is now locally scoped 
	// Listen for the jQuery ready event on the document
	$(function() {
		// slideshow
		
		var slideshow = $('ul.first').bxSlider({
			pager:true,
			mode:'horizontal',
			speed:1000,
			auto:true,
			pause:7000
		});
		
		$(window).resize(function(){
			setTimeout(function(){
				slideshow.destroySlider();				
				slideshow.reloadSlider({
					pager:true,
					mode:'horizontal',
					speed:1000,
					auto:true,
					pause:7000
				});				
			},300);		
		});	
		
		$( "section#feed div.filter ul li" ).removeClass('active');
		
		var clickLoadMore = true;
		
		// Checking if its a bot.
		
		 var isBot = false; 
	
		if ( isBot ) {
			// no cookie
			$( "section#feed div.filter ul li.joblo" ).addClass('active');

			$("form#pagination > input[name$='ajxp_joblo']").val(0);
			$("form#pagination > input[name$='ajxp_arrow']").val(0);
			$("form#pagination > input[name$='ajxp_hotties']").val(0);
			$("form#pagination > input[name$='ajxp_video']").val(0);
			
			getArticle("bot",0).done(function(data) {
				if(data.view_load_more===false){
					$("#portal_loadmore").hide();
				}
				$('#feed article').css('display','inline-block');
				$("#feed").append(data.html);
				hideNotCompletedLinesFeedArticles();
				$("section#feed article").delay(5000).removeClass('hidethis').addClass('test2');
				$('.cmd_portal_loadmore span').text('Load more movie news headlines');
			});
		} else {
			// first load
			if( $.cookie("joblo_portal_tabs_selected") != null ){
				// if having cookie
				var filter = $("form#select_filter > input[name$='filter']").val();
				var cookie_type = $.cookie("joblo_portal_tabs_selected");
				var substr = cookie_type.split(',');
				$.each( substr, function( key, value ) {
					$( "section#feed div.filter ul li."+value ).addClass('active');
				});

				getArticle(substr,0,filter).done(function(data) {
					if(data.view_load_more===false){
						$("#portal_loadmore").hide();
					}
					$("section#feed article").remove();
					$('#feed article').css('display','inline-block');
					$("#feed").append(data.html);

					// Add mobile add after 12.

					hideNotCompletedLinesFeedArticles();
					$("section#feed article").delay(5000).removeClass('hidethis').addClass('test2');
					$('.cmd_portal_loadmore span').text('Load more movie news headlines');
				});
			}else{	
				// no cookie
				$( "section#feed div.filter ul li.joblo" ).addClass('active');

				getArticle("start",0).done(function(data) {
					if(data.view_load_more===false)
						$("#portal_loadmore").hide();
					$('#feed article').css('display','inline-block');
					$("#feed").append(data.html);

					// Add mobile add after 12.

					hideNotCompletedLinesFeedArticles();
					$("section#feed article").delay(5000).removeClass('hidethis').addClass('test2');
					$('.cmd_portal_loadmore span').text('Load more movie news headlines');
				});
			}
		}
		
		// click on filter cmd
		$('.filter > ul > li').bind('click', function(e){
			e.preventDefault();
			
			$("form#pagination > input[name$='ajxp_joblo']").val(0);
			$("form#pagination > input[name$='ajxp_arrow']").val(0);
			$("form#pagination > input[name$='ajxp_hotties']").val(0);
			$("form#pagination > input[name$='ajxp_video']").val(0);
			
			//$("section#feed div.filter form#nbr_return_headline input[name$='nbr']").val(18);
			$('.filter ul li.all').removeClass('active');
			if( $(this).text().toLowerCase() != "all" ){
				if( $(this).hasClass('active') ){
					$(this).removeClass('active');
				}else{
					$(this).addClass('active');
				}
			}else{
				$('.filter ul li').removeClass('active');
			}
			if( ! $('.filter ul li').hasClass("active") ) {
				$('.filter ul li.all').addClass('active');
			}
		});
		
		// click on load more cmd
		$("#portal_loadmore").on("click", ".cmd_portal_loadmore", function(){
			if(clickLoadMore) {
				
				clickLoadMore = false;
				
				// Loading
				loadbutton = $(this).find('span').text();
				$(this).find('span').text('Loading...');

				$("section#feed div.filter form#nbr_return_headline input[name$='nbr']").val((5*4));
				var filter = $("form#select_filter > input[name$='filter']").val();
				var from = $("section#feed article").length;
				var type = getActiveType();

				getArticle(type,from,filter).done(function(data) {
					if(data.view_load_more===false)
						$("#portal_loadmore").hide();
					if(data.html!=null) {
						$('#feed article').css('display','inline-block');
						$("#feed").append(data.html);
						hideNotCompletedLinesFeedArticles();
						setTimeout(" $('section#feed article').removeClass('hidethis')", 150 );
						$('.cmd_portal_loadmore span').text(loadbutton);
						clickLoadMore = true;
					} else {
						$("#portal_loadmore").fadeOut();
						$('.cmd_portal_loadmore span').text(loadbutton);
					}
					$('.cmd_portal_loadmore span').text('Load more movie news headlines');
				});
			}
		});
		
		// change type
		$("#feed > .filter > ul").on("click", "li", function(){
			$("form#select_filter > input[name$='limit']").val('');
			$("#portal_loadmore").fadeIn();
			var filter = $("form#select_filter > input[name$='filter']").val();
			var type = getActiveType();
			
			$.cookie("joblo_portal_tabs_selected", type, { expires: 365 });
			
			$("form#pagination > input[name$='ajxp_joblo']").val(0);
			$("form#pagination > input[name$='ajxp_arrow']").val(0);
			$("form#pagination > input[name$='ajxp_hotties']").val(0);
			$("form#pagination > input[name$='ajxp_video']").val(0);

			getArticle(type,0,filter).done(function(data){
				if(data.view_load_more===false)
					$("#portal_loadmore").hide();
				$("section#feed article").remove();
				$('#feed article').css('display','inline-block');
				$("#feed").append(data.html);
				hideNotCompletedLinesFeedArticles();
				setTimeout(" $('section#feed article').removeClass('hidethis')", 150 );
			});
		});

		// click on filter
		$("#feed > .filter > .dropdown > ul").on( "click", "li", function(){
			$("#feed > .filter > .dropdown > span").html( $(this).text() );
			$("section#feed article").remove();
			$("#portal_loadmore").show();
			var filter = $(this).text().toLowerCase().replace(' ','_').replace(' ','_');
			var type = getActiveType();
			$("form#select_filter > input[name$='filter']").val(filter);
			$("form#select_filter > input[name$='limit']").val('all');
			$("#portal_loadmore .cmd_portal_loadmore").trigger("click");
			$("#portal_loadmore").hide();
		});
		
		// mobile filter menu
		$('.filter h1.btn').click(function(){
			$(this).toggleClass('closed');
			$('.filter > ul').slideToggle(200);
			
			$('.filter .search .button').removeClass('open');
			$('.filter .search form').slideUp(200);		
		});
		
		$('.filter .search .button').click(function(){
			$(this).toggleClass('open');
			$('.filter .search form').slideToggle(200);			
			if( $(window).width() < 975 ){
				$('.filter > ul').slideUp(200);
				$('.filter h1.btn').removeClass('closed');
			}
		});
		
		$( window ).resize(function() {
			hideNotCompletedLinesFeedArticles();
		});
		
		
		var ready_add_content = true;
		var pageCount = 1;
				
		$(window).scroll(function( ) {
			var docViewTop = $(window).scrollTop();
			var docViewBottom = docViewTop + $(window).height();
			var elemTop = $("#feed").offset().top;
			var elemBottom = elemTop + $("#feed").height();
					
			if(docViewBottom >= elemBottom && ready_add_content) {
	
				ready_add_content = false;
	
				// Loading
				loadbutton = $(this).find('span').text();
				$(this).find('span').text('Loading...');

				$("section#feed div.filter form#nbr_return_headline input[name$='nbr']").val((5*4));
				var filter = $("form#select_filter > input[name$='filter']").val();
				var from = $("section#feed article").length;
				var type = getActiveType();

				getArticle(type,from,filter).done(function(data) {
					if(data.view_load_more===false)
						$("#portal_loadmore").hide();
					if(data.html!=null) {
						$('#feed article').css('display','inline-block');
						$("#feed").append(data.html);
						hideNotCompletedLinesFeedArticles();
						setTimeout(" $('section#feed article').removeClass('hidethis')", 150 );
						$('.cmd_portal_loadmore span').text(loadbutton);
						ready_add_content = true;

						console.log('NOTICE: Pushing a pageview to google.');
						// add code push
						
						ga('send', 'pageview', 'http://www.joblo.com/infinite-scroll-'+pageCount);

						pageCount++;

					} else {
						$("#portal_loadmore").fadeOut();
						$('.cmd_portal_loadmore span').text(loadbutton);
					}
					$('.cmd_portal_loadmore span').text('Load more movie news headlines');
				});
				
			}	
		});  
			
	});
}(window.jQuery, window, document));

jQuery(window).load(function() {
	
	// Bubbletip management.
	jQuery("#tooltip-text").css('display','block');
	$('#tooltip').bubbletip($('#tooltip-text'), {
		deltaDirection: 'up',
		bindShow: 'mouseover',
		bindHide: 'mouseout',
		delayHide: '300'
	});
	
	jQuery('#tooltip').bind('click', function( event ) {
		jQuery('table.bubbletip').fadeOut(250);
	});

	jQuery('table.bubbletip').append('<div class="bubbleClose"></div>');
	jQuery('table.bubbletip div.bubbleClose').bind('click', function( event ) {
		event.preventDefault();
		jQuery('table.bubbletip').fadeOut(250);
	});
});

</script>

