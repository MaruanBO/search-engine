	<script>
	jQuery(document).ready(function($){
		
		$(window).on("resize", function(event){
			if( $(this).width() > 550 ) {
				$("#top-nav").slideUp();
				$(".toggle-nav").removeClass('open');
			}
			
		});
		
		
		$(".toggle-nav").on('click', function(e){
			e.preventDefault();
			if($(this).hasClass( "open" )){
				$(this).removeClass('open');
				$("#top-nav").slideUp();
			}else{
				$(this).addClass('open');
				$("#top-nav").slideDown();
			}
		});
	});
	</script>
	
	
	
	
