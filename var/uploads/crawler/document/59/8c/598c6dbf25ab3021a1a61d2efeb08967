<script type="text/javascript">
function start_countDown()
{
    var finalurl = "" == "" ? "/cgi/lottery/bingoblitzhour/timeRemaining.pl" : "/cgi/site/timeRemaining.pl?expiration=";
    finalurl += finalurl.indexOf("?") == -1 ? "?":"&";
    $.ajax({
        async: false,
        type: "GET",
        url: finalurl+"r="+Math.floor(Math.random()*1000000),
        success: function(data)
        {
            if(data > 0)
            {
                countDown(data, '_bingoblitzhour_countdown', "false", "false", "false");
            }
            else
            {
                $("#theTime_bingoblitzhour_countdown").html("00:00:00");
                $("#theTime_bingoblitzhour_countdown").html("00:00");
            }
        }
    });
}
start_countDown();
</script>
</p>
