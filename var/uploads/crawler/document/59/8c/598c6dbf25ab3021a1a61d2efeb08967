<script>
//After page is finished loading, apply our Knockout Bindings.
$j(document).ready(function() {
 
  ko.applyBindings(purchaseFlowModel);

 //////////////////////////////////////////////////////////////////
 //Attach appropriate click events to DOM elements
  
 // "Old" Header. show store
 $j('#purchase_tokens_link').on('click', function(e) {
  e.preventDefault();
   window.purchaseFlowModel.toggleStore();
 });

 // Open store from footer or the "get more tokens" link on gameInfo page, scroll up
 $j('#footer-mygsn-gettokens, #button_get_tokens').on('click', function(e) {
    e.preventDefault();

    //scroll user to top of screen to see store
    window.scrollTo(0, 0);

    //Open up the store.
    window.purchaseFlowModel.toggleStore();

    if($j('#addFundsButton').length)
    {
      $j('#addFundsButton').trigger('click');
    }
 });


  purchaseFlowModel.userRemembered(false);
  purchaseFlowModel.betsoftGameId(jsgc.util.getUrlParameter('betsoftGameId'));

  if(typeof responsiveHeader != 'undefined')
  {
    purchaseFlowModel.inResponsiveHeader(true);
  }

  //Pass in a game id, if we have one
  purchaseFlowModel.gameId();

  if(parseInt(jsgc.util.getUrlParameter('openStore')) === 1)
  {
    if(gsnConfig.user_logged_in && !$('#addFundsButton').length)
    {
        purchaseFlowModel.toggleStore();
    }
    else if(!gsnConfig.user_logged_in)
    {
      //User is not logged in, find a login link and click it.

      //Footer link appears to be a pretty safe and universal place to trigger a click.  Doesn't appear to be any asynchronous issues with binding.
      if($j('.footer_login').length)
      {
        $j('.footer_login').trigger('click');
      }
    }
  }
});

</script>



<script type="text/javascript">

// Use postMessage to talk cross domain and resize iframes accordingly.
// this is used to change from login/forgot to register and back.
// It's additionally used to talk from Gigya back to the top level of an iframed stack.
window.addEventListener('message', function(e) {
    /*console.log("message received: " + e);
    console.log(e);*/
    var $elements = $("#login-frame, #login-box");
    var eventName = e.data[0];
    var data = e.data[1];
    switch(eventName) {
        case 'animateiFrameHeight':
            $elements.animate({height: data+'px'});
        break;
        case 'reloadSelf':
            location.reload();
        break;
        case 'postSocialSSO':
            var redirect = 'https://www.gsn.com/cgi/account/social_sso.pl';
            var form = '';
            $.each( data, function( key, value ) {
                form += '<input type="hidden" name="'+key+'" value="'+value+'">';
            });
            $('<form action="' + redirect + '" method="POST">' + form + '</form>').appendTo($(document.body)).submit();
        break;
    }
}, false);

</script>





