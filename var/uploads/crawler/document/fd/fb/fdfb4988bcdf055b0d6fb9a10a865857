<script>
function getLCookie(cname)
 {
 var name = cname + "=";
 var ca = document.cookie.split(';');
 for(var i=0; i<ca.length; i++) 
   {
   var c = ca[i].trim();
   if (c.indexOf(name)==0) return c.substring(name.length,c.length);
  }
 return "";
 }
var currentCookies = document.cookie;
var cookieName = 'isLoggedIn'; 
var cookieValue = getLCookie(cookieName);
if (cookieValue == 1) {
   jQuery('#navMyAccount').css( "display", "block");
} else {
   jQuery('#navPricing').css( "display", "block");   
}

</script>						</div>
