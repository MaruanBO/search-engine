	<script type="text/javascript">
		Cufon.replace(".f1", { fontFamily: "demand", separate:"none" });
		Cufon.replace(".f2", { fontFamily: "demandlight", separate:"none" });
		Cufon.replace(" .Blog_RightSideSectionHead", { fontFamily: "demandlight", separate:"none" });
		Cufon.replace(" .mod_praiseslides_title_top", { fontFamily: "demandlight", separate:"none" });

		var os = '';
		var browser = '';
		var browserversion = '';
		var validate = '5b5c2d9bbf45aa5eaf3641a2022192bb';


		// =======================================================================================
		// = delay the click slightly so we can give time to get a tracking event over to Google =
		// =======================================================================================
		function recordGoogleTrackingEvent(link, category, value) {
			ga('send', 'event', category, value);
		    setTimeout('document.location = "' + link + '";', 100);
		}


		// =========================================================================================
		// = Functions managing the MAIN navigation menu which sits horizontally at the top of the
		// = site
		// =========================================================================================
		var MainMenuController = function() {
			var currNav = null; // ID of the currently selected navigation item
			var userNavState = false;  // us the user nav showing
			var animationQueue = [];

			return {
				mouseOverState : function(navItem, state) {
					if (state == "on") {
						jQuery("#cil_menu_item_"+navItem).removeClass("cil_menu_item");
						jQuery("#cil_menu_item_"+navItem).addClass("cil_menu_item_hover");
						jQuery("#cil_menu_subnav_item_"+navItem).toggle(250);
					} else if (state == "off") {
						if (navItem != currNav) {
							jQuery("#cil_menu_item_"+navItem).removeClass("cil_menu_item_hover");
							jQuery("#cil_menu_item_"+navItem).addClass("cil_menu_item");
						}
						if (jQuery("#cil_menu_subnav_item_"+navItem)) {
							jQuery("#cil_menu_subnav_item_"+navItem).toggle(false);
						}
					}
				},

				setCurrNav : function(navItem) {
					currNav = navItem;
					jQuery("#cil_menu_item_"+navItem).removeClass("cil_menu_item");
					jQuery("#cil_menu_item_"+navItem).addClass("cil_menu_item_hover");
				},

				toggleUserNav : function() {
					if (userNavState === false) {
						userNavState = true;
						jQuery("#logged_in_user_menu").show("slide",{direction:"up"},250);
					} else {
						userNavState = false;
						jQuery("#logged_in_user_menu").hide("slide",{direction:"up"},150);
					}
				},

				subNavHover : function(item_,state_) {
					if (state_ == "on") {
						jQuery(item_).css("background-color","#000");
					} else if (state_ == "off") {
						jQuery(item_).css("background-color","transparent");
					}
				},

				userMenuHover : function(state_) {
					if (state_ == "on") {
						jQuery("#logged_in_user").css("background-color","#a4a4a4");
					} else if (state_ == "off") {
						if (userNavState === false ) {
							jQuery("#logged_in_user").css("background-color","#b9b9b9");
						}
					}
				},

				userMenuSubNavHover : function(item_,state_) {
					if (state_ == "on") {
						jQuery(item_).css("background-color","#a4a4a4");
					} else if (state_ == "off") {
						jQuery(item_).css("background-color","transparent");
					}
				},

				showPage : function(evt_,page_) {
					window.location = page_;
					try {
						evt_.stopPropagation();
					} catch (ex) {
						evt_.cancelBubble = true;
					}
				},

				tryNowMenuHover : function(state_) {
					if (state_ == "on") {
						jQuery("#head_try_it_now").css("background-image","url(/templates/coveritlive/images/DM/TryItNow_BG_over.gif)");
					} else if (state_ == "off") {
						jQuery("#head_try_it_now").css("background-image","url(/templates/coveritlive/images/DM/TryItNow_BG.gif)");
					}
				},

				loginLogoutHover : function(state_) {
					if (state_ == "on") {
						jQuery("#login_no_user").css("background-color","#1a829e");
					} else if (state_ == "off") {
						jQuery("#login_no_user").css("background-color","#05748f");
					}
				}
			}
		}();
	</script>

