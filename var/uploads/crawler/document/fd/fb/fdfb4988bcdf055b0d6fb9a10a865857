<script>
	var submittingEmailAddress = false;
	function submitEmailAddress() {
		if (submittingEmailAddress) {
			return;
		}
		submittingEmailAddress = true;
		if (jQuery("#emailAddress").val().match(/[a-z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/i) === null) {
	            jQuery("#emailErrorMessage").css('visibility', 'visible');
                    submittingEmailAddress = false;
		} else {
                    var http = location.protocol;
                    var slashes = http.concat("//");
                    var host = slashes.concat(window.location.hostname);
	            jQuery.post(
				host + '/email.php',
		    	{ 
					'email' : jQuery('#emailAddress').val()
		    	},
	            function (data) {
					submittingEmailAddress = false;
		        	if (data.result == 'success') {
	                   jQuery('.cilNewsletter').hide(); 
	                   jQuery('.cilNewsletterSuccess').show(); 
	                } else {
	                   jQuery("#emailErrorMessage").css('visibility', 'visible');
	                }
		    	}, 
				"json"
			);	
		}
	}
    jQuery("body").delegate("#emailSubmit", "click", function() {
		submitEmailAddress();
    });
	jQuery("body").delegate("#emailAddress", "keypress", function(event) {
		var keyCode = (event.which ? event.which : event.keyCode);
		
		if (keyCode == 13) {
			submitEmailAddress();
		}
    });
jQuery(function() {
        if (window.location.hostname == 'now.coveritlive.com') {
            jQuery('.cilNewsletter').hide();
        }
	// determine if placeholder is supported 
	var placeholderTest = document.createElement('input');
	if (!('placeholder' in placeholderTest)) {
	
		// if placeholder is not supported, we add JS support for it
		jQuery(":input[placeholder]").focus(function(event_) {
			if (jQuery(this).attr("type") != "password") {
				if (jQuery(this).attr("placeholder") != '') {
					jQuery(this).data("placeholderValue",jQuery(this).attr("placeholder"));	
				}
				var input = jQuery(this);
			  	if (input.val() == input.attr('placeholder')) {
			    	input.val('');
			    	input.removeClass('placeholder');
			  	}
				jQuery(this).attr("placeholder","");
			}
		});
		jQuery(":input[placeholder]").blur(function(event_) {
			if (jQuery(this).attr("type") != "password") {
				if (jQuery.trim(jQuery(this).val()).length == 0) {
					jQuery(this).attr("placeholder",jQuery(this).data("placeholderValue"));
				}

			  	var input = jQuery(this);
			  	if (input.val() == '' || input.val() == input.attr('placeholder')) {
			    	input.addClass('placeholder');
			    	input.val(input.attr('placeholder'));
			  	}
			} 
		});
		// ensure placeholders show
		jQuery(":input[placeholder]").blur();
	}
});

</script>
<script type="text/javascript">
piAId = '28242';
piCId = '109822';

(function() {
	function async_load(){
		var s = document.createElement('script'); s.type = 'text/javascript';
		s.src = ('https:' == document.location.protocol ? 'https://pi' : 'http://cdn') + '.pardot.com/pd.js';
		var c = document.getElementsByTagName('script')[0]; c.parentNode.insertBefore(s, c);
	}
	if(window.attachEvent) { window.attachEvent('onload', async_load); }
	else { window.addEventListener('load', async_load, false); }
})();
</script>

