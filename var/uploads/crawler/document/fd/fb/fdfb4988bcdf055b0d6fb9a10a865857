			ga('send', 'pageview');

			// ==================================================
			// = Set the cookies and JS vars for CSRF prevetion =
			// ==================================================
			var currentCookies = document.cookie;
			var nonceCookie = currentCookies.indexOf('cilNonce');
			if (nonceCookie == -1) {
				var validate = '5b5c2d9bbf45aa5eaf3641a2022192bb';
				document.cookie = 'cilNonce=5b5c2d9bbf45aa5eaf3641a2022192bb;path=/;domain=.coveritlive.com';
			} else {
				var cookieStart = nonceCookie + 9;                           // Start of cookie value
				var cookieEnd = currentCookies.indexOf(";", cookieStart);          // End of cookie value
				if (cookieEnd == -1) {
					cookieEnd = currentCookies.length;
				}
				var cookieValue = currentCookies.substring(cookieStart, cookieEnd);      // Extract the value
				var validate = cookieValue;
			}
