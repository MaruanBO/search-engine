<script src="/ticket/online3/js/banners.js?m=78dc906686" type="text/javascript" charset="utf-8"></script>
<script src="/ticket/online3/js/home.js?m=244065ea65" type="text/javascript" charset="utf-8"></script>

<script  type="text/javascript" charset="utf-8">
$( document ).ready(function() {
	$("input#searchbox").val("");
	function getParameterByName(name) {
	    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	        results = regex.exec(location.search);
	    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}
		
	 	/*	Randomly choose top banner image */
	 	var initialLocale = "USA";
	 	var bannerList = []
	 
	 	if	( getParameterByName("country") == "DE" || initialLocale == "DEU" ){
	 		bannerList = banners.DE ;
	 	}else{
	 		bannerList = banners.US;
	 	}
	 
			 var bannerObj = bannerList[Math.floor(bannerList.length * Math.random())];
	
			$('.search-container').css('background-image', 'url(/ticket/online3/images/fv/' + bannerObj.image + ')');
			$("#banner-name").html(bannerObj.name);
			$("#banner-location").html(bannerObj.loc);
		/* end banners */


		
		
});
		

</script>


