<script type="text/javascript">
  $(function() {
    $(".no_thanks_button").click(function(event) {
      event.stopPropagation();
      event.preventDefault();
      $("#newsletterModal").addClass('hide fade');
    })

    $(".twitter_button").click(function(event) {
      event.stopPropagation();
      event.preventDefault();
      //$("#newsletterModal").addClass('hide fade');
      window.open('https://twitter.com/frommers');
    })

    $(".facebook_button").click(function(event) {
      event.stopPropagation();
      event.preventDefault();
      //$("#newsletterModal").addClass('hide fade');
      window.open('https://www.facebook.com/FrommersTravelGuides');
    })

    var newsletterViewedCookie = $.cookie('frommers-newsletter-popup-viewed');  
    var newsletterSubscribedCookie = $.cookie('frommers-newsletter-subscribed'); 

    if (newsletterViewedCookie==null && newsletterSubscribedCookie==null) {
      $("#newsletterModal").removeClass('hide');
      $("#newsletterModal").removeClass('fade');
      $.cookie('frommers-newsletter-popup-viewed', 'true', { expires: 45 });
    } else {
      console.log("seen it")
    }


  });

</script>


