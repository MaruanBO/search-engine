    jQuery(document).ready(function ($) {

        var slideshows = $('.cycle-slideshow').on('cycle-next cycle-prev cycle-pager-activated', function (e, opts) {
            // advance the other slideshow
            slideshows.cycle('goto', opts.currSlide);
        });

        $('#carouselImages .slide IMG').click(function () {
            //var index = $('#carouselImages').data('cycle.API').getSlideIndex(this);
            console.log(this);
            var index = $("#carouselImages div").index(this.parentNode);
            slideshows.cycle('goto', index);
        });

    });
</script>


