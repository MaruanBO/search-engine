document.observe('dom:loaded', function() {
	var nodes = $$('#main_nav>li');
	nodes.each( function(el) {
		var area = el.getAttribute('className') == null ? el.getAttribute('class') : el.getAttribute('className');
		
		area = area.split(' ')[0];
				
		if (area != 'search') {
			el.onmouseover = function() {
								
				el.style.backgroundColor = '#0092ef';
				
				$$('#subnav_wrapper>ul').each( function(sub_nav) {
					/*sub_nav.removeClassName('subnav_active');
					sub_nav.addClassName('subnav_passive');*/
				});
							
				var thisId = 'sub_' + area;	
								
				/*$(thisId).removeClassName('subnav_passive');
				$(thisId).addClassName('subnav_active');*/
			}
			
			el.onmouseout = function() {
				el.style.backgroundColor = '';	
			}
		}
	});	
	
	// when we mouse out, these functions run to deactive all sub menus ... the problem is that we have no way of detecting the mouseout event
	/*$('nav_wrapper').onmouseout = function() {
		$$('#subnav_wrapper>ul').each( function(el) {
			el.removeClassName('subnav_active');
			el.addClassName('subnav_passive');
	
			// this finds the current navarea and restores the sub menu for that area on mouse out
			$$('.current').each( function(el) {
