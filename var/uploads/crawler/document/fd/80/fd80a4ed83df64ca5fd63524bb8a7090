<script type="text/javascript">// temp patch for responsive nav without ul
jQuery(document).ready(function () {
  jQuery('.has-dropdown').each(function () {
    if (jQuery('ul',this).length === 0) {
      jQuery(this).removeClass('has-dropdown');
    }
  });
});

//header scroll
jQuery(window).scroll(headerScroll);

function headerScroll() {
	if (jQuery(this).scrollTop() > 5){  
		jQuery('.banner').addClass("collapsed");
	} 
	else{
   		jQuery('.banner').removeClass("collapsed");
	}

	if (jQuery(this).scrollTop() > 700){
		jQuery('.banner').addClass("change");
	}
	else{
		jQuery('.banner').removeClass("change");
	}
  
  //wonder if toggle class would work
  if(jQuery(this).scrollTop() > 480){
    jQuery('#home_nav').removeClass("active");
    jQuery('#about_nav').addClass("active");
  }
  else{
    jQuery('#about_nav').removeClass("active");
    jQuery('#home_nav').addClass("active");
  }
}

