jQuery(document).ready(function() {
  jQuery("#owl-demo").owlCarousel({
  	 navigation : true, // Show next and prev buttons
  	 navigationText: [
  	 	"<img src='http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/left_bl.png'>",
  	 	"<img src='http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/right_bl.png'>"
  	 ],
     //slideSpeed : 300,
     //paginationSpeed : 400,
     singleItem:true,
     autoHeight:true,
     autoPlay:true,
     rewind:false,
     slideSpeed:300,
     paginationSpeed:400,
     goToFirstSpeed:400,
     loop:true
  })

  jQuery(".owl-prev").html('<span class="left_bl"><img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/left.png"></span>');
  jQuery(".owl-next").html('<span class="right_bl"><img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/right.png"></span>');

  /* Hover (Left arrow) */
  jQuery( ".left_bl" ).hover(
  	function() {
  		$( this ).html('<img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/left_bl.png">');
  	}, function() {
  		$( this ).html('<img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/left.png">');
  	}
  );

  /* Hover (Right arrow) */
  jQuery( ".right_bl" ).hover(
  	function() {
  		$( this ).html('<img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/right_bl.png">');
  	}, function() {
  		$( this ).html('<img src="http://spoonuniversity.com/wp-content/themes/newstrick-child/owl-assets/right.png">');
  	}
  );

  //jQuery("div.owl-item").last().html(jQuery("div.owl-item").first().html());

 
});

</script>

