        	<script type="text/javascript">

        	jQuery(function() {

			  /* revolution slider jquery edits */

			  var obj_rev_api = "";

			  /* assign slider objects as per page */
			  if (typeof revapi7 != 'undefined') {
			    obj_rev_api = revapi7;
			  }

			  if (obj_rev_api) {

			  	/* calculate & apply layer css edits */
			  	function fn_apply_layer_css_edits() {
					var int_gutter = 0.7;
			  		var int_rev_slider_width = jQuery("div.rev_slider").width();
			  		var int_layer_left_const = 0;
			        if(jQuery(window).width()<=1024) {
			        	int_layer_left_const = 0.625;
			        }
			        else {
			        	int_layer_left_const = 0.62;
			        }
			  		var int_layer_left = (int_rev_slider_width * int_layer_left_const) + int_gutter;
			  		var int_play_font_size = (int_rev_slider_width * 0.033);
					var str_catg_style = ".hn-slider-catg div a { height:87px;} .hn-slider-catg div:last-child { border-bottom:none !important; }";

					/* safari fix */
					var isSafari = /constructor/i.test(window.HTMLElement);
					if(isSafari) {
						if(jQuery(window).width() >= 904 && jQuery(window).width() <= 906)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 909 && jQuery(window).width() <= 921)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 925 && jQuery(window).width() <= 940)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 949 && jQuery(window).width() <= 961)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 970 && jQuery(window).width() <= 981)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 991 && jQuery(window).width() <= 1003)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1007 && jQuery(window).width() <= 1011)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1015 && jQuery(window).width() <= 1018)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1025 && jQuery(window).width() <= 1027)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1030 && jQuery(window).width() <= 1032)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1036 && jQuery(window).width() <= 1040)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1046 && jQuery(window).width() <= 1048)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1051 && jQuery(window).width() <= 1057)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1067 && jQuery(window).width() <= 1079)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else if(jQuery(window).width() >= 1133 && jQuery(window).width() <= 1139)
						{
							str_catg_style += ".hn-slider-overlay-left {width:52.1% !important;}";
						}
						else
						{
							str_catg_style += ".hn-slider-overlay-left {width:51.87% !important;}";
						}
					}

					// safari fix for ios devices
					var standalone = window.navigator.standalone,
              			 userAgent = window.navigator.userAgent.toLowerCase(),
                            safari = /safari/.test( userAgent ),
                               ios = /iphone|ipod|ipad/.test( userAgent );

          			if( ios ) {
              			if ( !standalone && safari ) {
                  			//browser
                  			str_catg_style += ".hn-slider-overlay-left {width:51.87% !important;}";
              			}
          			}
          			//end - safari fix for ios devices

			  		if(int_rev_slider_width) {
			  			jQuery('<style>.tp-caption { left: '+int_layer_left+'px !important; } .hn-slider-overlay-play i {font-size:'+int_play_font_size+'px !important;} '+ str_catg_style +'</style>').appendTo('head');
			  		}
			  		return true;
			  	}

			  	jQuery( window ).resize(function() {
				  	fn_apply_layer_css_edits();
				});


			    obj_rev_api.bind("revolution.slide.onloaded",function (e) {

			      fn_apply_layer_css_edits();

			      /* build post section html */
			      var str_post_html = "";
			      var str_post_title = "";
			      var str_set_class = "";
			      var slider_class = 0;
			      jQuery( "div.hn-slider-overlay-left" ).each(function( index ) {
			        str_post_title = jQuery(this).find("h2").text();
			        str_set_class = "";
			        if(index < 1) {
			          str_set_class = " class='active' ";
			        }

			        var int_char_limit = 0;
			        if(jQuery(window).width()<=1024) {
			        	int_char_limit = 30;
			        }
			        else {
			        	int_char_limit = 40;
			        }
			        //console.log(int_char_limit);
			        if(int_char_limit > 0 && str_post_title.length > int_char_limit) {
			        	str_post_title = str_post_title.substring(0,int_char_limit) +"..." ;
			        }

			        str_post_html += '<div><a href="'+jQuery(this).find("a.hn-layer-title").attr("href")+'" title="'+jQuery(this).find("h2").text()+'" data-slideindex="'+(index+1)+'" '+str_set_class+'>'+ str_post_title +'</a></div>';

			        slider_class = jQuery("div.revolution-slider ul li:eq("+index+")").find(".hn-slider-overlayer:first").attr("class");
			      	//jQuery("div.revolution-slider ul li:eq("+index+")").append('<div class="hn-mob-slider-overlayer hn-slider-overlayer hn-slider-type-lifestyle"><a href="'+jQuery(this).find("a.hn-layer-title").attr("href")+'" class="hn-layer-title"><h2>'+str_post_title+'</h2></a></div>');
			      	jQuery("div.revolution-slider ul li:eq("+index+")").append('<div class="hn-mob-slider-overlayer '+slider_class+'"><div class="hn-mob-slider-title"><a href="'+jQuery(this).find("a.hn-layer-title").attr("href")+'" class="hn-layer-title"><h2>'+str_post_title+'</h2></a></div><div class="hn-mob-slider-sep"></div><div class="hn-mob-slider-nav"></div></div>');
			      });
			      jQuery( "div.hn-slider-catg" ).html(str_post_html);
			      /* END - build post section html */

			      /* render slide after click on title section */
			      jQuery("div.hn-slider-catg a").on("click",function(ec) {
			        ec.preventDefault();
			        var int_slide_index = jQuery(this).data("slideindex");
			        obj_rev_api.revshowslide(int_slide_index);
			      });
			      /* END - render slide after click on title section */

			      /* link click on slider to page detail */
			      jQuery("div.rev_slider .tp-bgimg").on("click",function(ec) {
			        ec.preventDefault();
			        var str_page_link = jQuery( "div.hn-slider-catg a.active" ).attr("href");
			        window.location.href = str_page_link;
			      });

			      /* END - link click on slider to page detail */
			    });

			    obj_rev_api.bind("revolution.slide.onchange",function (e,data) {
			      jQuery( "div.hn-slider-catg a" ).removeClass("active");
			      jQuery( "div.hn-slider-catg a[data-slideindex="+data.slideIndex+"]" ).removeClass("active").addClass("active");
			    });
			  }
			  /* END - revolution slider jquery edits */
