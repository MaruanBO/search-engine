<script type="text/javascript">
jQuery('.rotate div').hide();
jQuery(document).ready(function(){
	var length= jQuery('.rotate div').length;
	var range = [], touts = [];
	for(x=0;x<length;x++) range.push(x);
	for(x=0;x<length;x++) touts[x] = jQuery('.rotate div').eq(x).clone();
	range.sort(function() {return 0.5 - Math.random()})
	var newdiv = jQuery('<div class="random-touts"></div>');
	for(var i=0; i<38; i++) newdiv.append(touts[range[i]])
	jQuery('.rotate').html('');
	jQuery('.rotate').append(newdiv);
	jQuery('.rotate div').show();
});
</script></div>
