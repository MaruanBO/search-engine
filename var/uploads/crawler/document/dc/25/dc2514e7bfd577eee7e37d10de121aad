<script type="text/javascript">
  _gaq.push(['_trackEvent', 'Acquisition', 'Show Login']);
  function requestInvite() {
  
  	
    if (confirm_request) {
      var request_link = $('#join_button');
      request_link.addClass("disabled");
      request_link.html("Loading...");
    }
    
    var params = {
      'invite': {
        'recipient_email': $('#email').val()
      }
    }
    $.post('/invites.json', params, function(data) {
      if (data.status) {
        if (data.ajax) {
          $('#invite_wrapper').load(data.url, function() {
            $('#request_wrapper').fadeOut(200);
            setTimeout(function(){
              $('#invite_wrapper').fadeIn(200);
              setTimeout(function(){
                $("#user_password").focus();
              }, 200);
            }, 200);
          });
          
        } else {
        	 _gaq.push(['_trackEvent', 'Login', 'Success']);
        	 _gaq.push(['_trackEvent', 'Acquisition', 'Login - Success']);
			 //window.location.replace(data.url);
			 setTimeout("window.location.replace('"+data.url+"');", 100); // 1/10 second latency to allow Analytics events to log.
        }
        
      } else {
      	_gaq.push(['_trackEvent', 'Login', 'Bad Email Error']);
        if (confirm_request) {
          request_link.removeClass("disabled");
          request_link.html("Join FREE");
        }
        $("#bad_email_alert").show();
        $('#request_wrapper').fadeOut(200);
        setTimeout(function(){
          $('#invite_wrapper').fadeIn(200);
        }, 200);
        $('#continue_button').removeClass("disabled");
        $('#continue_button').val("Continue");
      }
    });
  }
    var confirm_request = false;
  head.ready(function(){
    $('#already_member').click(function(event) {
      event.preventDefault();
      $('#invite_wrapper').animate({opacity: 0}, 200, function() {
        confirm_request = true;
        $('#form_title').html("Login");
        $('#dont_have_invite').show();
        $('#already_member').hide();
        $('#invite_wrapper').animate({opacity: 1}, 200);
      })
    });
    
    $('#dont_have_invite').click(function(event) {
      event.preventDefault();
      $('#invite_wrapper').animate({opacity: 0}, 200, function() {
        confirm_request = false;
        $('#form_title').html("Enter your email for immediate access");
        $('#dont_have_invite').hide();
        $('#already_member').show();
        $('#invite_wrapper').animate({opacity: 1}, 200);
      })
    });
    
    $("#invite_recipient_email").focus();
    
    $('#check_status_form').submit(function(event) {
    	$('.alert').hide();
      event.preventDefault();
      _gaq.push(['_trackEvent', 'Login', 'HomeScreen', 'Attempt']);
      $('#continue_button').addClass("disabled");
      $('#continue_button').val("Loading...");
      $.post($(this).attr("action") + ".json", $(this).serialize(), function(data) {
        if (data.user) {
        	_gaq.push(['_trackEvent', 'Login', 'Returned Existing User']);
          $('#invite_wrapper').load('/login?email=' + encodeURIComponent($('#email').val()), function() {
            $("#user_session_password").focus();
          });
        } else if (data.invite) {
        	_gaq.push(['_trackEvent', 'Login', 'Returned Invite Request']);
          $('#invite_wrapper').load('/users/new?email=' + encodeURIComponent($('#email').val()), function() {
            $("#user_password").focus();
          });
        } else {
          if (confirm_request) { 
          	    if(typeof data.error !='undefined' && data.error=='invalid email'){
          	    	$('#bad_email_alert').show();
          	    	$('#continue_button').removeClass("disabled");
		  			$('#continue_button').val("Continue");
	          	    return false;
          	    }
          	_gaq.push(['_trackEvent', 'Login', 'Confirmed Invite']);
            $('#invite_wrapper').fadeOut(200);
            setTimeout(function(){
              $('#request_wrapper').fadeIn(200);
              $('#request_wrapper #requested_email').html($('#email').val());
            }, 200);
          } else {
            requestInvite();
            return true;
          }
        }
        $('#continue_button').removeClass("disabled");
        $('#continue_button').val("Continue");
      });
    });
    
    $('#join_button').click(function(event) {
      requestInvite();
    });
    
  });
  

  	
 

	if(typeof window.splashoption =='undefined'){window.splashoption=1;}
	if(typeof window.showsplashmessage =='undefined'){window.showsplashmessage=0;}
	$('.splash-display').html($('.splash-option'+window.splashoption).html())
	
	if(window.splashoption==2){$('.splash-display .prod img').css('height', '160px').css('width', '160px');}
	if(window.splashoption==3){

		
		
	}
	
	function showsplash(){
		if(window.showsplashmessage && window.innerWidth>979){
			$('#signup_modal .modal').addClass('has-splash-message');
			$('.splash_tagline, #invite_wrapper').css('max-width', '380px').css('float', 'right');
			$('.splash_underlay').parent().removeClass('splash-container').addClass('span12');
			$('#splash_instructions').show();
		}else{
			$('#signup_modal .modal').removeClass('has-splash-message');
			$('.splash_tagline, #invite_wrapper').css('max-width', 'none').css('float', 'none');
			$('.splash_underlay').parent().removeClass('span12').addClass('splash-container')
			$('#splash_instructions').hide();
		}
	}
	
	function productpreview(){
		if(window.splashoption==3){
			$('.splash_tagline, #signup_modal .tagline').html("We work directly with designers to offer pricing that is only available to our members.").css('fontSize', '18px').css('lineHeight', '30px').css('marginBottom', '4px').css('textAlign', 'justify');
			$('#signup_modal #splash_instructions h3').css('marginTop', '20px').css('lineHeight','45px');
			setTimeout("productrotate();",3000);
			return false;
		}
		if(typeof window.previewid=='undefined' || window.previewid> $('.splash-display .product-preview .prod').length){window.previewid=1;}
		$('.splash-display .product-preview .prod').fadeOut(400);
		setTimeout("$('.splash-display .product-preview .prod:nth-child("+window.previewid+")').fadeIn();",500);
		window.previewid++;
		setTimeout("productpreview()", 4000);
	}
	
	function productrotate(){
		if(typeof window.currentitem =='undefined' || window.currentitem> $('.splash-display .product-preview .prod').length ){window.currentitem=1;}
		if(typeof window.currenttarget =='undefined' || window.currenttarget > $('.splash-display .product-carousel .product-visible').length){window.currenttarget=1;}
		$('.splash-display .product-carousel .product-visible:nth-child('+window.currenttarget+')').fadeOut('slow',function(){	
			$(this).html($('.splash-display .product-preview .prod:nth-child('+window.currentitem+')').html()).fadeIn();
		
		})
		window.currenttarget++;
		window.currentitem++;
		setTimeout("productrotate()", 2000);

	}
	
	$(window).resize(function(){
		showsplash();
	});

	
	
	function startpreviewtimer(){
		$('.timer').html("15:00");	
		var target_date = new Date().getTime()+900000;
		var  minutes, seconds;
		setInterval(function () {
		    var current_date = new Date().getTime();
		    var seconds_left = (target_date - current_date) / 1000;
		    minutes = parseInt(seconds_left / 60);
		    seconds = parseInt(seconds_left % 60);
		    if(seconds<10){seconds="0"+ seconds;}
		    $('.timer').html(minutes + ":" + seconds);  
		}, 1000);
	}
	
	</script>
