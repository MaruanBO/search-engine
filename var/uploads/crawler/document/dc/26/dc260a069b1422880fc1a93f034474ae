<script type="text/javascript">
function sendMail(to, from, other_data, subject, content, url, bcc_address, callback){
    var data = {    
                "to": to,
                "from": from, 
                "subject": subject, 
                "content": content,
                "other_data": other_data,
                "url": url
            };
    if(bcc_address != false && bcc_address != ""){
        data["bcc_address"] = bcc_address;
    }
    jQuery.post(
    		"https://canopylabs.com/wp-content/themes/cl_foundation/library/inc/extensions/send-mail.php",
    		data, 
    		function(data){
        		callback(data);
    		}
    );
}
function subscribeInsights(email, callback){
    jQuery.post(
            "https://canopylabs.com/wp-content/themes/cl_foundation/library/inc/extensions/subscribeInsights.php",
            {"email": email}, 
            function(data){
                callback(data);
            }
        );
}
function sendPaper(name, email_address, paper_name, paper_file_name, paper_type, callback){
    var data = {   
                "whitePaperRequest" : 1,
                "email": email_address,
                "paper_file_name": paper_file_name,
                "paper_type": paper_type,
                "paper_name": paper_name,
                "name": name
            }
    jQuery.post(
            "https://canopylabs.com/wp-content/themes/cl_foundation/library/inc/extensions/send-mail.php",
            data, 
            function(data){
                callback(data);
            }
        );
}
</script>
