<script type="text/javascript">
    function shaddslashes(e) {
        "use strict";
        if (e != undefined) {
            e = e.replace(/%/g, "%25%32%35");
            e = e.replace(/\\/g, "%5C");
            e = e.replace(/\'/g, "%27");
            e = e.replace(/\"/g, "%22");
            e = e.replace(/\?/g, "%25%33%46");
            e = e.replace(/&/g, "%25%32%36");
