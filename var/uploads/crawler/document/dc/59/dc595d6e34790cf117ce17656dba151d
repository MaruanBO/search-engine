            $('.HDRsearch').hover(function () {
                $('.HDRsearch').css('border', '1px solid #616161');
                $('.searchGO .gobtn').css('background-position', '0 -41px');
            }, function () {
                if (sfcs == false) {
                    $('.HDRsearch').css('border', '1px solid #919191');
                    $('.searchGO .gobtn').css('background-position', '0 0');
                } else {
                    $('.HDRsearch').css('border', '1px solid #616161');
                    $('.searchGO .gobtn').css('background-position', '0 -41px');
                }
            });
            $('.HDRsearch').css('border', '1px solid #919191');

            // store current focus field so we know when to call the ajax lookup on the timer
            $(":input").focus(function () {
                focus_id = this.id;
            });

            // Capture enter key and redirect properly
            $('#CT_Header_0_txtKeyword').keyup(function (e) {
                e.preventDefault();
                if (e.keyCode == 13) {
                    document.location.href = '/search/?keywords=' + encodeURIComponent($('#CT_Header_0_txtKeyword').val());
                }
                else {
                    $(".hdnKeywords").val($('#CT_Header_0_txtKeyword').val());
                }
            });
            
            $('#CT_Header_0_txtKeyword').focus(function () {
                $('#CT_Header_0_txtKeyword').val('');
            });

            $('#CT_Header_0_txtKeyword').blur(function () {
                last_value = '';
                $(".hdnKeywords").val($('#CT_Header_0_txtKeyword').val());
                $('#CT_Header_0_txtKeyword').val('Search DaySpring.com');
                setTimeout('$("#SearchResults").html("");', 300);
            });

            LoadSearchResults()
        });
        function LoadSearchResults() {
            if (focus_id == 'CT_Header_0_txtKeyword') {
                var keywords = $('#CT_Header_0_txtKeyword').val();
                if (last_value != keywords && keywords.length > 2 && keywords != 'Search DaySpring.com') {
                    $.ajax({
                        url: "/storeajax.aspx",
                        data: "f=GetSearchResults&SiteId=1&Keywords=" + encodeURIComponent(keywords),
                        success: function (result) { $("#SearchResults").html(result); }
                    });
                    last_value = keywords;
                }
            }

            setTimeout(LoadSearchResults, 500);
        }
        function MyBuyAdd2Cart(url) {
            $.getJSON(url, function (o) {
                if (!o.Success) {
                    alert((o.Error ? o.Error : 'An error occurred while trying to add the item to your cart.'));
                    return;
                }

                $('#divItemAdded').html(o.Html);
                $('#spanCartTotal').html(o.Quantity + (parseInt(o.Quantity) == 1 ? ' Item ' : ' Items '));

                showRecentCart();
            });
        }
    });    
    //END idev search drop down
    //]]>
