<script language=javascript>
var BROWSER_NAME = "";
var BROWSER_VERSION = "";
var IE6Logo="";

/* 09/19/2012, Kevin Sun,  Added for solving undefined jq issue */
jq=jQuery.noConflict();
if (typeof jq === 'undefined') {
    // jq is undefined
    jq=jQuery.noConflict();
}
jq(document).ready(function() {

	checkIfbrowserIsIE6();
});

function checkIfbrowserIsIE6()
{
//Kevin Sun, 09/19/2012 modified, coz cookie plunin defined as jQuery.cookie
//    var browserIgnoreCookieVal = jq.cookie('ignore_browser_alert')
	if (typeof jQuery.cookie !=='undefined')
	{
	   var browserIgnoreCookieVal = jQuery.cookie('ignore_browser_alert');
	}
			
	if (browserIgnoreCookieVal == null || browserIgnoreCookieVal == undefined || browserIgnoreCookieVal == "")
	{
		var useragent = jq.browser;
		//var browserVersion = parseInt(useragent.version);
		var browserVersion = parseInt(BROWSER_VERSION);
		
		if(jq.browser.msie)
		{
			if(browserVersion < 7  && LANGUAGE == 'en')
			{
				IE6Logo="<img align='center' src='" + DAYCONTENT_IMAGE_URL + "/content/dam/etc/media_library/cn/0/cn/logo/browser/Ie6_logo.gif'/><br/>";
				showBrowserUpgradePopup();
				
			}
		}
		else if('Firefox' == BROWSER_NAME )
		{
			if(BROWSER_VERSION < 3  && LANGUAGE == 'en')
			{
				showBrowserUpgradePopup();
			}
		}


		jq('#browserSupportLater').click(function(e){
			jq.cookie('ignore_browser_alert', 'true', { expires: 60, path: '/' });
			jq('#browserUpgradeWrapper').addClass('hide');
			jq('#searchHotel').removeClass('hide');
		});

		jq('#closeBrowserUpgrade').click(function(e){
			
			jq.cookie('ignore_browser_alert', 'true', { expires: 1, path: '/' });
			jq('#browserUpgradeWrapper').addClass('hide');
			jq('#searchHotel').removeClass('hide');
		});
	}
	
	
}

function showBrowserUpgradePopup(){
	jq("#browserUpgradeWrapper").popupModalDialog();
	//jq('#browserUpgradeWrapper').removeClass('hide');
	jq('#searchHotel').addClass('hide');
	jq('#browserSupportLater').addClass("bClose");
	jq('#browserinfo').html("You are currently using <br/>" + IE6Logo + BROWSER_NAME + ": "  + BROWSER_VERSION);
}

</script>



	
