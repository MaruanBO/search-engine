<script>

function bluebarCloseAll(){
	jQuery('#contents1, #contents2, #contents3, #contents4, #contents5').slideUp("fast");
}
function blusebarChangeLinkCSS(data){
	data.css('background','#0a58a3').css('border','1px solid #0a58a3').css('color','#fff');
}
function bluebarToggleContents(data){
	data.slideToggle("fast");
}
function bluebarResetColorAll(){
	jQuery('#tab1link, #tab2link, #tab3link, #tab4link, , #tab5link').css('background','#fff').css('border','1px solid #efefef').css('border-top','none').css('color','#666');
}
function disableCurrentLocation(strLocation){

	jQuery('#' + strLocation).toggleClass('greyout');
	jQuery('#' + strLocation + "btn").toggleClass('nocursor')
	if (strLocation == "na")
	{
		jQuery("#tabnav li:nth-child(5)").css("display", "none");
	}
}

$(document).ready(function() {
	jQuery('#tab1link').click(function(){
		bluebarCloseAll();
		bluebarResetColorAll();
		blusebarChangeLinkCSS(jQuery(this));
		bluebarToggleContents(jQuery('#contents1'));
	});
	jQuery('#tab2link').click(function(){
		bluebarCloseAll();
		bluebarResetColorAll();
		blusebarChangeLinkCSS(jQuery(this));
		bluebarToggleContents(jQuery('#contents2'));
	});
	jQuery('#tab3link').click(function(){
		bluebarCloseAll();
		bluebarResetColorAll();
