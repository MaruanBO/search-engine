<script type="text/javascript" charset="utf-8">
  (function(){
      var cookies;

      var readCookie = function(name,c,C,i){
          if(cookies){ return cookies[name]; }

          c = document.cookie.split('; ');
          cookies = {};

          for(i=c.length-1; i>=0; i--){
             C = c[i].split('=');
             cookies[C[0]] = C[1];
          }

          return cookies[name];
      };

      window.readCookie = readCookie; // or expose it however you want
  })();

  if( geoip['region_code'] == 'MA' ){
    if (!window.readCookie('gotv-splash')) window.location = '/splash-pages/gotv';
  } else {
    if (!window.readCookie('learnmore-splash')) window.location = '/splash-pages/learnmore';
  }
  
</script>

    
