    <script type="text/javascript">
    function trackOutboundLink(link, category, action, proceed) {
        try {
            ga('send','event', category, action, encodeURIComponent(link));
        } catch(err) {}
        if(proceed) {
            setTimeout(function(){
                document.location.href = link;
            }, 100);
        }
    }

    //two severities on page load and on add to cart
    function trackOutOfStockPageview(severity, sku) {
        try {
            ga('send','event', severity, sku);
        } catch(err) {}
    }

    jQuery(document).ready(function(){
        //tracks offsite link clicks
        jQuery(document.links).each(function() {
            if(
                jQuery(this).attr('href').indexOf(document.domain) == -1
                    && jQuery(this).attr('href').indexOf('/') != 0
                    && jQuery(this).attr('href') != '#'
                ) {
                jQuery(this).click(function() {
                    if(jQuery(this).attr('target').indexOf('blank') == -1) {
                        trackOutboundLink(jQuery(this).attr('href'), 'Outbound Link', 'Click', true);
                    } else {
                        trackOutboundLink(jQuery(this).attr('href'), 'Outbound Link', 'Click', false);
                    }
                });
            }
        });


        //tracks iso page views
        jQuery('div.instoreonly').each(function() {
            var sku = jQuery(this).find('span:contains("Item #")').text().substring(jQuery(this).find('span:contains("Item #")').text().indexOf('Item #') + 6).trim();
            trackOutOfStockPageview('Out of Stock Page View', sku);
        });

        //tracks impression of the banners in the homepage featured rotator
        jQuery('.slide_images a img').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Homepage Rotator','Impression',imgName);
        });

        //tracks clicks of the banners in the homepage featured rotator
        jQuery('.slide_images a').click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Homepage Rotator', 'Click', imgName);
                }
            });
        });

        //tracks impression of the banners in promo slot one on the home page
        jQuery('.home-promo01 a img').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Homepage Middle Left','Impression',imgName);
        });

        //tracks the clicks of the banners in promo slot one on the home page
        jQuery('.home-promo01 a').click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Homepage Middle Left', 'Click', imgName);
                }
            });
        });

        //tracks impression of the banners in promo slot three on the home page
        jQuery('.home-promo03 a img').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Homepage Middle Right','Impression',imgName);
        });

        //tracks the clicks of the banners in promo slot three on the home page
        jQuery('.home-promo03 a').click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Homepage Middle Right', 'Click', imgName);
                }
            });
        });

        //tracks impression of the banners in promo slot two on the home page
        jQuery('.home-promo02 div a img').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Homepage Middle Center','Impression',imgName);
        });

        //tracks the clicks of the banners in promo slot two on the home page
        jQuery('.home-promo02 div a').click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Homepage Middle Center', 'Click', imgName);
                }
            });
        });

        //tracks the impressions global banners on the page, the rest of them
        jQuery('.widget-banner ul li a img').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Global Banner','Impression',imgName);
        });

        //tracks the clicks of global banners on the page,the rest of them
        jQuery('.widget-banner ul li a').click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Global Banner', 'Click', imgName);
                }
            });
        });

        //tracks the impressions targeted global banners on the page, the rest of them
        jQuery('.fcs-targeted-banner').each(function(index, value) {
            var imgSrc = jQuery(this).attr('src');
            imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
            imgName = imgName.substring(0, imgName.lastIndexOf('.'));
            ga('send','event','Global Banner','Impression',imgName);
        });

        //tracks the clicks of targeted global banners on the page,the rest of them
        jQuery('.fcs-targeted-banner').parent().click(function() {
            jQuery(this).children('img').each(function(index, value) {
                //this if is a fail safe, only record a click for the first image
                if(index == 0) {
                    var imgSrc = jQuery(this).attr('src');
                    imgName = imgSrc.substring(imgSrc.lastIndexOf('/') + 1);
                    imgName = imgName.substring(0, imgName.lastIndexOf('.'));
                    ga('send','event', 'Global Banner', 'Click', imgName);
                }
            });
        });

        // Perpetual Cart Events

        jQuery("a[href='/blog']").click(function() {
            ga('send','event', 'HomeLinks', 'Click', 'Visit Our Blog');
        });

        jQuery("#checkout-link-top").click(function() {
            ga('send','event', 'Perpetual Cart', 'Click', 'My Cart');
        });

        // Create function to add event to perp cart buttons so we can reuse it
        // when the cart is reloaded via ajax
        function addPerpCartEvents() {
            jQuery("#cart-sidebar .pc-buttons a:nth-child(1)").click(function() {
                ga('send','event', 'Perpetual Cart', 'Click', 'Edit Cart');
            });

            jQuery("#cart-sidebar .pc-buttons a:nth-child(2)").click(function() {
                ga('send','event', 'Perpetual Cart', 'Click', 'Checkout');
            });
        }

        addPerpCartEvents();
        jQuery("#perpetual_cart").ajaxStop(function() { addPerpCartEvents(); });

        // Track link clicks in footer navigation
        jQuery("ul.footerLinksCol li a").click(function()
        {
            var eventLabel = jQuery(this).text();

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Footer Links', 'Click', eventLabel);
            }
        });

        // Track link clicks in top navigation
        jQuery("a.navTab").click(function()
        {
            var eventLabel = jQuery(this).text();

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Navigation', 'Click', eventLabel);
            }
        });

        // Track link clicks in top navigation submenu
        jQuery("li.navItem div.sub a").click(function()
        {
            var eventLabel = 'unknown';

            var navItem = jQuery(this).parents('li.navItem');
            var parentTxt = navItem[0].firstChild.text;

            var subCat = jQuery(this).parent().parent().parent().prev();
            var subCatTxt = subCat[0].firstChild.text;

            if(subCatTxt != undefined){parentTxt += ' > '+subCatTxt;}

            var link = jQuery(this).text();
            var linkTitle = jQuery(this).attr('title');

            if(link != undefined && link.length > 0)
            {
                eventLabel = parentTxt+" > "+link;
            }

            else if(linkTitle != undefined && linkTitle.length > 0)
            {
                eventLabel = parentTxt+" > "+linkTitle;
            }

            ga('send','event', 'Sub Navigation', 'Click', eventLabel);
        });

        // Track top links
        jQuery("a.topAd-link").click(function()
        {
            var eventLabel = jQuery(this).text();

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Header Links', 'Click', eventLabel);
            }
        });

        // Track top nav links
        jQuery("div#topNav a").click(function()
        {
            var eventLabel = jQuery(this).text();

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Top Navigation', 'Click', eventLabel);
            }
        });

        // Track header links
        jQuery("div.topDiv a").click(function()
        {
            var eventLabel = jQuery(this).text();
            var eventLabelTitle = jQuery(this).attr('title');

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Top Header Links', 'Click', eventLabel);
            }

            else if(eventLabelTitle != undefined && eventLabelTitle.length > 0)
            {
                ga('send','event', 'Top Header Links', 'Click', eventLabelTitle);
            }
        });

        // Track social link clicks
        jQuery("div.socialLinks a").click(function()
        {
            var eventLabel = jQuery(this).attr('class').split(' ',1).shift();

            if(eventLabel != undefined && eventLabel.length > 0)
            {
                ga('send','event', 'Social Links', 'Click', eventLabel);
            }
        });

        // Track store locator lookups
        jQuery("#store_locator_form").submit(function()
        {
            var zip = jQuery('input#zip').val();
            var state = jQuery('select#state option:selected').val();

            if(state.length==2)
            {
                ga('send','event', 'Store Lookup', 'State Query', state);
            }

            else if(zip.length==5)
            {
                ga('send','event', 'Store Lookup', 'Zip Query', zip);
            }

            else
            {
                ga('send','event', 'Store Lookup', 'Invalid Query', zip);
            }
        });

        // Track store stock lookups
        jQuery("#product_storelookup_form").submit(function()
        {
            var zip = jQuery('input#zipcode').val();
            var sku = jQuery("input[name='sku']").val();

            if(zip.length==5)
            {
                ga('send','event', 'Store Stock Lookup', sku, zip);
            }

            else
            {
                ga('send','event', 'Store Stock Lookup', sku, 'Invalid Query: '+zip);
            }
        });

    });
    </script>

