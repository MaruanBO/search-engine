<script type="text/javascript">
    $J(document).ready(function() {
        /*
         *	The following code is used to display the coupon lightbox if the url contains the relevant 
         *	info to display the lightbox, will work on any page this is included one
         */
        var url = window.location.href;
        var hackedUrl = url.substring(0, url.indexOf("?"));
        var queryString = url.substring(url.indexOf("?")+1);
        var parameters = queryString.split("&");
        var showCoupon = false;
        var loyaltyCoupon = false;
        var loyaltyCouponId = null;
        var couponFile = null;
        var couponFolder = null;
        $J.each(parameters, function() {
            var t = this.split("=");
            switch(t[0].toLowerCase()) {
                case "sc":
                    if(t[1] == "true") {
                        showCoupon = true;
                    }
                    break;
                case "c":
                    if(t[1] == "loyalty") {
                        loyaltyCoupon = true;
                    }
                    break;
                case "coupid":
                    loyaltyCouponId = t[1];
                    break;
                case "file":
                    couponFile = t[1];
                    break;
                case "folder":
                    couponFolder = t[1];
                    break;
            }
        });
        if(showCoupon) {
            var couponUrl = null;

            if(loyaltyCoupon) {
                couponUrl = "http://www.127rewards.com/FCSLtyCouponPrint/Coupon/" + loyaltyCouponId;
            } else {
                couponUrl = "http://media.familychristian.com/" + couponFolder + "/" + couponFile;
            }

            $J("body").append($J("<iframe></iframe>").attr("id", "temp_print").attr("name", "temp_print").css("width", 0).css("height", 0));
            $J("body").append("<div id=\"cpn-lightbox\"></div>");
            $J("body").append("<div id=\"cpn-lightbox-panel\"></div>");
            $J("#cpn-lightbox-panel").empty().append("<img src=\"http://media.familychristian.com/images/ajax-loader.gif\" />").append("<br />Loading your coupon");
            $J("#cpn-lightbox").fadeTo(10, 0.8);
            $J("#cpn-lightbox-panel").fadeIn(10);

            $J("#cpn-lightbox-panel").empty().append("<img id=\"cpn-image\" />");
            /*if(loyaltyCoupon) {
             var width = 940;
             var height = 360;
             $J("#cpn-lightbox-panel").prepend("<a href=\"" + hackedUrl + "\" class=\"closeLightBox closeLightBoxImage\"><img src=\"http://media.familychristian.com/images/icon/close_16.png\" alt=\"Close Window\" /></a>");
             $J("#cpn-lightbox-panel").append("<iframe name=\"lightbox-load-iframe\" frameborder=\"0\" id=\"lightbox-load-iframe\" src=\"" + couponUrl + "\" width=\"" + width + "\" height=\"" + height + "\"></iframe>");
             $J("#cpn-lightbox-panel").append("<a href=\"" + couponUrl + "\" class=\"printLightBox\" style=\"text-decoration:none;\"><img src=\"http://media.familychristian.com/images/icon/printer.png\" alt=\"print coupon\" style=\"width:16px; height:16px; margin-bottom:-4px;\" border=\"0\" />&nbsp;print coupon</a>");
             $J("#cpn-lightbox").fadeTo(10, 0.8);
             $J("#cpn-lightbox-panel").fadeIn(10);
             $J("#cpn-lightbox-panel").animate({ width: (parseInt(width) + 40), height: (parseInt(height) + 40), left: parseInt(((parseInt($J("body").width() / 2) - ((parseInt(width) + 40)/2))) / 1) + 184 }, 400);
             $J(".printLightBox").click(function() {
             //$J("#temp_print").attr("src", couponUrl + "?print=1");
             window.open(couponUrl + "?print=1", "printCoupon", "toolbar=0, scrollbars=0, location=0, status=0, menubar=0, resizable=0, width=800, height=480");
             $J("#cpn-lightbox-panel").focus();
             return false;
             });
             makeCloseLightboxButton();
             } else {*/
            $J("#cpn-image").load(function() {
                $J("#cpn-lightbox-panel").animate({ width: parseInt(this.width) + 40, left: parseInt(($J("body").width() / 2) - (parseInt(this.width) + 40) / 2) + 200 }, 400);
                $J("#cpn-lightbox-panel").prepend("<a href=\"" + hackedUrl + "\" class=\"closeLightBox closeLightBoxImage\"><img src=\"http://media.familychristian.com/images/icon/close_16.png\" alt=\"Close Window\" /></a>");
                $J("#cpn-lightbox-panel").append("<a href=\"" + couponUrl + "\" class=\"printLightBox\" style=\"text-decoration:none;\"><img src=\"http://media.familychristian.com/images/icon/printer.png\" alt=\"print coupon\" style=\"width:16px; height:16px; margin-bottom:-4px;\" border=\"0\" />&nbsp;print coupon</a>");
                $J("body", $J("#temp_print").contents()).append($J("<img src=\"" + couponUrl + "\" />"));
                $J(".printLightBox").click(function() {
                    window.frames["temp_print"].focus();
                    window.frames["temp_print"].print();
                    $J("#cpn-lightbox-panel").focus();
                    /*$J("body").remove("#temp_print");
                     $J("#cpn-lightbox, #cpn-lightbox-panel").css("display", "block").css("position", "static");
                     $J("#cpn-lightbox, #cpn-lightbox-panel").fadeOut(100);
                     $J("#cpn-lightbox, #cpn-lightbox-panel").css("display", "none");*/
                    return false;
                });
                makeCloseLightboxButton();
            });
            $J("#cpn-image").attr("src", couponUrl);
            //}
        }
        /*
         *	This is the end of the lightbox coupon code
         */
        $J(".lightbox-load").click(function() {
            if($J(this).attr("url") != null && $J(this).attr("url") != "") {
                var url = $J(this).attr("url");
                $J("body").append("<div id=\"cpn-lightbox\"></div>");
                $J("body").append("<div id=\"cpn-lightbox-panel\"></div>");
                if($J(this).attr("background") != null) {
                    $J("#cpn-lightbox-panel").css("background", $J(this).attr("background"));
                }
                $J("#cpn-lightbox-panel").empty().append("<a href=\"" + hackedUrl + "\" class=\"closeLightBox closeLightBoxImage\"><img src=\"http://www.familychristian.com/skin/frontend/enterprise/familychristian/images/overlay-close.png\" alt=\"Close Window\" /></a>");
                var height = '360';
                var width = '810';
                if($J(this).attr("lb-height") != null) {
                    height = $J(this).attr("lb-height");
                }
                if($J(this).attr("lb-width") != null) {
                    width = $J(this).attr("lb-width");
                }
                $J("#cpn-lightbox-panel").append("<iframe name=\"lightbox-load-iframe\" frameborder=\"0\" id=\"lightbox-load-iframe\" src=\"" + url + "\" width=\"" + width + "\" height=\"" + height + "\"></iframe>");
                $J("#cpn-lightbox").fadeTo(10, 0.8);
                $J("#cpn-lightbox-panel").fadeIn(10);
                $J("#cpn-lightbox-panel").animate({ width: (parseInt(width) + 40), height: (parseInt(height) + 40), left: parseInt(((parseInt($J("body").width() / 2) - ((parseInt(width) + 40)/2))) / 1) + 184 }, 400);
                makeCloseLightboxButton();
                return false;
            }
        });
        /*
         *	This is the code to show an iframe in the lightbox with a given url
         */

        if($J(".overlay-generic").length) {
            $J("a.overlay-generic[rel]").overlay({
                mask: {
                    color: '#000',
                    loadSpeed: 200,
                    opacity: 0.3
                },
                fixed: false,
                onBeforeLoad: function() {
                    var wrap = this.getOverlay().find(".contentWrap");
                    wrap.empty();
                    wrap.load(this.getTrigger().attr("href"));
                }
            });
        }

    });

    function makeCloseLightboxButton() {

        $J(".closeLightBox").click(function() {
            $J("#cpn-lightbox, #cpn-lightbox-panel").css("display", "block").css("position", "static");
            $J("#cpn-lightbox, #cpn-lightbox-panel").fadeOut(100);
            $J("#cpn-lightbox, #cpn-lightbox-panel").css("display", "none");
            return false;
        });
    }
</script></div>
