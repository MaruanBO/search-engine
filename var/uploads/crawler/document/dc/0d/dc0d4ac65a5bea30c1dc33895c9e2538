<script>
(function($){
	$(function(){
		var ajaxurl = 'http://hotair.com/wp/wp-admin/admin-ajax.php';
		// Deleting
		if ( $.cookie( 'moderate_comments' ) ) {
			$('.comment .action-links').append( '<span class="delete-wrap"> | <a href="#" class="comment-delete-link">Delete</a> </span>' );
			$('.comment .comment-delete-link').click(function(e){
				e.preventDefault();
				var parent = $(this).parents('.comment');
				$.post( ajaxurl,
					{
						cookie: $.cookie( 'wordpress_logged_in_dc0d4ac65a5bea30c1dc33895c9e2538' ),
						action: 'ha_delete_comment',
						comment_id: parent.attr('id').replace( 'comment-', '' )
					}
				);
				parent.fadeOut();
			});
		} else {
			$('.comment .delete-wrap').remove();
		}
		// Banning
		if ( $.cookie( 'promote_users' ) ) {
			$('.comment .action-links').append( '<span class="ban-wrap"> | <a href="#" class="comment-author-ban-link">Delete and Ban</a></span>' );
			$('.comment .comment-author-ban-link').click(function(e){
				e.preventDefault();
				var parent = $(this).parents('.comment');
				if ( confirm( 'Are you sure you want to ban this user?' ) ) {
					$.post( ajaxurl,
						{
							cookie: $.cookie( 'wordpress_logged_in_dc0d4ac65a5bea30c1dc33895c9e2538' ),
							action: 'ha_ban_user',
							comment_id: parent.attr('id').replace( 'comment-', '' ),
							user_id: parent.attr('data-user-id'),
						}
					);
				}
				parent.fadeOut();
			});
		} else {
			$('.comment .ban-wrap').remove();
		}
	});
})(jQuery);
</script>		
