ay:true,timeoutDuration:7100,duration:400,items:1,pauseOnHover:false,onBefore:function(){$(".mantle-feature-title span").removeClass("mantle-timer");},onAfter:function(){$(".mantle-feature-title span").addClass("mantle-timer");}}});$("#mantleFeatureThumbs img").click(function(){var slideId=$(this).data("slide");$('#mantleFeatureSlides').trigger('slideTo',slideId);});});$(window).load(function(){$(".homepage-mantle").css("visibility","visible").css("height","auto").css("margin-bottom","10px");function show_carousel(){var carousel=$("#mantleFeatureSlides");carousel.parent().add(carousel).height(carousel.children().first().height());};window.setTimeout(show_carousel,500);});</script>			</div>

