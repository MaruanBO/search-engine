	<SCRIPT type="text/javascript">
		var frm=document.FF,ttip=document.getElementById('tooltip');
		frm.looking_for.selectedIndex=1;
		var slid=document.getElementById('sliding_form');
		function validate(){
			if (!frm.agree.checked) return chk(frm.agree,'terms');
			return(chk(frm.login,4) && chk(frm.pass,4) && chk(frm.email,'em'));
		}
		function tt_hide(){ ttip.style.display='none';	}
		function chk(obj,rule){
			var msg='',x=140,y=-24,v=1;
			if (rule>0 || rule=='em') v=obj.value;

			if (!v) msg="Required field!";
			else if (rule=='em') {
				if (!v.match(/[a-zA-Z0-9\._]+@[0-9a-zA-Z]+\.[a-zA-Z]{2,3}/)) msg="Email entered wrong";
			} else if (rule=='age'){
				x=5;
				var mon=frm.mon.options[frm.mon.selectedIndex].value;
				var day=frm.day.options[frm.day.selectedIndex].value;
				var year=frm.year.options[frm.year.selectedIndex].value;
				var D=new Date(), mm=D.getMonth()+1, dd=D.getDate(), yy=D.getFullYear();
				if (yy-year==18 && (mm<mon || mm==mon && dd<day)) msg="You have to be 18 or older";
			} else if (rule=='terms') { x=-1; msg="You have to agree to terms"; 
			} else if (v.length<rule) msg="Too short. Minimum "+rule+" letters";
			if (msg){
				var tt_txt=document.getElementById('tooltip_txt');
				do { x+=obj.offsetLeft;	y+=obj.offsetTop; } while (obj = obj.offsetParent);
				y=y-10;
				ttip.style.top=y+'px'; ttip.style.left='250px'; 
				tt_txt.innerHTML=msg;  ttip.style.display='block';
				return false;
			} else tt_hide();
			return true;
		}
		function next(){
			for(i=1;i<10;i++) setTimeout('slid.style.top="-'+(i*33)+'px"',i*30);
			return false;
		}
		function term_pop(a){ w=window.open(a,'TR','width=600,height=600,resizable=1,scrollbars=1'); }
	</SCRIPT>
