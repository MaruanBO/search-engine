		<script type="text/javascript">
		$('form.login-form').submit(function() {
			errorLog = $(this).children('div.errorLog');
			errorLog.addClass('hidden');
			var datas = $(this).serialize();
			$.post('http://fr.publicidees.com/logmein.php', datas, function(ret) {
				var r = JSON.parse(ret);
				if(r.result == 'ok') {
					if(r.type == 'aff') {
						var urlLogin = 'http://publisher.publicideas.com/entree_affilies.php';
						var formLogin = $('<form action="' + urlLogin + '" method="post">' +
							'<input type="hidden" name="login" value="' + r.login + '" />' +
							'<input type="hidden" name="pass" value="' + r.pass + '" />' +
							'<input type="hidden" name="h" value="' + r.h + '" />' +
							'<input type="hidden" name="rLang" value="fr_FR" />' +
							'</form>');
						$('body').append(formLogin);
						$(formLogin).submit();
					} else {
						var urlLogin = 'http://annonceur.publicidees.com/index.php';
						var formLogin = $('<form action="' + urlLogin + '" method="post">' +
							'<input type="hidden" name="login" value="' + r.login + '" />' +
							'<input type="hidden" name="pass" value="' + r.pass + '" />' +
							'<input type="hidden" name="action" value="log" />' +
							'</form>');
						$('body').append(formLogin);
						$(formLogin).submit();
					}
					if($('input#check' + r.type).is(':checked') == true) {
						$.cookie('pilog' + r.type, r.key , {
							 expires: 180,
							 path: '/'
						});
					} else {
						$.removeCookie('pilog' + r.type, { path: '/' });
					}
				} else {
					errorLog.removeClass('hidden');
				}
			});
			return false;
		});
		
		$(function() {
			if($.cookie('pilogaff') != null) {
				$.post('http://fr.publicidees.com/cookieme.php', { key: $.cookie('pilogaff') }, function(ret) {
					var r = JSON.parse(ret);
					if(r.result == 'ok') {
						$('input#loginAff').val(r.login);
						$('input#passAff').val(r.pass);
					}
				})
				$('input#checkaff').attr('checked', true);
			}
			if($.cookie('pilogann') != null) {
				$.post('http://fr.publicidees.com/cookieme.php', { key: $.cookie('pilogann') }, function(ret) {
					var r = JSON.parse(ret);
					if(r.result == 'ok') {
						$('input#loginAnn').val(r.login);
						$('input#passAnn').val(r.pass);
					}
				})
				$('input#checkann').attr('checked', true);
			}
		});
		</script>
