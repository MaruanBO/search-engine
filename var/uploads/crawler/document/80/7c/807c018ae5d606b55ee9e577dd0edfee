			<script>
				function process_mail_response(data){
					if (data.success == true) {
						$('#beta-feedback').html('<div id="feedback-thanks">');
						$('<h2>Your feedback has been received successfully. Thank you for your participation.</h2>').appendTo('#feedback-thanks');
						$('<p>We would be very grateful if you could take an additional 5 minutes to answer a few survey questions.</p>').appendTo('#feedback-thanks');
						$('<div class="center"><button class="fancy hoverable" data-href="http://www.surveymonkey.com/s/SZ76K9K" rel="external"><i class="s"></i><i class="e"></i><em>Take Survey</em></button></div>').appendTo('#feedback-thanks');
						$('<p>We read every survey and response and we use your feedback as the basis for future development of the service. We appreciate your support and loyalty.</p>').appendTo('#feedback-thanks');
						$('<p><em>The GenealogyBank Team</em></p>').appendTo('#feedback-thanks');
						$('<p id="feedback-support">If you have any immediate questions or need immediate help, please contact us at <a href="mailto:support@genealogybank.com">support@genealogybank.com</a> or call 1-866-641-3297 (10am-7pm ET Mon-Fri).</p>').appendTo('#feedback-thanks');
						$('<div class="dismiss ir"><a href="#" onclick="return false;">&#215;</a></div>').appendTo('#feedback-thanks');
						$('#beta-feedback .dismiss a').click(function(evt){
							evt.preventDefault();
							$('#beta-feedback').remove();
						});
					} else {
						$('#beta-feedback').html('<div id="feedback-thanks"><p style="margin-right: 40px !important;">An error occurred in sending your feedback. We\'re sorry for the inconvenience, and will get this fixed as soon as possible. Please try again later.</p></div>');
						$('<div class="dismiss ir"><a href="#" onclick="return false;">&#215;</a></div>').appendTo('#feedback-thanks');
						$('#beta-feedback .dismiss a').click(function(evt){
							evt.preventDefault();
							$('#beta-feedback').remove();
						});
					}				
				}
				
				function process_mail_failure(obj, status, error){
					$('#beta-feedback').html('<div id="feedback-thanks"><p>An error occurred in sending your feedback. We\'re sorry for the inconvenience, and will get this fixed as soon as possible. Please try again later.</p></div>');
					$('<div class="dismiss ir"><a href="#" onclick="return false;">&#215;</a></div>').appendTo('#feedback-thanks');
					$('#beta-feedback .dismiss a').click(function(evt){
						evt.preventDefault();
						$('#beta-feedback').remove();
					});
				}

				function feedback_form_submit(){
				
					if ($('#feedback-feedback').val() != ''){
					
						$('#feedback-submit button').attr('disabled', 'disabled');
						
						$('#beta-feedback input[data-placeholder]').each(function(){
							if ($(this).attr('value') == $(this).attr('data-placeholder')){
								$(this).attr('value', '');
							}
						});
						
						var urls = [];
						$('#beta-feedback .url-field').each(function(){
							if ($(this).attr('value') != ''){
								urls.push($(this).attr('value'));
							}
						});
						if (urls.length > 0){
							urls = '\n' + urls.join('\n');
							$('#feedback-urls').attr('value', urls);
						}
						
						var url = $('#beta-feedback form').attr('action');
						var form_data = $('#beta-feedback form').serialize();

						$.ajax({
							type: 'GET',
							url: url,
							data: form_data,
							success: process_mail_response,
							error: process_mail_failure,
							dataType: 'json'
						});
						
					}else{
					
						alert('Please enter your feedback before submitting.');
						focus_field($('#beta-feedback'));
						setTimeout(function(){
							$('#feedback-submit em').text('Submit feedback');
						}, 1);
						
					}
					
					return false;

				}

				$(document).ready(function(){
				
					var curr_url = window.location.href;

					var username = '';
					if (isValidEmailAddress(username)){
						$('#feedback-email').attr('value', username);
					}

					var useragent = '';
					$('#feedback-useragent').attr('value', useragent);

					$('#feedback-page').attr('value', curr_url);

					$('.add a').click(function(evt){
						evt.preventDefault();
						$('#addURL').before('<input type="text" class="url-field" data-placeholder="Ex. http://www.genealogybank.com/gbnk/" value="Ex. http://www.genealogybank.com/gbnk/">').fadeIn();
					});

					$('#beta-feedback-link > a').click(function(evt){
						evt.preventDefault();
						if ($('#beta-feedback:visible').length == 0){
							$('#beta-feedback').fadeIn();
							$('#feedback-email').focus();
						}else{
							$('#beta-feedback').fadeOut();
						}
					});
					
					$('#beta-feedback .dismiss a').click(function(evt){
						evt.preventDefault();
						$('#beta-feedback').fadeOut();
					});
					
				});	

			</script>

		
		
		
			
