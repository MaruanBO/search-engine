			(function() {
				var enabled = true, popupClass = 'site-abandon-popup', GBNK = $.cookie('GBNK'), triggerInY;
			
				if (!GBNK) {
					$(document).ready(function() {

						$('body').append('<div id="abandon-trigger"></div>');

						$('#abandon-trigger').css({
							position: "absolute", top: "0", height: "35px", width: "200px"
						});

						//Position abandonment trigger depending on OS
						if( navigator.appVersion.indexOf("Mac") != -1 ) $('#abandon-trigger').css({ right: "auto", left: "0" });
						else $('#abandon-trigger').css({ left: "auto", right: "0" });

						size_abandon_trigger();
	
						$('#abandon-trigger').hover(
							function(e) { triggerInY = e.pageY; },
							function(e) { if (e.pageY < triggerInY && $('#DOMWindow').length==0 /* Don't load if another lightbox is open */ ) doLightBox(); }
						);
			
						$('#close-abandon-popup').click(function() {
							$(this).parent().parent().closeDOMWindow();
							return false;
						});
					});
					
					$(window).resize(function(){
						size_abandon_trigger();
					});
					
					var container_right_edge, viewport_width, abandon_trigger_width;
					function size_abandon_trigger(){
						container_right_edge = $('#container').offset().left + $('#container').width();
						viewport_width = $(window).width();
						abandon_trigger_width = viewport_width - container_right_edge - 20;
						if (abandon_trigger_width < 0){
							abandon_trigger_width = 0;
						}
						$('#abandon-trigger').css('width', abandon_trigger_width);
					}
			
					function doLightBox() {
						if (!enabled) return false;
			
						var SP_LIST_ID = '1682864';
						var COOKIE_NAME = 'spreg_' + SP_LIST_ID, COOKIE_OPTIONS = { path: '/' };
						var THANK_YOU_MSG_DISPLAY_TIME = 3000;
			
						var DEFAULT_DOM_WINDOW_OPTIONS = {
							borderColor: '#E1E5B3',
							width: 480,
					        loader: 0,
							modal: 0,
							overlayColor: '#333',
							overlayOpacity: 70
						};
			
						var VARIATIONS = [
							{
								vId: '#site-abandon-offer-1',
								domWindowOptions: DEFAULT_DOM_WINDOW_OPTIONS
							}
						];
			
						var eventSet = false;
						var curVar = getRandomVariation(VARIATIONS);
						var curVarId = curVar.vId;
						var curVarDomWindowOptions = curVar.domWindowOptions;
						var formId = curVarId + '-form';
			
						curVarDomWindowOptions.windowSourceID = curVarId;
			
						function processData(data) {
							if (data.success == true) {
								enabled = false;
								$.cookie(COOKIE_NAME, "1", {path: "/", expires: 730});
								$('#DOMWindow').html(
									'<h1>Thank You!</h1><p>You are now registered to receive ' +
									'a newsletter and special offers from GenealogyBank.</p>'
								);
								setTimeout('$.closeDOMWindow();', THANK_YOU_MSG_DISPLAY_TIME);
							} else {
								alert('An error occurred while submitting your request. Please try again.');
							}
						}
			
						function validateField(fieldValue, fn, onSuccess, onFail) {
							if (fn(fieldValue)) {
								return onSuccess();
							} else {
								return onFail();
							}
						}
			
						if (!eventSet) {
							$(formId).submit(function(){
								var pass = true, errMsg = 'Please correct the following errors and try again:\n\n';
								var email = $(formId + ' input[name="EMAIL"]').val();
								var firstName = $(formId + ' input[name="FIRSTNAME"]').val();
								var lastName = $(formId + ' input[name="LASTNAME"]').val();
								var country = $(formId + ' select.country-selector-dropdown').val();
			
								validateField(email, isValidEmailAddress, function () {
									return;
								}, function () {
									pass = false;
									errMsg += '  - Enter a valid email address\n';
								});
			
								validateField(firstName, isNotEmpty, function () {
									return;
								}, function () {
									pass = false;
									errMsg += '  - Enter your first name\n';
								});
			
								validateField(lastName, isNotEmpty, function () {
									return;
								}, function () {
									pass = false;
									errMsg += '  - Enter your last name\n';
								});

								validateField(country, isNotEmpty, function () {
									return;
								}, function () {
									pass = false;
									errMsg += '  - Enter your country\n';
								});

								if (pass) {
									var url = $(formId).attr('action');
									var formData = $(formId).serialize()
			
									$(formId + ' .submit').html(
										'Processing <img id="site-abandon-offer-1-form-spinner" src="http://images.newsbank.com/infoweb/genealogybank/ajax-loader.gif" />'
									);
			
									$.ajax({
										type: 'GET',
										url: url,
										data: formData,
										success: processData,
										dataType: 'json'
									});
			
									return false;
								} else {
									alert(errMsg + ' ');
								}
			
								return pass;
							});
							eventSet = true;
						}
			
						var cookieValue = $.cookie(COOKIE_NAME);
			
						if (!(stringContains(cookieValue, "1"))) {
							$.cookie(COOKIE_NAME, "1", COOKIE_OPTIONS);
							$(curVarId).openDOMWindow(curVarDomWindowOptions);
							$('#DOMWindow').css({'overflow': 'hidden'});
							$('#DOMWindow').className = "abandonDOMWindow";
							$(formId + ' input[name="EMAIL"]').focus();
						}
					}
				}
			})();
		</script>
		
	


			
			
			
			
			
				
