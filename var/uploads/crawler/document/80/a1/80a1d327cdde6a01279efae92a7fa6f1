                        var gsTerm = "I'm looking for...";
                        $.getScript("https://www.google.com/jsapi", function (data, textStatus, jqxhr) {
                            google.load('search', '1', { "callback": gcseCallBack });
                            function gcseCallBack() {
                                google.search.CustomSearchControl.attachAutoCompletion('013796009449807619047:i9brh-ikdma', document.getElementById('google-search-input'), 'cse-search-box');
                                var gcsSubmit = function () {
                                    $('#cse-search-box').submit();
                                }
                                $('.feature-search-icon').live('click', function () { gcsSubmit(); });
                            }
                        });
                        $('#cse-search-box').submit(function (event) {
                            var sVal = $("#google-search-input").val().replace(/^[ ]+||[ ]+$/gim, "");
                            if (sVal == "" || sVal == gsTerm) {
                                alert("Please enter a search term");
                                $("#google-search-input").val("");
                                $("#google-search-input").focus();
                                event.preventDefault();
                            }
                        });
