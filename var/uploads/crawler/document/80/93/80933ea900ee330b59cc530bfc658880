    $(function() {
        $('.clickable.portal').mouseover(function() {
            $(this).stop();
            $(this).animate({paddingBottom: '20px'}, 500);
        }).mouseout(function() {
            $(this).stop();
            $(this).animate({paddingBottom: '10px'}, 500);
        });
        $('.prime-actions li').mouseover(function() {
            $(this).stop();
            $(this).animate({backgroundColor: '#E9FAF5'}, 500);
        }).mouseout(function() {
            $(this).stop();
            $(this).animate({backgroundColor: '#fff'}, 500);
        });

        $('#myisi-ad').mouseover(function() {
            $(this).stop();
            $(this).animate({backgroundColor: "#68BFEF"}, 500);
        }).mouseout(function() {
            $(this).stop();
            $(this).animate({backgroundColor: "#fff"}, 500);
        });
    });
