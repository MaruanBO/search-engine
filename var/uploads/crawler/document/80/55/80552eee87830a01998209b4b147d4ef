<script type="text/javascript">
    function swok(event){
		 if(event.which || event.keyCode){
			 if ((event.which == 13) || (event.keyCode == 13)) {
				 	return sp();
				 }
			 }
		 return true;
	 }
   function sp(){
		var sw = $("#sw").val();
		if(null==sw){
			sw = "";
		}else{
			sw=sw.replace(/^\s+/g,"").replace(/\s+$/g,"");
		}
		if(!sw){alert("Please Enter Keyword!");return false;}
		window.location.href="/Search.qs?k="+encodeURI(sw);
    return false;
	}
</script>

