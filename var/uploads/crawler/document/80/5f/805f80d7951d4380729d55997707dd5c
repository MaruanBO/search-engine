								jQuery(document).ready(function() {
									if(jQuery.cookie('newsletter_popup') == 'true' ){
										jQuery('.newsletter-popup').hide();
										jQuery('.newsletter-bg').hide();
									} else {
										jQuery('html .newsletter-bg').show();
										jQuery('html .newsletter-popup').show();
										jQuery('.mandatory .invisible').remove();
										jQuery('.newsletter-popup label').hide();
										jQuery('.newsletter-popup .submit').addClass('disabled');
										jQuery('.newsletter-popup input.email').attr('placeholder', jQuery('.newsletter-popup input.email').prev().text()); 
										
										jQuery('.newsletter-popup input.email').keypress(function() {
											jQuery('.newsletter-popup .submit').addClass('active').removeClass('disabled');
										});
										
										jQuery('.newsletter-bg').click(function(){
											jQuery('.newsletter-bg').hide();
											jQuery('.newsletter-popup').hide();
											jQuery.cookie('newsletter_popup','true',{expires: 999});
										}); 
										
										jQuery('.newsletter-popup .close').click(function(){
											jQuery('.newsletter-popup').hide();
											jQuery('.newsletter-bg').hide();
											jQuery.cookie('newsletter_popup','true',{expires: 999});
										}); 
										
										 jQuery('.newsletter-popup .submit').click(function() {
											if (jQuery(this).hasClass('disabled')) {
												return false;
											} else {
												jQuery.cookie('newsletter_popup','true',{expires: 999});
												jQuery('.newsletter-popup .submit').removeClass('active').addClass('disabled');
											}
										});
									} // end newsletter popup
									
									if(jQuery.cookie('wc_popup') == 'true' ){
										jQuery('.wc.popup').hide();
										jQuery('.popup-bg.wc').hide();
									} else {
										jQuery('html .popup-bg.wc').show();
										jQuery('html .wc.popup').show();
										
										jQuery('html .popup-bg.wc').click(function(){
											jQuery('html .popup-bg.wc').hide();
											jQuery('html .wc.popup').hide();
										}); 
										
										jQuery('html .wc.popup .close').click(function(){
											jQuery('html .wc.popup').hide();
											jQuery('html .popup-bg.wc').hide();
											jQuery.cookie('wc_popup','true',{expires: 999});
										}); 
										 
									} // end newsletter popup
									
								});
							</script>
			 	


