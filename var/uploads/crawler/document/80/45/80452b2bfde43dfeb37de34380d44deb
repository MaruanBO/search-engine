	<script language="javascript">
	function SetCookie(name, value, expiredays)
	{
		var expirationDate = new Date();
		expirationDate.setDate(expirationDate.getDate() + expiredays);
		document.cookie = name + "=" + escape(value) + ((expiredays == null) ? "" : "; expires=" + expirationDate.toGMTString()) + ";path=/;";
	}
	function closeTakeOver()
	{
		document.getElementById('TakeOver_content').style.top = '-500px';
		setTimeout(function(){document.getElementById('TakeOver_content').style.display = 'none'}, 2000 );
	}
	
	function ShowTakeOver()
	{
			if (GetCookie(ANY_TakeOverCookieName) == "")
			{
				if(GetCookie(CURRENT_TakeOverCookieName) == "")
					WebFontConfig = {
						google: { families: [ 'Source+Sans+Pro:400,600:latin' ] }
					  };
					  (function() {
						var wf = document.createElement('script');
						wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
						  '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
						wf.type = 'text/javascript';
						wf.async = 'true';
						var s = document.getElementsByTagName('script')[0];
						s.parentNode.insertBefore(wf, s);
					  })();
					document.getElementById('TakeOver_content').style.display = '';
					setTimeout(function(){document.getElementById('TakeOver_content').style.top = '12%'}, 2000 );
					SetCookie(CURRENT_TakeOverCookieName, 'true', CURRENT_TakeOverCookieDays);
					SetCookie(ANY_TakeOverCookieName, 'true', ANY_TakeOverCookieDays);
					_gaq.push(['_setCustomVar', 5, 'Homepage_TakeOver', 'Multi-Newsletter', 2]);
			}
	}
	if (!window.attachEvent && window.addEventListener && document.documentElement.clientWidth>1024) {
	ShowTakeOver();
	}
	</script>
