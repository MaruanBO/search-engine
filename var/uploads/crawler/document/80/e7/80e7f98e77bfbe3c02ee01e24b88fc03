            <script>

  window.addEventListener('load', function() { 
      document.getElementById('foxtrot-carousel').removeAttribute('style');
      foxtrotCarousel = new Swiper('.swiper-container',{
        loop:true,
        autoplay: 5000,
        speed: 600,
        resizeReInit: true,
        calculateHeight: true,
        noSwipingClass: 'swiper-wrapper',
        noSwiping: true,
        onSlideChangeEnd: function(swiper){
            pages = document.getElementsByClassName('carousel-page');
            Array.prototype.forEach.call(pages, function(el) {
                el.style.borderWidth='1px';
            });
            document.getElementById('carousel-page-'+swiper.activeLoopIndex).style.borderWidth='2px';
        }
      })
  }, false);

 </script>
    
