			if(!!bt && !!bt.levelIds && bt.levelIds.indexOf('_') > -1) {
				var levelsAry = bt.levelIds.split('_');
				var cat_id = levelsAry[levelsAry.length-1];
				br_data.cat_id = cat_id;
			}
			//Bloomreach changes Category Page - ECOM-312887 ends
		}  catch(err) {}
	</script>		
	
		<script type="text/javascript">		
		try{		
br_data.search_term = '';
br_data.session_id = '';
		 	
br_data.is_conversion = "0";
					br_data.user_id = ""; 
					br_data.basket_value = ""; 
				
			var file = ("https:" == document.location.protocol) ? "https://cdns.brsrvr.com/v1/br-trk-4077.js" : "http://cdn.brcdn.com/v1/br-trk-4077.js";		
document.write(unescape("%3Cscript src='" + file + "' type='text/javascript'%3E%3C/script%3E"));
		}  catch(err) {}	
		  </script>
	
			<script type="text/javascript">
try {
			
		 	 var tracker = BrTrk.getTracker(0.2, br_data);
  tracker.enableTracking();
}  catch(err) {}
</script>

