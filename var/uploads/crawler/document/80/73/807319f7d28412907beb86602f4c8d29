   /(dev\.ecom\.)/.test(location.hostname) ? 'https://web301p.dev.ch3.s.com:5743' :
   'https://sso.shld.net:443',
  isOutlet: /SHC\-HTS|SHC\-HAS|SHC\-AHS|SHC\-OUT/.test(navigator.userAgent),
  feedbackProvider: 'QUAL', // used by O_LC_Utility in shcGlobalTracking.js
  deptNav: {
    showNav: $.q('body').attr('id') === 'home',
    forceOpen: $.q('body').attr('id') === 'home'
  }
});
shc.asapAkamaiBasePath = shc.asapAkamaiBasePath || '';
//need to have path prefixed for dynamic file adds in IE8
shc.imagePath = (shc.imagePath.indexOf('http') >=0 ? '' : location.protocol) + shc.imagePath;

shc.URLs = $.extend(shc.URLs, {
    chronometer: shc.env === 'QA' ? '//navappvip.qa.ch3.s.com/' : shc.env === 'STAGE' ? '//navappvip.stress.ch3.s.com/' : '//nav.shld.net/',
    chronData: 'segments/',
    chronAssets: 'assets/',
