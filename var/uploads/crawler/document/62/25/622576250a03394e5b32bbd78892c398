	<script type="text/javascript">
		//<![CDATA[
		$(document).ready(function() {
	$(".focus").focus();
});
$(function() {
	$('#footer-news').hide();
	$('.arvest-news').removeClass('hidden');
	$('.hidenews').hide();
	$('.shownews').click(function() {
		$('#footer-news').slideToggle();
		$('.shownews').hide();
		$('.hidenews').show();
	});
	$('.hidenews').click(function() {
		$('#footer-news').slideToggle();
		$('.shownews').show();
		$('.hidenews').hide();
	});
	$(window).resize(function() {
		windowWidth = $(this).width();
		if ( windowWidth > 960) {
			$('#footer-news').hide();
			$('.shownews').show();
			$('.hidenews').hide();
		} else {

		}
	});
	$('.arvest-slider').cycle({
		slides: 'div',
		fx: 'scrollHorz',
		timeout: 0,
		pager: '.icon-button-row',
		pagerTemplate: ''
	});

	$('.tagline-overlay').cycle({
		slides: 'div',
		fx: 'scrollHorz',
		timeout: 0,
		pager: '.icon-button-row',
		pagerTemplate: ''
	});

	/**
	 * home page login swap
	 */
	$('.home-page-login .dropdown-menu a').on('click', function(e){
		if ( $(this).closest('.user-menu').length ) {
			console.warn('returning');
			return;
		}
		e.preventDefault();
		$('.home-page-login .dropdown-menu').fadeOut('fast');
		var $drop = $('.left-box .login-btn.dropdown-toggle'),
			title = $(this).html(),
			icon = $drop.find('i')[0].outerHTML;
			imgTag = $drop.find('img')[0].outerHTML;
		$drop.html(icon+' '+title+' '+imgTag);
		loginSwap($(this).prop('name'), {
			src: $('.nav-login .login-forms'),
			dst: $('.home-page-login .login-fields')
		});
	});

});

// Swipe functions
$('.home-container').touchwipe({
	wipeLeft: function() {
		$('.arvest-slider').cycle("next");
		$('.tagline-overlay').cycle("next");
	},
	wipeRight: function() {
		$('.arvest-slider').cycle("prev");
		$('.tagline-overlay').cycle("prev");
	},
	preventDefaultEvents: false
});
		//]]>
	</script>

