<!-- Function for Targeted Banner Details Popups -->
<script type="text/javascript">
	$(function(){
		function getSearchResults(query){
			query = query.replace(/(^\s*)|(\s*$)/g, ''); //trim
			query = query.replace("/", "_");
			query = query.replace('"', "Inches");
			query = query.replace(' ', '+');
			query = query.replace('&', 'AND');
			query = query.replace(':', ';');
			query = query.replace('-', '~');
			return searchURL = '/en-US/_/_/_/_/Srch0+'+query+'/Products.aspx';
		};
		
		$(".showDetails").live("click",function(){
			var openThis = $(this).attr('href');
			
			if (openThis.indexOf('#') > 0) {
				openThis = openThis.substring(openThis.indexOf('#'));
			}
			$(openThis).slideToggle();
				return false;
		});

		$(".close").live("click",function(){
			$('#swBannerPopup, #tarBannerPopup').slideToggle();
			return false;
		});
		
			
		
		$('#topNavSearchSubmit').click(function (){
			var query = $('#Ntt').val();
			if(query != '') {
					window.location = getSearchResults(query);
			}
			return false;
		});

		$('#Ntt').keypress(function (e){
			if(e.keyCode == 13) {
				var query = $('#Ntt').val();
				if(query != '') {
					window.location = getSearchResults(query);
				}
				return false;
				
			}
		});
	});
</script>

<script type="text/javascript">
	var CartQuantity = function () {
		var cookies = new CookieManager();
		var qty = cookies.getValue("CART_TOTAL_QUANTITY_90051", null);
		var link = document.getElementById("cartLink");
		if(link && qty && (qty > 0)){
		 $('#cartLink').addClass('notEmpty');
		link.innerHTML = qty;
	}
	}
	$(document).ready(CartQuantity);
</script><!-- Shop:90051_Template_TopMargin [Main] END --></div>
		
		
