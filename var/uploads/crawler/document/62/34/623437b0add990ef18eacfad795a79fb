<script type="text/javascript">
/* youtube video script */
$(function(){
  if(!$('body').hasClass('mobile')){

    // Video Modal
    var video = '';
    
    function closeModal(){
      
      $('.modalcover').animate({'width': 0});
      $('.footervideo').animate({'left': "150%"}, function(){
        video.remove();
      });
    }
    function openModal(){
      
      $('.modalcover').show().animate({'width': '100%'});
      $('.footervideo').animate({'left': "50%"}, function() {
        $('.footervideo').append(video);
      });
    }

    $('.videomodal').click(function(){
      video = $('<div class="videoframe"><iframe id="iframevideo" width="853" height="480" src="'+$(this).attr('href')+'" ></iframe></div>');
      openModal();
      return false;
    });
    $('.modalcover, .modalclose').click(function(){
      closeModal();
      return false;
    });
  }
});
