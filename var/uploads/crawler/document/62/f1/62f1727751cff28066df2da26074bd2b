            <script type="text/javascript">
                $(function () {
                    var isDesignMode = 0;

                    if (isDesignMode > 0) {
                        return;
                    }

                    window.scrollTo(0, 1);

                    $('.js #menu-toggle').click(function (e) {
                        $('body').toggleClass('active');
                        e.preventDefault();
                    });


                    var currNode = {'title':'CoastHills Credit Union - Another great day.','url':'/home'};

                    convertNavToDropDown($('.chNavWrap ul li a'), $('.nav-wrap .chNavSelectWrap'));

                    var $navScroll = $('.nav-wrap .nav-scroll');
                    var $navScrollLinks = $navScroll.find('ul li a');

                    if ($navScrollLinks.length) {

                        var jcarousel = $navScroll.find('.jcarousel').jcarousel({
                            list: 'ul'
                        });

                        $navScroll.find('.prev-nav a')
                            .on('jcarouselcontrol:active', function() {
                                $(this).removeClass('inactive');
                            })
                            .on('jcarouselcontrol:inactive', function() {
                                $(this).addClass('inactive');
                            })
                            .jcarouselControl({
                                target: '-=1'
                            });

                        $navScroll.find('.next-nav a')
                            .on('jcarouselcontrol:active', function() {
                                $(this).removeClass('inactive');
                            })
                            .on('jcarouselcontrol:inactive', function() {
                                $(this).addClass('inactive');
                            })
                            .jcarouselControl({
                                target: '+=1'
                            });

                        convertNavToDropDown($navScrollLinks, $navScroll);
                    } else {
                        $navScroll.hide();
                    }

		    
                    $('a[href^=http]').not("[href*=" + document.domain + "]").click(function () {
                        return confirm("You are about to visit a website over which we have no control. We do not warrant or guarantee that the information on the external website you are about to visit will be accurate or up to date or subject to our privacy policy.");
                    });

                    function convertNavToDropDown($links, $container) {
                        var $sel = $('<select>').appendTo($container);

                        $links.each(function () {
                            var $a = $(this);
                            $sel.append($("<option />", { value: $a.attr('href'), text: $a.text() }));
                        });

                        if (currNode) {
                            var $option = $('option', $sel).filter(function() {
                                return ($(this).val() == currNode.url);
                            });

                            if ($option.length) {
                                $option.prop('selected', true);
                            } else {
                                $sel.prepend($("<option />", { value: currNode.url, text: currNode.title }));
                            }
                        }
                        
                        $sel.on('change', onDropdownNavChange);
                    }

                    function onDropdownNavChange(e) {
                        var val = $(this).find("option:selected").val();
                        window.location.replace(val);
                    }

                });
            </script>
        
    

