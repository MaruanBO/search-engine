	<script type="text/javascript">$(document).ready(function(){$('.nav-refresh .active').css('background-color','#000000');$('.nav-refresh').hoverIntent({over:actionSportsDrop,out:actionSportsHide,timeout:500,selector:'#action-sports_nav'});$('.nav-refresh').hoverIntent({over:outdoorDrop,out:outdoorHide,timeout:500,selector:'#outdoor_nav'});$('.nav-refresh').hoverIntent({over:lifestyleDrop,out:lifestyleHide,timeout:500,selector:'#lifestyle_nav'});$('.nav-refresh').hoverIntent({over:activeStateShown,out:activeStateHide,timeout:500});$('.fa-search').hoverIntent({over:searchShown,out:searchHide,timeout:500});$('.social-drop').hoverIntent({over:socialShown,out:socialHide,timeout:500});$('.social-search').hoverIntent({over:searchSocialActive,out:searchSocialInctive,timeout:500});function actionSportsDrop(){$('.nav-item > a').not(this).css('background-color','transparent');$('.nav-refresh .active').css('background-color','transparent');$(this).find('#action-sports_link').addClass('hover-state');$('#outdoor_drop').hide();$('#lifestyle_drop').hide();$(this).find('#action-sports_drop').show();}
function actionSportsHide(){$('.nav-item > a').css('background-color','transparent');$(this).find('#action-sports_link').removeClass('hover-state');$(this).find('#action-sports_drop').hide();}
function outdoorDrop(){$('.nav-item > a').not(this).css('background-color','transparent');$('.nav-refresh .active').css('background-color','transparent');$(this).find('#outdoor_link').addClass('hover-state');$('#action-sports_drop').hide();$('#lifestyle_drop').hide();$(this).find('#outdoor_drop').show();}
function outdoorHide(){$('.nav-item > a').css('background-color','transparent');$(this).find('#outdoor_link').removeClass('hover-state');$(this).find('#outdoor_drop').hide();}
function lifestyleDrop(){$('.nav-item > a').not(this).css('background-color','transparent');$('.nav-refresh .active').css('background-color','transparent');$(this).find('#lifestyle_link').addClass('hover-state');$('#action-sports_drop').hide();$('#outdoor_drop').hide();$(this).find('#lifestyle_drop').show();}
function lifestyleHide(){$('.nav-item > a').css('background-color','transparent');$(this).find('#lifestyle_link').removeClass('hover-state');$(this).find('#lifestyle_drop').hide();}
function searchShown(){$('.social-accounts').hide();$('.social-drop').removeClass('social-state');$(this).addClass('search-state');$('.search-drop').prependTo('.social-search');$('.search-drop').show();}
function searchHide(){}
function socialShown(){$('.search-drop').hide();$('.fa-search').removeClass('search-state');$(this).addClass('social-state');$('.social-accounts').prependTo('.social-search');$('.social-accounts').show();}
function socialHide(){}
function activeStateShown(){$('.search-drop').hide();$('.social-accounts').hide();$('.fa-search').removeClass('search-state');$('.social-drop').removeClass('social-state');}
function activeStateHide(){$('.nav-refresh .active').css('background-color','#000000');}
function searchSocialActive(){$('.nav-refresh .active').css('background-color','transparent');}
function searchSocialInctive(){$('.search-drop').hide();$('.social-accounts').hide();$('.fa-search').removeClass('search-state');$('.social-drop').removeClass('social-state');$('.nav-refresh .active').css('background-color','#000000');}
$('.responsive-menu-btn').click(function(){if($(this).hasClass('responsive-shown')){$('.responsive-menu-refresh').slideUp();$(this).removeClass('responsive-shown');}
else{$(this).addClass('responsive-shown');$('.responsive-menu-refresh').slideDown();}});});$(window).load(function(){if($('.billboard div, .billboard iframe').length>0){$(".billboard").css("height","250px");}
var stickyNavTop=$("#siteHeaderRefresh").offset().top;var stickyNav=function(){var scrollTop=$(window).scrollTop();if(scrollTop>=stickyNavTop){$("#siteHeaderRefresh").addClass("sticky");$("#PageContainer").css("padding-top","50px");}else{$("#siteHeaderRefresh").removeClass("sticky");$("#PageContainer").css("padding-top","0px");}};stickyNav();$(window).scroll(function(){stickyNav();});$(window).on('resize',function(){$(".billboard").css("height","auto");}).trigger('resize');});</script>
	

	
