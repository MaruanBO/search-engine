<script type="text/javascript">
function clickInfoFunction(destinationURL)  { 
						var parameters = {};
						  parameters.storeId = this.storeId;
						  parameters.langId=this.langId;
						  parameters.catalogId=this.catalogId;
						  parameters. evtype= "CpgnClick";	
						  parameters. mpe_id= document.getElementById("mpe_id").value;
						
						dojo.xhrPost({
						    url: "ClickInfo",				
						    handleAs: "json-comment-filtered",
						    content: parameters,
						    service: this,
						    load:  ""
						  });
		window.location= destinationURL
}
</script>


