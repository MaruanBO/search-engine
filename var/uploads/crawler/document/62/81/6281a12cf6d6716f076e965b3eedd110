//var localsearchdata = [];
//localsearchdata.push.apply(agencies, onlineservices)

$(document).ready(function() {

  console.log("executing typeahead");

  var searchbox = $("input#searchBox");
// console.log("count of matches: " + searchbox.size() );

  searchbox.typeahead( [{
    name: 'Online Services',
    local: onlineservices,
    engine: Hogan,
    template: "<p>{{value}}</p>"
  },{
    name: 'Agencies',
    local: agencies,
    engine: Hogan,
    template: "<p>{{value}}</p>"
  },{
    name: 'Cities',
    local: cities,
    engine: Hogan,
    template: "<p>{{value}}</p>"
  },{
    name: 'Counties',
    local:counties,
    engine: Hogan,
    template: "<p>{{value}}</p>"
  },{
    name: 'Mountains',
    local: mountains,
    engine: Hogan,
    template: "<p>{{value}}, height: {{height}}</P>"
  },{
    name: 'State Parks',
    local: [{"value":"Bear Lake State Park","tokens":["Bear","Lake","State","Park","Campground","Vacation"]},{"value":"Lake Cascade","tokens":["Campground", "Vacation", "Lake", "Cascade", "State", "Park"]}],
    engine: Hogan,
    template: "<p>{{value}}</P>"


  }
    ]  ).on('typeahead:selected'
            , function($e, datum) {
              $("input#searchBox").val( datum.value );
              $("form#resultsSearch").submit();
              $("form#homeSearch").submit();
              
              } );

});
    </script>



