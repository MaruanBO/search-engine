      });

      showTabCurrentProgram("#pane1_channels_telecinco_hours_mod_201503997640166");


      // Busca el item que corresponde mostrar y hqace scroll hasta el
      function showTabCurrentProgram(tabClass) {

      // Muestra siempre la hora actual
      
      
      var curDate = 1424381761;
      

      var actualItem = "";
      jQuery(tabClass + " .item").each(function () {
        if (jQuery(this).find(".startTimestamp").html() <= curDate) {
          return true;
        }
	actualItem = BBT.getProperty(jQuery(this), "item") - 1;
	return false;
      });

      if (actualItem != "") {
        var scrobox = jQuery(".scrolling", tabClass);
        TL5.scrollMoveTo(scrobox, 'li.item', actualItem);
      }
  }
    -->
</script>



