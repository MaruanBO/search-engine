    jQuery("#topmost_mod_201480403831082").toolsTabs("#panes_mod_201480403831082 .pane", {
	tabs: 'li.tab',
	current: 'active',
	effect: 'default',
	onClick: function(e, index) {
	    // Repintamos la pestaña para que el iframe de Facebook se entere de que está siendo mostrado y calcule correctamente las dimensiones
	    if (index == 2) {
		var api = e.currentTarget;
		var iframe = api.getCurrentPane().find("iframe")[0];
		iframe.src = iframe.src;
	    }
	}
    });
