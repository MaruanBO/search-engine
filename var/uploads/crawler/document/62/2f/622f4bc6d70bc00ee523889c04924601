         var tab = jQuery(this).attr('rel').match(/tab_([0-9]+)/);

         tabIndex = parseInt(tab[1], 10);

         var url = "/video-tools-modal.json";
	 var contentId = BBT.getProperty(jQuery(this).parents(".shareToolbar"), "contentId");
         var params = { contentId: contentId, tabIndex: tabIndex};

         jQuery.post(url, params, function(_response) {
         var response = BBT.fixJson(_response);

         if(response["error"] == "0") {
            var html = response["data"];

           // fancybox setup
           jQuery.fancybox({
              // width: 530,
              centerOnScroll: true,
              scrolling: 'no',
              titleShow: false,
              transitionIn : 'none',
              transitionOut : 'none',
              content : html,
              onClosed: function() {
                // callback fn
              }
           });

         } else {
            BBT.log(response["message"]);
         }
       });
      return false;
      });

      });
      //-->
</script>


  </div><!-- .media-holder -->

