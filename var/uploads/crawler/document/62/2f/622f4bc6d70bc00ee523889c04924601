<script>
    jQuery(window).load(function() {
        var dataTiming = window.performance.timing;
        
        // En teoría las propiedades del objeto 'performance' no pertenecen al objeto en sí sino a su prototipo, 
        // pero no funciona así en Chrome, donde las popiedades pertenecen al objeto en sí.
        // Esta función nos devolverá las propiedades del objeto 'performance' independientemente de la implementación utilizada.
        var getPerfObjKeys = function (obj) {
            var keys = Object.keys(obj);
            return keys.length ? keys : Object.keys(Object.getPrototypeOf(obj));
        };
        
        // Utilizando 'setTimeout' nos aseguramos de que todos los eventos implicados, 
        // incluido el evento load, han finalizado.
        window.setTimeout(function() {
            
            var srcStr = 'http://stats.mediaset.es/ptime.gif',                 
                navStart = dataTiming.navigationStart || 0,
                ptKeys = getPerfObjKeys(dataTiming), 
                evtTime = 0;
        
            for (var x=0, len = ptKeys.length; x < len; x++){
                evtTime = dataTiming[ptKeys[x]] - navStart;
                srcStr += (x > 0 ? '&' : '?') + ptKeys[x] + '=' + (evtTime > 0 ? evtTime : 0);
            }
        
            var s = document.createElement("script");
            s.async = true;
            document.body.appendChild(s);
            s.src = srcStr;
            
        }, 0);
        
    });
</script>

<script type="text/javascript">
	var rand= "";
	while(rand.length < 9) {
		rand += "" + parseInt(Math.random(0) * 9);
	}

	var i = new Image(1, 1);
	i.src = "/bbtstats/pixel.gif?category=30&entityId=4000001" + "&rand=" + rand; 
</script>
