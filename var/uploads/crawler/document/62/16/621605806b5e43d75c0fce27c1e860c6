    <script type="text/javascript">

        function DisplayDiscountPlanPopup() {
            $('#SearchDiscountWidget').jqmShow();
            $('#SearchDiscountWidget').show();
        }

        function searchDiscountPlans() {
            $("#replaceWithSearchType").replaceWith("Plans ");
            $("#replaceWithZipCode").replaceWith($("#txtZipCodeForDiscount").val());
            LoadVirtualGoogleEventTracking('Button', 'Plan Selection');

            $.ajax({
                url: '/home/iszipcodevalid',
                type: "GET",
                data: { zipCode: $("#txtZipCodeForDiscount").val() },
                success: function (returnValue) {
                    if (returnValue == "false") {
                        $('#DiscountPlanError').text("ZIP Code not valid");
                    } else {

                        LoadVirtualGoogleEventTracking('Button', 'Search Submit');
                        $('.loadingImage').show();
                        $('#lnkSearchDiscountPlan').hide();

                        var zipCode = $("#txtZipCodeForDiscount").val();
                        var planType = $('input:radio[name=discountPlanType]:checked').val();
                        var url = "/dentalsearch/dentalplansearch?zipcode=" + zipCode + "&planType=" + planType + "&f=p";
                        $(location).attr('href', url);
                    }
                }
            });
        }


        $(document).ready(function () {

            $("#SearchDiscountWidget").jqm({ modal: false, refreshParent: false });  
            $('.loadingImage').hide();

            $("#txtZipCodeForDiscount").mask("99999");
            $("#txtZipCodeForDiscount").watermark("Enter Your ZIP Code");
            $("#txtZipCodeForDiscount").val('');

            $("#tblFindByZip").show();
            $("#findaplan").css("background", "#1a5c98");

            $('#lnkSearchDiscountPlan').click(function () {
                searchDiscountPlans();
            });

            $("#txtZipCodeForDiscount").keypress(function (e) {
                if (e.which == 13) {
                    searchDiscountPlans();
                }
            });


        });

    </script>    

