    <script type="text/javascript">
    $(document).ready(function () {
        if ($(".dpcouponcode").length > 0) {
            var affzone = $.cookie("AffZone");
            var zoneValues = affzone.split("|");
            if (zoneValues.length >= 35)
            {
                var couponCode = zoneValues[35];
                var couponcodeZoneValues = couponCode.split("~");
                var couponCodeValueToReplace = couponcodeZoneValues[2];
                $(".dpcouponcode").text(couponCodeValueToReplace);
            }
        }
    });
</script>

