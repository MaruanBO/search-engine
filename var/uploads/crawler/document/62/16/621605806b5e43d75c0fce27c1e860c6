<script type="text/javascript">

        var searchDiv = '#tblFindingZipWidgetHorizontal';

        function searchPlansHorizontal() {

            $("#replaceWithSearchTypeHorizontal").text("Plans ");
            $("#replaceWithZipCodeHorizontal").text($("#txtZipCodeForPlanHorizontal").val());

            LoadVirtualGoogleEventTracking('Button', 'Plan Selection');

            $.ajax({
                url: '/home/iszipcodevalid',
                type: "GET",
                data: { zipCode: $("#txtZipCodeForPlanHorizontal").val() },
                success: function (returnValue) {
                    if (returnValue == "false") {
                        $('#ErrorHorizontal').text("ZIP Code not valid");
                    } else {

                        LoadVirtualGoogleEventTracking('Button', 'Search Submit');

                        $(searchDiv).show();
                        var zipCode = $("#txtZipCodeForPlanHorizontal").val();
                        var url = "/dentalsearch/dentalplansearch?zipcode=" + zipCode + "&planType=individual&f=p";
                        $(location).attr('href', url);
                    }
                }
            });
        }

        function searchDentistsHorizontal() {

            $("#replaceWithSearchTypeHorizontal").text("Dentists ");
            $("#replaceWithZipCodeHorizontal").text($("#txtZipCodeForPlanHorizontal").val());

            LoadVirtualGoogleEventTracking('Button', 'Dentist Selection');

            $.ajax({
                url: '/home/iszipcodevalid',
                type: "GET",
                data: { zipCode: $("#txtZipCodeForPlanHorizontal").val() },
                success: function(returnValue) {
                    if (returnValue == "false") {
                        $('#ErrorHorizontal').text("ZIP Code not valid");
                    } else {

                        LoadVirtualGoogleEventTracking('Button', 'Search Submit');

                        $(searchDiv).show();
                        var zipCode = $("#txtZipCodeForPlanHorizontal").val();
                        var url = "/dentalsearch/dentistsearch?zipCode=" + zipCode + "&planType=individual&f=d";
                        $(location).attr('href', url);
                    }
                }
            });
        }

        $(document).ready(function () {


            $("#txtZipCodeForPlanHorizontal").val('');
            $("#tblFindByZip").show();

            $('#lnkSearchPlanHorizontal').click(function () {
                var radio = $('input:radio[name=lookup]:checked').val()
                if (radio == "planHorizontal") {
                    searchPlansHorizontal();
                }

                if (radio == "dentistHorizontal") {
                    searchDentistsHorizontal();
                }
            });

            $("#txtZipCodeForPlanHorizontal").keypress(function (e) {
                if (e.which == 13) {
                    $('#lnkSearchPlanHorizontal').click();
                }

                var k = e.which;

                if ((k < 48 || k > 57) && (k < 96 || k > 105)) {
                    e.preventDefault();
                    return false;
                }

            });
            $("#txtZipCodeForPlanHorizontal").keydown(function (event) {

                if (event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || event.keyCode == 13 ||
                // Allow: Ctrl+A
                     (event.keyCode == 65 && event.ctrlKey === true) ||
                // Allow: home, end, left, right
                    (event.keyCode >= 35 && event.keyCode <= 39)) {
                    // let it happen, don't do anything
                    return;
                }
                else {
                    // Ensure that it is a number and stop the keypress
                    if (event.shiftKey || (event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105)) {
                        event.preventDefault();
                    }
                }

            });

            $("#txtZipCodeForPlanHorizontal").click(function () {
                $("#txtZipCodeForPlanHorizontal").val("");
            });


            $("#lnkTryAgain").click(function () {
                $("#tblFindByZip").show();
                $(searchDiv).hide();
                $("#radioboxDiv").show();
            });

            $("#planHorizontal").css("background", "#1a5c98");
            $("#dentistHorizontal").css("background", "#1a5c98");
            $("#txtZipCodeForPlanHorizontal").watermark("Enter ZIP Code");
        });

</script>
