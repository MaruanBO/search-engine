    <script type="text/javascript">

    SetColors();

    $(function () {
        // Check for the rebranded site
        SetColors();
    });

    function SetColors() {
        $("tr, div, td, p, span, a, h1, h2, h3, strong, label").each(function () {
            var color = $(this).css("color");
            var backgroundColor = $(this).css("background-color");
            SetColor($(this), color, 'color');
            SetColor($(this), backgroundColor, 'background-color');

            if ($(this).css('border-color').length > 0) {
                var borderColor = $(this).css('border-color');
                SetColor($(this), borderColor, 'border-color');
            }
        });
    }

    function SetColor(object, color, attribute) {

        if (color == "#CF6A6A") {
            object.css(attribute, "#f89b23");
        } else if (color == "#041CAC") {
            object.css(attribute, "#0094a6");
        } else if (color == "rgb(4, 28, 172)") {
            object.css(attribute, "#0094a6");
        } else if (color == "rgb(207, 106, 106)") {
            object.css(attribute, "#f89b23");
        } else if (color == "rgb(235, 242, 248)") {
            object.css(attribute, "#d6f2f6");
        } else if (color == "rgb(0, 86, 150)") {
            object.css(attribute, "#00a5b4");
        } else if (color == "rgb(1, 87, 150)") {
            object.css(attribute, "#00a5b4");
        } else if (color == "rgb(1, 86, 151)") {
            object.css(attribute, "#00a5b4");
        } else if (color == "rgb(47, 122, 189)") {
            object.css(attribute, "#00a5b4");
        } else if (color == "rgb(0, 107, 168)") {
            object.css(attribute, "#00a5b4");
        } else if (color == "rgb(38, 134, 208)") {
            object.css(attribute, "#00a5b4");
            // Extra color codes
        } else if (color == "#ebf2f8") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#ebf2f8") {
            object.css(attribute, "#d6f2f6");
        } else if (color == "#005696") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#015796") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#015697") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#2f7abd") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#2686D0") {
            object.css(attribute, "#00a5b4");
        } else if (color == "#eaf5fd") {
            object.css(attribute, "#D6F2F6");
        } else if (color == "rgb(234, 245, 253)") {
            object.css(attribute, "#D6F2F6");
        }


    }
</script>

