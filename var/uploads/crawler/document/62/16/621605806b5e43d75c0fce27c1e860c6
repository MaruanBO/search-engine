    <script type="text/javascript">

        function searchPlans() {
            $("#replaceWithSearchType").replaceWith("Plans ");
            $("#replaceWithZipCode").replaceWith($("#txtZipCodeForPlan").val());
            LoadVirtualGoogleEventTracking('Button', 'Plan Selection');

            $.ajax({
                url: '/home/iszipcodevalid',
                type: "GET",
                data: { zipCode: $("#txtZipCodeForPlan").val() },
                success: function(returnValue) {
                    if (returnValue == "false") {
                        $('#PlanError').text("ZIP Code not valid");
                    } else {

                        LoadVirtualGoogleEventTracking('Button', 'Search Submit');

                        $("#tblFindingZip").show();
                        var zipCode = $("#txtZipCodeForPlan").val();
                        var planType = $('input:radio[name=planType]:checked').val();
                        var url = "/dentalsearch/dentalplansearch?zipcode=" + zipCode + "&planType=" + planType + "&f=p";
                        $(location).attr('href', url);
                    }
                }
            });
        }

        function searchDentists() {
            $("#replaceWithSearchType").replaceWith("Dentists ");
            $("#replaceWithZipCode").replaceWith($("#txtZipCodeForDentist").val());

            LoadVirtualGoogleEventTracking('Button', 'Dentist Selection');

            $.ajax({
                url: '/home/iszipcodevalid',
                type: "GET",
                data: { zipCode: $("#txtZipCodeForDentist").val() },
                success: function (returnValue) {
                    if (returnValue == "false") {
                        $('#divFindZipCode').jqm({ ajax: '/Home/ZipLookup?zipCodeNotFoundFor=Dentist', trigger: '#lnkFindZip', modal: true, refreshParent: false });
                        $('#DentistError').text("ZIP Code not valid");

                    } else {

                        LoadVirtualGoogleEventTracking('Button', 'Search Submit');

                        $("#tblFindingZip").show();
                        var zipCode = $("#txtZipCodeForDentist").val();
                        var dentistLastName = $('#txtDentistLastName').val();

                        if (dentistLastName == 'Dentist Last Name')
                            dentistLastName = '';

                        var url = "/dentalsearch/dentistsearch?zipCode=" + zipCode + "&dentistLastName=" + dentistLastName + "&f=d";
                        $(location).attr('href', url);

                    }
                }
            });
        }

        $(document).ready(function () {

            $('.searchMenu a').click(function () {

                if ($(this).attr('id') == 'tab1') {
                    $('#tab1 img').attr('src', '/Areas/s2/Content/images/tabs/plan_finder_o.png');
                    $('#tab2 img').attr('src', '/Areas/s2/Content/images/tabs/dentist-finder_u.png');
                    $('#txtZipCodeForPlan').val($('#txtZipCodeForDentist').val());
                }
                else {
                    $('#tab1 img').attr('src', '/Areas/s2/Content/images/tabs/plan_finder_u.png');
                    $('#tab2 img').attr('src', '/Areas/s2/Content/images/tabs/dentist-finder_o.png');
                    $('#txtZipCodeForDentist').val($('#txtZipCodeForPlan').val());
                    LoadVirtualGoogleEventTracking('Button', 'Dentist Finder Tab');
                }

            });



            $("#txtZipCodeForPlan, #txtZipCodeForDentist").mask("99999");
            $("#txtZipCodeForPlan, #txtZipCodeForDentist").watermark("Enter ZIP Code");
            $("#txtZipCodeForPlan, #txtZipCodeForDentist").val('');

            $("#tblFindByZip").show();
            $("#findaplan").css("background", "#1a5c98");
            $("#findadentist").css("background", "#1a5c98");

            $("#txtenteremail").watermark("Enter Email");

            $('#lnkSearchPlan').click(function () {
                searchPlans();
            });

            $("#txtZipCodeForPlan").keypress(function (e) {
                if (e.which == 13) {
                    searchPlans();
                }
            });

            $('#lnkSearchDentists').click(function () {
                searchDentists();
            });

            $("#txtZipCodeForDentist, #txtDentistLastName").keypress(function (e) {
                if (e.which == 13) {
                    searchDentists();
                }
            });


        });

    </script>    

