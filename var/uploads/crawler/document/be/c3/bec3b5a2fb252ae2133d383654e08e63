        function isFormCorrect() {
            var emailCtrl = document.getElementById("email_address");
            if (!emailCtrl) return false;

            if (emailCtrl.value.search(/^\s*$/) >= 0) {
                emailCtrl.focus();
                return false;
            }

            return true;
        }
    </script>
    
