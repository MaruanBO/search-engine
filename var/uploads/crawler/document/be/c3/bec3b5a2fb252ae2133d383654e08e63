<!-- Show-Hide -->
<script language="javascript" type="text/javascript">
function showHide(shID) {
	if (document.getElementById(shID)) {
		if (document.getElementById(shID+'-show').style.display != 'none') {
			document.getElementById(shID+'-show').style.display = 'none';
			document.getElementById(shID).style.display = 'block';
		}
		else {
			document.getElementById(shID+'-show').style.display = 'inline';
			document.getElementById(shID).style.display = 'none';
		}
	}
}
</script>


<script language="javascript" type="text/javascript">
    var goToContestPage = function () {
        var emailCtrl = document.getElementById("email_address");
        var contestUrl = "http://www.cruise.com/contest/index.aspx";
        if (!emailCtrl) {
            window.location = contestUrl;
            return;
        }

        if (emailCtrl.value.search(/^\s*$/) >= 0) {
            window.location = contestUrl;
            return;
        }

        window.location = contestUrl + "?email=" + encodeURIComponent(emailCtrl.value);
    }
</script>
