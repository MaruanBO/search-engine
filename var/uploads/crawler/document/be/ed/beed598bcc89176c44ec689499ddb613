<!-- Custom jQuery calls for PayPal -->
<script>
  $(document).ready(function(){

    $(".header .header-wrap .lia-search-form-wrapper input").focusin(function(){
        $(".header .header-wrap .lia-search-form-wrapper form").css("border-color", "#0079c1");
    });

    $(".header .header-wrap .lia-search-form-wrapper input").focusout(function(){
        $(".header .header-wrap .lia-search-form-wrapper form").css("border-color", "#bababa");
    });

    $(".welcomegoButton").click(function(){
        $("#ppl-blog-welcome").fadeOut("slow");
    });

    $(".secondary-logo").fadeIn(2000);

    $("#searchToggle").click(function(){
        $("header .searchbar").toggleClass("hide");
        $("#searchToggle").toggleClass("hide");        
    });

    $(".closeflexslider").click(function(){
        $(".flexslider").slideToggle("fast", 
             function(){
                $(".flexslider").css("height","0px");
             });
        $(".bannerShow").slideToggle("slow");
    });

    $(".bannerShow").click(function(){
        $(".flexslider").css("height","");
        $(".flexslider").slideToggle("fast");        
        $(".bannerShow").slideToggle("fast");

    });

    $( window ).scroll(function() {
        var ypos = $(document).scrollTop();
        var docheight = $(document).height();
        var canvasheight = $(window).height();

        $( "#bgimage" ).css( "opacity", "" + 1-(ypos/(docheight-canvasheight))  );
    });

    var jPM = $.jPanelMenu({
    excludedPanelContent: '#surveycontainer',
    direction: 'right',
    openPosition: '300px',
    duration: 10,
    afterOpen: function(){
        $(".at4-follow").css("right", "300px");
        $(".atss-right").css("right", "300px");
        $(".at-share-open-control-right").css("right", "300px");
        $(".at-follow-open-control").css("right", "300px");
        $(".at4m-dock-bottom").fadeOut();
        $("#mainimgBackground, .mainbg").css("right", "600px");

    },
    afterClose: function(){
        $(".at4-follow").css("right", "0px");
        $(".atss-right").css("right", "0px");
        $(".at-share-open-control-right").css("right", "0px");
        $(".at-follow-open-control").css("right", "0px");
        $(".at4m-dock-bottom").fadeIn();
        $("#mainimgBackground, .mainbg").css("right", "0px");
    },
});
    jPM.on();
  });


</script>
