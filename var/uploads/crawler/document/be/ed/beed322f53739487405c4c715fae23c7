function popup(mylink, windowname)
{
if (! window.focus)return true;
var href;
if (typeof(mylink) == 'string')
   href=mylink;
else
   href=mylink.href;
window.open(href, windowname, 'width=650,height=680');
return false;
}

function MM_preloadsite_images() { //v3.0
  var d=document; if(d.site_images){ if(!d.MM_p) d.MM_p=new Array();
    var i,j=d.MM_p.length,a=MM_preloadsite_images.arguments; for(i=0; i<a.length; i++)
    if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}


function setPOESUS(){
	//set the auroral oval and link
	document.getElementById('poes_image').innerHTML='<a href="http://services.swpc.noaa.gov/images/aurora-forecast-northern-hemisphere.png"><img src="POES/pics/usa_thumb.jpg" width="200" height="200" border="0"></a>';
}

