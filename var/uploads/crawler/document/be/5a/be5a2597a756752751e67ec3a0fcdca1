function checkForm() {
	var q = document.forms[0].q.value;
	
	if (q == "") {
		alert("Please enter your search word or phrase");
	    document.forms[0].q.focus();
	} else {
		document.forms[0].action += "?q=" + q;
		document.forms[0].submit();
	}
