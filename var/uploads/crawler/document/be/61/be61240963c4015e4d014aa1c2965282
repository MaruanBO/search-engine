<script>
/* On Air Module Tracking */
function omnitureTrackOnAirModule(city, linkName, linkTitle) { 
    nbcu.un = nbc_account;
	nbcu.tl(this,'o',' link tracking');
	s.un = s_account;
	s.linkTrackVars = 'prop57';
	s.prop57=city+":"+linkName+":"+linkTitle;
	s.tl(this,"o","Track On-Air Module"); 
}
</script>

