 <script type="text/javascript">
//<![CDATA[
/* Load weather data for the homepage */

jQuery(document).ready(function() {
  jQuery("#processingWeatherLoading").show();  
  
  if(U.readCookie("zipCode")) { 
    weatherZipCode =  U.readCookie("zipCode");
  } else {
        weatherZipCode = "20001";
  }
  
      dataString="h=weather_home_widget&zipCode="+weatherZipCode;
    nbc_adIframePartner = "";
  
  var urlpost='/i/dispatcher/';
  
    jQuery.ajax({
      type: "GET",
      url: urlpost,
      data: dataString,
      cache:false,
      dataType: "html",
      
      success: function(data){
        trimdata = jQuery.trim(data);
        jQuery("#weather-module-data-0").html(trimdata);
        if( jQuery(".changeLocation").length > 0 ) {
            nbcWeatherChangeLoc.init("#weather-module-data-0");
        };
        jQuery("#processingWeatherLoading").hide();
        if(nbc.omniture.pageType == "article") {
            nbc.weatherModuleUpdate.activateTracking(0);
            if(nbc.market == "nbcphiladelphia") {
                nbcgptengine.buildAnAdSlot("#weather_ad_0", "88,31", 1,"top","weather",false, nbc_adIframePartner);
            }
        } else {
            nbcgptengine.buildAnAdSlot("#weather_ad_0", "88,31", 1,"top","weather",false, nbc_adIframePartner);
            jQuery('#weather-module-data-0 .weather-module-wrapper a').click(function(e){
                e.preventDefault();
                var omniTrack = jQuery(this).data('tracking');
                nbcu.linkTrackVars="eVar7";
                nbcu.eVar7 = 'weather-rightrail|' + omniTrack; 
                nbcu.tl(this,'o',nbcu.eVar7,null,'navigate');
            })
        }
      }
    }); // END AJAX call
}); // END document.ready
//]]>
</script>

