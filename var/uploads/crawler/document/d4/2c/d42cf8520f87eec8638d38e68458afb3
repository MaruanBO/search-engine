<script type="text/javascript">

    $(document).ready(function () {

        initCouponWidget(); 

        try {
            couponService.couponModel.A_accudata_source("CIG_20150105_AllOurBest");
            couponService.couponModel.A_accudata_emailtosend("CIG_20150105_AllOurBest");
            couponService.couponModel.B_accudata_source("CIG_20150105_AllOurBest");
