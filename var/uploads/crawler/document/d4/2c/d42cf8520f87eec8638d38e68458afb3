<script>
    $(document).ready(function() {
        getATableModel.init("overlay");

        var currentSize = $(window).height();
        $("#overlay").height(currentSize + 10);
        $(window).on("resize", function () {
            currentSize = $(window).height();
            $("#overlay").height(currentSize + 10);
        });
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {
        $('input').keypress(function (e) {
            var code = (e.keyCode ? e.keyCode : e.which);
            if ((code == 13) || (code == 10)) {
                var clickDoneFlag = false;
                $("input[value=Next]").each(function () {
                    if (!clickDoneFlag) {
                        if ($(this).is(':visible')) { $(this).click(); clickDoneFlag = true; }
                    }
                });
                if (!clickDoneFlag) {
                    $("input[value=Confirm]").each(function () {
                        if (!clickDoneFlag) {
                            if ($(this).is(':visible')) { $(this).click(); clickDoneFlag = true; }
                        }
                    });
                }
                $(this).blur();
                return false;
            }
        });
    });
</script>
                </div>
            </div>



            
