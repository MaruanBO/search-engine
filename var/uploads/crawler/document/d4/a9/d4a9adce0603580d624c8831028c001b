        <script>
        
        function closeTour() {
          $('#takeTourBox').hide();
          $('html, body').animate({
            scrollTop: 0,
            scrollLeft: 0
          }, 1000);
        }
        function takeTour(section) {
          nextSection = section + 1;
          nextSectionFunc = 'takeTour(' + nextSection + ')';
          closeFunc = 'closeTour()'
          secondLine = '<br /><br /><a href="#" onclick="' + nextSectionFunc + '; return false;">Next</a> or <a href="#" onclick="' + closeFunc + '; return false;">Close</a>';
          lastLine = '<br /><br /><a href="#" onclick="takeTour(1); return false;">Replay</a> or <a href="#" onclick="' + closeFunc + '; return false;">Close</a>';
          if(section == 1)
          {
            $('.messageBoxHtml').html('Your Current Points & Sweeps Entries Balance' + secondLine);
            px = $('#notice').offset().top - $('#takeTourBox').height() + 16;
            $('#takeTourBox').css('margin-left', '5px').css('top', px);
          }
          else if(section == 2)
          {
            $('.messageBoxHtml').html('2. Your Available Surveys & Mini-Polls' + secondLine);
            px = $('#userSurveysNum').offset().top - $('#takeTourBox').height() - 16;
            $('#takeTourBox').css('margin-left', '5px').css('top', px);
          }
          else if(section == 3)
          {
            $('.messageBoxHtml').html('3. Complete Surveys to Earn Points & Sweeps Entries' + secondLine);
            px = $('section:eq(0)').offset().top - $('#takeTourBox').height() - 2;
            $('#takeTourBox').css('margin-left', '5px').css('top', px);
          }
          else if(section == 4)
          {
            $('.messageBoxHtml').html('4. Install our Mobile App for More Surveys & Rewards' + secondLine);
            px = $('section:eq(1)').offset().top - $('#takeTourBox').height() - 2;
            $('#takeTourBox').css('margin-left', '5px').css('top', px);
          }
          else if(section == 5)
          {
            $('.messageBoxHtml').html('5. Redeem Your Reward Points' + secondLine);
            px = $('section:eq(2)').offset().top - $('#takeTourBox').height() - 2;
            $('#takeTourBox').css('margin-left', '5px').css('top', px);
          }
          else if(section == 6)
          {
            $('.messageBoxHtml').html('6. Our Latest News' + secondLine);
            px = $('section:eq(3)').offset().top - $('#takeTourBox').height() - 2;
            if($(window).width() > 800) $('#takeTourBox').css('margin-left', '551px').css('top', px);
          }
          else if(section == 7)
          {
            $('.messageBoxHtml').html('7. Complete Mini-Polls to Earn More Sweeps Entries' + secondLine)
            px = $('section:eq(4)').offset().top - $('#takeTourBox').height() - 2;
            if($(window).width() > 800) $('#takeTourBox').css('margin-left', '551px').css('top', px);
          }
          else if(section == 8)
          {
