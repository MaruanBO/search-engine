<script type="text/javascript">

$j = $;


// Handle shipping messages 
if(CookieManager) 
{
  var tm_CookieManager = new CookieManager();
  var tm_UserCountry = tm_CookieManager.getValueIgnoreCase('Country')||'US';
  if(tm_UserCountry !== 'US') {
    $('#header-ship-us').hide();
    $('#header-ship-int').css('display','inline');
  }
}
// end shipping messages 


// Targeted banner details
$('.targ-details').live('click', function() { $('#targ-details-popup').show(); return false; });
$('#targ-details-popup .close').live('click', function() { $('#targ-details-popup').hide(); return false; });
// End targeted banner details


var closeCats = '';
var leftAdded = false;
var autoStarted = false;
var newAutoUI = true;
function AutoCompleteTwo() {
	if (!autoStarted) {
		$('#NttDrop').autocomplete("/WebServices/AutoComplete.ashx", { 
			dataType:'text',
			formatItem: function(row) {
				var s = row[0].split('~');
				if (s.length > 0) {
					if (newAutoUI) {
						return '<b>' + s.pop() + '</b><br/><span style="font-size:85%;font-style:italic">' + s.join('~') + '</span>';
					} else {
						if ((s[0].toLowerCase() == 'brand') ||
							(s[0].toLowerCase() == 'category')) {
							s.shift();
						}
						return s.join('~');
					}
				} else {
					return row[0];
				}
			},
			formatResult: function(data) {
				var s = data[0].split('~');
				if ((s[0].toLowerCase() == 'brand') ||
					(s[0].toLowerCase() == 'category')) {
						s.shift();
				}
				return s.join('~');
			}
		});
        $('#NttDrop').result(function(event, data) {
        	if (newAutoUI) {
	        	//selected category?
	        	var nvalCat = $('#nval-cat a').attr('href');
	        	if ((data.length > 1) && (!isNaN(parseInt(data[1], 10)))) {
	            	if ((nvalCat === undefined) || (isNaN(parseInt(nvalCat, 10)))) {
	              		window.location = "/Shopping/Results.aspx?icid=entiresitesearch&N=" + data[1];
	              	}
	            } else {
	              		window.location = "/Shopping/Results.aspx?icid=entiresitesearch&N=" + data[1] + "+" + nvalCat;
	            }	
	        }
	     	return false;
        });
		autoStarted = true;
	}
}
$(document).ready(function() {	
	
	var winLoc=window.location.href;
	var icidSET = false;
	
	
	
	
	$('body').append('<div id="drop-cats"><a href="#6207" alt="searchwithinwomens" title="">women</a><a href="#6208" alt="searchwithinmens" title="">men</a><a href="#6209" alt="searchwithingirls" title="">girls</a><a href="#6210" alt="searchwithinboys" title="">boys</a><a href="#6270" alt="searchwithinbags" title="">bags</a><a href="#6211" alt="searchwithinaccessories" title="">accessories</a><a href="#4294965982" alt="searchwithinsale" title="">sale</a></div>');

	$('#drop-cats').mouseenter(function() {
		if (closeCats) {
			clearTimeout(closeCats);	
		}
		$('#drop-cats').slideDown(80).css('top', dropTop);
	});
	$('#nval-cat a').click(function() {
		if (closeCats) {
			clearTimeout(closeCats);	
		}
		$('#drop-cats').slideToggle(80).css('top', dropTop);
		return false;
	});
	$('#nval-cat a, #drop-cats').mouseleave(function() {
		closeCats = setTimeout("$('#drop-cats').hide()",1000);
	});
	$('#drop-cats a').click(function() {
		icidSET = true;
		$('#leftsearch').val('1');
		//$('#icidSearchLabel').val('withindeptsearch');
		var icidVal = $(this).attr('alt');
		$('#icidSearchLabel').val(icidVal);
		var linkTxt = $(this).text();
		var linkHref = $(this).attr('href');
		var pos = linkHref.lastIndexOf('#') + 1;
		linkHref = linkHref.substr(pos);
		$('#nval-input').val(linkHref);
		$('#nval-cat a').text(linkTxt).attr('href', linkHref);
		$('#drop-cats').hide();
		if (!leftAdded) {
			$('form:first').append('<input type="hidden" name="leftsearch" value="1">');
			leftAdded = true;
		}
		return false;
	});
	$('.btn-search-submit').mouseenter(function() {
		$('#drop-cats').hide();
	});

	var searchOffset = $('#topnav-search').offset();
	var dropTop = Math.round(searchOffset.top) + $('#topnav-search').outerHeight();
});

// submit the search form
function SearchSubmit() {
	if (document.endeca2.Ntt.value == '') {
		return false;
	} else {
		return true;
	}
		return false;
}
</script>
