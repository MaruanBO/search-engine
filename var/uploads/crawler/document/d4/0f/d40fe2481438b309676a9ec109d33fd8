// Temporary fix for Runa.
if (CookieManager) {
  var tm_CookieManager = new CookieManager();
  var match = RegExp('[?&]partnerid=([^&]*)', 'i').exec(window.location.search);

  if (match) {
      var partnerId = decodeURIComponent(match[1].replace(/\+/g, ' '));
      if (partnerId.length > 0) {
        var d = new Date();
        tm_CookieManager.setCookie('runa_settings', partnerId.toUpperCase(), d.getDay() + 45, 'shoes.com');
      }
  }
}

