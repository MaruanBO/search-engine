<!-- E4X SPECIALS 08/24/2012 -->
<script type="text/javascript">
E4x.ajaxUpdate = function () {
    jQuery.ajax({
        type: 'POST',
        url: E4x.url,
        dataType: 'html',
        async: true,
        success: function (html) {
            html = RemoveScriptFromAjaxResponse(CleanAjaxResponse(html));
            var newOBj = $(html);
            // newOBj.find('p:last').after('<p style="font-size:.8em; width:480px;"> <span style="color:#D80E6E; font-style:italic;">SPECIAL!</span> <br /> <span style="color:#D80E6E;">Canada:</span> <br /> &bull; Free Duty on all Canadian Orders! </p>');
            $(E4x.container).html(newOBj);
            E4x.popupDiv();
        }
    });
};
</script>



