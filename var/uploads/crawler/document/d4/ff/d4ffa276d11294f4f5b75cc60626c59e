function rkg_track_sid(mid){
if(!(document.referrer&&document.referrer.match('://([^/]+)')[1].toLowerCase().match(document.domain.match('[^.]*\.[^.]*$')[0].toLowerCase()))){
var href="http://mct.rkdms.com/sid.gif?mid="+mid+"&ref="+encodeURIComponent(document.referrer);
var src=window.location.href.match(/rkg_src=.*(&|$)/);
if(src&&src[0]){
var s=src[0].replace('rkg_src=','');
href+="&src="+s;}
document.write('<img class="rkg" height="1" width="1" src="'+href+'"/>');}}
try{rkg_track_sid("thinkgeek");}catch(e){}
</script>
