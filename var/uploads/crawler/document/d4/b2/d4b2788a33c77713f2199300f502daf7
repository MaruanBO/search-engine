<script>
            	function onKeyDownFindCouplesTH() {
            		var fn=document.getElementById('topHdrCoupleSearch_inputFindGift1').value,
                        ln=document.getElementById('topHdrCoupleSearch_inputFindGift2').value;
            		if (fn==='First name') {
            			fn='';
            		}
            		if (ln==='Last name') {
            			ln='';
            		}
           window.open('http://registry.theknot.com/weddingregistrysearch?lt=SearchResults&st=SearchWidget&ss=HeaderSearch&sp=Variable&a=994&eventType=Wedding&cm_mmc=Affiliate-_-TheKnot-_-HPSearch-_-HPSearch&a=994&firstName=' + fn + '&lastName=' + ln);
            	}
            	(function () {
            		if (typeof $!=='undefined'&&typeof jQuery!=='undefined'&&$===jQuery) {
            			$('#topHdrCoupleSearch input').focus(function () {
            				this.select();
            			});
            			$('.inputFindGiftTH').keydown(function (e) {
            				if (e.which===13) {
            					onKeyDownFindCouplesTH();
            				}
            			});
            			$('.btnFindGiftTH').click(function (e) {
            				e.preventDefault();
            				onKeyDownFindCouplesTH();
            			});
            		}
            	} ());
            </script>
</div>
<script type="text/javascript">
            	$(function () {
            		$('.topHdrSearchTabs span').click(function () {
            			switch_topHdrSearchTabs($(this));
            		});
            		switch_topHdrSearchTabs($('.topHdrSearchTabsDefault'));
            	});
            	function switch_topHdrSearchTabs(obj) {
            		$('.topHdrSearchContent').hide();
            		$('.topHdrSearchTabs span').removeClass("selected");
            		var id=obj.attr("rel");

            		$('#'+id).show();
            		obj.addClass("selected");
            	}
            	$(function () {
            		if ($.browser.msie&&parseInt($.browser.version)<9) {

            			$('.topHdrSearchContent input').each(function () {
            				var p;

            				if (p=$(this).attr('placeholder')) {

            					$(this).val(p);

            					$(this).focus(function () {
            						if (p===$(this).val()) {
            							return $(this).val('');
            						}
            					});

            					$(this).blur(function () {
            						if ($(this).val()==='') {
            							return $(this).val(p);
            						}
            					});
            				}
            			});
            		}


                });

                var HeaderSearch = {

                    Load: function () {
                        var currSearch = $('#topHdrSearch').find('div').find('input');

                        currSearch.keydown(function (e) {
                            if ($(this).is(':focus')) {
                                if (e.keyCode == 13) {
                                    $("input:not(this)").live('click keypress keydown', function (event) {
                                        return false;
                                    });
                                    currSearch.sibling('a').click();
                                }
                            }
                        });
                    }
                };

                HeaderSearch.Load();
            </script>
