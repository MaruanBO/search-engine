				dojo.require("dojo.cookie");
				function setChosenLocale(locale) {
					dojo.cookie("chosen_locale", locale, {path: '/'});
					return true;
				}
				
				function setCookie(name,value,expire_days) {
					var cookie_value;
					if (expire_days != null) {
						var expire_date=new Date();
						expire_date.setDate(expire_date.getDate() + expire_days);
						cookie_value = escape(value) + "; expires="+expire_date.toUTCString();
					} else {
						cookie_value = escape(value);
					}
					document.cookie=name + "=" + cookie_value;
				}
			</script>
