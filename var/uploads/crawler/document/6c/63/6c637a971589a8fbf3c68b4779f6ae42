<script>
    function getTrackingPixels() {
        var targetNode = dojo.byId("trackingPixels");

        var xhrArgs = {
            url: "/wps/wcm/connect/StraightTalk/Home/Home",
            handleAs: "text",
            preventCache: true,
            content: {
                presentationtemplate: "Straighttalk Design/Tracking Pixels"
            },
            load: function(data) {
                targetNode.innerHTML = data;
            },
            error: function(error) {
                targetNode.innerHTML = "An unexpected error occurred: " + error;
            }
        }

        var deferred = dojo.xhrGet(xhrArgs);
    }
    if (dojo.isIE) {
      dojo.connect('onload', getTrackingPixels);
    } else {
      dojo.addOnLoad(getTrackingPixels);
    }
</script>
