	jQuery.noConflict();

	jQuery('.offer_fan').mouseover(function() {
	  	var offer_id = this.id.replace('offer_fan_','').replace('_body','').replace('_title','');
	    jQuery('.offer_fan_'+offer_id).addClass('offer_fan_hover');
	   });

	 jQuery('.offer_fan').mouseleave(function() {
	    var offer_id = this.id.replace('offer_fan_','').replace('_body','').replace('_title','');
	    jQuery('.offer_fan_'+offer_id).removeClass('offer_fan_hover');
	   });

	jQuery(document).ready(function($) {
		$('.product_type_hidden').hide();
		$('.product_type_more').show();
		$('.product_type_more_click').click(function() {
			$('#'+this.id.replace('_more','_status')).hide();
			$('#'+this.id+'_hidden').show();
			$('#showall').val('1');
			return false;
			});

		$('.call_me').click(function() {
			var phone_req=0;
			$('.call_me').each(function() {
				if (this.checked) {phone_req=1};
				});
			if (phone_req) {
				$('#dianomiPhoneRequired').show();
				$('#dianomiPhoneInput').addClass('required');
				}
			else {
				$('#dianomiPhoneRequired').hide();
				$('#dianomiPhoneInput').removeClass('required');
				}
			});
		$('tr.dianomiProductRow').mouseover(function() {
			$(this).addClass("over");
			}).mouseout(function() {
			$(this).removeClass("over");
			});

		$('.read_more').click(function() {
			my_id = this.id.replace('_less','');
			$('#'+my_id).toggle();
			$('#'+my_id+'_body').toggle();
			$('#'+my_id+'_less').toggle();
		});
		$('.dianomiProductRow').click(function(event) {
			if ((event.target.type != 'checkbox') && !$(event.target).hasClass('read_more') &&  !$(event.target).hasClass('label')) {
				var my_id=this.id.replace('ProductRow','CheckBox');
				$('#'+my_id).attr('checked',!$('#'+my_id).attr('checked'));
				var my_id_pdf = my_id + 'pdf';
				$('#'+my_id_pdf).attr('checked',!$('#'+my_id_pdf).attr('checked'));
				var my_id_call = my_id + 'call';
                                $('#'+my_id_call).attr('checked',!$('#'+my_id_call).attr('checked'));

				}
			});


jQuery.validator.messages.required = "";
    	// Only allows +,()\ plus digits.
	$.validator.addMethod("phoneUS", function(phone_number, element) {
    phone_number = phone_number.replace(/\s+/g, ""); 
	return this.optional(element) || phone_number.length > 9 &&
		phone_number.match(/^(1-?)?(\([2-9]\d{2}\)|[2-9]\d{2})-?[2-9]\d{2}-?\d{4}$/);
}, "Please specify a valid phone number");

    	$.validator.addMethod("custom_number", function(value, element) {
		    return this.optional(element) || value === "NA" ||
		    value.match(/^[0-9() ,\+-]+$/);
		}, "Please enter a valid number.");

		$.validator.addMethod("onecheck", function(value, element) {
            return $("input.checkbox:checked").length > 0;
        }, 'Please Select Atleast One CheckBox')

		// Only allows +,()\ plus digits.
    	$.validator.addMethod("custom_names", function(value, element) {
		    return this.optional(element) || value === "NA" ||
		    1 }, "Please do not enter digits.");		

		$.validator.addMethod("letterswithbasicpunc", function(value, element) {
			return this.optional(element) || /^[a-z-.,()'\"\s]+$/i.test(value);
		}, "Letters or punctuation only please");  

		$.validator.addMethod("notNumber", function(value, element, param) {
            var reg = /[0-9]/;
            if(reg.test(value)){
            	return false;
            }else{
            	return true;
            }
        }, "Numbers are not permitted");

        jQuery.validator.addMethod("date_ITA_UK", function(value, element) {
			var check = false;
			var re = /^\d{1,2}\/\d{1,2}\/\d{4}$/;
			if( re.test(value)){
				var adata = value.split('/');
				var gg = parseInt(adata[0],10);
				var mm = parseInt(adata[1],10);
				var aaaa = parseInt(adata[2],10);
				var xdata = new Date(aaaa,mm-1,gg);
				if ( ( xdata.getFullYear() == aaaa ) && ( xdata.getMonth () == mm - 1 ) && ( xdata.getDate() == gg ) )
					check = true;
				else
					check = false;
			} else
				check = false;
			return this.optional(element) || check;
		}, "Please enter a correct date");

		 jQuery.validator.addMethod("date_ITA_USA", function(value, element) {
			var check = false;
			var re = /^\d{1,2}\/\d{1,2}\/\d{4}$/;
			if( re.test(value)){
				var adata = value.split('/');
				var gg = parseInt(adata[0],10);
				var mm = parseInt(adata[1],10);
				var aaaa = parseInt(adata[2],10);
				var xdata = new Date(aaaa,gg-1,mm);
				if ( ( xdata.getFullYear() == aaaa ) && ( xdata.getMonth () == gg - 1 ) && ( xdata.getDate() == mm ) )
					check = true;
				else
					check = false;
			} else
				check = false;
			return this.optional(element) || check;
		}, "Please enter a correct date");
