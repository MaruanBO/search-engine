    <script type="text/javascript" src="//www.google.com/recaptcha/api/js/recaptcha_ajax.js"></script>

    <script>
        (function($) {

            /*fancy box support via jQuery plugin*/
            $(".fancybox").fancybox();

            if($('.js-emailShare').length) {
                $('.js-emailShare').on('click', 'a', function(e) {
                    e.preventDefault();
                    var $clickTarget = $(e.currentTarget);

                    new PF['views/modals/EmailShareArticleModalView']()
                            .init()
                            .setTarget($clickTarget)
                            .present();
                });
            }

            if($('.js-print-link').length > 0) {
                $(".js-print-link").on('click', function(e) {
                    e.preventDefault();

                    if ($('.slideshow').length > 0) {
                        var printWindow = window.open('?print=true', '_blank', '', true);
                        if (printWindow.addEventListener) {
                            printWindow.addEventListener('load', function () {
                                printWindow.print()
                            }, false);
                        } else {
                            printWindow.attachEvent('onload', function () {
                                printWindow.print()
                            })
                        }
                    } else {
                        window.print();
                    }
                });
            }

            /*form validation fallback via jQuery plugin
            plugin will disable HTML5 form validation
            custom validation for safari and browseres which dont support input.required*/
            var isSafari = Object.prototype.toString.call(window.HTMLElement).indexOf('Constructor') > 0;
            if (!Modernizr.input.required || isSafari) {
                $('form').validator(PF.VALIDATE_OPTIONS);
            }

            /*set the base url for pet photos*/
            PF['services/ServiceConfiguration'].photoHost = 'drpem3xzef3kf.cloudfront.net';

            /*create PetSearchFormView for all pages except search results
            PetSearchFormView is created and managed by PetSearchView for this page*/
            if(PF['Configuration/isPetSearchPage'] !== true) {
                var searchFormView = new PF['views/PetSearchFormView']();
                var petSearchRequestModel = new PF['models/PetSearchRequestModel']().updateFromCookies();
                searchFormView
                    .init()
                    .setPetSearchRequestModel(petSearchRequestModel)
                    .updateFromPetSearchRequestModel();
            }

            new PF['views/RecentlyViewedPetsView']()
                .init();

        /*create slider view if needed*/
            if ($('#fcs-container .slide').size() > 0){
                new PF['views/SliderView']()
                    .setEl($('#fcs-container'))
                    .update();
            }

        /*setup header view, it is a singleton*/
            PF['views/HeaderView']
                .init()
                .update();

        /*seutp up sidebar view*/
            new PF['views/SidebarView']()
                .init()
                .update();
            new PF['controllers/FormSubmitController']()
                .init();

            new PF['views/SearchPageView']().init();

        }(jQuery));
    </script>
