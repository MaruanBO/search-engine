<script type="text/javascript">
	jQuery(document).ready(function($) {

		var pf_load_happy_tail = function() {

			if ($('#ad-x73').length>0 && $('#ad-x73-placeholder').length>0) {
				if ($('#ad-x73-placeholder').is(':empty')) {
					//console.log('is empty');
				} else {
					//console.log('about to load ad');
					$('#ad-x73-placeholder .loading-script').remove();
					$('#ad-x73').append($('#ad-x73-placeholder').html());
				}
			}
		}



	var ht_loaded = 0;

	if (typeof sliderInstance != 'undefined') {
			sliderInstance.ev.on('rsAfterContentSet', function() {
				var $htSlide = $('#happy-tail-slide');
				if ($htSlide.length > 0) {
					if ( ht_loaded === 0 ) {
						pf_load_happy_tail();
						ht_loaded = 1;
					}
				}
			});
		}

		if ($('#happy-tail-slide').length > 0) {
			if ( ht_loaded === 0 ){
				pf_load_happy_tail();
				ht_loaded = 1;
			}
		}

	});
</script>
