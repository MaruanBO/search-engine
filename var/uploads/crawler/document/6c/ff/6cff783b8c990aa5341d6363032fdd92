}
#videoContent li.media:first-child { display: none }
#featuredheadline { padding-bottom: 20px }
</style>
<div id="market-sacbee-video-vertical">
<h1 class="heading"><a href="//www.youtube.com/SacramentoBee">Videos</a></h1>
<div class="market sacbee youtube-video vertical digest">
<div id="featured">
<img alt="Loading..." src="http://media.miamiherald.com/static/images/escenic-images/ajax-loader_media-assets.gif"/>
</div>
<div id="featuredheadline" class="media border-bottom element_spacing_small"> </div>
<ul id="videoContent"></ul>
<div class="read-more pull-left">
<a id="playlistlink" target="_blank" href="https://www.youtube.com/playlist?list=PLcmRv1KYt4kJciFWrwNDcIQCoG65G-U4V">View more</a>
</div>
</div>
</div>
<div class="clearfix">&nbsp;</div>
<script>
(function($) {
var $featured = $('#featured');
var $videoContent = $('#videoContent');
var $featuredheadline = $('#featuredheadline');
var videofeaturedheadline = '';
var videofeatured = '';
var videolist = '';
var vid = '';
var thumb = '';
var title = '';
$.getJSON('https://gdata.youtube.com/feeds/api/playlists/PLcmRv1KYt4kJciFWrwNDcIQCoG65G-U4V?v=2&alt=json&callback=?', function(data) {
$.each(data.feed.entry, function(i, item) {
vid = item['media$group']['yt$videoid']['$t'];
thumb = item['media$group']['media$thumbnail'] ? item['media$group']['media$thumbnail'][0].url : '';
title = item['media$group']['media$title']['$t'];
if (i < 1) {
videofeatured += '<iframe class="youtube-player" type="text/html" width="316" height="257" src="http://www.youtube.com/embed/' + vid + '" allowfullscreen frameborder="0"></iframe>';
videofeaturedheadline = '<h4 class="title">' + title + '</h4>';
} else if (i < 4) {
videolist += '<li class="media" id="vid' + vid + '">';
if (thumb) {
videolist += '<div class="video pull-left element_spacing_small"><a class="ytvideo" href="#' + vid + '"><img src="' + thumb + '" class="img-responsive"/></a></div>';
}
videolist += '<div class="headline element_spacing_small"><h4 class="title"><a class="ytvideo" href="#' + vid + '">' + title + '</a></h4></div></li>';
}	});
$featured.html(videofeatured);
$featuredheadline.html(videofeaturedheadline);
$videoContent.html(videolist);
});
$(document).ready(function() {
$videoContent.on('click', 'a.ytvideo', function() {
var vid = $(this).attr('href').replace('#', '');
var vidtitle = $('li#vid' + vid + ' h4.title a').text();
var loadingImage = '<img alt="Loading..." src="http://media.sacbee.com/static/img/escenic-images/ajax-loader_media-assets.gif"/>';
$featured.html(loadingImage);
// Create an iFrame with autoplay set to true
var videoembed = '<iframe class="youtube-player" type="text/html" width="316" height="257" src="http://www.youtube.com/embed/' + vid + '?autoplay=1" allowfullscreen frameborder="0"></iframe>';
var videohead = '<h4 class="title">' + vidtitle + '</h4>';
$featured.html(videoembed);
$featuredheadline.html(videohead);
$videoContent.find('li.media').show();
$videoContent.find('li#vid' + vid).hide();
$('html, body').scrollTop($featured.offset().top);
return false;
});
});
})(window.$);
</script>
</div>
<script type="text/javascript" language="javascript">
$(document).ready(function(){
// Removing empty paragraphs first
$("#content-body-713459 p").filter( function() {
var html = $.trim($(this).html());
if(html == '' || html == '&nbsp;')
return true;
}).remove();
// Taking a subset of paragraphs using offset and maximum
var offset = 1;
var max = -1;
if(offset > 1){
$("#content-body-713459").children().slice(0, offset-1).remove();
}
if(max > -1){
$("#content-body-713459").children().slice(max).remove();
}
// Show dateline field at the beginning of the first paragraph
var showDateline = true;
var datelineStr = "";
if(offset <= 1 && showDateline == true && datelineStr != "") {
$("#content-body-713459 p:first").prepend("<span>"+ datelineStr + "- </span>");
}
});
</script>
