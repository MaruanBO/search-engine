      jQuery(function($) {
        if ( ! $.cookie('0_hyman_ten_days_detox_diet_challenge_modal_dialog_cookie') ) {
          $("li a").each(function() {
            var _href = $(this).attr("href"); 
            $(this).attr("href", _href + '#openModal');
          });
        }
        setTimeout(function () {
          if ( ! $.cookie('0_hyman_ten_days_detox_diet_challenge_modal_dialog_cookie') ) {
            window.location = '#openModal'; 
          }
        }, 30000);
      });
      function set_hyman_modal_dialog_cookie() {
        jQuery(function($) {
          remove_open_modal_param();
          $.cookie('0_hyman_ten_days_detox_diet_challenge_modal_dialog_cookie', 1, { expires : 365, path: '/' });
        });
      }
      function remove_open_modal_param() {
        jQuery(function($) {
          $("li a").each(function() {
            var _href = $(this).attr("href"),
            return_href = _href.split('#openModal');
            $(this).attr("href", return_href[0]);
          });
        });
      }
