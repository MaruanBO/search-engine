$('#events').click(function() { 
$('#searchType').val('events');initEventDrop('');
});

$('#accom').click(function() { 
$('#searchType').val('accom');
});

$('#act').click(function() { 
$('#searchType').val('act');
});



$('#dp-start').click(function() { 
$('.dp').datepicker({

		
			dateFormat: 'M dd, yy'

		 });
});

$('#dp-end').click(function() { 
$('.dp').datepicker({

		
			dateFormat: 'M dd, yy'

		 });
}); 





	// bindings

$('a.search-tab').click( function(e) {

		// e.target.id would also work here
    var tabId = $(this).attr("id");

    // call setTab function to work it's magic
    setTab( tabId );
  
  e.preventDefault();

});





function initEventDrop(e) {
var region = e;
//alert( $('#searchType').val()) $('#event-region-side').val()

	var request = $.ajax({
	   type: "GET",
	   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setCats",
	   data: "region=" + region + "&type=Events&cacheBuster=" + Math.random()
	    });

		request.done(function(msg){
			$('#event-select-cat-side').find('option').remove();

			jsonArray = $.parseJSON(msg);

			$.each(jsonArray, function(idx, obj){
				if( obj.COUNT > 0){
					$('#event-select-cat-side')
	         .append($("<option></option>")
	         .attr("value",obj.CATID)
	         .text(obj.NAME.replace(/&amp;/, "&")));
				}			

			});

			$('#event-select-cat-side').val('All');
			
			$('#event-select-cat-side option:selected').prependTo('#event-select-cat-side');

			//$('.kidsCats-side').empty();
		});
}


function createEventDrop() {
var region = $('#event-region-side').val();
//alert( $('#searchType').val()) $('#event-region-side').val()

	var request = $.ajax({
	   type: "GET",
	   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setCats",
	   data: "region=" + region + "&type=Events&cacheBuster=" + Math.random()
	    });

		request.done(function(msg){
			$('#event-select-cat-side').find('option').remove();

			jsonArray = $.parseJSON(msg);

			$.each(jsonArray, function(idx, obj){
				if( obj.COUNT > 0){
					$('#event-select-cat-side')
	         .append($("<option></option>")
	         .attr("value",obj.CATID)
	         .text(obj.NAME.replace(/&amp;/, "&")));
				}			

			});

			$('#event-select-cat-side').val('All');
			
			$('#event-select-cat-side option:selected').prependTo('#event-select-cat-side');

			$('.kidsCats-side').empty();
		});
}

function createCatDropDown(){


	var request = $.ajax({
	   type: "GET",
	   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setCats",
	   data: "region=" + $('#region-side').val() + "&type=" + $('#searchType').val() + "&cacheBuster=" + Math.random()
	    });

		request.done(function(msg){
			$('#select-cat-side').find('option').remove();

			jsonArray = $.parseJSON(msg);

			$.each(jsonArray, function(idx, obj){
				if( obj.COUNT > 0){
			     $('#select-cat-side')
			         .append($("<option></option>")
			         .attr("value",obj.CATID)
			         .text(obj.NAME.replace(/&amp;/, "&") + ' (' + obj.COUNT + ')'));
			  }
			});

			$('#select-cat-side').val('All');
			
			$('#select-cat-side option:selected').prependTo('#select-cat-side')

			$('.kidsCats-side').empty();
		});
}

function setSubCats(e) { //alert (e.id)
	var runAjaxCall = false;
	if (e.id == 'acc-region-side'){
		var pid = $('#accomidationsParentCat').val();
		var region = e.value;
		var searchType = 'accom';
		runAjaxCall = true;
	}
	else if (e.id == 'select-cat-side') {
		var pid = e.value;
		var region = $('#region-side').val();
		var searchType = 'act';
		var targetElement = "#kids-subCats-side";
		
		
		
		
	var request = $.ajax({
	   type: "GET",
	   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setSubCats",
	   data: "parentId=" + pid + "&type=" + searchType + "&cacheBuster=" + Math.random() + "&region=" + region
	    });

		request.done(function(msg){

			$(targetElement).empty();

			jsonArray = $.parseJSON(msg);

			$.each(jsonArray, function(idx, obj){
				if( obj.COUNT > 0 ){
					$(targetElement)
				    	.append($("<option></option>")
				        .attr("value",obj.CATID)
				        .text(obj.NAME + ' (' + obj.COUNT + ')'));
						//setHash('subCats:' + obj.CATID);
				}
				else if(obj.NAME == 'All Types'){
					$(targetElement)
				    	.append($("<option></option>")
				        .attr("value",obj.CATID)
				        .text(obj.NAME));	
								
					}
			});

			$(targetElement).val('All');
			
			$(targetElement+' option:selected').prependTo(targetElement);
			
			
				
					
					
			
					

		});

		
		
		
		
	}
	else {
		var pid = e.value;
		var region = $('#region-side').val();
		var searchType = 'Events';
		runAjaxCall = true;
	}

	if (runAjaxCall) {
		var request = $.ajax({
		   type: "GET",
		   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setSubCats",
		   data: "parentId=" + pid + "&type=" + searchType + "&cacheBuster=" + Math.random() + "&region=" + region
		    });

			request.done(function(msg){

				$('.kidsCats-side').empty();
				
				$('.event-subCats-select').find('option').remove();
				
				jsonArray = $.parseJSON(msg);

				if (jsonArray.length > 0) 
				{
					$.each(jsonArray, function(idx, obj){

						// event specific processing
						if(e.id == 'event-select-cat-side'){

							if(obj.NAME == 'All Types'){
								$('.event-subCats-select')
					        .prepend($("<option></option>")
					        .attr("value",obj.CATID)
					        .text(obj.NAME.replace(/&amp;/, "&")));

							}
							else {
									$('.event-subCats-select')
					         .append($("<option></option>")
					         .attr("value",obj.CATID)
					         .text(obj.NAME.replace(/&amp;/, "&")));
					    }
						}

						else {
							//activities and Accommodations processing

							if(obj.NAME == 'All Types'){  
								if( obj.COUNT > 0){$('#acctype').empty();  $('#lodging-types').prepend('<h5 id="acctype">Types of Accommodations</h5>');
								 $('#acttype').empty(); $('#activity-types').prepend('<h5 id="acttype">Types of Activity</h5>');
									$(".kidsCats-side").prepend('<li class="all"><input type="checkbox" class="checkbox-option"   name="subCats" value="' + obj.CATID + '">' + obj.NAME.replace(/&amp;/, "&") +  ' (' + obj.COUNT + ')</li>');
								}
								else {$('#acttype').empty(); $('#acctype').empty(); }
							}
							else { 
								if( obj.COUNT > 0){
									$(".kidsCats-side").append('<li class="checkbox-option"><input type="checkbox"   name="subCats" value="' + obj.CATID + '">' + obj.NAME.replace(/&amp;/, "&") +  ' (' + obj.COUNT + ')</li>');
								}
							}
						}
						
					}); // end each loop

				}
				
				
					$(  '.checkbox-option' ).click(function() {    
				
						
				
					  var input = $(this).toggleClass('selected').find('input');
				
					  if(input[1]){ 
				
						input[1].checked = (input[1].checked) ? false : true;
				
						input[0].value = (input[1].checked) ? 'true' : 'false';
				
					  }
				
					  else{  
				
						input[0].checked = (input[0].checked) ? false : true;
				
						var parentList = input.parent().parent().parent();
				
						parentList.find('.master-checkbox')[0].value = getMultiValueOverride(input[0]);
				
						//console.log(parentList.find('.master-checkbox')[0]);
				
					  }
				
					  });


			}); // end request.done
	}
}


function setAccomCats() {

/**/
	 var pid = $('#accomidationsParentCat').val();
	
	//pid = 'FDF84D65-AD6F-EA2B-7240C6E92B161353';
		var region = $('#acc-region-side').val();
		var searchType = 'accom';
	// alert(region + pid);
	 
	var request = $.ajax({
	   type: "GET",
	   url: "/en/includes/themes/gohawaii/display_objects/custom-search/_components/remote.cfc?method=setSubCats",
	   data: "parentId=" + pid + "&type=" + searchType + "&cacheBuster=" + Math.random() + "&region=" + region
	    });


		request.done(function(msg){

			//$('.kidsCats-side').empty();
			
			
			jsonArray = $.parseJSON(msg);

			
						//activities and Accommodations processing
if (jsonArray.length > 0) 
			{
				$.each(jsonArray, function(idx, obj){ 	
					
						if(obj.NAME == 'All Types'){  
							if( obj.COUNT > 0){$('#acctype').empty();  $('#lodging-types').prepend('<h5 id="acctype">Types of Accommodations</h5>');
							 $('#acttype').empty(); $('#activity-types').prepend('<h5 id="acttype">Types of Activity</h5>');
								$(".kidsCats-side").prepend('<li class="all"><input type="checkbox" class="checkbox-option" name="subCats" value="' + obj.CATID + '">' + obj.NAME.replace(/&amp;/, "&") +  ' (' + obj.COUNT + ')</li>');
							}
							else { $('#acttype').empty(); $('#acctype').empty(); }
						}
						else { 
							if( obj.COUNT > 0){
								$(".kidsCats-side").append('<li class="checkbox-option"><input type="checkbox"   name="subCats" value="' + obj.CATID + '">' + obj.NAME.replace(/&amp;/, "&") +  ' (' + obj.COUNT + ')</li>');
							}
						}
			 })
			}
						
		

				$(  '.checkbox-option' ).click(function() {    
			
					
			
				  var input = $(this).toggleClass('selected').find('input');
			
				  if(input[1]){ 
			
					input[1].checked = (input[1].checked) ? false : true;
			
					input[0].value = (input[1].checked) ? 'true' : 'false';
			
				  }
			
				  else{  
			
					input[0].checked = (input[0].checked) ? false : true;
			
					var parentList = input.parent().parent().parent();
			
					parentList.find('.master-checkbox')[0].value = getMultiValueOverride(input[0]);
			
					//console.log(parentList.find('.master-checkbox')[0]);
			
				  }
			
				  });

				

		}); // end request.done
}



function setTab(e) {
	//item being passed in is the TabId
		$("#search-act").removeClass("show").addClass("hide");
		$("#search-accom").removeClass("show").addClass("hide");
		$("#search-events").removeClass("show").addClass("hide");
		
		$('.kidsCats-side').empty();

		if (e == 'act'){
			
		
			$("#search-act").removeClass("hide").addClass("show");
				
			$("#nav-dir").removeClass("nav-dir-activities").addClass("nav-dir-activities");
			$("#nav-dir").removeClass("nav-dir-lodging");
			$("#nav-dir").removeClass("nav-dir-events");
			createCatDropDown();

		}

		if (e == 'accom'){
			$("#search-accom").removeClass("hide").addClass("show");
			//$('#acc-region').trigger( 'change' );
			$("#acc-region").removeClass("hide").addClass("show");
			
			
			$("#nav-dir").removeClass("nav-dir-events");
			$("#nav-dir").removeClass("nav-dir-activities");
		 	$("#nav-dir").removeClass("nav-dir-lodging").addClass("nav-dir-lodging");
			setAccomCats();	

		}

		if (e == 'events' || e == 'event'){
			
			$("#search-events").removeClass("hide").addClass("show");
			
			$("#nav-dir").removeClass("nav-dir-activities").addClass("nav-dir-events");
			$("#nav-dir").removeClass("nav-dir-lodging");
			$("#nav-dir").removeClass("nav-dir-activities");
		}

}

function dateChange(e) {

	switch (e.value) {
	  case 'dateRange':
	   /* $("#start-date").removeClass().addClass("show");
	    $("#end-date").removeClass().addClass("show");*/
		$("#date-field-start").removeClass().addClass("show");
		$("#date-field-end").removeClass().addClass("show");
	    break;
	  default:
	    /*$("#start-date").removeClass().addClass("hide");
	    $("#end-date").removeClass().addClass("hide");*/
		$("#date-field-start").removeClass().addClass("hide");
		$("#date-field-end").removeClass().addClass("hide");
	    break;
	}
}

