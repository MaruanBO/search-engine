<script type="text/javascript">
 
	
 
	$(window).ready(function() {$('#toggleDiv').hide(1);
		 /*  
		 $("#f1").hide();
			$("#f2").hide();
			$("#f3").hide();
			$("#f4").hide();
			$("#f5").hide();	*/ 
