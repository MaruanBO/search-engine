<script>
 
  function dynamicSelect(zip)  
{  
	
	$.get( "/remote/forecast-data.cfm",{"zip":zip}, function( data ) {
	//alert(zip);
	  $( ".islandweather" ).html( data);
	
		
	});
	
	$.get( "/remote/weather-data.cfm",{"zip":zip}, function( data ) {
	
	  $( ".islandweathernow" ).html( data);
		
	});
	
}

</script>
