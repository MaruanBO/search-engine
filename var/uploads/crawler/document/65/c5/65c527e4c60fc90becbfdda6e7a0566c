<script>
 
$("#butt").click(function () { 


	$("#toggleDiv").toggle("fast");
	var n = $('.bla').text();
	/*var togglestate =  $('#butt').addClass('forecast-toggle');*/
	
	 	if (n =='Forecast'){ 
		   $('.bla').text('Collapse');
			var n = $('.bla').text('Collapse');
	var togglestate =  $('#butt').addClass('forecast-collapse');
			 }
			 else
		{
		   $('.bla').text('Forecast');
		   var n = $('.bla').text('Forecast');
		   	var togglestate =  $('#butt').removeClass('forecast-collapse');
			 }
			 
	
	
	
});
