	});

	function generateTabs(tabs) { 
		html = '';
		for (var i in tabs) { 
			tab = tabs[i];
			html = html /*+ '<li class="multipage_tab"><a rel="#header" href="#header" onclick="return $(\'#multipage\').gotopage(' + tab.number + ');$(\'#multipage\').validate();">' + tab.title + '</a></li>'*/;				
		}
		$('<ul class="multipage_tabs" id="multipage_tabs">'+html+'<div class="clearer"></div></ul>').insertBefore('#multipage');
			/*$('<ul class="multipage1_tabs" id="multipag1e_tabs">'+html+'<div class="clearer"></div></ul>').insertBefore('#multipage1');*/
	}
	
	function setActiveTab(selector,page) {  
		$('#multipage_tabs li').each(function(index){ 
			if ((index+1)==page) {  
				$(this).addClass('active');
				
			} else {
				/*$(this).removeClass('active');*/
				$(this).addClass('inactive').addStyle('display:none');
			}
		});
		
					
	}
	 
	function transition(from,to) {	 
		$(from).fadeOut('fast',function(){$(to).fadeIn('fast');});
	
	}
	
	function textpages(obj,page,pages) {  
		$(obj).html(/*page + ' of ' + pages*/);	
	}
	
	function hideit(){
		 
			
			 
			
			
			$('#f1').hide();
			
			$('#f2').hide();
			$('#f3').hide();	
			$('#f4').hide();
			$('#f5').hide();
	}
	
	
 
</script>           
