<script type="text/javascript">
    var chkContinueSelected = "";
    function SpeedBump(num) {
        chkContinueSelected = num;
        $("#main-overlay-speedbump #SpeedBump").show();
        $("#main-overlay-speedbump .wid-speed-overlay").show();
            if (chkContinueSelected == "1") {
                $("#speedLinkUrl").text('http://www.consumerreports.org/cro/2013/07/prepaid-cards-fees/index.htm');
            }
            if (chkContinueSelected == "2") {
                $("#speedLinkUrl").text('http://abclocal.go.com/kabc/story?section=news/consumer&id=9190474');
            }
            if (chkContinueSelected == "3") {
                $("#speedLinkUrl").text('https://www.healthcare.gov/');
            }
        
    }

    function CloseSpeedBump() {
        chkContinueSelected = "";
        $("#main-overlay-speedbump #SpeedBump").hide();
        $("#main-overlay-speedbump .wid-speed-overlay").hide();
    }


    $(function () {
        $(".lnkShowSpeedPop").click(function () {
            var chkselnum = $(this).attr("rel");
            SpeedBump(chkselnum);
        });

        $("#SpeedBumpClose").click(function () {
            CloseSpeedBump();
        });
        $("#SpeedBumpCont").click(function () {

            if (chkContinueSelected == "1") {
                window.open("http://www.consumerreports.org/cro/2013/07/prepaid-cards-fees/index.htm");
            }
            if (chkContinueSelected == "2") {
                window.open("http://abclocal.go.com/kabc/story?section=news/consumer&id=9190474");
            }
            if (chkContinueSelected == "3") {
                window.open("https://www.healthcare.gov/");
            }

            CloseSpeedBump();

        });


        var timeout;
        var delay_time = 7000;
        var trans_time = 800;
        var banner_index = 1;// On pageload the first banner is loaded so loop starts with 2nd banner.
        var banner_order = new Array("#section-landing", "#tax-landing", "#section-reload-at-the-register", "#paybill-payrent-landing", "#section-send-money");
       
        slideshow($(banner_order[banner_index]));
        function slideshow(el) {
            timeout = setTimeout(function () {
                for (var foKey in banner_order) {
                    if (foKey != banner_index) {
                        $(banner_order[foKey]).stop(true, true);
                        $(banner_order[foKey]).fadeOut(trans_time);
                    }
                }
                banner_index++;
                if (banner_index >= banner_order.length) banner_index = 0;
                $(el).stop(true, true);
                $(el).fadeIn(trans_time, slideshow(banner_order[banner_index]));
            }, delay_time);
        }


       $("#lnkFallSweep, #lnkFallSweep2, #lnkFallSweep3").click(function () {
            window.open("/greendot/page/fall#officialrules");
        }); 
    });
</script>
