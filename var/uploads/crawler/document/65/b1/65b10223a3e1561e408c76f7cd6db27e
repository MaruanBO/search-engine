<script type="text/javascript">

    $('#loginModel_TxtPassword, #loginModel_TxtAccountNoOrUserId').focusout(function (event) {
        if (this.id == "loginModel_TxtAccountNoOrUserId") {
            var checkRule = '(?!^[0-9]*$)^([a-zA-Z0-9]{6,12})$';
            var errors = "<div class='messages ui-state-error'>" + "Incorrect User ID. Please re-enter your information." + "</div>";
        } else {
            var checkRule = '(?!^[0-9]*$)(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{8,16})$';
            var errors = "<div class='messages ui-state-error'>" + "Incorrect Password. Please re-enter your information." + "</div>";
        }
        ValidateUserIdPasswordFormat(this.id, checkRule, errors);
    });

    $("#btnLogOn").click(function (event) {
        if (!isValidFormat) {
            event.preventDefault();
        }
        else {
            $(".alert").hide();
        }
    });


