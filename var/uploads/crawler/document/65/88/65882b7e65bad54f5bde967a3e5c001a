        function onAfter(curr, next, opts) {
            // commented out by cricketts 2.11.11
            //var index= opts.currSlide;
            //$('#prev')[index== 0 ? 'hide' : 'show']();
            //$('#next')[index== opts.slideCount - 1 ? 'hide' : 'show']();
            
                imageItem = $('#slideshow').find('.business').eq(opts.currSlide).find('.logo-clip img');
                iWidth = imageItem.width();
                iHeight = imageItem.height();
                
                if(iWidth && iHeight){
                    if(iWidth > iHeight){
                        imageItem.css("width", "150px");
                        imageItem.css("height", "auto");
                    } else {
                        imageItem.css("width", "auto");
                        imageItem.css("height", "80px");
                    }
                }
        }
        function pagerFactory(idx, slide) {
            var s= idx> 5 ? ' style="display:none"' : '';
            return '<li'+s+'><a href="#">'+(idx+1)+'</a></li>';
        };
    
    
