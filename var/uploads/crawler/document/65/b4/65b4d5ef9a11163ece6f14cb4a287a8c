	var currPage  = window.location.pathname;
	var redirPage = $('#redirLayout').attr('href');
	var params	  = window.location.search;
	params		  = params.replace('&site_preference=NORMAL','').replace('?site_preference=NORMAL','').replace('?','').replace('&&','&');
	redirPage	  = redirPage.replace('/',currPage);
	if(params != '' && params != null)
	{
		redirPage = redirPage+"&"+params;
	}
	$('#redirLayout').attr('href',redirPage);
