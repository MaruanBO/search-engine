<script type="text/javascript">
$(document).ready(function() {
	$('html').click(function(e) {
			 if( $('#newloginblock').is(':visible') ) { 
					$('#newloginblock-top').removeClass('newloginblock-top-background');
					$('#newloginblock').slideToggle('fast');
			 }
			 
	});
	
	$('#newloginblock-top, #newloginblock').click(function(event){
		if( $('#newloginblock').is(':visible') ) {
			  event.stopPropagation();
		}
	});
		  
	$("form#login-newhome, #find-doctor").jqTransform();
	
   
	$('#zip1').focus(function() {
		$('.error_zip').fadeOut('fast');
	})
	
	$('#userid').focus(function() {
        $('.error_userid').fadeOut('fast');
    })
    
    $('#password').focus(function() {
        $('.error_password').fadeOut('fast');
    })
    
	$('#find-doctor-results1').submit(function() {
		var fadblockerrors = 0;
		dcsMultiTrack('DCSext.event_type', 'loginblock-fad-search','WT.dl','100');
        try{
            __adroll.record_user({"adroll_segments": "search_conversion"})    
        } catch(err) {} 
      
      /* Validation */
      
      if($('#zip1').val() == '' || $('#zip1').val() == 'Zip Code') {
      	$('.error_zip').fadeIn('fast');
      	++fadblockerrors;
      	
      }  
      if (!(/^\s*\d{5}\s*$/.test($('#zip1').val()))) {
    	$('.error_zip').fadeIn('fast');
    	++fadblockerrors;
      }
      if (fadblockerrors > 0) {
    	  return false;      
      } 
        
	});

	$("#countrySelect").bind('click',function(e) {
		e.preventDefault();
		$("#countrySelectModal").modal({
			close		:	true,
			persist		:	true,
			containerId	:	'wideDialogContainer',
			overlayId	:	'dialogOverlay',
			minHeight	:	250
		});
	});
  
  });  
</script>

