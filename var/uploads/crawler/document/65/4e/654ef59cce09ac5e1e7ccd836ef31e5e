<script>
/* TOGGLE FLYOUT SCRIPT - REQUIRES JQUERY UI for TOGGLECLASS() */
	function toggleHeaderFlyOut( target ){
		var shell = $(target);
		if( shell.hasClass('xctiveFlyout') ){ 
			$('.xctiveFlyout :text').focus().blur();
			$(".activeFlyout").switchClass("activeFlyout","xctiveFlyout",250,"swing");
			shell.switchClass("xctiveFlyout","activeFlyout",250,"swing");
		}
		if( shell.hasClass('activeFlyout') ){ 
			shell.switchClass("activeFlyout","xctiveFlyout",250,"swing");
		}
	}
</script>

