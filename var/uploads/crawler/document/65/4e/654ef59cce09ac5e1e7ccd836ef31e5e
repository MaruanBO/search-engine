var vClickProtected = false;
var vUnappliedCheck = false;
var securePages = "retail-checkout.w,new-card.w,olc-cart-fm.w,retail-gift-wrap.w,retail-review.w,retail-login.w,retail-ccard.w,retail-account.w";


