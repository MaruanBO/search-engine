	<script type="text/javascript">
		$('.search input, input.search').keydown(function(e){
			if( e.keyCode == 13 ){
				e.preventDefault();
				search();
			}
		});
		function search()
		{
			var url = "/search/";
			var q = $("#q").val();
			
			url += "?q=" + q.replace(/ /g,  "+");
			
			if( q != '' && q != "'" && q != '"' && q != null && q != undefined && q != 'undefined' && q != false ){
				window.location = url;
			}
		}
		function searchCancel()
		{
			toggleHeaderFlyOut( '#searchBoxFlyOutShell' );
			$("#q").val('');
		}
	</script>

