/* KEY VALUE FROM QUERY STRING FUNCTION (Programmer: Nick Barcomb 12/27/2013) */
keyValURI = function(u){
	var uakv = {};
	var ua = u.replace('?','').split('&');
	for(i=0;i<ua.length;i++){
		var kv = ua[i].split('=');
		uakv[kv[0]] = kv[1];
	}
	return uakv;
}

blankField = function(id, str)
{
	var r = false;
	if( $(id).val().match(/^(\s+)$/) || $(id).val() === '' ){
		alert( 'Please provide ' + str );
	}else{
		r = true;
	}
	return r;
}

golink = function(vLink,vTarget)
{
   var vResetFlag;
  if (!vClickProtected){
    vClickProtected = true;
 if (vUnappliedCheck)
 {
     if (isUnapplied() && saveUnappForm())
        return;
 }
  var hostUrl  = hostDomain;
  var sHostUrl = shostDomain;
  var host     = "";
  var sPages   = securePages.split(",");
  var vParams=""; 
  var vStart=vLink.indexOf("?") + 1; 
  var vEnd=vLink.length; 
  if (vStart>0 && vEnd>vStart && vLink.indexOf("?") != -1)
 {
   vParams = vLink.substring(vLink.indexOf("?") + 1,vLink.length); 
   vLink = vLink.substring(0,vLink.indexOf("?")); 
  }
  for (var i=0; i< sPages.length; i++)
  {
	
     if (vLink.indexOf(sPages[i]) > -1)
     {
        host = sHostUrl;
        break;
     }
  }
  if (host == "")
     host = hostUrl;
  if(vParams.indexOf("=")!=-1)
     vParams = vParams + "&frames=no&target=" + vTarget; 
  else 
     vParams = vParams + "frames=no&target=" + vTarget; 
  vParams = vParams + "&sponsor=" + fdmSponsor;
  if(vTarget=="top")
  {
    document.location.href = host + appPath + "/" + vLink + "?" + vParams; 
  }
  else
  {
  if (hostUrl != host)
     top.document.location.href=host + appPath + "/site.w?location=" + vLink + "&" + vParams;
  else
     document.location.href=host + appPath + "/site.w?location=" + vLink + "&" + vParams;
 }
}
else
   vResetFlag = setTimeout("vClickProtected = false;",3000);
 }

function validEmail(emailObj)
{ 
var str  = emailObj.value; // email string 
var reg1 = /(@.*@)|(\.\.)|(@\.)|(\.@)|(^\.)/; // not valid 
var reg2 = new RegExp("^.+\\@(\\[?)[a-zA-Z0-9\\-\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$"); // valid 
if (!reg1.test(str) && reg2.test(str)) // if syntax is valid
{  
return true;
} 
else
{
alert("Please enter a valid email address to continue");
selectField(emailObj);
return false;
}
}

/* 
	Nick Barcomb 03-12-2014 
	alternative to the legacy validEmail() above which contains an unwanted error alert()
	TODO: convert all cases of validEmail to validEmailCheck and make error handling local.
*/
function validEmailCheck(emailObj){ 
var str  = emailObj; // email string 
var reg1 = /(@.*@)|(\.\.)|(@\.)|(\.@)|(^\.)/; // not valid 
var reg2 = new RegExp("^.+\\@(\\[?)[a-zA-Z0-9\\-\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$"); // valid 
if (!reg1.test(str) && reg2.test(str)) // if syntax is valid
{  
return true;
} 
else{
	return false;
}
}



//
//  added from AjaxProcessing.js to combine multiple JS files - Dave 10/30/13
//
function ajaxProcessing(vUrl,vFieldList,vStaticList) {
	if (typeof appPath == "undefined" || typeof hostDomain == "undefined")
		return;
	
	vFieldArr = vFieldList.split(",");
	vFieldList = "";
	if(vStaticList != null){
		vStaticArr = vStaticList.split(",");
	}else{
		vStaticArr = [];
	}

	$.blockUI({ message: '<div id="processing"><div style="padding: 10px 0px 0px 0px;"><img src="/' + imgPath + '/loaders/loader50x50-onWhite.gif"></div><div style="padding: 10px 0px 10px 3px; font-size: 14px; font-weight: bold; ">Processing Your Request. Please Wait...</div></div>' });

	$.each(vFieldArr,function(i) {
		var fieldName = $("#" + vFieldArr[i]).attr('name');
		var fieldValue = $("#" + vFieldArr[i]).val();
		if ($("#" + vFieldArr[i])){
			if (vFieldList == "")
				vFieldList += fieldName + "=" + fieldValue;
			else
				vFieldList += "&" + fieldName + "=" + fieldValue;
		}
	});
	$.each(vStaticArr,function(j) {
		var staticKeyValue = vStaticArr[j];
		if ( staticKeyValue && staticKeyValue != '' && staticKeyValue.match(/^([a-zA-Z0-9]+\=[a-zA-Z0-9]+)$/g) ){
			vFieldList += "&" + staticKeyValue;
		}
	});
	vFieldList += "&sponsor=" + fdmSponsor;

	if( vUrl.match(/(http\:\/\/www\.sharkclean\.com\/cgi\-bin\/livew\/b2c\/)/g) ){
		ajaxUrl = vUrl;
	}
	else{
		ajaxUrl = hostDomain + "" + vUrl;
	}

	$.ajax({
		type: "GET",
		url: ajaxUrl,
		dataType: "xml",
		data: vFieldList,
		complete: function(data) {
			var vMessage = $.xmlToJSON(data.responseXML).response[0].message;
			var vHtml = '<div style="padding: 10px; font-size: 14px; font-weight: bold; ">' + vMessage + '</div><div style="padding: 10px;"><input style="margin: 10px;" type="button" name="ok" id="ok" onclick="javascript:hideProcessing();" value="OK"></div>';
			
			$("#processing").html(vHtml);
		}
	});
}

function hideProcessing() { $.unblockUI(); }

/* EMAIL SIGNUP VALIDATION & PROCESSOR */

var submitOK = true;
var submitTimeOut;
function signup( field, errorShell, staticVarValue ) {

	var staticVarKey = 'promo';

    var formField = $('#' + field);
    var formFieldParentForm = formField.parents('form');
    var errorObj = $('#' + errorShell);
    var placeholder = formField.attr('placeholder');

	errorObj.empty().hide();

    if (formField.val() == "" || formField.val() == placeholder) {
    	/* Check for empty */
    	errorObj.append("* Please enter your email address to sign up.").show();
        formField.focus();
        return;
    }else if( !validEmailCheck(formField.val()) ){
    	/* Check for valid */
    	errorObj.append("* A valid email address required.").show();
        formField.focus();
        return;
    }else{
    	/* Stop spaam */
    	if( submitOK == true ){
		    var vCurrTime = new Date();
		    var vCurrSecs = (((vCurrTime.getHours() * 60) + vCurrTime.getMinutes()) * 60) + vCurrTime.getSeconds();
		    var vNoCache = "&nocache=" + vCurrSecs;
		
		    var formFields = formFieldParentForm.serialize();
		    formFields += "&emailList=YES";
		
		    ajaxProcessing('cgi-bin/livew/b2c/subscription-work.w', field , staticVarKey + '=' + staticVarValue);
	    	submitOK = false;
	    	submitTimeOut = setTimeout("submitOK = true;",10000);
	    }
    }

} //signup()

function emailSignup() {
    if (validEmail(document.getElementById("txtEmailAddress"))) {
        ajaxProcessing('cgi-bin/livew/b2c/subscription-work.w', 'txtEmailAddress,promo');
        document.getElementById("txtEmailAddress").value = "Your email";
    }
}


