        function authenticateSession(redirectUri, reloadWindow) {
			
            $.getJSON(redirectUri, function (result) 
            {	
                if(result===null || result.session===null)
                {
                    //  alert("Unable to get the Token from janrain");
                    //  window.location.reload();
                    $("#janrainError").css('visibility', 'visible');
                    $("#janrainRegisterError").css('visibility', 'visible');
                }
                else 
                {
                    janrain.capture.ui.createCaptureSession(result.session.accessToken);
                    if (reloadWindow) 
                    {
                        $("#janrainError").css('visibility', 'hidden');
                        $("#janrainRegisterError").css('visibility', 'hidden');
                        window.location.reload();
                    }
                }
            }).fail(function( jqxhr, textStatus, error ) {
                // window.location.reload();
                $("#janrainError").css('visibility', 'visible');
                $("#janrainRegisterError").css('visibility', 'visible');
            });
        }

        function completeLogin() {
            
            window.location.reload();
        }
        
       
        function janrainCaptureWidgetOnLoad() {
            janrain.capture.ui.start();
            mattel_janrain_alter(); 


            janrain.events.onCaptureScreenShow.addHandler(function (result) {

                setUTagView(result.screen);

            });
			
            //janrainCaptureWidgetOnLoad();
			


            janrain.events.onCaptureSuccessfulLogin.addHandler(function (result) {		
			
                authenticateSession(result.redirectUri, true);
                setUTagOnLoginSuccess(result);

				
            });

            janrain.events.onCaptureSuccessfulRegistration.addHandler(function (result) {
                authenticateSession(janrain.settings.capture.redirectUri + "?code=" + result.authorizationCode, false);
				
			    
                setUTagOnRegistrationSuccess(result);
                janrain.events.onModalClose.addHandler(function () {
                    // completeLogin();
                });
            });

        }
    var janrainLocaleName = $('#localeName').val();
    function login() {
        //alert("Test");
        janrain.capture.ui.modal.open();
        //document.location.href("\en-us\loginpage.html");
    }

    function register() {
        janrain.capture.ui.modal.open('createAccount');
               
    }

    function logout() {
        janrain.capture.ui.endCaptureSession();
        if (!janrain.settings.capture.federate) {
            var redirectLocale ="/" + janrainLocaleName + "/log-out";
            window.location.assign(redirectLocale);
        }
    }        

