            $(".carousel-wrap ul li").click(function (e) {
                dataLayer.push({ "event": "gaEvent", "eventCategory": "Home Modules", "eventAction": "Click", "eventLabel": "Home: Module " + ($(this).index() + 1), "eventValue": 1 });
                e.preventDefault();

                var link = $(this).children("a");
                if (link != null) {
                    if (link.attr("target") == "_blank") {
                        window.open(link.attr("href"));
                    }
                    else {
                        window.location.href = link.attr("href");
                    }
                }
            });

            $("#homebanner").click(function (e) {
                var link = $(this);

                dataLayer.push({ "event": "gaEvent", "eventCategory": "Home Hero", "eventAction": "Click", "eventLabel": "Home: Hero " + $(this).attr("href"), "eventValue": 1 });
                e.preventDefault();

                
                if (link != null) {
                    if (link.attr("target") == "_blank") {
                        window.open(link.attr("href"));
                    }
                    else {
                        window.location.href = link.attr("href");
