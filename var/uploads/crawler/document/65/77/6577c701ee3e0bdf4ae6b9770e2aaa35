        function onBeforeshowcasePlaylistbloxmultimediavideohd(curr, next, opts){
            $(curr).removeClass("active");
    		
                $(next).find(".image").find('img').css({'opacity':0});
            
        }

	// AFTER TRANSITION
	function onAftershowcasePlaylistbloxmultimediavideohd(curr, next, opts) {
            

            $(next).addClass("active");

            $('#showcasePlaylist-blox-multimedia-video-hd .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

                if(!$(next).hasClass("init")){
                    $(next).find(".image").each(function(){

                     letterboxImage(this);

                    });
                }
            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#showcasePlaylist-blox-multimedia-video-hd .item-list').cycle({
		fx: effectshowcasePlaylistbloxmultimediavideohd,
		sync: true,
		fit: 1,
		width: 585,
		timeout: 0,
                before: onBeforeshowcasePlaylistbloxmultimediavideohd,
		after:	onAftershowcasePlaylistbloxmultimediavideohd,
        pause:true,
		pager:'#showcasePlaylist-blox-multimedia-video-hd .nav',
		prev:'#showcasePlaylist-blox-multimedia-video-hd .prev',
		next:'#showcasePlaylist-blox-multimedia-video-hd .next'
	});
        

        

function letterboxImage(image){
    if($(image).filter(':in-viewport').length){

                var aHeight = $(image).height();
                var aWidth = $(image).width();

                var imgHeight = $(image).find("img").height();
                var imgWidth = $(image).find("img").width();
                var aspectRatio = imgHeight/imgWidth;

                if(imgHeight<=aHeight){ // too short
                    if(imgWidth>aWidth){ // if the image is too wide resize it to fit
                        $(image).find("img").css({'height':'auto','width':aWidth+'px'});
                        var imgHeight = Number($(image).find("img").height());
                    }

                    // letter box the image
                    $(image).find("img").css({'position':'relative','top':Math.ceil((aHeight-imgHeight)/2)+'px'});
                }else{ // too tall
                    $(image).find("img").css({'height':aHeight+'px','width':'auto'});
                    var imgHeight = $(image).find("img").height();
                    var imgWidth = $(image).find("img").width();

                    if(imgWidth<=aWidth){ // too wide
                        $(image).find("img").css({'height':aHeight+'px','width':'auto'});

                        var aWidth = $(image).width();
                        var imgWidth = $(image).find("img").width();

                        if(imgWidth<aWidth){
                            var imgPadding = Math.ceil((aWidth-imgWidth)/2);
                            $(image).css({'padding-left':imgPadding+'px'});
                        }
                    }
                }
                }
}

$(window).scroll(function(){
    var groups = $('#showcasePlaylist-blox-multimedia-video-hd .item-list .group.init');
    if(groups.length){
        if(groups.filter(':in-viewport').length){
            $('#showcasePlaylist-blox-multimedia-video-hd .item-list .group.init').filter(':in-viewport').each(function(){

                $(this).find(".image").each(function(){
                   letterboxImage(this);
                });

                $(this).removeClass("init");
            });
        }
    }
            });
            

}



