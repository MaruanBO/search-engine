<script>
    $(function () {
        var numSlides = 03;
        $('.topRatedInfo .image-link img.fitfill').imgFitFill({mode:'fill'});
        $('.top-news-item .image-link img.fitfill').imgFitFill({mode:'fill'});
        $('.news-item .title').equalHeight();
        $('#category-brand-tabs').tabs();
        $('#top-rated-cars-slide').jCarouselLite({
            pauseOnHover: true,
            visible: 1,
            speed: 500,
            circular: false,
            easing: 'easeInOutExpo',
            afterEnd: function (a) {
                var slideId = a.attr('id'),
                    slideNum = parseInt(slideId.substring(slideId.length - 1)); // slide-2
                if (slideNum > numSlides) {
                    slideNum = 1;
                }
                else if (slideNum < 1) {
                    slideNum = numSlides;
                }
                $('#top-rated-cars ul.tabs-dots > li > a.ui-tabs-selected').removeClass('ui-tabs-selected');
                $('#top-rated-cars ul.tabs-dots > li > a.' + slideNum).addClass('ui-tabs-selected');
            },
            btnNext: '#slide-cnt div.next',
            btnPrev: '#slide-cnt div.prev',
            btnEvent: 'click',
            btnGo: [
                'ul.tabs-dots > li > a.1',
                'ul.tabs-dots > li > a.2',
                'ul.tabs-dots > li > a.3',
                'ul.tabs-dots > li > a.4',
                'ul.tabs-dots > li > a.5'
            ]
        });
        $('#top-rated-cars > ul.tabs-dots > li > a.1').addClass('ui-tabs-selected');
        $('.likes-dislikes li').trimList({ relativeToSelector: '.topRatedInfo', fromBottom: 20 });
    });
</script>

