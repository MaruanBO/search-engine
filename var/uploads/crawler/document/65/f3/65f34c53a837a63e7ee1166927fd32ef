            $('li.usedlistings-cities a[title]').qtip({
                style: {
                    width: { max:250, min:110 },
                    padding: 1,
                    background: '#423c3c',
                    color: '#FFFFFF',
                    textAlign: 'center',
                    border: { width: 1, radius: 3, color: '#3a3434' },
                    tip: true,
                    name: 'light' // Inherit the rest of the attributes from the preset light style
                },
                show: {
                    when: {event: 'mouseover' },
                    ready: false
                },
                hide: {
                    when: { event: 'mouseout' }
                },
                content: { text:false },
                position: {
                    corner: { tooltip: 'topMiddle', target: 'bottomMiddle' }
                }
            });
        }
        $('#usedlistings0').submit(function () {
            getListing('', "0");
            return false;
        });
        UsedListing.currentModel = '';
    });
</script>
