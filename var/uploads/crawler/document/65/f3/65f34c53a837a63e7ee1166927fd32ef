<script>
(function () {
    Smooth_Reload.push({
        controller: 'member-links',
        divId: 'member-links-1',
        params: {
            showWelcome: 0,
            showroomLogin: 1        }
    });

    function updateShowroomCount(event) {
        $('.nav-showroomlink').html('(' + event.count + ')');
    }

    hgm.event.on('auth.status', function (/* hgm.auth.Status */ status) {
        if (status.isLoggedIn()) {
            Smooth_Reload.lin(
                                function () {
                    // FIXME Only do this when showroom is enabled
                    // Put login/logout callbacks into ReloadBlock
                    var showroom = hgm.showroom.user.get();
                    if (showroom) {
                        updateShowroomCount({count: showroom.count()});
                    }
                }
                            );
        }
        else {
            Smooth_Reload.lout();
        }
    });

    hgm.event.on('showroom.count', updateShowroomCount);
}());
</script>
