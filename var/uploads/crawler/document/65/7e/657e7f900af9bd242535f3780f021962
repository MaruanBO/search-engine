$(document).ready(function() {

    // Header Dropdown menus
    if($('#headerDropdowns').length > 0){
        $('#headerDropdowns select').each(function(){
            var selectIndex = $(this).index();
            var selectClass = $(this).attr('class');
            $('#headerDropdowns').append('<ul class="'+selectClass+' closed" />');

            //For Language Selection
            if(selectClass === 'languageLinks'){
                $(this).find('option').each(function(){
                    $('ul.languageLinks').append('<li><a href="'+$(this).attr('value')+'">'+$(this).text()+'</a></li>');
                    // bind to link: $('#languageForm').submit(); and push the language value
                });
                $('ul.languageLinks').prepend("<li>Select Language</li>");
