<script type="text/javascript">
    var header = $('#UA-header');
    var cont = $('.home_cont');
    var isFloatingButton = false;
    var windowWidth = $(window).width();
	var viewsNr = 428680282;
	
function getViewsCount()
{
	$.ajax({
		data: {page: 'get_views_count'},
		success: function(d) {
			if (d.code == 20){
				viewsNr = d.res;
				$('.view_counter').html(number_format(viewsNr));
				setInterval(increaseViewsCount, 6000);
			}
		}
	});
}

function increaseViewsCount()
{
	randNr = getRandNr(14, 16);
	viewsNr = viewsNr + randNr;
	$('.view_counter').html(number_format(viewsNr));
}

function getRandNr(min, max) {
  return Math.random() * (max - min) + min;
}

function number_format(number, decimals, dec_point, thousands_sep) {
  number = (number + '')
    .replace(/[^0-9+\-Ee.]/g, '');
  var n = !isFinite(+number) ? 0 : +number,
    prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
    sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
    dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
    s = '',
    toFixedFix = function(n, prec) {
      var k = Math.pow(10, prec);
      return '' + (Math.round(n * k) / k)
        .toFixed(prec);
    };
  s = (prec ? toFixedFix(n, prec) : '' + Math.round(n))
    .split('.');
  if (s[0].length > 3) {
    s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
  }
  if ((s[1] || '')
    .length < prec) {
    s[1] = s[1] || '';
    s[1] += new Array(prec - s[1].length + 1)
      .join('0');
  }
  return s.join(dec);
}


  $(document).ready(function() {
	setInterval(increaseViewsCount, 6000);
  });

function repositionCont()
{
    windowWidth = $(window).width();

    if (LOGGED) {
        scrolling();
        return;
    }

    var mt = header.height();

    if (windowWidth < 575) {
        isFloatingButton = true;
        $('#UA-header .hh_floatingBtn').addClass('hide');
        header.removeClass('shadow');
    }
    else {
        isFloatingButton = false;
        removeFloatng();
    }

    cont.css({
        marginTop: mt + 'px'
    });

    scrolling();
}

function removeFloatng(removeHideClass /* = false*/)
{
    $('.homeheader .makeabanner').removeClass('floatingBtn').removeAttr('style');
    $('#UA-header .UA-hmenu').removeClass('hidden');
    if (typeof removeHideClass != 'undefined' && removeHideClass) {
        $('#UA-header .UA-hmenu').removeClass('hide');
    }
}

function onHomeLogout()
{
    header.removeClass('logged').addClass('floating').removeAttr('style');
    $('.makeabanner').removeClass('logged');
    $('#UA-header .UA-hmenu').removeClass('hidden');
    repositionCont();
}

function scrolling()
{
    var btnTop = (isFloatingButton) ? ((parseInt($('.homeheader .hh_title').offset().top + $('.homeheader .hh_title').height()) - $('#UA-header').height() + $('.makeBtnCont .makeBtn').height())) : ((parseInt($('.makeabanner').offset().top) - $('#UA-header').height() + $('.makeabanner').height()));

    if (LOGGED) {
        if (windowWidth > 660) {
            $('#UA-header .hh_floatingBtn').addClass('hide');
            $('#UA-header .UA-hmenu').removeClass('hide');
            header.removeClass('shadow floating');
            cont.removeAttr('style');

            return false;
        }
        btnTop = parseInt($('.makeBtnCont .makeBtn').offset().top + $('.makeabanner').height());
        if (windowWidth < 575) {
            btnTop = parseInt($('.homeheader .hh_title').offset().top + $('.homeheader .hh_title').height());
        }
    }

    if ($(window).scrollTop() >= btnTop) {
        if (!isFloatingButton) {
            $('#UA-header .UA-hmenu').addClass('hide');
            $('#UA-header .hh_floatingBtn').removeClass('hide');
            header.addClass('shadow');

            if (LOGGED) {
                if (windowWidth > 575) {
                    header.addClass('floating');
                    cont.css({
                        marginTop: header.height() + 'px'
                    });
                }
                else {
                    $('.homeheader .makeabanner').addClass('floatingBtn');
                    $('#UA-header .hh_floatingBtn').addClass('hide');
                }
            }
        }
        else {
            $('#UA-header .UA-hmenu').addClass('hidden');
            if (!LOGGED) {
                $('.homeheader .makeabanner').addClass('floatingBtn').css({
                    top: ($('#UA-header').height() - 2) + 'px'
                });
            }
            else {
                $('#UA-header .hh_floatingBtn').removeClass('hide');
            }
        }
    }
    else {
        if (!isFloatingButton) {
            $('#UA-header .hh_floatingBtn').addClass('hide');
            $('#UA-header .UA-hmenu').removeClass('hide');
            header.removeClass('shadow');

            if (LOGGED) {
                header.removeClass('floating');
                cont.removeAttr('style');
            }
        }
        else {
            if (LOGGED) {
                header.removeClass('floating');
                cont.removeAttr('style');
            }
            //removeFloatng(true);
        }

        removeFloatng(true);
    }

}

$(document).bind('scroll', function(){
    scrolling();
});

repositionCont();

$(window).resize(function(){
    repositionCont();
});

</script>
                
