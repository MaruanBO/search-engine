	include("../js/app.js", ["jquery.tooltip", "jquery.validate", "jquery-ui"], function() {
		include.fireReady();
		app.page.setContext({"title":"Storefront","type":"storefront","ns":"storefront","uiComponent":"storefront","className":"pt_storefront"});
		app.product.tile = { init: function() { $(window).trigger('tileinit'); } };
		app.util.updateStateOptions = function(countrySelect) {
			var country = $(countrySelect);
			var form = country.closest("form");
			if (form) {
				var refresh = form.find('input[name="'+country.attr('id').replace('countries_country','refresh')+'"]');
				if (refresh.length) {
					refresh.attr('disabled', null);
					form.find('input[type!="hidden"]').attr('readonly', '1');
					form.find('input').each(function (index, input) {
						if (/_countries_country$/.test(this.name) || 'hidden' == this.type) return;
						if ('submit' == this.type || 0 == this.value.length) {
							this.disabled = true;
						}
					});
					form[0].submit();
					return; /* TODO AJAX REFRESH */
				}
			}
		};
	});
	include("../js/parts/smartresize.js", [include.jquery]);
	include("../js/notification.js");

	
		include("../js/pages/storefront.js");
	

	window.meta = "Shop direct from Columbia Sportswear. Buy Columbia jackets, pants, shirts, fleece, shoes, boots &amp;amp; sweaters. Visit our blog, watch videos &amp; meet our athletes.";
	window.keywords = "columbia sportswear company, apparel, clothing, clothes, shirts, shoes, jackets, pants";
</script>





