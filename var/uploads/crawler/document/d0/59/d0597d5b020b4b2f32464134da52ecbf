<script>
var userAgent = window.navigator.userAgent;

function addTohomeJS(src, cb) {
    var s = document.createElement('script');
    s.src = src;
    s.async = true;
    s.onreadystatechange = s.onload = function() {
      var state = s.readyState;
      if (!cb.done && (!state || /loaded|complete/.test(state))) {
        cb.done = true;
        cb();
      }
    };
    document.getElementsByTagName('head')[0].appendChild(s);
}

if(/iP(hone|ad)/.test(userAgent)) {
  addTohomeJS('http://www.wallstcheatsheet.com/wp-content/themes/wallstcheatsheet-v2/js/add2home.js?3e4c97', function() { 
     addToHomescreen({
        appID: 'cs_addtohome',
        message: 'Add The Cheat Sheet to your home screen now: tap %icon and then <strong>Add to Home Screen</strong>.',
        skipFirstVisit: true,
        maxDisplayCount: 1      
    });   
  });
}
</script>
