jQuery(document).ready(function($){
  var interval,
      counter = 0,
      limit = 30,
      fallback = $('#fallback');
  
  function st_check() {
    var st_div = $( "#must-read .str-adunit" );
    counter++;
      if ( ($(st_div).length > 0) && counter < limit ) {
        $(fallback).remove();

        clearInterval( interval );
      }
  }

  interval = setInterval( st_check, 500 );
 });
</script>
