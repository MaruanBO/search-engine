<script>
(function( $ ) {
  $(function() {
    $('#st-email-signup .sub-cats').hide();
    $('#st-list-subcats').hide();

    /* show selected lists */
    $('#st-email-signup input[type=checkbox]').each(function( index ) {
      if( (this).checked) {
      $(this).parents('.list-block').show();
    }
    });

    /*Make interests expand when clicked*/
    $('.st-parent-interest').click(function(event) {
      event.preventDefault();
      $(this).parent('div').find('.sub-cats').toggle();
    });

    /*Show lists when email input is focused*/
    $('#st-email').focus(function() {
      $('#st-list-subcats').show();
    });

    //set the referrer with js to avoid caching issues
    var ref = document.referrer;
    $('#referrer').val(ref);
  });
})(jQuery);
</script>
<script>

 
jQuery( document ).ready(function( $ ) {
  setTimeout(function(){
  var is_block = 'no';
   if ($('#div-cs-bb2').height() == 0) {
    console.log('ads are blocked');
    is_block = 'yes';
  }

 $('body').append('<img src="https://logs-01.loggly.com/inputs/9b168fea-86ac-4eb7-9868-112c6d35a913.gif?source=pixel&adblock='+is_block+'&screen_width='+cs_pd.screen_width+'&partner='+cs_sd.partner+'&template='+cs_sd.template+'&vertical_ad='+cs_pd.vertical_ad+'&country='+cs_sd.country+'&referrer='+document.referrer+'" />');
},2000);
});

</script>
