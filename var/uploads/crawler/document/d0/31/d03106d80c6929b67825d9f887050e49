<script type="text/javascript">
//<![CDATA[
var requestString = null;
var curSelectedChoice = null;
function pullSuggestItems() {
return (($('searchSuggests')) && ($('searchSuggests').getElementsByClassName('srchSuggestLinksBox')[0]) && ($('searchSuggests').getElementsByClassName('srchSuggestLinksBox')[0].getElementsByTagName('TD').length > 0)) ? $('searchSuggests').getElementsByClassName('srchSuggestLinksBox')[0].getElementsByTagName('TD') : false;
}
function prefillSearchStrField() {
opts = pullSuggestItems();
if (!opts) return false;
optContent = opts[curSelectedChoice].getElementsByTagName('B')[0];
if (optContent.innerHTML.length < 1) hideSuggests();
else setSuggest(optContent.innerHTML);
}
function arrowKeyMoveOver(kc) {
var curSelectedChoiceNew;
opts = pullSuggestItems();
if (!opts) return false;
if (kc == 38) curSelectedChoiceNew = (curSelectedChoice != null) ? curSelectedChoice - 1 : opts.length - 1;
else if (kc == 40) curSelectedChoiceNew = (curSelectedChoice != null) ? curSelectedChoice + 1 : 0;
if (curSelectedChoiceNew < 0) curSelectedChoiceNew = 0;
else if (curSelectedChoiceNew >= (opts.length-1)) curSelectedChoiceNew -= 1;
if (curSelectedChoice != null) opts[curSelectedChoice].className = 'srchSuggestLinkP';
opts[curSelectedChoiceNew].className = 'srchSuggestLinkA';
curSelectedChoice = curSelectedChoiceNew;
if(curSelectedChoice != null){
optContent = opts[curSelectedChoice].getElementsByTagName('B')[0];
document.getElementById('wellpageSearchForm').keyword.value = optContent.innerHTML;
}
return true;
}
function initSuggests() {
var suggests = document.getElementById('searchSuggests');
suggests.focused = false;
suggests.onmouseover = function() {
this.focused = true;
}
suggests.onmouseout = function() {
this.focused = false;
}
}
function showSuggests(event)
{
var keycode;
if (window.event) {
keycode = window.event.keyCode;
}
else if (event) {
keycode = event.which;
}
else {
return true;
}

if ((keycode == 37 || keycode == 39) && curSelectedChoice != null) {
return false;
} else if (($('searchSuggests').innerHTML.length > 0) && ((keycode == 38) || (keycode == 40))) {
return arrowKeyMoveOver(keycode); // ret FALSE if all ok
} else if (keycode == 13) {
if( curSelectedChoice != null ){
prefillSearchStrField();
} else {
startWellPageSearch();
}
return true;
} else {
curSelectedChoice = null;
sendSuggestsRequestDelayed();
return true;
}
}
function sendSuggestsRequestDelayed(){
setTimeout ( "sendSuggestsRequest()", 200 );
}
function sendSuggestsRequest() {

var reqValue = document.getElementById('wellpageSearchForm').keyword.value;
if(reqValue == undefined || reqValue == ""){
hideSuggests();
return;
}

if(requestString != null){
return;
}
requestString = reqValue;
new Ajax.Updater('searchSuggests'
, '/searchSuggests.s?value=' + encodeURI(reqValue)
, {
onComplete: function(){
var reqValueComp = document.getElementById('wellpageSearchForm').keyword.value;
if(requestString != reqValueComp){
if(requestString == ""){
requestString = null;
hideSuggests();
return;
}
requestString = null;
sendSuggestsRequest();
return;
}
requestString = null;
}
});
}
function hideSuggests() {
document.getElementById('searchSuggests').innerHTML = '';
curSelectedChoice = null;
return false;
}
function setSuggest(value) {
document.getElementById('wellpageSearchForm').keyword.value = value;
hideSuggests();
//startWellPageSearch();
}
function setStartSearch(value) {
document.getElementById('wellpageSearchForm').keyword.value = value;
hideSuggests();
startWellPageSearch();
}
function onBlurSearchString() {
if (!document.getElementById('searchSuggests').focused) {
hideSuggests();
}
}
//]]>
</script>
<script type="text/javascript">
function startWellPageSearch() {
if (($('wellpageSearchForm').keyword) && ($('wellpageSearchForm').keyword.value == 'ex. breast cancer, weight loss, diabetes, yoga...')) $('wellpageSearchForm').keyword.value = '';
submitForm('wellpageSearchForm');
}
function focusOnSearchBox() {
$('keyword').focus();
}
</script>
