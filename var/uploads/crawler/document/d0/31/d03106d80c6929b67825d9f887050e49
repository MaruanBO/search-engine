$('#lightbox_wrapper').fadeIn('slow');
//$('#lightbox_wrapper').show();
} else {
var lightbox = '<div id="lightbox_wrapper"><div id="lightbox" class="lightbox_center"><input type="image" class="close_button" src="/images/close.png" alt="X"/><div id="subscription_alert">\n <form class="email_subscribe_form" method="post" target="_blank" \n action="http://alerts.berkeleywellness.com/register/default.html?ap=wells&st=link&s=WPH_140601_001">\n <p class="title">Looking for trusted wellness advice?</p>\n <p>Subscribe to Wellness Alerts from BerkeleyWellness.com, a Wellsphere partner site. Receive practical advice for daily living from the UC School of Public Health delivered to your inbox. <a href="http://alerts.berkeleywellness.com/register/default.html" rel="nofollow">Learn More></a></p><img src="/images/UCSeal.jpg">\n <label>Email</label>\n <div class="form_field email">\n <input class="field email" type="email" name="user_pref_email" maxlength="50"></input>\n <span class="field_error_message"></span>\n </div>\n <input class="action" type="submit" value="SIGN UP"></input>\n </form>\n <div class="email_subscribe_ty">\n <p class="title">Welcome to Wellness Alerts!</p>\n <p>Thank you for your interest. Please check your email for further instructions on how to complete your sign up.</p>\n </div>\n</div></div></div>';
