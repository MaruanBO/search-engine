$('#lightbox_wrapper').fadeIn('slow');
$('.close_button').click(function() {
cookie.set('popup_expiry', { path : "/" });
$('#lightbox_wrapper').hide();
$('.box_sprite.box_tl,.box_sprite.box_tr, .box_sprite.box_bl, .box_sprite.box_br').show();
});
var $form = $('.email_subscribe_form');
$form.find('input.action').click(function(e) {
e.preventDefault();
var email_val = $form.find('input.field').val();
if (email_val.match(/\b[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b/i) !== null) {
$form.submit();
$('#subscription_alert').addClass('subscribed');
cookie.set('subscribed_to_alerts',true,{ expires : 10000, path : "/" });
} else {
$form.find(".form_field.email").addClass("error_validation");
$form.find(".field_error_message").text("Invalid Email");
}
});
},5000);
}
}
}
});
