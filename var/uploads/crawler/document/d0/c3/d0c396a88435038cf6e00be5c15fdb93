		<script charset="utf-8" type="text/javascript">
		$(document).ready(function(){
		  var nav = $("ul.topnav");

        //add indicators and hovers to submenu parents
	        nav.find("li").each(function() {
	            if ($(this).find("ul.subnav").length > 0) {
	                $(this).mouseenter(function() {
	                    $(this).find("ul.subnav").stop(true, true).delay(400).slideDown(400);
	                });

	                //hide submenus on exit
	                $(this).mouseleave(function() {
	                    $(this).find("ul.subnav").stop(true, true).delay(400).slideUp(400);
	                });
	            }
	        });

	   });

		function slideToggle(el, bShow){
		  var $el = $(el), height = $el.data("originalHeight"), visible = $el.is(":visible");
		  if( arguments.length == 1 ) bShow = !visible;
		  if( bShow == visible ) return false;
		  if( !height ){

			if ( $el.show().height() > 0 ){
				height = $el.show().height();
			} else {
				return false;

			}

			$el.data("originalHeight", height);

			if( !visible ) $el.hide().css({height: 0});
		  }

		  if( bShow ){
			$el.show().animate({height: height}, {duration: 200});
		  } else {
			$el.animate({height: 0}, {duration: 200, complete:function (){
				$el.hide();
			  }
			});
		  }
		}

		function weather(z,d){

			$(function(){
				$.getScript('http://sports.espn.go.com/rpm/jayski/weather.js?callback=handleWeather&zip='+z+'&day='+d);
			});

		}

		function handleWeather(j){
			if (!j.error){
				var ht = '<img alt="weather" src="'+j.image+'"/><br/>';
				ht = ht + j.date + '<br/>';
				ht = ht + j.high + '&#176;-' + j.low + '&#176;<br/>';
				ht = ht + 'Rain: ' + j.precip + '&#37;';
				$('div.weather').html(ht);
			} else {
				$('div.weather').hide();
			}
		}

    </script>

