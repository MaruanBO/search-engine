    angular.module("hallmarkecardsApp")
        .service("ContextService", function($timeout, storage){
            var currentUserObj = storage.get('totalRecall') || {};

            if (sessionStorage.totalRecall) {
                currentUserObj = JSON.parse(sessionStorage.totalRecall);
            }

            function addUserVoiceUser(user) {
                if (typeof UserVoice !== "undefined") {
                    var uvUser = {
                        email: user.email, // User’s email address
                        name: user.firstName + " " + user.lastName, // User’s real name
                        created_at: ((new Date(user.createdDate)).getTime()/1000), // Unix timestamp for the date the user signed up
                        id: user.id, // Optional: Unique id of the user (if set, this should not change)
                        type: user.analyticsUserState // Optional: segment your users by type
                        //account: {
                        //  id:           123, // Optional: associate multiple users with a single account
                        //  name:         'Acme, Co.', // Account name
                        //  created_at:   1364406966, // Unix timestamp for the date the account was created
                        //  monthly_rate: 9.99, // Decimal; monthly rate of the account
                        //  ltv:          1495.00, // Decimal; lifetime value of the account
                        //  plan:         'Enhanced' // Plan name for the account
                        //}
                    };
                    UserVoice.push(['identify', uvUser]);
                }
            }

            if (!_.isEmpty(currentUserObj)){
                $timeout(function(){
                    addUserVoiceUser(currentUserObj);
                }, 2000);
            }
            return {
                environment: "production",
                apiBaseUrl: "https://api.hallmarkecards.com/",
                hostBaseUrl: "https://www.hallmarkecards.com/",
                staticBaseUrl: "https://d31f6akgv38us3.cloudfront.net/",
                optimizelyPriceExperimentId: 1612165450,
                clientIp: "128.199.135.235",
                currentUser: currentUserObj,
                updateCurrentUser: function(user){
                    if (!_.isEmpty(user)){
                        if (currentUserObj == null) currentUserObj = {};
                        angular.extend(currentUserObj, user);

                        try {
                            if (user.rememberMe){
                                storage.set('totalRecall', user);
                            }
                            else {
                                sessionStorage.totalRecall = JSON.stringify(user)
                            }
                        }
                        catch(e) {

                        }
                        addUserVoiceUser(user);
                    }
                    else
                        currentUserObj = user;
                },
                userHasEntitlement: function (action) {
                    if (_.isEmpty(currentUserObj) || currentUserObj.entitlements.length == 0) return false;

                    foundEntitlement = _.find(currentUserObj.entitlements, "code", action);

                    return !!(foundEntitlement);
                },
                userIsAdmin: function(){
                    if (_.isEmpty(currentUserObj)) return false;

                    return (_.contains(currentUserObj.userGroups, 'admin'));
                }
            }
        });

