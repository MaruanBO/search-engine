            <div id="AjaxWorkingPopup" class="gray-shadow-box">
                <img src="http://media.salemwebnetwork.com/zcast/ls/icons/ajax-loader.gif" alt="Loading" />
                <div class="font-black-6">Working...</div>
            </div>

            
    <script>
        var sliderWrapper = $(".host-slider");
        var timer = setTimeout(DrawForward, 10000); // Trigger slider in 10 seconds
        var returnTimer;

        $(window).load(function () {

            $(".arrow-left").click(function () {
                DrawBackward();
            });

            $(".arrow-right").click(function () {
                DrawForward();
            });

            $(".host-slider li").mouseenter(function () {
                clearTimeout(timer);
                clearTimeout(returnTimer);

                var showId = $(this).find(".ministry-data").val();
                var activeShowId = $(".host-slider .active").find(".ministry-data").val();

                if (showId != activeShowId) {
                    var triangle = $('.host-slider-wrapper .triangle');
                    triangle.hide();
                    var position = $(this).position().left;
                    triangle.css('left', position + 100);
                    $(".host-slider .active").removeClass("active");
                    UpdateActive(false, showId);
                }
            });

            $(".host-slider li").mouseleave(function () {
                var showId = $(this).find(".ministry-data").val();
                var activeShowId = $(".host-slider li:eq(0)").find(".ministry-data").val();

                if (showId != activeShowId) {
                    restartSlider();
                }

                timer = setTimeout(DrawForward, 10000); // Trigger again in 10 seconds  
            });

            $('.loader-feature').mouseenter(function () {
                clearTimeout(timer);
                clearTimeout(returnTimer);
            });

            $('.loader-feature').mouseleave(function () {
                var showId = $(this).find(".ministry-data").val();
                var activeShowId = $(".host-slider li:eq(0)").find(".ministry-data").val();

                if (showId != activeShowId) {
                    restartSlider();
                }

                timer = setTimeout(DrawForward, 10000);
            });

            // truncate text for featured host titles
            truncateFeatureText();

            // truncate text for host slider title
            sliderWrapper.find('.episode-title').each(function () {
                Salem.Shared.Util.TruncateText(35, $(this), "episode-title");
            });

            // truncate text for "More ministries" title
            $('#home-page-columns .brief-image-list .show-info').each(function () {
                var title = $(this).find('.episode-title');
                Salem.Shared.Util.TruncateText(40, title, "episode-title font-black-5");

                var description = $(this).find('.host-show');
                Salem.Shared.Util.TruncateText(35, description, "host-show font-blue-4");
            });
        });

        function restartSlider() {
            returnTimer = setTimeout(function () {
                UpdateActive(true);
                $(".host-slider .active").removeClass("active");
                UpdateActive(false);
            }, 2000);
        }

        function DrawForward() {
            clearTimeout(timer);

            var first = $(".host-slider li:eq(0)");
            var last = $(".host-slider li:last-child");

            UpdateActive(true);

            sliderWrapper.animate({ 'left': '-=236px' }, 300, function () {
                sliderWrapper.css('left', "0");
                last.after(first);
                UpdateActive(false);
            });

            timer = setTimeout(DrawForward, 10000); // Trigger again in 10 seconds  
        }

        function DrawBackward() {
            clearTimeout(timer);

            var first = $(".host-slider li:eq(0)");
            var last = $(".host-slider li:last-child");

            UpdateActive(true);

            sliderWrapper.animate({ 'left': '+=236px' }, 300, function () {
                sliderWrapper.css('left', "0");
                first.before(last);
                UpdateActive(false);
            });

            timer = setTimeout(DrawForward, 10000); // Trigger again in 10 seconds  
        }

        function UpdateActive(hidden, showId) {
            var active = $(".host-slider .active");
            var arrow = $(".host-slider-wrapper .triangle");
            var newActive;

            if (showId) {
                newActive = $('input[type="hidden"][value="' + showId + '"]').parent();
            } else {
                newActive = $(".host-slider li:eq(0)");
            }

            if (hidden) {
                arrow.hide().css('left', 100);
                active.removeClass("active");
            } else {
                arrow.fadeIn(100);
                newActive.addClass("active");

                if (!showId) {
                    showId = newActive.find(".ministry-data").val();
                }

                $(".loader-feature .todays-broadcast, .loader-feature .special-offer").fadeOut(150, function () {
                    // take care of removing current feature
                    var $loaderFeature = $('div.loader-feature > div');
                    var $showEmpty = $('div.container-feature div.show-empty');
                    var currentFeatureId = $loaderFeature.attr('class');
                    $loaderFeature.find('> div').appendTo($showEmpty);
                    $showEmpty.removeClass('show-empty').addClass(currentFeatureId);

                    // take care of adding the new current feature
                    var newCurrentFeatureId = 'show-' + showId;
                    var $newCurrentFeature = $('div.container-feature div.' + newCurrentFeatureId);
                    $newCurrentFeature.find('> div').appendTo($loaderFeature);
                    $loaderFeature.removeClass(currentFeatureId).addClass(newCurrentFeatureId);
                    $newCurrentFeature.removeClass(newCurrentFeatureId).addClass('show-empty');

                    // truncate text for featured host titles
                    truncateFeatureText();

                    $(".loader-feature .todays-broadcast, .loader-feature .special-offer").fadeIn(150);
                });
            }
        }

        // truncate text for featured host titles
        function truncateFeatureText() {
            Salem.Shared.Util.TruncateText(60, $('div.loader-feature .todays-broadcast .details p.episode-title'), "episode-title font-black-1");
            Salem.Shared.Util.TruncateText(45, $('div.loader-feature .todays-broadcast .details p.host-name'), "host-name font-blue-1");
            Salem.Shared.Util.TruncateText(50, $('div.loader-feature .special-offer .details p.offer-title'), "offer-title font-black-2");
            Salem.Shared.Util.TruncateText(90, $('div.loader-feature .special-offer .details p.offer-description'), "offer-description font-black-4");
        }

    </script>

