<script>
    $(document).ready(function(){
        
            $('#contentTwitter').bxSlider({
                mode: 'fade',
                pager: true,
                auto: false,
                touchEnabled: false
            });


        $.getJSON("https://graph.facebook.com/kingcountylibrarysystem/feed?access_token=465683343509360|wrzc_EhQnXH2QGKKORuTJ0nxeXQ&limit=20", function(data){

            obj= data.data;
            Object.keys(obj).forEach(function(key, index){
                wantedId = obj[key].from.id;
                notNull = obj[key].message

                if(wantedId == '24179204472' && typeof notNull != 'undefined'){

                    $('#contentFB').append('<div class="post index'+index+'"><div class="message">'+obj[key].message+'</div></div>');
                }



            })
            setTimeout(function(){
                $('body .post:lt(9)').addClass('showThis')

setTimeout(function(){
            var childrenTweets = $('.post.showThis');
            $cnt = childrenTweets.length;

            var numDivs= childrenTweets.length;


            for (i=0; i< numDivs; i=i+3){
                childrenTweets.eq(i).add(childrenTweets.eq(i+1)).add(childrenTweets.eq(i+2)).wrapAll('<div class="threeTweets"></div>') ;

            }

            $('#contentFB').children('.post').remove();

            $('#contentFB').bxSlider({
                mode: 'fade',
                pager: true,
                auto: false,
                touchEnabled: false
            });

            $('.message').each(function(){

                $(this).prepend('<span class="blue lft" style="margin-top:-2px;font-size:200%;line-height: 20px;height: 12px;">"</span>')
                $(this).append('<span class="blue" style="line-height: 0px;position: relative;top: 5px;font-size:200%">"</span>')

            })
        },10)

            },10)

        })

    })

setTimeout(function(){
var oldHeight = $('.events .bx-viewport').height();
//var newHeight = oldHeight*(0.55);

//$('.events .bx-viewport').height(newHeight);*/

},1500)

</script>


