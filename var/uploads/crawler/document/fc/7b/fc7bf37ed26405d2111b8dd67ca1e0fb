		<script type="text/javascript">
		jQuery.ajax({
			'async': false,
			'url': "http:\/\/foodgawker.com\/wp-content\/themes\/Foodgawker\/gawkerverse-parent\/functions\/fg_popular_count.php",
			'context': document.body,
			'data': {
				'post_ids': [2142414,2142720,2142795,2142797,2142805,2142841,2142845,2142859,2142883,2142907,2142919,2142953,2142993,2143053,2143061,2143065,2143225,2143239,2143285,2143309,2143313,2143315],
				'type': "",
				'period': "",
				'action': 'fg_popular_counts',
			},
			'success': function(response) {
				var $ = jQuery,
					$page = $(".post-section:first"),
					results = $.parseJSON(response),
					$card, $favorites, $gawked, numeric;
				for (var postId in results) {
					if (postId.length > 0) {
						$card = $page.find('#post-'+postId);
						if ($card.length > 0) {
							$favorites = $card.find('.faved');
							$gawked = $card.find('.gawked');
							if ($favorites.length > 0 && typeof results[postId].favorites !== "undefined") {
								numeric = parseInt(results[postId].favorites.replace(/,/, ''), 10);
								if (numeric >= 10000) {
									numeric = Math.round(numeric / 100) / 10;
									numeric = numeric.toString() + 'k';
								}
								else {
									numeric = results[postId].favorites;
								}
								$favorites.html(numeric);
							}
							if ($gawked.length > 0 && typeof results[postId].gawked !== "undefined") {
								numeric = parseInt(results[postId].gawked.replace(/,/, ''), 10);
								if (numeric >= 10000) {
									numeric = Math.round(numeric / 100) / 10;
									numeric = numeric.toString() + 'k';
								}
								else {
									numeric = results[postId].gawked;
								}
								$gawked.html(numeric);
							}
						}
					}
				}
			}
		});
	</script>
