        $(".click-to-chat-toggle").show();
      });
      $(".click-to-chat-toggle").click(function() { 
        olark("api.box.expand"); 
      });

      olark("api.chat.onOperatorsAway", function() {
        if (ShopatronOlarkSetup.group != "default") {
          ShopatronOlarkSetup.group = "default";
          ShopatronOlarkSetup.setOlarkGroupAndState();
          return;
        }
        else {
          olark("api.box.hide");
          $(".click-to-chat-toggle").hide();
        }
      });
    }
