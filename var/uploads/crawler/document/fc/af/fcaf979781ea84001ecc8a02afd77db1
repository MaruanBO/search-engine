		AD_DESIGN = 'script';
	}
	else {
		AD_DESIGN = 'gpt';
	}

	if (window['localStorage'] !== null) {
		
		if (localStorage.getItem("GPT_HP4") == null) {
			localStorage.setItem("GPT_HP4", AD_DESIGN);
		}
		AD_DESIGN = localStorage.getItem("GPT_HP4");
		
	}

	// Add to ab_test.js
	(function() {
		if (AB_AD_TEST && AD_DESIGN) {
