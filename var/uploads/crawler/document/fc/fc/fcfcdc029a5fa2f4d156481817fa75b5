    <script type="text/javascript">
        $(document).ready(function () {
            $('input.email-signup').keyup(function (e) {
                if (e.keyCode === 13) {
                    $('#ctl00_cphMainContent_uxEmailSignUp_imgSubmit').click();
                }
            });

            $('input.sms').keyup(function (e) {
                if (e.keyCode === 13) {
                    var regex = /[^0-9]/;
                    var phonenumber = jQuery("#smsNumber").val();
                    if (phonenumber == "") {
                        alert("Phone number Required");
                        return false;
                    }
                    else {
                        if (phonenumber.length == 10) {
                            if (phonenumber.match(regex) != null) {
                                alert("Phone number should be a valid number");
                                return false;
                            }
                        }
                        else {
                            alert("Phone number should be 10 digits");
                            return false;
                        }
                    }

                    $('#btnTextalertsubmit').click();
                }
            });
        });
    </script>
    
