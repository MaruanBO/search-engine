            width: 450px;
            height: 349px;
        }
    }
    @media screen and (max-width:519px) {
        #PopUpModal {
            width: 284px;
            height: 220px;
        }
    }
    #PopUpModal {
        position: fixed;
        z-index: 10001;
        display: none;
    }
    #PopUpMask {
        position: fixed;
        left: 0px;
        top: 0px;
        z-index: 10000;
        display: none;
    }
</style>

<script type="text/javascript">
    function ShowPopUp(PopUpID) {
        var PopUpMask = document.getElementById('PopUpMask');
        PopUpMask.style.display = "block";

        var PopUpModal = document.getElementById('PopUpModal');
        PopUpModal.style.display = "block";

        doResize();
    }
</script>
<script type="text/javascript">
    function HidePopUp(PopUpID) {
        var PopUpMask = document.getElementById('PopUpMask');
        PopUpMask.style.display = "none";

        var PopUpModal = document.getElementById('PopUpModal');
        PopUpModal.style.display = "none";

        var current_date = new Date;
        var cookie_year = current_date.getFullYear() + 1;
        var cookie_month = current_date.getMonth();
        var cookie_day = current_date.getDate();
        set_cookie("viewedpopid", PopUpID, cookie_year, cookie_month, cookie_day, "/", "bestfriends.org");
    }
</script>
<script type="text/javascript">
    function PopUpCheck(PopUpID) {
        if (!get_cookie("viewedpopid") || get_cookie("viewedpopid") != PopUpID) {
            ShowPopUp(PopUpID);
        }
    }
</script>
<script type="text/javascript">
    window.onresize = doResize;
    function doResize() {
        var windowWidth = 0, windowHeight = 0, modalLeft = 0, modalTop = 0;
        var PopUpSubmit = document.getElementById('ctl00_cphMainContent_PopUpAd_HyperLinkSubmit');
        var PopUpMask = document.getElementById('PopUpMask');
        var PopUpModal = document.getElementById('PopUpModal');
        if (typeof (window.innerWidth) == 'number') {
            //Non-IE
            windowWidth = window.innerWidth;
            windowHeight = window.innerHeight;
            modalLeft = (windowWidth / 2) - (PopUpModal.offsetWidth / 2);
            modalTop = (windowHeight / 2) - (PopUpModal.offsetHeight / 2);
        } else if (document.documentElement && (document.documentElement.clientWidth || document.documentElement.clientHeight)) {
            //IE 6+
            windowWidth = document.documentElement.clientWidth;
            windowHeight = document.documentElement.clientHeight;
            modalLeft = (windowWidth / 2) - (document.all.PopUpModal.offsetWidth / 2);
            modalTop = (windowHeight / 2) - (document.all.PopUpModal.offsetHeight / 2);
        } else if (document.body && (document.body.clientWidth || document.body.clientHeight)) {
            //IE 4 compatible
            windowWidth = document.body.clientWidth;
            windowHeight = document.body.clientHeight;
            modalLeft = (windowWidth / 2) - (document.all.PopUpModal.offsetWidth / 2);
            modalTop = (windowHeight / 2) - (document.all.PopUpModal.offsetHeight / 2);
        }

        PopUpMask.style.width = windowWidth + "px";
        PopUpMask.style.height = windowHeight + "px";

        PopUpModal.style.left = modalLeft + "px";
        PopUpModal.style.top = modalTop + "px";

        if (PopUpSubmit) {
            PopUpSubmit.style.left = (modalLeft + 90) + "px";
            PopUpSubmit.style.top = (modalTop + 70) + "px";
        }
    }
</script>
<div id="PopUpMask" class="CommonModalMask">
</div>
<div id="PopUpModal" class="CommonModal">
    <img id="ctl00_cphMainContent_PopUpAd_ImageBackground" class="PopUpBackGround" src="" />
    <a id="ctl00_cphMainContent_PopUpAd_HyperLinkSubmit" class="PopUpSubmit" target="_blank"></a>
    <a id="ctl00_cphMainContent_PopUpAd_HyperLinkDecline" class="PopUpDecline"></a>
    <a id="ctl00_cphMainContent_PopUpAd_HyperLinkClose" class="CommonModalClose"></a>
</div>


