        {
            if(aIsError=="1")
            { 
                aMsg = "<Font class='lblError'><img src='../Images/Cancel.gif'/>&nbsp;"+aMsg+"</Font>"
            }
            else
            {
                aMsg = "<Font class='lblSuccess'><img src='../Images/OK.gif'/>&nbsp;"+aMsg+"</Font>"
            }
            var strComments="<div class='SuccessFailRadAlert'><p>"+aMsg+"</p></div>";
            radalert(strComments);
            //pageScroll();
        }

        function ConfirmURLToRedirect(pArgVal, pArgID) {
            var msgResult = null;
            vURLArgVal = pArgVal;
            if (pArgID == "0") 
            {
                radconfirm('<a class=RadConfirmMsg><b>' + vConfirmRedirectMsg1 + '</b></a><br>', confirmGridPromptCallBackFn, 500, 100, null, 'Redirection Confirmation');
            }
            return false;
        }

        //Firing the event to the server on the basis of flag:arg-- not the embedded telerik code
        function confirmGridPromptCallBackFn(arg) {
            if (arg) 
            {
                window.document.location = vURLArgVal;
            }
            return arg;
        }
         
        function Do_The_Focus(sClientId) {   
//            var e = document.getElementsByName(sClientId);
//            if (e.length > 0) {    
//                e[0].focus();   
//                //e[0].select();   
//            }
        }

        function ResponseEndforLoadingPanel() {
            if (document.getElementById("ctl00_MainPlaceHolder_UCZipCode1_hdnLoadingPanelInfo")) {
                if (document.getElementById("ctl00_MainPlaceHolder_UCZipCode1_hdnLoadingPanelInfo").value == "Save") {
                    document.getElementById("ctl00_MainPlaceHolder_UCZipCode1_hdnLoadingPanelInfo").value = "";
                    SaveLoadingPanel();
                    //pageScroll();
                    
                }
                else {
                    pageScroll();
                    return;
                }
            }
        }

        function SaveLoadingPanel() {
            $find("ctl00_MainPlaceHolder_UCZipCode1_SaveRadAjaxPanel").ajaxRequest();
        }

        function FireCustomEvent(pArgCtrl) {
            //debugger;
            //alert(pArgCtrl.which);
            var keynum = -1;
            if (window.event != null && window.event.keyCode == 13) {
                keynum = window.event.keyCode;
            } else if (pArgCtrl.which == 13) {
                keynum = pArgCtrl.which;
            }

            if (keynum==13){
                __doPostBack('ctl00$MainPlaceHolder$UCZipCode1$btnImgCheckZip', '');
            }
//            if (window.event.keyCode == 13) {
//                event.returnValue = false;
//                event.cancel = true;
//            }
        }

        function SetToScroll() {
            //debugger;
            elm = document.getElementById("ctl00$MainPlaceHolder$UCZipCode1$txtHidden");
            elm.value = "1";
        }

        function pageScroll() {
            //debugger;                   
                window.scrollBy(0, 1000); // horizontal and vertical scroll increments
            //window.location = "#bottom";
            }
    </script>
    
