    <script language="javascript" type="text/javascript">
    
   

    function fnCenteredHelpWindow(argSelect, title) {
        
        switch (argSelect) {        
            case 1:
                aURL = "http://www.virginmobileusa.com/legal/privacy-policy";
                break;

            case 2:
                aURL = "http://www.virginmobileusa.com/about-virgin-mobile";
                break;

            case 3:
                
                    aURL = "https://www2.virginmobileusa.com/myaccount/home.do";
                
                break;

            case 4:
                aURL = "https://www2.virginmobileusa.com/lifeline";
                break;

            case 5:
                aURL = "http://assurancewireless.mwnewsroom.com/feedback.aspx";
                break;
                
            case 6:
                
                    aURL = "http://www.virginmobileusa.com/legal/terms-of-website-use";
                
                break;
                
            case 7:
                //Changes by WL for Ticket #1261: Link Change Request on AW Website - Dated: 08/18/2011                
                
                    aURL = "http://www.virginmobileusa.com/legal/copyright-info";
                
                break;                
                
            case 8:
                // Ticket #1433: AW Website Enhancement - Footer Change               11/3/2011 WL
                    aURL = "http://www.virginmobileusa.com/networkmanagement";
                break;                         
             
            case 9:
            // Ticket #1491: AW Website Enhancement - FAQ's Page -WL
                aURL = "http://virginmobileusa.custhelp.com/app/answers/detail/a_id/5657/kw/Understand%20Wireless%20Emergency%20Alerts%2FCMAS/session/L3RpbWUvMTMyMjU4MjAxMi9zaWQvOW43KiprS2s%3D";
            break;       
            
            case 10:

            aURL = "/Public/TellAFriend.aspx";
            break;
            
           case 11:

            aURL = "http://awweb.surveyanalytics.com";
            break;

            case 12:

            aURL = "Jax.aspx";
            break;          
                
            default:
            
                aURL = "http://intranet.solixinc.com";
                break;
         }

        //x = Width; y = Height
        
        if (aURL && aURL.indexOf("TellAFriend.aspx") >= 0 )
            {
             var screenWidth = 650;
             var screenHeight = 600;
            }
        else
            {
             var screenWidth = document.body.offsetWidth;
             var screenHeight = document.body.offsetHeight;
            }
        
        //var screenHeight = document.body.offsetHeight;
        var x = parseInt(screenWidth * 0.80);
        var y = parseInt(screenHeight * 0.80);        

        var d = new Dialog({title: title, height: y, width: x});
        d.launch_iframe(aURL);
    }

    function fnCenteredHelpWindowURL(argUrl, title) {
        var screenWidth = document.body.offsetWidth;
        var screenHeight = document.body.offsetHeight;
        var x = parseInt(screenWidth * 0.80);
        var y = parseInt(screenHeight * 0.80);

        var d = new Dialog({title: title, height: y, width: x});
        d.launch_iframe(argUrl);
    }
    
    function fnCenteredHelpWindowURLLanguage(argUrl, title, Language) {
        var screenWidth = document.body.offsetWidth;
        var screenHeight = document.body.offsetHeight;

        var x, y;
        if (argUrl.indexOf("/Secure/") > 0) {
        	/* tkt 1699/1728 */
        	x = parseInt(screenWidth * 0.94);
        	y = parseInt(screenHeight * 0.94);
        }
        else {
        	x = parseInt(screenWidth * 0.80);
        	y = parseInt(screenHeight * 0.80);
        }

        var d = new Dialog({title: title, height: y, width: x, close: SurveyLauncher.launch });
        d.launch_iframe(argUrl);
    }
    
