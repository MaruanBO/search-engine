    <script language="javascript" type="text/javascript">
      
        function EligibleWindow(x, y, Title, Language, State, ZipCode) {
            var d = new Dialog({ title: Title, height: y, width: x, close: SurveyLauncher.launch });
            if (State === "CA")
                d.launch_iframe('../Public/ZipCodeActiveWindow_CA.aspx');
            else
                d.launch_iframe('../Public/ZipCodeActiveWindow.aspx?State=' + State + '&ZipCode=' + ZipCode);
        }

        function NotEligibleWindow(x, y, Title, Language) {
            var d = new Dialog({title: Title, height: y, width: x});
            d.launch_iframe('../Public/ZipCodeInActiveStateWindow.aspx?Language=' + Language);
        }
        
    </script>


