  $('#tabEmployers').addClass('active');

  $(document).ready(function () {
  if (!window.olark) {
    $('.chatTrigger').hide();
    var interval = setTimeout(function() {
      if (window.olark) {
        clearTimeout(interval);
      }
    }, 1000);
  }
  //chatbox popup
  $('.chatTrigger').on('click',function(e) {
    olark('api.box.expand');
    return false;
  });
});

      /* Trigger tooltip */
    $('[rel=tooltip]').tooltip({placement:"bottom",delay: { show: 300, hide: 100 }});
  
});
