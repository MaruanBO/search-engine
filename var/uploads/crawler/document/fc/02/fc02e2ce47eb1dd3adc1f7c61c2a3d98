    <script type="text/javascript">
$(function() {
  // add trim to String for IE8
if(typeof String.prototype.trim !== 'function') {
  String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, '');
  }
}


var $loginTab = $("#loginTab");
if( !$loginTab.length ){
  $loginTab = $("A.sign_in_link");
}
$loginTab.hover(
  function() {
    $(this).addClass( "noPop" );
  }, function() {
    $(this).removeClass( "noPop" );
  }
);

setTimeout(function() {
  $('body').mousemove(function( event ) {
    var clientY = event.clientY;
    var clientX = event.clientX;
    if ( clientY < 8 && clientX != 0 && ! $(".noPop")[0] && ! $.cookie('vPostNow')) {
      var $reg_block = $("#reg_block");
      if ( !$reg_block || $reg_block.css('display') == 'none') {
        var $exitPop = $('#exitPop4787');
        if( $exitPop.length ){
          $exitPop.modal('show');
          $.cookie('vPostNow', 'hJam42S', { path: '/' });
        }
      }
    }
  });
}, 1000);



$(":text:first").focus();


$('.user_create_form').submit(function(){
  var $submitButtons = $('input[type=submit]', this);
  $submitButtons.prop('disabled', true);
  window.setTimeout(function(){ $submitButtons.prop('disabled', false);  }, 2000);
  return true;
});

$.validator.addMethod('phone', function(value) {
  return value.match(/\d{3}.*\d{3}.*\d{4}/);
}, 'Please enter a valid phone number'
);

$("#user_create_form").validate({

  rules: {
    requested_plan: {
      required: true
    },
        org_name: {
      required: true
    },
    org_type: {
      required: true
    },
    phone_number: {
      required: true,
      phone: true
    },
    password: {
      required: true,
      pwcheck: true,
      minlength: 6
    }
  },
  messages: {
    org_name: "Please enter your company name",
    phone_number: "Please enter phone number (with area code)",
    password: {
      required: "Please create a password (minimum 6 characters)",
      pwcheck: "Your password is too weak."
    }
  }
});

function updateUserReportedSpecific(type){
  var $specific = $(".source_type_user_reported_specific");

  var changeText;
  if ( type == 'radio_or_podcast' ) {
    changeText =  'Which radio station did you hear us on? (optional)';
  }else if( type == 'tv' ){
    changeText = 'Which TV station did you see us on? (optional)';
  }else if( type == 'mail' ){
    changeText = 'Offer code (optional, find at bottom of letter)';
  }else{
    $specific.parent().hide();
  }

  if( changeText ){ 
    var currentValue = $specific.val();
    $specific.attr('placeholder', changeText).parent().show();
    $specific.val(currentValue);
  }

}

$(".source_type_user_reported").on("change", function() {
  updateUserReportedSpecific( $(this).val() );
});


  
function addTwoStepCreateFormHooks(){
var $min_form = $(".user_create_form_min");

$min_form.validate({
  rules: {
    email_address: {
      required: true,
      email: true
    },
    name: {
      required: true
    },
  },
  messages: {
    email_address: "Please enter a valid email address, example: you@yourdomain.com",
    name: "Please enter your name"
  },
  submitHandler: function(form) {
    var $form = $(form);
    var data = $form.serialize();
    $.ajax({
      url: '/user/create_step1',
      data: data,
      dataType: "json",
    }).done(function(data, textStatus, jqXHR){
      if(data){
        if(data.success){
          var $div = $('#create_step2_div');
          $div.modal('show');
          $div.show();

          var isInputSupported = 'placeholder' in document.createElement('input');
          if(isInputSupported){
            $("#password").focus(); // focus on first field.
          }

          var main_create_form = document.getElementById('user_create_form');
          $(main_create_form.email_address).val(form.email_address.value);
          $( main_create_form.name ).val( form.name.value ) ;

          main_create_form.agrees_to_terms.checked = form.agrees_to_terms.checked;

          return;
        }else if(data.form){
          $form.replaceWith(data.form);
          addTwoStepCreateFormHooks(); // need to update our validation hooks.
          return;
        }else if(data.error){
          alert(data.error);
          return;
        }
      }
      alert("Unable to submit your request. Please refresh your page and try again.");
    }).fail( function(){
      alert("Unable to submit your request. Please refresh your page and try again.");
    });
    return;
  }
});

}

  addTwoStepCreateFormHooks();
  

  var textRotate = function() {
	var current = $('.rotateQuotes > .current');
	if(current.next().length == 0)
	{
	current.removeClass('current').hide();
	$('.quote:first').addClass('current').fadeIn(1000);
	}
	else
	{
	current.removeClass('current').hide();
	current.next().addClass('current').fadeIn(1000);
	}
}
function setupRotator() {
	if($('.quote').length > 1)
	{
	$('.quote:first').addClass('current').fadeIn(1000);
	setInterval(textRotate, 7000);
	}
}

setupRotator();


