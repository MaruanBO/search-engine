<script type="text/javascript">
    
    //console.warn('before cmDisplayRecs via MiniShopCartDisplay');
    //dojo.addOnLoad(function() {cmDisplayRecs();});
    //console.warn('after cmDisplayRecs via MiniShopCartDisplay');
    
    var cmMiniCartLoaded = false;
    
	function getMiniCartDetails()
	{	
    	var ajaxOptions = {
				   type: "GET",
				   dataType: "html",
				   url: "/webapp/wcs/stores/servlet/MiniShopBagAJAXView",
				   data: "catalogId=" + 14001 + "&storeId=" + 13751 + "&langId=" + -1 ,
				   success: function(html){
				   		$(".carticon").hover(
					        function()
					        { 
								$("#box").fadeIn("slow");
					        },
					        function()
					        {
					        }
				    	);
					    $("#mini-bag-popUp").hover(
					        function(){},
					        function()
					        {
					        	$("#box").fadeOut("slow");
					        }
					    );					   
				   		$("#mini-bag-popUp").html(html);
			  			}
			   };
			   
		  var ajaxOptionsMassoc = {
				   type: "GET",
				   dataType: "html",
				   url: "/webapp/wcs/stores/servlet/MiniShopBagMassocAJAXView",
				   data: "catalogId=" + 14001 + "&storeId=" + 13751 + "&langId=" + -1 ,
				   success: function(html){
				   		$(".carticon").hover(
					        function()
					        { 
								$("#box").fadeIn("slow");
					        },
					        function()
					        {
					        }
				    	);
					    $("#mini-bag-massoc").hover(
					        function(){},
					        function()
					        {
					        	$("#box").fadeOut("slow");
					        }
					    );
					    if(!cmMiniCartLoaded) {
                            $("#mini-bag-massoc").html(html);
                            cmMiniCartLoaded = true;
					    }
			  			}
			   };
		  
		  var viewName = window.location.href;
		  
		  if(document.resetPasswordForm != undefined || (viewName != null && viewName.indexOf("Logon") >= 0 && viewName.indexOf("?") < 0))	 
		  {
			//don't do Ajax call in such case 
		  }
		  else
		  {
			$.ajax(ajaxOptions);
			$.ajax(ajaxOptionsMassoc);
		  }

		return false;
	}

</script>

