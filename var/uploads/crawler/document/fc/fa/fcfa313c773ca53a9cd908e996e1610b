	<script type="text/javascript">
		var loc = location.href;
	
		//If coming from search, home or logon (OrderCalculate) and the page is secure, redirect to non-secure.  Dont do anything if in the "preview" context	
		if(loc.indexOf('https://') >=0 && loc.indexOf('/preview/') <0 && (loc.indexOf('CatalogSearchResultCmd') >=0 || loc.indexOf('ProductDisplay') >=0 || loc.indexOf('TopCategoriesDisplay') >=0)){
			loc = loc.replace('https://', 'http://');
			window.location = loc;
		}	
	</script>
	
	
	
	<script type="text/javascript">
	    function submitZipCodeForm(form, zipcodeValue)
	    {
	        if (isValidZipCodeFormat(zipcodeValue))
	        {
	            //save the zipcode
	            $.cookie('cvStoreZipCode', zipcodeValue);
	        } else
	        {
	            //delete the cookie if zipcode is not valid
	            $.cookie('cvStoreZipCode', null);
	        }
	        form.submit();
	    }
	
	    //valid zip code format is 5 digit number
	    function isValidZipCodeFormat(val)
	    {	
	        //check if 5 digits
	        if (val.length != 5)
	        {
	            return false;
	        }
	        //checks if positive whole number
	        for (var i = 0; i < val.length; i++) 
	        {
	            var ch = val.charAt(i);
	            if (ch < "0" || ch > "9") 
	            {
	                return false;
	            }
	        }
	        return true;
	    }
	
	    
	    function makeLinksNonSecured() {
	        var hostName = window.location.hostname;
	        var pathName = window.location.pathname;
	        pathName = pathName.substring(0,pathName.lastIndexOf("/"));
	        var pathFullQualifier = "http://"+ hostName + pathName + "/";
	        var hostQualifier = "http://"+ hostName;
	        var tempvar = "";
	
	        
	        $("a").each(function(i) {
	            if($(this).attr("href") === undefined || $(this).attr("href") == "")
	            {
	                // it does not have any link associated...Don't do any thing  
	            }
	
	            
	            else if ( (  /^(http|ftp):\/\/.*$/i.test( $(this).attr("href") ) ) 
			      	|| ( /^(javascript).*$/i.test( $(this).attr("href") ) || (($(this).attr("href") !== undefined) && $(this).attr("href").replace(/^\s+|\s+$/, '').indexOf('#' ) == 0 )) )
	            {
	                // Don't do any thing 
	            }
	            
	            
	            else if ( ($(this).attr("href") !== undefined) && ($(this).attr("href").indexOf("/") == 0))
	            {
	                $(this).attr("href" ,  hostQualifier +   $(this).attr("href"));
	            }
	
	            		      
	            else
	            {
	                tempvar = tempvar + " " + $(this).attr("href") + "\n" ;
	                $(this).attr("href" ,     pathFullQualifier +   $(this).attr("href"));
	            }
	        });
	    }
	</script>
	
	
	
	<script language="javascript" >
	    function submitUserLogonPopOverForm(form)
	    {
	        disableButton("userLogonBtn");
	        form.submit();
	        return true;
	    }
    </script>
