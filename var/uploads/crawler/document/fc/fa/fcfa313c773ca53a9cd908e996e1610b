<script type="text/javascript">
$(window).load(function(){
    function beefyTAni(loopvar){
        $("#btnreplay").fadeOut(500);
        $("#btncover").fadeIn(1000);
        setTimeout(function(){$("#btpeople,#bttext1").fadeOut(1000);},2000);
        setTimeout(function(){$("#bt70s,#bt70s_text").animate({left:"0"},1500)},3000);
        setTimeout(function(){
            $("#bt70s_text").fadeOut(500);
            $("#bt80s,#bt80s_text").animate({left:"0"},1500)
        },5500);
        setTimeout(function(){
            $("#bt80s_text").fadeOut(500);
            $("#bt90s,#bt90s_text").animate({left:"0"},1500)
        },8000);
        setTimeout(function(){
            $("#bt70s,#bt80s,#bt90s,#bt90s_text").fadeOut(1000,function(){
                $("#bt70s,#bt80s,#bt90s,#bt70s_text,#bt80s_text,#bt90s_text").removeAttr("style");
            });
        },11500);
        setTimeout(function(){
            $("#btpeople,#bttext1").fadeIn(1500);
            if((loopvar==1) || (typeof loopvar==="undefined")){
                $("#btnreplay").fadeIn(1500);
                $("#btncover").fadeOut(1500);
            }
        },12800);
        if((loopvar!=1) && (typeof loopvar!=='undefined')){
            setTimeout(function(){
                loopvar--;
                beefyTAni(loopvar);
            },14300);
        }
    };
    if(!HNS.isMobile){beefyTAni(3);}
    $("#btnreplay").click(function(e){
        e.preventDefault();
        beefyTAni(1);
    });
});
</script>
