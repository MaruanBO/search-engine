  <script type="text/javascript">
    function submitEmailSignUpFormPopupAlt(email, emailConfirm, form) {
      var ERR1 = jQuery("#emailSignUp_error_msg_1_alt").html(); //Email addresses must match
      var ERR2 = jQuery("#emailSignUp_error_msg_2_alt").html(); //Invalid email address
      var DISP1 = jQuery("#emailSignUp_display_msg_1_alt").html(); //Enter your email address
      var DISP2 = jQuery("#emailSignUp_display_msg_2_alt").html(); //Confirm your email address
      var COLOR = jQuery("#emailSignUp_error_text_color_alt").html(); //#666666

      var isValid = false;
      var e1 = jQuery(email).val();
      var e2 = jQuery(emailConfirm).val();

      emailValidationAlt(COLOR, DISP1, DISP2, ERR1, ERR2, e1, e2, "#emailSignUpPopupForm_alt", email, emailConfirm, form);

      return isValid;
    }
    function openPopupAlt() {
      $('#Email_Signup_Alt_Container').fadeIn('slow');
    }
    function closePopupAlt() {
      $('#Email_Signup_Alt_Container').fadeOut('slow');
    }
    function doClearPopupAlt(theText) {
      var COLOR = jQuery("#emailSignUp_msg_text_color_page").html(); //#666666
      if (theText.value == theText.defaultValue || theText.value == 'Email addresses must match' || theText.value == 'Invalid email address' || theText.value == 'Enter your email address') {
        theText.style.color = COLOR;
        theText.value = "";
      }
      if (theText.id == 'emailSignUp_page') {
        if (($("#emailSignUp_page_confirm").val() == 'Invalid email address') || ($("#emailSignUp_page_confirm").val() == 'Email addresses must match'))
        { theText.value = ''; }
      }
    }
    function setTextPopupAlt(theText, elmName) {
      var DISP1 = jQuery("#emailSignUp_display_msg_1_alt").html(); //Enter your email address
      var DISP2 = jQuery("#emailSignUp_display_msg_2_alt").html(); //Confirm your email address
      var COLOR = jQuery("#emailSignUp_msg_text_color_alt").html(); //#666666
      jQuery(elmName).val(theText.value);
      if (theText.value == "") {
        if (elmName == "#emailSignUp_popup_alt")
          theText.value = DISP1;
        if (elmName == "#emailSignUp_popup_confirm_alt")
          theText.value = DISP2;
        theText.style.color = COLOR;
      }
    }
    function emailValidationAlt(COLOR, DISP1, DISP2, ERR1, ERR2, e1, e2, formName, email, emailConfirm, form) {
      var isValid = false;

      if ((e1.length > 0 && e1 != DISP1 && e1 != ERR1 && e1 != ERR2) && (e2.length > 0 && e2 != DISP2 && e2 != ERR1 && e2 != ERR2)) {
        if (e1 != e2) {
          jQuery(emailConfirm).val(ERR1);
          jQuery(emailConfirm).css("color", COLOR);
          return isValid;
        } else if (!isValidEmail(e1)) {
          jQuery(emailConfirm).val(ERR2);
          jQuery(emailConfirm).css("color", COLOR);
          return isValid;
        }
        isValid = true; // return true when e1=e2 (and length>0) and it's a valid email address
      } else if (!isValidEmail(e1) && (e2 == DISP2 || e2 == ERR1 || e2 == ERR2)) {
        jQuery(emailConfirm).val(ERR2);
        jQuery(emailConfirm).css("color", COLOR);
        return isValid;
      } else if (!isValidEmail(e2) && (e1 == DISP1 || e1 == ERR1 || e1 == ERR2)) {
        jQuery(email).val(ERR2);
        jQuery(email).css("color", COLOR);
        return isValid;
      } else if (isValidEmail(e1) || isValidEmail(e2)) { // if none of above matches, shopper only enters one valid email address
        isValid = true;
      }

      if (isValid) {
        //alert(sId);alert(cId); // set in Header1CachedDisplay.jsp
        form.storeId.value = sId;
        form.catalogId.value = cId;

        if (isValidEmail(e1)) {
          form.emailSub.value = e1;
        } else if (isValidEmail(e2)) {
          form.emailSub.value = e2;
        }

        jQuery(formName).ajaxSubmit(
          {
            url: '/webapp/wcs/stores/servlet/HanesEEMSEmailSignUpCmd',
            type: 'post',
            error: function (data) {
              showErrorMessage(data);
            },
            success: function (html, status) {
              //closePopup();
              closePopupAlt();
              showEmailSignupSuccess('divEmailSignupSuccess');
              addEmailSignUpInPageTag(form.emailSub.value);
              callEFTag();
            }
          });
      }
    }
    function privacyPopupAlt() {
      var w = window.open('/webapp/wcs/stores/servlet/staticb_PrivacyPopup_13201_-1_13501', 'openwindow',
'directories=0,width=600,height=600,scrollbars=yes,resizable=no,maximize=yes,screenX=20,screenY=20,left=20,top=20');
    }
  </script>
</div>
            </div>
        
    
</div>
