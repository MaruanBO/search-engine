                if(jsCIOSearchTerm == null || jsCIOSearchTerm == ''){
                    var ref = document.referrer;
                    ref = decodeURIComponent(ref);
                    
                    if(ref.indexOf('google') >= 0 || ref.indexOf('bing') >= 0 || ref.indexOf('yahoo') >= 0 || ref.indexOf('aol') >= 0 || ref.indexOf('ask') >= 0){
                        if (ref.indexOf('?') >= 0){
                            var qs = ref.substr(ref.indexOf('?')+1);
                            var qsa = qs.split('&');
                            for (var i=0;i<qsa.length;i++) {
                                var qsip = qsa[i].split('=');
                              if (qsip.length == 1) continue;
                              if (qsip[0] == 'q' || qsip[0] == 'as_q' || qsip[0] == 'as_epq' || qsip[0] == 'as_oq' || qsip[0] == 'x_query' || qsip[0] == 'dq' || qsip[0] == 'hl' || qsip[0] == 'query' || qsip[0] == 'p' || qsip[0] == 'Q' || qsip[0] == 'aps_terms' || qsip[0] == 'encquery' || qsip[0] == 'ask') { 
                                    var wordstring = unescape(qsip[1].replace(/\+/g,' '));
                                    jsCIOSearchTerm = wordstring;
                              }
                            }
                        }
                    }
                    
                }

                IntelligentOfferJS.declareRefreshController("WC_IntelligentOfferESpot_controller_ID_HDZ1",  "WC_IntelligentOfferESpot_context_ID_HDZ1");
