	                        <script language="javascript" type="text/javascript">
	                        var isSecure = ("https"==jQuery.url.attr("protocol"))?true:false;
                            var qvBaseURL = isSecure? "http://s7d5.scene7.com/is/image/Hanesbrands/" : "https://a248.e.akamai.net/f/248/9086/10h/origin-d5.scene7.com/is/image/Hanesbrands/";
                            var qvStoreNameS7 = "HNS_";
                            var qvS7params = "?wid=225&resMode=sharp2&op_usm=1.7,1.0,15,0";
                            var qvPartNumber = "";
                            var qvColor = "";
	                        var deviceType;
	                        var s7ZoomViewerQV;
	                        
	                        var s7server = "http://s7d5.scene7.com"; // "http://s7d5.scene7.com";  "https://a248.e.akamai.net/f/248/9086/10h/origin-d5.scene7.com";
	                        if(isSecure){
	                           s7server = 'https://a248.e.akamai.net/f/248/9086/10h/origin-d5.scene7.com';
	                        }
	                        
	                        function displayS7Image(partnumber, featureColor){
		                        if(!window.isMobile){/*allow flash*/
		                            deviceType=sjGetDeviceType();
		                             
		                            //var s7sku='OHP_22738_Black_is';
		                            var s7sku= 'HNS_'+partnumber+'_'+featureColor+"_is";
		                            
	                                if(document.getElementById("s7Container") == null){ // this is required for Scene7 to find the container div
	                                    document.getElementById("s7ContainerDiv").innerHTML = 
	                                    '<div id="s7Container"><img alt="" src="/wcsstore/HanesB2CStorefrontAssetStore//images/NoImageIcon.jpg" border="0" class="product_image" id="productFullImage" /></div>';  
	                                }                                
		                            	                          
		                            s7ZoomViewerQV = new S7ZoomViewerQV('Hanesbrands');
		                            	                            
	                                s7ZoomViewerQV.setServerUrl(s7server+'/is/image/');
	                                s7ZoomViewerQV.setContentRoot(s7server+'%2fskins');
	                                //LISO-475 Change template change
	                                s7ZoomViewerQV.setCodeRoot(s7server+'/is-viewers-4.1/flash/');
		                            s7ZoomViewerQV.setViewerWidth('225');
		                            s7ZoomViewerQV.setViewerHeight('422');
		                            s7ZoomViewerQV.setBGColor('#ffccff');
		                            s7ZoomViewerQV.setDiv(document.getElementById("s7Container"));
		                            s7ZoomViewerQV.setNoFlashPreset('s7product');
		                            s7ZoomViewerQV.setConfig('Hanesbrands/DTC_view2_QV');
		                            s7ZoomViewerQV.setSKU(s7sku);  //Pass imagesetname
		
		                            switch (deviceType) {
		                            case 2: result = drawFlash(); break;
		                            case 1: break;
		                            }
		                        }
		                        else
		                        { deviceType=0; }
	                        }
	                        
	                        function drawFlash(){
	                               s7ZoomViewerQV.draw();
	                        }
	
	                        function setColor(col) {
	                               switch (deviceType) {
	                                      case 2: 
	                                            s7ZoomViewerQV.setFlashParam('image', col,isSecure);
	                                            break;
	                                      default: 
	                                             var img = document.getElementById("productFullImage");
	                                             //img.src = 'http://s7d5.scene7.com/is/image/' + col + '?wid=200';
	                                             //img.src = s7server+'/is/image/' + col + '?wid=200';
	                                             img.src = qvBaseURL + qvStoreNameS7 + qvPartNumber + "_" + qvColor + qvS7params;
	                                             break;
	                               }
	                        }
	                                                
	                        function setOptions(chosen,swatch,partNumber) {
	                            var hanesProductBaseImageQV = 'Hanesbrands/HNS_'+partNumber+'_'; 
	                            
	                            var chosenImage = chosen.replace(/\//g, "\\");
	                            chosenImage = chosenImage.replace(/\\/g, "");
	                            chosenImage = chosenImage.replace(/-/g, "");
	                            chosenImage = chosenImage.replace(/ /g, "");
                                qvColor = chosenImage;

	                            setColor(hanesProductBaseImageQV + chosenImage + '_is');
	                            updateSizeCombo(chosen);
	                            document.getElementById("featureColorSelected").innerHTML = chosen;                            
	                            document.getElementById("productColor").innerHTML = chosen;
	                            document.getElementById('productColorPriceDiv').style.display = 'inline-block';
	                        }                                                
	                                                                        
