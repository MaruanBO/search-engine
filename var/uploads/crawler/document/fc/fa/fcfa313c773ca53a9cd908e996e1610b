<script type="text/javascript">
    
	var atcDLSrc = 'disabled';
	var atcDLType = 'disabled';
	var atcDLCat = 'disabled';
    
    // need to replace "Continue Shopping" URL to enable page refresh in checkout page
    dojo.addOnLoad(function() {   
        if("https"==jQuery.url.attr("protocol")){
            bSetCheckOut = false;
            if(document.URL.indexOf("?") != -1){
                var url = document.URL.substring(0, document.URL.indexOf("?"));
                if(url.indexOf("OrderItemDisplay")!=-1){
                    bSetCheckOut = true;
                }
            }else{
                if(document.URL.indexOf("OrderItemDisplay") != -1){
                    bSetCheckOut = true;
                }
            }
            
            if(bSetCheckOut){
                jQuery('#continue_shopping').click(function(){
                     closeAllDialogs();
                     location.reload(); 
                     return true;                    
                });
                jQuery('#continueLink').attr('href', 'javascript:closeAllDialogs();location.reload();');                            
            }
        }        
    });

    dojo.addOnLoad(function() {
        parseWidget("quickview_shopcart_popup"); 
    });
    
    dojo.addOnLoad(function(){
        dojo.connect(window, 'onresize', function(){
            //MessageHelper.adjustCoordinates();
        });
    });    
</script>

