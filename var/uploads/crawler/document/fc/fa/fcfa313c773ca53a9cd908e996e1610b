                console.warn('before cmRecRequest');
                cmRecRequest('HDZ1','','_TS_','', jsCIOSearchTerm);
                cmDisplayRecs();

                //The out-of-the-box sequence, confirmed by Coremetrics to be correct, is eluminate.js needs to be loaded in the head of the HTML, cmRecRequest() is called, followed by cmDisplayRecs()...
                //For some unknown reason, the eluminate js, hosted by lib.coremetrics.com, can fail to load the io_zone (TypeError: this.io_zone is undefined) and _io_config (TypeError: _io_config is undefined) variables
                //When this happens, no recommendations are loaded. We can, however evaluate the length of the zone widget (e.g. WC_IntelligentOfferESpot_container_ID_HDZ1); If the length is zero, no recommendations were loaded
                //Attempting re-retrieval 3 times
                var cmMaxAttempts = 3;
                var cmIntervalDuration = 2500; //in milliseconds
                var cmIntervalHDZ1 = setInterval(function(){cmTimerHDZ1()}, cmIntervalDuration);
                var cmLoadedHDZ1 = false;

                setTimeout(function(){
                    updateCMVCPrefixHDZ1();
                    if((!$.trim($('#WC_IntelligentOfferESpot_container_ID_HDZ1').html()).length)) {
                        console.error('cmRecRequest failed after maximum number of re-attempts (' + cmMaxAttempts + '); _io_config: ',_io_config);
                    }
                    clearInterval(cmIntervalHDZ1);
                    cmLoadedHDZ1 = true;
                },(cmMaxAttempts * cmIntervalDuration));

                function cmTimerHDZ1() {
	                if((!$.trim($('#WC_IntelligentOfferESpot_container_ID_HDZ1').html()).length && !cmLoadedHDZ1)) {
                        console.warn('cmRecRequest failed; re-attempting; before cmRecRequest');
		                cmRecRequest('HDZ1','','_TS_','', jsCIOSearchTerm);
		                cmDisplayRecs();
	                } else {
                        updateCMVCPrefixHDZ1();
	                    clearInterval(cmIntervalHDZ1);
	                    cmLoadedHDZ1 = true;
                        console.log('cmRecRequest successful');
	                }
                }
                
                function updateCMVCPrefixHDZ1() {
                    var zoneCMVCPrefix = 'cm_vc=hanes_' + getSiteType() + '_';
                    var zoneHTML = $('#WC_IntelligentOfferESpot_container_ID_HDZ1').html();
                    zoneHTML = zoneHTML.replace(/cm_vc=hanes_desktop_/g, zoneCMVCPrefix);
                    $('#WC_IntelligentOfferESpot_container_ID_HDZ1').html(zoneHTML);
                }
