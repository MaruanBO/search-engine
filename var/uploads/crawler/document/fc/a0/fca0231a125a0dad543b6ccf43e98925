    <script type="text/javascript">
        var productUrlStart = "/Product/offerCode";  
        
        $(document).ready(function()
        {
                
                // Setup Home Page Overlay dialog
                var $overlayContainer = jQuery('#homepage-overlay-container'),
                    dialogOptions = {
                        autoOpen: true,
                        modal: true,
                        resizable: false,
                        draggable: false,
                        dialogClass: 'homepage-overlay-dialog',
                        width: "auto",
                        height: "auto"
                    };

                $overlayContainer.dialog(dialogOptions);

                // on window resize, reposition everything
                $(window).resize(function ()
                {
                    $overlayContainer.dialog('option', 'position', 'center');
                });
                

            // Setup last 12 items on air carousel
            $("#last-items-aired-carousel").touchCarousel({
                pagingNav: false,
                snapToItems: false,
                itemsPerMove: 2,
                scrollToLast: false,
                loopItems: false,
                scrollbar: false
            });

            resizeText();
            
            var watchUsLive = $.connection.onAirHub;

            $.extend(watchUsLive.client, {
                HomepageOnAirProductUpdate: function (onAirItemData)
                {
                    if (onAirItemData.OnAirItem && onAirItemData.OnAirItem.OfferFound === true &&
                        onAirItemData.OnAirItem.OfferCode !== "")
                    {
                        onCurrentOnAirChanged(onAirItemData);
                    }
                }
            });
            
            //

            //var configOptions = {};
            //if (shopHq.pageUtils.isIPad())
            //{
            //    configOptions = { transport: 'longPolling' };
            //}

            //$.connection.hub.start(configOptions);

        });
        
        function onCurrentOnAirChanged(onAirItemData)
        {
            //UpdateCurrentItem          
            var imgHref;
            var offerCode = $("#currentOfferCode").val();
            var currentDescription = offerCode + " - " + $("#divOAProduct a")[0].innerHTML;
            var currentOnAirImage = $("#currentShowHeaderLink img");
            var offerLink = "";

           //Don't update the current offer if the updated and current are the same...
            if (offerCode === onAirItemData.OnAirItem.OfferCode)
            {
                return;
            }

            var currentOfferData =
            {
                offerCode: offerCode,
                imgUrl: currentOnAirImage[0].src,
                title: currentDescription
            };

            imgHref = productUrlStart.replace("offerCode",onAirItemData.OnAirItem.OfferCode) +
                '?storeid=1&icid=OnAirNow-_-' + onAirItemData.NowPlayingShow + ' -_-' + onAirItemData.OnAirItem.OfferCode + '&cm_re=TV1-_-ONAIR-_-N';

            $("#currentShowHeaderLink").attr("href", imgHref);
            currentOnAirImage.attr("src", onAirItemData.OnAirItem.ImageUrl);
            currentOnAirImage.attr("alt", onAirItemData.OnAirItem.OfferCode);

            $("#onAirShow").attr("title", onAirItemData.NowPlayingShow).html(onAirItemData.NowPlayingShow);

            offerLink = productUrlStart + "/" + onAirItemData.OnAirItem.OfferCode + "?" + onAirItemData.OnAirItem.TrackingCode + "&track=-10102&ciid=11281";
            
            $(".homeThisHourHeader").trigger("destory.dot");
            $("#divOAProduct a").trigger("destroy.dot");
            
            $("#divOAProduct a").attr("href", offerLink);
            $("#divOAProduct a")[0].innerHTML = onAirItemData.OnAirItem.TopLine;

            $("#divOAProduct span")[0].innerHTML = onAirItemData.OnAirItem.PriceLabel + ":";
            $("#divOAProduct span")[1].innerHTML = onAirItemData.OnAirItem.CurrentPrice;

            $("#currentOfferCode").html(onAirItemData.OnAirItem.OfferCode);
            
            resizeText();

            updateLastTwelveItems(currentOfferData);
            shopHq.pageUtils.flashBackground($(".homeOnAir"), '255,255,204');
        }
        
        function resizeText()
        {
            $(".homeThisHourHeader").dotdotdot({ height: 30 });
            $("#divOAProduct a").dotdotdot({ height: 50 });
        }
        
        function updateLastTwelveItems(offerData)
        {
            var currentLastItem = $("#last-items-aired-carousel li.last").clone(true, true);
            var nextLastItem = $("#last-items-aired-carousel li.last").prev();
                
            if (currentLastItem.length > 0)
            {
                if ($(currentLastItem).attr("data-offercode") !== offerData.offerCode &&
                    offerData.offerCode !== "")
                {
                    currentLastItem.removeClass("last");
                    $(currentLastItem).attr("data-offercode", offerData.offerCode);

                    $("img", currentLastItem).attr("alt", offerData.offerCode).attr("src", offerData.imgUrl)
                        .attr("title", offerData.title);

                    currentLastItem.addClass("addQuickViewButton");

                    $("#last-items-aired-carousel ul").prepend(currentLastItem);
                    $("#last-items-aired-carousel li.last").remove();
                    $(nextLastItem).addClass("last");
                }
            }
        }
    </script>

    </footer>
    

