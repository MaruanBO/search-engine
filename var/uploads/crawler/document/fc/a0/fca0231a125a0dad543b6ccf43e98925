<script type="text/javascript">
    shopHq.quickview.viewModel = new shopHq.quickview.QuickviewViewModel({"OfferId":0,"OfferCode":null,"FamilyId":null,"Title":null,"ShortDescription":null,"LongDescription":null,"PriceLabel":null,"Price":{"Low":0,"High":0},"Colors":[],"Sizes":[],"DeliveryOptions":[],"Skus":[],"ColorSwatches":[],"AdditionalPhotos":[],"Promotions":[]});

    $("#quick-view-dialog").on("click", "#more-photos img, #quick-view-color-carousel img", function () {
            var useOverlay = false;

            if (this.getAttribute("data-colorcode") !== null) {
                shopHq.quickview.viewModel.selectedColorText(this.getAttribute("alt"));
                useOverlay = true;
            }
            
            shopHq.quickview.viewModel.setSelectedImage(this.src, useOverlay);

        });
        $("#quick-view-dialog").on("change", "#product-options select", function () {
            var index;
            var viewModel = shopHq.quickview.viewModel;

            if (viewModel.OfferCode() != null) {
                switch (this.id) {
                    case "quickViewColorDropDown":
                        // Scroll to selected item in color carousel
                        index = this.selectedIndex;

                        if (index > 0) {
                            if (viewModel.carousel.numItems > 3)
                                viewModel.carousel.goTo(index - 1);

                            viewModel.setSelectedImage($(viewModel.carousel.items[index - 1].item).children("img")[0].src, true);
                        }
                        else
                            if (viewModel.AdditionalPhotos().length > 0) {
                                viewModel.setSelectedImage(viewModel.AdditionalPhotos()[0].Url(), true);
                            }
                            

                        break;
                    case "quickViewSizeDropDown":
                        // Update the image only if there are colors
                        if (viewModel.Colors().length > 1 && viewModel.carousel !== null) {
                            index = 0;

                            if (viewModel.selectedColorValue() !== undefined) {
                                for (var i = 0; i < viewModel.Colors().length; i++) {
                                    if (viewModel.Colors()[i].Value() === viewModel.selectedColorValue()) {
                                        index = i;
                                        break;
                                    }
                                }
                            }

                            viewModel.setSelectedImage($(viewModel.carousel.items[index].item).children("img")[0].src, true);
                        }
                        break;

                    case "quickViewProgramDropDown":
                        if (viewModel.DeliveryOptions().length > 0) {
                            viewModel.adprogramChange();
                        }
                     break;
                }
            }
        });
    
    
</script>

