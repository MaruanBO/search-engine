    <script type="text/javascript">
        $(document).ready(function ()
        {
            /* Start - To highlight the menu button in global nav*/
            $("li.category").each(function () {
             var curUrl = window.location.href;
             var ipos = (curUrl.indexOf("/c/") > -1) ? curUrl.indexOf("/c/") + 3 : (curUrl.indexOf("/b/") > -1 ? curUrl.indexOf("/b/") + 3 : -1);
               
                if (ipos > -1) {
                    var menucat;
                    if (curUrl.indexOf("/", ipos) > -1)
                        menucat = curUrl.substring(ipos, curUrl.indexOf("/", ipos));
                    else if (curUrl.indexOf("?", ipos) > -1)
                        menucat = curUrl.substring(ipos, curUrl.indexOf("?", ipos));
                    else {
                        menucat = curUrl.substring(ipos);
                    }
                    if ($(this).find("a").attr("href").indexOf(menucat.toLowerCase()) > -1)
                        $(this).removeAttr("class").attr("class", "category active");
                    else {
                        $(this).removeAttr("class").attr("class", "category");
                    }
                } else {
                    $(this).removeAttr("class").attr("class", "category");
                }
            });
            /* End - To highlight the menu button in global nav*/

            shopHq.pageUtils.initialize();
            shopHq.pageUtils.initializeSiteCatalystTracking();
        });
    </script>
