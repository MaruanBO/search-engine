    $(document).ready(function ()
    {
        $('#overlay-email').onEnter(function () { shopHq.creative.subscribeToNewsletter(this.value); $('#homepage-overlay-container').dialog('close');});
    });
 
    $.fn.onEnter = function (func)
    {
        this.bind('keypress', function (e)
        {
            if (e.keyCode == 13) func.apply(this, [e]);
        });
        return this;
    };
    
    return creative;
})(shopHq.creative = shopHq.creative || {}, jQuery);
 
</script>
