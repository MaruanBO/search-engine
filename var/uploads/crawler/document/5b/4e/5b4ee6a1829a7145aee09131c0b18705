<script language="javascript" type="text/javascript" src="http://attonlineoffers.com/att_js/footprint.js?v=1424382310"></script>
<script language="javascript" type="text/javascript">
	/* generate atlas tags for channel entry content */
	gen_tags();

	// get source from query string and append to all links
	appendVariableToPageLinks('attonlineoffers.com/specialoffers', 'source');
	appendVariableToPageLinks('attonlineoffers.com/specialoffers', 'bkvcid');
	appendVariableToPageLinks('attonlineoffers.com/specialoffers', 'mtag');
	 
	footprintTrackPageView('');

	function showTags() {
		elem = '';
		$(document).ready(function() {
			var atlas_links = '';
			var atlas_links_desc = '';
			var a_link_temp = '';
			var all_links = new Array();
			var a_link_temp_sub_array = new Array();
			var a_link_temp_sub = '';
			var a_tag_count = 0;
			
			$('a[href*=view]').each(function() {
				a_tag_count++;
				
				a_link_temp = $(this).attr("href");
				
				if(inLinkArray(a_link_temp, all_links))
				{
				
				}
				else
				{
					/* set all titles to current href */
					$(this).attr('title', $(this).attr('href'));
					all_links.push(a_link_temp);
					atlas_links += '<li style="margin:4px; padding:0; font-size:11px; list-style:none; cursor:pointer; text-decoration:underline;" onclick="revealTag(\'' + a_tag_count + '\')">' + a_link_temp + '</li>';
					
					
					$(this).css({
						'border': '3px solid #900',
						'background': 'rgba(200, 54, 54, 0.5)',
						'color': '#fff',
						'text-shadow': 'none'
					});
					$(this).attr('name', 'atlas_tag_' + a_tag_count);
					$(this).addClass('atlas_tag_' + a_tag_count);
					a_link_temp_sub_array = a_link_temp.split('_');
					a_link_temp_sub = a_link_temp_sub_array[1];
					atlas_links_desc += '<li style="margin:4px; padding:0; font-size:11px; list-style:none; border-bottom:1px solid #292929;">' + a_link_temp_sub + '</li>';
				}
			});

			$(function() {
				$('ul#atlas_ul').html(atlas_links);
				$('ul#atlas_desc_ul').html(atlas_links_desc);
			});
			
			/* add cover for tag reveal when clicked */
			$('body').append('<div class="showtags_cover" onclick="removeShowTagsCover()" style="position:fixed; display:block; top:0; left:0; width:100%; height:100%; background:rgba(0, 0, 0, 0.8); z-index:999;">&nbsp;</div>');
			$('.showtags_cover').hide();
			$('.atlas_tag_table').show();
		});
	}
	
	function inLinkArray(needle, haystack) {
		var length = haystack.length;
		for(var i = 0; i < length; i++) {
			if(haystack[i] == needle) return true;
		}
		return false;
	}
	
	function revealTag(id)
	{
		elem = $('.atlas_tag_' + id);
		elem_id = id;
		elem.css({ 'position': 'absolute', 'z-index': '9999'});
		$('.showtags_cover').fadeIn('fast');
		$('html,body').animate({
			scrollTop: elem.offset().top - 250
		}, 500);
	}
	
	function removeShowTagsCover()
	{
		elem = $('.atlas_tag_' + elem_id);
		elem.css({ 'z-index': '99'});
		$('.showtags_cover').hide();
	}
</script>
