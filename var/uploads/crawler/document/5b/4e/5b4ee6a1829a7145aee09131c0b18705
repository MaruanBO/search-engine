	$(document).ready(function() {
		$('body').prepend('<div class="blackCover"></div>');
		$('body').prepend('<div id="modal">test test</div>');
		$('.modal_button').live('click', function() {
			var modal_id = parseInt($(this).attr('modal-key'));
			var modal = modalsJSON.modals[modal_id];
			var actTag = $.trim($(this).attr('modal-tag'));
			$('#modal').html(modal.content);
			$('#modal').prepend('<div class="modalClose">x</div>');
			$('#modal').css({'height': modal.h, 'width': modal.w, 'left': ($(window).width() / 2) - (modal.w / 2), 'padding': modal.p});
			$('.blackCover').fadeIn();
			if(actTag.length){$('#modal').append('<s'+'cript language="JavaScript" src="http://view.atdmt.com/jaction/'+actTag+'"></s'+'cript>');}
			$('#modal').fadeIn();
			$('#modal').animate({'top': modal.t});
			return false;
		});
		
		$('.blackCover, .modalClose').live('click', function() {
			$('#modal').animate({'top': '-1500px'}, function() {
				$('.blackCover').fadeOut();
				$('#modal').html('');
				$('#modal').hide();
			});
		});
	});
</script>





