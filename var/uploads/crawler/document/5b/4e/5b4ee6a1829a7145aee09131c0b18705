    $(window).load(function () {
      $('#slider').attSlider({
        slideTime: 6000,
        slideWidth: 974,
        slideSpeed: 600,
        easeType: "swing",
        playing: true,
        playpauseSelector: '#playpause'
      });
    
    // FOR MAIN NAV HOVER EFFECT
      
    
    // FOR BEST OFFERS MENU
    $('li.nav-alt,li.noHover').hover(function () {
       $('.menu ul li > a').removeClass("selected bg-white");
       var ID=$(this).attr('id');
       //alert(ID);
       if(ID=="home" || ID=="wireless-service" || ID=="tablets" || ID=="wireless-cards" || ID=="holidays")
       {
       $('.menu ul li > a').removeClass("selected bg-white");
       //alert(ID);
       $('li.nav-alt ul').hide();
       }else
       {
            
            
             $('.menu ul li > a').removeClass("selected bg-white");
             $('ul',this).show();
   
      }
     
    },function() {
    var hre = jQuery(location).attr('href');
    var seg1="specialoffers";
    var seg2="";
    if(seg1=="bestoffers")
    {
     if(seg2=="triple-bundles" || seg2=="double-bundles" )
     {
      $('.bundles-menu').show();
      $("li.nav-alt > a[href='http://attonlineoffers.com/bestoffers/triple-bundles']").addClass("selected"); 
     }
     else if(seg2=="smartphones" || seg2=="refurb-phones" || seg2=="gophones" || seg2=="android-phones" || seg2=="popular-phones")
     { 

      $('.phones-menu').show();
      $("li.nav-alt > a[href='http://attonlineoffers.com/bestoffers/popular-phones']").addClass("selected"); 
     }
     else if(seg2=="tv" || seg2=="internet" || seg2=="home-phone" || seg2=="moving-transfer-service")
     { 

      $('.uverse-menu').show();
      $("li.nav-alt > a[href='http://attonlineoffers.com/bestoffers/tv']").addClass("selected"); 
     }
     
     else
     { 
      
       $("[href='"+hre+"']").addClass("selected bg-white");
     }
    }
    
    if(seg1=="specialoffers")   // FOR SPECIAL OFFERS MENU
    {
          if(seg2=="smartphones" || seg2=="refurb-phones" || seg2=="gophones" || seg2=="android-phones" || seg2=="popular-phones")
           { 
      
            $('.phones-menu').show();
            $("li.nav-alt > a[href='http://attonlineoffers.com/specialoffers/popular-phones']").addClass("selected"); 
           }
           else
           { 
          
           $("[href='"+hre+"']").addClass("selected bg-white");
           }
            
      
      }
    });
    
    
    

    
    
    
    
    
    
    
    });
    
    
    
