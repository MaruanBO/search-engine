	// <![CDATA[
	jQuery(function() {
		jQuery('#blog-chooser select').change(function () {
			if (this.selectedIndex > 0)
				location.href = jQuery(this).val();
		});
		jQuery('#blog-chooser input').click(function () {
			if (this.form.to.selectedIndex > 0)
				location.href = jQuery(this.form.to).val();
		});
	});
