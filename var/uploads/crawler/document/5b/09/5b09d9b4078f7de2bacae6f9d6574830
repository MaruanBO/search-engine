<script type="text/javascript">
jQuery(document).ready(function() {
	var searchInputUnselectedText = "Search Local Businesses";
	if(searchInputUnselectedText != jQuery("div#hst_topclassifieds_search div.header span.buttons input.searchInput").val() )
	{
		jQuery("div#hst_topclassifieds_search div.header span.buttons input.searchInput").addClass("focus");
	}
	
	jQuery("div#hst_topclassifieds_search div.header span.buttons input.searchInput").focus( function() {
		if(searchInputUnselectedText == jQuery(this).val())
		{ 
			jQuery(this).val("");
			jQuery(this).addClass("focus");
		}
	});
	jQuery("div#hst_topclassifieds_search div.header span.buttons input.searchInput").blur( function() {
		if("" == jQuery(this).val())
		{ 
			jQuery(this).val(searchInputUnselectedText);
			jQuery(this).removeClass("focus");
		}
	} );

	$("div#hst_topclassifieds_search ul li").hover(function(event) {
		$("div#hst_topclassifieds_search ul li").unbind('click');
		$("div#hst_topclassifieds_search ul li").unbind('handle');
	});
});
</script>

