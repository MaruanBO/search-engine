function MM_goToURL() {
   var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
   for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}
function popit(winURL, winName, winFeatures, winObj) {
   var theWin;
   if (winObj != null) {
      if (!winObj.closed) {
         winObj.focus();
         return winObj;
      } 
   }
   theWin = window.open(winURL, winName, winFeatures); 
   return theWin;
}
function focusInput(id) {
	var formInput = document.getElementById(id);
		if (formInput.value == 'Username') { formInput.value = ''; }
	if (formInput.value == 'password') { formInput.value = ''; }
}
function blurInput(id) {
	var formInput = document.getElementById(id);
		switch(id) {
		case 'loginuser':
			var resetValue = 'Username';
			break;
		case 'loginpass':
			var resetValue = 'password';
			break;
	}
	if (formInput.value == '') { formInput.value = resetValue; }
}
function submitLogin() {
	var user = document.getElementById('loginuser').value;
	var pass = document.getElementById('loginpass').value;
	if ((user!='Username')&&(pass!='password')) {
		document.login1.submit();

	}
}
