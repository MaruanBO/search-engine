		var len1 = fcat[n][0].length;
		var popcatadata = "";
		for(var i=0;i<len1;i++)
		{
			var ss = fcat[n][0][i].split('||');
			if(ss[1] != "See More")
			{
				popcatadata += "<a href='" +ss[0]+"' class='hm_catlink' onclick=\"trackURL('http://track.rediff.com/click?url=___"+encodeURIComponent(ss[0])+"___&cmp=USHP&lnk=shopping_top_"+encodeURIComponent(ss[1])+"&nsrv1=USHP');return false;\" >";
