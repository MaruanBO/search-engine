function show_recommendation(data)
{
	try{
		var totalrecom = 0;
		var rndno = 0
		var recoproduct = data.product_list.CAT_DATA[0].PRODUCTS;
		if(!recoproduct){return false;}
		totalrecom = recoproduct.length;
		
		if(totalrecom == 0){return false;}
		var recomertic_ids = "";
		rndno = (Math.floor(Math.random()*totalrecom));

		if(rndno == (totalrecom -1)){rndno = 0;}
		
		var recodata = "<p class='bold f14'>Recommended Products For You</p>";

		for (var i=rndno,s=0; s<2 ;i++,s++)
		{
			var prdurl = recoproduct[i].URL ;
			var prdimg = recoproduct[i].IMAGE;
			var prdtitle = recoproduct[i].NAME;
			var prdlink = recoproduct[i].LINK_NAME;
			var prdid = recoproduct[i].ID;
			var listprice = recoproduct[i].PRICE;
			var webprice = recoproduct[i].OFFER_PRICE;
			var displistprice = recoproduct[i].DISPLAY_PRICE;
			var dispwebprice = recoproduct[i].DISPLAY_OFFER_PRICE;
			var pvrfnbr = recoproduct[i].pvrfnbr;

			recodata += "<div class='prd_innerbox ";
			if(s==0){recodata += "floatL'>";}else{recodata += "floatR'>";}
			recodata += "<a href='"+prdurl+"' class='black' onclick=\"trackURL('http://track.rediff.com/click?url=___"+prdurl+"?sc_cid=ushome_recommended_"+prdid+"___&cmp=USHP&lnk=shopping_recommended_"+prdid+"&nsrv1=USHP');return false;\" >";
			recodata += "<div class='prdtrnd_img'>";
			recodata += "<img src=\""+prdimg+"\" alt=\""+prdtitle+"\" />";
			recodata += "</div>";
			recodata += "</a>";
			recodata += "<div class='trend_copy'>";
			recodata += "<a href='"+prdurl+"' class='black' onclick=\"trackURL('http://track.rediff.com/click?url=___"+prdurl+"?sc_cid=ushome_recommended_"+prdid+"___&cmp=USHP&lnk=shopping_recommended_"+prdid+"&nsrv1=USHP');return false;\" >";
			recodata += "<p title=\""+prdtitle+"\" style='height:3.5em;overflow:hidden;' class=f12>"+prdtitle+"</p>";
			recodata += "</a>";

			recodata += "<i class=ht5></i><p class=shprice>"
			if(listprice > webprice)
			{
			recodata += "<span class=listprice><del>Rs."+displistprice+"</del></span>&nbsp; ";
			}
			recodata += "<span class='black'>Rs."+dispwebprice+"</span>";
			recodata += "</p><i class='ht5'></i>";

			recodata += "<a href=\"http://commerce.rediff.com/commerce/v3/newaddcart.jsp?prtitle="+prdlink+"&amp;prodtags=&amp;catid=&amp;primg="+prdimg+"&amp;type=S&amp;prrfnbr="+prdid+"&amp;pvrfnbr="+pvrfnbr+"&amp;menbr=&amp;wishlistadd=false&amp;showpopup=false&amp;prodkey=&amp;dynaprice=&amp;dynatype=0&amp;frompg=ushome_recommended_"+prdid+"&amp;flnm=/recommendation/"+prdlink+"/"+prdid+"&amp;flag=*\">";
			
			recodata += "<span class='trnd_buynow'>Buy now</span>";
			recodata += "</a>";

			recodata += "</div>";
			recodata += "</div>";
			recomertic_ids += "RC"+(s+1)+"_"+prdid+"," ;
		}
		recodata += "<i class=clear></i><div class='newmorediv bold'><a href='http://shopping.rediff.com/recommended-for-you' onclick=\"trackURL('http://track.rediff.com/click?url=___http://shopping.rediff.com/recommended-for-you/?sc_cid=ushome_recommended_more___&amp;cmp=USHP&amp;lnk=shopping_recommended_more&amp;nsrv1=USHP');return false;\">More recommended products <img src='"+imgpath+"/blank.gif' class='homesprite morearrow1'></a></div>";

		if(document.getElementById('div_recom_prd_box'))
		{
			document.getElementById('div_recom_prd_box').innerHTML = recodata;
			document.getElementById('div_recom_prd_box').style.display = 'block';
			if(document.getElementById('tabtrack')){document.getElementById('tabtrack').src = "http://metric.us.rediff.com/www.rediff.com/recoimp?rkey="+Math.floor(Math.random() * 1000000)+"&shop="+recomertic_ids;}
		}
	}
	catch(e)
	{
		
	}
}

var recoscript = document.createElement('script');
recoscript.src = 'http://recoprofile.rediff.com/shopping_recommendation/get_recommendation.php/?function=show_recommendation';
document.body.appendChild(recoscript);

