<script type="text/javascript">
function strpos( haystack, needle, offset){
var i = (haystack+'').indexOf( needle, offset );
return i===-1 ? false : i;
}

function trim(s){
s = s.replace(/^\s*/,'').replace(/\s*$/, '');
return s;
}


