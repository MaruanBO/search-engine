function showSearchTab(n) {
    var srcincopy = new Array("", "Search for a product, category or brand", "Search for the book you want to read", "Search for current and trending news", "Search for images and photographs");
	if(document.getElementById("srch_tab"+n).className == "srchtabsel")
	{
		if(n==1)
		{
			document.location.href = "http://shopping.rediff.com";
		}
		else if(n==2)
		{
			document.location.href = "http://books.rediff.com";
		}
		else if(n==3)
		{
			document.location.href = "http://realtime.rediff.com";
		}
	}

    for (var i = 1; i <= 3; i++) {
        if (i == n) {
            document.getElementById("srch_tab" + i).className = "srchtabsel";
            document.getElementById("srchque_" + i).style.display = "block";
            srch_tab_sel = srch_tab_arr[i - 1];
            kHost = "http://sns.rediff.com/HomeSns/api?command=home_sns&scount="+srch_tab_sel+",6&get_categories=1&get_products=1&prefix=";
            srch_tab_num = i - 1;
            document.getElementById('srchurl').value = services_url[i - 1];
            document.getElementById('srchservice').value = services[i - 1];
            document.getElementById("srchinputcopy").innerHTML = "<span class='homesprite mgglass'></span> " + srcincopy[n];
            document.getElementById("srchword").value = "";
            document.getElementById("srchword").blur();
            document.getElementById("srchinputcopy").style.display = "block";
			document.getElementById("bigimg_"+i).style.display = "block";
        } else {
            document.getElementById("srch_tab" + i).className = "srchtabnormal";
            document.getElementById("srchque_" + i).style.display = "none";
			document.getElementById("bigimg_"+i).style.display = "none";
        }
    }
    if (document.getElementById('srchword')) {
        document.getElementById('srchword').focus();
    }
}


