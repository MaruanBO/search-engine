function validmailuser()
{
	var Rl	="";
	var isvalidmailuser = true;
	Rl	= getcookie("Rl");
	var tmp_rsc = "";
	tmp_rsc = getcookie("Rsc");
	if(Rl.indexOf("@rediffmail") < 0 )
	{
		showDiv1('trans_div');
		showDiv1('div_mailsignin_ad');
		document.getElementById('login1').focus();
		document.getElementById('tabtrack').src = "http://metric.us.rediff.com/www.rediff.com/nxr?rkey="+Math.floor(Math.random() * 1000000);	
