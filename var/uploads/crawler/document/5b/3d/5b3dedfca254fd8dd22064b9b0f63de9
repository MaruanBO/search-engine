<script type="text/javascript">
    jQuery(document).ready(function ($) {
        $("a.uwLogInLink").click(function (e) {
            e.preventDefault();

            selectedLoginWidget = 'ModalRegistrationDefault';
            var data = $(this).data("login");

            if ($.isPlainObject(data)) {
                openLoginWLogInFlow(data.RegistrationSourceId, null, null, null, 'HeaderLogIn');
            }
            else {
                openLoginWLogInFlow(null, null, null, null, 'HeaderLogIn');
            }
        });

        if (($.browser.msie && navigator.userAgent.indexOf('Trident') !== -1) && parseInt($.browser.version) != 9) {
            /* IE 8  specific code goes here. */
        }
        else {
            // function from the internets: http://jsfiddle.net/tF8TU/2/
            $(function () {
                $(window).resize(function () {
                    // get the screen height and width  
                    var maskHeight = $(window).height();
                    var maskWidth = $(window).width();

                    // calculate the values for center alignment
                    var dialogTop = (maskHeight - $('.registrationDialogClass').height()) / 2;
                    var dialogLeft = (maskWidth - $('.registrationDialogClass').width()) / 2;

                    var dialogResizeHeight = (maskHeight * 0.6);
                    var dialogResizeWidth = (maskWidth * 0.8);

                    // assign values to the overlay and dialog box                
                    $('.registrationDialogClass').css({ top: dialogTop, left: dialogLeft, position: "fixed", width: dialogResizeWidth, height: dialogResizeHeight });
                    $('.registrationDialogContainer').css({ width: dialogResizeWidth, height: dialogResizeHeight });
                    $('.registrationDialogContainer iframe').css({ width: dialogResizeWidth, height: dialogResizeHeight });
                }).resize();
            });
        }

        jQuery("a.uwLogInLink").click(function (e) {
            if (jQuery('.navToggle:visible').length > 0) {
                window.Betty.navigation.closeFlyout();
            }
        });
    });
