        googletag.cmd.push(function () {
            googletag.defineSlot('/15704463/Betty_Crocker/HA/Homepage/Pos3',[300,250],'main_0_content_1_sidebar_5_googleAd_AdContainer').addService(googletag.pubads());
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
            googletag.display('main_0_content_1_sidebar_5_googleAd_AdContainer');
        });
    } else {
        // Viewport is wrong size, so don't show the ad and hide the ad container
        var $adContainer = jQuery('#main_0_content_1_sidebar_5_googleAd_AdContainer');
        $adContainer.hide();
        $adContainer.closest('#leadwrap').hide(); // Leaderboard container
        $adContainer.closest('.flexAd').hide(); // Flex Ad container
        $adContainer.closest('.modRightCol').hide(); // Right side of homepage ad slide
        $adContainer.closest('.advertisement').hide(); // Ad.ascx container
    }
</script>
    
</div>
    <div id="main_0_content_1_sidebar_5_pnlAdvertisementText" class="adBottomText">
	
