<script type="text/javascript">
var fixVideoPlayerLocation = function() {
	var videoPlayers = jQuery('.OoyalaVideoPlayer, div[id$="_MobileContainer"]'); 
	var videoContainers = jQuery('.videoView'); 
	videoPlayers.each(function(index) { 
		var expectedContainer = jQuery(this).closest('.videoView'); 
		var isInExpectedContainer = (expectedContainer.length > 0); 
		var containerIsAvailable = (videoContainers.length > index); 
		if (!isInExpectedContainer && containerIsAvailable) { 
			jQuery(this).prependTo(videoContainers.eq(index)); 
		} 
	});
};
jQuery(document).ready(function() {
	setTimeout('fixVideoPlayerLocation();', 100);
	setTimeout('fixVideoPlayerLocation();', 200);
	setTimeout('fixVideoPlayerLocation();', 500);
	setTimeout('fixVideoPlayerLocation();', 1000);
	setTimeout('fixVideoPlayerLocation();', 2000);
	setTimeout('fixVideoPlayerLocation();', 5000);
	setTimeout('fixVideoPlayerLocation();', 10000);
	setTimeout('fixVideoPlayerLocation();', 20000);
});

// Added by Jake 5/20/13: Fixes for Pinterest query string. -- location:footer in sitecore
jQuery(document).ready(function() {
	var fixPinterestQueryString = function () {
		var imageUrl = jQuery('meta[property="og:image"]').attr('content');
		var description = jQuery('meta[property="og:title"]').attr('content');
		var originalHref = jQuery('.gig-button-container-pinterest-pinit a').attr('href');
		if (!originalHref) return;
		var correctedHref = originalHref.replace(/%25/g, '%');
		correctedHref = correctedHref.replace(/&media=[^&#$]*/, '&media=' + imageUrl);
		correctedHref = correctedHref.replace(/&description=[^&#$]*/, '&description=' + description);
		jQuery('.gig-button-container-pinterest-pinit a').attr('href', correctedHref);
	};

	// Wait for Gigya to load
	setTimeout(fixPinterestQueryString, 100);
	setTimeout(fixPinterestQueryString, 200);
	setTimeout(fixPinterestQueryString, 300);
	setTimeout(fixPinterestQueryString, 500);
	setTimeout(fixPinterestQueryString, 1000);
	setTimeout(fixPinterestQueryString, 2000);
	setTimeout(fixPinterestQueryString, 5000);
	setTimeout(fixPinterestQueryString, 10000);
});

// Added by Vasant 9/25/13: Fix for removing comment section.
jQuery(document).ready(function() {
   jQuery("#fixPositionMemberComment").css("display", "none");
 });

// Fix for login screens on small screen devices
jQuery(document).ready(function() {
    jQuery("a.uwLogInLink").click(function (e) {
        if (jQuery('.navToggle:visible').length > 0) {
           jQuery('body').removeClass('navVisible');
           jQuery('.navTogglePageOverlay').remove();
           jQuery('.globalNavigationWrapper').animate({ 'margin-right': '-' + '270px' }, 350, 'easeOutExpo');
           jQuery('.bcHead').animate({ 'left': 0 }, 350, 'easeOutExpo');
           jQuery('.pageContainer').animate({ 'right': 0 }, 350, 'easeOutExpo');
           jQuery('#globalNavigation > ul > li').removeClass('hover');           
        }
    });
});

</script>

