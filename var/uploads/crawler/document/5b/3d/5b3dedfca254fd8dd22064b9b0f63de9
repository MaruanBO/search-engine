<script type="text/javascript">
    jQuery(function () {
        
        var updateNewsletterStatus = function () {
            var newsletterCtaWrapperElements = jQuery(".smartHeader .newsletterCtaWrapper .newsletterCtaContainer");
            if (newsletterCtaWrapperElements.length > 0 && newsletterCtaWrapperElements.height() > 0) {
                jQuery(".smartHeader").addClass("hasNewsletterCta");
            } else {
                jQuery(".smartHeader").removeClass("hasNewsletterCta");
            }
        }

        //the try/catch is because older parts of reg will send all sorts of messages of all sorts of types
        var doThisWhenMessageReceived = function (e) {
            var data;
            try {
                data = JSON.parse(e.data);
                if (data.source && data.source.indexOf("headernewslettercta") > 0 ) {
                    var myfunction = updateNewsletterStatus;
                    myfunction();
                }
            }
            catch (e) { }
        };

        if (window.addEventListener) {
            window.addEventListener("message", doThisWhenMessageReceived, false);
        } else {
            window.attachEvent("onmessage", doThisWhenMessageReceived);
        }

    });
</script>

