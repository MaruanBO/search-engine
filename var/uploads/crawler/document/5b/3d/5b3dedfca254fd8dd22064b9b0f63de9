<script type="text/javascript">
    jQuery(document).ready(mobileHomeSideMenuCollapser);

    function mobileHomeSideMenuCollapser() {
        jQuery('.modRightCol').each(function () {
            var selector = jQuery(this).find('.headingContainer');
            selector.click(function () {
                var linksSelector = selector.next('.links, .modRightVideos');
                var spanSelector = selector.find('.mobileBullet');
                if (spanSelector.is(':visible')) {
                    linksSelector.each(function () {
                        if (jQuery(this).is(':visible')) {
                            selector.find('.mobileBullet').text("+");
                            return linksSelector.slideUp('slow');
                        } else {
                            selector.find('.mobileBullet').text("–");
                            linksSelector.slideDown('slow');
                        }
                    });
                } else {
                    return;
                }
            });
        });
    }
</script>

