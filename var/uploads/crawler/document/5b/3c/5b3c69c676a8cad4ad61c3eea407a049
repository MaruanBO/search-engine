     <script type="text/javascript">

         function subSearchForm(subtype) {

             var URL = '/site-search.aspx?q=' + document.getElementById("q").value;

             document.location.href = URL;

             return false;
         }

         function checkReturn(e) {
             if (window.event) // IE
             {
                 keynum = e.keyCode
             }
             else if (e.which) // Netscape/Firefox/Opera
             {
                 keynum = e.which
             }
             if (keynum == 13) {
                 return subSearchForm('rec');
             }

         }

    </script>


