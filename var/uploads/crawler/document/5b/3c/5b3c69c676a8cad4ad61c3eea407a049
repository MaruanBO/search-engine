        function open_window() {
            if (window.location.href.search(/&/) != -1) { window.open(window.location.href.replace(/#/, '') + '&p=1', 'mywin', 'width=670, height=600, location=no, menubar=no, status=no, toolbar=no, scrollbars=yes, resizable=no'); }
