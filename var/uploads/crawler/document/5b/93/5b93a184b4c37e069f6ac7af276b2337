function PCHGamesRedirect(url)
{
   if(typeof url == 'undefined'){
      if(typeof PCHGames != 'undefined' && typeof PCHGames.returnURL != 'undefined'){
         url = PCHGames.returnURL;
      }else{
         url = "/";
      }
     
   }

   window.location.href = url;
}
