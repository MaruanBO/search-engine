		function getStarted() {
			$(".chalkboard").animate({height:60}, {
				complete:function(){
					$(".chalkboard").addClass("minimized");
					$(".rounded .url").eq(0).focus();
				}
			});
		}
		$(document).ready(function() {
			var hash = window.location.hash;
			if (hash.indexOf("getStarted") >= 0) {
				getStarted();
			}
		});
		</script>
