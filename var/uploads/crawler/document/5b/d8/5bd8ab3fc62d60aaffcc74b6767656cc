	$(".pixel-url").each(function() {
		
		var link = $(this);
		link.click(function() {
			var url = $(this).val();
			if (url.indexOf("www.facebook.com") > 0 && url.indexOf("uuid") > 0) {
				var container = $("#dialog-facebook-audience");
				container.find("span.data").html("&nbsp;");
				container.find("#facebook-aud-name").html("Loading...");
				var qt = link.qtip({
					position: {
						at: "top center",
						my: "bottom center"
					},
					style: {
						tip: {corner:"bottom center", width:12, height:12},
						classes: "ui-tooltip-dropbox ui-tooltip-shadow"
					},
					content:container,
					show:{event: false},
					hide:{
						fixed:true,
						event:'unfocus'
					}
				});
				qt.qtip('api').show();
				$.getJSON("facebookAudienceInfo", {"url":url}, function(data) {
					if (data) {
						container.find("#facebook-aud-name").text(data.name);
						container.find("#facebook-aud-description").text(data.description);
						container.find("#facebook-aud-status").text(data.delivery_status.description);
						container.find("#facebook-aud-size").text(data.approximate_count);
						container.find("#facebook-aud-days").text(data.retention_days);
					} else {
						container.find("#facebook-aud-name").html("Failed to load audience");
					}
					//alert(data.delivery_status.description + "\n\n" + "Approximate size: " + data.approximate_count + "\n\n" + "Retention days: " + data.retention_days);
				});
			}
		});
	});
