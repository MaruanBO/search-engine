  <script>
//<![CDATA[

    (function() {
      var f = $("user_mini_feed");
      if(!f) {
        return;
      }

      Event.observe(f, (/Firefox/i.test(navigator.userAgent) ? "DOMMouseScroll" : "mousewheel"), function(e) {
        var delta = e.detail ? e.detail * -1 : e.wheelDelta;

        if(0 == f.scrollTop && delta > 0) {
          Event.stop(e);
        }

        if(f.getHeight() + f.scrollTop > f.scrollHeight && delta < 0) {
          Event.stop(e);
        }
      })
    })();

    var gameCategoryPageNumber = 0;
    var showMoreElement = $('show_more_categories');
    var loadingCategories = false;
    var previousScroll = 0;

    Event.observe($('show_more_link'), 'click', updateGameCategories);

    function updateGameCategories() {
      $('show_more_link').hide();
      $('show_more_spinner').show();
      loadingCategories = true;

      new Ajax.Request('http://www.kongregate.com/homepage/more', {
        method: 'get',
        parameters: { page: gameCategoryPageNumber },
        onSuccess: function(transport) {
          if(++gameCategoryPageNumber < 3) {
            $('show_more_link').show();
          } else {
            $('show_more_categories').hide();
          }
          $('show_more_spinner').hide();
          $('extra_games').insert(transport.responseText);
          loadingCategories = false;
        }
      });
    }

//]]>
</script><!--[if gte IE 8]><script src="http://www.googletagservices.com/tag/js/gpt.js"></script><![endif]-->
