        $$('.game_chooser').first().update($('game_chooser_loading').innerHTML);
        $$('.game_chooser').first().show();
        $$('.game_chooser li.all').first().observe('click', function(e) { $('#search').submit(); });
        $$('.hide_ad_always').invoke('hide');
      }
      return value;
    }

