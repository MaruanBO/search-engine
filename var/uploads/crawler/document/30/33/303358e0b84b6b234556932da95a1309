	<!-- hide
	function pausescript(millis)
	{
	var date = new Date();
	var curDate = null;
	do { curDate = new Date(); }
	while(curDate-date < millis);
	} 
	// open new window
	function CountChecks() {
	var maxchecked = 4;
	var count = 0;
	if(document.compare.compare_prod1.checked == true) { count++; }if(document.compare.compare_prod2.checked == true) { count++; }if(document.compare.compare_prod3.checked == true) { count++; }	if(count > maxchecked) {
		alert('Sorry, you may only compare up to ' + maxchecked + ' items.');
		return false;
		}
	return true;
	}
