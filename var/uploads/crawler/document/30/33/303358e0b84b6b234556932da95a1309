	<!-- Ajax/Java Code -->
	<script language="JavaScript" type="text/javascript"><!-- hide
	function getXMLRecRequest() {
	try {
	req = new XMLHttpRequest();
	// Added this for FF issues //
	} catch(err1) {
	  try {
	  req = new ActiveXObject("Msxml2.XMLHTTP");
	  } catch (err2) {
		 try {
		 req = new ActiveXObject("Microsoft.XMLHTTP");
		 } catch (err3) {
			req = false;
		 } 
	  } 
	}
	return req;
	}
	var http = getXMLRecRequest();
	function find_recommends(the_tab) {
	  document.getElementById('rechdrtxt').innerHTML = '<img src="http://www.snowblowersdirect.com/images/ajax_loading_icon.gif" alt="" />';
	  var myurl = 'includes/recommend_fetch.php';
		// Adding other Variables to my URL String //
		var params = '';	
		params += 'tab=' + the_tab;
	
	  var modurl = myurl; 
	  http.open("POST", modurl, true);
	  
	 //Send the proper header information along with the request
	http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	http.setRequestHeader("Content-length", params.length);
	http.setRequestHeader("Connection", "close"); 
	  
	http.onreadystatechange = function() {//Call a function when the state changes.
		if(http.readyState == 4 && http.status == 200) {
			useHttpRecResponse();
		}
	}
	http.send(params);  
	}
	
	function useHttpRecResponse() {
		if (http.readyState == 4) {
		 if(http.status == 200) {
		  // Build Data //
		  document.getElementById ('rechdrtxt').innerHTML = http.responseXML.getElementsByTagName('rechdrtxt')[0].firstChild.nodeValue;
		  document.getElementById ('bestname').innerHTML = http.responseXML.getElementsByTagName('bestname')[0].firstChild.nodeValue;
		  document.getElementById ('bettername').innerHTML = http.responseXML.getElementsByTagName('bettername')[0].firstChild.nodeValue;
		  document.getElementById ('goodname').innerHTML = http.responseXML.getElementsByTagName('goodname')[0].firstChild.nodeValue;
		  document.getElementById ('bestprice').innerHTML = http.responseXML.getElementsByTagName('bestprice')[0].firstChild.nodeValue;
		  document.getElementById ('betterprice').innerHTML = http.responseXML.getElementsByTagName('betterprice')[0].firstChild.nodeValue;
		  document.getElementById ('goodprice').innerHTML = http.responseXML.getElementsByTagName('goodprice')[0].firstChild.nodeValue;
		  document.getElementById ('bestimg').src = '/product-images/' + http.responseXML.getElementsByTagName('bestimg')[0].firstChild.nodeValue;
		  document.getElementById ('betterimg').src = '/product-images/' + http.responseXML.getElementsByTagName('betterimg')[0].firstChild.nodeValue;
		  document.getElementById ('goodimg').src = '/product-images/' + http.responseXML.getElementsByTagName('goodimg')[0].firstChild.nodeValue;
		  document.getElementById ('compare_prod3').value = http.responseXML.getElementsByTagName('bestid')[0].firstChild.nodeValue;
		  document.getElementById ('compare_prod2').value = http.responseXML.getElementsByTagName('betterid')[0].firstChild.nodeValue;
		  document.getElementById ('compare_prod1').value = http.responseXML.getElementsByTagName('goodid')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('bestid')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink2').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('bestid')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink3').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('bestid')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('betterid')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink2').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('betterid')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink3').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('betterid')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('goodid')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink2').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('goodid')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink3').href = '/product_info.php?products_id=' + http.responseXML.getElementsByTagName('goodid')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink').title = http.responseXML.getElementsByTagName('goodname')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink2').title = http.responseXML.getElementsByTagName('goodname')[0].firstChild.nodeValue;
		  document.getElementById ('goodlink3').title = http.responseXML.getElementsByTagName('goodname')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink').title = http.responseXML.getElementsByTagName('bettername')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink2').title = http.responseXML.getElementsByTagName('bettername')[0].firstChild.nodeValue;
		  document.getElementById ('betterlink3').title = http.responseXML.getElementsByTagName('bettername')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink').title = http.responseXML.getElementsByTagName('bestname')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink2').title = http.responseXML.getElementsByTagName('bestname')[0].firstChild.nodeValue;
		  document.getElementById ('bestlink3').title = http.responseXML.getElementsByTagName('bestname')[0].firstChild.nodeValue;
			if(http.responseXML.getElementsByTagName('goodrec')[0].firstChild.nodeValue != '0') {
				document.getElementById ('goodreclink').innerHTML = '<a href="/recommends/p' + http.responseXML.getElementsByTagName('goodid')[0].firstChild.nodeValue + '.html#reviews" style="color:#000000; text-decoration:underline; font-size:11px; font-family:Arial, Helvetica, sans-serif;" title="Expert Reviews @ Snow Blowers Direct">Joe\' Review</a>';
			} else {
				document.getElementById ('goodreclink').innerHTML = '&nbsp;';
			}
			if(http.responseXML.getElementsByTagName('betterrec')[0].firstChild.nodeValue != '0') {
				document.getElementById ('betterreclink').innerHTML = '<a href="/recommends/p' + http.responseXML.getElementsByTagName('betterid')[0].firstChild.nodeValue + '.html#reviews" style="color:#000000; text-decoration:underline; font-size:11px; font-family:Arial, Helvetica, sans-serif;" title="Expert Reviews @ Snow Blowers Direct">Joe\' Review</a>';
			} else {
				document.getElementById ('betterreclink').innerHTML = '&nbsp;';
			}
			if(http.responseXML.getElementsByTagName('bestrec')[0].firstChild.nodeValue != '0') {
				document.getElementById ('bestreclink').innerHTML = '<a href="/recommends/p' + http.responseXML.getElementsByTagName('bestid')[0].firstChild.nodeValue + '.html#reviews" style="color:#000000; text-decoration:underline; font-size:11px; font-family:Arial, Helvetica, sans-serif;" title="Expert Reviews @ Snow Blowers Direct">Joe\' Review</a>';
			} else {
				document.getElementById ('bestreclink').innerHTML = '&nbsp;';
			}
		} else {
			document.getElementById('rechdrtxt').innerHTML = 'HTTP Error';
		}
	  } else {
	  document.getElementById('rechdrtxt').innerHTML = 'Searching...';
	  }
	}
	function useHttpRecResponse2() {
	   if (http.readyState == 4) {
		if(http.status == 200) {
		  // Build Data //
		  while ( document.getElementById ('rec_options').options.length ) document.getElementById ('rec_options').options[0] = null;
		  option = new Option( "Select Category", "0" );
		  document.getElementById ('rec_options').disabled = false;
		  document.getElementById ('rec_options').style.background = '#FFFFFF';
		  document.getElementById ('rec_options').options[document.getElementById ('rec_options').length] = option;
		  for ( i = 0; i < http.responseXML.getElementsByTagName('num')[0].firstChild.nodeValue; i++ ) {
				// Create a new drop down option with the
				// display text and value from arr
				option = new Option( http.responseXML.getElementsByTagName('name_'+i)[0].firstChild.nodeValue, http.responseXML.getElementsByTagName('id_'+i)[0].firstChild.nodeValue );
				// Add to the end of the existing options
				document.getElementById ('rec_options').options[document.getElementById ('rec_options').length] = option;
			}
						if (document.getElementById('rec_options').options.length == 2){
				document.getElementById('rec_options').value = http.responseXML.getElementsByTagName('id_0')[0].firstChild.nodeValue;
				setTimeout(function() {find_recommends(http.responseXML.getElementsByTagName('id_0')[0].firstChild.nodeValue);},500);
			}
		} else {
			document.getElementById('rechdrtxt').innerHTML = 'HTTP Error';
		}
	  } else {
	  document.getElementById('rechdrtxt').innerHTML = 'Searching...';
	  }
	}
	function find_recommends_list(the_category) {
		var myurl = 'includes/recommend_fetch_list.php';
		// Adding other Variables to my URL String //
		var params = '';	
		params += 'category=' + the_category;
		
		var modurl = myurl; 
		http.open("POST", modurl, true);
		
		//Send the proper header information along with the request
		http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		http.setRequestHeader("Content-length", params.length);
		http.setRequestHeader("Connection", "close"); 
		
		http.onreadystatechange = function() {//Call a function when the state changes.
			if(http.readyState == 4 && http.status == 200) {
				useHttpRecResponse2();
			}
		}
		http.send(params);  
	}
	// done hiding --></script>
