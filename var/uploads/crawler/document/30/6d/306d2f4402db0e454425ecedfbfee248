<script type="text/javascript">
var startY = 300;

$(window).scroll(function(){
    checkY();
});

function checkY(){
    if( $(window).scrollTop() > startY ){
        $('.miniheader').slideDown();
    }else{
        $('.miniheader').fadeOut();
    }
}

// Do this on load just in case the user starts half way down the page
checkY();
</script>
