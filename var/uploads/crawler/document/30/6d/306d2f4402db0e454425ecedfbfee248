<script type="text/javascript">

        function transitionImage(url) {

		switch(url)
	        {
			case "images/search-bing.png":
				newURL = "images/search-yahoo.png";
			break;
			case "images/search-yahoo.png":
				newURL = "images/search-google.png";
			break;
			case "images/search-google.png":
				newURL = "images/search-bing.png";
			break;
		}

		$('#searchdiagramcontainer').css("background-image", "url("+newURL+")");
		$('#searchdiagram').fadeOut(2000, function(){
			$('#searchdiagram').attr("src", newURL);
			$('#searchdiagram').fadeIn();
		});

		setTimeout(function(){
			transitionImage(newURL);		
		},4000);
    }

    $(document).ready(function(){
        transitionImage('images/search-google.png');
    });


</script>
