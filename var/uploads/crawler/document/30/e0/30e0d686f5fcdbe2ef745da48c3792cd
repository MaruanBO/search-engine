<script type="text/javascript">
//<![CDATA[
    function redirect() {
        var loc = window.location.href;
        var parts = new Array();
        parts = loc.split('/');
        var lastpart;
        for (var i = 0; i < parts.length; i++) {
            if (parts[i] != "" && parts[i] != "#") lastpart = parts[i];
        }
        var newloc;
        if (lastpart.toLowerCase() == "giftsforyounow.com" || lastpart.toLowerCase() == "www.giftsforyounow.com")
            newloc = "http://www.giftsforyounow.com/Sitemap.aspx";
        else
            newloc = "http://www.giftsforyounow.com";
        window.location = newloc;
    }
    //]]>
</script>
