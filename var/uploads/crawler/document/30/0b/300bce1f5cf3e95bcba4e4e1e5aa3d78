    <h3></h3>
            <div class="pic">   
                    <a href="http://www.wired.com/2015/01/microsoft-nadella"><img src="http://www.wired.com/wp-content/uploads/2013/02/nadellaxrail.jpg" alt=""></a>                
            </div>

            <h2><a href="http://www.wired.com/2015/01/microsoft-nadella">&nbsp;</a></h2>
</div></div>
					<div class="textwidget"><div class="scrape commentary" id="photo-galleries">
    <div class="mod5">
        <h3>Photo Galleries</h3>
        <div class="inner">
                <h2><a href="">&nbsp;</a></h2>
                <blockquote>		<script type="text/javascript">
//<!--
			// initializing Global Array
			var glide_galleries = new Array();			
			jQuery(document).ready(function(){
				init_glider(); // Initializing Glider					
				// Left Button, 
				// Image Hover Controllers
				jQuery('#xrail-photo-glide-btn-left').mouseenter(function(){ 
				LeftHover();
				});
				jQuery('#xrail-photo-glide-btn-left').mouseout(function(){
				LeftHoverOut();
				});				
				// Right Button, 
				// Image Hover Controllers
				jQuery('#xrail-photo-glide-btn-right').mouseenter(function(){
				RightHover();
				});
				jQuery('#xrail-photo-glide-btn-right').mouseout(function(){
				RightHoverOut();
				});				
				//Left and Right Button
				// Click Controllers
				jQuery('#xrail-photo-glide-btn-left').click(function(){
				glide_fromLeft();
				});				
				jQuery('#xrail-photo-glide-btn-right').click(function(){
				glide_fromRight();
				});
			});
			function init_glider() {
				var glider_elems = jQuery('#xrail-photo-glide-disp').children();
					glide_galleries = new Array();
					
					for(i=0;i<glider_elems.length;i++){
					var g_elem = glider_elems[i];
					glide_galleries.push(g_elem);
				}				
			}					
			function LeftHover() {
				jQuery('#xrail-photo-glide-btn-left').attr('src','http://www.wired.com/images_blogs/about/2011/11/left-hover.png'); //darker image overlay
			}			
			function RightHover() {			  
				jQuery('#xrail-photo-glide-btn-right').attr('src','http://www.wired.com/images_blogs/about/2011/11/right-hover.png'); //darker image overlay
			}			
			function LeftHoverOut(){
				jQuery('#xrail-photo-glide-btn-left').attr('src','http://www.wired.com/images_blogs/about/2011/11/left-up.png'); // returns hover states
			}			
			function RightHoverOut(){
				jQuery('#xrail-photo-glide-btn-right').attr('src','http://www.wired.com/images_blogs/about/2011/11/right-up.png'); // returns hover states
			}
			function glide_fromLeft() {				
				var disp_elem_old = glide_galleries[0];
				var disp_elem_new = glide_galleries.pop(); // create var disp_elem_new, insert the last array item into it.

					glide_galleries.unshift(disp_elem_new); // push the new array item to the 0 slot, for viewing.
				
					jQuery(disp_elem_new).css({'left':'301px', 'z-index':'10'});
					jQuery(disp_elem_old).css({'z-index':'1'}); // sets element to the right, positioned and prepared to animate
				
					jQuery(disp_elem_new).animate({left:'0px'}, 370, function() {
						jQuery(disp_elem_old).css({'left':'301px'});
					}
				);
			}								
			function glide_fromRight() {
				//alert('Moving Right');
							
				var disp_elem_old = glide_galleries.shift(); // gets the active element, puts inside variable.
				var disp_elem_new = glide_galleries[0];	
				
					glide_galleries.push(disp_elem_old);
				
					jQuery(disp_elem_new).css({'left':'-301px', 'z-index':'10'});
					jQuery(disp_elem_old).css({'z-index':'1'}); 
				
					jQuery(disp_elem_new).animate({'left':'0px'}, 370, function() {
						jQuery(disp_elem_old).css({'left':'-301px'});
					}
				);
			}
//-->		
</script>			
		<div id="xrail-photo-glide-cont">		
			<img id="xrail-photo-glide-btn-left" src="http://www.wired.com/images_blogs/about/2011/11/left-up.png">			
