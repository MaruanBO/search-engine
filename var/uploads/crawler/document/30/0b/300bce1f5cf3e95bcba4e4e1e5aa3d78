<script type="text/javascript" id="polar-partial">

(function(){

  var compiledTemplate0 = '';


window.NATIVEADS = window.NATIVEADS || {};
window.NATIVEADS.injectedAt = new Date().getTime();
window.NATIVEADS.onReady = function(ads) {

  ads.setPropertyID("NA-WIRE-11236120");
  ads.setSecondaryPageURL("/sponsored");

  // get the section name from the dart zone for targeting, i.e. design, gadgetlab
  // remove the trailing semicolon
  var dartzone = CN.dart.get('zone').replace(";", "");

  ads.insertPreview({
    label: "Right rail",
    unit: {
      "server":"dfp",
      "id": "/3379/wiredcom.dart/" + dartzone,
      "size":"290x95",
      "targets": {
        "category":"news",
        "placement":"top"
      }
    },
    location: "#promo-item-count3",
    infoText: "",
    infoButtonText: "",
    template: compiledTemplate0,
    onRender: function($element) { console.log('Right rail Polar rendered.')},
    onFill: function(data) { console.log(data) },
    onError: function(error) { console.log('There was a problem with ' + error)}
  });

  ads.injectCSS(" ", "head");


  ads.configureSecondaryPage({
    binding: {
      sponsor: {
        link: "#sponsor-link",
        logo: "#sponsor-logo",
        name: "#sponsor-name"
      },
      title: "#title",
      summary: "#summary",
      content: "#content",
      author: "#author",
      pubDate: "#pub-date",
      image: {
        href: "",
        caption: "#media-caption",
        credits: "#media-credits"
      }
    },
    onFill: function(data) {
      var title = data.title;
      var description = data.summary;
      var link = window.location.href;
      var image_url = data.image.href;

      //Facebook Share button Update
      var APP_ID = "d53bb4300a345fefeeccdfadf717335c";
      ads.$('#share-button').bind('click', function(e){
          e.preventDefault();
          url = "https://www.facebook.com/dialog/feed?app_id=" + APP_ID +
              "&link=" + encodeURIComponent(link) +
              "&name=" + encodeURIComponent(title) +
              "&caption=" + encodeURIComponent('Shared from http://www.wired.com') +
              "&description=" + encodeURIComponent(description) +
              "&picture=" + encodeURIComponent(image_url) +
              "&redirect_uri=" + encodeURIComponent(link);
          console.log(url);
          window.open(url);
      });
    },
    //onFill: function(data) { console.log(data);},
    onRender: function() {
      console.log('Sponsored article rendered.');
      var images = ads.$('.phoenix-inline-image img');
      // articleFeaturedDiv appears only on Mobify.
      var articleFeaturedDiv = ads.$('#article-featured.image-featured');
      // Check to see if Mobify
      if ((articleFeaturedDiv).length) {
        // For Mobify, this will remove the image within the first instance of class "phoenix-inline-image" and append it to the div with id "article-featured."
        // This hoists this first main image above the headline in Mobify.
        articleFeaturedDiv.append(images[0]);
      } else {
          // Add CSS to smaller images for text wrap on Desktop only.
          for (var i = 0; i < images.length; i++) {
            var img = images[i];
            if (img.width < 400){
              ads.$(img).css({"float":"left","padding-right":"1em"});
            }
          }
      }
    },
    //onRender: function() { console.log('Sponsored article rendered.'); },
    onError: function(error) { console.log('Something went wrong with ' + error); },
    track: function() { }
  });

};

  /*

   This function represents a pre-compiled Handlebars template. Pre-compiled
   templates are not pretty, but they provide a very significant performance
   boost, especially on mobile devices. For more information, see
   http://handlebarsjs.com/precompilation.html.

   Note that this code has been generated from the following markup:

<li class="promo-item" id="promo-item-count3" style="background:#f7f7f7;">
    <div class="promo-thumbnail">
        <a href="{{link}}" onclick="s_objectID=&quot;{{title}}_1&quot;;return this.s_oc?this.s_oc(e):true" rel="nofollow">
            <img nopin="nopin" src="{{image.href}}" class="post-thumb" alt="undefined" width="140" height="70">
        </a>
    </div>
    <div class="promo-wrapper">
      <div class="promo-headline"><span style="color:gray;font-size: 13px;font-family: helvetica;line-height: 20px;">Sponsor Content:</span><br><a class="module-rsswidget" href="{{link}}" onclick="s_objectID=&quot;{{link}}_2&quot;;return this.s_oc?this.s_oc(e):true" rel="nofollow">{{title}}</a><br><strong>{{sponsor.name}}</strong>
        </div>
    </div>
</li>

  */

  compiledTemplate0 = function (Handlebars,depth0,helpers,partials,data) {
  this.compilerInfo = [4,'>= 1.0.0'];
helpers = this.merge(helpers, Handlebars.helpers); data = data || {};
  var buffer = "", stack1, stack2, functionType="function", escapeExpression=this.escapeExpression;


  buffer += "<li class=\"promo-item\" id=\"promo-item-count3\" style=\"background:#f7f7f7;\">\n    <div class=\"promo-thumbnail\">\n        <a href=\"";
  if (stack1 = helpers.link) { stack1 = stack1.call(depth0, {hash:{},data:data}); }
  else { stack1 = depth0.link; stack1 = typeof stack1 === functionType ? stack1.apply(depth0) : stack1; }
  buffer += escapeExpression(stack1)
    + "\" onclick=\"s_objectID=&quot;";
