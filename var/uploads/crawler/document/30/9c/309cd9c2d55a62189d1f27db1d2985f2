                var Dom = YAHOO.util.Dom;
                
                
                    BCUS.GlobalElement = {};

                    
                    var cssLoginFlowButtonsToggle = true;

                  function checkInput(){

                    
                    BCUS.GlobalElement.customerLogin = Dom.get("loginButton-button");


                BCUS.setupPreventDoubleSubmit();
                if (Dom.hasClass("username","error")){
                    Dom.removeClass("homepageError","hide");
                }
                var nlh = Dom.get("notLoggedHeader");
                
                var cookieFirstName = ("null" == "null") ? null : "null";
                if (cookieFirstName){
                    nlh.innerHTML = "Welcome back, "  + cookieFirstName;
                }
                if (Dom.get("applyButton")){
                    YAHOO.util.Event.on("applyButton", "click", function(e){
                        
                                window.location = "http://www.aboutbarclaycardus.com/prodsresource_products.php"
                    });
                }
                if (Dom.get("learnMoreButton")){
                    YAHOO.util.Event.on("learnMoreButton", "click", function(e){
                                window.location = "http://www.barclaycardus.com/apply/Landing.action?campaignId=2043&cellNumber=9&referrerid=BCSBA1014HPG"
                    });
                }
                if (Dom.get("newsButton")){
                    YAHOO.util.Event.on("newsButton", "click", function(e){
                                window.location = "http://www.aboutbarclaycardus.com/news.php"
                    });
                }

                    BCUS.placeholderText({el:"username",txt:"Enter your username"});

                 ;};
            YAHOO.util.Event.onDOMReady(function(e){
                checkInput();

                    
                    
                    

                YAHOO.util.Dom.get("username").focus();
                //YAHOO.util.Event.on("loginForm", "focusin", BCUS.toolTip);
                //YAHOO.util.Event.on(Dom.getElementsByClassName("validate"), "focusout", BCUS.validate.execute,{beanclass:"com.barclaycardus.app.cws.action.HomeActionBean"});
            });

            function checkValue() {
               if(Modernizr.input.placeholder){
                  // you should already have a valid input
               } else {
                  if(YAHOO.util.Dom.get("username").value == "Enter your username"){
                     YAHOO.util.Dom.get("username").value = "";
                  }
               }
            }
            post_deviceprint();


            var slideShow = {
                tiles: document.getElementById('viewer').getElementsByTagName('a'),
                tileCount: 0,
                currentTile: 0,
                nextTile: 1,
                timer: null,
                init: function() {
                    var d = document, s = slideShow;
                    s.tileCount = (s.tiles.length - 1);
                    s.tiles[1].children[0].src = 'https://gif.barclaycardus.com/servicing/2afbff2a/img/base/homeCarousel/browseCards.jpg';
                    s.tiles[2].children[0].src = 'https://gif.barclaycardus.com/servicing/2afbff2a/img/base/homeCarousel/Apple_Pay_AvailableNow.png';
                    if (d['addEventListener']) {
                        d.getElementById('controlDots').addEventListener('click', s.selectTile);
                    }
                    else {
                        d.getElementById('controlDots').attachEvent('onclick', s.selectTile);
                    }
                    setTimeout(s.loadCheck, 5000);
                },
                loadCheck: function () {
                    var s = slideShow;
                    if (s.tiles[1].children[0].complete || s.tiles[1].children[0].readyState == 'complete') {
                        s.show();
                        s.timer = setInterval(s.show, 5000);
                    }
                    else {
                        setTimeout(s.loadCheck, 500);
                    }
                },
                show: function() {
                    var s = slideShow;
                    s.tiles[s.currentTile].className = 'slideOutLeft';
                    s.tiles[s.nextTile].className = 'slideInLeft';
                    document.getElementById('tile' + s.nextTile).className = 'selected';
                    document.getElementById('tile' + s.currentTile).className = '';
                    s.currentTile = s.nextTile;
                    s.nextTile = (s.currentTile === s.tileCount) ? 0 : (s.currentTile + 1);
                },
                selectTile: function(e) {
                    var s = slideShow, el = (e['target'] || e.srcElement);
                    var tileIndex = el.getAttribute('data-tileIndex');
                    if (tileIndex && tileIndex != s.currentTile) {
                        clearInterval(s.timer);
                        s.nextTile = parseInt(tileIndex);
                        s.show();
                    }
                }
            };
            window.onload = slideShow.init;

         </script>
      
            
                
                












 
 










    
    
    
    
    
    
    
        
    








    
        
        
    
    
    
    
    
    









    

    

    

    

    

    

    

    

    

    

    

    

    




    
        
    
    









   
   
   
   
   
   
   
   
   
      
   
   
   
   
   
   
   
   
    
   
   
   
   
   
   
    
    
    
    
    






