<script>
	// Load GPT asynchronously
	var googletag = googletag || {};
	googletag.cmd = googletag.cmd || [];
	(function() {
		var gads = document.createElement('script');
		gads.async = true;
		var useSSL = 'https:' == document.location.protocol;
		gads.src = ( useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
		var node = document.getElementsByTagName('script')[0];
		node.parentNode.insertBefore(gads, node);
	})();

	// GPT slots
	var gptAdSlots = [];
	googletag.cmd.push(function() {
		// OVERLAY ADVERT - This does not need a size mapping.  Overlays on different devices will be controlled via Ad Trafficking
		
				gptAdSlots[0] = googletag.defineSlot('4818/sew.us/home', [[600,500]], 'overlay-ad-slot').addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// WALLPAPER ADVERT - Define a size mapping object.  The first parameter to addSize is
        // a viewport size, while the second is a list of allowed sizes.
		var wallpaperMapping = googletag.sizeMapping().
   		addSize([1000, 200], [1320,900]).
		build();
		
		// Define the GPT slot for the wallpaper ad
				gptAdSlots[1] = googletag.defineSlot('4818/sew.us/home', [[1320,900]], 'wallpaper-ad-slot').defineSizeMapping(wallpaperMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// LEADERBOARD AND BILLBOARD ADVERTS - Define a size mapping object. The first parameter to addSize is
        // a viewport size, while the second is a list of allowed ad sizes.
        var headerMapping = googletag.sizeMapping().
		addSize([320, 200], [[320, 50], [300, 50]]).
        addSize([480, 200], [[468, 60], [320, 50], [300, 50]]).
        addSize([768, 200], [728, 90]).
        addSize([1000, 200], [[970,250], [728, 90]]).
		build();
		
		// Define the GPT slot for header ad
				gptAdSlots[2] = googletag.defineSlot('4818/sew.us/home', [728, 90], 'header-ad-slot').setTargeting('pos', ['ldb1']).defineSizeMapping(headerMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		var footerMapping = googletag.sizeMapping().
		addSize([320, 200], [[320, 50], [300, 50]]).
        addSize([480, 200], [[468, 60], [320, 50], [300, 50]]).
        addSize([768, 200], [728, 90]).
		build();
		
		
		// Define the GPT slot for footer ad
				gptAdSlots[3] = googletag.defineSlot('4818/sew.us/home', [728, 90], 'footer-ad-slot').setTargeting('pos', ['ldb2']).defineSizeMapping(footerMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		
		// RIGHT HAND COLUMN ADVERTS - Define a size mapping object. The first parameter to addSize is
        // a viewport size, while the second is a list of allowed ad sizes.
        var rightHandColumnMpuMapping = googletag.sizeMapping().
           addSize([320, 200], [3, 1]).
           addSize([975, 200], [[300, 250], [300, 600]]).
        build();
		
		// Define the GPT slot for Right hand column MPU
				gptAdSlots[4] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'rhc-mpu-ad-slot1').setTargeting('pos', ['mpu1']).defineSizeMapping(rightHandColumnMpuMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		
		// Define the GPT slot for Right hand column MPU
				gptAdSlots[5] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'rhc-mpu-ad-slot2').setTargeting('pos', ['mpu2']).defineSizeMapping(rightHandColumnMpuMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// Define the GPT slot for Right hand column MPU
				gptAdSlots[6] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'rhc-mpu-ad-slot3').setTargeting('pos', ['mpu3']).defineSizeMapping(rightHandColumnMpuMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
	   // PRIMARY CONTENT AREA ADVERTS - Define a size mapping object. The first parameter to addSize is
        // a viewport size, while the second is a list of allowed ad sizes.
        var primaryContentAreaMapping = googletag.sizeMapping().
           	addSize([320, 200], [[300, 250], [300, 600], [320, 50], [300, 50]]).
        	addSize([975, 200], [3, 1]).
        build();	
		
	
		// Define the GPT slot for Primary content area
				gptAdSlots[7] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'pca-mpu-ad-slot1').setTargeting('pos', ['mpu1']).defineSizeMapping(primaryContentAreaMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// Define the GPT slot for Primary content area
        		gptAdSlots[8] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'pca-mpu-ad-slot2').setTargeting('pos', ['mpu2']).defineSizeMapping(primaryContentAreaMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// Define the GPT slot for Primary content area
        		gptAdSlots[9] = googletag.defineSlot('4818/sew.us/home', [300, 250], 'pca-mpu-ad-slot3').setTargeting('pos', ['mpu3']).defineSizeMapping(primaryContentAreaMapping).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
				
		// Set the targetting for all ad slots
                googletag.pubads().setTargeting('pagetype', ['home']);
                
	// Start ad fetching
		googletag.pubads().enableSingleRequest();
		googletag.pubads().enableAsyncRendering();
		googletag.enableServices();
	});

</script>
