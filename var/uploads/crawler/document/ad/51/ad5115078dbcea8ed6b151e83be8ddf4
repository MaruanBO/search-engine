<script type="text/javascript">
	(function(){
		function collapseDetails($elem) {
			if(!$elem){
				$('.item_details').stop().slideUp();
				$('.item_details').parent().removeClass('expanded');
			} else {
				$('.item_details').not($elem.siblings('.item_details')).stop().slideUp();
				$('.item_details').parent().not($elem.parent()).removeClass('expanded');
			}
		}
		var $links = $('.products-strip .item a.toggle');
		$links.on('click', function(e){
			e.preventDefault();
			var $this = $(this);
			//$this.siblings('.item_details').slideToggle();
			collapseDetails($this);
			$this.siblings('.item_details').stop().slideToggle();
			$this.parent().toggleClass('expanded');
		});
		$('body').on('click', function(e){
			if(!$links.is(e.target) && $links.has(e.target).length === 0) {
				collapseDetails();
			}
		});
	})(jQuery);
</script>
<!-- END CachedTopCategoriesHPBestfavs.jsp -->

<script type="text/javascript">
	(function(){
		var $replaceFirstName = $('.replace-first-name');
		//alert($replaceFirstName.length);
		if(!$replaceFirstName.length) return false;
		
		var $firstName = $('input[name="firstName"]');
		var firstName = $firstName.length ? $firstName.val() : false;
		if(!firstName) return false;

		$replaceFirstName.each(function(){
			var $this = $(this);
			if($this.text().toUpperCase() === 'YOUR') {
				$this.text(firstName + '\'s');
			}else if($this.hasClass('comma_before')) {
				$this.text(', ' + firstName);
			}else {
				$this.text(firstName);
			}
			
		});
		
	})(jQuery);
</script>
	
