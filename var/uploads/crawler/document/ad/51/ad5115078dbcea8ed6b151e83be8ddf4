	<script type="text/javascript">
		(function(){
			// fix the ordering of the hidden list
			var selectedStore = 'ca';
			if (selectedStore == 'us'){
				var esDomain = 'http://es.lenscrafters.com'.replace(/^https?:\/\//, '').replace(/\/.*/, '');
				var parent = $('#countrySelector li.list ul');
				if (document.domain == esDomain){
					// remove all non-es options from the list that is initially shown  
					$('#countrySelector > li.selected a').show().not('.es').remove();
					// move the es option to the top of the list that is shown after the dropdown is clicked
					$('#countrySelector > li.list li.es').prependTo(parent);
				}else{
					$('#countrySelector > li.selected a').show().not('.us').remove();
					$('#countrySelector > li.list li.us').prependTo(parent);
				}
			}
			else if (selectedStore == 'ca'){
				var frHome = 'http://fr.lenscrafters.ca';
				var frDomain = frHome.replace(/^https?:\/\//, '').replace(/\/.*/, '');
				var parent = $('#countrySelector li.list ul');
				if (document.domain == frDomain){
					$('#countrySelector > li.selected a').show().not('.fr').remove();
					$('#countrySelector > li.list li.fr').prependTo(parent);
				}else{
					$('#countrySelector > li.selected a').show().not('.ca').remove();
					$('#countrySelector > li.list li.ca').prependTo(parent);
				}
			}
			$('#countrySelector li.list li').first().addClass('current').find('a').append('<span></span>');
		})();
		function switchLanguage(lang) {
		    MP.SrcUrl=unescape('mp_js_orgin_url');MP.UrlLang='mp_js_current_lang';MP.init();
	       	    MP.switchLanguage(MP.UrlLang==lang?'en':lang);
		    return false;
		}

	</script>
