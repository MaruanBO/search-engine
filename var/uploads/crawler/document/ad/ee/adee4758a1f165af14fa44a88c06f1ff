var FindCitiCountryListObject = FindCitiCountryList.split("|"); 

 

var countryValueFromContent = document.getElementById("countryFlyOut").options[document.getElementById("countryFlyOut").selectedIndex].value; 

        var isFindMyCitiFlag = "false"; 

        for(var index=0;index<FindCitiCountryListObject.length;index++) 

        { 

                                var countryURLPair=FindCitiCountryListObject[index].split('^'); 

                if(countryValueFromContent == countryURLPair[0]) 

                {                 

                isFindMyCitiFlag="true"; 

                findMyCitiURL =countryURLPair[1]; 

                

 

                } 

        } 

 

if(isFindMyCitiFlag=="true"){ 

findMyCitiURL = "http://" + findMyCitiURL; 

window.open (findMyCitiURL ,"mywindow"); 

} 

else{ 

document.forms['find-form'].submit(); 

} 

} 

