<!--/fi:/csp/mediapool/sites/shared/includes/sportsScoreboardWidget.csp, e:.00023 -->

          <article id="lead-story" class="h-entry hentry">
						<time class="dt-published updated published" style="display: none;" datetime="2015-02-19 14:46:45">
							Feb 19, 2015 14:46
						</time>
						<div class="promo-text">
							
<!-- fi:/csp/mediapool/sites/shared/includes/promoText.csp -->

<!--/fi:/csp/mediapool/sites/shared/includes/promoText.csp, e:.000258 -->

						</div>
            <h1 class="type__font-family--sans-cond type__font-weight--bold p-name entry-title">
              
<!-- fi:/csp/mediapool/sites/shared/includes/storyHeadlineLink.csp?class=type__color--base -->
<!-- home url -->

<a href="http://theadvocate.com/news/politics/11637899-123/jeremiah-wright-says-blacks-shouldnt" class="type__color--base" style="">
  
<!-- fi:/csp/mediapool/sites/shared/includes/storyHeadline.csp -->
