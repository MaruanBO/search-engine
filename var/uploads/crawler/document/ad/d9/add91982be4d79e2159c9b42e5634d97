                    <script type="text/javascript">
                    $.get('/blog/xml/news-and-press.xml', function(xml){
                        $('#home-news-list-display').html(''); /* clear result div */
                        var rss = $.xml2json(xml);
                        $.each(rss.channel.item, function(i, item){
                            if (item.showonhome == '1'){
                                if (item.icon !== ''){
                                    $('#home-news-list-display').append('<li class="has-icon">');
                                } else {
                                    $('#home-news-list-display').append('<li>');
                                }
                                if (item.icon !== ''){
                                    $('#home-news-list-display').append('<img class="home-news-icon" height="13" src="'+item.icon+'" width="16" />');
                                }
                                $('#home-news-list-display').append('<span class="home-events-list">');
                                trimmedtext = item.text.slice(0, -1);

                                if (item.type == 'news'){
                                    $('#home-news-list-display').append('<a target="_blank" href="'+item.link+'">'+trimmedtext+'</a></span></li>');
                                } else {
                                    $('#home-news-list-display').append('<a href="'+item.link+'">'+trimmedtext+'</a></span></li>');
                                }
                            }
                        });
                    });
                    </script>

                    <ul class="home-events-list">
                        <div id="home-news-list-display"></div>
                    </ul>
                </div>
