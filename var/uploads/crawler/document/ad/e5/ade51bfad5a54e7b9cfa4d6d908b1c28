<script type="text/javascript">
/* <![CDATA[ */
(function() {
    Modernizr.load([
        {
            /* Logical list of things we would normally need*/
            test : jQuery.doTimeout,
            /* Modernizr.load loads css and javascript by default*/
            nope : {"dotimeout":'http://s2.mdpcdn.com/web/js-min/js/mdp/lib/jquery/plugins/dotimeout/jquery.ba-dotimeout.min_b94d1c551ed3dceaefc594bc386500af.js'},
            callback: function (url, result, key) {
                if (key === 'dotimeout') {
                    loadDynamicLead();
                }
            }
        }
    ]);

    var currentItem = 0;
    var oldItem = -1;
    var itemsArray = $("#leadObject .leadItem"),
        lookupArray = [];
    var itemsArrayLen = itemsArray.length;
    var currentItemArray = [];
    var oldItemArray = [];

    var goToDestination = function(url) {
        document.location = url;
    };


    var loadDynamicLead = function(){
        itemsArray.children(".leadDesc, .leadImage").fadeOut();
        itemsArray.eq(currentItem).addClass('on').find(".leadDesc, .leadImage").fadeIn();

        itemsArray.each(function(i, e) {
            lookupArray.push({
                "leadItem":$(e),
                "leadImage":$(e).children(".leadImage"),
                "heading2":$(e).find(".heading2"),
                "heading2a":$(e).find(".heading2 a"),
                "gotoDestination":$(e).find(".heading2 a").attr("href"),
                "leadDesc":$(e).find(".leadDesc"),
                "index":i
            });

            lookupArray[i].heading2.html(lookupArray[i].heading2a.text());

        });

        clickItems( currentItem );

        $.doTimeout( 'autoStart', 4000, function(){
            changeSlide( null );
            return true;
        } );

        $("#leadObject").fadeIn("slow");
    };

    var changeSlide = function( slideNumber ){
        oldItem = currentItem;

        if (slideNumber==null) {
            currentItem++;
        } else {
            currentItem = slideNumber;
        }

        if ( currentItem >= itemsArrayLen ) currentItem = 0;
        if ( currentItem < 0 ) currentItem = itemsArrayLen-1;


        currentItemArray = lookupArray[ currentItem ];
        oldItemArray = lookupArray[ oldItem ];

        lookupArray[oldItem].leadDesc.fadeOut("fast", function() {
            lookupArray[oldItem].leadImage.fadeOut("fast");
            lookupArray[oldItem].leadItem.removeClass("on");
            lookupArray[currentItem].leadItem.addClass("on");
            lookupArray[currentItem].leadDesc.fadeIn("fast");
            lookupArray[currentItem].leadImage.fadeIn("slow");
            clickItems(currentItem);
        });
    };


    var clickItems = function( click_currentItem ) {

        $(lookupArray).each(function(i, e) {
            if( i != click_currentItem ){
                lookupArray[i].heading2.unbind("click").click(function() {
                    $.doTimeout( 'autoStart' );
                    $.doTimeout( 'reAutoStart' );
                    $.doTimeout( 'reAutoStart', 6000, function(){
                        $.doTimeout( 'autoStart', 4000, function(){
                            changeSlide( null );
                            return true;
                        } );
                    } );
                    changeSlide( lookupArray[i]["index"] );
                });


            } else {

                lookupArray[i].heading2.unbind("click").click(function() {
                    goToDestination(lookupArray[i].gotoDestination);
                });
                lookupArray[i].leadDesc.unbind("click").click(function() {
                    goToDestination(lookupArray[i].gotoDestination);
                });
                lookupArray[i].leadImage.unbind("click").click(function() {
                    goToDestination(lookupArray[i].gotoDestination);
                });

            }
        });

    };

})();
    /* ]]> */
</script>
