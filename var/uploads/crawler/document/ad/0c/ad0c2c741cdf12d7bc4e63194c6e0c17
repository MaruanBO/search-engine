<script type="text/javascript">
if(col4news.feedName && col4news.feedName.length > 0){
	CScol4news();
}

function CScol4news(){
	var status = 0;
	var totalParse = col4news.feedCategory.length;
	var storage = new Array(totalParse);
	
	for(var ii=0;ii<totalParse;ii++){
		WNHttpRequestManager.makeRequest(col4news.feedCategoryPath[0] + col4news.feedCategory[ii] + col4news.feedCategoryPath[1], {
			onSuccess: {callback : initFeed, args : ii},
			onError: function() {}
		});	
	}
	
	function initFeed(num){
		status++;
		col4newsParseFeed(this,num);
		if(status === totalParse){
			organizeItem();
		}
	}
	
	function col4newsParseFeed(xml,arg){
		var fullFeed = xml.response.responseXML;
    	var $target = $wn(fullFeed).find('story').not('[displaysize="19"]').first();
		if($target.length > 0){
			var id = $target.find("id:first").text();
			var headline = $target.find("headline:first").text();
			var pageURL = $target.find("seo:first pageurl").text();
			if(!pageURL){
				pageURL = "article";
			}
			var fullURL = "/story/" + id + "/" + pageURL;
			storage[arg] = "<li class='wnItem feature'><h4 class='wnContent headline'><div class='section'><a href='" + col4news.feedSectionURL[arg] + "'>" + col4news.feedName[arg] + "</a></div><div class='text'><a href='" + fullURL + "'>" + headline + "</a></div></a></h4><div class='wnClear'></div></li>";
		}
	}
	
	function organizeItem(){
		$wn.each(storage,function(index, value){ 
			if(value === undefined){
				storage.splice(index,1);
			}
			$wn("#CScol4news .contentGroup").append(storage[index]);
		});
		$wn("#CScol4news .feature:first").addClass("first");
		$wn("#CScol4news .feature:last").addClass("last");
	}
	if(col4news.blockTitle.length){
		$wn("#CScol4news .header .text").html(col4news.blockTitle);
	} else {
		$wn("#CScol4news .header").hide();
	}
}
</script></div><script>Worldnow.Helpers.debug('UB WTVT customized At A Glance module with split radar & Livestream player(315115)', 'UB_315115');</script></div>
