<script type="text/javascript">
var setNewBadge = false;
function CStsaFixDate($el){
	$el.find("script").remove();
	$el.find("noscript").remove();
	$el.html($el.text());
	
	var sd = $el.text();
	sd = sd.slice(sd.indexOf("Updated: ") + 9, sd.length);
	sd = new Date(sd);
	var currentTime = new Date();
	var iTime = CStimeDiff(currentTime.getTime(), sd.getTime());
	var timeAgo = CStimeAgo(iTime, currentTime, sd);
	$el.html(timeAgo);
}

function CStimeDiff(Cms,Ims){
	var ms = Cms - Ims;
	var sec = Math.floor(ms/1000);
	ms = ms % 1000;
	t = ms + "MS";
	
	var min = Math.floor(sec/60);
	sec = sec % 60;
	t = sec + "S" + t;
	
	var hr = Math.floor(min/60);
	min = min % 60;
	t = min + "M" + t;
	
	var day = Math.floor(hr/24);
	hr = hr % 24;
	t = hr + "H" + t;
	t = day + "D" + t;
	
	return t;
}

function CStimeAgo(fullTime, cT, iD){
	var tdays = fullTime.substring(0, fullTime.indexOf("D"));
	var thours = fullTime.substring(fullTime.indexOf("D") + 1, fullTime.indexOf("H"));
	var tmins = fullTime.substring(fullTime.indexOf("H") + 1, fullTime.indexOf("M"));
	tmins = (60 * parseInt(thours) + parseInt(tmins));
	if (tdays == 0 && tmins == 0) {
		setNewBadge = true;
		tmins = "Updated moments ago";
		return tmins;
	} else if (tdays == 0 && tmins > 0 && tmins <= 59) {
		setNewBadge = true;
		tmins = "Updated " + tmins + " minutes ago";
		return tmins;
	} else {
		setNewBadge = false;
		return;
	}
}

function CStsaRestyle(){
	$wn("#wnHideParent").parents(".wnDVUtilityBlock").hide();
	$wn("#WNDS-18, #WNDS48, #WNCol2 .wnDS-20").addClass("csTSA");
	$wn("#WNTopStoryWrap").addClass("csTSA").find("#WNTSWContent").before("<div id='csTSAHeader'><span class='title'>Featured Stories</span><span class='more'><a href='/category/230109/news'>More &gt;&gt;</a></span></div>");
	
	var maxThumbItem = 6;
	$wn("#WNTSWContent .wnDS-20 .wnDSItems-standard").append("<div class='thumbGroup'></div>");
	var $targetDiv = $wn("#WNTSWContent .wnDS-20 .wnDSItems-standard .thumbGroup");
	var $mainItem = $wn("#WNTSWContent .wnDS-20 .wnDVSummary:first");
	
	if($mainItem.find(".wnDate").length > 0){
		CStsaFixDate($mainItem.find(".wnDate"));
	}
			
	if($mainItem.find("h4").hasClass("wnRole-CLIP") || $mainItem.find(".wnVideoIncluded").length){
		$mainItem.find("h4").removeClass("wnRole-CLIP");
		$mainItem.find("p").addClass("wnRole-CLIP");
	}
	if(!$mainItem.find(".wnImage").length){
		$mainItem.find("h4").before("<div class='wnImage'><a href='" + $mainItem.find("h4 a").attr("href") + "'><img border='0' title='" + $mainItem.find("h4 a").attr("text") + "' alt='' src='" + csTSAbackupImg + "'></a></div>");
	} else {
		$mainItem.find(".wnImage").removeClass().addClass("wnImage");
	}
	if($wn("#WNCol2 .wnDS-20 .wnDVSummary").length > 0){
		$mainItem.addClass("hasSibling");
		var $thumbItem = $wn("#WNCol2 .wnDS-20 .wnDVSummary:lt(" + maxThumbItem + ")");
		$thumbItem.each(function(){
			if($wn(this).find(".wnDate").length > 0){
				CStsaFixDate($wn(this).find(".wnDate"));
				if(setNewBadge === true){
					$wn(this).find(".wnDate").addClass("newBadge");	
					$wn(this).find(".wnDate.newBadge").html("New");
				}
			}
			if($wn(this).find(".wnVideoIncluded").length){
				$wn(this).find("h4").addClass("wnRole-CLIP");
			}
			if($wn(this).find(".wnImage").length > 0){
				$wn(this).find(".wnImage").removeClass().addClass("wnImage").insertBefore($wn(this).find("h4"));
			} else {
				$wn(this).find("h4").before("<div class='wnImage'><a href='" + $wn(this).find("h4 a").attr("href") + "'><img border='0' title='" + $wn(this).find("h4 a").attr("text") + "' alt='' src='" + csTSAbackupImg + "'></a></div>");	
			}
			$wn(this).addClass("thumb").appendTo($targetDiv);
		});
		$wn(".thumb:odd").addClass("odd");
		$wn(".thumb:even").addClass("even");
	}
}

PLATFORM.EventMan.registerToEvent(function(){
	if($wn("#WNTopStoryWrap").length > 0){
		CStsaRestyle();
	}
},'WNCol23done');

