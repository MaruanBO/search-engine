(function (jQuery) {
// For Request More Info and Try Now  -- based on iFrame
    jQuery.braviPopUp = function (title, src, width, height) {
        //Destroy if exist
        jQuery('.overlay').remove();
        //create the popup html
        var html = '<section class="overlay_wrap" style="width:' + width + 'px; height:' + height + 'px;">';
        html += ' <a href="javascript:void(0)" class="closed" style="background:none">X</a>';
        html += ' <section id="dv_no_move">';
		html += ' <h3>' + title + '</h3>';
        html += ' <iframe id="url" scrolling="no" src="' + src + '"  style="border:none; background:transparent" frameborder="0" allowtransparency="true" width="100%" height="80%"></iframe>';
        html += ' </section>';
        html += ' </section>';

        //add to body
	   jQuery('<section class="overlay22"></section>').prependTo('body').attr('id', 'overlay22');
	   jQuery('.overlay22').html(html);
	   

//		jQuery(".overlay").animate({opacity:0.8}, 400);
		var winheight = jQuery(window).height();
		var toppostwarp = (winheight-height)/2;
		jQuery(".overlay_wrap").css('top', toppostwarp);

		jQuery(window).resize(function() {
		var winheight = jQuery(window).height();
		var toppostwarp = (winheight-height)/2;
		jQuery(".overlay_wrap").css('top', toppostwarp);	
		});

        //enable dragable
    };

