var pageType = '';
var isProductionPublish = false;
var cqPageName = 'home:en-us:Home';


		isProductionPublish = true;


	function getCurrentPageName(){
		return cqPageName;
	}
	
	function captureSocialEvent(url){
		CQ_Analytics.record({event: 'likesfollows', 
			values: { 'SocialLinks': url,'pageName':getCurrentPageName() }, 
			collect: false,
			componentPath: 'iyogimainsite/components/page/basepage'});
	}

