	jQuery(document).ready(function() {
	var loc = jQuery(location).attr('href');

    if (loc.indexOf("/support") > -1) {
    jQuery("#supportmenu a:first").addClass( "active" );
    }

    if (loc.indexOf("/services") > -1) {
    jQuery("#servicesmenu a:first").addClass( "active" );
    }

    if (loc.indexOf("/delivery") > -1) {
    jQuery("#deliverymenu a:first").addClass( "active" );
    }

    if (loc.indexOf("/about-us") > -1) {
    jQuery("#aboutusmenu a:first").addClass( "active" );
    }

	if (loc.indexOf("contact.html") > -1) {
    jQuery("#contactusmenu a:first").addClass( "active" );
    }

	});
</script>    

