// For GEO
function showHint(){
	dataString = "";
    jQuery.ajaxSetup({ cache: false });
	jQuery.get("/.IPLigenceServlet.sjson",function(data,status){
	var CountryCode = data.CountryCode;
    var RedirectURL = data.RedirectURL;
     var URL = window.location.hostname;    
     if(URL == 'www.iyogi.com')	{
            if(CountryCode == 'IN') {
               document.location = 'http://www.iyogi.in';
            }
        }
	//if(RedirectURL != '') {
	//		document.location = text;
	//	}
	});
}


