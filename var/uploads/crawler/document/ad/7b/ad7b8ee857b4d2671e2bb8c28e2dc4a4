<script type="text/javascript">
jQuery(document).ready(function() {
    if(jQuery.cookie("iyogicom_global_tfn")!=undefined && jQuery.cookie("iyogicom_global_tfn")!="") {
        var ele= document.getElementsByClassName("iyogicom_global_tfn");
        for(var i=0;i<ele.length;i++){
            ele[i].innerHTML=jQuery.cookie("iyogicom_global_tfn");
        }

        var globalTfnHref=jQuery.cookie("iyogicom_global_tfn");
        globalTfnHref.replace(/\s+/g, '-');
        // alert("globalTfnHref==>"+globalTfnHref);
        var hrefElement= document.getElementsByClassName("iyogicom_global_tfn_href");
        for(var i=0;i<hrefElement.length;i++){
            hrefElement[i].href='tel:'+globalTfnHref;
        }
    }
});
</script>
<script language="javascript">
    /* Auto Chat code Time : 60 Second */
    jQuery(document).on('ready',function(){
        //setInterval(updateDiv,60000);
        setInterval(updateDiv,60000);
  	});

	function updateDiv(){

        if (document.cookie && document.cookie.indexOf('chatCookies=iYogi') != -1) {
            //alert('Cookies present');
            //jQuery('.chat_pop').show('slow');
        } else {
            jQuery('.chat_pop').show('slow');
            writeCookie ("chatCookies", "iYogi", 1);
            //alert('Cookie is NOT present');
        }

    }

	/* Auto Chat code Time : 60 Second */
    function writeCookie (key, value, hours) {
        var date = new Date();
        date.setTime(+ date + (hours * 86400000)); 
        window.document.cookie = key + "=" + value + "; expires=" + date.toGMTString() + "; path=/";
        return value;
    };
    function Get_Cookie(w) {
        cName = "";
        pCOOKIES = new Array();
        pCOOKIES = document.cookie.split('; ');
        for(bb = 0; bb < pCOOKIES.length; bb++){
            NmeVal  = new Array();
            NmeVal  = pCOOKIES[bb].split('=');
            if(NmeVal[0] == 'expires'){
                cName = unescape(NmeVal[1]);
            }
        }
        return cName;
	}
</script>

