<script type="text/javascript">
function TT_addMbox(mboxName){
  var TT_container = document.getElementById('TT_container');
  if (TT_container == null || TT_container.length < 1){
    // create element to contain refreshable mbox
    TT_container = document.createElement("div");
    TT_container.id = 'TT_container';
    // add element to body
    jQuery(TT_container).prependTo(jQuery('body'));
  }
  // add empty mbox div
  var mboxDiv = document.createElement("div");
  mboxDiv.className = "mboxDefault";
  TT_container.appendChild(mboxDiv);
  // add create script
  var script = document.createElement("script");
  script.type = "text/javascript";
  if(getProfileUcId() && getProfileUcId().length > 1){
    script.text = "mboxCreate('"+mboxName+"','mbox3rdPartyId="+getProfileUcId()+"');";
  } else {
    script.text = "mboxCreate('"+mboxName+"');";
  }
  TT_container.appendChild(script);
}
// add specific mbox's to the page
try{
  // add to specific pages
  if(!jQuery('body').hasClass('intlCN')){
    if(pageDefinitionId){
      if(pageDefinitionId === 'Cart'){
        TT_addMbox('nm_template_Cart3');
        TT_addMbox('nm_template_Cart5');
      }
    }
  }
}catch(e){
  // do nothing
}
</script>

	
	
		
			
				
				
					
