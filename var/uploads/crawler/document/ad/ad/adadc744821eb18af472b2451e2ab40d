			tellapartBreadcrumb="";
			var breadCrumbText="";  // get the breadcrumb and remove white space
			if(!eVar45Flag){
				try {
					breadCrumbText = jQuery(".breadcrumbelements")[0].innerHTML;
					var untrimmed = jQuery('<div/>').html( breadCrumbText ).text();
					breadCrumbText = jQuery.trim(untrimmed).replace(/[\s\xA0]+/g,' ').replace(/:/g, '>');
				} catch (error) {}
				if(breadCrumbText==""){
					try {
						breadCrumbText = document.getElementById( "breadcrumb" ).innerHTML; 
						var untrimmed = jQuery('<div/>').html( breadCrumbText ).text();
						if (untrimmed.toLowerCase().indexOf("search") == 0) {
							breadCrumbText = "search";
						} else {
							breadCrumbText = jQuery.trim(untrimmed).replace(/[\s\xA0]+/g,' ').replace(/:/g, ' >');
							
							//this replaces first forward slashes from CUSP breadcrumb for correct tellapart tagging format
							breadCrumbText = breadCrumbText.replace(/\/\/\s/,'');
							//this replaces forward slashes from CUSP breadcrumb with gtr sign, eclipse is confused on this one.
							//tagging must match all other brands
							breadCrumbText = breadCrumbText.replace(/\/\//g,' >');
						}
					} catch (error) {}
				}
			}
			if(breadCrumbText != ""){
				var lowerBreadCrumbTest = breadCrumbText.toLowerCase();
				if(lowerBreadCrumbTest.indexOf("search") == 0){
					tellapartBreadcrumb = "Search";
					return "Search"
				} else {
					tellapartBreadcrumb = breadCrumbText;
					return breadCrumbText;
				}
			}

			// Ntt should only be added to the url if on the search page
			var Ntt = getUrlParam("Ntt");
			if(!Ntt){  // grab omniture tag
				if(s.eVar45){
					var cats = s.eVar45.split( "|" );
					catsup = cats[0].replace(/:/g, ' > ');
