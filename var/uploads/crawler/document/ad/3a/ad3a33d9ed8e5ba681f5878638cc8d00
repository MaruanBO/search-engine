<script type="text/javascript">
$(function(){

    $('#postjob-yes').click(function(){

        $('#postJobModal').on('hidden.bs.modal', function (e) {
            $('#postJobModal').off('hidden.bs.modal');
            setTimeout(function() {
                $('#postJobFormModal').modal('show');
            }, 100);
        });
        
        $('#postJobModal').modal('hide');
    });
   
    $('#postjob-no').click(function(){
        window.open("http:\/\/www.indeed.com\/hire?indpubnum=5431754608249411&chnl=en_us",'_blank');
    });

    var postJobFormHtml = $("#postJobFormModal div.modal-body").html();
    $('#postJobFormModal').on('hidden.bs.modal', function (e) {
        $("#postJobFormModal div.modal-body").html(postJobFormHtml);
        $('#postjob-send').removeAttr('disabled');
    });

    $('#postjob-send').click(function(){
        
        $.ajax({
            type : 'POST',
            url  : '/ajax',
            data : {
                action : 'post_job_form',
                company: $("#postJobForm input[name=company]").val(),
                website: $("#postJobForm input[name=website]").val(),
                name: $("#postJobForm input[name=name]").val(),
                phone: $("#postJobForm input[name=phone]").val(),
                email: $("#postJobForm input[name=email]").val()
            },
            success : function() {}
        });
        
        $("#postJobFormModal div.modal-body").text("Thank you for contacting us. We will get back to you.");
        $('#postjob-send').attr('disabled', 'yes');
        
        setTimeout(function(){
            $('#postJobFormModal').modal('hide');
        }, 3000);
    });
    
});    
</script>
    

