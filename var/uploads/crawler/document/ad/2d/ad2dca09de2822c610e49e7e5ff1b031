<script type="text/javascript" charset="utf-8" src="//static.atgsvcs.com/js/atgsvcs.js"></script><script type="text/javascript">

(function(){
    var second = 0;
    var configCalendarRaw = "Feb 18, 2015 05:30:00 AM";
    var aCalendarResults = configCalendarRaw.split(" ");
    // convert datepicker month output string to integer
    var iMonth = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"].indexOf(aCalendarResults[0]) + 1;
    var iDay = parseInt(aCalendarResults[1]);
    var iYear = parseInt(aCalendarResults[2]);
    var aTime = aCalendarResults[3].split(":");
    var iHour = parseInt(aTime[0]);
    var iMinute = parseInt(aTime[1]);
    var meridiems = aCalendarResults[4];
    var daysLabel = "Days";
    var hoursLabel = "Hrs";
    var minutesLabel = "Mins";
    var secondsLabel = "second";
    var separator = ":";
    var endMessage = "";
    var endImage = "//static.zumiez.com/skin/frontend/delorum/default/images/12-20-to12-20-holiday-banner-462x46.jpg";

            var leadingText = "Extended!";
    
        var trailingText = "";
    
    // Perform 24 hour conversion
    if  ( meridiems === 'PM') {
        iHour = parseInt(iHour) + 12;
    } else if ( meridiems == 'AM' && iHour == 12) {
        iHour = 0;
    }

    //console.log('iMonth: ' + iMonth  + ' iDay: ' + iDay + ' iYear: ' + iYear + ' iHour: ' + iHour + ' iMinute: ' + iMinute + ' meridiems: ' + meridiems );

    var target = "prez-timer";

    countdown(target, iYear, iMonth, iDay, iHour, iMinute, second);
    
    function countdown(clockID, year, month, day, hour, minute, second) {
        Today = new Date();
        Todays_Year = Today.getFullYear();
        Todays_Month = Today.getMonth();
        todaysDate = (new Date(Todays_Year, Todays_Month, Today.getDate(),
                         Today.getHours(), Today.getMinutes(), Today.getSeconds())).getTime();
        targetDate = (new Date(year, month - 1, day, hour, minute, 00)).getTime();

        //Find their difference, and convert that into seconds.                  
        timeLeft = Math.round((targetDate - todaysDate) / 1000);
        var clock = document.getElementById(target);

        if (timeLeft < 0) timeLeft = 0;

        if ((timeLeft == 0) && (clock)) {
                            $(clock).update('<span class="outOfTime">' + endMessage + '</span>');
                        $(clock).show();
            return false;
        }

        // Calculates the time that is left
        days = Math.floor(timeLeft / (60 * 60 * 24));
        days = ((days < 10) && (days !=0)) ? "0" + days : days;
        timeLeft %= (60 * 60 * 24);
        hours = Math.floor(timeLeft / (60 * 60));
        hours = hours < 10 ? "0" + hours : hours;

        timeLeft %= (60 * 60);
        minutes = Math.floor(timeLeft / 60);
        minutes = minutes < 10 ? "0" + minutes : minutes;
        timeLeft %= 60;
        seconds = timeLeft < 10 ? "0" + timeLeft : timeLeft;

        if ((daysLabel == 'days') && (days == 1)) daysLabel = 'day';
        if ((daysLabel == 'days') && (days == 0)) daysLabel = '0 days';

        if(clock){
            var clockString = '<div id="countdownContainer"><span class="cdLeadingText">' + leadingText + '</span>' +
                '<div class="dayContainer"><span class="cdDays"> ' + days + '</span><span class="daysLabel">' + daysLabel + '</span></div>' +
                '<div class="timeSeparator">' + separator + '</div>' +
                '<div class="hourContainer"><span class="cdHours">' + hours + '</span><span class="hoursLabel">' + hoursLabel + '</span></div>' +
                '<div class="timeSeparator">' + separator + '</div>' +
                '<div class="minutesContainer"><span class="cdMinutes">'+ minutes + '</span><span class="minutesLabel">' + minutesLabel + '</span></div>' +
                '<div class="timeSeparator">' + separator + '</div>' +
                '<div class="secondsContainer"><span class="cdSeconds">'+ seconds + '</span><span class="SecondsLabel">' + secondsLabel + '</span></div>' +
                '<span class="cdTrailingText">' + trailingText + '</span>' + '</div>';


            $(clock).update(clockString);
            $(clock).show();
        }
        //Recursive call, keeps the clock ticking.
        second += 1000;
        setTimeout(function(){countdown(clock, year, month, day, hour, minute, second);}, 1000);
    }
})();
</script>

