<script>
$(function() {
    $('.indDiv').click(function () {
        if ($('.visible-phone').is(':visible')) {
        $(this).next('.indTog ').animate({height: 'toggle'});
        $(this).find('.icon-chevron-down').toggle();
        $(this).find('.icon-chevron-up').toggle();
        return false;
        }
    });

    $(window).resize(function() {
    clearTimeout(this.id);
    this.id = setTimeout(doneResizing, 300);
    });

    function doneResizing(){
        if ($('.visible-phone').is(':hidden')) 
        {
            $('.indDiv span[class^="icon-chevron"]').hide();
            $('.indTog.hidephone').show(); 
        }
        else 
        {
            $('.indDiv .icon-chevron-down').show();
            $('.indDiv .icon-chevron-up').hide();
            $('.indTog.hidephone').hide();    
        }  
    };
});
