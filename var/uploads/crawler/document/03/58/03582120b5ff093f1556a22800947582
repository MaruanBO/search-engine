<script>
jQuery(document) . ready(function(){
	jQuery(".account").on('mouseenter mouseleave click', function(e){
		if (e.type== 'mouseenter') {
			jQuery("ul.sub-nav").slideDown();
		} else if (e.type == 'mouseleave') {
			jQuery("ul.sub-nav").slideUp();
		} else {
			jQuery("ul.sub-nav").slideToggle();
		}
	});
	jQuery('table.tag-nav td') . click(function(){
		if (jQuery(this).attr('class') == 'current')
			return;
		
		jQuery('div.dy-content') . html('') . addClass('loading');
		jQuery('table.tag-nav td.current') . removeClass();
		jQuery.post(
			'/wp-admin/admin-ajax.php', 
			 {
				action: 'home_nav_content',
				data: {word: jQuery(this) . data('word')},
				async: false
			 }, 
			function(response){
				jQuery('div.dy-content') . removeClass('loading') . html(response);
			 }
		);
		jQuery(this) . addClass('current');;
	});
});

function fbLogin(pos){
	FB.login(function(response){
		if (response.status === 'connected'){
			if (pos == 'body') {
				window.location.href = "/?action=logined&active=new-form";
			} else {
				window.location.href = "/?action=logined";
			}
		}
	}, {scope: 'email,user_likes'});
}
function login(){
	fbLogin('body');
}
function showTip(){
	if(jQuery('#tipbox').css('display') == 'block'){
		jQuery('#tipbox').hide();
		return;
	}
	var top = jQuery("#tip_img").offset().top - jQuery('#tipbox').height() * 1.75;
	var left = jQuery("#tip_img").offset().left - jQuery('#tipbox').width()/2.2;
	
	jQuery('#tipbox').css('top',top);
	jQuery('#tipbox').css('left',left);
	jQuery('#tipbox').show();
}
</script>
