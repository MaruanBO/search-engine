    <script>
    document.write(unescape("%3Cscript src='" + document.location.protocol + "//munchkin.marketo.net/munchkin-beta.js' type='text/javascript'%3E%3C/script%3E"));
</script>
<script>
(function () {
    function containsItem(itemToFind, allItems) {
        var isFound = false;

        for (var i = 0; i < allItems.length && !isFound; i++) {
            isFound = (itemToFind.substr(0, allItems[i].length) === allItems[i]); // match prefixes
        }

        return isFound;
    }

    function initializeMarketo()
    {
        if (typeof Munchkin !== 'undefined') { // Might be undefined if munchkin.js script hasn't finished loading
            var marketoAccountId = '417-NLK-080'; // '417-NLK-080';
            var pagePath = window.location.pathname.toLowerCase();
            var pagesToTrack = [
                '/hr',
                '/biz/',
                '/compensation',
                '/payscale-index',
                '/createaccount.aspx'
                ];

            if (window.location.hostname.toLowerCase() === 'marketrate.payscale.com' || (window.location.pathname === '/') || containsItem(pagePath, pagesToTrack)) {
                Munchkin.init(marketoAccountId);
            } else {
                // Workaround for Marketo not being able to handle all of our traffic
                Munchkin.init(marketoAccountId, {cookieAnon: false});
            }
        }
    }

    initializeMarketo();
})();
</script>


<script type="text/javascript">checkLoadTime()</script>

    

