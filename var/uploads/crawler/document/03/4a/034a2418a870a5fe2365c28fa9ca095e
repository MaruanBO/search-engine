    TSTBrowserCheck = {
       browserMessageShown:false,
       cookieName:"tstbrowsercheck",
       browserCheckCleared:false,
       browserCheck: function(){
          try {
            if(document.cookie.indexOf(TSTBrowserCheck.cookieName) == -1) {
                var uagent = navigator.userAgent;
                if (/MSIE (\d+\.\d+);/.test(uagent)){
                   var ieversion=new Number(RegExp.$1);
                   if (ieversion < 8 &&  (uagent.indexOf("Trident/") == -1)) {
                      try {
                         var el = document.getElementById('browserWarning');
                         el.style.display = 'block';
                         TSTBrowserCheck.browserMessageShown = true;
                      } catch(e) {}
                   } else {
                      //set the cookie for >=ie8
                      document.cookie = TSTBrowserCheck.cookieName + "=true;path=/;domain=.thestreet.com";
                   }
                } else {
                   //set the cookie for other browsers
                   document.cookie = TSTBrowserCheck.cookieName + "=true;path=/;domain=.thestreet.com";
                }
            }
          }catch(e){}
       },

       closeBrowserMessage:function(evt){
          try {
             //message has been seen and acknowleged. Set cookie
             document.cookie = TSTBrowserCheck.cookieName + "=true;path=/;domain=.thestreet.com";
             var el = document.getElementById('browserWarning');
             el.style.display = 'none';
             TSTBrowserCheck.browserMessageShown = false;
             TSTBrowserCheck.browserCheckCleared = true;
             TSTWelcomOverlay.init();
          } catch(e){}
       }
    }

    TSTBrowserCheck.browserCheck();
</script>

