<script  type="text/javascript">

    $(document).on("welcomeAdRemoved", function(event){
        fireTSTOnReady();
    })



    TSTWelcomOverlay = {
        waitForWelcomeComplete:false,
        showWelcome:true,
        fireCompleteEvent: function(){
            $.event.trigger({
                type: "welcomeAdRemoved"

            });
        },
        remove: function(){
            $('#welcomeOverlay').hide();
//            try {
//                $('#tstSecondDiv').show();
//            }catch(e){}
            TSTWelcomOverlay.fireCompleteEvent();
            window.clearTimeout(window.TSTWelcomOverlayTimeout);

        },
        getQueryVariable :function(variable){
            var ref = document.referrer.split('?');
            if(ref.length == 1) return "";
            var vars = ref[1].split('&');
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split('=');
                if (decodeURIComponent(pair[0]) == variable) {
                    return decodeURIComponent(pair[1]);
                }
            }
            return "";
        },
        init:function(){
            //      _gaq.push(['_trackEvent', 'Debug', "welcome_ad", "eligible"]);
            if (TSTWelcomOverlay.decideShow()){
                TSTWelcomOverlay.waitForWelcomeComplete=true;

                var scriptAd   = document.createElement("script");
                scriptAd.type  = "text/javascript";
                scriptAd.text="googletag.cmd.push(function() {googletag.pubads().display('/' + '367/tsc-thestreet.com/overlay',[[300,250],[640,480]], 'div-gpt-ad-welcome');});"
                document.getElementById("div-gpt-ad-welcome").appendChild(scriptAd);

                var  selectedDate = new Date()
                var day=selectedDate.getDate();
                var month=selectedDate.getMonth() +1;
                var dateString =month+"-"+day;
                var url =  'http://www.thestreet.com/files/tsc/v2008/ads/overlay/welcome/dates/'+dateString+'.html';
                $.ajax({
                    type: 'GET',
                    url: url,
                    async: false,
                    jsonpCallback: 'jsonCallback',
                    contentType: "application/json",
                    dataType: 'jsonp',
                    success: function(fact) {
                        var factDate = fact.substr(0,fact.indexOf('-'));
                        var factDesc = fact.substr(fact.indexOf('-')+1);
						$('#fact').html(factDate + "<br>" + factDesc);
					},
                    error: function(e) {

                    }
                });


                $('#welcomeOverlay').show();

//                  try {
//                    $('#tstSecondDiv').hide();
//                  }catch(e){}
                window.TSTWelcomOverlayTimeout=setTimeout(TSTWelcomOverlay.remove,15000);
                //       _gaq.push(['_trackEvent', 'Debug', "welcome_ad", "shown"])



            } else {
               if(typeof TSTBrowserCheck != 'undefined' && typeof TSTBrowserCheck.browserCheckCleared != 'undefined' && TSTBrowserCheck.browserCheckCleared) {
                  fireTSTOnReady();
               }
            }

        },
        decideShow:function(){
            try {
                try{
                    var isGoogle = navigator.userAgent.toLowerCase().indexOf('googlebot') > 0;
                    var isBingbot = navigator.userAgent.toLowerCase().indexOf('bingbot') > 0;
                    if (isGoogle || isBingbot){
                        return false;
                    }
                }  catch(e){ }
                if (document.URL.indexOf('/k/') != -1) {
                    return false;
                }
                var now = new Date();
                var cookieName="tstwelcomeoverlay_"+now.getMonth()+""+now.getDate()+""+now.getYear();
                if(document.cookie.indexOf(cookieName) == -1) {
                    var expires = new Date();
                    expires.setHours(23);
                    expires.setMinutes(59);
                    expires.setSeconds(59);
                    expires = expires.toGMTString();
                    document.cookie = cookieName + "=" + now + ";expires=" + expires + ";path=/;domain=.thestreet.com";
                }
                else{
                    return false;
                }

                var sourceUrl =document.URL.toLowerCase();
                if ((sourceUrl.indexOf("cm_ven=msn")!=-1)||(sourceUrl.indexOf("cm_ven=outbrain")!=-1)||(sourceUrl.indexOf("cm_ven=taboola")!=-1)||(sourceUrl.indexOf("cm_ven=yahoo")!=-1)||(sourceUrl.indexOf("cm_ven=aol")!=-1)||(sourceUrl.indexOf("cm_ven=cnnmoney")!=-1)||(sourceUrl.indexOf("cm_ven=tsmktwatch")!=-1)||(sourceUrl.indexOf("cm_ven=rmmktwatch")!=-1)||(sourceUrl.indexOf("cm_ven=msearthlink")!=-1)||(sourceUrl.indexOf("cm_ven=tscearthlink")!=-1)||(sourceUrl.indexOf("cm_ven=unitedonli")!=-1)||(sourceUrl.indexOf("cm_ven=tradehud")!=-1)||(sourceUrl.indexOf("cm_ven=tsczacks")!=-1) || (sourceUrl.indexOf("cm_ven=msearthlinkcf")!=-1) || (sourceUrl.indexOf("cm_ven=tscaolpromo")!=-1) || (sourceUrl.indexOf("cm_ven=msmsnp")!=-1)){
                   return false;
                }

                 var exclude="aol.com business-news.thestreet.com cnnmoney.com dailyfinance.com earthlink.net jrcads.com juno.com marketing. marketwatch.com money.cnn.com msn.com nasdaq.com netzero.net outbrain.com taboola.com untd.com yahoo.com".split(" ");
                var override="mail.aol.com mail.yahoo.com msn.mail.com search.yahoo.com webmail.earthlink.net".split(" ");

                TSTWelcomOverlay.showWelcome=true;
                if( 'referrer' in document ) { // if document has a referrer
                    var tsc_ref = document.referrer;
                    try{
                        tsc_ref = tsc_ref.toLowerCase();
                    }  catch(e){ }

                    for (i=0;i<exclude.length;i++){
                        var excludeString=exclude[i];
                        if (tsc_ref.indexOf(excludeString)!=-1){

                            TSTWelcomOverlay.showWelcome=false;
                            break;
                        }
                    }

                    if (  TSTWelcomOverlay.showWelcome==false){
                        for (i=0;i<override.length;i++){
                            overrideString=override[i];
                            if (tsc_ref.indexOf(overrideString)!=-1){
                                TSTWelcomOverlay.showWelcome=true;
                                break;
                            }
                        }
                    }



                }

                return TSTWelcomOverlay.showWelcome;

            }

            catch(e){ return false;}
            return false;
        }
    }
    if(typeof TSTBrowserCheck == 'undefined' || typeof TSTBrowserCheck.browserMessageShown == 'undefined' || !TSTBrowserCheck.browserMessageShown) {
       TSTWelcomOverlay.init();
    }

</script>



<script type="text/javascript">
    window.TSTOnReadyFired=false;


    function fireTSTOnReady(){

        if ( window.TSTOnReadyFired==false){
            $.event.trigger({
                type: "tstOnReady"

            });
        }
        window.TSTOnReadyFired=true;
    }



    $(document).ready(function() {
        if (typeof TSTWelcomOverlay != "undefined"){

            if (TSTWelcomOverlay.waitForWelcomeComplete==false && (typeof TSTBrowserCheck == 'undefined' || typeof TSTBrowserCheck.browserMessageShown == 'undefined' || !TSTBrowserCheck.browserMessageShown)){
                fireTSTOnReady();
            }
        }
        else{
            if (typeof TSTBrowserCheck == 'undefined' || typeof TSTBrowserCheck.browserMessageShown == 'undefined' || !TSTBrowserCheck.browserMessageShown) {
               fireTSTOnReady();
            }

        }

    })
</script>
<div id="div-gpt-ad-interstitial-oop"></div>
<script  type="text/javascript">
 TSTInterstitial = {

    decideShow:function() {
      showInterstitial=false;
      if( 'referrer' in document ) { // if document has a referrer
         var tsc_ref = document.referrer;
         try{
             tsc_ref = tsc_ref.toLowerCase();
         }  catch(e){ }
         if(tsc_ref.indexOf('thestreet.com') != -1) {
            showInterstitial=true;
         }
      }
      return showInterstitial;
    },

    writeInterstitialSlot:function(){
      var forwardSlash="/";
      var scriptOopAd   = document.createElement("script");
      scriptOopAd.type  = "text/javascript";
      scriptOopAd.text = "googletag.cmd.push(function () {googletag.defineOutOfPageSlot('/' +'367/tsc-thestreet.com/404', 'div-gpt-ad-interstitial-oop').addService(googletag.pubads()).setTargeting('ptn','oop-2');});\n";
      scriptOopAd.text += "googletag.cmd.push(function() {googletag.display('div-gpt-ad-interstitial-oop');});";
      document.getElementById("div-gpt-ad-interstitial-oop").appendChild(scriptOopAd);
    },

    init:function() {
      if(TSTInterstitial.decideShow()) {
         TSTInterstitial.writeInterstitialSlot();
      }
    }
 }

 if(typeof TSTBrowserCheck == 'undefined' || typeof TSTBrowserCheck.browserMessageShown == 'undefined' || !TSTBrowserCheck.browserMessageShown) {
     TSTInterstitial.init();
  }

</script><!-- fix for TimeValueCalculators' "#results" anchor tag - url suck -->
