	<script type="text/javascript">
		
		require(["jquery", "jquery-ui", "autocomplete", "json2"], function() {
			$(function() {
	    		function htmlEscape(string) {
	    			if (!string || (string == null)) {
	    				return "";
	    				
	    			} else {
	    				var div = $("<div></div>");
	    				div.text(string);
	    				return div.html();
	    			}
	    		}
	    		
				var cache = {}, lastXhr;
				
				$('#query').autocomplete({
					source: function(query, callback) {
						query.term = jQuery.trim(query.term);
						
						if (query.term.length < 3) {
							callback([]);
						}
						
						var term = query.term;
						
						if (term in cache) {
							callback(cache[term]);
							return;
						}
						
						lastXhr = jQuery.ajax({
							type: "GET",
							url: "/ajax/suggestQuestion",
							dataType: 'json',
							data: { jsonInput : JSON.stringify({ 'text' : query.term }) },
							success: function(data, textStatus, xhr) {
								var results = [];
								jQuery.each(data.response.suggestions, function(i,val) {
									var ellipses = false;
									var questionVal = jQuery.trim(val.name).split(" ").slice(0, -1).join(" ");
    								
    								if (questionVal.length > 100) {
    									questionVal = jQuery.trim(questionVal.substring(0, 100));
    									ellipses = true;
    								}
    								
																			results[i] = {
											value: questionVal,
											label: (htmlEscape(questionVal) + (ellipses ? "..." : "") + '')
										};
																		
									if (val.type && (val.type == "ANSWERED")) {
										results[i].suggid = val.id;
									}
								});
								
								cache[term] = results;
								
								if (xhr === lastXhr) {
									callback(results);
								}
							}
						});
					},
					select: function(event, ui) { 
						$("#query").val(ui.item.value);
						$("#suggid").val(ui.item.suggid);
						setTimeout(function() {$("#search").submit();}, 50);
					},
					html: true,
					delay: 100,
					minLength: 3,
					appendTo: $("#searchForm")
				});
			});
		});
		
		require(["jquery"], function() {
			$('#search').bind('submit', function() {
				var retVal =  ((document.getElementById('query').value != '$webFormatter.escapeJavascriptString($defaultValue)') && (document.getElementById('query').value != ''));
				
				if (!$("#suggid").val() || ($("#suggid").val().length < 1)) {
					$("#suggid").remove();
				}
			});
		});
	</script>



