					for (var i = 0; i < dartParams.length; i++) {
						dartSrc += dartParams[i] + ';';
					}
				}
				
				dartSrc += dartParams + ';';
				
				if (sessionStateLoaded && sessionStateData.hasOwnProperty("affiliateQt")) {
					dartSrc += 'qt=' + sessionStateData.affiliateQt + ';';
				}
				
				dartSrc += 'tile=' + adPos + ';';
				
				if (dartUserData != null) {
					dartSrc += 'u=' + dartUserData + ';';
				}
				
				dartSrc += 'ord=' + dartOrd + '?';
				
