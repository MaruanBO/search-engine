<div id="fb-root" style="float: left;"></div>
<script type="text/javascript">
	var userIsAuthenticated =  false; 	 var currentUserId = null; 	
	var postAuthenticationTask = null;
	
	function afterLogin() {
		if (postAuthenticationTask != null) {
			var task = postAuthenticationTask;
			postAuthenticationTask = null;
			task();
		}
	}
	
	window.fbAsyncInit = function() {
		FB.init({appId: '106478212752700', status: true, cookie: true,
			xfbml: true});

		FB.Event.subscribe('auth.authResponseChange', function(response) {
			if(response.status === 'connected') {
				if (!userIsAuthenticated) {
					faceBookAPI();
				}

				require(["jquery"], function() {
					$(".logout").click(function() {
						FB.logout(function(response) {});
					});
				});

			} else if (response.status === 'not authorized') {
				FB.login( function(response) {
				}, { scope : 'email' });
			} else {
				FB.login( function(response) {
				}, { scope : 'email' });
			}
		});

		FB.Event.subscribe('edge.create', function(response) {
			s.events='event29';
			s.tl(true,'o','Facebook Like');
		});
	};

	// Asynchronously Load Facebook Like Social Plugin
	//    (function() {
	//        var po = document.createElement('script');
	//        po.type = 'text/javascript';
	//        po.async = true;
	//        po.src = 'http://connect.facebook.net/en_US/all.js#xfbml=1';
	//        var s = document.getElementsByTagName('script')[0];
	//        s.parentNode.insertBefore(po, s);
	//    })();
	
	(function(d) {
		var js, id = 'facebook-jssdk';
		var ref = d.getElementsByTagName('script')[0];
		if (d.getElementById(id)) { return; }
		js = d.createElement('script');
		js.id = id;
		js.async = true;
		js.src = "http://connect.facebook.net/en_US/all.js";
		ref.parentNode.insertBefore(js, ref);
	} (document));

// Called after FB has connected with our app
function faceBookAPI() {
	FB.api('/me', function(response) {
		var facebookLoginReq = {};
		
	    // User's FB ID
	    facebookLoginReq.fbUserID = response.id;
	    // User's first name
	    facebookLoginReq.firstName = response.first_name;
	
	    // User's last name
	    facebookLoginReq.lastName = response.last_name;
	
	    // Gender
	    facebookLoginReq.gender = response.gender;
	
	    // Birthdate - Date string in MM/DD/YYYY format
	    // Needs user_birthday permission
	    facebookLoginReq.birthday = response.birthday;
	
	    // Email - needs extended permission (email)
	    facebookLoginReq.email = response.email;
	
	    facebookLoginReq.fbToken = FB.getAuthResponse().accessToken;
	    
	    facebookLoginReq.fbTokenExpires = FB.getAuthResponse().expiresIn;
	    
	    if(!response.email || response.email.length === 0){
			return;
	    } else {
			require(["jquery", "json2"], function() {
		     	jQuery.ajax({
		     		type: "POST",
		     		url: "https://www.chacha.com/ajax/facebookLogin",
		     		dataType: 'jsonp',
		     		data: { jsonInput : JSON.stringify(facebookLoginReq) },
		     		success: function(data, textStatus, xhr) {
		     			if (data.success) {
		     				userIsAuthenticated = true;
		     				currentUserId = data.userId;
		     				$("#userAccountStatus .account-login").hide();
		     				$("#userAccountStatus .account-username .username").text(data.nickname);
		     				$("#userAccountStatus .account-username").show();
		     						     				checkForAnswerNotifications(0);
		     						     				
		     				afterLogin();
		     				
		     				if (location.pathname.indexOf("/askChaCha") == 0) {
		     					location.reload(true);
		     				}
		     				
		     			} else {
							$("#userMessageIcon").addClass("icon-warning-sign");
		     				$("#userMessageContent").html(data.errorMsg);
							$("#userMessage").addClass("alert-error").toggle();
		     			}
		
		     			$("#myModalLogin").modal("hide");
		     			$("#myModalCreate").modal("hide");
	     			}
	     		});
			});
		}
	});
}
</script>
