	require(["jquery", "jquery-ui", "jquery.validate", "bootstrap"], function(){
		jQuery.validator.addMethod("regex", function(value, element, param) {return this.optional(element) || param.test(value);}, "Invalid format");
		
		$('#signupForm').validate({
			rules: {
				emailSignup: {
					required: true,
					email: true,
					maxlength: 255
				},
				firstName: {
					rangelength: [2, 50],
					regex: /^[A-z]+$/
				},
				lastName: {
					rangelength: [2, 50],
					regex: /^[\sA-z'-]+$/
					
				},
				passwordSignup: {
					minlength: 6
				},
				passwordSignupConfirm: {
					equalTo: "#passwordSignup"
				}
			},
			messages: {
				emailSignup: {
					required: "Please enter your email address",
					email: "Please enter a valid email address"
				},
				passwordSignupConfirm: {
					equalTo: "Passwords must match"
				},
				firstName: {
					regex: "Only letters are allowed"
				},
				lastName: {
					regex: "Only letters, apostrophes and hyphens are allowed"
				}
			}
		});
    });
