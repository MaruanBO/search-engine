                        var adtag = get_aamCookie('aam_adtag');
                        var uuid = get_aamCookie('aam_uuid');
                        adtag = typeof adtag != 'undefined' ? adtag : '';
                        uuid = typeof uuid != 'undefined' ? uuid : '';

