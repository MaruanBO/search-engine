			npReady(function () {
				
				var iDelay,
					oBox = npJ('#npHoverBox4765'),
					oImg = oBox.find('.npHoverImg');
				
				oBox
				
				// mouse-over behaviour
				.find('li a').live('mouseenter', function (e) {
					var oA = npJ(this),
						sHref = oA.attr('href'),
						sSrc = oA.parents('li').attr('data-src-fullsize');
					if (oImg.attr('src') !== sSrc) {
						if (iDelay) {
							clearTimeout(iDelay);
						}
						oBox.find('.npHeading a').attr('href', sHref).text(oA.text().trim());
						oImg.attr('src', sSrc).load(function () {
							clearTimeout(iDelay);
							oImg.parent('a').removeClass('npLoading');
						}).parent('a').attr('href', sHref);
						iDelay = setTimeout(function () {
							oImg.parent('a').addClass('npLoading');
						}, 250);
					}
				});
			});
