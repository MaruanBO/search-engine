    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_ALL_GLOBAL_RIGHTRAIL_A !== 'undefined') && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById("AMS_ALL_GLOBAL_RIGHTRAIL_A");
        div.innerHTML = "";

        
        jQuery("#AMS_ALL_GLOBAL_RIGHTRAIL_A").html(pageAds.AMS_ALL_GLOBAL_RIGHTRAIL_A.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
        jQuery(div).css({
            visibility: 'visible'
        });
    }

    
});

</script>



        </div>
    


        

