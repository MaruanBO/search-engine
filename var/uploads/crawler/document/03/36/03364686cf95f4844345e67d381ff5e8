                //<!--
                // Log in functions
                function isLoggedIn() {
                        var cookie_name="amg_user";
                        var nameEQ = cookie_name + "=";
                        var ca = document.cookie.split(';');
                        //alert("ca "+ca)
                        for (var i=0; i<ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0)==' ') c = c.substring(1,c.length);
                            if (c.indexOf(nameEQ) == 0) return true;
                        }
                        return false;
                }

                // See if user logged in from facebook
                function isLoggedInFromFacebook() {
                        var cookie_name="1af287f0b09203a571ddc7c0c4b2218d_session_key";
                        var nameEQ = cookie_name + "=";
                        var ca = document.cookie.split(';');
                        for (var i=0; i<ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0)==' ') c = c.substring(1,c.length);
                            if (c.indexOf(nameEQ) == 0) return true;
                        }
                        return false;
                }

                function getCookie(cname){
                    var C = document.cookie.split(';'), c;
                    while(c = C.pop()) {
                        c = c.replace(/^\s*/, '');
                        if (c.indexOf(cname+'=') == 0){
                            return c.substring(cname.length + 1, c.length);
                        }
                    }
                    return false;
                }

                function username(){
                    var aur = getCookie('amg_user_record');
                    var username = aur ? aur.replace(/.*username=([^|]+).*/,"$1") : false;
                    // check length.
                    var nameLen = username.length;
                    if (nameLen > 25) {
                      return username.substring(0, length+22) + "...";
                    }
                    return username;
                }
