<script>
    (function() { // Everything is wrapped in an anonymous function to avoid polluting the global space
        jQuery(document).ready(function() {
            var email = "";
            if (AARP && AARP.User && AARP.User.email) {
                email = AARP.User.email;
            } 
            
            if (email == "")
            {
                var emailCookie = Cookies.get("email");
                if (emailCookie)
                {
                    email = emailCookie;
                    if (AARP.User)
                    {
                        AARP.User.isPartiallyLoggedIn = true;
                        AARP.User.email = email;
                    }
                }
                
            }
 
            var postData = {
                numberOfOffers: 4,
                interactionPoint: "SB-Homepage",
                interactionChannel: "AARP.org",
                referUrl: document.referrer,
                email: email,
                channel: "Homepage",
                subChannel: "Homepage"
            };   
            
            var mSB = 0;     
            
            AARP.IMAX.setupOffers(
                    "http://services.aarp.org/bestoffer/getIMAXOffers", 
                    postData, 
                    showOffers1424379208026, 
                    offerError1424379208026, 
                    0,
                    mSB
            );
                
            var targetDiv = jQuery("#servdisc1424379208026");
            targetDiv.show();            
        });
        var defaults1424379208026 = [
            "/etc/aarp/offer/discountsAndBenefits/hampton-inn1","/etc/aarp/offer/discountsAndBenefits/smjoinorenew1","/etc/aarp/offer/discountsAndBenefits/regal1","/etc/aarp/offer/discountsAndBenefits/brain-health-center1"
        ];
        function defaultOffers1424379208026() {
            AARP.IMAX.useDefaultOffers(defaults1424379208026.splice(0,4), showOffers1424379208026, offerError1424379208026);
        }
        function offerError1424379208026(state, order) {
            if (defaults1424379208026.length > 0) {
                if (!state) {
                    defaultOffers1424379208026();
                } else if (defaults1424379208026.length > 0) {
                    AARP.IMAX.queueOfferLookup(state, order, defaults1424379208026.splice(0,1)[0], 3, 3);
                }
            } else {
                // Not even default offers can be shown -- remove the loading message at least
                var targetDiv = jQuery("#servdisc1424379208026");
                targetDiv.empty();
            }
        }

        function showOffers1424379208026(offers) {
            var targetDiv = jQuery("#servdisc1424379208026");
            var templateDiv = jQuery("#servdisctemplate1424379208026");
            var isSkyboxIMAX = jQuery("#servdisc1424379208026").parents().hasClass('skyboxSD');

            targetDiv.empty();
            for (var i = 0; i < offers.length; i++) {
                var offer = offers[i];
                // Skip the property if it is not an offer node
                if (!offer["jcr:primaryType"] || offer["jcr:primaryType"] !== "cq:Page") {
                    continue;
                }
                AARP.IMAX.postImpression(offer.treatment, "http://services.aarp.org/bestoffer/postIMAXEvent");
                // Grab the offer content and show it
                var content = offer["jcr:content"];
                // Pull out offer properties, taking defaults as necessary
                var title = content["jcr:title"] || prop;
                var linkImg = content.imageReference || content.imageBrowse || "";
                var linkUrl = content.linkUrl || "#";
                var linkUrlSplit = linkUrl.split("?");
                var linkUrlAppendCharacter = '';
                var linkText = content.text || "There is no text";
                var linkTarget = ((content.newWindow || "current") === "new") ? "_blank" : "_top";
                var imageCaption = content.imageCaption || false;
                var imageCredit = content.imageCredit || false;
                var position = content.imagePosition || "left" || "left";
                var entry = templateDiv.find("div." + position).clone();
                // TODO: add first-node
                var credit = entry.find(".credit");
                var caption = entry.find(".caption");
                
                
                if(isSkyboxIMAX == true){       
                    offerTitle = title;                    	
                    title = content.skyboxtitle || "There is no title";                
                    linkText = content.skyboxtitle || "There is no text";
                    var skyboxText = content.skyboxtext || "There is no title";
                    linkImg = content.skyboximageReference || content.skyboximageBrowse || "";
                    linkUrl = content.skyboxlinkUrl || "#";
                    entry.find(".offersTextContainer").append("<p>" + skyboxText + "</p>");    
                    var linkUrlAppendCharacter = '?';

                   if(linkUrl != '#'){  
                       var linkUrlSplit = linkUrl.split("?");
                       if (linkUrlSplit[1]){
                           linkUrlAppendCharacter = '&';
                       };
                   }                
                }           
                   var trackingType1 = "intcmp";                  
                   var trackingCode = "IMAX-SB-HP";
                   var trackingCodetitle = offerTitle.replace(/ /g, '_');
              
                   if(trackingType1 && trackingCode){
                       linkUrl = linkUrl + linkUrlAppendCharacter + trackingType1 + "=" + trackingCode + "-" + trackingCodetitle;
                   }    
                // Handle different variations of image with/without caption and/or credits
                if (linkImg) {
                	// Set JSONP calls to pull image from live Domain
                	var environment = location.host;
                	var linkImgType = linkImg.split(".");
                	var linkRevNumber = ".20539";
                		linkRevNumber = linkRevNumber.split(".");
                	linkImg =  (environment.match(/cms|cms-pre|cms-s|cms-d|local.aarp.org:8080|(local|localhost).aarp.org:4502/gi)) ? linkImg : "//" + "cdn.aarp.net" + linkImgType[0] + ".imgcache.rev" + linkRevNumber[1] + ".web.80.60." + linkImgType[1];                	
                	entry.find(".offerImage").prop("src", linkImg);
                    entry.find(".offerImage").prop("alt", title);
                    entry.find(".offerImage").prop("title", title);
                    if (imageCredit) {
                        entry.find(".credit").html("&mdash; " + imageCredit);
                        if (imageCaption) {
                            caption.text(imageCaption);
                            caption.append(credit);
                        } else {
                            caption.remove();
                        }
                    } else {
                        credit.remove();
                        caption.remove();
                    }
                } else {
                    entry.find(".offerImage").remove();
                    credit.remove();
                    caption.remove();
                }

                entry.find("a").prop("href", linkUrl);
                entry.find("a").prop("target", linkTarget);
                entry.find("a").prop("treatment", offer.treatment);
                entry.find("a").attr("data-treatment", offer.treatment);

                entry.find("a").mousedown(function() {
                    AARP.IMAX.trackClick(jQuery(this).prop("treatment"), "http://services.aarp.org/bestoffer/postIMAXEvent");
                });
                entry.find(".offerText").text(linkText);
                entry.appendTo(targetDiv);
            }
                // Provide more height to view IMAX offers in Author mode
                if (!AARP.modePreview && !AARP.modePublish) {
                        $j("#home.cq-wcm-edit .skyboxContent.open").css('height','87px');
                }               
                // Load titles when Skybox is closed state
                if (($j.cookie("skybox")) && (AARP.modePublish)) {
                    if ($j.cookie("skybox") == "closed") {
                        $j(".skyboxContent .skyboxIMAXoffers .offersTextContainer a").clone().appendTo($j(".skyboxContent .titles"));
                    } 
                } else {
                    if ($j(".skyboxContent .expand").hasClass("closed")) {
                        $j(".skyboxContent .skyboxIMAXoffers .offersTextContainer a").clone().appendTo($j(".skyboxContent .titles"));
                    } 
                }
            }
    })(); // Everything is wrapped in an anonymous function to avoid polluting the global space
 
    // Tracking fix for minimized Skybox 
	$j( ".skyboxContent" ).on( "mousedown", ".titles a", function() {
	    var postIMAXEventUrl = $j('input[name="postIMAXEventUrl"]').val();
	    var treatmentCode = $j(this).data( "treatment" );
	    AARP.IMAX.trackClick(treatmentCode, postIMAXEventUrl);
	});
</script></div>

    </div>
    
 </div>

