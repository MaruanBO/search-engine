<script type="text/javascript">
  (function() {
    var buster,
      gumshoe = document.createElement('script'),
      firstScript = document.getElementsByTagName('script')[0],
      now = new Date(),
      month = now.getMonth(),
      day = now.getDate(),
      year = now.getFullYear(),
      hours = now.getHours(),
      minutes = now.getMinutes(),
      buster = '' + month + day + year + hours;

      // bust the cache in five minute intervals.
      // interval to be increased to 1 hour+ upon Gumshoe 1.x.x
      buster += (minutes - (minutes % 5));

      gumshoe.type = 'text/javascript';
      gumshoe.async = true;
      gumshoe.src = '//gilt-assets.s3.amazonaws.com/s/gumshoe/gumshoe.js?=' + buster;

      firstScript.parentNode.insertBefore(gumshoe, firstScript);
  })();
</script>


