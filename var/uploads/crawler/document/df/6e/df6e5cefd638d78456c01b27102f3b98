  <script type="text/javascript"><!--

				<!--
					if (!getCookie('languageset')) {
						setCookie("homeReferrer",document.referrer,1);
						window.location = "/cpo/mc/languageswitcher.jsf";
					}else{
						setCookie("homeReferrer","",1);
					}
					
					// uncomment if you want splash
					/*if (!getCookie('titanic')) {
						setCookie("homeReferrer",document.referrer,1);
						window.location = "/cpo/mc/splash.jsf";
					}else{
						setCookie("homeReferrer","",1);
					}*/
					
					function goToTrack(frmSegmentTrack) {
						// Boolean to determine if radio button was selected.
						var selected = false;
						// Radio button selection (Personal or Business).
						var choice = false;
						// Loop through radio buttons
						for (counter = 0; counter < frmSegmentTrack.trackingType.length; counter++) {
							// Check if radio button was selected.
							if (frmSegmentTrack.trackingType[counter].checked) {
								selected = true;
								// Check which button was selected.
								if (counter == 0) {
									choice = "personal";
								} else {
									choice = "business";
								}
							}
						}
						
						// Display correct error message is user has not specified a tracking number or if neither radio button was selected or both.
						if (frmSegmentTrack.trackingNumber.value == "" && !selected) {
							$("#errorMessage1").removeClass("hidden");
							$("#errorMessage2").addClass("hidden");
							$("#errorMessage3").addClass("hidden");
							$(".widget .contents").height("100%").setAllToMaxHeight();
							return false;
						} else if (!selected) {
							$("#errorMessage2").removeClass("hidden");
							$("#errorMessage1").addClass("hidden");
							$("#errorMessage3").addClass("hidden");
							$(".widget .contents").height("100%").setAllToMaxHeight();
							return false;
						} else if (frmSegmentTrack.trackingNumber.value == "") {
							$("#errorMessage1").addClass("hidden");
							$("#errorMessage2").addClass("hidden");
							$("#errorMessage3").removeClass("hidden");
							$(".widget .contents").height("100%").setAllToMaxHeight();
							return false;
						} 
						
						// Determine if user selected personal or business.
						var url = "";
						if (choice == "personal") {
							url = "http://" + window.location.hostname + ":" + window.location.port + "//cpotools/apps/track/personal/findByTrackNumber";
						} else {
							url = "http://" + window.location.hostname + ":" + window.location.port + "//cpotools/apps/track/personal/findByTrackNumber";
						}
						// Redirect user to personal or business tracking page.
						document.frmSegmentTrack.action = url;
						document.frmSegmentTrack.submit();
					}	
				//-->
                
//--></script>
