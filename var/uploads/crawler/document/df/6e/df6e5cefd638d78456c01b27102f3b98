			function refLink(val, object) {
				var obj = new Object();
				obj = object;
				
				for (var i in obj)
					{
  						if (i.toString() == val)
    					return obj[i]
					}
			}
			
			$(function(){
				// Dialog Links
				$("#apple_h, #apple_tf, #apple_f, #apple_fpcf, #apple_farf, #apple_fpof").click(function(){
