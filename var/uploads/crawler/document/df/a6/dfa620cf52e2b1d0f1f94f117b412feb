<script>

 $().ready(function () {
    var ckim = new ExciteYDNJsFlat.Ydn({
                
                'source_id': 'excite_jp_ct_ckim', //ソースIDを指定
                'im_type' : 'ckim',
                'type': 'blog_customkw', //必要な場合
                
                'display': {'ex_ydn_ckim': 1}
            }
    );
    ckim.load();
    $('#ex_ydn_ckim').ajaxComplete(function(){
        if ($('p', this).length < 1) return;
        $('.desc', this).insertBefore($('.label', this));
        $('.label', this).children().text('[PR]').css({'margin-left': "10px"});
        $(this).css("display", "block");
    });
});

</script>
