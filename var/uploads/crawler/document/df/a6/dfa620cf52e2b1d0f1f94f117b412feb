    $('input[name="t"]:radio').change(function () {
      ga('send', 'event', 'BlogSearch', 'change', $(this).parent().text());
      $('#search_type label').removeClass("selected");
      $(this).parents("label").addClass("selected");
    });
    $('#search').submit(function () {
      ga('send', 'event', 'BlogSearch', 'submit', $('#search_text').val());
    });
