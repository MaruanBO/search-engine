$(function($){
    $('#exHeaderSbm, #exFooterSbm').click(function() {
        $('#exHeaderSearchWord, #exFooterSearchWord').each(function(){
            var str=$(this).val();
            if(str=='ウェブ検索'){
                $(this).val('');
            }
        });
        return true;
    });
});
</script>

