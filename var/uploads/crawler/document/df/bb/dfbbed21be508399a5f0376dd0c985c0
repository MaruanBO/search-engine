<script type="text/javascript" language="javascript">

var images='/Publish/Content/image/gif/GWMOL/ME_home_600-pluses-offer-banner.gif,/Publish/Content/image/gif/GWMOL/ME-home-trading-price-competitive-banner.gif,/Publish/Content/image/gif/GWMOL/ME-home-zero-dollar-trades-600-promotion-banner.gif,/Publish/Content/image/jpeg/GWMOL/me_hm_outlook_arn9ljsl.jpeg,/Publish/Content/image/gif/GWMOL/ME_PreferredRewards_HP_Banner.gif';	
var slideCount =0;
function loadHeroArea() {
 	
	var slideShow = setInterval(showNextSlide, 6000);
slideCount = $(".hero-area .ha-slide").length - 1;

	$(".ha-slide-wrapper, .ha-pager-wrap").mouseenter(function () {
		clearInterval(slideShow);
	});

$(".ha-slide-wrapper, .ha-pager-wrap").mouseleave(function () {
		slideShow = setInterval(showNextSlide, 6000);
	});
}

$(".ha-pager a").click(function () {
currentSlide = parseInt($(this).index());
if (currentSlide > slideCount)
currentSlide = slideCount;
showSlide();
}); 

function showNextSlide() {
if (currentSlide >= slideCount)
currentSlide = 0;
else
currentSlide++;
showSlide();
}

var currentSlide = 0;
function showSlide() {
$(".hero-area .ha-pager a").removeClass("hap-active");
$('.hero-area .ha-slide-wrapper .ha-slide').removeClass("has-active");


$(".hero-area .ha-pager a").eq(currentSlide).addClass("hap-active");
$('.hero-area .ha-slide-wrapper .ha-slide').eq(currentSlide).addClass("has-active");
}
$(document).ready(function() {
var preloads = images.split(',');

    $(preloads).each(function () {
        $('<img />').attr('src',this).appendTo('body').css('display','none').height(1).width(1);
    });

 loadHeroArea();
  });

</script>

   
