<script type="text/javascript">
$(document).ready(function(){

$("#jq_DrawerHandle").click(function () {
		$("#jq_DrawerHandle").toggleClass("is-open").toggleClass("is-close");
		$(".drawer .content").animate({
			height: 'toggle',
			opacity: 'toggle'
		}, 500);
	});

	$(".drawer .img-tags .tag").mouseenter(function () {
    var $myDiv = $(this).find('.tag-rollver');
    if ( $myDiv.length > 0){
           $(this).find("h4, img").hide(); //.fadeOut('fast');
	          $myDiv.show(); //.fadeIn('fast');
    }
   }).mouseleave(function () {
		$(this).find("h4, img").show();
		$(this).find(".tag-rollver").hide();
	});
});
</script>


