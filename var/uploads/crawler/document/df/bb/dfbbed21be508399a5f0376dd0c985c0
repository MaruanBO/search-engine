//<![CDATA[
var hns_target = '';
$(function() {
$(document).click(function () {

     
	if (hns_target != 'helpAndSupport' && $(".helpAndSupport").css("display") === "block") 
               
	{
  
		$(".helpAndSupport").hide();
	
	}
	hns_target = '';
});
	$("#jq_helpAndSupport").click(function (e) {
		e.stopPropagation();
		
	   var leftpos =  $(this).position().left;
       var toppos =  $(this).position().top;
	   var totalwidthofbubble = ($(".helpAndSupport").css("width").replace("px", ""));
	   var percentage = 80;
	   var widthpercent = totalwidthofbubble * percentage;
	   var totpercentage = widthpercent / 100 ;
	   
       $('.helpAndSupport').css({'top':toppos+36,'left':leftpos-totpercentage}).toggle();
	 
		
});
	
		

	$(".helpAndSupport .iconClosePopup").click(function () {
		$(".helpAndSupport").hide();
	});

	$(".helpAndSupport").click(function () {
		hns_target = 'helpAndSupport';
	});
	
	

 




	
});

//]]>
</script>


