            $(".simple-cycle-6").each(function(k, sc) {
                var $sc = $(sc);
                $sc.children(".flow-slider").FlowSlider({
                    marginStart: 0,
                    marginEnd: 0,
                    infinite: true,
                    startPosition: 0.0,
                    position: 0.0,
                    animationOptions: {
                        snap: true
