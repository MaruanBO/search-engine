			function getMissedADayDate(){
				var returnDate;
				var yesterday = new Date().add(-1).day();
				returnDate = yesterday.toString("yyyyMMdd");
				return returnDate;
			}
			function setTheTime(){
				var now = new Date();
				var hours = now.getHours();
				if(hours == 0){
					hours = 12;
				}
				if(hours > 12){
					hours = hours - 12;
				}
				var dateTimeString = now.toString("dddd, MMMM d, yyyy - ") + hours + ":" + now.toString("mm tt");
				$("#current_time").html(dateTimeString);
			}
			function Checkage(theForm) {
				if (theForm.age[0].selected==true) {
					alert('COPA prohibits user registration by children below the age of 13.');
					document.location.href = "/under13";
					return false;
				}
			}
