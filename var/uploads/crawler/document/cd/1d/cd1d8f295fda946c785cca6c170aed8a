<script type="text/javascript">

function slideshow_left(idx_in) {
	var next = $('.slide').eq(idx_in);
	if(next.hasClass('current') || $('.slide.lock').length > 0) {
		return false;
	}
	var current = $('.slide.current');
	current.addClass('lock')
		.animate({
			marginLeft: '-660px',
			marginRight: '660px'
		}, 1000, function() {
		current.removeClass('lock');
	});
	next.css('margin-left','660px')
		.css('margin-right','-660px')
		.animate({
			marginLeft: '0px',
			marginRight: '0px'
		}, 1000, function() {
			current.removeClass('current');
			next.removeClass('next').addClass('current');
			$('#dots a').removeClass('active');
			$('#dots a').eq(idx_in).addClass('active');
		});
}
function slideshow_right(idx_in) {
	var next = $('.slide').eq(idx_in);
	if(next.hasClass('current') || $('.slide.lock').length > 0) {
		return false;
	}
	var current = $('.slide.current');
	current.addClass('lock')
		.animate({
			marginLeft: '660px',
			marginRight: '-660px'
		}, 1000, function() {
		current.removeClass('lock');
	});
	next.css('margin-left','-660px')
		.css('margin-right','660px')
		.animate({
			marginLeft: '0px',
			marginRight: '0px'
		}, 1000, function() {
			current.removeClass('current');
			next.removeClass('next').addClass('current');
			$('#dots a').removeClass('active');
			$('#dots a').eq(idx_in).addClass('active');
		});
}
$('#dot_1').click(function() {
	slideshow_right(0);
	return false;
});
$('#dot_2').click(function() {
	if($('#dot_1').hasClass('active')) {
		slideshow_left(1);
	} else {
		slideshow_right(1);
	}
	return false;
});
$('#dot_3').click(function() {
	if($(this).hasClass('active'))
		return false;
	slideshow_left(2);
	return false;
});
$(document).ready(function() {
	ich_slideshow_secs = 0;
	ich_slideshow_increment = function() {
		ich_slideshow_secs++;
		if(ich_slideshow_secs > 5) {
			if($('#dot_1').hasClass('active')) {
				slideshow_left(1);
			} else if($('#dot_2').hasClass('active')) {
				slideshow_left(2);
			} else {
				slideshow_left(0);
			}
			ich_slideshow_secs = 0;
		}
	}
	var ich_slideshow_interval = setInterval("ich_slideshow_increment()", 1000);
});
</script>



