    window.cycleClients = function (dir) {
      dir = dir || 'next';
    
      var ul = $('.customers ul'),
          left = (dir == 'next')? parseInt(ul.css('left')) + 1 : parseInt(ul.css('left')),
          next = (left - 1) >= -( (475 * (ul.children().length - 1)) - 475 ),
          prev = left < -1;
    
      if(left > -475){
        left = 0;
      }
    
      if(dir == 'next' && !next && left != 0){
        left = 476;
      }
    
      if(dir == 'prev' && !prev){
        left = -( (475 * (ul.children().length - 1)) + 1 );
      }
    
      var newPosition = (dir == 'next')? left - 476 : left + 475 ;
    
      ul.css('left', newPosition);
    }
  //]]>
</script>

