<script type="text/javascript">
  // KM Ad Campaign Hit
  try {
    var argsKM = window.location.search;
    var splitArgsKM = argsKM.substring(1).split("&");
    var urlKMObj = {};
    for (var i in splitArgsKM) {
      var argNVPair = splitArgsKM[i].split("=");
      urlKMObj[argNVPair[0]] = argNVPair[1]; 
    }
    if("slintcid" in urlKMObj) {
      if(console){
        console.log("urlKMObj: ", urlKMObj);
      };
      jQuery(function(){
        var adCampaignHit = newKissSl(kisssl);
        adCampaignHit.init({
           "SiteCatalyst": (typeof s !== 'undefined')?s:{},
           callback: function(){
              var globalVars = this.userDataObj;                    
              globalVars['Campaign Source'] = urlKMObj.slintcid;
              globalVars['Campaign Name'] = urlKMObj.slcmpname;
              globalVars['Marketing Key'] = urlKMObj.mkt;
              if(console){
                console.log("'Ad Campaign Hit' , globalVars: ", globalVars);
              };
              _kmq.push(['record', 'Ad Campaign Hit', globalVars]);
           }
        });
      });
     };
  } catch(err) {
      if(console) {
        console.log("Issue with KM tracking for 'Ad Campaign Hit': ", err);
      };
  };
</script>


    
    



<script>
var currPath = window.location.pathname;

var adService = new(function(){
    var newAds = []
    var site_page;
    var query;
    var userType;
    var initialLoad = false;
    if(!userInfo){
        var userInfo = jQuery.cookie('USERINFO');
    }
    if(userInfo){
        userInfoParsed = JSON.parse(userInfo)
        userType = userInfoParsed.user[0].usType
    }else{
        userType = "free"
    }
    return{
        loadAds :function(passedAdVars){
            query = passedAdVars.keywords+','+userType;
            site_page = passedAdVars.site_page

            // find ads on the page
            var newAds = [];
            jQuery(".advertisment, .advertisement, .Advertisement").each(function(index, value){
                var width = jQuery(this).width();
                var height = jQuery(this).height();
                var position = jQuery(this).attr('id').replace('oas_',"");
                newAds.push({
                    "position":position,
                    "width":width,
                    "height":height
                })
            })
            if(newAds.length == 0){
                if(window.console){
                    console.log('No ad positions found, ad system setup correctly')
                }
            }

            // async script tag creation
            var randNum = Math.random();
            oas_tag = {};
            oas_tag.url = 'oascentral.surfline.com';
            var keywords;
            var campaign;


            // ad size return is overrided so that the ad team can specify the ad sizes.
            // However if the ad width and height are omitted from the setup on the ad server, then it will default to these sizes
            oas_tag.sizes = function(){
                var numOfAds = newAds.length;
                for (var i = 0; i < numOfAds; i++) {
                    if(window.console){
                        console.log('AD Requested:',newAds[i].position,'|',newAds[i].width,newAds[i].height)
                    }
                    oas_tag.definePos(newAds[i].position,[newAds[i].width,newAds[i].height]);
                }
            };
            oas_tag.site_page = site_page;
            oas_tag.query = query;
            oas_tag.analytics = true;
            oas_tag.allowSizeOverride = true;
            oas_tag.callbackHandler = function(){
                initialLoad = true;
                oas_tag.addHandler('callbackId', function(data){
                    jQuery.each( data, function( index, value ){
                        var sitepageConsoleDisplay = "unknown"
                        var keywords = "unknown"
                        try{
                            sitepageConsoleDisplay = value.clickUrl.replace('http://oascentral.surfline.com/5c/','')
                            sitepageConsoleDisplay = sitepageConsoleDisplay.split("/L")
                            sitepageConsoleDisplay = sitepageConsoleDisplay[0]

                            keywords = value.clickUrl.split("displayAds")
                            keywords = keywords[1]

                        }catch(err){
                            sitepageConsoleDisplay = "unknown"
                            sitepageConsoleDisplay = "unknown"
                        }

                        // allows the homepage to have even padding if the bottom1 unit is delivered
                        if(sitepageConsoleDisplay == "www.surfline.com/home/home2" && value.position == "Bottom1" && value.width > 0){
                            jQuery('#oas_Bottom1').css('margin-top','10px')
                            if(window.console){
                                console.log('Ad: homepage bottom1 margin top added')
                            }
                        }
                        if(window.console){
                            console.log("AD Served:", value.position, '|',value.width,'x',value.height,'|',sitepageConsoleDisplay,'|',keywords)
                            console.log(value);
                        }
                    });
                });
            };

            // async script tag creation
            (function(){
                oas_tag.version ='1';
                var oas = document.createElement('script'),
                protocol = 'https:' == document.location.protocol?'https://':'http://',
                node = document.getElementsByTagName('script')[0];
                oas.type = 'text/javascript';
                oas.async = true;
                oas.id = "oasscript";
                oas.src = protocol + oas_tag.url + '/om/' + oas_tag.version + '.js';
                document.getElementsByTagName("head")[0].appendChild(oas)
            })();
        },
        reloadAds : function(subjectKey){


            if(currPath.indexOf('photos/') != -1 && currPath.indexOf('photosdev/') != -1 && currPath.indexOf('photosqa/') != -1){
                if(window.console){
                    console.log('reload ads called')
                }
                oas_tag.reloadAds();


            // photos system load and reload
            }else{
                if(window.console){
                    console.log('Photos FLIPADS() called adService')
                }

                // check if middle2 is present if not create it where it can be replaced
                var middle2Exists = document.getElementById("oas_Middle2");
                if(!middle2Exists){
                    if(window.console){
                        console.log('Middle2 does not exist')
                    }

                    // #slis-canvas so it gets removed on view change
                    jQuery('#slis-canvas').append("<div class='advertisement placeholderMiddle2' id='oas_Middle2'></div>")
                }


                // declare sitepage
                var site_page = "www.surfline.com/photos"
                var hash = window.location.hash

                if(hash){
                    if(hash.indexOf('#!/uploader') != -1 || jQuery('#slisImageDetail').width() != null ){
                         site_page = "www.surfline.com/photos-middle2"
                    }
                }

//slisImageDetail
//slisSlideshow

                // initial loading call
                if(initialLoad == true){
                    oas_tag.site_page = '';
                        if(window.console){
                            console.log('%c reloading ads: '+site_page, 'background:black; color:#FFFFFF');
                        }

                    oas_tag.site_page = site_page
                    oas_tag.reloadAds();
                }else{
                    var adVars = {
                        "keywords": "",
                        "site_page": site_page
                    }
                    console.log('%c loading ads: '+site_page, 'background:black; color:#FFFFFF');

                    adService.loadAds(adVars) // sets initialLoad = true on callback;

                } // end load/reload if


            } // end photos system calls
        }
    }
});


// photos system calls adService.loadAds(adVars) after its finished loading
if(currPath.indexOf('photos/') == -1 && currPath.indexOf('photosdev/') == -1 && currPath.indexOf('photosqa/') == -1){
    if(typeof adVars != 'undefined'){
        if(window.console){
            console.log('load ads')
        }
        adService.loadAds(adVars)
    }else{
        if(window.console){
            console.log('OAS ad variables have not been setup on this page')
        }
    }
}
</script>




    
    
