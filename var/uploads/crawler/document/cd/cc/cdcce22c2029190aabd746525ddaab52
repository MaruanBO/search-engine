<script>
var promoInterval;
function toppromoswitch(){
	console.log('ROTATE')
	var current = (jQuery('#toppromo ul li.activepromo')?jQuery('#toppromo ul li.activepromo'):jQuery('#toppromo ul li:first'));
	var theNext = ((current.next().length)?((current.next().hasClass('activepromo'))?jQuery('#toppromo ul li:first'):current.next()):
	jQuery('#toppromo ul li:first'));
	theNext.css({opacity: 0.0}).addClass('activepromo').animate({opacity: 1.0},400);
	current.animate({opacity: 0.0},400).removeClass('activepromo');
};
jQuery(function(){
	jQuery('#toppromo').load('/global_includes/header/includes/inc_toppromo.cfm?userType=free #promos',function(){
		jQuery('.toppromoinactive').css({opacity: 0.0});
		promoInterval = setInterval(function(){toppromoswitch()}, 5000);
	});
});
</script>

