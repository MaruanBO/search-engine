<script>
jQuery('.promoboxSlide').click(function(e){
    try{
        e.preventDefault();
        var link = jQuery(this).find(".promoboxSlideLink").attr('href')
        var title = jQuery(this).find("h2").text()
        if(console){
            console.log('promobox: '+title ,link)
        }
        s.tl(this,'o','slhome:promobox:'+title);
        setTimeout(function(){
            window.location.href=link
        },1000)
    }catch(err){
        if(console){
            console.log('error clicking promobox',err)
        }
    }
})
</script>


  


<script type="text/javascript" charset="utf-8">
jQuery.noConflict();
function initPromobox(){
  jQuery('.surflinepromobox').surflinepromobox({
      controlsContainer: ".surfline-container",
      directionNav: true,
    slideshowSpeed: 6000,
      animationDuration: 500,
      slideToStart:0
  });
}
jQuery(function(){
   initPromobox();
});
</script>


