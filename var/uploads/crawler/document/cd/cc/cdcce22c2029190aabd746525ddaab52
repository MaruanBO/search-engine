<script type="text/javascript">
  //
  if (jQuery("#heatofthedayli").hasClass("current") == true || jQuery("#scoringswitchli").hasClass("current") == true) {
      jQuery("#scoringswitchtab").text("Score");
      jQuery("#heatofthedaytab").text("Heat Of The Day");
      jQuery('.tabmodule li:eq(1)').removeClass();
      jQuery('.tabmodule li:eq(2)').removeClass();
      var noCookieCheck = true;
  }
  //
  if (noCookieCheck != true) {
    var selectedTab=getCookie("selectedTab");
    if (selectedTab!=null && selectedTab!=""){
      if(selectedTab == "home-cam-tab") {
        jQuery("#cam-of-day-tab").removeClass("current");
        jQuery("#home-cam-tab").addClass("current");
      }
    }
  }
  loadTab(jQuery('#surfline-home-cam LI.current A'),'#surfline-home-cam-container');

</script>
