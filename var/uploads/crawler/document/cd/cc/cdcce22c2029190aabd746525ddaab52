			// jQuery("#is-mobile").text(window.mobilecheck());

			if(window.mobilecheck()){

				var mobileRedirectCookie = jQuery.cookie("sl_mobile_redirect");

				if(!mobileRedirectCookie){
					jQuery("#mobile-banner").show();

					// non session cookie based
					jQuery("#text-mobile-banner-section, #go-mobile-banner-button").click(function(){
						jQuery.cookie("sl_mobile_redirect",true, { expires : 365, path: '/', domain : 'surfline.com' });
						jQuery("#mobile-banner").remove();
						window.location = mobile_url;
					});

					// session cookie based request from Frank
					jQuery("#remove-mobile-banner-section").click(function(){
						jQuery.cookie("sl_mobile_redirect",false, { expires : 1, path: '/' , domain : 'surfline.com' });
						jQuery("#mobile-banner").remove();
					});

				// redirect to mobile site
				}else if(mobileRedirectCookie == "true"){
					window.location = mobile_url;


				}else{
					/* View full site */
				};

			};

		});

	</script>

