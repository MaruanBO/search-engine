  <script type="text/javascript">
    var photoModSettings = {"rated":"PG"};
  </script>


<script type="text/javascript">
  /* THIS OBJECT IS REUSABLE AS A PLUGIN */
  var hpPhotoMod = {
    init: function( config ) {
      this.template = config.template;
      this.container = config.container;
      this.callback = config.callback;
      this.template_system = config.template_system;
      this.numPhotos = config.number_of_photos_per_source;
      this.urlCore = 'http://photo-service.surfline.com/surfline-image-service/gallery/';
      this.params = '/?sort=DATE&size='+(this.numPhotos*2)+'&range='+config.day_range+'&callback=?';
      this.rated = config.rated;
      if(config.rated == "G" || config.rated == "g"){
        this.urls = config.gRating;
      }else{
        this.urls = config.noRating;
      }
      this.fetch();
      },
    attachTemplate: function() {
      var template = this.template;
      var container = this.container;
      var mainObject = this.mainObject;
      var objectArray = this.objectArray;
      var conditionsObjectArrayProperties = "template";
      for (var key in objectArray[0]) {
        conditionsObjectArrayProperties += ".replace( /{{"+key+"}}/ig, (obj."+key+"!= undefined) ? obj."+key+" : '')";
      }
      var forecastTemplateStack = '';
      for(var x = 0; x < objectArray.length; x++ ) {
        var obj = objectArray[x];
        var path = '/photos/index.cfm#!/image/';
        var galleryContextPath = obj.galleryContextPath;
        var reelArray = galleryContextPath.split ( '/' );
        var galleryType = reelArray[0];
        var galleryPath = reelArray[1];
        var link=path+obj.key;
        var link=link+"?"+this.getReelContext(galleryType,galleryPath);
        forecastTemplateStack += eval(conditionsObjectArrayProperties+".replace(/{{context_aware_link}}/ig,'"+link+"')");
      }
      jQuery('.'+container).append('<div id="'+container+x+'"></div>');
      jQuery('#'+container+x+'').append(forecastTemplateStack);
      this.callback(this.objectArray);
    },
    limitCount: function(){
      var number_of_photos = this.numPhotos;
      var filteredPhotoObjects = this.filteredPhotoObjects;
      var filteredCategoriesAvailable = this.filteredCategoriesAvailable;
      var photoArrayOfObjectsLimited = [];
      var photoGalleryContextPath = [];
      var newTypeFoundAt = [];

      for(var k = 0;k<filteredCategoriesAvailable.length;k++){
        for(var i = 0;i<filteredPhotoObjects.length;i++){
          var thePath = photoGalleryContextPath.indexOf(filteredPhotoObjects[i].galleryContextPath);
          if(filteredCategoriesAvailable[k] == filteredPhotoObjects[i].galleryContextPath){
            if(thePath == -1){newTypeFoundAt.push(i);};
            photoArrayOfObjectsLimited.push(filteredPhotoObjects[i]);
            photoGalleryContextPath.push(filteredPhotoObjects[i].galleryContextPath);
            if(k<filteredCategoriesAvailable.length){
              var limitedArray = [];
              for(var j = 0;j<newTypeFoundAt.length;j++){
                for(var x = 0;x<number_of_photos;x++){
                  limitedArray.push(filteredPhotoObjects[newTypeFoundAt[j]+x]);
                }
              }
            }
          }
        }
      }
      
      if(this.template_system){
        /* TEMPLATE SYSTEM FOR THIS ONE */
        this.objectArray = limitedArray; 
        this.attachTemplate();
      }else {
        /* NO TEMPLATE SYSTEM FOR THIS ONE */
        this.callback(limitedArray,this.rated);
      }
    },
    removeDuplicates: function(){
      var filteredPhotoObjects = [];
      var filteredPhotoObjectsKey = [];
      var filteredCategoriesAvailable = [];
      var photoObjects = this.photoObjects;
      var poserLen = photoObjects.length;
      for(var i = 0;i<photoObjects.length;i++){
        for(var j = 0;j<photoObjects[i].photos.length;j++){
          if(filteredPhotoObjectsKey.indexOf(photoObjects[i].photos[j].key) == -1){
            filteredPhotoObjects.push(photoObjects[i].photos[j]);
            filteredPhotoObjectsKey.push(photoObjects[i].photos[j].key);
            if(filteredCategoriesAvailable.indexOf(photoObjects[i].photos[j].galleryContextPath) == -1){ 
              filteredCategoriesAvailable.push(photoObjects[i].photos[j].galleryContextPath);
            }
          }
          if(i == poserLen-1 && j == photoObjects[i].photos.length-1){
            this.filteredPhotoObjects = filteredPhotoObjects;
            this.filteredCategoriesAvailable = filteredCategoriesAvailable;
            this.limitCount();
          }
        }
      }

    },
    getReelContext: function(type,key){
      var reelTypeVal;
      switch(type){
        case"meta":reelTypeVal="META_SEARCH_GALLERY";break;
        case"location":reelTypeVal="LOCATION_GALLERY";break;
        case"category":reelTypeVal="CATEGORY_GALLERY";break
      }
      return"reelCtx="+encodeURIComponent(JSON.stringify({sortOrder:"DATE",keyChain:type+"/"+key,reelType:reelTypeVal}))
    },
    fetch: function() {
    var self = this;
    var countTo = 0;
    var photoObjectArray = [];
    var urls = this.urls;
    var urlCore = this.urlCore;
    jQuery.each(urls, function (i, url) {
        jQuery.getJSON(urlCore+url+self.params, function (json) {
            countTo++;
            photoObjectArray.push(json);
            if (countTo == urls.length) {
                self.photoObjects = photoObjectArray;
                self.removeDuplicates();
            }
        })
    });
    }
  };

function objectFactory(obj) {
    var o = {};
    for (var prop in obj) {
        o[prop] = obj[prop];
    }
    return o;
};

//IF IE DOESN'T HAVE indexOf
[].indexOf || (Array.prototype.indexOf = function(v,n){
  n = (n==null)?0:n; var m = this.length;
  for(var i = n; i < m; i++)
    if(this[i] == v)
       return i;
  return -1;
});

//IF IE DOESNT'T HAVE JSON
var JSON = JSON || {};
JSON.stringify = JSON.stringify || function(obj) {
    var t = typeof (obj);
    if (t != "object" || obj === null) {
        if (t == "string")
            obj = '"' + obj + '"';
        return String(obj);
    } else {
        var n, v, json = [], arr = (obj && obj.constructor == Array);
        for (n in obj) {
            v = obj[n];
            t = typeof (v);
            if (t == "string")
                v = '"' + v + '"';
            else if (t == "object" && v !== null)
                v = JSON.stringify(v);
            json.push((arr ? "" : '"' + n + '":') + String(v));
        }
        return (arr ? "[" : "{") + String(json) + (arr ? "]" : "}");
    }
};

jQuery(function() {
  var photoMod = objectFactory(hpPhotoMod);
  photoMod.init({
    gRating:['category/boards-and-quivers','category/weather-and-hurricanes'],
    noRating:['category/bikinis','meta/highest-rating','meta/most-views'],
    rated: photoModSettings.rated,
    number_of_photos_per_source: 4,
    day_range: 15,
    template_system: false,
    template:jQuery('#hpmodule').html(), 
    container:'hp-photo-module',
    callback:function (data,rated){
        var bikini = 0,mostViewed = 0,highestRated = 0,boards = 0,weather = 0;
        console.log("THE DATA: ", data);
        for(var i = 0;i<data.length;i++){
          var obj = data[i];
          var path = '/photos/index.cfm#!/image/';
          var galleryContextPath = obj.galleryContextPath;
          var reelArray = galleryContextPath.split ( '/' );
          var galleryType = reelArray[0];
          var galleryPath = reelArray[1];
          var link=path+obj.key;
          // var link=link+"?"+this.getReelContext(galleryType,galleryPath);
          var imgThumb = data[i].previewSrc;
          var type = data[i].galleryContextPath;
          if(rated != "G" || rated != "g"){
            jQuery(".hp-photo-row-1-title").html("<a href='/photos/index.cfm#!/gallery/meta/most-views' onclick=\"s.tl(this,'o','homepage:photos:mostviewed');\">Most Viewed</a>");
            if(type == "category/bikinis" && bikini < 3){bikini++;if(bikini < 2){jQuery(".bikini-1").append("<a href='"+link+"' onclick=\"s.tl(this,'o','homepage:photomod:mostviewed:bikini');\"><img border='0' src='"+imgThumb+"'/></a>");}}
            if(type == "meta/most-views" && mostViewed < 3){
              mostViewed++;
              jQuery(".most-viewed").append("<a href='"+link+"' onclick=\"s.tl(this,'o','homepage:photomod:mostviewed:surf');\"><img border='0' src='"+imgThumb+"'/></a>")}
            jQuery(".hp-photo-row-2-title").html("<a href='/photos/index.cfm#!/gallery/meta/highest-rating' onclick=\"s.tl(this,'o','homepage:photos:highestrated');\">Highest Rated</a>");
            if(type == "category/bikinis" && bikini == 2 ){
              bikini++;
              jQuery(".bikini-2").append("<a href='"+link+"'  onclick=\"s.tl(this,'o','homepage:photomod:mostviewed:bikini');\"><img border='0' src='"+imgThumb+"'/></a>");}
            }
            if(type == "meta/highest-rating" && highestRated < 3){
              highestRated++;
              jQuery(".highest-rated").append("<a href='"+link+"' onclick=\"s.tl(this,'o','homepage:photo:highestrated:surf');\"><img border='0' src='"+imgThumb+"'/></a>")
          } else if (rated == "G" || rated == "g") {
            jQuery(".hp-photo-row-1-title").html("<a href='/photos/index.cfm#!/gallery/category/boards-and-quivers' onclick=\"s.tl(this,'o','homepage:photos:surfboards');\">Boards & Quivers</a>");
            if(type == "category/boards-and-quivers" && boards < 4){boards++;jQuery(".board-photos").append("<a href='"+link+"'><img border='0' src='"+imgThumb+"'/></a>")}
            jQuery(".hp-photo-row-2-title").html("<a href='/photos/index.cfm#!/gallery/category/weather-and-hurricanes' onclick=\"s.tl(this,'o','homepage:photos:weather');\">Weather and Hurricanes</a>");
            if(type == "category/weather-and-hurricanes" && weather < 4){weather++;jQuery(".weather-photos").append("<a href='"+link+"'><img border='0' src='"+imgThumb+"'/></a>")}

          }
        }

    }
  });
});
</script>

