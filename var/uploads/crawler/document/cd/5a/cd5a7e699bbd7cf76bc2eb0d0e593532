            },
            overlayClose: true,
            opacity: 40
        });
    });
    $('#privacy, #privacyPolicy, #privacy2').live('click', function () {
    	$("#privacyContainer .content").html('<div style="text-align:center;"><img src="https://s3.amazonaws.com/spyfly/signup/loader_big.gif" width="50" height="50" /></div>');
    	$.get("/home/content/privacy-policy.php", function(data) {
		    $("#privacyContainer .content").html( data );
		});
        $('#privacyContainer').modal({
            close: true,
