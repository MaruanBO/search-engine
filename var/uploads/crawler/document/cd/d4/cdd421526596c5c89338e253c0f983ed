function build_oauth_lb(type) {
	var uproxx_action_href = 'http://profile.uproxx.com/signup';
	if (type == 'fb') {
		build_lb(uproxx_action_href + "?action=fbauth&social=facebook&back_url=" + uproxxsso_back, 650, 290);
	}
	if (type == 'tw') {
