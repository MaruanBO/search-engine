}*/

  $(".small-video-player-main").hover(
    function () {
      $(this).addClass("story-hover");
    },

    function () {
      $(this).removeClass("story-hover");
    }
  );

  $(".small-video-player-main-image").click(function() {
    $(".small-video-player-main-play").css("display", "none");
    $(".small-video-player-main-image").css("display", "none");
    $(".small-video-player-main-video").css("display", "block");

    addPlayer();
  });

  $(".small-video-player-main-play").click(function() {
    $(".small-video-player-main-play").css("display", "none");
    $(".small-video-player-main-image").css("display", "none");
    $(".small-video-player-main-video").css("display", "block");

    addPlayer();
  });

  $(".small-video-player-item div").click(function() {
    if(isPlayerAdded == true) {
      isPlayerAdded = false;
      
      removePlayer();
    }

    $(".small-video-player-item.selected").removeClass("selected");

    $(this).parent().addClass("selected");

    $(".small-video-player-main-play").css("display", "block");
    $(".small-video-player-main-image").css("display", "block");
    $(".small-video-player-main-video").css("display", "none");

    $(".small-video-player-main-image").html($(this).find(".small-video-player-item-image-big").html());
    $(".small-video-player-main-title").text($(this).find(".small-video-player-item-title").text());
    $(".small-video-player-main-date").text($(this).find(".small-video-player-item-date").text());
    $(".small-video-player-main-video-length").text($(this).find(".small-video-player-item-video-length").text());

    $(".small-video-player-main-facebook").attr("rel", $(this).find(".small-video-player-item-url").text());
    $(".small-video-player-main-facebook").attr("title", $(this).find(".small-video-player-item-title").text());
    $(".small-video-player-main-facebook").attr("rev", $(this).find(".small-video-player-item-image-big img").attr("src"));

    $(".small-video-player-main-twitter").attr("rel", $(this).find(".small-video-player-item-url").text());
    $(".small-video-player-main-twitter").attr("title", $(this).find(".small-video-player-item-title").text());

    $(".small-video-player-main-linkedin").attr("rel", $(this).find(".small-video-player-item-url").text());
    $(".small-video-player-main-linkedin").attr("title", $(this).find(".small-video-player-item-title").text());

    $(".small-video-player-main-plusone").attr("rel", $(this).find(".small-video-player-item-url").text());
    $(".small-video-player-main-plusone").attr("title", $(this).find(".small-video-player-item-title").text());
  });

  $(".small-video-player-main-facebook").click(function() {
    var url = $(this).attr("rel");
    var title = $(this).attr("title");
    var image = $(this).attr("rev");

    window.open("http://www.facebook.com/sharer/sharer.php?s=100&p[url]=" + url + "&p[images][0]=" + image + "&p[title]=" + title, "Share", "width=780,height=410,modal=yes,alwaysRaised=yestoolbar=0,scrollbars=0,status=0,resizable=0,location=0,menuBar=0,left=400,top=200");
    return false;
  });

  $(".small-video-player-main-twitter").click(function() {
    var url = $(this).attr("rel");
    var title = $(this).attr("title");

    window.open("https://twitter.com/share?url=" + url + "&text=" + title, "Share", "width=780,height=410,modal=yes,alwaysRaised=yestoolbar=0,scrollbars=0,status=0,resizable=0,location=0,menuBar=0,left=400,top=200");
    return false;
  });

  $(".small-video-player-main-linkedin").click(function() {
    var url = $(this).attr("rel");
    var title = $(this).attr("title");

    window.open("http://www.linkedin.com/shareArticle?mini=true&url=" + url + "&title=" + title, "Share", "width=780,height=410,modal=yes,alwaysRaised=yestoolbar=0,scrollbars=0,status=0,resizable=0,location=0,menuBar=0,left=400,top=200");
    return false;
  });

  $(".small-video-player-main-plusone").click(function() {
    var url = $(this).attr("rel");
    var title = $(this).attr("title");

    window.open("https://plus.google.com/share?url=" + url, "Share", "width=780,height=410,modal=yes,alwaysRaised=yestoolbar=0,scrollbars=0,status=0,resizable=0,location=0,menuBar=0,left=400,top=200");
    return false;
  });
</script>    </div>
  
  
  
  
  
  
