  var player;
  var modVP;
  var modExp;
  var modCon;
  var isPlayerAdded = false;
  var playerTemplate =  '<object id="myExperience" class="BrightcoveExperience">'+
                          '<param name="bgcolor" value="#ffffff" />'+
                          '<param name="width" value="{{width}}" />'+
                          '<param name="height" value="{{height}}" />'+
                          '<param name="playerID" value="{{playerID}}" />'+
                          '<param name="@videoPlayer" value="{{videoPlayer}}"; />'+
                          '<param name="adServerURL" value="http://www.doubleclick.com" />'+
                          '<param name="additionalAdTargetingParams" value="" />'+
                          '<param name="wmode" value="transparent" />'+
                          '<param name="width" value="{{width}}" />'+
                          '<param name="height" value="{{height}}" />'+
                          '<param name="autoStart" value="true" />'+
                          '<param name="isVid" value="true" />'+
                          '<param name="isUI" value="true" />'+
                          '<param name="includeAPI" value="true" />'+
                          '<param name="templateReadyHandler" value="TemplateReady" />'+
                          '<param name="templateLoadHandler" value="TemplateLoaded" />'+
                          ' <param name="templateErrorHandler" value="TemplateError" />'+
                        '</object>';

  function addPlayer() {
    var videoPlayer = $(".small-video-player-item.selected div").attr("id");

    var playerData = {"width": "295",
                      "height": "166",
                      "playerID": "2094656764001",
                      "videoPlayer": videoPlayer,
                      "width": "295",
                      "height": "166"};

    if(isPlayerAdded == false) {
      isPlayerAdded = true;

      var playerHTML = "";
      
      playerHTML = markup(playerTemplate, playerData);
      
      document.getElementById("MainVideoItem").innerHTML = playerHTML;
      
      brightcove.createExperiences();

/*var player = brightcove.getExperience("myExperience" + videoPlayer);

player.modules.videoPlayer.addEventListener(brightcove.api.events.MediaEvent.COMPLETE, MediaEvent);
var modVP = player.getModule(brightcove.modules.videoPlayer.APIModules.VIDEO_PLAYER);
var modExp = player.getModule(brightcove.modules.videoPlayer.APIModules.EXPERIENCE);

modExp.addEventListener(brightcove.events.ExperienceEvent.TEMPLATE_READY, MediaEvent);*/
    }
  }

  function removePlayer() {
    if(isPlayerAdded == true) {
      isPlayerAdded = false;

      experienceModule.unload();
      
      document.getElementById("MainVideoItem").innerHTML = "";
    }
  }
  
  function markup(html, data) {
    var m;
    var i = 0;
    var match = html.match(data instanceof Array ? /{{\d+}}/g : /{{\w+}}/g) || [];

    while(m = match[i++]) {
      html = html.replace(m, data[m.substr(2, m.length-4)]);
    }

    return html;
  }

/*function TemplateReady(evt) {
alert("Template Ready");
console.log(brightcove.api);
}

function TemplateLoaded(evt) {
alert("Template Loaded");
console.log(brightcove.api);
}

function TemplateError(evt) {
var videoPlayer = $(".small-video-player-item.selected div").attr("id");
var player = brightcove.getExperience("myExperience" + videoPlayer);

modVP = player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
modExp = player.getModule(brightcove.api.modules.APIModules.EXPERIENCE);
modExp.addEventListener(brightcove.api.events.ExperienceEvent.TEMPLATE_READY, onTemplateReady);
}
 
function onTemplateReady(evt) {
alert("TemplateReady")
