<script type="text/javascript">
try {
window.picturefill();
} catch(e) {
// later
}
function initTopGallery() {
    var headerNews = $('#header-news'),
        galleryClass = 'owl-carousel',
        windowWidth = 640;

    if(headerNews.length) {
        var stories = headerNews.find('> article');

        $(window).resize(function(e){
            clearTimeout(doit);
            var doit = setTimeout(function() {
                checkSize();
            }, 100);
        });

        if (window.innerWidth > windowWidth) headerNews.slideDown(800, function() {
            // trigger needs for sticky nav
            $(window).trigger('resize');
        });
        checkSize();
    }

    function checkSize() {
        if (window.innerWidth <= windowWidth) {
            headerNews.hide();
        } else if (window.innerWidth > windowWidth) {
            headerNews.show().slideDown(800);
        }
    }
}
initTopGallery();
</script>



