    <a href="/?page=2" rel="next" data-page="2" data-max="200" data-start-index="16" class="more-articles" onClick="ga('send', 'event', 'Links', 'Show More Button', '/homepage');">Show More<span class="arrow">arrow</span></a>
    <script type="text/javascript">
        jQuery(function(){
            jQuery(".more-articles").on("click", function() {
                var moreAnchor = $(this),
                    href = window.location.protocol + "//" + window.location.hostname + window.location.pathname,
                    dataPage = moreAnchor.attr("data-page"),
                    dataPage = parseInt(dataPage, 10),
                    startIndex = moreAnchor.attr("data-start-index"),
                    startIndex = parseInt(startIndex, 10),
                    dataMax = moreAnchor.attr("data-max"),
                    dataMax = parseInt(dataMax, 10),
                    href = href + "?ajax=true&page=" + dataPage + "&startIndex=" + startIndex + "&w=" + $(document).width()+"&keywords=&sortBy=";
                          
                jQuery.ajax(href, {
                    dataType: "html",
                    success: function(response){
                        dataPage++;
                        var div = $("<div/>"),
                            articles = div.html(response.toString()).find('article'),
                            newStartIndex = startIndex + 9 + 1;
                        if(dataMax - newStartIndex > 0) {
                            moreAnchor.css("opacity", "1").attr("href", href);
                        } else {

                            moreAnchor.remove();
                        } 


                        $('#stories-holder').append(articles).isotope("appended", articles, function(){
                            moreAnchor.attr("data-page", dataPage);
                            moreAnchor.attr("data-start-index", newStartIndex);
                            var storyIDsArray = $("#stories-holder .story-item:last").attr("data-stories-loaded");
                            var p = window.picturefill();
                            setTimeout(function(){
                                $('#stories-holder').isotope({}, function() { fixRightRail(); });
                                  gigya.comments.getStreamInfo({categoryID:"General Comments", streamIDs: storyIDsArray, callback: getCommentCount });
                              }, 1000);
                                    
                        });
                    }
                });
                return false;
            });
        })

    </script>

