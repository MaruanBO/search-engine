    <script>
    $(document).ready(function () {
        var articles = new Bloodhound({
          datumTokenizer: function(d) { return Bloodhound.tokenizers.whitespace(d.tokens); },
          queryTokenizer: Bloodhound.tokenizers.whitespace,
          limit: 3,
          prefetch: {
            url: '/templates/ta-articles.json?sdffff',
            ttl: 600000
          }
        });
         
        var authors = new Bloodhound({
          datumTokenizer: function(d) { return Bloodhound.tokenizers.whitespace(d.title); },
          queryTokenizer: Bloodhound.tokenizers.whitespace,
          prefetch: '/templates/ta-authors.json?sdfsdf'
        });

        var topics = new Bloodhound({
          datumTokenizer: function(d) { return Bloodhound.tokenizers.whitespace(d.title); },
          queryTokenizer: Bloodhound.tokenizers.whitespace,
          prefetch: '/templates/ta-topics.json?sdfff'
        });

        articles.initialize();
        authors.initialize();
        topics.initialize();

        var navTypeAhead = $('.nav-search .typeahead');
        navTypeAhead.typeahead({
          highlight: true
        },
        {
          name: 'articles',
          source: articles.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Stories</h3>',
                suggestion: Handlebars.compile([
                    '<div class="tt-article-details">',
                    '<div class="tt-image"><img src="{{thumbnail}}" /></div>',
                    '<h3 class="tt-article-header">{{title}}</h3>',
                    '<p class="tt-byline-time"><span class="byline">{{author}}</span> | {{startDate}}</p>',
                    '</div>'
                ].join('')),
                footer: Handlebars.compile([
                    '<div class="tt-suggestion more-search"><a href="/search?keywords={{query}}">See more</a></div>'
                ].join(''))
            }
        },
        {
          name: 'authors',
          source: authors.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Authors</h3>',
                footer: '<div class="more-search" id="all-authors"><a href="/author">All Authors</a></div>',
                suggestion: Handlebars.compile([
                '<span>{{title}}</span>'
                ].join(''))
            }
        },
        {
          name: 'topics',
          source: topics.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Topics</h3>',
                footer: '<div class="more-search" id="all-topics"><a href="/topic">All Topics</a></div>',
                suggestion: Handlebars.compile([
                '<span>{{title}}</span>'
                ].join(''))
            }
        });

        // Search Page
        var searchPageTypeAhead = $('.search-by-text .typeahead');
        searchPageTypeAhead.typeahead({
          highlight: true
        },
        {
          name: 'search-articles',
          source: articles.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Stories</h3>',
                suggestion: Handlebars.compile([
                    '<div class="tt-article-details">',
                    '<div class="tt-image"><img src="{{thumbnail}}" /></div>',
                    '<h3 class="tt-article-header">{{title}}</h3>',
                    '<p class="tt-byline-time"><span class="byline">{{author}}</span> | {{startDate}}</p>',
                    '</div>'
                ].join('')),
                footer: Handlebars.compile([
                    '<div class="tt-suggestion more-search"><a href="/search?keywords={{query}}">See More</a></div>'
                ].join(''))
            }
        },
        {
          name: 'search-authors',
          source: authors.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Authors</h3>',
                footer: '<div class="more-search" id="all-authors"><a href="/author">All Authors</a></div>',
                suggestion: Handlebars.compile([
                '<span>{{title}}</span>'
                ].join(''))
            }
        },
        {
          name: 'search-topics',
          source: topics.ttAdapter(),
            templates: {
                header: '<h3 class="tt-header">Topics</h3>',
                footer: '<div class="more-search" id="all-topics"><a href="/topic">All Topics</a></div>',
                suggestion: Handlebars.compile([
                '<span>{{title}}</span>'
                ].join(''))
            }
        });

        $('#nav-search-text').bind('typeahead:selected', function(obj, datum, name) {
            if(typeof datum === 'undefined'){
                window.location = '/search?keywords='+$('#nav-search-text').val();
            }else{
                window.location = datum.url;
                
            }   
        });

        $('#advanced-search-id-1').bind('typeahead:selected', function(obj, datum, name) {
            if(typeof datum === 'undefined'){
                window.location = '/search?keywords='+$('#advanced-search-id-1').val();
            }else{
                window.location = datum.url;
                
            }   
        });
     });
</script>
