    <script type="text/javascript">
    	var ctrlSearch = 'ctl00_txtSearch';

        function doSearch(elem) {
            if (elem.value.toUpperCase() != 'SEARCH') {

            	window.location = "http://search.gibson.com/search?q=" + elem.value + "&btnG=Search&sort=date%3AD%3AL%3Ad1&output=xml_no_dtd&spell=1&client=default_frontend&oe=UTF-8&ie=UTF-8&proxystylesheet=default_frontend&site=default_collection";
            }
        }
