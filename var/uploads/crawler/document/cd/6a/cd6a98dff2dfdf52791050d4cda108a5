<script>
	var thumbClick = false;
	var offSet = 0;
	var len = 0;

	$(document).ready(
		function () {
			len = $('.HeroThumbs img').length;

			$('.HeroThumbs img:gt(0)').fadeTo(0, 0.35);
			$('.HeroThumbs img:lt(1)').addClass('SelectedThumb');
			$('.HeroFader a:gt(0)').hide();

			$('.HeroThumbs img').hover(
				function () {
					$(this).addClass("HoverThumb");
				},
				function () {
					$(this).removeClass("HoverThumb");
				}
			);

			setInterval(function () {
				if (!thumbClick) {
					found = false;
					$('.HeroThumbs img').each(function (index, element) {
						if ($(element).hasClass('SelectedThumb') && !found) {
							found = true;
							oldThumb = $(element);

							if (index == len - 1) {
								newThumb = $('.HeroThumbs img:first-child');
							}
							else {
								newThumb = $(element).next('img');
							}
						}
					});

					oldHero = $('.HeroFader a:first-child');

					oldHero.fadeOut(1700).next('a').fadeIn(1700).end().appendTo('.HeroFader');
					oldThumb.fadeTo(1700, 0.35);
					
					newThumb.fadeTo(1700, 1.0);

					oldThumb.removeClass('SelectedThumb');
					newThumb.addClass('SelectedThumb');

					offSet++;
					offSet = offSet % len;
				}
			}, 10000);

			$(".HeroThumbs img").click(function () {
				thumbClick = true;
				if (!$('.HeroFader a').is(':animated')) {
					var index = $(".HeroThumbs img").index(this);
					if (!$('.HeroFader a').eq(index - offSet).is(':visible')) {

						$(".HeroThumbs img").removeClass("SelectedThumb");
						$(this).addClass("SelectedThumb");

						$(".HeroThumbs img").fadeTo(300, 0.35);
						$(this).fadeTo(0, 1.0);

						$('.HeroFader a').fadeOut(300);
						$('.HeroFader a').eq(index - offSet).fadeIn(300);
					}
				} 
			});

			$(".HeroThumbs img").hover(
				function () {
					$(this).fadeTo(300, 1.0);
				}, function () {
					if (!$(this).hasClass('SelectedThumb')) {
						$(this).fadeTo(300, 0.35);
					}

				}
			);
		}
	);

</script>

