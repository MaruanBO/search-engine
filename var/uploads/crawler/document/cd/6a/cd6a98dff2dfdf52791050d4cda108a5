	<script type="text/javascript">
		$(function () {
			$('menu>li>div>ul>li').mouseenter(function () {
				var img = $('img', this),
					holder = $(this).closest('div').find('>img')
				holder.attr({
					src: img.attr('src'),
					alt: img.attr('alt'),
					title: img.attr('title')
				})
			}).parent().each(function () {
				$(this).children().eq(0).trigger('mouseenter')
			})

			if (window.Touch) {
				$('menu>li>span').click(function () {
					$('.hover').not($(this).parent().toggleClass('active')).removeClass('active')
				})
			}
		})
	</script>

