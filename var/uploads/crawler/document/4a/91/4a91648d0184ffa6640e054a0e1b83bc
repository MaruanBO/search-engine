<script id="stickyjs">
    YZap2it.use('node', 'zap2it-services','event', function(Y) {
        Y.later(1000, this, function() {

            if (Y.one("#right") && ! Y.one('body.nostickyright')) {
                new Y.zap2it.services.StickyService({
                    node: Y.one("#right>div"),
                    stickPosition: "bottom",
                    offsetWidth: 30
                });

            }
            if (Y.one("#header")) {
                new Y.zap2it.services.StickyService({
                    node: Y.one("#header"),
                    stickPosition: "top",
                    ignore: "width",
                    callback: function(unstuck) {
                        if (!unstuck) {
                            document.getElementById('header-padder').className = '';
                        } else {
                            document.getElementById('header-padder').className = 'zap2it-hide';
                        }

                    }
                });
            }
            var favManager = new Y.zap2it.services.FavShowsManager();
            var magicAds = Y.all(".z2t-magicad"),
                triggeredAdsForLandscape = magicAds.size() == 0;

            if(!triggeredAdsForLandscape){
                Y.on("orientationchange", function(e){
                    var dir = window.orientation,
                        idsForMagicAds = magicAds.get("id"),
                        idsLen = idsForMagicAds.length,
                        tmp;

                    if (!triggeredAdsForLandscape && (dir == "90" || dir == "-90" )){
                        triggeredAdsForLandscape = true;
                        magicAds.removeClass('z2t-magicad');
                        for( tmp= 0 ; tmp < idsLen; tmp++){
                            GPTWrapper.displayAd(idsForMagicAds[tmp]);
                        }
                    }
                });
            }

        });
    });

</script>

<script id="lazy-images">
    // Lazy loading images
    YZap2it.use('node', 'imageloader', function(Y) {
        Y.later(10, this, function() {
            var foldGroup = new Y.ImgLoadGroup({name: 'fold group', foldDistance: 25});
            var nodes = Y.all('.zap2it-lazy')
            nodes.each(function(node) {
                var id = node.generateID();
                var img = node.getAttribute('data-src');
                node.removeClass('zap2it-lazy');
                foldGroup.registerImage({domId: id, srcUrl: img});
            });
        });
    });
</script>

