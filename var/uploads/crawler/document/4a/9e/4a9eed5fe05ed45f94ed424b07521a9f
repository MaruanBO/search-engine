<script>
jQuery(function($)
{
	var url = location.href;
	
	$('a').each(function() 
	{
		if ($(this).prop('href') == window.location.href) 
		{
			if(url.search('-_-vitaminsminerals') != -1) 
			{
				$('#VITAMINSMINERALS').attr('class','current');
			}Â 
			else if (url.search('-_-supplements') != -1)
			{
				$('#SUPPLEMENTS').attr('class','current');
			}
			else if (url.search('-_-herbs') != -1)
			{
				$('#HERBS').attr('class','current');
			}
			else if (url.search('-_-sportsnutrition') != -1)
			{
				$('#SPORTSNUTRITION').attr('class','current');
			}
			else if (url.search('-_-dietlifestyle') != -1)
			{
				$('#DIETLIFESTYLE').attr('class','current');
			}
			else if (url.search('-_-healthbeauty') != -1)
			{
				$('#HEALTHBEAUTY').attr('class','current');
			}
			else if (url.search('-_-men') != -1)
			{
				$('#MENS').attr('class','current');
			}
			else if (url.search('-_-women') != -1)
			{
				$('#WOMENS').attr('class','current');
			}
		}
	});
});
</script>
	
	
	
	
