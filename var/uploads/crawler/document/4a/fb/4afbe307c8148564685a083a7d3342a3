<script type="text/javascript" language="javascript">
var SignUpBusy = false;
function submitTrailMailSignUpForm() {
  SignUpBusy = true;
  var frm = document.forms.TrailMailSignUpForm;
  if (validateTrailMailSubmit(frm)) {
    frm.submit();
  } else {
    SignUpBusy = false;
  }
}

function validateTrailMailSubmit(frm) {
  // alert("In validateTrailMailSubmit()" + frm);
  // alert("frm.email1.value = '" + frm.email1.value + "'");
  var errorString = '';

  // alert("isValidEmail(frm.email1.value) = " + isValidEmail(frm.email1.value));
  if (isValidEmail(frm.email1.value) != 'true') {
    errorString = 'Please enter your email address in this format: youremail@domain.com';
  }

  // alert("errorString = '" + errorString + "'");
  if (errorString != '') {
    writeErrorMSG(errorString);
    //scrolling page to the top so that error msgs are visible
    window.scrollTo(0,0);
    return false;
  } else {
    //No errors! -> Conditionally submit the form:
   return true;
  }
}
</script>
