var cartQuantity = "0";
var cartTotal    = "$0.00";
if (null == readCookie("WC_SESSION_ESTABLISHED") || (null != readCookie("WC_USERACTIVITY_-1002") && null != readCookie("WC_AUTHENTICATION_-1002"))) {
  console.debug("Generic guests do not have carts - creating cookies for SEO home page");
  createCookie("CartQty", "0", 1);
  createCookie("CartTtl", "$0.00", 1);
} else {
  console.debug("Not a generic guest - read cookies for SEO home page");
  if (null != readCookie("CartQty") && readCookie("CartQty") != '') {
    cartQuantity = readCookie("CartQty");
  }
  if (null != readCookie("CartTtl") && readCookie("CartTtl") != '') {
    cartTotal = readCookie("CartTtl");
  }
}
