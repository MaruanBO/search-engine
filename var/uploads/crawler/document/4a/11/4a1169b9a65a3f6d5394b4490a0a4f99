	function  getFormValues(fields ,name) {
		var returnValue = "";
	    jQuery.each(fields, function(i, field){
	    	if (field.name == name )
	    	{
	    		 returnValue =  field.value;
	    		return false; // this breaks out of the each
	    		
	    	}
	    });
	    return returnValue;
	  }	
	
	function createHttpOnlyCookie(name,value, minutes) {
		  
		  if (!(typeof value == "undefined")) 
		  	{ var expires = ""; if (minutes) { var date = new Date(); date.setTime(date.getTime()+(minutes *60*1000)); 
		  	   expires = "; expires="+date.toGMTString();
			}
			document.cookie = name+"="+value+expires+"; path=/;";
		  }
	}
