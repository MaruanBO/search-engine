CRD.ready(function(){
	function typeVehicleCenter(){
		var el = $('.entry-type-vehicle-center-wrap > a'),
			el_len = $(el).length,
			hold_width = $('.entry-type-vehicle-center').width(),
			el_w = Math.floor(hold_width/el_len),
			clear = '';

		$('.entry-type-vehicle-center-wrap').width(hold_width+el_w*4);

		$(el).off('mouseenter mouseleave').on({
			mouseenter:function(){
				var _that = $(this);
				clear = setTimeout(function(){
					$(el).css({width:el_w/2+'px'}).eq(el_len-1).css({width:el_w*2+'px'});
					_that.css({width:el_w*2+'px'});
					_that.find('div').stop(true,true).slideDown();
				}, 100);
			},
			mouseleave:function(){
				clearInterval(clear);
				$(this).find('div').slideUp();
			}
		});
		$('.entry-type-vehicle-center').off('mouseleave').on({
			mouseleave:function(){
				$(el).css({width:el_w+'px'});
			}
		});
	}
	if (CRD.device.media.desktop) {
		typeVehicleCenter();
	}
	CRD.device.mediaChange(function() {
		if (CRD.device.media.desktop) {
			typeVehicleCenter();
		}
	});
});
</script>
