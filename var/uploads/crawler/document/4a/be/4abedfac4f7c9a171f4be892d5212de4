<script>
	var user;
	

	$LAB
	.script('//fast.wistia.net/static/embed_shepherd-v1.js')
	.wait(function () {
		function logVideoEvent(video, eventName) {
			_gaq.push(['_trackEvent', 'Video', eventName, video.name() + '_' + video.hashedId()]);
		}

		wistiaEmbeds.onFind(function(video) {
			var quarterDuration;
			var threeQuartersDuration;

			video.bind('play', function() {
				quarterDuration = Math.floor(video.duration() / 4);
				threeQuartersDuration = quarterDuration * 3;

				if (user && user.Email)
					video.email(user.Email);
				
				logVideoEvent(video, 'Play');
				return this.unbind;
			}).bind('end', function() {
				logVideoEvent(video, 'Complete');
				return this.unbind;
			}).bind('secondchange', function(second) {
				if (second === quarterDuration)
					logVideoEvent(video, '25% Played');
			
				if (second === threeQuartersDuration) {
					logVideoEvent(video, '75% Played');
					return this.unbind;
				}
			});
		});
	});
</script>

<script type="text/javascript">
	if (typeof(Sys) !== 'undefined' && Sys.WebForms) {
		Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(WebForms_BeginRequest);
		Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(WebForms_PageLoaded);
	}

	function WebForms_BeginRequest(sender, args) {
		$(args._postBackElement).addClass('loading').blur();
	}

