			var timeSpent = Math.floor((new Date().getTime() - scriptLoadTime) / 1000);
			_gaq.push(['_trackEvent', 'Outbound Links', this.hostname, e.target.href, timeSpent, true]);
			_uaTracker('send', 'event', 'Outbound Links', this.hostname, e.target.href, timeSpent, {'nonInteraction': 1});
		});
	}());
</script>

	<script>
		isMobilePage = false;
	</script>
	
	
