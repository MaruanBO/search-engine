            function showForm() {
                var theForm = document.getElementById('F9ContainerLayout');
                theForm.style.visibility = "visible";
            }

            // This method is used to get the defult airport name in area.
            function GetFirstAreaAirport(areaAirPortsReplace) {
                $.ajax({
                    type: "POST",
                    url: "/F9_Services/Wordwheel/WordWheelLocal.asmx/GetDefaultAirportNameByArea",
                    data: "{'areaAirPortsReplace': '" + areaAirPortsReplace + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: false,
                    success: function (msg) {
                        firstAirPortName = msg.d;
                    },
                    error: AjaxFailed
                });

                function AjaxFailed(result) {
                    alert(result.status + ' ' + result.statusText);
                }
            } 

