            function FromCityPopulatedItem(sender, e) {
                var target = sender.get_completionList();
                var children = target.childNodes;
                var child = children[0];
                var ContextValue = child._value;
                $("#hdStorePopulatedKey").val(ContextValue);
                $find("ToCitySearchComplete").set_contextKey(ContextValue);
            }

            // Method is modifed by Nageswararao on 18/03/2013 to fix - #704 
            function FromCitySelectItem(sender, e) {
                var ContextValue = $.trim(e.get_value());
                document.getElementById('hdStoreKey').value = ContextValue;
                if (ContextValue.indexOf("airports") != -1) {
                    //					        alert("Home");                        
                    //                          $("#fromCity").val("");
                    //                          notHideAutocomplete();
                    var areaAirPortsReplace = ContextValue.replace('airports', '');
                    GetFirstAreaAirport(areaAirPortsReplace);
                    ContextValue = firstAirPortName;
                    hideAutocomplete();
                    $("#hdStorePopulatedKey").val(ContextValue);
                    $("#hdStoreKey").val(ContextValue);
                    $("#fromCity").val(ContextValue);
                    $find("ToCitySearchComplete").set_contextKey(ContextValue);
                } else {
                    hideAutocomplete();
                    $find("ToCitySearchComplete").set_contextKey(ContextValue);
                    $("#fromCity").val(ContextValue);
                }
            }

            // Method is modifed by Nageswararao on 18/03/2013 to fix - #704 
            function ToCitySelectItem(sender, e) {
                var ContextValue = $.trim(e.get_value());
                document.getElementById('hdStoreKey').value = ContextValue;
                if (ContextValue.indexOf("airports") != -1) {
                    //	$("#toCity").val("");
                    //	notHideAutocomplete();
                    var areaAirPortsReplace = ContextValue.replace('airports', '');
                    GetFirstAreaAirport(areaAirPortsReplace);
                    ContextValue = firstAirPortName;
                    hideAutocomplete();
                    $("#toCity").val(ContextValue);

                } else {
                    hideAutocomplete();
                    $("#toCity").val(ContextValue);
                }
            }

            function ToCityPopulatedItem(sender, e) {
                var target = sender.get_completionList();
                var children = target.childNodes;
                var child = children[0];
                var ContextValue = child._value;
                $("#hdStorePopulatedKey").val(ContextValue);
            }

            // Method is modifed by Nageswararao on 18/03/2013 to fix - #704 
            function FlightStatusFromCitySelectItem(sender, e) {
                var FlightStatusContextValue = $.trim(e.get_value());
                document.getElementById('hdStoreKey').value = FlightStatusContextValue;
                if (FlightStatusContextValue.indexOf("airports") != -1) {

                    //  $("#flightStatusFrom").val("");
                    //	notHideAutocomplete();

                    var areaAirPortsReplace = FlightStatusContextValue.replace('airports', '');
                    GetFirstAreaAirport(areaAirPortsReplace);
                    FlightStatusContextValue = firstAirPortName;
                    hideAutocomplete();
                    $("#hdStorePopulatedKey").val(FlightStatusContextValue);
                    $("#hdStoreKey").val(FlightStatusContextValue);
                    $("#flightStatusFrom").val(FlightStatusContextValue);
                    $find("FlightStatusToCitySearchComplete").set_contextKey(FlightStatusContextValue);


                } else {
                    $find("FlightStatusToCitySearchComplete").set_contextKey(FlightStatusContextValue);
                    $("#flightStatusFrom").val(FlightStatusContextValue);
                    hideAutocomplete();
                }
            }
            function FlightStatusFromCityPopulatedItem(sender, e) {
