	<span class="text abridgedHeadline" style="font-size: 16px; font-weight: bold !important; color: rgb(5, 114, 177); font-family: Verdana,times new roman,serif!important; display: inline-block; margin: 0px; line-height: 18px !important; font-style: normal !important; font-variant: normal !important;">
	Headlines</span><a class="wnContent more enabled" href="http://www.nbc-2.com/category/170699/news" itemprop="url" style="font-size: 12px; color: rgb(5, 114, 177); padding: 0px; margin: 0.2em; text-decoration: none; float: right; display: block; font-family: georgia, 'times new roman', serif; font-weight: normal;"><span class="text" style="font-size: 12px !important; cursor: pointer; font-weight: bold !important; color: #174170; font-family: Verdana,times new roman,serif!important; display: inline; margin: 0px; line-height: normal !important; font-style: normal !important; font-variant: normal !important; float: none; text-decoration: inherit !important;">More&gt;&gt;</span></a></h3></div>
<script type="text/javascript" src="http://ftpcontent.worldnow.com/wncustom/js/jquery.csutils.js"></script>
<script type="text/javascript">

var csTSA = {
    tsaBackupImage : "http://wbbh.images.worldnow.com/images/273803_G.jpg"
}

function CStsa(){
  // Load story data
  var mainData  = $wn('.wnDS-20 .wnDVSummary').wnDVSummaryExtract();
  if (mainData.length < 1) {
    $wn('#csTSA').parents('.wnDVUtilityBlock').hide();
    return;
    }
    
    $wn('#WNTopStoryWrap, #WNCol2 .wnDS-20').hide();
    // Build top side
    var story = mainData[0];
    story.hasVideo = (story.isClip || story.isVideo);
    if (!story.graphicSrc) {
        story.graphicSrc= csTSA.tsaBackupImage;
    }
    // hack for last updated issue with first tsa
    var updatedTime= new Date(mainData[0].dateText) ;
console.debug(mainData[0].dateText);
    if (new Date() <= new Date('Sun Nov 2 2014 02:00:00 GMT-0400 (Eastern Daylight Time) ')){
      var newUpdatedTime = new Date(updatedTime.setTime(updatedTime.getTime() + (1*60*60*1000)));
      mainData[0].dateText = newUpdatedTime.toLocaleString();
console.debug(newUpdatedTime);
    }
    $wn('#csTSAleft').csFormatterAppend(
      '<div class="image">' +
      '<a href="{$headlineHref}"><img src="{$graphicSrc}" /></a>' + 
      '</div>' + 
      '<div class="info">' +
    '<h4>' +
    '<a href="{$headlineHref}">{$headlineText} {if $hasVideo} <img class="video" src="http://wbbh.images.worldnow.com/images/static/container-e/icon_video_included.gif" />{/if}</a>' +
    '</h4>' +
    '{if $dateText}<em>Updated: {$dateText}</em><br/>{/if}' +
    '{if $summaryHtml}<font>{$summaryHtml}</font>{/if}' +
    '</div>' +
    '<div class="wnClear"></div>', story
    );
    
}
function MoveElements(){
  $wn("#csTSAright").append($wn("#DisplaySizeId-7"));
  $wn("#DisplaySizeId-4").prepend($wn("#latestHeadlinesHeader"));
}
function CheckSumImages(){
  var $tsSrc = $wn("body #WNColsAll #WNCols234-5 #WNCols23-4 #WNCol23 #WNCol2 .displaySizeId-4 ul.wnGroup.contentGroup");
  var BackupImage="http://wbbh.images.worldnow.com/images/273803_G.jpg";
  $tsSrc.find("li.wnItem").each(function(){
		if($wn(this).find("div.wnContent.summaryImage").length) {
			$wn(this).find("h4.wnContent.headline").prepend($wn(this).find("div.wnContent.summaryImage"));
 		};
		if(!$wn(this).find("div.wnContent.summaryImage").length) {
			$wn(this).find("h4.wnContent.headline").prepend("<div class='wnImage wnImageLeft wnImageWidth-180'><a href='"+ $tsSrc.find("h4 a").attr("href") +"'><img src='"+ BackupImage +"' alt='' title='' border='0' width='180' style='display:block;'></a></div>");
 		};
  });
}

PLATFORM.EventMan.registerToEvent(function(){
  CStsa();
  MoveElements();
  CheckSumImages();
},'WNCol23done');

$(document).ready(function(){
  $wn("#DisplaySizeId78").prepend($wn("#WNDS35"));
  $wn("#DisplaySizeId78").append($wn("#DisplaySizeId27"));
});
</script></div><script>Worldnow.Helpers.debug('UB HOMEPAGE REDESIGN CSS - UPDATED FEBRUARY 2014(323175)', 'UB_323175');</script></div><!--END wnDSItems-->
