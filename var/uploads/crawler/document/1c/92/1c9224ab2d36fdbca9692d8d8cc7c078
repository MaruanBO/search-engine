<style type="text/css">
.FeaturedStoryHeadline {
  height: 7em;
}

.FeaturedStorySlideShowThumbs {
  float: right;
  position: relative;
}

#FeaturedStoryThumbSlider {
  border-top: 1px solid #000;
  border-left: 1px solid #000;
  border-right: 2px solid #6d9873;
  border-bottom: 2px solid #6d9873;
  position: absolute;
  width: 19px;
  height: 19px;
}

a.FeaturedStorySlideThumb {
  background-color: #999;
  color: #fff;
  float: left;
  font-size: 12px;
  line-height: 150%;
  margin: 1px 8px 1px 0;
  text-align: center;
  text-decoration: none;
  width: 20px;
  height: 20px;
}

.FeaturedStorySlideShowMain a img {
  visibility: hidden;
}
</style>

<script type="text/javascript">
<!--
var FeaturedStoryNumSlides = 0;
var FeaturedStorycurrentID = 1;
var FeaturedStorySlide = 0;

/**********************
 * SwitchSlide()
 **********************/
function FeaturedStorySwitchSlide() {
  clearTimeout(FeaturedStorySlide);

  var image = (this.id)
            ? FeaturedStorySwitchSlide.images[this.id]
            : FeaturedStorySwitchSlide.images["Image:imgThumb" + FeaturedStorycurrentID];

  FeaturedStorycurrentID = image.index;

  //move slider
  var newPos = FeaturedStorycurrentID*20 - 20 + (FeaturedStorycurrentID-1)*8;
  new Effect.Morph('FeaturedStoryThumbSlider', {style:"left:+" + newPos + "px;", duration: 0.5});

  //switch image
  $$("div.FeaturedStorySlideShowMain").invoke("hide");
  $("Image:Main"+FeaturedStorycurrentID).show();


  if ($('FeaturedStoryThumbSlider').style.width != '19px') {
    new Effect.Morph('FeaturedStoryThumbSlider',
      { style:"width:19px;", duration: 0.5 }
    );
  }

  if (FeaturedStorycurrentID >= FeaturedStoryNumSlides) {
    FeaturedStorycurrentID = 1;
  }
  else {
    FeaturedStorycurrentID++;
  }

  if (FeaturedStoryNumSlides > 1) {
    FeaturedStorySlide = setTimeout('FeaturedStorySwitchSlide()', 6000);
  }
}

FeaturedStorySwitchSlide.images = {};

Event.observe(document, "dom:loaded", function () {
  // Set total number of slides.
  FeaturedStoryNumSlides = $$("div.FeaturedStorySlideShowThumbs a").length;

  // Only activate if we have slides.
  if (FeaturedStoryNumSlides) {
    $$("div.FeaturedStorySlideShowThumbs a").each(function (element) {
      element.observe("mouseover", FeaturedStorySwitchSlide);
      element.onclick = function() {
        return false;
      };
    });

    $$(".FeaturedStorySlideShowMain a img").each(function (element) {
      element.style.visibility = 'visible';
    });

    // first image
    var initImage = new FeaturedStorySwitchSlide();
  }
});
//-->
</script>

