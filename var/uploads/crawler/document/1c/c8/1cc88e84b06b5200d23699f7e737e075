<script type="text/javascript">
    function ShowSuccessForWishlist(wishlistId) {
        var isPDP = "";
        if (isPDP)
            showResultMessage('Successfully added to <a href="/w/wishlists-' + wishlistId + '" id="ViewWishlist">Wishlist</a>');
        else
            showResultMessage('Successfully added to <a href="javascript:void(0)" onclick="OpenWishlist('+ wishlistId + ');">Wishlist</a>');
        $("#transferWishlistId").val(wishlistId);
    };

    function ShowErrorMsgForWishlist() {
        showResultMessage('Please Choose an Item and Input Qty.', 'error');
    }

    function GetSkuAndQtys() {
        if (!getInputedSkuAndQty('wishList')) {
			var selectedSku = getCurrentSku();
			if (typeof (selectedSku) == "undefined" || selectedSku == "") {
				return false;
			}else {
				var skuAndQtys = selectedSku + ":" + 1 + ",";
                $("#skuAndQty").val(skuAndQtys);
                return true;
            }
        }
        return true;
    }

    function addToWishlistBtnClickCallBack(data, status) {
        var result = eval(data);
        if (result.IsLogin == "true") {
            wishlistHopupWindow(592, result.url, true)
        }
        else {
            parent.location = result.url;
        }

    }

    function addToWishlistHouupBtnClickCallBack(data, status) {
        var result = eval(data);
        if (result.IsLogin == "true") {
            parent.wishlistHopupWindow(592, result.url, true);
            setTimeout(function () {
                parent.quidsiUrlNoMarginPopup.close();
            }, 500);
        }
        else
            parent.location = result.url;
    }
</script>
