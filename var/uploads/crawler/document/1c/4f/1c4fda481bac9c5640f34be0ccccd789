<script type="text/javascript">
$wn(document).bind('csWxCol4Ready', function() {
	
	CSWxManager.setOption('wsi');
	CSWxManager.setOption('iconSize', 60);
	CSWxManager.loadConfig([
		{station: '29201', name: 'Columbia'},
		{station: '29071', name: 'Lexington'},
		{station: '29063', name: 'Irmo'},
		{station: '29150', name: 'Sumter'},
		{station: '29115', name: 'Orangeburg'},
		{station: '29180', name: 'Winnsboro'},
		{station: '29108', name: 'Newberry'},
		{station: '29020', name: 'Camden'},
		{station: '29102', name: 'Manning'}
	]);

	CSWxManager.ready(function(wx) {
		if (!wx.hasData()) { return; }
    
		// Specific to the MyWeather XML format
		function rotate() {
			$wn('#csCol4Wx #csWx1 .wxIcon').css('background-image', wx.fetch('iconcss'));
			$wn('#csCol4Wx #csWx1 .wxTemp').html(wx.fetch('temperature') + '&deg;');
			$wn('#csCol4Wx #csWx1 .wxCity').text(wx.fetch('city'));
			$wn('#csCol4Wx #csWx1 .wxFeels').html(wx.fetch('feelslike') + '&deg;');
			$wn('#csCol4Wx #csWx1 .wxFeels').prepend('Feels like ');

			$wn('#csCol4Wx #csWx2 .wxDew').html(wx.fetch('dewpoint') + '&deg;');
			$wn('#csCol4Wx #csWx2 .wxHumid').html(wx.fetch('humidity') + '&#37;');
			$wn('#csCol4Wx #csWx2 .wxWind').text(wx.fetch('windspeed') + 'mph');
			$wn('#csCol4Wx #csWx2 .wxDirection').text(wx.fetch('winddirection'))
			wx.next();
		}
		rotate();
		setInterval(rotate, 5000);
	});


});

</script>
