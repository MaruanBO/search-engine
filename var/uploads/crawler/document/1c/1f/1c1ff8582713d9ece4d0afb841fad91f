            .css(contentProps)
            .prependTo(".MapContainer")
            .animate(animProps, 500, function() {
              var close = $("<img src='/images/x.png' alt='close map' />")
                .css({
                  position: "absolute", 
                  left: 540, 
                  top: $("#zoom").offset().top - 110,
                  zIndex: 4}
                ).prependTo(".MapContainer")
                .bind("click", function(e) {
                  $(".fullsize").hide();
                  var newProps = {width: 50, height: 50, left: 199, top: 550, bottom: 600};
                  $("#zoom").animate(newProps, 500, function(){
                    $(this).remove();
                  });
                  $(this).remove();
               });
            });
          return false;
        });

