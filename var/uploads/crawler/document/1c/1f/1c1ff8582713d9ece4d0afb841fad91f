        $("a.SearchMapLink").bind("click", function(e) {
          e.stopPropagation();
          var me = $("img", this);
          var meXY = me.offset();
          var sect = $(".SellYourHomeContainer");
          var sectXY = sect.offset();

          meXY.height = (me.height() * (350 / me.width())) + 35;
          meXY.left = 199;

          var contentProps = { position: "absolute", width: me.width(), height: me.height(), left: meXY.left, top: meXY.top, overflow: "hidden"};
          var animProps = {width: 350 };
          animProps.bottom = (contentProps.top + contentProps.height);
          animProps.top = (animProps.bottom - meXY.height);
          animProps.height = meXY.height;

