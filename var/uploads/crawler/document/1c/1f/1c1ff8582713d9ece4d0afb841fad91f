        $("select[name='SearchType']").bind("change", function(){
          var selected = $(this).val();
          var myID = "#"+selected+"Options";
          $(".otherOptions").not(myID).hide();
          $(myID).show();

          var myAction = $(myID).find("input[name='MyAction']").val();
          $("#SearchMHVillage").attr("action", myAction);

          // turn on/off park searching
          if (selected == "Dealers")
            $("#Location").autocomplete({ extraParams: { park: "no" } });
          else
            $("#Location").autocomplete({ extraParams: { park: "yes" } });
        });

  $("#Location")
    .defaultText({
      defaultText: "Enter zip or city, state",
      defaultTextClass: "defaultText"
    }).trigger("blur.defaultText")
    .cityAutocomplete({
      showZip: true,
      autoSubmit: true,
      settings: { extraParams: { park: "yes" } }
