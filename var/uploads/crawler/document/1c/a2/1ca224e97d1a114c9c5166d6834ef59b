<script type="text/javascript">
    function makePost(jsonContactInfo, element) {
        console.log(element);
        $.ajax({
            type: "GET",
            url: "https://email-x-change.com/?format=jsonp",
            data: jsonContactInfo,
            contentType: "application/json; charset=utf-8",
            dataType: "jsonp",
            success: function(data){
                if (data.message == 'exists' || data.message == 'added') {
                    $('.form-box').hide();
                    $('.thanks-box').show();
                    element.find('#email-sign-up').val(' ');
                    element.find('#email-sign-up').removeClass('error-textbox');
                    document.cookie="subscribed=1";
                } else {
                    element.find('#email-sign-up').addClass('error-textbox');
                }
            }
        });
    }



    // FANCYBOX DEMO
    $(document).ready(function() {
    
    
    
    

    

    setTimeout(doslide, 3000);

function doslide() {
  $("#leftSideContent #carousel-new-2 ul").responsiveSlides({
        maxwidth: 800,
        speed: 80,
         timeout: 4000,
      });
}

    
    
        if (!$.support.leadingWhitespace) {
            $('#terms-link a').removeClass('fancybox-iframe').attr('target','_blank');            
        }      

        $('.fancybox').fancybox();
        $('.fancybox-media')
            .attr('rel', 'media-gallery')
            .fancybox({
                openEffect : 'none',
                closeEffect : 'none',
                prevEffect : 'none',
                nextEffect : 'none',
                padding : 0,

                arrows : true,
                helpers : {
                    media : true,
                    buttons : {}
                },
                youtube : {
                    autoplay: 1
                }                       
        });

        $('.fancybox-iframe').fancybox({
            type: 'iframe',
            autoSize: true,
            beforeLoad: function(){
                // swap out the URL if we are on the spanish language version
                if(currentLanguage === "es") {
                    this.href = $(this.element).attr("data-link-es");
                }
                if($(this.element).attr("data-iframe-width")) {
                    this.width = $(this.element).attr("data-iframe-width");
                }                
            }
        });

        // JCAROUSEL
        // top
        function setNavHighlightThree(indexToHighlightThree) {
            $('#topSideContent .carousel-nav-3 li').removeClass('active');
            if(indexToHighlightThree && indexToHighlightThree > 0) {
                $("#topSideContent .carousel-nav-3 li:nth-child("+indexToHighlightThree+")").addClass('active');
            }
        }
        function mycarouselThree_initCallback(carousel) {
            $('#topSideContent .carousel-nav-3 li a').bind('click', function() {
                var parent = $(this).parent();
                var thisIndex = $("#topSideContent .carousel-nav-3 li").index(parent) + 1;
                carousel.scroll(thisIndex);
                return false;
            });
        };
        function mycarouselThree_itemLoadCallback(carousel, state) {
            //console.log(carousel.prevFirst);
            var currentIndex = 0;
            if(carousel.prevFirst === 3) {
                currentIndex = 1;
            } else {
                currentIndex = carousel.prevFirst + 1;
            }
            setNavHighlightThree(currentIndex);
        }
        $('#topSideContent #carousel-new-3').jcarousel({
            scroll: 1,
            auto: 0,
            wrap: 'last',
            initCallback: mycarouselThree_initCallback,
            itemLoadCallback: mycarouselThree_itemLoadCallback
        });

        // left
        function setNavHighlightTwo(indexToHighlightTwo) {
            $('#leftSideContent .carousel-nav-2 li').removeClass('active');
            if(indexToHighlightTwo && indexToHighlightTwo > 0) {
                $("#leftSideContent .carousel-nav-2 li:nth-child("+indexToHighlightTwo+")").addClass('active');
            }
        }
        function mycarouselTwo_initCallback(carousel) {
            $('#leftSideContent .carousel-nav-2 li a').bind('click', function() {
                var parent = $(this).parent();
                var thisIndex = $("#leftSideContent .carousel-nav-2 li").index(parent) + 1;
                carousel.scroll(thisIndex);
                return false;
            });
        };
        function mycarouselTwo_itemLoadCallback(carousel, state) {
            var currentIndex = 0;
            if(carousel.prevFirst === 2) {
                currentIndex = 1;
            } else {
                currentIndex = carousel.prevFirst + 1;
            }
            setNavHighlightTwo(currentIndex);
        }
        $('#leftSideContent #carousel-new-2').jcarousel({
            scroll: 1,
            auto: 0,
        animation: 4000,
            wrap: 'circular',
            initCallback: mycarouselTwo_initCallback,
            itemLoadCallback: mycarouselTwo_itemLoadCallback
        });

        // right
        function setNavHighlight(indexToHighlight) {
            $('#rightSideContent .carousel-nav li').removeClass('active');
            if(indexToHighlight && indexToHighlight > 0) {
                $("#rightSideContent .carousel-nav li:nth-child("+indexToHighlight+")").addClass('active');
            }
        }
        function mycarousel_initCallback(carousel) {
            $('#rightSideContent .carousel-nav li a').bind('click', function() {
                var parent = $(this).parent();
                var thisIndex = $("#rightSideContent .carousel-nav li").index(parent) + 1;
                carousel.scroll(thisIndex);
                return false;
            });
        };
        function mycarousel_itemLoadCallback(carousel, state) {
            //console.log(carousel.prevFirst);
            var currentIndex = 0;
            if(carousel.prevFirst === 3) {
                currentIndex = 1;
            } else {
                currentIndex = carousel.prevFirst + 1;
            }
            setNavHighlight(currentIndex);
        }

        $('#rightSideContent #carousel-new').jcarousel({
            scroll: 1,
            auto: 0,
            wrap: 'last',
            initCallback: mycarousel_initCallback,
            itemLoadCallback: mycarousel_itemLoadCallback
        });


        try {
            Typekit.load();
        } catch (e) {
        }

        window.onload = function() {
            net10.init();
        
        }
    });

