function omnitureAddToCartCallback(subcatId, productId, qty)
{
  var fc_tgw = s_gi(s_account);
  fc_tgw.linkTrackVars='products,events';
  fc_tgw.linkTrackEvents='scAdd,scOpen';
  fc_tgw.events='scAdd,scOpen';
  fc_tgw.products=';' + subcatId + ';;;;eVar25=' + productId + '-' + subcatId;
  fc_tgw.tl(this,'o','Cart Add');
  return;
}

function omnitureAddToCartCustomCallback(customProductId, customClubsData)
{
  var fc_tgw = s_gi(s_account);
  fc_tgw.linkTrackVars='products,events';
  fc_tgw.linkTrackEvents='scAdd,scOpen';
  fc_tgw.events='scAdd,scOpen';
  fc_tgw.products=';CustomProd-' + customProductId;
  fc_tgw.tl(this,'o','Cart Add');
  return;
}

function omnitureAddToCartGiftCardCallback(productId, qty)
{
  var fc_tgw = s_gi(s_account);
  fc_tgw.linkTrackVars='products,events';
  fc_tgw.linkTrackEvents='scAdd,scOpen';
  fc_tgw.events='scAdd,scOpen';
  fc_tgw.products=';GiftCard-' + productId + ';;;;eVar25=' + productId + '-4613';
  fc_tgw.tl(this,'o','GCard Add');
  return;
}

function omnitureAddToCartGiftCertCallback(productId, qty, valueDollars)
{
  var fc_tgw = s_gi(s_account);
  fc_tgw.linkTrackVars='products,events';
  fc_tgw.linkTrackEvents='scAdd,scOpen';
  fc_tgw.events='scAdd,scOpen';
  fc_tgw.products=';GiftCert-' + valueDollars + ';;;;eVar25=' + productId + '-836';
  fc_tgw.tl(this,'o','GC Add');
  return;
}

FloatCart.global.registerAddToCartCallback(omnitureAddToCartCallback);
FloatCart.global.registerAddToCartCustomCallback(omnitureAddToCartCustomCallback);
FloatCart.global.registerAddToCartGiftCardCallback(omnitureAddToCartGiftCardCallback);
FloatCart.global.registerAddToCartGiftCertCallback(omnitureAddToCartGiftCertCallback);

