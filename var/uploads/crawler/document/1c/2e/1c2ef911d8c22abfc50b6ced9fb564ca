function MenuHeader(_menuName,_link,_linkStyle,_jsWindowParams,_altText,_tabWidth,_windowWidth,_numColumns,_columnWidths,_styleClassName,_sections)
{
  this.menuName=_menuName;
  this.link=_link;
  this.linkStyle=_linkStyle;
  this.jsWindowParams=_jsWindowParams;
  this.altText=_altText;
  this.tabWidth=_tabWidth;
  this.windowWidth=_windowWidth;
  this.numColumns=_numColumns;
  this.columnWidths=_columnWidths;
  this.styleClassName=_styleClassName;
  this.sections=_sections;
}

function MenuItem(_itemName,_displayName,_link,_linkStyle,_jsWindowParams,_styleClassName,_childMenuItems)
{
  this.itemName=_itemName;
  this.displayName=_displayName;
  this.link=_link;
  this.linkStyle=_linkStyle;
  this.jsWindowParams=_jsWindowParams;
  this.styleClassName=_styleClassName;
  this.childMenuItems=_childMenuItems;
}

