(function() {

  var container = document.querySelector('.module-trending');

  if( container ) {

    var items   = container.getElementsByTagName('li');

    if( items && items.length ) {

      var item;

      for(i = 0, t = items.length; i < t; i++) {

        item    = items[i].querySelector('span');

        if( item && item.clientHeight > 26 ) {

          while( item.clientHeight > 26 ) {

            item.innerHTML = item.innerHTML.substring(0, item.innerHTML.length - 4) + '...'

          }

        }

      }

    }

  }

})();
