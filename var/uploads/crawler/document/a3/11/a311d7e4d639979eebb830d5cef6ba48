    <script type="text/javascript">
        function getUpdateValidation(msg) {
            var validation_html = '';
            $.each(msg.validation, function(key, value) {
                switch(key) {
                    case 'email':
                        $('.overlay .email').addClass('not-valid');
                        if (value == 'empty') {
                            validation_html += 'Please enter your email address<br />';
                        }
                        if (value == 'not-valid') {                                            
                            validation_html += 'Please enter correct email address<br />';
                        }
                        break;
                    case 'zip':
                        $('.overlay .zip').addClass('not-valid');
                        if (value == 'empty') {
                            validation_html += 'Please enter your zip code<br />';
                        }
                        if (value == 'not-valid') {         
                            validation_html += 'Please enter correct Zip Code<br />';
                        }
                        break;
                }
            });
            return validation_html;
        }
		$(function() {
			$('.top-header-menu ul li:last').addClass('donate');

            $('.fancybox')
                .attr('rel', 'media-gallery')
                .fancybox({
                    openEffect : 'none',
                    closeEffect : 'none',
                    prevEffect : 'none',
                    nextEffect : 'none',

                    arrows : false,
                    helpers : {
                        media : {},
                        buttons : {}
                    }
                });

            $('input').placeholder();    

            $("nav select").change(function() {
                window.location = $(this).find("option:selected").val();
            });
            
             
                $("body").bgStretcher({
                    images: ["http://www.randpac.com/wp-content/themes/randpac/img/splash_big.jpg"],
                    // images: ["http://www.randpac.com/wp-content/themes/randpac/img/splash_big.jpg"],
                    imageWidth: 1920,
                    imageHeight: 1080
                });
                $('.overlay .continue').click(function() {
                    // $('.overlay').remove();
                    $.cookie('hide_splash', '1', { expires: 365, path: '/' });
                    location.reload();
                    return false;
                });
                $("body").css("overflow", "hidden");
                // document.body.addEventListener('touchstart', function(e){ e.preventDefault(); });
                if (
                    (window.addEventListener != undefined) && 
                    (this.wheel != undefined)
                ){
                 window.addEventListener('DOMMouseScroll', this.wheel, false);
                }
                if (window.onmousewheel != undefined) {
                    window.onmousewheel = this.wheel;
                }
                if (document.onmousewheel != undefined) {
                    document.onmousewheel = this.wheel;
                }
                $('.overlay form').submit(function() {
                    $('.overlay input').removeClass('not-valid');
                    $('.validation').hide();
                    $.ajax({
                        type: "POST",
                        url: "/wp-content/themes/randpac/ajax/updates.php",
                        dataType: 'json',
                        data: { email: $('.overlay .email').val(), zip: $('.overlay .zip').val() }
                    }).done(function( msg ) {
                        // console.log(msg);
                        if (msg.status == 'ok') {
                            $('.overlay .form').animate({left:'-'+$('.overlay .form').width()+'px'},1000, function() {
                                $('.overlay .form').hide();
                                $('.overlay .form_done').show();
                                $('.overlay .form_done').animate({left:'0px'},1000);
                                $.cookie('hide_splash', '1', { expires: 365, path: '/' });
                                setTimeout(function() {
                                    location.reload();
                                },2000);
                            });
                        } else if (msg.status == 'fail') {
                            validation_html = getUpdateValidation(msg);
                            $('.validation').html(validation_html).show();
                        }
                    });
                    return false;
                });
                if ($(window).width() < 768) {                    
                    $.cookie('hide_splash', '1', { expires: 365, path: '/' });
                    location.reload();
                    return false;
                }
                        $('#newsletter_form').submit(function() {
                $('#mce-FNAME').removeClass('not-valid');
                $('#mce-ZIP').removeClass('not-valid');
                $('.newsletter_form_error').hide();
                $.ajax({
                    type: "POST",
                    dataType: 'json',
                    url: "/wp-content/themes/randpac/ajax/updates.php",
                    data: { email: $('#mce-EMAIL').val(), zip: $('#mce-ZIP').val() }
                }).done(function( msg ) {
                    if (msg.status == 'ok') {
                        $('#newsletter_form').hide();
                        $('.newsletter_form_done').show();                    
                    } else if (msg.status == 'fail') {
                        validation_html = getUpdateValidation(msg);
                        $('.newsletter_form_error').html(validation_html).show();
                    }
                });
                return false;
            });
		});

