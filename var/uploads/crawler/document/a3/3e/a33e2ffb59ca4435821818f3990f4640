        $("#helpheader").append("<div class=\"chat-custserv-header-button\"><a href=\"#\" onClick=\"cmCreateConversionEventTag('Live Chat',1,'Chat',0); cmCreateConversionEventTag('Live Chat',2,'Chat',0); startChatAndCobrowse(gIChannel, gServer, gAttachedData, prefillValues, agentOnlyValues, bEnterOnQueuePage); return false;\"><img src=\"/images/eb/global/pixel.gif\" height=\"186\" width=\"254\" border=\"0\" /></a></div>");
        var siteprotocol = window.location.protocol;
        var fbLoad = '<div class="fb_rollover"><a href="javascript:void(0);" alt="Facebook - Recent Activities" title="Facebook - Recent Activities" onClick="mopen(\'m1\')" onMouseOut="mclosetime()"><img src="//www.champssports.com/images/cs/global/pixel.gif" border="0" height="23" width="23" alt="Facebook" /></a><div id="m1" class="fb_drop" onMouseOver="mcancelclosetime()" onMouseOut="mclosetime()"><div class="fb-recent-activities"><iframe src="//www.facebook.com/plugins/likebox.php?href=http%3A%2F%2Fwww.facebook.com%2Fkidsfootlocker&amp;width=438&amp;height=395&amp;colorscheme=light&amp;show_faces=false&amp;border_color&amp;stream=true&amp;header=false&amp;appId=134365333327857" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:438px; height:395px;" allowTransparency="true"></iframe></div></div></div><iframe src="//www.facebook.com/plugins/like.php?href=https%3A%2F%2Fwww.facebook.com%2Fkidsfootlocker&amp;send=false&amp;layout=button_count&amp;width=83&amp;show_faces=false&amp;action=like&amp;colorscheme=light&amp;font=arial&amp;height=21&amp;appId=134365333327857" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:83px; height:21px;" allowTransparency="true"></iframe>';
        if (siteprotocol == 'http:') {
            $('#header_social').empty();
            $('#header_social').append(fbLoad);
        }
    });
</script>
