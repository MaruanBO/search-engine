			//revmove any URL params after logout only if in shopping cart
			if(thisurl.indexOf("shoppingcart/default.cfm") !== -1){
				thisurl = thisurl.substring(0, thisurl.indexOf('?'));
			}

			window.location=thisurl;
		}
		
