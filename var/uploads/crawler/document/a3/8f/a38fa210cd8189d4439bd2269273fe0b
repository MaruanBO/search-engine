        $(document).ready(function () {
            $('.popup').click(function (e) {
                e.preventDefault();
                var link = $(this);
                var url = link.attr("href");
                var popup = window.open(url, "mywindow", "location=1,status=1,scrollbars=1,width=500,height=400");
                popup.moveTo(100, 200);
            });
        });
