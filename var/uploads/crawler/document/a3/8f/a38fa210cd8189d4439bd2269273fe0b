<script>
    $(document).ready(function () {
        BST.Search.SetBibleBrowseChaptersPartUrl('');
        BST.Search.SetLocalizedTexts('Select Book', 'Select Chapter');
        BST.Search.SetLibrarySubcategoriesUrl('http://www.biblestudytools.com/getreferencesubcategories.json/');
        BST.Search.LoadSearchPartial(".browse-bible-panel", "http://www.biblestudytools.com/search/browse-bible.part/");
        BST.Search.LoadSearchPartial(".bible-search-panel", "http://www.biblestudytools.com/search/bible-search.part/");
        BST.Search.LoadSearchPartial(".reference-search-panel", "http://www.biblestudytools.com/search/reference-search.part/");


        $("#search-dropdown li ul li a").click(function () {
            $("#search-item-active").text($(this).find(".search-item").text());

            var section = $(this).data("section");

            if (section == 'undefined' || section === '') {
                $("#section-selected").attr("disabled", "disabled");
            } else {
                $("#section-selected").val(section);
            }

            $("#headerSearchBox").attr("placeholder", "Search " + $(this).find(".search-item").text());
        });

        $(".toggle-bar a.search-tab").click(function () {
            $(".browse-panel").fadeOut(function () {
                $(".search-panel").fadeIn();
            });

            $(".toggle-bar a").removeClass("active");
            $(this).addClass("active");
        });

        $(".toggle-bar a.browse-tab").click(function () {
            $(".search-panel").fadeOut(function () {
                $(".browse-panel").fadeIn();
            });

            $(".toggle-bar a").removeClass("active");
            $(this).addClass("active");
        });

        $(".browse-tabs a").click(function () {
            $(".browse-tabs a").removeClass("active");
            $(this).addClass("active");
        });
    });
</script>
                </div>
            </div>
        </div>
    </div>
