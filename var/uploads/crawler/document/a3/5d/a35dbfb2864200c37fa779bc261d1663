bMovableBookNowButton.init();
}
if (booking.env.b_action == 'hotel') {
if (booking && booking.google) {
booking.google.trackEvent(booking.google.viewTracker, 'Tab ' + i, booking.env.b_lang_for_url);
if ( i === 4 ) {
booking.track.exp( 'BUNffHIEXO' );
booking.google.trackEvent(booking.google.viewTracker, 'Tab 4 aa', booking.env.b_lang_for_url);
}
}
}
if (i == 4) {
$(".toggle_overview").show();
$(".toggle_review").hide();
} else {
$(".toggle_review").show();
$(".toggle_overview").hide();
}
}
}
function popup( url, w, h ) {
newWindow = window.open( url, 'popupWindow', 'width=' + w + ',height=' + h + ',menubar=no,toolbar=no,location=no,bookmarks=no,status=no,scrollbars=yes,resizable=yes' );
if ( window.focus ) {
newWindow.focus();
}
}
