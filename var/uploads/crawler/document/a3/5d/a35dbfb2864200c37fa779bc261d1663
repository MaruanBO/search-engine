weekDays = {"6":{"short":"Sat","name_from":"Saturday","name_to":"Saturday","name":"Saturday","shortest":"Sa"},"1":{"short":"Mon","name_from":"Monday","name_to":"Monday","name":"Monday","shortest":"Mo"},"4":{"short":"Thurs","name_from":"Thursday","name_to":"Thursday","name":"Thursday","shortest":"Th"},"3":{"short":"Wed","name_from":"Wednesday","name_to":"Wednesday","name":"Wednesday","shortest":"We"},"7":{"short":"Sun","name_from":"Sunday","name_to":"Sunday","name":"Sunday","shortest":"Su"},"2":{"short":"Tues","name_from":"Tuesday","name_to":"Tuesday","name":"Tuesday","shortest":"Tu"},"5":{"short":"Fri","name_from":"Friday","name_to":"Friday","name":"Friday","shortest":"Fr"}};
