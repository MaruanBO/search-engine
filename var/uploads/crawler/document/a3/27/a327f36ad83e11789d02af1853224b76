<script>
function cii_ShowCBLButton(rsSKU,roPrime,roAlt,rnInst,rnRGID,rsLID,rnH,rnW){
  if(!rsSKU){return;}
  var sUSku=rsSKU.toUpperCase(),bOL=true,oL={},aO=[];
  if(!rnRGID){rnRGID=-1;}
  if(!rsLID){rsLID='';}
  if(!oCIICBLDataObject[sUSku]){return;}
  var o=oCIICBLDataObject[sUSku];
  if(!o.dealerCount){return;}
  var nCnt=parseInt(o.dealerCount,10);
  if(nCnt>0&&roPrime){oL=roPrime;}
  else if(nCnt<=0&&roAlt){oL=roAlt;}
  else{return;}
  if(oL.linkurl&&oL.linkurl!==''){aO[aO.length]='<a href="'+ciiPlcHldr(oL.linkurl,rsSKU,rnInst,rnRGID,rsLID)+'">';}
  else if(oL.customlinkurl&&oL.customlinkurl!==''){aO[aO.length]=ciiPlcHldr(oL.customlinkurl,rsSKU,rnInst,rnRGID,rsLID);}
  else if(nCnt>0){
    aO[aO.length]='<a href="JavaScript:manu_CD';
    if(rnRGID>0){
      aO[aO.length]='RuleGroupLink(\x27seagate\x27,\x27'+rsSKU+'\x27,\x27'+rnRGID+'\x27,';
    }else{
      aO[aO.length]='Link(\x27seagate\x27,\x27'+rsSKU+'\x27,';
    }
    aO[aO.length]=(typeof(rnH)==='undefined'||rnH===null?'null':rnH)+','+(typeof(rnW)==='undefined'||rnW===null?'null':rnW)+',null,\x27'+rsLID+'\x27)">';
  }else{bOL=false;}
  if(oL.imageurl&&oL.imageurl!==''){
    aO[aO.length]='<img src="'+oL.imageurl+'" border="0"';
    var oImage=new Image();
    oImage.src=oL.imageurl;
    if(oImage.width&&oImage.width>1&&oImage.height&&oImage.height>1){aO[aO.length]=' height="'+oImage.height+'" width="'+oImage.width+'"';}
    if(oL.onmouseover&&oL.onmouseover!==''){aO[aO.length]=' onMouseOver="'+oL.onmouseover+'"';}
    if(oL.onmouseout&&oL.onmouseout!==''){aO[aO.length]=' onMouseOut="'+oL.onmouseout+'"';}
    aO[aO.length]=' alt="'+(!oL.alt?'':oL.alt)+'" />';
  }
  else if(oL.customimagehtml&&oL.customimagehtml!==''){aO[aO.length]=ciiPlcHldr(oL.customimagehtml,rsSKU,rnInst,rnRGID,rsLID);}
  else if(oL.linktext){aO[aO.length]=ciiPlcHldr(oL.linktext,rsSKU,rnInst,rnRGID,rsLID);}
  if(bOL){aO[aO.length]='</a>';}
  //document.write(aO.join(''));
  return aO;
}
</script>
