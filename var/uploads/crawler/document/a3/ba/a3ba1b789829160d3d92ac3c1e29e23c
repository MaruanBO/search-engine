<script type="text/javascript">
		
		Invodo.init({
			 pageName: "HomePage", 
			 pageType: "Home"
			 });
		

jQuery(document).ready(function(){

	appendOverlayLink();
	addInvodoWidget();
	
	

});

function appendOverlayLink(){
	if(jQuery('a[id^="overlayLink_"]').length){
		jQuery('a[id^="overlayLink_"]').each(function(){
			var idVal=jQuery(this).attr('id');
			var firstIndex=idVal.indexOf("_");
			var prodId=idVal.substring(firstIndex+1);
			if(prodId.length){
				jQuery("#overlayCollections").append('<div id = '+idVal+ ' style="display:none;"></div>');
				jQuery("#videoCollections").append('<div id=videoContainer_'+prodId+ ' style="height:270px;width:480px;">');
			}
			
		});
	}else{
		jQuery("#overlayCollections").append('<div id = "overlayLink" style= "display:none;"></div>');
	
	}
	
}

function addInvodoWidget(){
	
	if(jQuery('a[id^="overlayLink_"]').length){
		jQuery('a[id^="overlayLink_"]').each(function(){
			var idVal=jQuery(this).attr('id');
			var firstIndex=idVal.indexOf("_");
			var refProdId=idVal.substring(firstIndex+1);
			if(refProdId.length){
				
				Invodo.Widget.add({widgetId:"cta1"+refProdId, type:"cta", refid:refProdId, data:document.getElementById("overlayLink_"+refProdId)});
				Invodo.Widget.add({widgetId:"player1"+refProdId, type:"inplayer", mode:"embedded", refid:refProdId, parentDomId:"videoContainer_"+refProdId, autoplay:true, onpodload: function()
					  {
						jQuery("#overlayLink_"+refProdId).show().fancybox(
					    {
					     scrolling: "no",
					     onCleanup: function()
					     {
					      Invodo.Widget.get("player1"+refProdId).stop();
					     }
					    })
					   }
					  });    
					  }
					 });
	}
	else{
		//todo
	}
				
	}
</script>

