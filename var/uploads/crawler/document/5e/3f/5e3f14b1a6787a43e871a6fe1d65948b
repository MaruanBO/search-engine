<script type="text/javascript">
    jQuery('.toplink-10').hoverIntent({
        over: function(){
            animateDropDown('.dropdown-block', true);
        },
        out: function(){
            animateDropDown('.dropdown-block', false);
        },
        timeout:300
    });
    
    function animateDropDown(caller, show) {
        if (show) {
            jQuery(caller).animate({
                opacity: 1,
                height: 'toggle'
            }, 'fast', 'swing');
        } else {
            jQuery(caller).animate({
                opacity: 0,
                height: 'toggle'
            }, 'fast', 'swing');
        }
    }
