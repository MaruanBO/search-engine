<script type="text/javascript">
     
    var auto;
    var speeds = 800;
    jQuery(document).ready(function(){start_banner_timer();});
	    var first = jQuery('.banner-items a:eq(0)').addClass('act').children('img').attr('src');
    jQuery('.billboard').css('background','#D0DAE6 url('+first+') no-repeat top center');
    var firsthref = jQuery('.banner-items a:eq(0)').attr('href');
    jQuery('.shop-link').attr('href',firsthref); jQuery('.part').attr('href',firsthref);
     
    jQuery('.banner-items a').each(function(i) {
        var bgsrc = jQuery(this).children('img').attr('src');
        var href = jQuery(this).attr('href');
        jQuery(this).click(function(){
            jQuery('.billboard').hide();
            jQuery('.banner-items a').removeClass('act');jQuery(this).addClass('act');
            jQuery('.billboard').fadeIn(speeds).css('background','#D0DAE6 url('+bgsrc+') no-repeat top center');
            jQuery('.shop-link').attr('href',href); jQuery('.part').attr('href',href);
            clearInterval(auto);
            return false;
        });
    });
    var k =1;
    function start_banner_timer(){
    auto  = setInterval(function(){
        var n = jQuery('.banner-items a').length;
        if(k >= n) k = 0;
        if(k< n){
            jQuery('.billboard').hide();
            var autosrc = jQuery('.banner-items a:eq('+k+')').children('img').attr('src');
            var autohref = jQuery('.banner-items a:eq('+k+')').attr('href');
            jQuery('.banner-items a').removeClass('act');jQuery('.banner-items a:eq('+k+')').addClass('act');
            jQuery('.billboard').fadeIn(speeds).css('background','#D0DAE6 url('+autosrc+') no-repeat top center');
            jQuery('.shop-link').attr('href',autohref); jQuery('.part').attr('href',autohref);
        k++;}
    },7000);
    }

    jQuery('.shop-now a.arr.prev').click(function(){
        jQuery('.billboard').hide();
        var count = jQuery('.banner-items a').length -1;
        var prev = jQuery(this).parents('.bill-inner').find('a.act').removeClass('act').parents('li').prev().children('a').addClass('act').children('img').attr('src');
        if(prev == undefined) {
            prev = jQuery('.banner-items a:eq('+count+')').addClass('act').children('img').attr('src');
        }
        jQuery('.billboard').fadeIn(speeds).css('background','#D0DAE6 url('+prev+') no-repeat top center');
        var prevhref = jQuery(this).parents('.bill-inner').find('a.act').parents('li').children('a').attr('href');
        jQuery('.shop-link').attr('href',prevhref); jQuery('.part').attr('href',prevhref);
    });
    jQuery('.shop-now a.arr.next').click(function(){
        jQuery('.billboard').hide();
        var next = jQuery(this).parents('.bill-inner').find('a.act').removeClass('act').parents('li').next().children('a').addClass('act').children('img').attr('src');
        if(next == undefined) {
            next = jQuery('.banner-items a:eq(0)').addClass('act').children('img').attr('src');
        }
        jQuery('.billboard').fadeIn(speeds).css('background','#D0DAE6 url('+next+') no-repeat top center');
        var nexthref = jQuery(this).parents('.bill-inner').find('a.act').parents('li').children('a').attr('href');
        jQuery('.shop-link').attr('href',nexthref); jQuery('.part').attr('href',nexthref);
    });
    jQuery('.part.lefts, a.arr.prev').mouseenter(function(){jQuery('a.arr.prev').show();});
    jQuery('.part.lefts, a.arr.prev').mouseleave(function(){jQuery('a.arr.prev').hide();});
    jQuery('.part.rights, a.arr.next').mouseenter(function(){jQuery('a.arr.next').show();});
    jQuery('.part.rights, a.arr.next').mouseleave(function(){jQuery('a.arr.next').hide();});

    //jQuery('.title1 input').focus(function(){jQuery(this).val('');});
    //jQuery('.title1 input').blur(function(){jQuery(this).val('Sign up for tips & savings!');});

    jQuery('.banner-items, .arr').mouseleave(function(){
        clearInterval(auto);
        start_banner_timer();
    });
    jQuery('.banner-items, .arr').mouseenter(function(){
        clearInterval(auto);
    });
	</script>            </div>
