<script type="text/javascript">
		function set_cookie ( name, value, exp_date, path, domain, secure ) {
		var cookie_string = name + "=" + escape ( value );
		expiry = new Date();
		expiry.setTime(1000*exp_date);
		cookie_string += "; expires=" + expiry.toGMTString();
		if ( path ) cookie_string += "; path=" + escape ( path );
		if ( domain ) cookie_string += "; domain=" + escape ( domain );
		if ( secure ) cookie_string += "; secure";
		document.cookie = cookie_string;
	}
	function get_cookie ( cookie_name ) {
		var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );
		if ( results ) return ( unescape ( results[2] ) );
		else return null;
	}
	function show_ad() {
		if (get_cookie('user') != 'yes') {
			set_cookie ( "user", 'yes', 1455917697, '/');
		}
		if (get_cookie('do_not_show_ad') != 'yes' && get_cookie('user') == 'yes' && get_cookie('last_url') != document.location.href) {
			set_cookie('do_not_show_ad', 'yes', 1424381704.2, '/' );
			return true;
		} else {
			return false;
		}
		return false;
	}

	function requestload() {
		$('#wrapper').show();
		$('.qtwrapper').hide();
	}
	
	$(document).ready(function(){
		var AdText = '0';
		if (!get_cookie('last_url')) {
			set_cookie ( "last_url", document.location.href , 1426973697, '/');
		}
		if (document.location.href == 'http://therealdeal.com/' || AdText != '1' || get_cookie('last_url') == document.location.href || !show_ad() ) {
			requestload();
		} else {
			$('.return a').click(function() {
				requestload();
				return false;
			});
			$('#wrapper').hide();
			$('.qtwrapper').show();
			setTimeout( requestload, 1000*7);
		}
		set_cookie ( "last_url", document.location.href , 1426973697, '/');
		
	});
</script>
<style>
	.qtcontent {
		width: 300px;
		height: 250px;
		position:absolute;
		left:50%;
		top:50%;
		margin:-125px 0 0 -150px;
	}
	.ss-inbox {
		width: 300px;
		height: 250px;
		position:absolute;
		overflow: hidden;
	}
	.qtwrapper .return {
		height:58px;
		padding:30px;
	}
	.qtwrapper .return img {
		float:left;
	}
