<div id="uiDialog"></div>

<script>

  $("div#firstMarquee").cycle({
    fx: 'fade',
    timeout: 7000,
    pager:   '#firstMarqueeNav',
    speed: 550, 
    pause:  1,
    pagerAnchorBuilder: marqueeFactory
  });
  $("div#secondMarquee").cycle({
      fx: 'fade',
      timeout: 7000,
      pager:   '#secondMarqueeNav',
      speed: 550, 
      pause:  1,
      pagerAnchorBuilder: marqueeFactory
    });
  function marqueeFactory(idx, slide) {
      return '<a style="display: none" class="lilNavs" href="#">'+(idx+1)+'</a>';
  };
  
  $("div.marqueeNav a").each(function(e){ 
    if (e > 0) {
      $("div.marqueeNav a").css('display','block');
    }
  });
</script>




  
  
  
  
  
  
  

  





  
  

  
  
  
  

  


      
        





  
    
    
  
  

  
