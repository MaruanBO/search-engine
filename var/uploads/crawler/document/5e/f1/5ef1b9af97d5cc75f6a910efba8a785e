        <script type="text/javascript">
            (function($){
                $('.fb-modal-opener').click(function() {
                        var $content = $(this.hash);
                        $content[0].style.width = '';
                        $content.dialog({
                            width: $content.width() || 560,
                            modal: true,
                            draggable: false,
                            resizable: false,
                            dialogClass: 'fb-modal-dialog',
                            open: function() {
                                var popup = $(this);
                                $('.ui-widget-overlay').click(function() {
                                    popup.dialog('close');
                                });
                            }
                        });
                        return false;
                    });
            })(jQuery);

            // Fitbit 101 modal trigger
            $("#wrapper-footer").delegate("a.fb101","click", function(e) {
                var a = e.target,
                    sectionId = a.hash.replace(/^#+/, '');

                // section id allows for deep linking in the modal
                $("#fb101").fitbit101({sectionId: sectionId});
                e.preventDefault();
            });

            // if page requests has "referrer=client" then auto-launch
            

            $(function(){
                var target = $("#profileDropdownTarget"), dd = $("#profileDropdown"), hideTimer;
                if (target.length) {
                    var off = target.offset();
                    dd.appendTo($("body")).css({left: (off.left + 10) + "px", top: (off.top + 20) + "px"});;

                    var show = function(){ hideTimer && clearTimeout(hideTimer); dd.show(); };
                    var hide = function(){ hideTimer && clearTimeout(hideTimer); hideTimer = setTimeout(function(){ dd.hide(); }, 200); };
                    target.hover(show, hide);
                    dd.hover(show, function(ev){ if(ev.relatedTarget !== target[0]) hide(); });
                }
            });
        </script>

        




