                <script type="text/javascript">
                    (function ($) {
                    
                    	function isMobileDetected() {
							return (/Android|iPhone|iPad|iPod|BlackBerry|IEMobile/i).test(navigator.userAgent || navigator.vendor || window.opera);
						}
                    
                        $('.wrapper-header .hdr-menu').click(function(event){
                            if(event.target === this) {
                                $(this).toggleClass('active');
                            }
                        });
						
						if (!isMobileDetected()) {
							$('.wrapper-header .hdr-menu > ul > li').hover(
								function(){
									$(this).addClass('highlight');
								},
								function(){
									$(this).removeClass('highlight');
								}
							);	
						}

                        $('.wrapper-header .hdr-menu > ul > li').click(function(event){
                            if ($(this).hasClass('active')) {
                                $(this).removeClass('active');
                            } else {
                                $('.wrapper-header .hdr-menu > ul > li').removeClass('active');
                                $(this).addClass('active');
                            }
                        });
                    })(jQuery);


                    (function ($) {
                        var lessIe9 = $.browser.msie && parseInt($.browser.version) <= 8;
                        var mediaConditions = ['max-width-1099', 'max-width-767', 'min-width-768', 'min-width-1100'];

                        $('link').each(function () {
                            var match;
                            if ((match = /marketing--((?:max|min)-width-\d+)/i.exec($(this).attr('href')))) {
                                $(this).attr('data-href', $(this).attr('href'));
                                $(this).addClass(match[1]);
                            }
                        });

                        $.each(mediaConditions, function (i, cond) {
                            $('body').prepend($('<div class="media-detect ' + cond + '">&nbsp;</div>'));
                        });

                        var datectTimeout = null;

                        function detect (e) {
                            var widthWin = $(window).width();
                            clearTimeout(datectTimeout);

                            $.each(mediaConditions, function (i, cond) {

                                var num = parseInt(/-(\d+)$/.exec(cond)[1]);
                                var condition = (cond.indexOf('min') == -1) ? widthWin <= num : widthWin >= num;

                                if ($('.media-detect.' + cond).is(':visible') || (lessIe9 && condition)) {
                                    $('link.' + cond).each(function () {$(this).attr('href', $(this).data('href'))});
                                } else {
                                    $('link.' + cond).attr('href', '');
                                }
                            });

                            if (typeof e !== typeof undef) {
                                datectTimeout = setTimeout(detect, 300);
                            }
                        }

                        $(window).on('resize', detect);
                        detect();
                    })(jQuery);

                </script>

                
                
