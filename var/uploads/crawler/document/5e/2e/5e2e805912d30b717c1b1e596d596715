<li id="text-8" class="widget widget_text"><h2 class="widgettitle">Newsletter Signup</h2>			<div class="textwidget"><script type="text/javascript">
                $(document).ready(function() {
                    $('#newsletter-input')
                        .focus(function() {
                            if ($(this).val() == 'Email Address')
                                $(this).val('');
                        })
                        .blur(function() {
                            if ($(this).val() == '')
                                $(this).val('Email Address');
                        });
                });
            </script>
