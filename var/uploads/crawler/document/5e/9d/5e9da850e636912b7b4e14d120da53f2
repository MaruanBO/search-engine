function show_tab(i) {
	for(var j=1;j <= 4;j++) {
		if(j != i) {
			document.getElementById("tab-td-"+j).style.backgroundColor = "#627996";
			document.getElementById("tab-"+j).style.display = "none";
		}
	}
	document.getElementById("tab-td-"+i).style.backgroundColor = "#264367";
	document.getElementById("tab-"+i).style.display = "block";
}

</SCRIPT>

