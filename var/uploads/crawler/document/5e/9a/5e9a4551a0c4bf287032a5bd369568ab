<script type="text/javascript">
	require(['jquery'], function($) {
	$('form#news_subscribe').on('submit', function(e) {
		var $this = $(this);
		e.preventDefault();
		$.ajax({
			type:'POST',
			url: '/ni/ajax/mailings/signup.tcl',
			data: $this.serialize()
		}).done(function(data){
			$('div#subscribeemail > div').prepend(data.msg);
			$this.css('display', 'none');
			$('div#subscribeemail > div').find('#verify_send_again').on('click', function(e){
				e.preventDefault();
				var formData = $this.serialize() + '&retry=1';
				$.ajax({
					type:'POST',
					url: '/ni/ajax/mailings/signup.tcl',
					data: formData
				}).done(function(data){
					$('div#subscribeemail > div').prepend(data.msg);
				});
			})
		}).fail(function(){
			$('div#subscribeemail > div').prepend("<div class='alert alert-danger'>Failed to signup for the Rubylane mailings, Please try again later</div>");
		})
	})
});
</script>


