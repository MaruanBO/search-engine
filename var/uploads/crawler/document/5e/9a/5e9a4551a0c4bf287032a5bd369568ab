		<script type="text/javascript">
		require(['jquery', 'rubylane'], function($, _rl) {
			var _failedLoadAttmp = 0,
				_preloadImgs = [];

			$('.rotator-overlay').ready(function() {
				$('.rotator-overlay img').each(function() {
					if (this.complete || this.readyState === 4) {
						// do on load complete	
						$('.rotator-container').height($('.rotator-overlay').height());
					} else {
						$(this).one('load', function(){	
						// do on load complete	for cached images
							$('.rotator-container').height($('.rotator-overlay').height());
						});
					}
				});
			});

			$(document).ready(function() {
				$('.rotator-container').height($('.rotator-overlay').height());
				$('.rotator').fadeIn(1000);
				$('.rotator ul li:first').fadeIn(1000); // add for IE
			});

			$(window).resize(function() {
				$('.rotator-container').height($('.rotator-overlay').height());
			});

//Preload Images before starting the rotator
			$(window).load(function() {
				cnt = 0;
				imgcnt = $('#siteheader_background li').length || $('#shopheader_background li').length;
				$('#siteheader_background li').each(function(indx, elem){
					imgurl = $(elem).attr('data-lazyload-url')
					if (typeof imgurl != 'undefined') {
						_preloadImgs[cnt] = new Image();
						_preloadImgs[cnt].src = imgurl;
						cnt++;
					}
				})
//Do image check if there is more than one image, otherwise we don't need to rotate
				if (imgcnt > 1) {
					checkImages();
				}
			});

			function checkImages() {
				// Make sure images load before showing next header
				// if we have tried 20 (arbitrary number) failed attempts just show any way (maybe bad image)
				if (_failedLoadAttmp < 20) {
					for(i=0; i < _preloadImgs.length; i++) {
						if (_preloadImgs[i].width == 0 && _preloadImgs[i].height == 0) {
							_tmrChkImg = setTimeout(function() { checkImages() }, 1000);
							_failedLoadAttmp++;
							return 0;
						}
					}
				}
				_failedLoadAttmp = 0;
				_rl.rlstartRotating();
			}

		});
	</script>
