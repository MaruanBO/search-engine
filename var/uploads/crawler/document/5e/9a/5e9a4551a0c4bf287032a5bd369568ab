				var selectionText = $.trim($('#dbselect option:selected').text());
				if (selectionText == "This Week's Arrivals") {
					if ($('span#withinsearch').length != 0) {
						$('span#withinsearch').html("<input type=hidden name=since value='7'>");
					} else {
					$("<span id='withinsearch'><input type=hidden name=since value='7'></span>").prependTo('#nav-search');
					}
				} else if (selectionText == "Today's Arrivals") {
					if ($('span#withinsearch').length != 0) {
						$('span#withinsearch').html("<input type=hidden name=since value='1'>");
					} else {
						$("<span id='withinsearch'><input type=hidden name=since value='1'></span>").prependTo('#nav-search');
					}
				} else if (selectionText == "Ruby Red Tag 50% Off Sale") {
					if ($('span#withinsearch').length != 0) {
						$('span#withinsearch').html("<input type=hidden name=types value='redtag'>");
					} else {
						$("<span id='withinsearch'><input type=hidden name=types value='redtag'></span>").prependTo('#nav-search');
					}
				} else {
					$('#nav-search span#withinsearch').remove();
				}
				$("<input type=hidden name=sb value=go>").prependTo('#nav-search');
				$('#nav-search').submit();
			})
		});
	});
