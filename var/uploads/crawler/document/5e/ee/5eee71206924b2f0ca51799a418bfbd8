<script type="text/javascript">
	/***************************
			Set Cookie
	***************************/
	/* New Set Cookie Method 12/4/2013 - opanitch */
	function setCookie(c_name, value, exdays) {
		var exdate = new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
		document.cookie = c_name + "=" + c_value;
	}
    
	/***************************
			Read Cookie
	***************************/	
	/* New Read Cookie Method 12/4/2013 - opanitch */
    function getCookie(c_name) {
		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + c_name + "=");
		
		if (c_start == -1) {
	  		c_start = c_value.indexOf(c_name + "=");
		}
		
		if (c_start == -1) {
			c_value = null;
		} else {
	  		c_start = c_value.indexOf("=", c_start) + 1;
	  		var c_end = c_value.indexOf(";", c_start);
	  		if (c_end == -1) {
				c_end = c_value.length;
			}
			c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;
	}

	/***************************
			Delete Cookie
	***************************/
	/* Old Delete Cookie Method (not used anyway)
    document._eraseCookie = function(name) {
		console.log('erase cookie');
        createCookie(name, "", -1);
    } */
	
	/***************************
			Check Cookie
	***************************/
	function checkCookie(value) {
		var cookieVal = getCookie(value);
		if (cookieVal != null && cookieVal != "") {
			return true;
		} else {
			return false;
		}
	}
    
    function modalPopupCenter(pageURL, name, w, h) {
        var pg2 = document.getElementsByTagName("iframe");
        for(var i = 0,  p; i < pg2.length; i++) {
        	if(pg2[i].getAttribute("name") == name){
        		pg2 = pg2[i];
        		break;
        	}
        }
        pg2.src = pageURL;
        return true;
    } 

    jQuery(document).ready(function($){
        var ztop = 0, cookieName = "KCP_EMAIL_POPUP", cookieValue = "1";
		//console.log("Pre-Check ", checkCookie(cookieName));
        // ready
        var modal = $("#modal_email").appendTo("body");
        var back = modal.children(".back").eq(0);
        var wind = modal.children(".window").eq(0);
        var wind2 = modal.children(".window").eq(1);
        var lt_ie9 = (!!$.browser.msie && parseInt($.browser.version) <= 8);
		var isCookie = checkCookie(cookieName);
		var flagged;
		if(!isCookie) {
			setCookie(cookieName, cookieValue, 30);
			flagged = true;
		} else if(isCookie && parseInt(getCookie(cookieName)) != parseInt(cookieValue)) {
        	flagged = false;
		} else {
			flagged = true;
		}
		
        /* 
         *  Reason for the computationally expensive loop: 
         *  #headerContainer zIndex: 30000.
         *  Really? 30,000? Seriously?! 30000?! Jeez!!!
         *
         */
        $("div").each(function(){ 
            if($(this).css("display") == "static") return;
            var zcur = parseInt($(this).css("z-index"), 10);
            if(zcur > ztop) {
                ztop = zcur;
            }
        });
        // set
        modal.css({
            background: "none transparent",
            height: $(document).height() + "px",
            left: "0",
            overflow: "visible",
            position: "fixed",
            top: "0",
            width: $(document).width() + "px",
            zIndex: ztop + 1 // see, it only needs to be "1" higher. (30000...)
        }) 
        back.css({
            background: (!lt_ie9?"none rgba(0,0,0,0.5)":
                'transparent url("../images/back_50percent_000000.png") repeat 0 0'),
            height: $(document).height() + "px",
            left: "0",
            position: "relative",
            top: "0",
            width: $(document).width() + "px"
        });
        wind.center = function(){
        	var wnd = !arguments.length? modal.children(".window").eq(0) : $(arguments[0]);
        	var lft = $(window).width()/2 - wnd.outerWidth(false)/2;
            var top = $(window).height()/2 - wnd.outerHeight(false)/2;
            if(lft < 0) lft = 0;
            if(top < 0) top = 0;
        	wnd.css({
        		'left': lft + "px", 
        		'top': top + "px",
        		'z-index': ztop + 1
        	});
        	/*
        	// Firefox position shimmy fix
        	if(browser.gecko && !arguments.length){
        		var t = wnd.find("input[type=text]").get(0);
        		var s = wnd.find(".modalEmailSubmitter").get(0);
        		//t.style.position = "relative";
        		//t.style.top = t.offsetTop == s.offsetTop? "-1px" : "";
        	}
        	*/
        	return wind;
        }
        modal.disable = function(){
            // flag the pop up as disabled
            var duration = !!arguments.length && !isNaN(parseInt(arguments[0]))? arguments[0] : null;
            if(!!duration) setCookie(cookieName, cookieValue, duration);
            modal.css({display:"none"});
        }
        //go
        if(!flagged) { //if flag is false, disable overlay
            modal.disable();
            return;
        }
        modal.find(".closer, #modalEmailForm input[type=submit]").bind("click", function(e){
            e.stopPropagation();
            if(!!this.getAttribute("type") 
                && this.getAttribute("type").toLowerCase() == "submit"
                && !!this.form && !!this.form["email"] && !!this.form["email"].value
                && /^\s*$|Enter(\+|\s)Email/i.test(this.form["email"].value)){
            	return;
            }
            var w = $(this).closest(".window");
            var m = $(this).closest("#modal_email").hide();
            modal.disable(!!this.getAttribute("type") 
                    && this.getAttribute("type").toLowerCase() == "submit"
                    && !!this.form && !!this.form["email"] && !!this.form["email"].value
                    && !/Enter(\+|\s)Email/i.test(this.form["email"].value)? 30 : null );
            if(!!this.getAttribute("type") 
                    && this.getAttribute("type").toLowerCase() == "submit"
                    && !!w.next(".window").children(".email_thanks").length) {
            	var w2 = m.show().children(w).next(".window").removeClass("building");
            	w.hide().addClass("building");
                wind.center($(".window").not(".building"));
            }
			setCookie(cookieName, 0, 30);
        });
        /*
        modal.find("iframe").bind("load", function(e){
        	e.stopPropagation();
        	// ...
        });
        */
        $(window).bind("resize", function(e){
            var h1 = $(document).height();
            var h2 = $(window).height();
            var w1 = $(document).width();
            var w2 = $(window).width();
            back.css({
                height: (h1 >= h2? h1:h2) + "px",
                width: (w1 >= w2? w1:w2) + "px"
            });
            wind.center($(".window").not(".building"));
        });
        wind.center();
        $(window).bind("load", function(){ 
        	wind.removeClass("building").hide().fadeIn();
        });
    });
    

</script>



