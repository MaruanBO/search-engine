			jQuery('#menu-search').fadeToggle("fast");
			jQuery(this).toggleClass('active');
		}
	);
	//search form submission
	jQuery("#searchformtop input").keypress(function(event) {
		if (event.which == 13) {
			event.preventDefault();
			jQuery("#searchformtop").submit();
