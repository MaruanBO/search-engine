<script>
  $(function() {
    var select = $( "#volume" );
    var slider = $( "<div id='slider'></div>" ).insertAfter(select ).slider({
        value: 0,
        min: 1,
        max: 9,
        range: "min",
        value: select[ 0 ].selectedIndex + 1,
        slide: function( event, ui ) {
          change_fields();
          select[ 0 ].selectedIndex = ui.value - 1;
        }
    });

    $("#volume").change(function() {
      slider.slider( "value", this.selectedIndex + 1 );
      change_fields();
    });

    function change_fields() {
      var blockPrice = $("#volume > :selected").attr("data-unit-value");
      var premPrice = $("#volume > :selected").attr("data-premium");
      $(".professional span.ult_price_figure").text("$" + blockPrice + " /mo");
      $(".premium span.ult_price_figure").text("$" + premPrice + " /mo" );
      var monthlyPrice = $("#volume > :selected").attr("data-moprof");
      var premiumPrice = $("#volume > :selected").attr("data-moprem");
      $("#monthlyprof").text("$" + monthlyPrice);
      $("#monthlyprem").text("$" + premiumPrice);
      var annualTickets = $("#volume > :selected").val();
      $("span#tickets").val("Chosen" + annualTickets);
    }
  });
</script>

