    <script type="text/javascript">

       
        var delayb4scrollmaster = 7000 //Specify initial delay before marquee starts to scroll on page (2000=2 seconds)
        var marqueespeedmaster = 1 //Specify marquee scroll speed (larger is faster 1-10)
        var pauseitmaster = 1 //Pause marquee onMousever (0=no. 1=yes)?



        var copyspeedmaster = marqueespeedmaster
        var pausespeedmaster = (pauseitmaster == 0) ? copyspeedmaster : 0
        var actualheightmaster = ''

        function scrollmarqueemaster() {
            if (parseInt(cross_marqueemaster.style.top) > (actualheightmaster * (-1) + 8))
                cross_marqueemaster.style.top = parseInt(cross_marqueemaster.style.top) - copyspeedmaster + "px"
            else
                cross_marqueemaster.style.top = parseInt(marqueeheightmaster) + 8 + "px"
        }

        function initializemarqueemaster() {
            cross_marqueemaster = document.getElementById("vmarqueemaster")
            cross_marqueemaster.style.top = 0
            marqueeheightmaster = document.getElementById("marqueecontainermaster").offsetHeight
            actualheightmaster = cross_marqueemaster.offsetHeight
            if (window.opera || navigator.userAgent.indexOf("Netscape/7") != -1) { //if Opera or Netscape 7x, add scrollbars to scroll and exit
                cross_marqueemaster.style.height = marqueeheightmaster + "px"
                cross_marqueemaster.style.overflow = "scroll"
                return
            }
            setTimeout('lefttime=setInterval("scrollmarqueemaster()", 160)', delayb4scrollmaster)
        }

        if (window.addEventListener)
            window.addEventListener("load", initializemarqueemaster, false)
        else if (window.attachEvent)
            window.attachEvent("onload", initializemarqueemaster)
        else if (document.getElementById)
            window.onload = initializemarqueemaster


</script>

