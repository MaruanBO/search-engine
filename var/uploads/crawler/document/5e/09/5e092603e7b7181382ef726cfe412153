<script type="text/javascript">
$(function() {
	 var show_index = Math.floor(Math.random()*($(".show-rotation").length));
	 $(".show-rotation").eq(show_index).css("display", "block");
	 $(".promo-boxes").equalColumns(".promobox a .promo-content");
	 $(".featured-shows").equalColumns(".simple nav .location-status");
});
</script>
<script type="text/javascript">
(function(){
	$.fn.equalColumns = function(elements){
		var elementArray = elements.split(" ");
		var selector = "";
		
		$(this).each(function(index) {
			height_array = [];
			selector = $(this);
			for(var i = 0; i < elementArray.length; i++){
				selector = $(selector).children(elementArray[i]);
			}
			$(selector).each(function(index) {
				height_array.push($(this).outerHeight(true));
			});

			height_array = height_array.sort().reverse();
			$(selector).css("height",height_array[0]);
		});
	}
})(jQuery);
</script>
