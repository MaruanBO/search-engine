			var carouselMarket = null;
			function setPrefMarket(page, cid, isSlidePlaying, index){
				var CNBCMarketJSON = getMarketCookie(page);
				CNBCMarketJSON[page]['CNBCMarket'] = CNBCMarketJSON[page]['CNBCMarket'] || { };
				CNBCMarketJSON[page]['CNBCMarket'][cid] = { 'play' : isSlidePlaying, 'index': index };
				CNBC_Utils.createCookie('CNBC_UserPreferences', JSON.stringify(CNBCMarketJSON) ,10000, true);
			}
			
			function getMarketCookie(page){
				var CNBCMarketJSON = JSON.parse(CNBC_Utils.readCookie('CNBC_UserPreferences', true));
				CNBCMarketJSON = CNBCMarketJSON || { };
				CNBCMarketJSON[page] = CNBCMarketJSON[page] || { };
				CNBCMarketJSON[page]['CNBCMarket'] = CNBCMarketJSON[page]['CNBCMarket'] || { };
				return CNBCMarketJSON;
			}
			CNBC_Utils.execute('Carousel', function(){
				// instantiate carousel 1
				carouselMarket = new CNBC_Carousel();
				carouselMarket.init({
								container: 'market_component',
								transitionType: 'sliding',
								pageType: 'tab',
								autoTransitionInterval: 8000,
								transitionSpeed: 0.8,
								initialSpeed: 0,
								swipeInitialSpeed: 0,
								carouselWrapper: 'market_component',
								beforeTransition: function() { },
								afterTransition: function(index) {
									var title1 = $('#market_component .carousel_body ul li:nth-child(2)').attr('market_title');
									var url1 = $('#market_component .carousel_body ul li:nth-child(2)').attr('market_title_url');
									$('#market_component .next-title').replaceWith('<a class=\'next-title\' href=\'' + url1 + '\'>' + title1 + '</a>');
									if(!carouselMarket._isSlidePlaying){
										setPrefMarket(CNBC_Settings.pageNodeId, 'market_component', carouselMarket._isSlidePlaying, index);
									}
									
									
			                            var cSymSlide = $($('#market_component .carousel_body li .tickers-container')[1]).find('.tickers');
			                            for(i=0; i < cSymSlide.length; i++){
			                                if($.inArray($(cSymSlide[i]).attr('data-markets-symbol'), CNBC_Quotes.nonStreamableSymbolList) >= 0){
			                                    CNBC_Quotes.reRender(true);
			                                    return;
			                                }
			                            }
		                            
								},
								callBackControl:function(isSlidePlaying, index){
									setPrefMarket(CNBC_Settings.pageNodeId, 'market_component', isSlidePlaying, index);
							    }
				});
			});
			var CMarketJ = getMarketCookie(CNBC_Settings.pageNodeId);
			if(CMarketJ[CNBC_Settings.pageNodeId]['CNBCMarket'] && CMarketJ[CNBC_Settings.pageNodeId]['CNBCMarket']['market_component']){
				carouselMarket.setPlayPause(CMarketJ[CNBC_Settings.pageNodeId]['CNBCMarket']['market_component']['play'], CMarketJ[CNBC_Settings.pageNodeId]['CNBCMarket']['market_component']['index']);
			}else{
				carouselMarket.setPlayPause(false, 0);
			}
			
	
