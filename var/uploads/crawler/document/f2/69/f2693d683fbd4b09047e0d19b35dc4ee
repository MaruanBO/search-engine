								,contentSelector:'> ul'
								,expandText:'SHOW LESS'
								,collapseText:'SHOW MORE'
								,afterToggle: function() {
									var showMoreArrow = $('#tab_contents_tab_2 div.show_more div.show_more_content .icon');
									if(showMoreArrow.hasClass('arrow_down')) {
										showMoreArrow.removeClass('arrow_down').addClass('arrow_up');
										setTopNewsCookie(CNBC_Settings.pageNodeId, 'tab_contents_tab_2', true);
									}
									else if(showMoreArrow.hasClass('arrow_up')) {
										showMoreArrow.removeClass('arrow_up').addClass('arrow_down');
										setTopNewsCookie(CNBC_Settings.pageNodeId, 'tab_contents_tab_2', false);
									}
								}
							});
							var CTopNewsJ = getTopNewsCookie(CNBC_Settings.pageNodeId);
							if(!CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews'] || !CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews']['tab_contents_tab_2'] || CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews']['tab_contents_tab_2']['show']){
								readmore._toggleContent();
