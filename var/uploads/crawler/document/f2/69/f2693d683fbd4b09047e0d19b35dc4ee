					, listeners: [null, tableChartRenderer, marketsRenderer]
				  	, settings: settings
				}
				
				
			]);			
		});
	

	
							//jsinit - 0
				
						CNBC_Utils.execute('read more tabs', function(){
							function setTopNewsCookie(page, cid, isShowTab){
								var CNBCMarketJSON = getTopNewsCookie(page);
								CNBCMarketJSON[page]['CNBCTopNews'] = CNBCMarketJSON[page]['CNBCTopNews'] || { };
								CNBCMarketJSON[page]['CNBCTopNews'][cid] = { 'show' : isShowTab };
								CNBC_Utils.createCookie('CNBC_UserPreferences', JSON.stringify(CNBCMarketJSON) ,10000, true);
							}
							
							function getTopNewsCookie(page){
								var CNBCMarketJSON = JSON.parse(CNBC_Utils.readCookie('CNBC_UserPreferences', true));
								CNBCMarketJSON = CNBCMarketJSON || { };
								CNBCMarketJSON[page] = CNBCMarketJSON[page] || { };
								CNBCMarketJSON[page]['CNBCTopNews'] = CNBCMarketJSON[page]['CNBCTopNews'] || { };
								return CNBCMarketJSON;
							}
							
							
							var readmore = new CNBC_ReadMore();
							readmore.init({
								container:$('#tab_contents_tab_0')
								,toggleSelector:'div.show_more div.show_more_content'
								,contentSelector:'> ul'
								,expandText:'SHOW LESS'
								,collapseText:'SHOW MORE'
								,afterToggle: function() {
									var showMoreArrow = $('#tab_contents_tab_0 div.show_more div.show_more_content .icon');
									if(showMoreArrow.hasClass('arrow_down')) {
										showMoreArrow.removeClass('arrow_down').addClass('arrow_up');
										setTopNewsCookie(CNBC_Settings.pageNodeId, 'tab_contents_tab_0', true);
									}
									else if(showMoreArrow.hasClass('arrow_up')) {
										showMoreArrow.removeClass('arrow_up').addClass('arrow_down');
										setTopNewsCookie(CNBC_Settings.pageNodeId, 'tab_contents_tab_0', false);
									}
								}
							});
							var CTopNewsJ = getTopNewsCookie(CNBC_Settings.pageNodeId);
							if(!CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews'] || !CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews']['tab_contents_tab_0'] || CTopNewsJ[CNBC_Settings.pageNodeId]['CNBCTopNews']['tab_contents_tab_0']['show']){
								readmore._toggleContent();
								readmore._toggleLink();
							}
						});
					
