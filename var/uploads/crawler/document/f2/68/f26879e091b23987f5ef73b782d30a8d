	{
		var mmpi = document.getElementById('myspace-player');
		var mmpis = document.getElementById('myspace-script');
		if (mmpi)
		{
			try { mmpi.parentNode.removeChild(mmpi); } catch (e) {}
		}
		if (mmpis)
		{
			try { mmpis.parentNode.removeChild(mmpis); } catch (e) {}
		}

		var js = document.createElement('script');
		js.setAttribute('id', 'myspace-script');
