		function onPageLoad() {
			onBlurArtist();
			document.FindArtistForm.ArtistScreenname.blur();
			onBlurSchool();
			document.FindSchoolForm.SearchText.blur();
			self.setTimeout("onTimeout()", 1000);
		}
		
		function onTimeout() {
			
			timeoutCounter = timeoutCounter + 1;
			
			// slideshow advances every four timeout 'ticks'
			if ( timeoutCounter % 4 == 0 ) {
				advanceSlideshow();
			}

			
			// AOTW slideshow every two timeout 'ticks'
			if ( timeoutCounter % 4 == 2 ) {
				advanceAotwSlideshow();
			}
			

			// advance banner rotation
			/*
			if ( timeoutCounter % 4 == 0 ) {
				document.getElementById("Banner_Idol").style.display = ( isPromoVisible ? "" : "none" );
				document.getElementById("Banner_Promo").style.display = ( isPromoVisible ? "none" : "" );
				isPromoVisible = !isPromoVisible;
			}
			*/

			// reset the timeout for the next interval
			self.setTimeout("onTimeout()", 1000);
		}

		function advanceSlideshow() {

			var params = art[index].split("|");
			
			var htm = "<a href=\"/museum/art.asp?id=" + params[0] + "\">";
			htm += "<img src=\"http://images.artsonia.com/art/medium/" + params[0] + ".jpg\" border=\"0\" width=\"" + params[1] + "\" height=\"" + params[2] + "\" style=\"border:10px #FFFFFF solid;\">";
			htm += "</a>";
			document.getElementById("divArtImage").innerHTML = htm;

			var htm = "by <a href=\"/museum/art.asp?id=" + params[0] + "\">";
			htm += params[4];
			htm += "</a>";
			if ( params[5] != "" ) {
				htm += ", " + params[5];
			}
			htm += "<br/>";
			htm += params[7];
			document.getElementById("divArtInfo").innerHTML = htm;

			if ( ++index >= art.length ) index = 0;
			//index = Math.floor(Math.random()*art.length);

			var params = art[index].split("|");
			imgs[index].src = "http://images.artsonia.com/art/medium/" + params[0] + ".jpg"
		}
		
		function advanceAotwSlideshow() {
			var newSlotIndex = Math.floor(Math.random()*4);
			while ( newSlotIndex == aotwSlotIndex ) {
				newSlotIndex = newSlotIndex + 1;
				if ( newSlotIndex == 4 ) { newSlotIndex = 0; }
			}
			aotwSlotIndex = newSlotIndex;
			var artID = aotwArtIDs[aotwArtIndex];
			var img = document.getElementById("Finalist" + aotwSlotIndex.toString());
			img.src = "http://images.artsonia.com/art/thumb/" + artID.toString() + ".jpg";
			if ( ++aotwArtIndex >= aotwArtIDs.length ) aotwArtIndex = 0;
			aotwImgs[aotwArtIndex].src = "http://images.artsonia.com/art/thumb/" + aotwArtIDs[aotwArtIndex] + ".jpg";
		}
		
		function advanceIdolSlideshow() {
			var artID = idolArtIDs[idolArtIndex];
			var img = document.getElementById("imgIdolArt");
			img.src = "http://images.artsonia.com/art/small/" + artID.toString() + ".jpg";
			if ( ++idolArtIndex >= idolArtIDs.length ) idolArtIndex = 0;
			idolImgs[idolArtIndex].src = "http://images.artsonia.com/art/small/" + idolArtIDs[idolArtIndex] + ".jpg";
		}

		function onFocusArtist() {
			var obj = document.FindArtistForm.ArtistScreenname;
			if ( obj.value == "enter first name or screenname" ) {
				obj.style.color = "#000000";
				obj.value = "";
			}
		}
		function onBlurArtist() {
			var obj = document.FindArtistForm.ArtistScreenname;
			if ( obj.value == "" ) {
				obj.style.color = "#777777";
				obj.value = "enter first name or screenname";
			}
		}
		function onSubmitArtist() {
			var obj = document.FindArtistForm.ArtistScreenname;
			if ( obj.value == "enter first name or screenname" || obj.value == "" ) {
				alert("You must enter a first name OR screenname.\n\nExample #1: Jenna29\nExample #2: Chandler");
				return(false);
			}
			else {
				return(true);
			}
		}

		function onFocusSchool() {
			var obj = document.FindSchoolForm.SearchText;
			if ( schoolHasDefaultText ) {
				obj.style.color = "#000000";
				obj.value = "";
				schoolHasDefaultText = false;
			}
		}
		function onBlurSchool() {
			var obj = document.FindSchoolForm.SearchText;
			if ( obj.value == "" ) {
				obj.style.color = "#777777";
				obj.value = (schoolTab == "Name") ? "enter any part of school name" : "enter city, state, or zipcode *";
				schoolHasDefaultText = true;
			}
		}
		function onSubmitSchool() {
			var obj = document.FindSchoolForm.SearchText;
			if ( schoolHasDefaultText || obj.value == "" ) {
				alert("You did not enter anything into the school search box.");
				//alert("You must enter a city and state OR zipcode.\n\nExample #1: Chicago, IL\nExample #2: 60031");
				return(false);
			}
			else {
				return(true);
			}
		}

		function onPageUnload() {
		}
		
		function onClickSchoolTab(tab) {
			document.getElementById("TabSearchSchoolByName").style.background = ( tab == "Name" ? "#44aaff" : "" );
			document.getElementById("TabSearchSchoolByName").style.color = ( tab == "Name" ? "#ffffff" : "#3377dd" );
			document.getElementById("TabSearchSchoolByName").style.textDecoration = ( tab == "Name" ? "none" : "underline" );
			document.getElementById("TabSearchSchoolByLocation").style.background = ( tab == "Location" ? "#44aaff" : "" );
			document.getElementById("TabSearchSchoolByLocation").style.color = ( tab == "Location" ? "#ffffff" : "#3377dd" );
			document.getElementById("TabSearchSchoolByLocation").style.textDecoration = ( tab == "Location" ? "none" : "underline" );
			document.getElementById("LinkAdvancedSchoolSearch").style.visibility = ( tab == "Location" ? "" : "hidden" );
			schoolTab = tab;
			document.FindSchoolForm.SearchText.value = "";
			document.FindSchoolForm.action = (tab == "Name") ? "/schools/search.asp" : "/schools/find.asp";
			onBlurSchool();
		}
		
		$(function() {
		
			// Randomly display a banner and rotate through them (if applicable)
			var banners = 0;
			var bannerActive;
			if ( banners > 0 ) {
				bannerActive = Math.floor(Math.random()*banners);
				$("#divPromoBanner"+bannerActive).show();
				if ( banners > 1 ) {
					window.setInterval(function() {
						$("#divPromoBanner"+bannerActive).fadeOut();
						if ( ++bannerActive >= banners ) bannerActive=0;
						$("#divPromoBanner"+bannerActive).fadeIn();
					}, 8000);
				}
			}
		
		});

	</script>
	
