				function headerSearch() {
					var txt = document.getElementById("HeaderSearchText").value;
					txt = txt.replace(/^\s+|\s+$/g, "");
					if ( txt == "" ) {
						alert("You did not type anything into the search box.");
					}
					else {
						location.href = "/search/?qt=" + escape(txt);
					}
				}
			</script>
