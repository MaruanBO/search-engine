    (function($) {
        var newsletterSubscriberFormDetailAjax = new VarienForm('newsletter-validate-detail-footer');

        $("#newsletter-validate-detail-footer").on('submit', function() {
            var form = $(this);
            if (newsletterSubscriberFormDetailAjax.validator.validate()) {
                $.ajax({
                    type: "POST",
                    url: "http://www.charmingcharlie.com/newsletter/subscriber/new/event_area/footer/",
                    data: form.serialize(),
                    success: function(data)
                    {
                        if (data.success) {
                            $.fancybox.open({
                                content  : data.success_message,
                                autoSize : false,
                                width    : 420,
                                height   : 330,
                                padding  : 0
                            });
