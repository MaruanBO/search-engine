<script>
var currentMainSlide = 1;
var hideSlideCSS = { opacity: "0", visibility: "hidden", zIndex: "0" };
var showSlideCSS = { opacity: "1", visibility: "visible", zIndex: "1" };

jQuery('#gallery-thumbs li').hover(
		function() {
			jQuery('#gallery-thumbs li.active').not(this).removeClass('active');
			jQuery(this).addClass('active');
			item = jQuery('#gallery-thumbs li').index(this) + 1;
			if ( currentMainSlide == item ) return;
			jQuery('div.slide-' + item).addClass('active').css(showSlideCSS);
			jQuery('div.slide-' + currentMainSlide).removeClass('active').css(hideSlideCSS);
			currentMainSlide = item;
		},
       function() { }
);
</script>
