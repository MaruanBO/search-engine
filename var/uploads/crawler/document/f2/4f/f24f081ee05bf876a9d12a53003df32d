	<!-- mobile navigation drop-down code -->
	<script>
		jQuery(document).ready(function() {

			// Create the mobile dropdown
			jQuery("<select />").appendTo("div#navigation_mobile");
			jQuery("<option />", {"selected": "selected", "value"   : "", "text"    : "Go to..."}).appendTo("div#navigation_mobile select");

			jQuery("#navigation > .menus > ul > li").each(function() {
				var el = jQuery(this);
				var hasChildren = el.find("> ul"),
				children = el.find("> ul > li");
				if (hasChildren.length) {
					jQuery("<option />", {
						"value": el.find("> a").attr("href"),
						"text": el.find("> a").text()
					}).appendTo("div#navigation_mobile select");
					children.each(function() {

						var el2 = jQuery(this);
						var hasChildren2 = el2.find("> ul"),
						children2 = el2.find("> ul > li");

						if (hasChildren2.length) {
							jQuery("<option />", {
								"value":  jQuery(this).find("> a").attr("href"),
								"text":  "- " + jQuery(this).find("> a").text()
							}).appendTo("div#navigation_mobile select");

							children2.each(function() {
								jQuery("<option />", {
									"value":  jQuery(this).find("> a").attr("href"),
									"text":  "-- " + jQuery(this).find("> a").text()
								}).appendTo("div#navigation_mobile select");

							});



						} else {

							jQuery("<option />", {
								"value":  jQuery(this).find("> a").attr("href"),
								"text": " - " + jQuery(this).text()
							}).appendTo("div#navigation_mobile select");

						}

					});
				} else {
					jQuery("<option />", {
						"value"   : el.find("> a").attr("href"),
						"text"    : el.text()
					}).appendTo("div#navigation_mobile select");
				}
			}); // each top-level navigation element

			jQuery("div#navigation_mobile select").change(function() {
			  window.location = jQuery(this).find("option:selected").val();
			});

		}); // document
	</script>

