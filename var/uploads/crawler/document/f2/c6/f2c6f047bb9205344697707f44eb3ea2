<SCRIPT LANGUAGE="JavaScript"> 
<!--
function nameDefined(ckie,nme)
{
   var splitValues;
   var i;
   for (i=0;i<ckie.length;++i)
   {
      splitValues=ckie[i].split("=");
      if (splitValues[0]==nme) 
	return true;
   }
   return false;
}
function delBlanks(strng)
{
   var result="";
   var i;
   var chrn;
   for (i=0;i<strng.length;++i) {
      chrn=strng.charAt(i);
      if (chrn!=" ") 
	result += chrn;
   }
   return result;
}
function getCookieValue(ckie,nme)
{
   var splitValues;
   var i;
   for(i=0;i<ckie.length;++i) {
      splitValues=ckie[i].split("=");
      if(splitValues[0]==nme) 
	return splitValues[1];
   }
   return "";
}
function testCookie(cname, cvalue) {  //Tests to see if the cookie 
   var cookie=document.cookie;           //with the name and value 
   var chkdCookie=delBlanks(cookie);  //are on the client computer
   var nvpair=chkdCookie.split(";");
   if(nameDefined(nvpair,cname))       //See if the name is in any pair
   {   
      tvalue=getCookieValue(nvpair,cname);  //Gets the value of the cookie
      if (tvalue == cvalue) 
	return true;
      else 
	return false;
   }
   else return false
}
function IsCookieHideMethodsSet() {
   if (testCookie("showMethods", "no")) {
      $('#paymentmessage').hide();		//Hide Methods
      //$('#headermessage').show();		//Show HeaderMessage
   }
   else{
      var futdate = new Date();		//Get the current time and date
      var expdate = futdate.getTime();  //Get the milliseconds since Jan 1, 1970
      expdate += 3600*1000*24*365;  //expires in 365 days(milliseconds)
      futdate.setTime(expdate);
      var newCookie="showMethods=no; path=/;";	//Set the new cookie values up
      newCookie += " expires=" + futdate.toGMTString();
      window.document.cookie=newCookie; //Write the cookie
      $('#paymentmessage').show();		//Show Methods
      //$('#headermessage').hide();		//Hide HeaderMessage
   }
}

IsCookieHideMethodsSet();
//  -->
 
</SCRIPT>
</div>

<script language="javascript">   
CelebrosAutoComplete("Herrsch", "ctl13_txtSearchString", onSelect, "Herrsch-ac.celebros.com", "Herrsch-ac.celebros.com");
function onSelect(aParameter)
{ 
		if ((aParameter["SelectedURL"]!="")&&(aParameter["IsAutoComplete"]))
		{
var sCmp = (encodeURIComponent(aParameter["SelectedURL"]).indexOf("?") == -1) ? "?" : "&";
window.location = aParameter["SelectedURL"] + sCmp + "cmp=cel" + "&trigger=ac";
		}
//else if (window['UITemplateConfigurationMaster'] != undefined) 
//{
//var e = jQueryCel.Event("keypress");
//e.which = 13; e.keyCode = 13;
//			jQueryCel('#ctl13_txtSearchString').trigger(e);
//			return false;
//		}
		else
		{ 
var targetLocation = "/Searchpage.aspx?k=" + encodeURIComponent(aParameter["SelectedQuery"]);
if (aParameter["IsAutoComplete"].toString().toLowerCase() == "true")
targetLocation += "&Trigger=ac";
window.location = targetLocation;	
		}
	}
</script>
