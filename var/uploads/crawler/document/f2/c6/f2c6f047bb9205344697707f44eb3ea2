	$(document).ready(function() {
		$(".arrow").each(function() {
			$(this).html('&#9658;');
		});

	    $('.feature-box-outer .header').mouseenter(function() {
	        var slideout = $(this).find('.slideout');
	        $(this).find('.arrow').html('&#x25BC;');
	        slideout.animate({ top: "52px" }, 1000, function() { });
	    });
	    $('.feature-box-outer .header').mouseleave(function() {
	        var slideout = $(this).find('.slideout');
	        $(this).find('.arrow').html('&#9658;');
	        slideout.stop().animate({ top: "-200px" }, 1000, function() { });
	    });
	
	  var mySwiper = $('.swiper-container').swiper({
	    mode:'horizontal',
	    loop: true,
	    autoplayDisableOnInteraction: false,
	    autoplay: 7000,
	    speed: 500
	  });
	});

	function getOffsetDate(offsetInMintues) {
	  var d = new Date();
	  d.setMinutes(d.getMinutes() + d.getTimezoneOffset() - offsetInMintues);
	  return d;
	}


</script>


