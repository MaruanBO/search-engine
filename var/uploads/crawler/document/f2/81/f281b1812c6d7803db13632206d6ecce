$(function() {
	$("#slide-carousel").cycle({
		fx: 'fade',
		speed: 2000,
		timeout: 6000,
		sync: 1,
		delay: -3000,
		prev: 'a.prev',
		next: 'a.next',
		easing: 'jswing',
		before: function (currSlideElement, nextSlideElement) {
			// if nextSlideElement src is empty, load data-src into src
			var $nextImg = $(nextSlideElement).find(".slide"),
				nextSrc = $nextImg.attr("src");

			if (!nextSrc) {
				$nextImg.attr("src" , $nextImg.data("src"))
			}
		}
	});
});
</script>

