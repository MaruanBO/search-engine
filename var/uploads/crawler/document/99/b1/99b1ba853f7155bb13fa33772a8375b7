    <div id="AdMarvel" style="position: fixed; bottom: 0; overflow: hidden; width: 350px; height: 50px; text-align: center; margin: 0; z-index: 100000; border: solid 0px #000000; background-color: transparent; display: none;">
        <script type="text/javascript">
            function resizeAd()
            {
                // Create the measurement node
                var scrollDiv = document.createElement("div");
                scrollDiv.className = "scrollbar-measure";
                document.body.appendChild(scrollDiv);

                // Get the scrollbar width
                var scrollbarWidth = scrollDiv.offsetWidth - scrollDiv.clientWidth;
                // console.warn(scrollbarWidth); // Mac:  15

                // Delete the DIV
                document.body.removeChild(scrollDiv);

                var clientWidth = 0;
                var screenWidth = 0;
                if( typeof( window.innerWidth ) == 'number' ) {
                    //Non-IE
                    screenWidth = window.innerWidth;
                    clientWidth = document.documentElement.clientWidth;
                } else if( document.documentElement && ( document.documentElement.clientWidth || document.documentElement.clientHeight ) ) {
                    //IE 6+ in 'standards compliant mode'
                    screenWidth = document.documentElement.clientWidth;
                    clientWidth = document.documentElement.clientWidth;
                } else if( document.body && ( document.body.clientWidth || document.body.clientHeight ) ) {
                    //IE 4 compatible
                    screenWidth = document.body.clientWidth;
                    clientWidth = document.body.clientWidth;
                }

                var scale = (screenWidth-scrollbarWidth) / 350;
                var leftOffset = Math.floor(screenWidth / 2) - 190;
                var bottomOffset = Math.floor((50 * scale) / 2) - 25;

                jQuery('#AdMarvel').css('margin-left',leftOffset + 'px');
                jQuery('#AdMarvel').css('transform','scale(' + scale + ',' + scale + ')');
                jQuery('#AdMarvel').css('-ms-transform','scale(' + scale + ',' + scale + ')'); /* IE 9 */
                jQuery('#AdMarvel').css('-moz-transform','scale(' + scale + ',' + scale + ')'); /* Firefox */
                jQuery('#AdMarvel').css('-webkit-transform','scale(' + scale + ',' + scale + ')'); /* Safari and Chrome */
                jQuery('#AdMarvel').css('-o-transform:','scale(' + scale + ',' + scale + ')'); /* Opera */
                jQuery('#AdMarvel').css('margin-bottom', bottomOffset + 'px');
            }

            function AdMarvelInit()
            {
                var AdMarvel = {
                    encode : function (string) {
                        return escape(this._utf8_encode(string));
                    },

                    _utf8_encode : function (string) {
                        string = string.replace(/\r\n/g,"\n");
                        var utftext = "";

                        for (var n = 0; n < string.length; n++) {

                            var c = string.charCodeAt(n);

                            if (c < 128) {
                                utftext += String.fromCharCode(c);
                            }
                            else if((c > 127) && (c < 2048)) {
                                utftext += String.fromCharCode((c >> 6) | 192);
                                utftext += String.fromCharCode((c & 63) | 128);
                            }
                            else {
                                utftext += String.fromCharCode((c >> 12) | 224);
                                utftext += String.fromCharCode(((c >> 6) & 63) | 128);
                                utftext += String.fromCharCode((c & 63) | 128);
                            }
                        }
                        return utftext;
                    }
                }

                var partnerId = "d6be43944c067f30";
                var siteId = "33535";

                jQuery('#AdMarvel').css('display','');
                var m3_u = 'http://ads.admarvel.com/fam/javascriptGetAd.php';
                var m3_r = Math.floor(Math.random()*99999999999);
                document.write ("<scr"+"ipt type='text/javascript' src='"+m3_u);
                document.write ("?partner_id=" + partnerId);
                document.write ('&amp;site_id=' + siteId);
                //document.write ('&amp;target_params=' + targetparams_str);
                document.write ('&amp;version=1.5');
                document.write ('&amp;language=javascript');
                document.write ('&amp;format=wap');
                document.write ('&amp;cb=' + m3_r);
                document.write ("'><\/scr"+"ipt>");

                resizeAd();
                setInterval(function() { resizeAd(); }, 12);
            }

            // var userAgent = "Mozilla/5.0 (iPhone; U; CPU iPhone OS 3_0 like Mac OS X; en-us) AppleWebKit/528.18 (KHTML, like Gecko) Version/4.0 Mobile/7A341 Safari/528.16";
            // var userAgent = "Mozilla/5.0 (Linux; U; Android 2.1; en-us; Nexus One Build/ERD62) AppleWebKit/530.17 (KHTML, like Gecko) Version/4.0 Mobile Safari/530.17";
            (function(a) {
                if (/(Mobile)/i.test(a))
                {
