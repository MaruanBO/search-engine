    <script type="text/javascript">
            //PD-12286 - This adds a wrapper function for the updateEvars created by the CQ SiteCatalyst framework
            CQ_Analytics.CCM.addListener("storesinitialize", function(e) {
                if (CQ_Analytics.CCM.updateWrapper == undefined) {
                    var oldUE = CQ_Analytics.Sitecatalyst.updateEvars;
                    var wrapper = function() {
                        oldUE();
                        try {
                            if (window.omniPageName == '' && window.channel == '') {
                                window.omniPageName = s.pageName;
                                window.channel = s.channel;
                            }
                        } catch (e) {

                        }
                        var uniqueEvents = [];
                        //Here duplicate events are removed, only a problem with page load events
                        $.each(CQ_Analytics.Sitecatalyst.events, function(i, el){
                            if($.inArray(el, uniqueEvents) === -1) uniqueEvents.push(el);
                        });
                        CQ_Analytics.Sitecatalyst.events = uniqueEvents;
                        s.events = uniqueEvents.join();
                    }
                    CQ_Analytics.Sitecatalyst.updateEvars = wrapper;
                    CQ_Analytics.CCM.updateWrapper = true;
                }
            });
    </script>
