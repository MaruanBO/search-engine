        <script type="text/deferred-javascript">
            (function(exports, dom) {
                var head = dom.getElementsByTagName("head")[0];

                function link(url, doc) {
                    var lnk = (doc || dom).createElement("link");

                    lnk.setAttribute("rel", "stylesheet");
                    lnk.setAttribute("type", "text/css");
                    lnk.setAttribute("href", url);

                    return lnk;
                };

                exports.loadStyle = function(url) {
                    var lnk = link(url);
                    head.appendChild(lnk);
                };

                exports.preloadStyle = function(url) {
                    var frame = dom.createElement('iframe');

                    frame.style.display = 'none';
                    dom.body.appendChild(frame);

                    var doc = (frame.contentDocument || frame.contentWindow);

                    if (doc.document)
                        doc = doc.document;

                    var head = (doc.getElementsByTagName("head") || [])[0];
                    head || doc.appendChild(head = doc.createElement('head')); // ie
                    head.appendChild(link(url, doc));
                };
            })(this, document);
        </script>

