    <script type="text/deferred-javascript">
        (function (require) {

            if (!require) {
                return;
            }

            require(['analytics/tracking'], function () {

                api.identity.get(function (err, data) {

                    if (err) {
                        return;
                    }

                    setTimeout(function () {

                        require('bus').emit('account:authenticate', {
                            id: data.id
                        });

                    }, 1000);//todo: think how to avoid timeout and emit event after landing info had been saved

                });

            });

        })(window.require);
    </script>
    <script type="text/deferred-javascript">
        //hack for IE8 and lower against ieapi in apic.
        if (!('postMessage' in window)) {
            globalConfig.redirectOrigin = '';
        }

        jQuery(function($) {
						
            var container = $('.registration-form-block');
            new ad.registration.Index(
                container,
                new system.net.channels.xpost(
                    (globalConfig.redirectOrigin || '') + container.data('action'),
                    (location.origin || (location.protocol + '//' + location.host)) + '/xdpoint.html',
                    window.document,
                    JSON
                ),
                JSON,
                document.getElementById('birthdate-template'),
                '',
							$(document.body).hasClass('cds-7fe09e85') ? ((globalConfig.redirectOrigin || '') + '/free-trial/') : undefined,
							'main',
							document.referrer || '',
							'http://www.globalcompanions.com/StartNow.aspx#/personal-data'
						);
						
        });

        //block below is obsolete (link to free trial was removed from main page)
        (function($, serializer, element) {
            element.click(function(e) {
                var
                formContainer = $('.registration-form-block'),
                data = {
                    "first-name": $('#first-name').val(),
                    "last-name": $('#last-name').val(),
                    "birth-date": {
                        "month": $('[name=month]', formContainer).val(),
                        "day": $('[name=day]', formContainer).val(),
                        "year": $('[name=year]', formContainer).val()
                    },
                    "country": $('[data-name=country] select', formContainer).val(),
                    "email": $('#email').val(),
                    "family-coregistration": $('[name=family-coregistration]', formContainer).prop('checked')
                };
                data['_saved'] = window.name;
                data = JSON.stringify(data);
                window.name = data;
            });
        })(jQuery, JSON, $('.get-free-trial'));

        (function() {
            //due to load acceleration
            var id = setInterval(function() {

                var el = document.body;
                var bodyStyle = window.getComputedStyle ? window.getComputedStyle(el, null) : el.currentStyle;
                if (bodyStyle.display === 'block')
                    clearInterval(id);
                else
                    return;

                var sliderWrap = $('.full-height-container .slider-wrap');
                if (!sliderWrap[0]) return;

                var slider = sliderWrap.find('.slider'),
                    intermediatePosition = sliderWrap.width() - slider.width() + 7,
                    finish = sliderWrap.width() - slider.width();

                if($.support.cssProperty('transition')) {
                    sliderWrap.addClass('transition-on');
                    var eventName,
                        stop = function() {
                            sliderWrap.addClass('stop');
                            slider.css('left', finish);
                            slider.unbind('.slide');
                        };

                    if( $.browser.webkit ) {
                        eventName = "webkitTransitionEnd";
                    } else if( $.browser.mozilla ) {
                        eventName = "transitionend";
                    } else if ($.browser.opera) {
                        eventName = "oTransitionEnd";
                    }

                    eventName += '.slide';
                    slider.bind(eventName, stop)
                    slider.css('left', intermediatePosition);
                } else {
                    var message = slider.find('.end-animation-message');
                    slider.animate({ left: intermediatePosition }, 500, function() {
                        slider.animate({ left: finish }, 250, function(){
                            message.show(250);
                        });
                    });
                }

            }, 15);
        })();

        
    </script>
