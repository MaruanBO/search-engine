<script no-defer type="text/javascript">
(function(window) {
    var document = window.document,
        rcleanScript = /^\s*<!(?:\[CDATA\[|\-\-).*?[\r\n]+/,

        listen = function(el, event, listener) {
            if (el.addEventListener)
                el.addEventListener(event, listener, false);
            else if (el.attachEvent)
                el.attachEvent("on" + event, listener);
        },

        globalEval = function(data) {
            if (data) {
                (window.execScript || function(data) {
                    window["eval"].call(window, data);
                })(data);
            }
        },

        foreach = function(tag, check, cb) {
            var els = document.getElementsByTagName(tag),
                col = [],
                toGo = {left: 0};

            for (var i = 0, l = els.length; i < l; i++) {
                if(check(els[i])) {
                    col.push(els[i]);
                }
            }

            toGo.left = col.length;

            for (var i = 0, l = col.length; i < l; i++) {
                cb(col[i], toGo);
            }
        },

        restrainEval = function(url, el, lab) {
            lab = lab || $LAB.setOptions({ AlwaysPreserveOrder: true, AllowDuplicates: true });

            var parentContext = el.parentNode,
                parentStyle = window.getComputedStyle ? window.getComputedStyle(parentContext, null) : parentContext.currentStyle,
                container = document.createElement((parentStyle && parentStyle.display) === 'inline' ? 'span' : 'div'),
                htmlContent = '';

            parentContext.insertBefore(container, el);

            lab = lab
                    .wait(function() {
                        document._old_write = document.write;
                        document.write = document.writeln = function(html) {
                            htmlContent += html;
                        };
                    })
                    .script({ src: url })
                    .wait(function() {
                        document.write = document.writeln = document._old_write;
                        typeof jQuery !== 'undefined' ? jQuery(container).append(htmlContent) : (container.innerHTML += htmlContent);
                    });

            return lab;
        },

        proceedScripts = function() {
            var els = [],
                foreigns = [],
                elements = document.getElementsByTagName('script'),
                lab = $LAB.setOptions({ AlwaysPreserveOrder: true, AllowDuplicates: true }),
                toGo = 0;

            function tick () {
                if (--toGo === 0) {
                    optimizeComplite('scripts');
                }
            }

            for (var i = 0, l = elements.length; i < l; i++) {
                if (elements[i] && /^(\s*text\/deferred-javascript\s*)?$/.test(elements[i].getAttribute('type') || null/*IE<8 hack*/)) {
                    (elements[i].getAttribute('restrain') != null ? foreigns : els).push(elements[i]);
                }
            }

            els = els.concat(foreigns);
            toGo = els.length;

            for (var j = 0; j < els.length; j++)
                (function() {
                    var el = els[j],
                        parentContext = el.parentNode,
                        src = el.getAttribute('href'),
                        innerScript = el.innerHTML,
                        foreign = el.getAttribute('restrain') != null;

                    if (src && foreign) {
                        restrainEval(src, el, lab).wait(tick);
                    } else {
                        lab = src ? lab.script({ src: src }) : lab.wait(function() {
                            globalEval((innerScript || '').replace(rcleanScript, "/*$0*/"));
                        });
                        lab.wait(tick)
                    }

                    parentContext.removeChild(el);
                }());
        },

        fixStyles = function() {

			var stylesWatchingInterval;

            function isStyle(el) {
                return (el.getAttribute('rel') === 'stylesheet' || el.getAttribute('rel') === 'deferred-stylesheet');
            }

			function checkStylesLoaded() {

				var stylesCount = document.getElementsByTagName('style').length;

				foreach('link', isStyle, function(el, toGo) {

					stylesCount++;

					if(el.getAttribute('rel') === 'deferred-stylesheet')
						el['rel'] = 'stylesheet';
				});

				return document.styleSheets.length === stylesCount;
			}

			if(!checkStylesLoaded()) {
				stylesWatchingInterval = setInterval(function() {

					if(checkStylesLoaded()) {

						clearInterval(stylesWatchingInterval);
						 window.onStylesReady && window.onStylesReady();

					}

				}, 100);
			}
			else {
				window.onStylesReady && window.onStylesReady();
			}

        },

        fixImages = function() {

            function isDefer(el) {
                return !!el.getAttribute('href');
            }

            foreach('img', isDefer, function (el) {
                el.src = el.getAttribute('href');
            });

        },

        fixIFrames = function() {

            function isDefer(el) {
                return !!el.getAttribute('href');
            }

            foreach('iframe', isDefer, function (el) {
                el.src = el.getAttribute('href');
            });

        },

        fixScripts = function() {
            var ct = document.createElement('script');
            ct.type = 'text/javascript';
            ct.async = false;
            ct.src = '//29asb.itocd.net/chat/js/LAB.js?1&v20829dd0';
            ct.onload = ct.onreadystatechange = function() {
                typeof $LAB !== 'undefined' && proceedScripts();
            };
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ct, s);
        };

    window.restrainEval = restrainEval;

    listen(window, 'load', function() {
        window.setTimeout(function() {
            var sink = document.createElement('div');
            sink.style.display = 'none';
            document.body.appendChild(sink);

            document.write = document.writeln = function(html) {
                typeof jQuery !== 'undefined' ? jQuery(sink).append(html) : (sink.innerHTML += html);
            };

            fixIFrames();
            fixStyles();
            fixScripts();
            fixImages();
        }, 0);
    });

	window.onStylesReady = function() {
		optimizeComplite('styles');
	};

})(window);

//Optimization completed event.
var optimizeComplite = (function() {
	var loaded = [];

	function trigger() {
        var args = Array.prototype.slice.call(arguments),
            container = args.shift(),
            event = args.shift();

        container
            .data('event-' + event, JSON.stringify(args))
            .trigger.apply(container, args.unshift(event) && args);
    };

	return function(value) {
		var event = 'optimization:completed';

		loaded.push(value);
		loaded.indexOf('styles') != -1 && loaded.indexOf('scripts') != -1 && setTimeout(function() {trigger($('body'), event);}, 0);
	}

}());
</script>


    
