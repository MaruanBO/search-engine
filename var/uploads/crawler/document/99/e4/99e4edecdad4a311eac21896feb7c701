        jQuery('#content').waypoint(function(direction) {
            if (direction == 'down') {
                $head.attr('class', 'minimize');
                $stop.attr('class', 'visible');
            } else if (direction == 'up') {
                $head.attr('class', 'maximize');
                $stop.attr('class', 'invisible');
