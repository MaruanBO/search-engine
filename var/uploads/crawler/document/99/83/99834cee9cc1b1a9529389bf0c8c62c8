<article class='visible-desktop' id='vendors-near-widget'>
<h2>Featured Vendors Near You</h2>
<ul class='vendors'>
<script type="text/html" id="vendor_near_template">
  <li>
    <a href="/vendor/ww_shortcut/{$T.vendor_id}">
      {#if $T.image_asset_urls.image_sq_130x130_url}
      <img  src='{$T.image_asset_urls.image_sq_130x130_url}' class="vendor-near-image" width="130" height="130">
      {#else}
      <img src="http://cdn2.projectwedding.com/assets/my-reviews/nophoto-220-a445486dbb8d5cc85f8001df57171004.png" class="vendor-near-image" width="130" height="130">
      {#/if}
    </a>
    <div class="info">
      <a class="name" href="/vendor/ww_shortcut/{$T.vendor_id}">{$T.vendor_name}</a>
      <div class="location">{#if $T.locations[0].city}{$T.locations[0].city}, {#/if}{#if $T.locations[0].state_code}{$T.locations[0].state_code}{#/if}</div>
      <div class="review-info">
        <div class=" review-stars " data-reviewvalues="">
  <i class="icon-star-empty"></i><i class="icon-star-empty"></i><i class="icon-star-empty"></i><i class="icon-star-empty"></i><i class="icon-star-empty"></i>
</div>

        <a class="num-reviews" href="/vendor/ww_shortcut/{$T.vendor_id}?tab=reviews">{$T.review_wedding_count}{$T.event_wedding_count}</a>
      </div>
    </div>
  </li>
</script>

</ul>
</article>
</section>
<script>
    $(function(){
        var catalog_url = "http://www.projectwedding.com/vendor/reviews/:location/bands";
  
        $('#find-vendors-module-form').submit(function(){
          var session_loc = "Washington, DC";
  
          var loc = $.trim($('#location').val());
          if(loc.length === 0){
            if(session_loc.length === 0){
              custom_alert("Please enter a location", '', '');
              return false;
            }else{
              loc = session_loc;
            }
          }
          window.location = catalog_url.replace(":location",loc).replace("bands", $('#category').val());
          return false;
        });
  
        var vendorNearTemplate = $.createTemplate($('#vendor_near_template').html(), [], {filter_data:false});
        //console.log("Hitting API with: http://api.weddingwire.com/api/v3/Vendors?category=venue&partner_key=ejcs5rzx&location=Washington%20DC&page_size=2&page=1");
        $.get(    'http://api.weddingwire.com/api/v3/Vendors',
            {
                partner_key: 'ejcs5rzx',
                category: 'venue',
                location: 'Washington DC',
                page_size: '2',
                page: '1'
            },
            function(data){
                //console.log(data);
                if(data.results == null || data.results.length==0){
                    $('#vendors-near-widget').hide();
                }
                if(data.results != null){
                  $('#vendors-near-widget').show();
                    $.each(data.results.slice(0,2), function(i, val) { $('ul.vendors').append(vendorNearTemplate.get(val) ) });
                }
            },
            "jsonp"
        )
    });
</script>

