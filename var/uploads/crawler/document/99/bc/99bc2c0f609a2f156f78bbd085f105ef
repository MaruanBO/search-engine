<script>
$( document ).ready(function() {
	if (typeof(querySt('pmid')) != 'undefined' )
	{
		document.getElementById(querySt('pmid')).className += " active";	
	}
	else if (typeof(querySt('cid')) != 'undefined' )
	{
		document.getElementById(querySt('cid')).className += " active";
	}	
});

function querySt(Key) {
    var url = window.location.href;
    KeysValues = url.split(/[\?&]+/);
    for (i = 0; i < KeysValues.length; i++) {
        KeyValue = KeysValues[i].split("=");
        if (KeyValue[0] == Key) {
            return KeyValue[1];
        }
    }
}
</script>


