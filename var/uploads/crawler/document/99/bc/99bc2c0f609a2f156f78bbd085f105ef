								RR.jsonCallback=function(){				
								var strProdIds;
								var link_urls = [];
								strProdIds = "";
								var objitm;
								var linkurl;
								var stratmsg;
								stratmsg = "";
								var placetype;
								//rr1 is index 0
								if (rr_recs.placements != null)
								{				
									if (rr_recs.placements.length > 0)
									{
									 for (var i = 1; i <= rr_recs.placements.length; i++) {
									   var obj = rr_recs.placements[i-1];		
									   strProdIds = "";
									   if (obj.json)
									   {  
									   	stratmsg = obj.json.strategy_message;
									   }
									   if ( obj.placementType =='home_page.rr1'  ){
	   									   for(var j = 0; j <= obj.json.items.length-1; j++) {						    
										    	objitm = obj.json.items[j];
										    	linkurl = objitm.link_url;
										    	link_urls.push(linkurl);
										    	if (j== obj.json.items.length -1 )
										    	{	
										    		strProdIds = strProdIds + objitm.id ;
										    	}
										    	else
										    	{
										    		strProdIds = strProdIds + objitm.id + ",";
										    	}
										   }
										   
	   									//strProdIds = strProdIds + strProdIds ;
	   									   var retdata;
										   retdata = "";
											jQuery.ajax({
									            async:false,
									            cache:false,
									            url:"http://www.payless.com/on/demandware.store/Sites-payless-Site/default/Product-StoreRRRecProds" + "?rrrecprods=" + strProdIds + "&placementtype=homepgrr" + i + "",
									            success: function(data) {
									            	$('#homepgrr1').html(data);
									            	retdata = data;
													//only for homepage rr1
													if (retdata.length > 0 )
													{	
														l=0;
														$('[id="homepgrr1"]').each(function(){
															//alert($(this).attr('class'));
			
															$(this).find('div').each(function(){
																if ($(this).attr('class')=='product-name')
											   			     	{
																	href = $(this).find('a').attr('href');
							    									$(this).find('a').attr('href',link_urls[l]);
																	l++;
											   			     	}				   			     	
															});
														});
			
														l=0;							
