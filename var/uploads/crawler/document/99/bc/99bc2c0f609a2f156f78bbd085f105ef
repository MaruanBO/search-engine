	$('[id="homepgrr1"]').each(function(){
		$(this).find('ul').each(function(){
			if ($(this).attr('id')=='popular-carousel')
	     	{	
	     		var carousel3 = $(this);
	      		if(!carousel3 || carousel3.length === 0 || carousel3.children().length === 0 || carousel3.children().length <= 3) {
	      			 
	      		}
	      		else
	      		{	
	      			carousel3.jcarousel(app.components.carouselSettings);
	      		}

	     	}
		});
	});
	
	$('[id="homepgrr1"]').each(function(){
		$(this).find('ul').each(function(){
			if ($(this).attr('id')=='popular-carousel')
	     	{	
				if ($(this).find('li').length <= 5) {
			  		var jc = $(this).parent().closest('div').parent().parent().closest('div');
					$(jc).find('div').each(function(){
						if ($(this).attr('class') == "jcarousel-prev jcarousel-prev-horizontal jcarousel-prev-disabled jcarousel-prev-disabled-horizontal"){
					  		$(this).css('display','none');
						} 
						if ($(this).attr('class') == "jcarousel-next jcarousel-next-horizontal jcarousel-next-disabled jcarousel-next-disabled-horizontal"){
							$(this).css('display','none');							
						} 						
					});  		
			  	}
	     	}
		});
	});
	
	$('[id="defpopprod"]').each(function(){
		$(this).find('div').each(function(){
			if ($(this).attr('class')=='eyebrow-container'  || $(this).attr('id')=='BVInlineRatings'  ){
				$(this).remove();
	        } 
		});
			     	
	});
	

