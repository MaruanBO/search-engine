  /**
   * The naming convention for property video: It must be Motel 6 ID in length of 4 (if not, '0' will be padded to the left side. example 0001.flv).
   * @m6id  selected Motel 6 id
   */
  var gm6_videoId = '';
  function playm6videoH(ptype, m6id) {
    gm6_videoId = m6id;
		G6Ga.trackEvent('Videos', 'Homepage', m6id+'.flv')
    var callobj = $jq('#m6HGswf').get(0);
    if(m6id.length < 4) for(i=0 ; i<=(4-m6id.length) ; i++) m6id='0'+m6id;

    var popcontentId = '';
    if (ptype == 'M6' || ptype == 'S6') popcontentId = 'm6playdivbox';
    else popcontentId = 'promoplaydivbox';

    hs.zIndexCounter = 999999;
    hs.width=672;
    hs.height=470;
    hs.htmlExpand(callobj, { contentId: popcontentId } );

    var flashvars = { width: "662", height: "410" };
    var params = { allowscriptaccess: "always", wmode: "transparent" };
    var attributes = { id: "m6video" };
    
    if (ptype == 'M6' || ptype == 'S6') swfobject.embedSWF('/i/videos/wrapper.swf?introPath=m6starter.flv&docPath='+m6id+'.flv', 'm6videobox', "662", "410", "9.0.0", "/i/cfx/playerProductInstall.swf", flashvars, params, attributes);
    else swfobject.embedSWF('/i/videos/wrapper.swf?docPath='+m6id+'.flv', 'promovideobox', "662", "410", "9.0.0", "/i/cfx/playerProductInstall.swf", flashvars, params, attributes);

    try {
      egmTrack(m6id+'.flv');
    } catch(err) {}
  }

  function m6videolinks(arg) {
    if (arg == 'mr') location.href='/ms/check-availability.do?property='+gm6_videoId;
    else if (arg == 'pd') location.href='/reservations/motel_detail.aspx?num='+gm6_videoId;
  }
//]]>
</script>
