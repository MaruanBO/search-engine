    <script type="text/javascript">


        jQuery(document).ready(function () {

            jQuery('.expanding-header a').click(function () {
                var target = jQuery(this).attr('href');
                if (jQuery(this).hasClass('active')) {
                    jQuery(target).slideUp('fast');
                    jQuery(this).removeClass('active');
                    jQuery(this).children('span').html('+');
                } else {
                    jQuery(target).slideDown('fast');
                    jQuery(this).addClass('active');
                    jQuery(this).children('span').html('-');
                }
                return false;
            });

            jQuery('#mobile-toggle a').click(function () {

                if (jQuery(this).hasClass('active')) {
                    jQuery('#mobile_navigation').slideUp('fast');
                    jQuery(this).removeClass('active');
                } else {

                    jQuery('#mobile_navigation').slideDown('fast');
                    jQuery(this).addClass('active');

                }

                return false;

            });

            jQuery('#phone').change(function () {
                val = jQuery(this).val();
                jQuery('.this-number').html(val);
            });

            jQuery('#mobile-nav li a').click(function () {

                if (jQuery(this).siblings().size() > 0) {

                    if (jQuery(this).parent().hasClass('active')) {

                        jQuery(this).parent().children('ul').slideUp('fast');
                        jQuery(this).parent().removeClass('active');

                    } else {

                        jQuery('#mobile-nav li.active ul').slideUp('fast');
                        jQuery('#mobile-nav li.active').removeClass('active');
                        jQuery(this).parent().children('ul').slideDown('fast');
                        jQuery(this).parent().addClass('active');

                    }

                    return false;
                }

            });

            jQuery("#form1").validationEngine({
                'custom_error_messages': {
                    '#email_addr': { 'required': { 'message': "* Email Address is required " } },
                    '#first_name': { 'required': { 'message': "* First Name is required " } },
                    '#last_name': { 'required': { 'message': "* Last Name is required " } },
                    '#zip_code': { 'required': { 'message': "* Zip Code is required " } },
                    '#phone': { 'required': { 'message': "* Phone Number is required " } },
                    '#18orOlder': { 'required': { 'message': "* You must be 18 or older to sign up " } },
                    '#checkone': { 'required': { 'message': "* Please select at least 1 sport " } }
                }
            });



            /* If any of these are checked the validation surrogate checkbox will be checked to satisfy the minimum check boxes */
            $("#baseball").click(function () { checkSports(); });
            $("#archery").click(function () { checkSports(); });
            $("#aerobics").click(function () { checkSports(); });
            $("#basketball").click(function () { checkSports(); });
            $("#backpacking").click(function () { checkSports(); });
            $("#bicycling").click(function () { checkSports(); });
            $("#bowling").click(function () { checkSports(); });
            $("#boating").click(function () { checkSports(); });
            $("#running").click(function () { checkSports(); });
            $("#football").click(function () { checkSports(); });
            $("#camping").click(function () { checkSports(); });
            $("#swimming").click(function () { checkSports(); });
            $("#golf").click(function () { checkSports(); });
            $("#diskgolf").click(function () { checkSports(); });
            $("#tennis").click(function () { checkSports(); });
            $("#hockey").click(function () { checkSports(); });
            $("#fishing").click(function () { checkSports(); });
            $("#weight_training").click(function () { checkSports(); });
            $("#lacrosse").click(function () { checkSports(); });
            $("#hunting").click(function () { checkSports(); });
            $("#soccer").click(function () { checkSports(); });
            $("#iceskating").click(function () { checkSports(); });
            $("#volleyball").click(function () { checkSports(); });
            $("#skiing").click(function () { checkSports(); });
            $("#sledding").click(function () { checkSports(); });
            $("#snowboarding").click(function () { checkSports(); });
            $("#snowmobiling").click(function () { checkSports(); });
        });

        function checkSports() { /* Validation surrogate check */
            if (document.form1.baseball.checked ||
                    document.form1.archery.checked ||
                    document.form1.aerobics.checked ||
                    document.form1.basketball.checked ||
                    document.form1.backpacking.checked ||
                    document.form1.bicycling.checked ||
                    document.form1.bowling.checked ||
                    document.form1.boating.checked ||
                    document.form1.running.checked ||
                    document.form1.football.checked ||
                    document.form1.camping.checked ||
                    document.form1.swimming.checked ||
                    document.form1.golf.checked ||
                    document.form1.diskgolf.checked ||
                    document.form1.tennis.checked ||
                    document.form1.hockey.checked ||
                    document.form1.fishing.checked ||
                    document.form1.weight_training.checked ||
                    document.form1.lacrosse.checked ||
                    document.form1.hunting.checked ||
                    document.form1.soccer.checked ||
                    document.form1.iceskating.checked ||
                    document.form1.volleyball.checked ||
                    document.form1.skiing.checked ||
                    document.form1.sledding.checked ||
                    document.form1.snowboarding.checked ||
                    document.form1.snowmobiling.checked) {
                document.form1.checkone.checked = true;
            } else {
                document.form1.checkone.checked = false;
            }
        }
    </script>

