                                $("#eoyDonation").hide();
                                var writeCookie = {
                                    key : "pbs.eoyCampaign",
                                    value :'true',
                                    options : {
                                         expires : 14,
                                         path : '/',
                                         domain : 'pbs.org',
                                    }
                                }
                                var days = writeCookie.options.expires, t = writeCookie.options.expires = new Date();
                                t.setDate(t.getDate() + days);

                                document.cookie = [encodeURIComponent(writeCookie.key), '=',  encodeURIComponent(writeCookie.value),
                                '; expires=' + writeCookie.options.expires.toUTCString(),
                                '; path=' + writeCookie.options.path,
                                '; domain=' + writeCookie.options.domain].join('');
