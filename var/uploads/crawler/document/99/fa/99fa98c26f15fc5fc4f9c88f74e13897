                document.cookie = 'isMobile=true; expires=; path=/';
                if (expectedMobileURL.indexOf('/mobile') !== -1) {
                    if (getCookie('siteVersion') == '') {
                        fetchStatus(expectedMobileURL, true);
                    } else if (getCookie('siteVersion') == 'mobile') {
                        window.location = expectedMobileURL;
                    }
                } else {
                    if (getCookie('siteVersion') == 'mobile' && expectedMobileURL == '' && document.location.indexOf('.ushmm.org/mobile/') === -1) {
                        // We want a mobile page, there is no redirect url and we are not on a mobile page
                        alert('There is no mobile version of this page. The mobile layout will begin at the next matching page.');
                    }
                }
            } else {
                document.cookie = 'isMobile=false; expires=; path=/';
            }
        }

        // THE ONLY THING WE HAVE TO SET
        // this is the destination page for the mobile redirect
        var expectedMobileURL = '/mobile';

        // the expectedURL is for the mobile site
        if (window.addEventListener) window.addEventListener("load", goMobile(navigator.userAgent||navigator.vendor||window.opera), true);
        else window.onload = goMobile(navigator.userAgent||navigator.vendor||window.opera);

        // if we made it this far we are staying here, please continue
