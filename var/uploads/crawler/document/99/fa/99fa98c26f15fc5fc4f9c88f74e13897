        <script>
        function fetchStatus(address, shouldReturn) {
            var client = new XMLHttpRequest();
            client.onreadystatechange = function() {
                // in case of network errors this might not give reliable results
                if (this.readyState == 4 && shouldReturn) {
                    returnStatus(this, this.status);
                    // We have what we need, break out of any possible loop
                    // (Windows Phone Fix)
                    client.onreadystatechange = null;
                    return;
                }
            };
            client.open("HEAD", address);
            client.send();
        }

        function returnStatus(req, status) {
            if(status == 200) {
                // mobile page exists
                // alert for redirect
                if (confirm('View Mobile Version?')) {
                    // they want to go, so let's set a cookie and go!
                    document.cookie = 'siteVersion=mobile; expires=; path=/';
                    fetchStatus("http://www.ushmm.org/api/switchToMobile", false);
                    window.location = expectedMobileURL;
                } else {
                    // they want to stay so let's remember that and reload!
                    document.cookie = 'siteVersion=full; expires=; path=/';
                    fetchStatus("http://www.ushmm.org/api/switchToFull", false);
                    location.reload();
                }
            } else {
                // stay here, there is no mobile version
            }
        }

        function getCookie(name) {
            var cookie;
            var parts = document.cookie.split(name + "=");
            if (parts.length == 2) {
                cookie = parts.pop().split(";").shift();
            }
            if (cookie !== undefined) {
                return cookie;
            } else {
                return '';
            }
        }

