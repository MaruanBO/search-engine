			touchEnabled: true,
			pager: false,
			displaySlideQty: 1,
			moveSlideQty: 1,
			mode: 'fade',
			speed: 400,
			onBeforeSlide: function(currentSlide, totalSlides){
				var orientation = $('.home-carousel').children('li').eq(currentSlide).attr('class');
				if (orientation=="vertical"){
					$('.bx-wrapper').removeClass('horizontal vertical').addClass('vertical');
				} else {
					$('.bx-wrapper').removeClass('horizontal vertical').addClass('horizontal');
				}
			},
			onAfterSlide: function(currentSlide, totalSlides){
				$('.home-carousel').children('li').hide();
				$('.home-carousel').children('li').eq(currentSlide).show();
			}
		});

		var orientationLoad = "horizontal"; // $('.home-carousel').children('li').eq(0).attr('class');
		if (orientationLoad=="horizontal"){$('.bx-wrapper').addClass('horizontal');} else {$('.bx-wrapper').addClass('vertical');}
	});

</script>
