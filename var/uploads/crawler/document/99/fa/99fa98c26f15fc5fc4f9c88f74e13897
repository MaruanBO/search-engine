        <script type="text/javascript">
        function validate_site_search() {
            var value = document.search.q.value;

            if( value == '' || value == 'Ex: Kristallnacht' )
            {
                alert( "Please enter a search term." );
                document.search.q.value = '';
                document.search.q.focus();
                return false;
            }

            return true;
        }
        </script>

