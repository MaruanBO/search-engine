	var sI = jQuery.noConflict();
	sI(document).ready(function(){
		
		sI('#login-trigger').mouseenter(function() {
		  sI(this).next('#login-content').slideDown();
		  sI(this).find('.arrow').html('&#x25B2;');
		  sI('#username').focus();
		});
		sI('#loginMod').mouseleave(function() {
			if(!sI('#username').is(":focus") && !sI('#passwd').is(":focus")){
				sI('#login-content').slideUp();
				sI('.arrow').html('&#x25BC;');
			}
		});
		var toggle = 0;
		sI('#login-trigger').click(function(){
			sI('#loginMod').unbind('mouseleave');
			
			if(toggle==0){
				toggle++;
				sI(this).next('#login-content').slideDown();
				sI(this).find('.arrow').html('&#x25B2;');
				sI('#username').focus();
			}else{
				toggle--;
				sI('#login-content').slideUp();
				sI('.arrow').html('&#x25BC;');
				sI('#loginMod').mouseleave(function() {
					if(!sI('#username').is(":focus") && !sI('#passwd').is(":focus")){
						sI('#login-content').slideUp();
						sI('.arrow').html('&#x25BC;');
					}
				});
			}
		});
	});
