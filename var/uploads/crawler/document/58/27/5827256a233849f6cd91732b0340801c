<script language="javascript" type="text/javascript">
	$j(document).ready(function () {
		$j(".alphaPager").bind('click', function (evt) {
			evt.preventDefault();
			var agencyLetter = $j(this).attr('alpha-character');
			$j("#agencyDirectory").children().children().filter(":not(.agencyLetter_" + agencyLetter + ")").hide();
			$j(".agencyLetter_" + agencyLetter + "").show();
			$j(".letterPager a").filter(":not(.pager_" + agencyLetter + ")").css("color", "#000000");
			$j(".pager_" + agencyLetter + "").css("color", "#0b75df");
		});
	});
</script>

