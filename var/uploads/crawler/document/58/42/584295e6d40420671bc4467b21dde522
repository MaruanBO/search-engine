<script>
	jQuery(document).ready(function ($) {
	// 20130930JC for C1008603 on 10/2/13
	// replacing 3rd test: .ma-list-cycle (because all sassy lists are static) with blog widget
		yepnope([
			{
			//test for presence of slideshow. slideshow css included in <head> to avoid repaint
				test: $('.ma_slideshow').length,
				yep: ['/resources/scripts/vendor/jquery.cycle.all.js',
						'/resources/scripts/slideshow.js'],
				callback: function(url, result, key) {
					if ( url.match('slideshow.js') ) {
						$('body').addClass('js');
						$('.ma_slideshow').mAss( {timeout: 7000} );
						//  ER181 - slideshow accessibility enhancement - 9/26/2013 YG
						// display the slideshow control if JS is on.
						$('.screenreader').attr('aria-hidden','false');
						$('.slides_control').attr('aria-hidden','false');
						// aria polite on slides container
						$('h2.slideshow').focusin(function () {
							$('div.ma_slideshow  div.slides ul').attr('aria-live', 'polite');
						});
						$('.screenreader button').click(function () {
							// pause the slide show
							$('.ma_slideshow div.slides > ul').cycle('pause');
								// turn off aria-live
								$('div.ma_slideshow  div.slides ul').attr('aria-live', 'off');
							// make all slides visible
							$('.ma_slideshow div.slides > li').removeAttr('style');
							// hide contole below the slideshow
							$('.ma_slideshow div.slides_control').Attr('aria-hidden','true');
							// move role="region" from div.ma_lideshow to div.slides ul to prevent a screen reader to read entire div.ma_lideshow whenever slides change.
							$('.ma_slideshow').removeAttr('role');
							$('.ma_slideshow div.slides ul').Attr('role','region');
						});
						//   end - ER181
					}
				}
			},
			{
			//test for presence of horizontal scrolling slideshow. slideshow css included in <head> to avoid repaint
				test: $('.ma_hshow').length,
				yep: ['/resources/scripts/vendor/jquery.cycle.all.js',
						'/resources/scripts/horizontal-slideshow.js'],
				callback: function(url, result, key) {
					if ( url.match('horizontal-slideshow.js') ) {
						$('body').addClass('js');
						$('.ma_hshow').massGov( {timeout: 0} );
					}
				}
			},
			{
			//test for presence of blog widget. blog widget css included in <head> to avoid repaint
				test: $('.mablog-rss').length,
				yep: ['/resources/scripts/blog-feed.js'],
				callback: function(url, result, key) {
					if ( url.match('blog-feed.js') ) {
						$('body').addClass('js');
						$('.mablog-rss').makeFeed(
							$('.mablog-rss').data('title'),
							$('.mablog-rss').data('agency'),
							$('.mablog-rss').data('display')
						);
					}
				}
			},
			{
			//test for presence of analytics
				test: window._gaq,
				yep: ['/resources/scripts/tracking.js'],
				callback: function(url, result, key) {
					if ( url.match('tracking.js') ) {
					// goog.PNs81 Async Doc
						$('a[href]').each(function(){
							$(this).trackPls();
						});
					}
				}
			},
			{
			//C1010357: 4-16-2014 ER5 JC
			//test for presence of share widget
				test: $('.magov-share').length,
				yep: ['/resources/scripts/share.js'],
				callback: function(url, result, key) {
					if ( url.match('share.js') ) {
						$('body').addClass('js');
						$('.magov-share').maShare();
					}
				}
			},
			{
			  //Toggle Switch for privacy policy update & recommendation module 5/2014
			  //test for presence of recommendation opt in/out switch
			  test: $('.toggle-personalization').length,
			  yep: [ '/resources/scripts/maToggle.js',
						'/resources/scripts/modal.js' ],
			  callback: function(url, result, key) {
				if ( url.match('maToggle.js') ) {
				  $('body').addClass('js');
				  // below bnCookie is FIRST queried and if not set then it's set to true.
				  // as of 20140620 mass.gov sets cookie but once baynote js is launched this line might change to Cookies.get
				  var bnCookie = Cookies.getSet('baynoteOn', 'true'), // respect pre-existing value or set to true if no value exists
				  contentFlag = $('.toggle-switch').hasClass('privacy') ? false : true; // no content to show/hide on privacy page only
				  $('.toggle-personalization').makeToggle({
					 toggleFlag : bnCookie, // determines how switch should be setup initially -- predetermined in global header
					 toggleContent : contentFlag, // is there content to show / hide on switch toggle  -- predetermined above
					 bnFlag: true // flag set to perform bn functionality on toggle
				  });
				}
			  }
			}
		]);

	}); //noConflict()
</script>
