  <script>
    var headers = J('#link-exchange .headers').children(),
        panels = J('#link-exchange .panels').children();
    
    headers.each(function(i){
      var header = J(this), panel = J(panels[i]);
      header.click(function(){
        if (!header.hasClass('on')){
          ANN.toggle(headers.filter('.on'));
          ANN.toggle(panels.filter('.on'));
        }
        ANN.toggle(header);
        ANN.toggle(panel);
      });
    });
    
  </script>

