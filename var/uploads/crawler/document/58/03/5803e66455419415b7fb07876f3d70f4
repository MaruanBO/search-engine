<script type="text/javascript">

var xmlhttp
function  getdetails(cat,sp,url,lowpg)
{
xmlhttp=GetXmlHttpObject();
if (xmlhttp==null)
  {
  alert ("Your browser does not support AJAX!");
  return;
  }
  
xmlhttp.onreadystatechange = function () {
        stateChanged(cat,sp);
    };
	url=url+"?cat="+cat;
url=url+"&lowpg="+lowpg+"&sp="+sp;

xmlhttp.open("GET",url,true);
xmlhttp.send(null);
}

function stateChanged(cat,sp)
{
	if (xmlhttp.readyState==4)
	{
	var ulnames='ul'+cat;
	
	$("#"+ulnames+" li:last-child").remove();
	$("#"+ulnames).append(xmlhttp.responseText).listview('refresh');
	}
}

function GetXmlHttpObject()
{
if (window.XMLHttpRequest)
  {
  // code for IE7+, Firefox, Chrome, Opera, Safari
  return new XMLHttpRequest();
  }
if (window.ActiveXObject)
  {
  // code for IE6, IE5
  return new ActiveXObject("Microsoft.XMLHTTP");
  }
return null;
}

function GetSelectedValue(cat,sp,url,lowpg)
{
	getdetails(cat,sp,url,lowpg)
}

function showsmall_widgets()
{
xmlhttp=GetXmlHttpObject();
if (xmlhttp==null)
  {
  alert ("Your browser does not support AJAX!");
  return;
  }
xmlhttp.onreadystatechange = function () {
sitestateChanged();
    };
siteurl="http://www.medindia.net/includes-rwd/widgets/med-home/small-widgets-mobile.asp"
xmlhttp.open("GET",siteurl,true);
xmlhttp.send(null);
}

function sitestateChanged()
{
	if (xmlhttp.readyState==4)
	{
    $("#loadsmallwid").remove();
	$("#smallwid").append(xmlhttp.responseText).listview('refresh');
	}
}

function pollcheck()									// for Health Poll
{
	var fname = document.Poll;
	var chlen = fname.choice.length;
	var count=0;
	var isSelected=false;
	var selChoice;
	for (count = 0; count < chlen; count++)
	{
		if (fname.choice[count].checked == true)
		{
			selChoice=fname.choice[count].value;
			isSelected=true;
			break;
		}
	}
	if (isSelected == true)
	{
	 document.Poll.action='http://www.medindia.net/poll/index.asp?Choice='+selChoice
       document.Poll.submit()
	return true;
	}
	else
	{
		alert("Please Select A Choice");
		return false;
	}
}
</script>
  
