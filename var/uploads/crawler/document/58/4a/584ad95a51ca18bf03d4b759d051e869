	function AnvPlaylistSlide(divId,playlist,playerInstance,playlistHeight,numVideos) {
		console.log("@AnvPlaylist playlist:",playlist);
		var holderWidth = playlist.length * 211;
		var hiddenWidth = playlist.length * 206;
		var holderPx = holderWidth+"px";
		var hiddenPx = hiddenWidth+"px";		
		var container=document.getElementById(divId);
		container.className="storyplaylist";
		var containerInner0=document.createElement("div");
		containerInner0.id="playListCont";
		containerInner0.className="row-fluid content_wrap";
		var containerInner1=document.createElement("div");
		containerInner1.id="playListMaster";	
		var containerInner2=document.createElement("div");
		containerInner2.id="playListSlide";
		containerInner2.style.width = hiddenPx;
		var containerInner3=document.createElement("div");
		containerInner3.id="holder";	
		containerInner3.style.width = holderPx;
		for(var i=0,len=playlist.length; i<len;	i++) {		
			var entry=playlist[i];
			var itemSpan=document.createElement("div");	
			itemSpan.className="vidPLcont";
			var item=document.createElement("div");
			var thumb=document.createElement("img");
