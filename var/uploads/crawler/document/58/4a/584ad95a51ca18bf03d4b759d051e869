			    }
			    var tabTabs = document.getElementsByClassName('tabTab206418801');
			    for (var i = 0; i < tabTabs.length; i++) {       
			        tabTabs[i].setAttribute('class','tabTab206418801');
			    }
			    // change tabsX into tabs-X in order to find the correct tab content
			    var tabContentIdToShow = element.id+"tab";
			    document.getElementById(tabContentIdToShow).style.display = 'block';
			    document.getElementById(element.id).setAttribute('class','tabTab206418801 active');
			}
			</script>
