							$(function(){
								var max = 10 - 1;
								var random1 = Math.floor((Math.random() * max));
								var random2 = Math.floor((Math.random() * max));
								if (random1 == random2){
									if (random2 == max){
										random2--;
									}else{
										random2++;
									}
								}
								$("#articles_opinion02041420815884428469735 .col-xs-7:eq(" + random1 + ")").removeClass("hide");
								$("#articles_opinion02041420815884428469735 .col-xs-7:eq(" + random2 + ")").removeClass("hide");
							});
						</script>
