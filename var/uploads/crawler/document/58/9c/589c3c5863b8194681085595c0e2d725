<script type="text/javascript">
									var wordArray = new Array('friendship','teammates','competition','leagues','instruction');
									var wordCounter = 0;
									var targetDiv;
									var textCounter;
									var fadeDelay = 3000;
									var fadeSpeed = 1000;

									$(document).ready(function(){
										targetDiv = $('.infoAd a p');
										textCounter = setTimeout("wordChange();", fadeDelay);
										targetDiv.text(wordArray[0]);
									});

									function wordChange(){
										targetDiv.fadeOut(fadeSpeed, function(){
											if(wordCounter == (wordArray.length-1)){
												wordCounter = 0;
											} else {
												wordCounter++;
											}
											
											targetDiv.text(wordArray[wordCounter]).fadeIn(fadeSpeed, function(){
													textCounter = setTimeout("wordChange();", fadeDelay);	
											});

										});
									}
							</script>


						</div>
