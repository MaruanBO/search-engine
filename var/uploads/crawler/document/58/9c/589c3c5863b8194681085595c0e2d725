<script type="text/javascript">

    if ($.cookie('SSO') != null && $.cookie('SSO').toString() != '[object Object]') {
        if ($.cookie('AESSO') != null && $.cookie('AESSO').toString() != '[object Object]') {
            var cookieParams = getParamsFromCookie($.cookie('AESSO'));
            if (cookieParams[2] != $.cookie('SSO')) {
                resetAESSOCookie();
            } else {
                displayWelcomeText(cookieParams);
            }
        } else {
            resetAESSOCookie();
        }
    } else {
        if ($.cookie('AESSO') != null && $.cookie('AESSO').toString() != '[object Object]') {
            $.cookie('AESSO', '', { path: '/', expires: -7, domain: '.usta.com' });
        }
        displayDefault();
    }

    function displayDefault() {
        $('#headerAreYouAMember').css('visibility', 'visible');
        $('#headerLoginOrRenew').css('visibility', 'visible');
    }

    function resetAESSOCookie() {
        $.getJSON('https://ssl.usta.com/ajax/ssoajax.aspx?mode=createaesso&profile=' + encodeURIComponent($.cookie('SSO')) + '&callback=?', function(data) {
            if (data.cookieText != '') {
                $.cookie('AESSO', data.cookieText, { path: '/', domain: '.usta.com' });
                displayWelcomeText(getParamsFromCookie(data.cookieText));
            } else {
                displayDefault();
            }
        });
    }

    function displayWelcomeText(cookieParams) {
        $('#headerWelcomeText').html('<div>Hi, <strong>' + cookieParams[0] + ' ' + cookieParams[1] + '</strong>&nbsp;&nbsp;<a href="https://tennislink.usta.com/Dashboard/Main/Home.aspx">My Tennis Page</a> | <a onclick="doLogOut(); return true;" href="#">Logout</a></div>');
        $('#headerAreYouAMember').css('visibility', 'hidden');
        $('#headerWelcomeText').removeClass('nnLinkEs').addClass('nnLinkEsLoggedIn');
        $('#headerLoginOrRenew').html('<div><a href="https://membership.usta.com/user/main.jsp">Access Member Benefits</a></div>');
        $('#headerLoginOrRenew').removeClass('nnLoginRenew');
        $('#headerLoginOrRenew').addClass('nnAccessMembership');
        $('#headerLoginOrRenew').css('visibility', 'visible');
    }

    function getParamsFromCookie(cookieData) {
        var cookieParams = new Array;
        var splitData = cookieData.split('&');
        for (var i = 0; i < splitData.length; i++) {
            var sParam = splitData[i];
            if (sParam.indexOf('=') > -1) {
                var sKeyValues = sParam.split('=');
                cookieParams[i] = sParam.substring(sParam.indexOf('=') + 1);
            }
        }
        return cookieParams;
    }

    function doLogOut() {
        $.cookie('AESSO', '', { path: '/', expires: -7, domain: '.usta.com' });
        $.cookie('SSO', '', { path: '/', expires: -7, domain: '.usta.com' });
        location.reload(true);
    }
</script>

