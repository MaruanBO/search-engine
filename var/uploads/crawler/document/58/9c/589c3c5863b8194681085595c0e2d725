    function NewsletterSignupRedirect() {
        var aid = document.getElementById("aid").value;
        var n = document.getElementById("n").value;
        var ea = document.getElementById("ea").value;
        var url = "http://ebm.cheetahmail.com/r/regf2?a=0&aid=#aid#&n=#n#&email=#ea#";
        var redirUrl = url.replace("#aid#", aid);
        redirUrl = redirUrl.replace("#n#", n);
        redirUrl = redirUrl.replace("#ea#", ea)
        window.open(redirUrl);
    }

