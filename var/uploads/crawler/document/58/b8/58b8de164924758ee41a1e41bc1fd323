	function mobileCheck() {
		var obj = (document.location + "").toQueryParams("&"); 
		if (typeof obj.set_mobile_pref === 'undefined') {
			if (isMobileDevice() && (Cookie.get("mobile_pref") == null)) {
				Cookie.set('mobile_pref', "mobile", 3600 * 24 * 365 * 10);  
			}
		} else {
			Cookie.set('mobile_pref', obj.set_mobile_pref, 3600 * 24 * 365 * 10);  
		}
	
		if (Cookie.get("mobile_pref") == "mobile") {
			if (typeof mobileRedirectUrl === 'undefined') {
				// window.location.href = "/mobile";
			} else {
				window.location.href = mobileRedirectUrl;
			}
		} 
	}

	mobileCheck();
