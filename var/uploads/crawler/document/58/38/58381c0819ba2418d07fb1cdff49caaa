<script type="text/javascript">
objSearchForm = new qForm("formSearch");
objSearchForm.required("searchString");
objSearchForm.onValidate = _customValidation;
function _customValidation() {
var iChars = "*|,\":<>[]{}`\';()@&$#%\\\/+";
var string = objSearchForm.searchString.getValue();
for (var i=0; i < string.length; i++) {
if (iChars.indexOf(string.charAt(i)) != -1) {
objSearchForm.searchString.throwError("Search String must not contain special characters.");
}
}
if (string == 'search') {
objSearchForm.searchString.throwError("Please enter a search string.");
}
if (string.trim().length < 3) {
objSearchForm.searchString.throwError("Please enter a search string with at least 3 characters.");
}
}
$(function() {
var default_value = 'search';
$("#block\\.search\\.field").focus(function(){
if ($(this).val() == default_value) $(this).val("");
});
$("#block\\.search\\.field").blur(function(){
if ($(this).val() == "") $(this).val(default_value);
});
});
</script>
