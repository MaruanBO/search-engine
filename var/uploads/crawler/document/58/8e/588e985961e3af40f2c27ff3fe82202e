<script language="JavaScript" type="text/javascript">


function pressEnter(evt){
	// Grab the cross browser event
	if(!evt) evt = window.event;

	// Character code of key pressed
	var keyCode = !evt.keyCode ? (!evt.which ? evt.charCode : evt.which) : evt.keyCode;
	
	if (keyCode == 13){
		return mysearch(); 
	}
}



function mysearch() {
	
	//if (document.searchForm.Ntk.options[document.searchForm.Ntk.selectedIndex].value == 'All') { 
	//	document.getElementById("N").value = "0";
	//} else if (document.searchForm.Ntk.options[document.searchForm.Ntk.selectedIndex].value == 'Recipes'){ 
	//	document.getElementById("N").value = "78";
	//} else if (document.searchForm.Ntk.options[document.searchForm.Ntk.selectedIndex].value == 'Products'){ 
	//	document.getElementById("N").value = "77";
	//}
	//alert("using my search"); 
	
	var alertbox = ""; 
	var stripValue = ""; 
	stripValue = document.getElementById("searchField").value;  
	if("Search Wegmans" == stripValue){
		stripValue="";
	}
	alertbox = alertbox + "the value is " + stripValue + "\n"; 
	var stripped = stripValue.replace(/[<^>\[\]+\(\)]/ig,""); 
 	alertbox = alertbox + "the stripped value " + stripped + "\n"; 
	//stripped = escape(stripped);
	//alertbox = alertbox + "the escaped value is " + stripped + "\n";  
	stripped = stripped.replace(/%20/ig, " "); 
	alertbox = alertbox + "the second stripped value is " + stripped + "\n"; 
	//alert(alertbox); 
	document.getElementById("nav_Ntt").value = stripped;
	document.getElementById("searchField").value = ""; 
	//document.getElementById("D").value = stripped;
	
	if( stripped.toLowerCase().indexOf("recipe") != -1 || stripped.toLowerCase().indexOf("recipes") != -1 )
	{
		var recipeSearchTerm = stripped.substring(0, stripped.toLowerCase().indexOf("recipe")); // removing 'recipe' from terms like 'chicken recipe' and just search for 'chicken' on Recipe Search
		
		recipeSearchTerm = recipeSearchTerm.replace(/^\s+|\s+$/g,""); // trim search term
		
		if( recipeSearchTerm.length > 0 )
		{
			document.getElementById("nav_Ntt").value = recipeSearchTerm;
			document.getElementById("nav_forwardto").value="RecipesView";
		}
	}
	
	sendOmnitureHeaderSearchInfo("event10", stripped); 
	document.searchForm.submit(); 
	return false;
}				
</script>



