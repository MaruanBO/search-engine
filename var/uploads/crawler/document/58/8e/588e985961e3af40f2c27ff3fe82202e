<div id="checkCookieVal" style="display:none;">false</div>

<script type="text/javascript">

	$(document).ready(function(){	
		var cookieVal = jQuery( "#checkCookieVal" ).html();
		
		if( cookieVal == 'true' && readCookie( 'cleanupCookie' ) == null )
		{
			setTimeout("openCleanupModal()", 2500);
		}
		/*
		if( cookieVal == 'true' && readCookie( 'cleanupCookie' ) == null )
		{
			jQuery(".cleanup-login").cleanuplogin();
			jQuery(".cleanup-login").cleanuplogin("open");
		}
		*/
		
		jQuery( ".modal-cleanuplogin" ).siblings( ".modalCloseImg" ).click( function() {			
			modalCloseLogic();
		});
		
		jQuery("#simplemodal-overlay").bind("click.simplemodal", function() {
			modalCloseLogic();
		});
	});
	
	function openCleanupModal() {
			jQuery(".cleanup-login").cleanuplogin();
			jQuery(".cleanup-login").cleanuplogin("open");
	}

	function modalCloseLogic() {

		jQuery.ajax({
			type: "POST",
			url: "RunServletCodeView",
			data: "codeToRun=getOmnitureUserInfo&catalogId=10002&langId=-1&storeId=10052",
			async: false,
			success: function(data){				
				var allHtml = jQuery( data );
				var myStore = allHtml.find("#ajaxUserStore").html();
				var myPref = allHtml.find("#ajaxUserPref").html();
				var myState = allHtml.find("#ajaxUserState").html();
				
				if( myStore == "" ) myStore = "Unknown";
				if( myPref == "" ) myPref = "Unknown";
				if( myState == "" ) myState = "Unknown";
				
				if( myState != "Unknown" ) // REGISTERED USER
				{
					jQuery.ajax({
						type: "POST",
						url: "RunServletCodeView",
						data: "codeToRun=setMemberId11&catalogId=10002&langId=-1&storeId=10052",
						async: false,
						success: function(data){},
						error: function (XMLHttpRequest, textStatus, errorThrown) {}
					});	
				}
				
				createCookie('cleanupCookie','wegmans',365);					
				sendOmnitureCleanupXClick( "event70", myStore, myPref, myState );		
			},
			error: function (XMLHttpRequest, textStatus, errorThrown) {}
		});
		
		window.location.reload();
	}
	
</script> 


