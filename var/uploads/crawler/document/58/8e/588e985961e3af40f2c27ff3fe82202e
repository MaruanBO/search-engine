		<script type="text/javascript">
			var storeDetailLoaded = false;

			function positionStoreDetail() {
				var offset = jQuery("#identity").offset();
				jQuery("#find-store-over").css({ left: (offset.left + 84) + "px", top: (offset.top + 51) + "px" });
			}

			function showStoreDetail() {



				selectStore();

	

				sendOmnitureFindStoreClick('event11');
			}

			function hideStoreDetail() {
				jQuery("#find-store").show();
				jQuery("#find-store-closer").hide();
				jQuery("#find-store-over").hide();
				jQuery("#find-store-overlay").hide();
				jQuery(document).unbind("keyup", checkKeystroke);
				jQuery(window).unbind("resize", positionStoreDetail);
			}

			function selectStore() {
				jQuery(".storechooser").storechooser();
				jQuery(".storechooser").storechooser("open", "WegStoreChooserStartView?storeId=10052&catalogId=10002&langId=-1");
			}

			function selectDifferentStore() {
					hideStoreDetail();
					selectStore();
					return false;
			}
			
			function storeChangeInfoUpdate(){
	
		jQuery.ajax({
		type: "POST",
		url: "ProductDetailsOmnitureAJAXView",
		data: "idsToCheck=ITEM_123456&catalogId=10002&langId=-1&storeId=10052",
		async: false,
		success: function(data){
			console.log(data);
			var allHtml = jQuery( data );
			console.log(allHtml.find("#myAccounttoreName").html());
			var myAccounttoreName = allHtml.find("#myAccounttoreName").html();
			$('#myAccounttoreName').html("<b>"+myAccounttoreName+"</b>");
			console.log(myAccounttoreName);
			var address1 = allHtml.find("#address1").html();
			var address2 = allHtml.find("#address2").html();
			var city = allHtml.find("#city").html();
			var state = allHtml.find("#state").html();
			var zipCode = allHtml.find("#zipCode").html();
			var phone = allHtml.find("#phone").html();			
		},
		error: function (XMLHttpRequest, textStatus, errorThrown) {
			console.log(errorThrown);
		}
	});
	}

			function checkKeystroke(evt) {
				if (evt.which == 27) // check for ESC pressed
					hideStoreDetail();
			}

			jQuery(function() {
				jQuery("body").prepend(jQuery("#find-store-over").detach());
				jQuery("#find-store-over").wrap(jQuery("<div></div>").addClass("refresh-2009"));

				jQuery("#find-store, #myStoreName a").click(function () { showStoreDetail(); return false; });
				jQuery("#find-store-closer").click(function () { hideStoreDetail(); return false; });

				jQuery("#find-store").keyup(function(evt) { if (evt.which == 13) showStoreDetail(); });
			});
		</script>

