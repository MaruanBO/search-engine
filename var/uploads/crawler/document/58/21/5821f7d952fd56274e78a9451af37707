<link type="text/css" rel="stylesheet" href="http://cdn.collegebound.net/siteadmin/css/jqueryui.css"/>
<script type="text/javascript" src="//cdn.collegebound.net/js/jquery-1.7.2.min.js"></script>
<!--  <script type="text/javascript" src="http://cdn.collegebound.net/findtherightjob/mobile/js/jobCategoryTitleLoad.js"></script>-->
<script type="text/javascript" src="http://cdn.collegebound.net/js/jquery.bgiframe.min.js"></script>
<script src="http://cdn.collegebound.net/js/jquery/jquery.validate.min.js" type="text/javascript"></script>
<script src="http://cdn.collegebound.net/js/jquery/jquery.validate-base.min.js" type="text/javascript"></script>
<script src="http://cdn.collegebound.net/siteadmin/js/jquery-ui-custom.min.js" type="text/javascript"></script>
<script>
var campaignCategoryId = '';
var pre_selectedJobCategory = '';
var pre_selectedJobTitle = '';
var campaign_JobCategory = '';
var campaign_JobTitle = '';
var campaign_JobTitleName = "";

$(document).ready( function() {


    $('form').validate();
    
    function formatItem(row) {
        return row[0] + " (<strong>id: " + row[1] + "</strong>)";
    }
    function formatResult(row) {
        return row[0].replace(/(<.+?>)/gi, '');
    }

    $("#job_title_name").focus(function() {
        if (this.value == 'See All'){
            this.select();
        } else if (this.value == ''){
            this.focus();
        }
    });
    /*jQuery.extend(jQuery.validator.messages, {
        required: ""
    });*/
});
var job_category = "";
var job_title = "";
var school_type = "";
if(typeof $('#pre_selectedJobCategory').val() != "undefined"){
    var pre_selectedJobCategory = $('#pre_selectedJobCategory').val();
}
if(typeof $('#pre_selectedJobTitle').val() != "undefined"){
    var pre_selectedJobTitle  = $('#pre_selectedJobTitle').val();
}
function bySortedValue(obj, context) {
var tuples = [];
for (var key in obj) tuples.push([key, obj[key].profession_name, obj[key].image, obj[key].profession_text]);
tuples.sort(function(a, b) { return a[1] > b[1] ? 1 : a[1] < b[1] ? -1 : 0 });
return tuples;
}

function bySortedCategory(obj, context){
  var tuples = [];
  for (var key in obj) tuples.push([key, obj[key].profession_name, obj[key].image, obj[key].description]);
  tuples.sort(function(a, b) { return a[1] > b[1] ? 1 : a[1] < b[1] ? -1 : 0 });
  return tuples;
}

images_by_cat   = [];
images_by_title = [];
descr_by_title  = [];
states = false;
$(document).ready( function() {
	if(campaign_JobCategory == '2019'){
        $('#job_title_name').val(campaign_JobTitleName);    
        $('#job_header').html('Internships');    
        $('#btm-category_' + campaign_JobCategory).show();     
        loadJobTitles(campaign_JobTitle); 
        $('#zip').focus();  
   } else if($("#job_category").val() != '' && $('#seo_home').html() == null){
		$('#btm-category_' + campaign_JobTitle).show();
        loadJobTitles(campaign_JobTitle);
        $('#zip').focus();    
    }else if(campaign_JobCategory != ''){
        $('#job_title_name').val(campaign_JobTitleName);    
        $('#btm-category_' + campaign_JobCategory).show();    	
        $('#zip').focus();
    } else if($('#seo_home').html() != null) { 
    	$('.btm-category').hide();   
    } else {
        $('#btm-category_0').show();
        $('#zip').focus();
    }
	//set a flag to show a featured job instead of a school
	if(campaignCategoryId == '17799'){
        $('<input>').attr({
            type: 'hidden',
            id: 'showFeaturedJob',
            name: 'showFeaturedJob',
            value: '1'
        }).appendTo('#form1');
		//$('#showFeaturedJob').val("1");
	}
	$("#job_category").change(function() {
        $("#search_header").html('');

        if($('#seo_home').html() != null) { 
            $('.btm-category').hide();   
            loadJobTitles();
        } else if($("#job_category").val() != ''){
            loadJobTitles();
            $('.btm-category').hide(); //hide all
            $('#btm-category_' + $("#job_category").val()).show();
            $("#search_header").html('ALL');
        } else {
            $('#job_title').html('');
           // $("<option>").attr("value", "").text("See All").appendTo("#job_title");
            $("#search_header").html('ALL');
        }
        if($("#job_category").val() == '2019'){
            //internships
            $("#job_header").html('Internships');

        } else {
            $("#job_header").html('Jobs');
        }
        if($("#job_category").val() != '789' && $('.searchbox').css("margin-left") != '155px' && states == false){
            if($("#seo_home").html() == null){
                //$("#company-logo-div").hide();
                //$('.searchbox').css({"width": "550px"});
                //$('.searchbox').animate({"margin-left": "+=120px"}, "slow");
            }
        }

        //changeCategoryImage();
       //changeDescription();
        if($("#job_category").val() != ''){
            $("#category-title").html($('#job_category option:selected').text());
        } else {
            $("#category-title").html('<span style="font-size: 26px">The Right Job, The Right You</span>');
        }
    });
    $("#job_title").change(function() {
    	 if($("#job_category").val() == '789' && $('.searchbox').css("margin-left") != '60px' && states == false){
            changeImage(true);
        } else {
            changeImage();
        }
        changeDescription();
        $("#search_header").html($('#job_title option:selected').text());
    });
        
    if($('#campaign_id').val() == '13274835'){
    	$('#job_title_name').val('');
    	$('#job_title_name').addClass('required');
        $('#job_title_name').focus();
    }
    
});
function loadJobTitles(job_title) {
    $('#job_title').html('');
    var matched_program = '';
    var required = false;
    //check if this is a required field if not change text to choose one
    var classList =$('#job_title').attr('class').split(/\s+/);
    $.each( classList, function(index, item){
        if (item === 'required') {
            required = true;
         }
     });
    if(required){
        $("<option>").attr("value", "").text("Choose One").appendTo("#job_title");
    } else {
        //$("<option>").attr("value", "").text("See All").appendTo("#job_title");
    }
        $.ajax( {
            type : "GET",
            url : "index.php",
            data : "module=dbviewdata&hive_op=job_titles&output=1&job_category=" + $("#job_category").val(),
            success : function(data) {
                    decoded_data = jQuery.parseJSON(data);
                    val = $("#job_category").val();
                    dereferenced_data =  bySortedValue(decoded_data[val], this);
                    $.each(dereferenced_data, function(key, value) {
                        $("<option>").attr("value", value[0]).text(value[1]).appendTo("#job_title");
                        if(value[0] == job_title){
                            $("#search_header").html(value[1]);
                        }
                        //build image allocation array
                        images_by_title['"' + value[0] + '"'] = value[2];
                        descr_by_title['"' + value[0] + '"'] = value[3];
                    });
                        //$("#job_title").focus();
                        $("#job_title").val(job_title);
                        changeImage(true);
                        changeDescription();

            }
        });
}
function changeImage(move_searchbox){
    if(images_by_title['"' + $("#job_title").val() + '"'] != '' && typeof images_by_title['"' + $("#job_title").val() + '"'] != 'undefined'){
        if($("#job_category").val() == '1458'){
            $('#company-logo-div').css({'background-image': 'url("' + images_by_title['"' + $("#job_title").val() + '"'] + '")'});
            $('#company-logo-div').css({'height': '190px'});
            $('#company-logo-div').css({'width': '200px'});
            $("#company-logo-div").show();
            if(move_searchbox){
                $('.searchbox').css({"width": "450px"});
                $('.searchbox').css({"margin-left": "60px"});
            }
        }

        //$('.searchbox').css({'margin-left': '20px'});
    }
}
function changeCategoryImage(){
    if(images_by_cat['"' + $("#job_category").val() + '"'] != '' && typeof images_by_cat['"' + $("#job_category").val() + '"'] != 'undefined'){
        $('#categoryPicture').css({'background-image': 'url("' + images_by_cat['"' + $("#job_category").val() + '"'] + '")'});
        $("#categoryPicture").show();
    } else {
        //$('#categoryPicture').css({'background-image': 'url("http://cdn.collegebound.net/collegebound/images/jobcategories/general.jpg")'});
        $("#categoryPicture").show();
    }
}
function changeDescription(){
    if(descr_by_title['"' + $("#job_title").val() + '"'] != '' && typeof descr_by_title['"' + $("#job_title").val() + '"'] != 'undefined'){
        $("#category-title_" + campaign_JobTitle).html("About " + $("#job_title option:selected").text());
        $("#description_" + campaign_JobTitle).html(descr_by_title['"' + $("#job_title").val() + '"']);
    }else if(descr_by_title['"' + $("#job_category").val() + '"'] != '' && typeof descr_by_title['"' + $("#job_category").val() + '"'] != 'undefined'){
        $("#description").html(descr_by_title['"' + $("#job_category").val() + '"']);
    } else {
        $("#description").html("Study after study shows that if you love your job, you'll be 150% happier with your life than one of the 84 percent of Americans who dread their jobs. Why stay in a career that drains you when you can enroll in a certificate or degree program that can- literally - change your life for the better!? ");
    }
}
</script>

