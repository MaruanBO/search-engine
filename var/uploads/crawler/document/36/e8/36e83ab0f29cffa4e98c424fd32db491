                </div>
            </div>





        </ospheader2013_body>
        <script type="text/javascript">
            /* menu slide animation duration (in milliseconds) */
            var duration = 100;

            $(document).ready(function () {

                /* menus */
                //New one 12
                $('.OSPHeader2013_menu').addClass('noneImportant');


                $('.OSPHeader2013_staticText').hover(
                   function () {
                       'use strict';

                       var currentIndex = $(this).index();
                       // $('div.OSPHeader2013_nav div.OSPHeader2013_expandable').stop().animate({ width: '0px' }, duration);
                       $('.OSPHeader2013_menu').hide();
                       $('.OSPHeader2013_menu').eq(currentIndex).show();
                       $('.OSPHeader2013_offer').addClass('OSPHeader2013_offerCollapsed');
                       $('.OSPHeader2013_offer').removeClass('OSPHeader2013_offerExpanded');
                   },
                   function () {
                       'use strict';

                       $('.OSPHeader2013_menu').hide();
                       $('.viewExtras').hide();
                       $('.viewAll').show();
                   }
               );

                $('.OSPHeader2013_tab, .OSPHeader2013_body').hover(
                   function () {
                       'use strict';
                       $('.OSPHeader2013_offer').addClass('OSPHeader2013_offerCollapsed');
                       $('.OSPHeader2013_offer').removeClass('OSPHeader2013_offerExpanded');
                   },
                   function () {
                       'use strict';
                       $('.OSPHeader2013_offer').removeClass('OSPHeader2013_offerCollapsed');
                       $('.OSPHeader2013_offer').addClass('OSPHeader2013_offerExpanded');
                   }
               );



                $('.OSPHeader2013_menu > div').hover(
                    function () {
                        'use strict';

                        $(this).closest(".OSPHeader2013_menu").show();
                    },
                    function () {
                        'use strict';

                        $('.OSPHeader2013_menu').hide();
                        $('.viewExtras').hide();
                        $('.viewAll').show();
                        $('div.OSPHeader2013_nav div.OSPHeader2013_menu a.expander').removeClass('current');
                        $('div.OSPHeader2013_nav div.OSPHeader2013_expandable').stop().animate({ width: '1px' }, duration);
                    }
                );

                /* find the width of the nvaigation and dynamically sets the width fo the padding inbetween based on the number of nav items*/
                var startingNavWidth = 0;
                $('.OSPHeader2013_staticText').each(function (index) {
                    startingNavWidth += $(this).width();
                });

                var paddingToAdd = (($('.OSPHeader2013_header').width() - startingNavWidth) / $('.OSPHeader2013_staticText').length) / 2;

                $('.OSPHeader2013_staticText').each(function (index) {
                    $(this).css('padding-left', Math.floor(paddingToAdd) + 'px');
                    $(this).css('padding-right', Math.floor(paddingToAdd) + 'px');
                });


                


                /* position the nav dynamically*/
                var currentLeftPosition = 0;
                $('.OSPHeader2013_nav').children('a').each(function (index) {
                    var paddingLeft = $(this).css('padding-left').replace("px", "");
                    var paddingRight = $(this).css('padding-right').replace("px", "");
                    var fullWidth = parseInt(paddingLeft) + parseInt(paddingRight) + $(this).width();

                    if ($.browser.msie && parseFloat($.browser.version) < 8) {
                        $('.OSPHeader2013_menu').eq(index).addClass('ie7');
                    }

                    /* find the width of the tab with added padding */
                    $('.OSPHeader2013_menu').eq(index).find('.OSPHeader2013_tab').eq(0).width(fullWidth);

                    /* this if else is for checking if we want the information to be on the left instead of right so we can move the tabs accordingly */
                    if ($('.OSPHeader2013_menu').eq(index).hasClass('OSPHeader2013_right-side')) {
                        $('.OSPHeader2013_menu').eq(index).find('.OSPHeader2013_body').eq(0).show();
                        var hasDeal = $('.OSPHeader2013_menu').eq(index).has(".NavDDFeatureProduct");
                        if (hasDeal.length > 0) {
                            $('.OSPHeader2013_menu').eq(index).css('left', currentLeftPosition - (380 - fullWidth));
                        }
                        else {
                            $('.OSPHeader2013_menu').eq(index).css('left', currentLeftPosition - (190 - fullWidth));
                        }
                    }
                    else {
                        $('.OSPHeader2013_menu').eq(index).css('left', currentLeftPosition);
                    }
                    /* we might have leftover padding so lets go in and add it to the last element so its flush */
                    if ($('.OSPHeader2013_nav').children('a').length == (index + 1)) {
                        $(this).css('padding-right', '0px');
                        $(this).css('padding-left', paddingLeft - 1 + 'px');
                        var addedPaddingForLastElement = ($('.OSPHeader2013_header').width() - currentLeftPosition - fullWidth - 2/* -2 for border*/);

                        $('.OSPHeader2013_menu').eq(index).find('.OSPHeader2013_tab').eq(0).css('padding-right', addedPaddingForLastElement + 'px');
                        var hasDeal = $('.OSPHeader2013_menu').eq(index).has(".NavDDFeatureProduct");
                        if (hasDeal.length > 0) {
                            $('.OSPHeader2013_menu').eq(index).css('left', currentLeftPosition - (381 - fullWidth) + addedPaddingForLastElement);
                        }
                        else {
                            $('.OSPHeader2013_menu').eq(index).css('left', currentLeftPosition - (191 - fullWidth) + addedPaddingForLastElement);
                        }

                    }
                    $(this).css('visibility', 'visible');
                    /* store the position for the aboslute position of the next item */
                    currentLeftPosition += fullWidth;
                });


                $('.OSPHeader2013_staticText').css('visibility', 'visible');

                if (!($.browser.msie && (parseFloat($.browser.version) < 8)) && !location.href.match(/AffiliateBrowsing|bags.onestopplus|art.onestopplus|pbj.onestopplus|stg.bags/)) { newDeptIdGet(); }

                function newDeptIdGet() {

                    $('.OSPHeader2013_menu').removeClass('noneImportant');

                    var DeptIDArray = "";
                    /* dynamic adding of divs */
                    $(".OSPHeader2013_column[deptid]").each(function (index) {
                        var cutoff = $(this).attr('cutoff');
                        if (index == 0) {
                            DeptIDArray = $(this).attr('deptid');
                        }
                        else {
                            DeptIDArray += "|" + $(this).attr('deptid');
                        }
                    });

                    $.ajax({
                        type: 'GET',
                        url: '../MainMenu.ashx',
                        data: {
                            deptid: DeptIDArray
                        },
                        dataType: "json",
                        success: function (data) {
                            AjaxGetCategorySuccess(data);
                        },
                        error: AjaxGetCategoryError
                    });

                    function AjaxGetCategorySuccess(data) {
                        if (data == null) {
                            //console.log(currentColumn.attr('deptid') + " is null");
                        }
                        if (!($.browser.msie && (parseFloat($.browser.version) < 8)) && !location.href.match(/AffiliateBrowsing|bags.onestopplus|art.onestopplus|pbj.onestopplus|stg.bags/)) {
                            $(".OSPHeader2013_column[deptid]").each(function (index) {
                                var cutoff = $(this).attr('cutoff');
                                var currentColumn = $(this);
                                //console.log("reading each");
                                $.each(data, function (i, item) {
                                    // console.log(item);
                                    if (item.id == currentColumn.attr('deptid')) {
                                        //console.log(item.id +  " is correct" + currentColumn.attr('deptid'));
                                        //console.log(item.childCategories);
                                        // console.log(currentColumn);
                                        $.each(item.childCategories, function (i, subItem) {
                                            //console.log(subItem);
                                            if (typeof cutoff !== 'undefined' && cutoff !== false) {
                                                if (i >= cutoff - 1) {
                                                    if (i == cutoff - 1 && currentColumn.hasClass('two')) {
                                                        $("<a />").html('View More').attr("href", "/Plus-Size-New-Arrivals.aspx?DeptId=" + currentColumn.attr('deptid')).appendTo(currentColumn);
                                                        return false;
                                                    }
                                                    if (currentColumn.hasClass('one') && i == cutoff - 1) {
                                                        $("<a />").html('View More').addClass("viewAll").appendTo(currentColumn);
                                                        var currentATag = $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(currentColumn).addClass("viewExtras");
                                                    }
                                                    else {
                                                        var currentATag = $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(currentColumn).addClass("viewExtras");
                                                    }
                                                }
                                                else {
                                                    var currentATag = $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(currentColumn)
                                                }
                                            }
                                        });
                                    }
                                });
                            });
                        }
                        $('.viewAll').on('click', function () {
                            'use strict';
                            //console.log('click view all');
                            $('.viewExtras').css('display', 'block');
                            $(this).hide();
                        });

                        setAfterAjax();
                    }

                    function AjaxGetCategoryError(XMLHttpRequest, textStatus, errorThrown) {
                        console.log("XMLHttpRequest: " + XMLHttpRequest);
                        console.log("textStatus: " + textStatus);
                        console.log("errorThrown: " + errorThrown);
                    }
                }


                function setAfterAjax() {

                    /* sliding sub-menus */
                    $('div.OSPHeader2013_nav div.OSPHeader2013_menu .one a').on('mouseover', function () {
                        'use strict';
                        $('div.OSPHeader2013_expandable').show();
                        /*
                        div.OSPHeader2013_expandable is the animated wrapper. the element to be shown in the wrapper
                        is in the data-target attribute of the link that expans the menu. that
                        element's width (from css) is used to decide the final width of the wrapper.
                        */
                        var $this = $(this);
                        var parentIndex = $(this).parent().parent().parent().index();
                        var subIndex = $(this).index();


                        $('div.OSPHeader2013_nav div.OSPHeader2013_menu a').removeClass('current');
                        if ($this.hasClass('expander')) {
                            //console.log("mouseover 'a' if expander");
                            $this.addClass('current');
                            $('div.OSPHeader2013_nav div.OSPHeader2013_expandable').stop().css('width', '1px');
                            var expanderSelection = $('.OSPHeader2013_menu').eq(parentIndex - 10).find('.OSPHeader2013_expandable').find('div').eq(subIndex);
                            $('div.OSPHeader2013_expandable').stop().animate({ width: expanderSelection.width() + "px" }, duration);

                            $('.OSPHeader2013_offer').addClass('OSPHeader2013_offerExpanded');
                            $('.OSPHeader2013_offer').removeClass('OSPHeader2013_offerCollapsed');
                        }
                        else {
                            //console.log("mouseover 'a' else");
                            $('div.OSPHeader2013_nav div.OSPHeader2013_expandable').stop().css('width', '1px');

                            $('.OSPHeader2013_offer').addClass('OSPHeader2013_offerCollapsed');
                            $('.OSPHeader2013_offer').removeClass('OSPHeader2013_offerExpanded');
                        }
                        $('div.OSPHeader2013_expandable div').hide();
                        expanderSelection.show();

                    });

                    var isTouchDevice;
                    if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i) || navigator.userAgent.match(/iPad/i))
                    { var isTouchDevice = true; }
                    else
                    { var isTouchDevice = false; }

                    /* fix for Ipad  so you dont have to double click an a tag*/
                    if (isTouchDevice) {
                        var dragging = false;
                        $("body").on("touchmove", function(){
                            dragging = true;
                        });

                        $("body").on("touchstart", function(){
                            dragging = false;
                        });

                        $('body').on('click touchend', function (event) {
                            //We are scrolling dont do anything
                            if (dragging){return;}

                            var target = $(event.target);
                            if (target.parents('.OSPHeader2013_menu').length > 0) {
                                if (!target.hasClass("expander") && target.is("a") && !target.hasClass("viewAll") && !target.parent.hasClass("OSPHeader2013_tab")) {
                                    var link = target.attr('href');
                                    window.location = link;
                                }
                            }
                            else {
                                if (!target.hasClass('OSPHeader2013_staticText')) {

                                    $('.OSPHeader2013_menu').hide();
                                }
                                if (target.hasClass('mui_gn_filter_value') && target.attr('href') !== 'undefined') {
                                    var link = target.attr('href');
                                    window.location = link;
                                }
                                if(target.parents('#left-nav').length > 0)
                                {
                                    if(target.is( "a" ))
                                    {
                                        var link = target.attr('href');
                                        window.location = link;
                                    }
                                }
                            }
                        });


                        $('a, img').on('click', function (e) {
                            if (dragging){return;}
                            var target = $(event.target);
                            if (target.parents('.OSPHeader2013_tab').length <= 0) {
                                if (target.hasClass('OSPHeader2013_staticText')) {
                                    var currentIndex = target.index();
                                    $('.OSPHeader2013_menu').eq(currentIndex).show();
                                }
                                else {
                                    if(!target.hasClass("viewAll") && !target.parents('.product-wrapper') && !target.hasClass("OSPHeader2013_tab"))
                                    {
                                        var el = $(this);
                                        var link = el.attr('href');
                                        window.location = link;
                                    }
                                }
                            }
                        });
                    }
                }
            });
        </script>
    </div>
</div>

