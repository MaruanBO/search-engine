<script language="javascript" type="text/javascript">
    if (typeof (oJFK) == 'undefined') document.write('<script src="/ajax-data/booker-flights.js"><\/script>');
    var scheduleExtDate = {
    "year": 2015,
    "month": 10,
    "day": 24
};
    function prePopulateBooker(from, to) {
        try {
            var fromObj = eval('o' + from.toUpperCase());
            jQuery('#from_field').attr('value', fromObj.name);

            if(!to) {
            jQuery('#to_field').attr('value', '');
            }

            var toObj = eval('o' + to.toUpperCase());
            jQuery('#to_field').attr('value', toObj.name);
        } catch (err) {
        }
    }
    function adultSelect(booker_type) {
        var adultOpt = document.getElementById(booker_type + "adult_count");
        var adultNum = adultOpt.options[adultOpt.selectedIndex].value;
        var infantOpt = document.getElementById(booker_type + "infant_count");
        var infantNum = infantOpt.options[infantOpt.selectedIndex].value;
          
        if (adultNum < infantNum) {
            infantOpt.selectedIndex = 0;
            jQuery('#' + booker_type + 'inf_dd_num').html("0");
            jQuery('.dd_num').html("0");
            // @mx3.0 - #451 -  usercontrols/booker infantNum fix
           jQuery('#' + booker_type + 'inf_dd_num2').html("0");

        }
    }
    function setMaxInfant(booker_type) {
        var adultOpt = document.getElementById(booker_type + "adult_count");
        var adultNum = adultOpt.options[adultOpt.selectedIndex].value;
        var maxInfant = adultNum > 3 ? 3 : adultNum;
      
        var liElem = jQuery('#' + booker_type + 'infant_display li');
        for (var i = 0; i <= 3; i++) {
            if (i <= maxInfant)
                jQuery(liElem[i]).css('display', 'block');
            else
                jQuery(liElem[i]).css('display', 'none');
        }
        var actualHeight = 50 + maxInfant * 23 + 'px';
        jQuery('#' + booker_type + 'infant_count_display').css('height', actualHeight);
    }
    jQuery(document).ready(function () {
        var cookies = document.cookie.split(";");
        var ffCookie = "";
        
        for (var i = 0; i < cookies.length; i++) {
            if (jQuery.trim(cookies[i].substr(0, cookies[i].indexOf("="))) == "fflight-form-data") {
                ffCookie = unescape(cookies[i].substr(cookies[i].indexOf("=") + 1));
                break;
            }
        }
        
        if (ffCookie.length > 24) {
            var baseAirport = ffCookie.substr(21, 3);
            prePopulateBooker(baseAirport);
            var expireDate = new Date();
            expireDate.setDate(expireDate.getDate() + 1000);
            document.cookie="base_airport=" + baseAirport + "; expires=" + expireDate.toUTCString() + "; path=/";
        }
    });

