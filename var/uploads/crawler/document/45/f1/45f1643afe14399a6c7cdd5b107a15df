<script>
var currentPositionPopup = 0;
var slideWidthPopup = 938;
var slidesPopup = $('#wrap-product-ul');
var numberofproductPopup = 7;
var numberOfSlidesPopup = parseInt('10');
// Remove scrollbar in JS
$('#slidesContainerPopup').css('overflow', 'hidden');
// Wrap all .slidesPopup with #slideInnerPopup div
slidesPopup
.wrapAll('<div id="slideInnerPopup"></div>')
// Float left to display horizontally, readjust .slidesPopup width
.css({
'float': 'left'/*,
'width': slideWidthPopup*/
});
// Set #slideInnerPopup width equal to total width of all slidesPopup
if ($.browser.msie) {
$('#slideInnerPopup').css('width', slideWidthPopup * numberOfSlidesPopup + numberOfSlidesPopup * 10);
} else {
$('#slideInnerPopup').css('width', slideWidthPopup * numberOfSlidesPopup);
}
// Hide left arrow control on first load
manageControls(currentPositionPopup);
//$('.sc-left-button')
$("#navLeftfirst")
.bind('click', function () {
currentPositionPopup = currentPositionPopup - 1;
// Hide / show controls
manageControls(currentPositionPopup);
// Move slideInnerPopup using margin-left
$('#slideInnerPopup').animate({
'marginLeft': slideWidthPopup * (-currentPositionPopup)
});
});
//$('.sc-right-button').bind('click', function () { ProcessSlide(); });
$("#navRightfirst").bind('click', function () { ProcessSlide(); });
function ProcessSlide() {
//Determine new position
currentPositionPopup = currentPositionPopup + 1;
// Hide / show controls
manageControls(currentPositionPopup);
// Move slideInnerPopup using margin-left
$('#slideInnerPopup').animate({
'marginLeft': slideWidthPopup * (-currentPositionPopup)
});
}
// manageControls: Hides and Shows controls depending on currentPositionPopup
function manageControls(position) {
// Hide left arrow if position is first slide
var minvalue = position * 7;
var indeavalue = minvalue;
while (indeavalue <= minvalue + 7 && indeavalue >= 7) {
if ($("#wrap-product-ul img[lineindex=" + indeavalue + "]").length > 0 && $("#wrap-product-ul img[lineindex=" + indeavalue + "]").attr("attrsrc") != "") {
$("#wrap-product-ul img[lineindex=" + indeavalue + "]").attr("src", $("#wrap-product-ul img[lineindex=" + indeavalue + "]").attr("attrsrc"));
$("#wrap-product-ul img[lineindex=" + indeavalue + "]").fadeTo("slow", 1)
$("#wrap-product-ul img[lineindex=" + indeavalue + "]").attr("attrsrc", "");
}
indeavalue = indeavalue + 1;
}
if (position == 0) { $('#navLeftfirst').hide() } else { $('#navLeftfirst').show() }
// Hide right arrow if position is last slide
if (position == numberOfSlidesPopup - 1) { $('#navRightfirst').hide() } else { $('#navRightfirst').show() }
}
function showmode8item() {
numberOfSlidesPopup = parseInt(numberofproductPopup / 8);
if (numberofproductPopup % 8 > 0)
numberOfSlidesPopup = numberOfSlidesPopup + 1;
manageControls(currentPositionPopup);
if ($("#slideInnerPopup").length > 0) {
$("#slideInnerPopup").attr("class", "top8-product");
}
// Set #slideInnerPopup width equal to total width of all slidesPopup
if ($.browser.msie) {
$('#slideInnerPopup').css('width', slideWidthPopup * numberOfSlidesPopup + numberOfSlidesPopup * 10);
} else {
$('#slideInnerPopup').css('width', slideWidthPopup * numberOfSlidesPopup);
}
}
</script>
