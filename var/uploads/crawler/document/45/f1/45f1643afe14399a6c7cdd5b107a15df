<script>
$("#brandtopmenu").bind("mouseover", function () { $("#idListBrandTopMenu").show(); $("#idListBrandMenu").hide(); $('#idanchor li').attr('class', ''); $('#idanchor li:first').attr('class', 'balpha-select'); });
function js_FilterBrand(keyword, obj) {
$('#idanchor li').attr('class', '');
$(obj).attr("class", "balpha-select");
$("#idListBrandTopMenu").hide();
$("#idListBrandMenu").show();
$("#idListBrandMenu").find("div").hide();
if ($("div[note=" + keyword + "]").length > 0) {
$("div[note=" + keyword + "]").show();
} else {
$("#brandmenuloading").show();
var url = "/incpages/ajaxfilterbrand_new.asp?key=" + Math.random() + "&keyword=" + keyword;
$.ajax({
url: url,
type: "GET",
success: function (response) {
$("#brandmenuloading").hide();
$("#idListBrandMenu").append(response)
$("#idListBrandMenu").show();
}
});
}
}
</script>
