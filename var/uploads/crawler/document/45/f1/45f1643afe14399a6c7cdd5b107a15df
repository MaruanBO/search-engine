function scrollBrands() {
if (!over) {
currPosX = currPosX + scroll;
jQuery(".brands-inner").css("background-position", currPosX + "px 0px");
}
}
function scrollLeft() {
scroll = -1;
}
function scrollRight() {
scroll = 1;
}
jQuery(document).ready(function() {
jQuery(".brands-inner").css({ backgroundPosition: '0 0' });
setTimeout("setInterval('scrollBrands()', 60)", 60);
jQuery(".brand-left").bind('mouseover', function() { scrollLeft(); });
jQuery(".brand-right").bind('mouseover', function() { scrollRight(); });
jQuery(".brands-inner").bind('mouseover', function() { over = true; });
jQuery(".brands-inner").bind('mouseout', function() { over = false; });
jQuery(".brands-inner").bind('click', function(eventObj) { handleBrandClick(eventObj); });
});
function handleBrandClick(eventObj) {
//get the x / y position of click within brands element;
var outerLeft = jQuery(".brands-inner").offset().left;
var outerTop = jQuery(".brands-inner").offset().top;
var clickPosX = eventObj.pageX - outerLeft;
var clickPosY = eventObj.pageY - outerTop;
//var backgroundPos = jQuery(".brands-inner").css("background-position");
//var backX = parseInt(backgroundPos.split(" ")[0]); 
var adjustedX1 = (clickPosX - currPosX) % brandsBgWidth;
//alert(clickPosX + '   ' + currPosX  + ' ' +  adjustedX1 + ' x1 ' + imageMap[0].pos[0] + 'x2 ' +  imageMap[0].pos[2]  + 'y1 ' +  imageMap[0].pos[1] + ' y2 ' + imageMap[0].pos[3]);
//alert(clickPosX + '   ' + currPosX  + ' ' +  adjustedX1 );
for (var i = 0; i < imageMap.length; i++) {
var adjustedX = (clickPosX - currPosX) % brandsBgWidth;
if (adjustedX >= imageMap[i].pos[0] && adjustedX <= imageMap[i].pos[2] && clickPosY >= imageMap[i].pos[1] && clickPosY <= imageMap[i].pos[3]) {
if (imageMap[i].isopen)
window.open(imageMap[i].url, "_blank");
else
location.href = imageMap[i].url;
break;
}
}
}
