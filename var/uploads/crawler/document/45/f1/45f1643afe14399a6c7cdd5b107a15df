<div class="modal-overlay" id="modal-overlay-popup-details" style="display:none" onclick="closePopupDetails();"></div>
<script type="text/javascript">
var currentPositionPopup_what = 0;
var slideWidthPopup_What = 938;
var slidesPopup_What = $('#wrap-product-ul-what');
var numberofproductPopup_what = 7;
var numberOfSlidesPopup_What = parseInt('7');
// Remove scrollbar in JS
$('#slidesContainerPopupWhat').css('overflow', 'hidden');
// Wrap all .slidesPopup with #slideInnerPopup div
slidesPopup_What
.wrapAll('<div id="slideInnerPopupWhat"></div>')
// Float left to display horizontally, readjust .slidesPopup width
.css({
'float': 'left'/*,
'width': slideWidthPopup*/
});
// Set #slideInnerPopup width equal to total width of all slidesPopup
if ($.browser.msie) {
$('#slideInnerPopupWhat').css('width', slideWidthPopup_What * numberOfSlidesPopup_What + numberOfSlidesPopup_What * 10);
} else {
$('#slideInnerPopupWhat').css('width', slideWidthPopup_What * numberOfSlidesPopup_What);
}
// Hide left arrow control on first load
manageControls_What(currentPositionPopup_what);
//$('.sc-left-button')
$('#navLeft').bind('click', function () {
currentPositionPopup_what = currentPositionPopup_what - 1;
// Hide / show controls
manageControls_What(currentPositionPopup_what);
// Move slideInnerPopup using margin-left
$('#slideInnerPopupWhat').animate({
'marginLeft': slideWidthPopup_What * (-currentPositionPopup_what)
});
});
//$('.sc-right-button').bind('click', function () { ProcessSlide(); });
$('#navRight').bind('click', function () { ProcessSlide_What(); });
function ProcessSlide_What() {
//Determine new position
currentPositionPopup_what = currentPositionPopup_what + 1;
// Hide / show controls
manageControls_What(currentPositionPopup_what);
// Move slideInnerPopup using margin-left
$('#slideInnerPopupWhat').animate({
'marginLeft': slideWidthPopup_What * (-currentPositionPopup_what)
});
}
// manageControls: Hides and Shows controls depending on currentPositionPopup
function manageControls_What(position) {
// Hide left arrow if position is first slide
var minvalue = position * numberofproductPopup_what;
var indeavalue = minvalue;
while (indeavalue <= minvalue + numberofproductPopup_what && indeavalue >= numberofproductPopup_what) {
if ($("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").length > 0 && $("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").attr("attrsrc") != "") {
$("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").attr("src", $("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").attr("attrsrc"));
$("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").fadeTo("slow", 1)
$("#wrap-product-ul-what img[lineindex=" + indeavalue + "]").attr("attrsrc", "");
}
indeavalue = indeavalue + 1;
}
if (position == 0) { $('#navLeft').hide() } else { $('#navLeft').show() }
// Hide right arrow if position is last slide
if (position == numberOfSlidesPopup_What - 1) { $('#navRight').hide() } else { $('#navRight').show() }
}
var facebookAppId = '564859843612044';
function openFacebookSharePopup(link, image, title, summary, customerName) {
link = encodeURIComponent(link);
image = encodeURIComponent(image);
title = encodeURIComponent(title);
summary = encodeURIComponent(summary);
if (pageType == "") {
customerName = encodeURIComponent('I  ' + customerName + '\'s look!');
}
else if (pageType == 'PW') {
customerName = encodeURIComponent(customerName);
}
var redirect_uri = encodeURIComponent('http://www.swimoutlet.com/incpages/showcase-style-share-facebook-success.html');
var url = 'https://www.facebook.com/dialog/feed?app_id=' + facebookAppId + '&display=popup&caption=' + title + '&link=' + link + '&picture=' + image + '&description=' + summary + '&name=' + customerName + '&redirect_uri=' + redirect_uri;
popUpCenter(url);
}
function openPinterestSharePopup(link, media, description) {
link = encodeURIComponent(link);
media = encodeURIComponent(media);
description = encodeURIComponent(description);
var url = "https://pinterest.com/pin/create/button/?url=" + link + "&media=" + media + "&description=" + description;
popUpCenter(url);
}
function openTweetSharePopup(content) {
content = encodeURIComponent(content);
var url = "http://twitter.com/home?status=" + content;
popUpCenter(url);
}
function popUpCenter(url) {
var w = 626;
var h = 436;
var left = (screen.width / 2) - (w / 2);
var top = (screen.height / 2) - (h / 2);
var attributes = "status = 1, width=" + w + ", height=" + h + ", top=" + top + ", left=" + left + ", toolbar=no,menubar=no,scrollbars=no,location=no,directories=no, resizable=no";
window.open(url, "_blank", attributes);
}
function renderHtmlForDetailsPopup(data, pageType) {
var customerId = customeridlogin;
var showcase = data.showcase;
var customerName = showcase.customer_name;
var styleLiked = showcase.liked;
var titleLike, textLike;
if (styleLiked == "liked") {
styleLiked = "loved";
titleLike = "Like it";
textLike = "Liked it";
}
else {
styleLiked = "unloved";
titleLike = "";
textLike = "Like it";
}
var link, image_link, title, summary;
link = "http://www.swimoutlet.com/style-showcase/outfits/" + showcase.showcase_id;
image_link = "https://www.cdn-outlet.com/photos/showcase/img_460w_" + showcase.photo_name;
var titlePinterestShare = '<3 this look from the #SwimOutlet Style Showcase!';
var titleTwitterShare = '<3 this look from the @SwimOutlet Style Showcase! Check out';
var titleFacebookShare = '#SwimOutlet.com Style Showcase';
var summarryFacebookShare = 'I spotted this swimsuit on the #SwimOutlet.com Style Showcase. Check it out!';
if (pageType == 'PW') {
link = "http://www.swimoutlet.com/what-the-pros-wear/outfits/" + showcase.showcase_id;
titlePinterestShare = 'What the Pros Wear - Checkout the gear from ' + customerName;
titleTwitterShare = 'What the Pros Wear - Checkout the gear from ' + customerName;
titleFacebookShare = 'What the Pros Wear';
summarryFacebookShare = 'Checkout the gear from ' + customerName;
}
var products = data.products;
var widthListProducts = 918;
if (products.length > 0) {
widthListProducts = 153 * products.length;
}
var currentIndexProduct = 1;
var styleDisplay = '';
if (products.length <= 3) {
styleDisplay = 'style="display:none"';
}
var html = '';
html += '<div id="popup-details-inner-' + showcase.showcase_id + '">\
<div class="modal-wrapper wrapper-popover" style="width:1038px; position: fixed;">\
<div class="modal-view-popover">\
<div class="prev-outfit">\
<span class="prev-outfit-slide" onclick="goPreviousShowcase(\'' + showcase.showcase_id + '\',\'' + pageType + '\')"></span>\
</div>\
<div class="outfit-detail">\
<div class="image-part">\
<img src="https://www.cdn-outlet.com/photos/showcase/img_460w_' + showcase.photo_name + '" height="' + showcase.height + '" style="max-height:' + showcase.height + 'px" alt="' + customerName + '" />\
</div>\
<div class="detail-part">\
<span class="close-showcase-popup" title="close" onclick="closePopupDetails();"></span>\
<div class="user-part">\
<h3 class="uploader-name">';
if (pageType == 'SS') {
html += '<a href="/style-showcase/users/' + showcase.customer_id + '" title="' + customerName + '">' + customerName + '</a>';
}
else if (pageType == 'PW') {
if (showcase.tags.length > 0) {
html += '<a href="/what-the-pros-wear/' + getFriendlyTagLink(showcase.tags[0].link_name) + '" title="' + customerName + '">' + customerName + '</a>';
}
else {
html += '<a href="#" title="' + customerName + '">' + customerName + '</a>';
}
}
html += '</h3>\
<p class="date-added">Added ' + showcase.added_time + '</p>\
<div class="clear"></div>\
<div class="uploader-extra-info">\
<div class="love-it-button">\
<div class="love-button ' + styleLiked + '" title="' + titleLike + '" data-icon="&#10084;" onclick="likeShowcase(this,' + customerId + ',' + showcase.showcase_id + ', 1);">' + textLike + '</div>\
<div class="loves-count"><span></span>' + showcase.like_count + '</div>\
</div>\
<div class="share-widget">\
<a href="#" title="Facebook" onclick="openFacebookSharePopup(\'' + link + '\',\'' + image_link + '\',\'' + titleFacebookShare + '\',\'' + summarryFacebookShare + '\',\'' + customerName + '\'); return false;">Facebook</a>\
<a href="#" title="Twitter" onclick="openTweetSharePopup(\'' + titleTwitterShare + ' ' + link + '\'); return false;">Twitter</a>\
<a href="#" title="Pinterest" onclick="openPinterestSharePopup(\'' + link + '\',\'' + image_link + '\',\'' + titlePinterestShare + '\'); return false;">Pinterest</a>\
</div>\
<div class="clear"></div>\
</div>\
</div>';
if (products.length > 0) {
html += '<div class="shop-the-look">\
<h3>Shop the look</h3>\
<div class="the-product-list">\
<div class="slider-showcase">\
<span class="prev-pd-slide" ' + styleDisplay + ' onclick="slideListProduct(this,' + products.length + ',\'prev\',' + widthListProducts + ')"></span>\
<div class="inner-slider-showcase">\
<!--slide to left/right: +- 153px // 918px = 153px * 6products -->\
<div class="current-index-product" style="display:none">' + currentIndexProduct + '</div>\
<div class="standard-row" style="width:' + widthListProducts + 'px;">';
var p, product_link;
var image_link_src;
for (i = 0; i < products.length; i++) {
p = products[i];
if (p.color_id != 0) {
image_link_src = "https://www.cdn-outlet.com/photos/SOResource/options/auto-thumbnails/" + p.product_code + "-" + p.color_id + "-T-168x196-AUTO.jpg";
}
else {
image_link_src = "https://www.cdn-outlet.com/photos/SOResource/" + p.product_code + "-T-168x196-AUTO.jpg";
}
product_link = "/p/" + p.product_name.toLowerCase().replace(/'/g, "").replace(/"/g, "").replace(/ /g, "-").replace("\\", "").replace("/", "").replace("/", "").replace(/%/g, "").replace(/#/g, '') + "-" + p.product_code + "/";
html += '                   <div class="row-data">\
<div class="pd-attr">\
<a href="' + product_link + '" title="' + p.product_name + '" class="pd-photo" target="_blank">\
<img src="' + image_link_src + '" alt="product photo" width="135" height="158" />\
<div class="associated-photo-ribbon">' + p.tag_style.toUpperCase() + '</div>\
</a>\
</div>\
<!-- end div.pd-attr -->\
<div class="pd-details">\
<a href="' + product_link + '" title="' + p.product_name + '" class="pd-name" target="_blank">' + p.product_name + '</a>\
<br/>';
html += renderPriceAndRating(p.list_price, p.product_price, p.sale_price, p.rating, p.stock_status);
html += '</div>\
<!-- end div.pd-details -->\
</div>';
}
html += '                               </div>\
<!-- end div.standard-row -->\
</div>\
<!--end div.inner-slider-showcase -->\
<span class="next-pd-slide" ' + styleDisplay + ' onclick="slideListProduct(this,' + products.length + ',\'next\',' + widthListProducts + ')"></span>\
</div>\
<!-- end div.slider-showcase -->\
</div>\
<!-- end div.the-product-list -->\
<div class="clear"></div>\
</div>';
}
if (showcase.tags.length > 0) {
html += '               <ul class="outfit-tags">';
var t;
for (i = 0; i < showcase.tags.length; i++) {
t = showcase.tags[i];
html += '<li><a href="/' + (pageType == 'SS' ? 'style-showcase' : 'what-the-pros-wear') + '/' + getFriendlyTagLink(t.link_name) + '" title="' + t.display_name + '">' + t.display_name + '</a></li>';
}
html += '</ul><!-- end ul.outfit-tags -->';
}
html += '</div>\
<!-- end div.detail-part -->\
</div>\
<!-- end div.outfit-detail -->\
<div class="next-outfit">\
<span class="next-outfit-slide" onclick="goNextShowcase(\'' + showcase.showcase_id + '\',\'' + pageType + '\')"></span>\
</div>\
</div>\
<!-- end div.modal-view-popover -->\
</div>\
<!-- end div.modal-wrapper -->\
<!-- end HTML for Showcase Popup -->\
</div>';
return html;
}
var customeridlogin = ROCookies.readCookie("www%2Eswi%2FCustomerID");
if (customeridlogin == null || customeridlogin == "")
customeridlogin = "0";
function getFriendlyTagLink(tag) {
return tag.toLowerCase().replace(/'/g, "").replace(/\\/g, "").replace(/\//g, "").replace(/%/g, "").replace(/ /g, "-").replace(/\"/g, "-").replace(/&/g, "-");
}
function closePopupDetails() {
$('#modal-overlay-popup-details').hide();
$(".popup-details:visible").hide();
//$('.sc-right-button, .sc-left-button').css('z-index', 10);
return false;
}
function renderPriceAndRating(listPrice, productPrice, salePrice, rating, stockStatus) {
var results = '';
if (parseFloat(listPrice) > 0) {
if ((parseFloat(salePrice) > 0 && parseFloat(listPrice) > parseFloat(salePrice)) || (parseFloat(productPrice) > 0 && parseFloat(listPrice) > parseFloat(productPrice))) {
results += '<span class="pd-list-price-number">$' + parseFloat(listPrice).toFixed(2) + '</span>';
}
}
if (parseFloat(salePrice) > 0) {
results += '<span class="pd-sale-price-number">$' + parseFloat(salePrice).toFixed(2) + '</span>';
}
else if (parseFloat(productPrice) > 0) {
results += '<span class="pd-product-price-item">$' + parseFloat(productPrice).toFixed(2) + '</span>';
}
if (1 == 2 && parseFloat(rating) > 0) {
results += '<br>\
<a href="#" style="text-decoration:none">\
<img src="http://www.swimoutlet.com/templates/1/images/stars/star_s' + rating.toString() + '_5.png" width="60" height="11" alt="' + rating + '" />\
<span class="textPoll">';
if (stockStatus > 0) results += "(" + stockStatus.toString() + ")";
results += '</span>\
</a>';
}
return results;
}
function slideListProduct(el, numberOfProduct, mode, maxWidth) {
if (numberOfProduct > 0) {
var index = 0;
var style = "";
if (mode == "next") {
index = parseInt($(el).prev().find(".current-index-product").html());
if (index + 2 < numberOfProduct) {
style = "-" + ((index + 2) * 153).toString() + "px";
$(el).prev().find(".standard-row").animate({
'marginLeft': style
});
index += 3;
$(el).prev().find(".current-index-product").html(index);
}
}
else {
index = parseInt($(el).next().find(".current-index-product").html());
if (index - 3 > 0) {
style = "-" + ((index - 4) * 153).toString() + "px";
$(el).next().find(".standard-row").animate({
'marginLeft': style
});
index -= 3;
$(el).next().find(".current-index-product").html(index);
}
}
}
}
function showDetailsPopup(showcaseId, pageType) {
//$('.sc-right-button, .sc-left-button').css('z-index', 0);
var width = $(window).width();
var height = $(window).height();
var leftForPopup = 0;
var topForPopup = 0;
var heightTemp = 574;
if ($("#popup-details-" + showcaseId).html() != "") {
$('#modal-overlay-popup-details').show();
var popup = $("#popup-details-" + showcaseId);
popup.show().css('visibility', 'hidden');
if (popup.find('.outfit-detail').eq(0).height() < height) {
topForPopup = (height - popup.find('.outfit-detail').eq(0).height()) / 2;
}
else {
var image = popup.find('img').eq(0);
if (height > heightTemp) {
image.attr('height', height + 'px');
}
else {
image.attr('height', heightTemp + 'px');
}
}
if (width > 1127) {
leftForPopup = (width - 1127) / 2;
}
popup.find(".modal-wrapper").eq(0).css('top', topForPopup + 'px');
popup.find(".modal-wrapper").eq(0).css("left", (leftForPopup + 9) + 'px');
popup.css('visibility', 'visible');
}
else {
$.ajax({
url: "/incpages/LoadDetailsShowcase.aspx",
dataType: 'json',
data: { customerId: customeridlogin, showcaseId: showcaseId, pageType: pageType },
success: function (data) {
var html = renderHtmlForDetailsPopup(data, pageType);
$('#modal-overlay-popup-details').show();
var popup = $("#popup-details-" + showcaseId);
popup.html(html).show().css('visibility', 'hidden');
if (popup.find('.outfit-detail').eq(0).height() < height) {
topForPopup = (height - popup.find('.outfit-detail').eq(0).height()) / 2;
}
else {
var image = popup.find('img').eq(0);
if (height > heightTemp) {
image.attr('height', height + 'px');
}
else {
image.attr('height', heightTemp + 'px');
}
}
if (width > 1127) {
leftForPopup = (width - 1127) / 2;
}
popup.find(".modal-wrapper").eq(0).css('top', topForPopup + 'px');
popup.find(".modal-wrapper").eq(0).css("left", (leftForPopup + 9) + 'px');
popup.css('visibility', 'visible');
}
});
}
}
function checkLogin() {
if (customeridlogin=="0") {
return false;
}
return true;
}
function likeShowcase(el, customerId, showcaseId, action) {
if (checkLogin()) {
$.ajax({
url: "/incpages/LikeViewShowcase.aspx",
dataType: 'json',
data: { customerId: customerId, showcaseId: showcaseId, action: action },
success: function (data) {
var likeCount;
if ($(el).hasClass("style-bg-like")) {
likeCount = parseInt($(el).next(".style-number-like").find(".style-static-number").eq(0).html());
if ($(el).hasClass("style-like-check")) {
likeCount -= 1;
$(el).removeClass("style-like-check");
if ($("#popup-details-" + showcaseId).length > 0) {
$("#popup-details-" + showcaseId).find(".love-button").eq(0).removeClass("loved").addClass("unloved").html("Like it");
}
}
else {
likeCount += 1;
$(el).addClass("style-like-check");
if ($("#popup-details-" + showcaseId).length > 0) {
$("#popup-details-" + showcaseId).find(".love-button").eq(0).removeClass("unloved").addClass("loved").html("Liked it");
}
}
$(el).next(".style-number-like").find(".style-static-number").eq(0).html(likeCount);
if ($("#popup-details-" + showcaseId).length > 0) {
$("#popup-details-" + showcaseId).find(".loves-count").eq(0).html('<span></span>' + likeCount);
}
}
else if ($(el).hasClass("love-button")) {
likeCount = parseInt($(el).next(".loves-count").eq(0).html().replace(/<span.*<\/span>/, ""));
if ($(el).hasClass("loved")) {
likeCount -= 1;
$(el).removeClass("loved").addClass("unloved").html("Like it");
if ($("#image-" + showcaseId).length) {
$("#image-" + showcaseId).find(".style-bg-like").eq(0).removeClass("style-like-check");
}
}
else {
likeCount += 1;
$(el).removeClass("unloved").addClass("loved").html("Liked it");
if ($("#image-" + showcaseId).length) {
$("#image-" + showcaseId).find(".style-bg-like").eq(0).addClass("style-like-check");
}
}
$(el).next(".loves-count").eq(0).html('<span></span>' + likeCount);
if ($("#image-" + showcaseId).length) {
$("#image-" + showcaseId).find(".style-static-number").eq(0).html(likeCount);
}
}
}
});
}
else {
window.location = "/login.asp";
}
}
function goNextShowcase(currentShowcaseId, pageType) {
var curPopup = $('#popup-details-' + currentShowcaseId);
var nextShowcaseId;
if (curPopup.parents('li').next().length) {
nextShowcaseId = curPopup.parents('li').next().attr('data-id');
}
else {
nextShowcaseId = $('#style-list-share-' + pageType + ' > li:first-child').attr('data-id');
}
$('#popup-details-' + currentShowcaseId).hide();
showDetailsPopup(nextShowcaseId, pageType);
}
function goPreviousShowcase(currentShowcaseId, pageType) {
var curPopup = $('#popup-details-' + currentShowcaseId);
var previousShowcaseId;
if (curPopup.parents('li').prev().length) {
previousShowcaseId = curPopup.parents('li').prev().attr('data-id');
}
else {
previousShowcaseId = $("#style-list-share-" + pageType + " > li:last-child").attr("data-id");
}
$('#popup-details-' + currentShowcaseId).hide();
showDetailsPopup(previousShowcaseId, pageType);
}
$('.li-pros-wear').click(function () {
var _this = $(this);
var id = _this.attr('id').replace('li-pros-wear-', '');
//$('#divHomeShowCase .pros-wear .home-showcase-slide').css('z-index', 121);
//$('#divHomeShowCase .showcase-home .home-showcase-slide').css('z-index', 120);
showDetailsPopup(id, 'PW');
return false;
});
$('.li-style-showcase').click(function () {
var _this = $(this);
var id = _this.attr('id').replace('li-style-showcase-', '');
//$('#divHomeShowCase .showcase-home .home-showcase-slide').css('z-index', 121);
//$('#divHomeShowCase .pros-wear .home-showcase-slide').css('z-index', 120);
showDetailsPopup(id, 'SS');
return false;
});
</script>
