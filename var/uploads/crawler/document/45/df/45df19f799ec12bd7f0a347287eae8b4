<script type="text/javascript">
	require(['app']);
	require(['app', 'dojo/query', 'rl/head', 'rl/topnav'], function(app, $, head, topnav) {
		var fadeinDelay = null;
		if (fadeinDelay != null)
	    fadeinDelay = clearTimeout(fadeinDelay);

		if (dojo.isIE <= 8)
			$('select').style({ display: 'none' });

		var hideElements = $('.brand_nav, .body, .foot, .copyright');
		if (!dojo.isIE || dojo.isIE > 8) {
			hideElements.style({ opacity: 0, visibility: 'visible' });
		}
		
		app.prepareUI(document.body);

		dojo.ready(function() {
			if (fadeinDelay != null)
				fadeinDelay = clearTimeout(fadeinDelay);
			if (!dojo.isIE || dojo.isIE > 8) {
				fadeinDelay = setTimeout(function() {
					hideElements.forEach(function(element, index) {
						app.animate(element, { opacity: 1 }, { duration: 500 });
					});
				}, 500);
			}else{
				hideElements.style({ opacity: 1, visibility: 'visible' });
			}
			if('' == 'STYLEGUIDE'){
				if (head('rl_head')) {
					head('rl_head').lite();
				}
			}else{
				head('rl_head').firePageLoadEvents();
			}
		});
		//init topnav
		topnav('rl_brand_nav');
	});
</script>
