<script type="text/javascript">
// Summary: Returns the ShipTo country.
// Description: Parse the given URL to set the country parameter. If such parameter cannot be found, returns null. 
// Returns: the string for country parameter like 'HK', and null if the parameter cannot be found in the given URL.
function getShipToCountryFromURL(url) {
    if (url == null || url == undefined || url.length == 0) {
        url = document.URL;
    }
    var country = null;
    var tmpCountry = null;
    var countryParam = url.match(/[&\?]country=([^&]*)/g);
    if (countryParam != null && countryParam.length > 0) {
        tmpCountry = countryParam[0].substring(9);
    }
    var countryPattern = /^[A-Z][A-Z]$/g;
    if(tmpCountry != undefined & tmpCountry != null & tmpCountry != "" & countryPattern.test(tmpCountry)) {
        country = tmpCountry;
    }
    return country;
}

// Summary: Returns the ShipTo currency.
// Description: Parse the given URL to get the currency parameter. If such parameter cannot be found, returns null. 
// Returns: the string for currency parameter like 'HKD', and null if the parameter cannot be found in the given URL.
function getShipToCurrencyFromURL(url) {
    if (url == null || url == undefined || url.length == 0) {
        url = document.URL;
    }
    var currency = null;
    var tmpCurrency = null;
    var currencyParam = url.match(/[&|\?]currency=([^&]*)/g);
    if (currencyParam != null && currencyParam.length > 0) {
        tmpCurrency = currencyParam[0].substring(10);
    }
    var currencyPattern = /^[A-Z][A-Z][A-Z]$/g;
    if(tmpCurrency != undefined & tmpCurrency != null & tmpCurrency != "" & currencyPattern.test(tmpCurrency)) {
        currency = tmpCurrency;
    }
    return currency;
}

// Summary: Add ShipTo Country and Country parameter to a given URL.
// Description: Use the current document URL as reference, extract the country and currency information, and set it to a given URL.
// Returns: the URL with country and currency parameter.
function setShipToCountryAndCurrencyForURL(originalurl) {
    if (originalurl == null || originalurl == undefined || originalurl.length == 0) {
        return null;
    }
    var targetCountry = getShipToCountryFromURL();
    var targetCurrency = getShipToCurrencyFromURL();
    var currentCountry = getShipToCountryFromURL(originalurl);
    var currentCurrency = getShipToCurrencyFromURL(originalurl);
    var targeturl = originalurl;
    if (targetCountry != null && currentCountry == null) {
        targeturl = targeturl.replace(/country=([^&]*)&/g,"");
        targeturl = targeturl + "&country=" + targetCountry;
    }
    if (targetCurrency != null && currentCurrency == null) {
        targeturl = targeturl.replace(/country=([^&]*)&/g,"");
        targeturl = targeturl + "&currency=" + targetCurrency;
    }
    return targeturl;
}
</script>
