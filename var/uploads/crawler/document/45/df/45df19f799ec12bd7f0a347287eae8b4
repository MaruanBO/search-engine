	<script type="text/javascript">
		function applyShipping(){
			var currency;
			if(document.getElementById('shippingCurrencies')!=null){
				currency=document.getElementById('shippingCurrencies').options[document.getElementById('shippingCurrencies').selectedIndex].value;
			}
			var country;
			if(document.getElementById('shippingCountry')!=null){
				country=document.getElementById('shippingCountry').options[document.getElementById('shippingCountry').selectedIndex].value;
			}

			if(currency == null || currency == '' || country == null || country == ''){
				return;
			}
			
			if(analyticsEnabled == "true"){
				cmCreateElementTag ('Country Toggle - ' + country, 'Country Toggle', '-_-Country: ' + country);
				cmCreateElementTag ('Currency Toggle - ' + currency, 'Currency Toggle', '-_-Currency: ' + currency);
			}
			
			AEPUserPreferences.setUserPreferencesToCookie(
				{"country":""+country+"",
				"currency":""+currency+"",
				"language":""}
				);
			AEPCommon.closeDojoDialog('inter_shipping');
		}
		
		/*
		*show country and currency selection dialog
		*initial close button listen event
		*/
		function showOverlayDialog(){
			AEPCommon.showDojoDialog('inter_shipping','inter_shipping');
			var selectCmp1 = app.Select('shippingCountry'),
				selectCmp2 = app.Select('shippingCurrencies'),
				mask = dojo.query('.mm_ui_mask'),
				btn = dojo.query("#inter_shipping .btn_link");

			// black transparent background
			mask.style('background', '#000000');
				
			selectCmp1.addEvent('change', function(index){
				if(index > 0){
					MessageHelper.hideErrorMessage("country_msg_1");
					if(selectCmp2.getSelectedIndex() > 0){
						btn.removeClass('btn_4');
						btn.addClass('btn_1');
					}
				} else {
					MessageHelper.showErrorMessage("country_msg_1", "Please select your location.", "country_msg_1");
					btn.removeClass('btn_1');
					btn.addClass('btn_4'); 
				}
			}); 
			
			selectCmp2.addEvent('change', function(index){
				if(index > 0){
					MessageHelper.hideErrorMessage("country_msg_2");
					if(selectCmp1.getSelectedIndex() > 0){
						btn.removeClass('btn_4');
						btn.addClass('btn_1');
					}
				} else {
					MessageHelper.showErrorMessage("country_msg_2", "Please select your currency.", "country_msg_2");
					btn.removeClass('btn_1');
					btn.addClass('btn_4'); 
				}
			}); 
		}
		
		
	</script>
