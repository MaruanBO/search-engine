<script type="text/javascript">
		var UrlFormatter = {
		// public method for url encoding
		encode : function (string) {
			return escape(this._utf8_encode(string));
		},
		// public method for url decoding
		decode : function (string) {
			return this._utf8_decode(unescape(string));
		},
		// private method for UTF-8 encoding
		_utf8_encode : function (string) {
			string = string.replace(/\r\n/g,"\n");
			var utftext = "";
		 
			for (var n = 0; n < string.length; n++) {
				var c = string.charCodeAt(n);
				if (c < 128) {
					utftext += String.fromCharCode(c);
				}
				else if((c > 127) && (c < 2048)) {
					utftext += String.fromCharCode((c >> 6) | 192);
					utftext += String.fromCharCode((c & 63) | 128);
				}
				else {
					utftext += String.fromCharCode((c >> 12) | 224);
					utftext += String.fromCharCode(((c >> 6) & 63) | 128);
					utftext += String.fromCharCode((c & 63) | 128);
				}
			}
			return utftext;
		},
	 
		// private method for UTF-8 decoding
		_utf8_decode : function (utftext) {
			var string = "";
			var i = 0;
			var c = c1 = c2 = 0;
			while ( i < utftext.length ) {
				c = utftext.charCodeAt(i);
				if (c < 128) {
					string += String.fromCharCode(c);
					i++;
				}
				else if((c > 191) && (c < 224)) {
					c2 = utftext.charCodeAt(i+1);
					string += String.fromCharCode(((c & 31) << 6) | (c2 & 63));
					i += 2;
				}
				else {
					c2 = utftext.charCodeAt(i+1);
					c3 = utftext.charCodeAt(i+2);
					string += String.fromCharCode(((c & 15) << 12) | ((c2 & 63) << 6) | (c3 & 63));
					i += 3;
				}
			}
			return string;
		}
	}
	function goSearch(){
		// get the brand name and take the id as search conditions
		var searchTerm=document.getElementById("searchInput").value.replace(/^\s*(.*?)[\s\n]*$/g,'$1');
		document.getElementById("searchTermInput").value = removeSpecialCharsFromSearchTerm(searchTerm);
		dojo.byId('CatalogSearchForm').submit();
	}
</script>
<script type="text/javascript">
	dojo.addOnLoad(function() {
		countOrderQty();
	});
	function countOrderQty() {
		var url = "http://www.ralphlauren.asia/webapp/wcs/stores/servlet/AjaxCountItemsInShopCartDisplayView?catalogId=12551&langId=-1&storeId=12151";
		
		var protocol = document.location.protocol;
		if(protocol=="http:" && url.indexOf("https:") >= 0){
			url = url.replace("https","http");
		}
		
		if(protocol=="https:" && url.indexOf("http:") >= 0){
			url = url.replace("http","https");
		}
		
		var absoluteURL = getAbsoluteURL();
		if(url.indexOf(absoluteURL)<0){
			url = absoluteURL + url;
		}
		
		dojo.xhrPost({
			url: url,
			load: function(response) {
				//get the order count and person lastname & firstname
				if(response != null && response != undefined){
					var tempDiv = document.createElement("div");
					tempDiv.innerHTML = response;
					var elements = dojo.query('[id=shopCartData]',tempDiv);
					
					if(elements.length){
						var shopCartData = elements[0].innerHTML;
						var datas = shopCartData.split(':');
						var orderCount = datas[0];
						var personInfo = datas[1];
						var partiallyAuthenticated=datas[2];
						var isRememberMe = datas[3];
						var userType = datas[4];
						
						if(orderCount != null && orderCount != ""){
							if(dojo.byId("SignOffQty")){
								dojo.byId("SignOffQty").innerHTML=parseInt(trim(orderCount));
							}
							if(dojo.byId("SignInQty")){
								dojo.byId("SignInQty").innerHTML=parseInt(trim(orderCount));
							}
						}
						
						if(partiallyAuthenticated=='true' || userType == 'G'){
							dojo.byId("welcomeinfo_link_ul").style.display="none";
							dojo.byId("signin_link_ul").style.display="block";
						}
						else{
							dojo.byId("welcomeinfo_link_ul").style.display="block";
							dojo.byId("signin_link_ul").style.display="none";
						}
						
						if(personInfo != null && personInfo != ""){
							dojo.byId("userNameSpan").innerHTML = personInfo;
						}
					}
				}
			},
			error: function(response) {
					
			}
		});
	};
	
</script>
