var siteRedirect = '' ;
// var siteRedirect = null;
var siteRedirectParam = document.URL.match(/[&\?]siteRedirect=([^&]*)/g);
if (siteRedirectParam != null && siteRedirectParam.length > 0) {
    siteRedirect = siteRedirectParam[0].substring(14);
}
var stayOnDesktop = '' ;

// temporary fix for leegardens
var desktoponlyPattern = /http(s)?:\/\/[^\/]*\/(en|tc)\/leegardens/gi;
var currentURL = document.URL;
var desktoponlyMatched = currentURL.match(desktoponlyPattern);
if (desktoponlyMatched != null && desktoponlyMatched.length > 0) {
    siteRedirect = 'false';
}

if(stayOnDesktop == 'true'){
	SiteSwitcher.addShowFullSiteCookie();
}

console.info('siteRedirect:' + siteRedirect + ' & stayOnDesktop:' + stayOnDesktop);

var showFullSite = SiteSwitcher.getCookie('RLShowFullShipToESite');
if (showFullSite == null || showFullSite == undefined || showFullSite == 'false') {
	if (SiteSwitcher.isMobileDevice()) {
		if (siteRedirect == 'false') {
			
			var url = SiteSwitcher.getMobileRedirectUrlFromWhiteListOrNull();
			console.info('url:' + url);
			if (url) {
				window.location.href = url;
			} else {
				
				SiteSwitcher.addShowFullSiteCookie();
			}
		} else {
			var url = SiteSwitcher.getMobileRedirectUrlFromWhiteListOrDefaultPage();
			window.location.href = url;
		}
	}
} else {
	console.info('showFullSite: ' + showFullSite);
}
