		function getGener(){
		 var genderVal='GENDER'
		 var gender = document.getElementsByName('gender');
		 for(i=0; i<gender.length; i++) { 
		     if(gender[i].checked) { 
		       genderVal = gender[i].value;break; 
		     }
		  }
		  return genderVal;
		}
