<script>
var de = de || {};
de.bild = de.bild || {};
de.bild.feedLoader = (function($) {
 var settings = {
  'rssUrl' : '-30373542,sort=1,n=1,view=rss2.bild.xml', //URL des Feeds
  'template' : '<div class="hentry  overlay t6o5"><a rel="bookmark" href="/regional/berlin/brandstiftung/pakete-mit-napalm-brandsaetzen-verschickt-39825374,artgalid=30340124,clicked=1.bild.html"><img alt="##TITLE##" src="##ENCLOSURE##" class="photo"><h2 class="entry-title"><span class="kicker">##KICKER##</span><span class="headline">##HEADLINE##</span> </h2></a></div>', //Template String mit Replacements ##TITLE##,##LINK## ...
  'containerSelector' : '.te-bln-heute', //Selector, des zu befüllenden Containers
  'elementCount' : '0', //Anzahl der Elemente
  'startIndex' : '0', //Startindex der Elemente
  'itemNode' : 'item', //Knoten die traversiert werden
  'autoProcess' : true, //Parameter der angibt, ob ob das Parsen durch die Funktion genutzt werden soll, andernfalls dient der ajaxCompleteCallback
  'ajaxCompleteCallback' : function() {}, //Funktion die nach dem erfolgreichen Ajax Laden, bekommt das Ergebniss übergeben
  'beforeAjaxCallback' : function() { }, //Function die vor dem Ajaxaufruf aufgerufen wird
  'manipulateData' : function(data){return data},
  'afterEachCallback' : function() { }, //Function, die nach jedem Knoten aufgerufen wird
  'finishCallback' : function() {} //Callback zur Initialisierung default: Videoinit
 }
 //Funktion um Settings zu überschreiben
 function updateSettings(data) {
  if(data) {
   // intialise runtime functions
   for(var key in data) {
    // the variable must already exist and be of the same type
    if(settings.hasOwnProperty(key) && typeof settings[key] === typeof data[key]) {
     settings[key] = data[key];
    }
   }
  }
  return settings;
 };
 function init($elem) {
  settings.beforeAjaxCallback($elem);
  $.ajax({
   type : 'GET',
   dataType : "xml", 
   url : settings.rssUrl,
   fail : function() {
     $elem.closest('.module').hide();
   },
   success : function(data) {
    settings.ajaxCompleteCallback(data);
    if(settings.autoProcess) {
     for(var i = 0; i <= settings.elementCount; i++) {
      var $data = $(data).find(settings.itemNode + ':eq(' + i + ')');
      var template = settings.template;
      var dataObj = {};
     
      dataObj["title"] = $data.find('title').text().replace('*** BILDplus Inhalt *** ', '');
      dataObj["link"] = $data.find('link').text();
      dataObj["description"] = $data.find('description').text();
      dataObj["guid"] = $data.find('guid').text();
      dataObj["category"] = $data.find('category').text();
      var el = ''+$data.find('enclosure').attr('url');      
      dataObj["enclosure"] = el.replace(',w=120',',w=468');
      dataObj["pubDate"] = $data.find('pubDate').text();
           
     if(dataObj["title"].indexOf(' - ')!=-1){
      var t = dataObj["title"].split(' - ');
      dataObj["headline"] = t[1]; 
      dataObj["kicker"] = t[0]; 
      }
      else{
       dataObj["headline"] = ''; 
      dataObj["kicker"] = ''; 
      }
     
     
      dataObj = settings.manipulateData(dataObj);
     
      template = template
      .replace(/##TITLE##/g, dataObj["title"])
      .replace(/##LINK##/g, dataObj["link"])
      .replace('##DESCRIPTION##', dataObj["description"])
      .replace('##GUID##', dataObj["guid"])
      .replace('##CATEGORY##', dataObj["category"])
      .replace('##ENCLOSURE##', dataObj["enclosure"])
      .replace('##PUBDATE##', dataObj["pubDate"])
      .replace('##KICKER##', dataObj["kicker"])
      .replace('##HEADLINE##', dataObj["headline"]);
      $elem.append(template);
      settings.afterEachCallback();
     }
    }
    settings.finishCallback($elem);
   }
  })
 }
 return {
  'init' : function(newSettings) {
   if(newSettings) {
    updateSettings(newSettings);
   }
   if($(settings.containerSelector).size() > 0) {
    init($(settings.containerSelector));
   }
  }
 }
})(jQuery);

de.bild.feedLoader.init();

</script>



</div>
            </div>
        </div>
