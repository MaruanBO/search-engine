<script>
  
 var teaserTmpl = '<div class="hentry landscape t9l">'+
                  '<a href="##LINK##" rel="bookmark" target="_blank">'+
                      '<img src="##IMG##" alt="" class="photo" />'+
       '<div>'+
                  '<h3 class="entry-title">'+
                   '<span class="kicker">##DZ##</span>'+
                   '<span class="headline">##TEXT##</span>'+
                  '</h3>'+
       '</div> '+             
      '</a>'+
     '</div>';

 
 var STBurls=["/home/sportbild/kt-03-26987392,contentContextId=16237890.bild.html","/home/sportbild/kt-02-26987390,contentContextId=16237890.bild.html","/home/sportbild/kt-01-26987386,contentContextId=16237890.bild.html"]; 

for(var i=0;i<STBurls.length;i++){
jQuery.ajax({
url:STBurls[i],
cache : true,
success:function(data,xhr){
data = jQuery(data);
var headline = jQuery('.headline',data).eq(0).text();
var text = jQuery('p',data).eq(0).text();
var link = '';
jQuery('p',data).each(function() {
    var tx = jQuery(this).text();
    if(tx.indexOf('http://') != -1) {
        link = tx;
    }
});
var img = jQuery('img',data).attr('src');
img = img.replace(/h=[0-9]+/, 'h=99');
var teaser = teaserTmpl.replace("##DZ##",headline).replace("##TEXT##",text).replace("##IMG##",img).replace("##LINK##",link);
jQuery('#sportbildNews .clearfix').append(teaser);
}
});

 }
</script>
