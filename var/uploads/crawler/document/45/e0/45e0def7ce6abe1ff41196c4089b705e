<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function(){
    var sHeaderSearch = 'Advanced Search';
    $('#blox-header-search input.search-bar').attr('value', sHeaderSearch);
    $('#blox-header-search input.search-bar').focus(function(){
        if($(this).attr('value') == sHeaderSearch) {
            $(this).attr('value', '');
        }
    });
    $('#blox-header-search input.search-bar').blur(function(){
        if($(this).attr('value') == '') {
            $(this).attr('value', sHeaderSearch);
        }
    });
});
//]]>
</script>

