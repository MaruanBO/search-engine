function canPreloadImages() {
    
    if (! window.location) return true;
    
    var fragment = window.location.hash;
    if (! fragment) return true;
    
    if ( fragment.indexOf("page=g") !== -1 && 
            (fragment.indexOf("dept=") === -1 || 
                    fragment.indexOf("dept="+window.User.PreferredDepartment) !== -1)) {
        return true;
    }
    return false;
}
if (canPreloadImages()) {

}



