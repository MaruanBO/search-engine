				    function _heroFade(){
						$(".hero-nav li:first").addClass("selected");
                                                
                                                 var rotate_interval = 10;
                                                
						    rotate_interval = 10;
                                                
                                                rotate_interval = rotate_interval * 1000;
                                                var c={slideChange:function(e){$(".hero-nav li").removeClass("selected");$(".hero-nav li:eq("+e+")").addClass("selected")},slideType:"fade",showTime:rotate_interval};
                                                 
						$(".hero-content").dumbCrossFade(c);
						var d=0;
						$(".hero-nav li a").each(function(){$(this).attr("href",d+"");d++});
						$(".hero-nav li a").click(function(){$(".hero .hero-content").dumbCrossFade("jump",parseInt($(this).attr("href"))).dumbCrossFade("stop");return false})
					}
					_heroFade();
				  </script>

                   
