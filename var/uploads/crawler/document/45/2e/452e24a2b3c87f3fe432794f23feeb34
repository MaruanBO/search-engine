  var BBVA_TopNavInited = false;
  function BBVA_initTopNav(){
	  $(".nav li").each(function(){
		var heightRow1 = $(this).find("div.row1").map(function ()
		{ return jQuery(this).height();
		}).get(),
	
		maxHeightRow1 = Math.max.apply(null, heightRow1);
		var heightRow2 = $(this).find("div.row2").map(function ()
		{
			return $(this).height();
		}).get(),
	
		maxHeightRow2 = Math.max.apply(null, heightRow2);	
		$(this).find('div.row1').height(maxHeightRow1);
		$(this).find('div.row2').height(maxHeightRow2);
	   });
	   BBVA_TopNavInited = true;
           if(typeof(console) != 'undefined'){ console.log('Top nav inited'); }
  }
</script>
 












