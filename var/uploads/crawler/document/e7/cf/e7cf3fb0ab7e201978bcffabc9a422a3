<div id="node-509426" class="node node-homepage node-promoted view-mode-full clearfix" about="/homepage/clone-hp-21815" typeof="sioc:Item foaf:Document">

  
        <h2 class="node__title node-title"><a href="/homepage/clone-hp-21815">HP 2/19/15</a></h2>
    <span property="dc:title" content="HP 2/19/15" class="rdf-meta element-hidden"></span><span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span>
      <div class="content">

    <div class="full-width-wrap" id="hero-wrap">
      

<div class="module-feature-hero">
  <div class="jcarousel-wrap">
    <div class="hero-gallery jcarousel" data-jcarousel="true">
      <ul class="hero-data-carousel">

        
          
          <li data-carousel-index="1"class="active">
            <a href="http://news.instyle.com/category/fashion-week/fashion-week-fall-2015/">
                              <img src="http://cdn-img.instyle.com/sites/default/files/styles/1000x563/public/images/2015/02/021815_FashionWeek2_1000_0.jpg?itok=g0LKjEJs" alt="hero"/>
                          </a>
                          <div class="blurb ">
                <article class="post">
                  <div class="article-content">
                    <div class="article-type">
                      <span class="slug slug-awards-events">Fall Fashion Week</span>
                    </div>
                    <h2 class="article-title">
