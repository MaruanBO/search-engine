jQuery('#slider-0').find(".item").css({"width":itemWidth});
			}else if(jQuery("#slider-0").hasClass("playlist")){
				jQuery('#slider-0').find(".group").css({"padding":"0px"});

				var groupWidth = jQuery('#slider-0').find(".group").width();
				var itemWidth = (groupWidth/3)-40;

				jQuery('#slider-0').find(".item").css({"height":"154px","width":itemWidth});
				jQuery('#slider-0').find(".item").find(".image").css({"width":itemWidth});

			}
		}

		


	jQuery('#slider-0 .prev').show();
	jQuery('#slider-0 .next').show();


        // BEFORE TRANSITION
        function onBeforeslider0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAfterslider0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#slider-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#slider-0 .item-list').cycle({
		fx: effectslider0,
		sync: true,
		
		
		timeout: 35,
                before: onBeforeslider0,
		after:	onAfterslider0,
        pause:true,
		pager:'#slider-0 .nav',
		prev:'#slider-0 .prev',
		next:'#slider-0 .next'
	});
        
            $('#slider-0 .pause').on('click',function(){
                if($('#slider-0').hasClass('paused')){
                    $('#slider-0').removeClass('paused').find('.item-list').cycle('resume');
                }else{
                    $('#slider-0').addClass('paused').find('.item-list').cycle('pause');
                }
            });
        

        

}



// ]]> -->
</script>



</div></div>
        </div>
        
    
        </div>
    </div>
