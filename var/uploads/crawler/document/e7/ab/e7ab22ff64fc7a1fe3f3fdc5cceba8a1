        //<![CDATA[
        var $scroTop = jQuery.noConflict();
        var fixnav = 100;
        function resetnav() {
            var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
            if (scrollTop > fixnav && navbar.className === "main-nav") {
                document.getElementById("navbar").className = "nav-fixed";
            }
            else if (scrollTop < fixnav && navbar.className === "nav-fixed") {
                document.getElementById("navbar").className = "main-nav";
            }
        }
        $scroTop(document).ready(function () {
            if (!window.addEventListener) {
                window.attachEvent("scroll", resetnav);
            }
            else {
                window.addEventListener("scroll", resetnav, false);
            }
        });
        //]]>
    </script>
    

    
  
    <script type="text/javascript">
    var $mobiNav = jQuery.noConflict();
    $mobiNav(document).ready(function(){	

	    var menuStatus;
 
        $mobiNav("#left").bind("click",function(){
            //alert('1');
            if(menuStatus != true)
            {
                //alert('2');
                $mobiNav("#wrapper").animate({marginLeft: "65%", marginRight: "-65%",}, 300, function(){menuStatus = true});
                return false;
            } 
            else 
            {
                //alert('3');
                $mobiNav("#wrapper").animate({marginLeft: "0px", marginRight: "0px",}, 300, function(){menuStatus = false});
                return false;
            }
        });
  
        $mobiNav("#menu li a").click(function(){
            var p = $mobiNav(this).parent();
            if($mobiNav(p).hasClass('active')){
                $mobiNav("#menu li").removeClass('active');
            } else {
                $mobiNav("#menu li").removeClass('active');
                $mobiNav(p).addClass('active');
            }
        });
       
    });
    </script>
    
    
    
