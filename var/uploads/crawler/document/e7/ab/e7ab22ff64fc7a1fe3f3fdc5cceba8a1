    <script type="text/javascript" src="http://www.image.ie/_local/scripts/jquery.reveal.js" ></script>
    <script type="text/javascript">
        var $signUp = jQuery.noConflict();
        if (!$signUp.cookie('bShowPop')) {
            
            $signUp('#myModal').reveal({
                animation: 'fadeAndPop',                   //fade, fadeAndPop, none
                animationspeed: 300,                       //how fast animtions are
                closeonbackgroundclick: true,              //if you click background will modal close?
                dismissmodalclass: 'close-reveal-modal'    //the class of a button or element that will close an open modal
            })

            //set the cookie
            $signUp.cookie('bShowPop', 'false', { expires: 14, path: '/' });
        }

    </script>
    
    
    

    <script type="text/javascript">

        

        var $adLoad = jQuery.noConflict();

        //IF WE ARE SCROLLING LEFT OR RIGHT USING THE ARROOWS BESIDE THE MAIN PHOTO-----------------------
        $adLoad('.carousel-stage').on('scroll.jcarousel', function (event, carousel, target, animate) {

            if (target == "+=1") {
                if ($adLoad(".next-stage").attr("class") != "next next-stage inactive") {
                    //this raises the hashchange event below
                    window.location.hash = '!img=' + ($adLoad('.carousel-stage').jcarousel('target').index() + 2);
                }
            }
            else if (target == "-=1") {
                if ($adLoad(".prev-stage").attr("class") != "prev prev-stage inactive") {
                    //this raises the hashchange event below
                    window.location.hash = '!img=' + ($adLoad('.carousel-stage').jcarousel('target').index());
                }
            }
        });

        //IF WE ARE USING THE BOTTOM SMALL CAROUSEL TO CHOOSE AN IMAGE--------------------------------
        $adLoad('.carousel-navigation li img').on('click', function () {
            //this raises the hashchange event below
            window.location.hash = '!img=' + ($adLoad(this).parent('li').index() + 1);
        });

        //THIS FUNCTION CHANGES THE URL FOR THE BENEFIT OF THE AD------------------------------------
        //THEN RELOADS THE ADS ON THE PAGE
        function FireAd(iIndex) {

            //SORT OUT THE SHARE BUTTONS
            $adLoad(".addthis_toolbox, .addthis_button_facebook_like, .addthis_button_tweet, .addthis_button_pinterest_pinit, .addthis_button_google_plusone").attr("addthis:url", window.location);

            //save the current ad code
            var topAdHtml = $adLoad(".top-ad").html();
            //var shareHtml = $adLoad(".addthis_toolbox").html();

            var notScriptAd1 = ((topAdHtml.indexOf("<iframe") > topAdHtml.indexOf("<script")) && (topAdHtml.indexOf("<script") == -1));

            //console.log("notScriptAd1: " + notScriptAd1)

            //check if the ads are iFrames - must be, or else do nothing.
            if (topAdHtml.indexOf("<iframe") != -1 && notScriptAd1) {

                //console.log("topAdHtml fired");

                //ensure no movement on screen...
                $adLoad(".top-ad").attr("style", "min-height:90px")

                //clear the ad divs
                $adLoad(".top-ad").empty();

                //reload in the ad code
                $adLoad(".top-ad").html(topAdHtml);
            }

            //same again for the RHS ad
            //but this time we need to loop thru all the side ads
            $adLoad(".sidecol .ad, .mobile-article-ad, .bottom-ad").each(function () {

                var rightAdHtml = $adLoad(this).html();

                //make sure the ads re-render only with the following rules.
                //If the ad is in this format / render or not:
                //<iFrame only - then re-render OK
                //<script only - DONT re-render
                //<script followed by <iFrame - DONT re-render
                //this was put in place on 11/Apr/2014 because sometimes
                var notScriptAd2 = ((rightAdHtml.indexOf("<iframe") > rightAdHtml.indexOf("<script")) && (rightAdHtml.indexOf("<script") == -1));

                //console.log("notScriptAd2: " + notScriptAd2)

                if (rightAdHtml.indexOf("<iframe") != -1 && notScriptAd2) {

                    //console.log("rightAdHtml fired");

                    $adLoad(this).empty();

                    $adLoad(this).html(rightAdHtml);
                }

            });


            //$adLoad(".addthis_toolbox").empty();
            //$adLoad(".addthis_toolbox").html(shareHtml);

            //$adLoad.getScript('http://s7.addthis.com/js/300/addthis_widget.js#async=1');
            //addthis.init();
            //addthis.toolbox('.addthis_toolbox');

        }

        //SCROLLING TO THE CORRECT IMAGE IF THE # IMG IS SPECIFIED------------------
        $adLoad(document).ready(function () {
            //make sure there is a #present in th url
            if (window.location.hash) {
                //use the jcarousel scroll method
                //scroll to the correct position
                //get the position by splitting img=5 on the '=' and taking the 2nd element in array
                //add one cause its zero based
                $adLoad('.carousel-stage').jcarousel('scroll', parseInt(window.location.hash.substring(1).split("=")[1])-1);
            }

        });

        //listener:
        //THIS DEALS WITH FIRING THE FIREAD FUNCTION EVERY TIME
        //THAT THE IMAGE LEFT/RIGHT OR CAROUSEL IS CLICKED
        //OR THERE IS A BACK / FORWARD IN BROWSER------------------------------------
        $adLoad(window).bind('hashchange', function () {
            //get the position by splitting img=5 on the '=' and taking the 2nd element in array
            //add one cause its zero based

            //alert("hashchange fired");
            FireAd(parseInt(window.location.hash.substring(1).split("=")[1]));
            $adLoad('.carousel-stage').jcarousel('scroll', parseInt(window.location.hash.substring(1).split("=")[1]) - 1);

            //_gaq.push(['_trackPageview']);

            //July 2014 - commented out at Noelles request
            //Left/Right photo clicks should not show on GA
            //ga('send', 'pageview');
        });

    </script>
    <!-- Quantcast Tag -->
    <script type="text/javascript">
        var _qevents = _qevents || [];

        (function () {
            var elem = document.createElement('script');
