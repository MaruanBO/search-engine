		<script>	
			function fnFBLogin() {
			
			my_form=document.createElement('FORM');
			my_form.name='myForm';
			my_form.method='POST';
			my_form.action='https://www.tracfone.com/direct/signin/facebook';

			my_tb=document.createElement('INPUT');
			my_tb.type='HIDDEN';
			my_tb.name='scope';
			my_tb.value='email,offline_access';
			my_form.appendChild(my_tb);

			my_tb=document.createElement('INPUT');
			my_tb.type='HIDDEN';
			my_tb.name='request_value';
			my_tb.value='signin_via_facebook_in_tracfone';
			my_form.appendChild(my_tb);
			
			my_tb=document.createElement('INPUT');
			my_tb.type='HIDDEN';
			my_tb.name='lang';
			my_tb.value='en';
			my_form.appendChild(my_tb);
			
			document.body.appendChild(my_form);

			my_form.submit();
 	     }
        	</script>
        
