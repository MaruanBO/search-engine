<script>
    if(window['postMessage']){
        document.getElementById("headerLoginForm").src = 'https://www.woodcraft.com/headerloginform.aspx?proto='+document.location.protocol.replace(':','');
        var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
        var eventer = window[eventMethod];
        var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";
        eventer(messageEvent,function(e){
            if(e.origin=='https://www.woodcraft.com'){
                var hmsga=e.data.split(':')[0];
                if(hmsga=='0')AJUpdateHeader();
                if(hmsga=='1')AJacRedirect();
                if(hmsga=='2')location.href=e.data.split(':')[1];
            }
        });
    }else{$('#tiptop_account_default').css('display','none');}
</script>


