    <script>
        function mobilegetCookie(c_name)
        {
            var i,x,y,ARRcookies=document.cookie.split(";");
            for (i=0;i<ARRcookies.length;i++)
            {
              x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
              y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
              x=x.replace(/^\s+|\s+$/g,"");
              if (x==c_name)
                {
                return unescape(y);
                }
              }
        }
        if(window.location.search == '?MobileOptOut=1'){
            var today = new Date();
            var expires_date = new Date( today.getTime() + (7776000000) );
            document.cookie = "MobileOptOut=1;expires="+expires_date.toGMTString()+";path=/";
        }
        var agent = navigator.userAgent;
        if(mobilegetCookie('MobileOptOut') != 1 && !agent.match(/iPad/i))
            if(agent.match(/iPhone/i)||agent.match(/Mobile/i)||agent.match(/iPod/i)||agent.match(/BlackBerry/i)||agent.match(/Windows\ CE/i)||agent.match(/Windows\ Mobile/i)||agent.match(/Windows\ Phone/i)||agent.match(/iemobile/i)||agent.match(/webOS/i)||agent.match(/Palm/i)||agent.match(/PalmOS/i)||agent.match(/Smartphone/i)||agent.match(/Handheld/i)||agent.match(/Opera\ Mini/i)||agent.match(/HTC/i)||agent.match(/GooglebotMobile/i)||agent.match(/MSN\ Mobile\ Proxy/i))
                window.location = 'http://m.woodcraft.com';
    </script>

