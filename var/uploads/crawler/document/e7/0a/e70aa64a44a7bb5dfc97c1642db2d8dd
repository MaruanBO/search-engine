    <script>
		var badChar = /<.*?>|\[.*?\]/gi;
		var retVal;
		function checkFormFields(){
			retVal = true;
			$("input:text").each(function(index) {
				if($(this).val().match(badChar) != null)
				{
					$(this).css('color','#990000');
					retVal = false;
				}
			});
			return retVal;
		}
