<script type="text/javascript">
<!-- <![CDATA[

ninja=false;if(typeof(ninjaLog)!="undefined"){ninja=true;};

$(function(){
	shorecast0sliderSizeAdjust();
});

effectshorecast0 = "fade";

function shorecast0sliderSizeAdjust(){

	if(ninja){ninjaLog.Update({text:"Slider shorecast-0 Initial Transition is: fade"});}

	jQuery('#shorecast-0 .loading').hide();
	
	jQuery('#shorecast-0 .item-list').show();

	if(((jQuery('#shorecast-0').width()<301) || ("horizontal" == "vertical")) && ("horizontal" != "horizontal")){

		jQuery("#shorecast-0").removeClass("horizontal").addClass("vertical");
		jQuery("#shorecast-0").height(jQuery("#shorecast-0").find(".item").height()*3+65+(12*3));
		jQuery("#shorecast-0").find(".item-list").height(jQuery("#shorecast-0").find(".item").height()*3+(12*3));


		jQuery(jQuery("#shorecast-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<3){
    				jQuery(this).height("100%");
			}
		});

		if(effectshorecast0 == "scrollHorz"){ effectshorecast0 = "scrollVert";}

		if(ninja){
			ninjaLog.Update({text:"Slider is Vertical",style:""});
			ninjaLog.Update({text:"Using effect: "+effectshorecast0});
		};
	}

	if(effectshorecast0=="scrollHorz"){

		var sliderAreaWidth = jQuery("#shorecast-0").width();
		var columnAdjust = 38;

		if(typeof(ninjaLog)!="undefined"){
			ninjaLog.Update({text:"Slider is Horizontal",style:""});
			ninjaLog.Update({text:"area width is: "+sliderAreaWidth});
			ninjaLog.Update({text:"item width is: "+jQuery("#shorecast-0").find(".item").width()});
			ninjaLog.Update({text:"columnAdjust is: "+columnAdjust});
		}
	}

		if(3>1){


			if((sliderAreaWidth-2-(columnAdjust*3)<(jQuery("#shorecast-0").find(".item").width())*3)||(false)){
				if(ninja){ninjaLog.Update({text:"Width of items require adjustment",style:""});};
				jQuery("#shorecast-0").find(".item").width(Math.ceil((sliderAreaWidth-2-(columnAdjust*3))/3));
			}
		}else{
			if(typeof(ninjaLog)!="undefined"){
				ninjaLog.Update({text:"Adjusting for a single column: 70px"});
			}
			jQuery("#shorecast-0").find(".item").width(sliderAreaWidth-70);
		}

		jQuery(jQuery("#shorecast-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<3){
    				jQuery(this).width("100%");
			}
		});

		if(jQuery.browser.msie && Number(jQuery.browser.version)<7){
//alert("#"+jQuery(jQuery("#shorecast-0").parents(".tncms-region")[0]).attr("id")+" width: "+jQuery(jQuery("#shorecast-0").parents(".tncms-region")[0]).width());

			//jQuery("#shorecast-0").find(".group .item .image").hide();
			jQuery("#shorecast-0").find(".group .item .text").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#shorecast-0").find(".group .item .text .description").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#shorecast-0").width(jQuery("#shorecast-0").parent().width());

			if(jQuery("#shorecast-0").hasClass("showcase")){
				jQuery('#shorecast-0').find(".group").css({"padding":"0px"});

				var itemWidth = jQuery("#shorecast-0").find(".item").width()-((3) * 10);

				var groupWidth = jQuery('#shorecast-0').find(".group").width();
var itemWidth = (groupWidth/3)-40;
jQuery('#shorecast-0').find(".item").css({"width":itemWidth});
			}else if(jQuery("#shorecast-0").hasClass("playlist")){
				jQuery('#shorecast-0').find(".group").css({"padding":"0px"});

				var groupWidth = jQuery('#shorecast-0').find(".group").width();
				var itemWidth = (groupWidth/3)-40;

				jQuery('#shorecast-0').find(".item").css({"height":"154px","width":itemWidth});
				jQuery('#shorecast-0').find(".item").find(".image").css({"width":itemWidth});

			}
		}

		


	jQuery('#shorecast-0 .prev').show();
	jQuery('#shorecast-0 .next').show();


        // BEFORE TRANSITION
        function onBeforeshorecast0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAftershorecast0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#shorecast-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#shorecast-0 .item-list').cycle({
		fx: effectshorecast0,
		sync: true,
		fit: 1,
		
		timeout: 0,
                before: onBeforeshorecast0,
		after:	onAftershorecast0,
        pause:true,
		pager:'#shorecast-0 .nav',
		prev:'#shorecast-0 .prev',
		next:'#shorecast-0 .next'
	});
        

        

}



// ]]> -->
</script>



</div></div><div class="clear"></div>
    
