<script type="text/javascript">
<!-- <![CDATA[

ninja=false;if(typeof(ninjaLog)!="undefined"){ninja=true;};

$(function(){
	seen_slideshow0sliderSizeAdjust();
});

effectseen_slideshow0 = "none";

function seen_slideshow0sliderSizeAdjust(){

	if(ninja){ninjaLog.Update({text:"Slider seen_slideshow-0 Initial Transition is: none"});}

	jQuery('#seen_slideshow-0 .loading').hide();
	
	jQuery('#seen_slideshow-0 .item-list').show();

	if(((jQuery('#seen_slideshow-0').width()<301) || ("horizontal" == "vertical")) && ("horizontal" != "horizontal")){

		jQuery("#seen_slideshow-0").removeClass("horizontal").addClass("vertical");
		jQuery("#seen_slideshow-0").height(jQuery("#seen_slideshow-0").find(".item").height()*1+65+(12*1));
		jQuery("#seen_slideshow-0").find(".item-list").height(jQuery("#seen_slideshow-0").find(".item").height()*1+(12*1));


		jQuery(jQuery("#seen_slideshow-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<1){
    				jQuery(this).height("100%");
			}
		});

		if(effectseen_slideshow0 == "scrollHorz"){ effectseen_slideshow0 = "scrollVert";}

		if(ninja){
			ninjaLog.Update({text:"Slider is Vertical",style:""});
			ninjaLog.Update({text:"Using effect: "+effectseen_slideshow0});
		};
	}

	if(effectseen_slideshow0=="scrollHorz"){

		var sliderAreaWidth = jQuery("#seen_slideshow-0").width();
		var columnAdjust = 38;

		if(typeof(ninjaLog)!="undefined"){
			ninjaLog.Update({text:"Slider is Horizontal",style:""});
			ninjaLog.Update({text:"area width is: "+sliderAreaWidth});
			ninjaLog.Update({text:"item width is: "+jQuery("#seen_slideshow-0").find(".item").width()});
			ninjaLog.Update({text:"columnAdjust is: "+columnAdjust});
		}
	}

		if(1>1){


			if((sliderAreaWidth-55-(columnAdjust*1)<(jQuery("#seen_slideshow-0").find(".item").width())*1)||(false)){
				if(ninja){ninjaLog.Update({text:"Width of items require adjustment",style:""});};
				jQuery("#seen_slideshow-0").find(".item").width(Math.ceil((sliderAreaWidth-55-(columnAdjust*1))/1));
			}
		}else{
			if(typeof(ninjaLog)!="undefined"){
				ninjaLog.Update({text:"Adjusting for a single column: 70px"});
			}
			jQuery("#seen_slideshow-0").find(".item").width(sliderAreaWidth-70);
		}

		jQuery(jQuery("#seen_slideshow-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<1){
    				jQuery(this).width("100%");
			}
		});

		if(jQuery.browser.msie && Number(jQuery.browser.version)<7){
//alert("#"+jQuery(jQuery("#seen_slideshow-0").parents(".tncms-region")[0]).attr("id")+" width: "+jQuery(jQuery("#seen_slideshow-0").parents(".tncms-region")[0]).width());

			//jQuery("#seen_slideshow-0").find(".group .item .image").hide();
			jQuery("#seen_slideshow-0").find(".group .item .text").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#seen_slideshow-0").find(".group .item .text .description").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#seen_slideshow-0").width(jQuery("#seen_slideshow-0").parent().width());

			if(jQuery("#seen_slideshow-0").hasClass("showcase")){
				jQuery('#seen_slideshow-0').find(".group").css({"padding":"0px"});

				var itemWidth = jQuery("#seen_slideshow-0").find(".item").width()-((1) * 10);

				var groupWidth = jQuery('#seen_slideshow-0').find(".group").width();
var itemWidth = (groupWidth/1)-50;
jQuery('#seen_slideshow-0').find(".item").css({"width":itemWidth});
			}else if(jQuery("#seen_slideshow-0").hasClass("playlist")){
				jQuery('#seen_slideshow-0').find(".group").css({"padding":"0px"});

				var groupWidth = jQuery('#seen_slideshow-0').find(".group").width();
				var itemWidth = (groupWidth/1)-40;

				jQuery('#seen_slideshow-0').find(".item").css({"height":"154px","width":itemWidth});
				jQuery('#seen_slideshow-0').find(".item").find(".image").css({"width":itemWidth});

			}
		}

		


	jQuery('#seen_slideshow-0 .prev').show();
	jQuery('#seen_slideshow-0 .next').show();


        // BEFORE TRANSITION
        function onBeforeseen_slideshow0(curr, next, opts){
            $(curr).removeClass("active");
    		
                $(next).find(".image").find('img').css({'opacity':0});
            
        }

	// AFTER TRANSITION
	function onAfterseen_slideshow0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#seen_slideshow-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

                if(!$(next).hasClass("init")){
                    $(next).find(".image").each(function(){

                     letterboxImage(this);

                    });
                }
            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#seen_slideshow-0 .item-list').cycle({
		fx: effectseen_slideshow0,
		sync: true,
		fit: 1,
		
		timeout: 0,
                before: onBeforeseen_slideshow0,
		after:	onAfterseen_slideshow0,
        pause:true,
		pager:'#seen_slideshow-0 .nav',
		prev:'#seen_slideshow-0 .prev',
		next:'#seen_slideshow-0 .next'
	});
        

        

function letterboxImage(image){
    if($(image).filter(':in-viewport').length){

                var aHeight = $(image).height();
                var aWidth = $(image).width();

                var imgHeight = $(image).find("img").height();
                var imgWidth = $(image).find("img").width();
                var aspectRatio = imgHeight/imgWidth;

                if(imgHeight<=aHeight){ // too short
                    if(imgWidth>aWidth){ // if the image is too wide resize it to fit
                        $(image).find("img").css({'height':'auto','width':aWidth+'px'});
                        var imgHeight = Number($(image).find("img").height());
                    }

                    // letter box the image
                    $(image).find("img").css({'position':'relative','top':Math.ceil((aHeight-imgHeight)/2)+'px'});
                }else{ // too tall
                    $(image).find("img").css({'height':aHeight+'px','width':'auto'});
                    var imgHeight = $(image).find("img").height();
                    var imgWidth = $(image).find("img").width();

                    if(imgWidth<=aWidth){ // too wide
                        $(image).find("img").css({'height':aHeight+'px','width':'auto'});

                        var aWidth = $(image).width();
                        var imgWidth = $(image).find("img").width();

                        if(imgWidth<aWidth){
                            var imgPadding = Math.ceil((aWidth-imgWidth)/2);
                            $(image).css({'padding-left':imgPadding+'px'});
                        }
                    }
                }
                }
}

$(window).scroll(function(){
    var groups = $('#seen_slideshow-0 .item-list .group.init');
    if(groups.length){
        if(groups.filter(':in-viewport').length){
            $('#seen_slideshow-0 .item-list .group.init').filter(':in-viewport').each(function(){

                $(this).find(".image").each(function(){
                   letterboxImage(this);
                });

                $(this).removeClass("init");
            });
        }
    }
            });
            

}



// ]]> -->
</script>



</div></div>
        </div>
        
