<script type="text/javascript">
	function changeAction(url){
	<!--//
		document.multiSearch.action = url;
		if (document.multiSearch.siteSource[1].checked == true)
			{
				document.getElementById('sitesearch').style.display = "none";
				document.getElementById('newsbank').style.display = "block";
			}
		
		else if (document.multiSearch.siteSource[0].checked == true)
			{
				document.getElementById('sitesearch').style.display = "block";
				document.getElementById('newsbank').style.display = "none";
			}
