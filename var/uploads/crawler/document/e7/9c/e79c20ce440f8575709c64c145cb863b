        function onBeforeprintads0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAfterprintads0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#printads-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#printads-0 .item-list').cycle({
		fx: effectprintads0,
		sync: true,
		fit: 1,
		
		timeout: 0,
                before: onBeforeprintads0,
		after:	onAfterprintads0,
        pause:true,
		pager:'#printads-0 .nav',
		prev:'#printads-0 .prev',
		next:'#printads-0 .next'
	});
        

        

