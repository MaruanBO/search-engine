<script type="text/javascript">
var _snaq = _snaq || [];
_snaq.push(['setCollectorUrl', 'www.i.matheranalytics.com']);
_snaq.push(['setAppId', 'v1']);
_snaq.push(['setCustomerId', 'ma39366']);
_snaq.push(['setMarket', '7442282']);
