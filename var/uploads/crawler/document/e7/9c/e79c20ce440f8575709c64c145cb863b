<script type='text/javascript'>
    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 14808;
    _sf_async_config.domain = 'pressofatlanticcity.com';
    _sf_async_config.useCanonical = true;
    _sf_async_config.sections = 'pressofAtlanticCity.com';
