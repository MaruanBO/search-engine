<section class="content stomach-wrapper">
	<!-- Find The Perfect Journey -->
	<div class="find-journey">
		<h2>Find The Perfect Journey</h2>
		<form>
			<!-- Region -->
			<fieldset class="select-wrapper">
				<select name="region" id="region" style="width: 240px;" class="chzn-select-no-results" tabindex="6">
				</select>
				<span class="icons select-icon"></span>
			</fieldset>
			<!-- Date -->
			<fieldset class="select-wrapper">
				<select id="year" name="year" style="width: 240px;" class="chzn-select-no-results" tabindex="6">
				</select>
				<span class="icons select-icon"></span>
			</fieldset>
			<!-- River Cruises -->
			<fieldset class="select-wrapper">
				<select name="type" id="type" style="width: 240px;" class="chzn-select-no-results" tabindex="6">
				</select>
				<span class="icons select-icon"></span>
			</fieldset>
			<!-- Search Journey -->
			<fieldset class="search-journey-button">
				<input class="search-btn" type="submit" value="SEARCH" />
				<input class="search-img-btn" type="image" alt="SEARCH" value="SEARCH" src="Assets/images/search-btn.png" />
				<span class="icons double-arrow-right-med"></span>
			</fieldset>
			<script type="text/javascript">
			    $(function () {
			        $.ajax({
			            type: "POST",
			            url: '/WebMethods/VantageService.asmx/GetJourneyFilters',
			            dataType: 'json',
			            contentType: "application/json; charset=utf-8",
			            data: JSON.stringify({}),
			            success: function (result) {
			                if (result.hasOwnProperty('d')) {
			                    result = result.d;
			                }
			                var o = [], y, year, r, t, region, type;
			                o.push('<option value="" disabled selected>Select a Date</option>');
			                for (y in result.YearList) {
			                    year = result.YearList[y];
			                    o.push('<option value="');
			                    o.push(year);
			                    o.push('" class="chz-option">');
			                    o.push(year);
			                    o.push('</option>');
			                }
			                $('#year').html(o.join(''));
			                o = [];
			                o.push('<option value="" disabled selected>Select a Region</option>');
			                for (r in result.RegionList) {
			                    region = result.RegionList[r];
			                    o.push('<option value="');
			                    o.push(region);
			                    o.push('" class="chz-option">');
			                    o.push(region);
			                    o.push('</option>');
			                }
			                $('#region').html(o.join(''));
			                o = [];
			                o.push('<option value="" disabled selected>Select Type</option>');
			                for (t in result.TypeList) {
			                    type = result.TypeList[t];
			                    o.push('<option value="');
			                    o.push(type);
			                    o.push('" class="chz-option">');
			                    o.push(type);
			                    o.push('</option>');
			                }
			                $('#type').html(o.join(''));

			            }
			        });
			        $('.find-journey .search-journey-button .search-btn').off('click.search').on('click.search', function () {
			            var year = $('#year').val(), region = $('#region').val(), type = $('#type').val(), queryString = [];
			            if (year != null) {
			                queryString.push('year=' + year);
			            }
			            if (region != null) {
			                queryString.push('region=' + encodeURIComponent(region));
			            }
			            if (type != null) {
			                queryString.push('type=' + encodeURIComponent(type));
			            }
			            vantage.googletrackingManager.sendEvent(['Find The Perfect Journey', region, year + ',' + type]);
			            if (queryString.length > 0) {
			                window.location = '/OurJourneys.aspx?' + queryString.join('&');
			            } else {
			                window.location = '/OurJourneys.aspx';
			            }
			            return false;
			        });
			    });
			</script>
		</form>
	</div>	
</section>
