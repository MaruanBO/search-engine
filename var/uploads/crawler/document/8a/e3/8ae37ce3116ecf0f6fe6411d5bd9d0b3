	<script type="text/javascript">
		$(function () {
			$('#btnGlobalSearch').off('click.btnGlobalSearch').on('click.btnGlobalSearch', function () {
				var search = $.trim($('#txtGlobalSearch').val());
				if (search.length > 0) {
					window.location = '/SearchResults?search=' + search;
				} else {
					return false;
				}
				return true;
			});
			$('#txtGlobalSearch').off("keypress.txtGlobalSearch").on("keypress.txtGlobalSearch", function (e) {
				if (e.keyCode == 13) {
					$('#btnGlobalSearch').trigger('click');
					e.preventDefault();
					return false;
				}
				return true;
			});
		});
	</script>
</form>


