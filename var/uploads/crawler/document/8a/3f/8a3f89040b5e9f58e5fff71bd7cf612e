<script type="text/javascript">
/* <![CDATA[ */
	$('.recipes-thumbnail .story-block').each(function(){
		var itemCount = $(this).find('.item').size();
		var random = Math.floor(Math.random()*itemCount);
		$(this).find('.item').each(function(i){
			if(i == random) {
				$(this).show();
			} else {
				$(this).remove();
			}
		});
	});

	$('.recipes-thumbnail .story-block .item').each(function(){
		$(this).find('img.link-target').wrap('<a href="'+$(this).find('a.link-source').attr('href')+'"></a>');
	});
/* ]]> */
</script>

