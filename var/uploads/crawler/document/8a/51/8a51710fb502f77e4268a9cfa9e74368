}
for (i = 0; i <= 1; i += (1 / steps)) {
setTimeout("setOpacity1('"+pParam1+"', " + i + ")", i * duration);
}
}
function fadeOutHome(pParam1) {
for (i = 0; i <= 1; i += (1 / steps)) {
setTimeout("setOpacity('"+ pParam1 +"'," + (1 - i) + ")", i * duration);
}
