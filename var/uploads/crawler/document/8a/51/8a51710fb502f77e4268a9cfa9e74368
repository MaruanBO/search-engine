if (pParam1 == "Labelesst3") {
element = document.getElementById("Labelesst1");
element.style.display = "none";
element = document.getElementById("Labelesst2");
element.style.display = "none";
}
if (pParam1 == "Labelesst2") {
element = document.getElementById("Labelesst3");
element.style.display = "none";
element = document.getElementById("Labelesst1");
element.style.display = "none";
}
if (pParam1 == "Labelesst1") {
element = document.getElementById("Labelesst3");
element.style.display = "none";
element = document.getElementById("Labelesst2");
element.style.display = "none";
}
for (i = 0; i <= 1; i += (1 / steps)) {
setTimeout("setOpacity1('"+pParam1+"', " + i + ")", i * duration);
}
}
function fadeOut(pParam1) {
for (i = 0; i <= 1; i += (1 / steps)) {
setTimeout("setOpacity('"+ pParam1 +"'," + (1 - i) + ")", i * duration);
}
//setTimeout("fadeIn()", duration);
}
/* start the effect */
//--> 
</script>
