			<script type="text/javascript">
			      var numItemsVisibleInShelf = 4;
			      var sizeToScroll = 4;
			      $(document).ready(function(){
			            $(function() {
			                 
			                  $(".pShelfC > div").each(function(){
			                	  	var scrollable = $(".scrollable", this);
			                        $(scrollable).scrollable({
			                              onBeforeSeek: function (event, index) {
			                                    if (this.getIndex() >= this.getSize() - numItemsVisibleInShelf) {
			                                          if (index > this.getIndex()) {
			                                                return false;
			                                          }                 
			                                    }                                         
			                              },
			                            onSeek: function(event, index) {
			                                  
			                               var wrap = this.getRoot();
			                               var numItemsTotalInShelf = this.getItems().length;
			                               if (this.getIndex()+numItemsVisibleInShelf >= numItemsTotalInShelf) {
			                            	
			                                 $(wrap).siblings(".pShelfR").children(".next").addClass("disabled");
			                               }
			                               
			                            }, size: sizeToScroll
			                        });
		
			                        //hide arrow on page load if size of sizeToScroll <= numItemsVisibleInShelf
			                        var sizeOfTab = $("> .items > .col", scrollable).length;
			                        if (sizeOfTab<=numItemsVisibleInShelf)
			                        {
			                        	$(scrollable).siblings(".pShelfR").children(".next").addClass("disabled");
					                }
			                  });
		
			                  $(".pShelf").tabs({
			                        show: function(event, ui) {
			                      		  //check if tab has a title or not
			                      		  if($("li",this).first().hasClass("hastitle"))
			                      		  {
											$(this).removeClass("singleTabShelfNoTitle");
			                          	  }
		
			                          	
			                      		  //Adjust height for the first 4 items
			                              $("#smartShelfDummyPlaceholder").remove();
			                              $(".pShelf").show();                          
			                        }
			                  });
			                  			                                    
			            });
			      });
			</script>	
		
			
				
				
				
			
				
				
		
					
						
								
