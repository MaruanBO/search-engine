<script type="text/javascript">
//<![CDATA[
	function bindEvent(element, type, handler) {
		if (element.addEventListener) {
			element.addEventListener(type, handler, false);
		} else {
			element.attachEvent('on' + type, handler);
		}
	}

	function initAc(inputId, acDivId) {
var qAcScriptUrl = '/autocompleteskills/'
var lAcScriptUrl = '/lach?csl=&l='

var itemClass = 'itm';
var selectedItemClass = 'itm sitm';
var nomatches = 1;
var sindex = -1;
var arrowUpKeyCode = 38;
var arrowDownKeyCode = 40;
var h = {
	inputBox: document.getElementById(inputId),
	acdiv: document.getElementById(acDivId),
	selectedItem: '',
	lastTyped: '',
	isLocation: inputId == 'l',
	unbold: function (s) { return s.replace(/\<\/?b\>/gi, ''); },

	diac: function (show) {
		var iframeId = 'pjfifid';
		var ifrm = document.getElementById(iframeId);
		if (!ifrm) {
			ifrm = document.createElement('iframe');
			ifrm.id = iframeId;
			ifrm.style.position = 'absolute';
			ifrm.frameBorder = 0;
			ifrm.setAttribute('tabindex', 1000);
			h.acdiv.parentNode.insertBefore(ifrm, h.acdiv.parentNode.firstChild);
		}
		if (show) {
			ifrm.width = h.acdiv.offsetWidth;
			ifrm.height = h.acdiv.offsetHeight;
			ifrm.style.display = '';
			document.onmousedown = function () {
				h.diac(0);
				document.onmousedown = null;
			};
		} else {
			ifrm.style.display = 'none';
		}
		h.acdiv.style.display = show ? 'block' : 'none';
	},

	keyuphandler: function (e) {
		if (!e && window.event) e = window.event;
		var kc = e.keyCode;
		switch (kc) {
			case arrowUpKeyCode:
			case arrowDownKeyCode:
			case 9: case 12: case 16: case 17: case 18: case 19: case 20: case 27: case 33: case 34: case 35: case 36: case 37: case 39: case 45:
				break;
			case 13:
				h.inputBox.blur();
				break;
			default:
				h.queueAcRequest();
		}
		return 1;
	},

	keydownhandler: function (e) {
		if (!e && window.event) e = window.event;
		var kc = e.keyCode;
		switch (kc) {
			case 9:
				h.tabout(e);
				return 0;
			case 13:
				h.tabout(e);
				break;
			case 27:
				h.clrsugg();
				break;
			case arrowUpKeyCode:
			case arrowDownKeyCode:
				if (!e.shiftKey) h.arrows(kc);
				break;
		}
		return 1;
	},

	clrsugg: function () {
		h.diac(0);
		h.selectedItem = '';
		setTimeout(function () { h.inputBox.value = h.lastTyped; }, 10);
	},

	tabout: function (e) {
		if (!e && window.event) e = window.event;
		h.diac(0);
		h.selectedItem = (sindex >= 0) ? h.inputBox.value : '';
	},

	arrows: function (keyCode) {
		if (nomatches) return;
		var hidn = h.acdiv.style.display == 'none';
		if (hidn) h.diac(1);
		var items = h.acdiv.getElementsByTagName('div');
		if (!hidn) {
			if (keyCode == arrowUpKeyCode) {
				if (sindex < 0) sindex = items.length;
				sindex--;
			} else if (keyCode == arrowDownKeyCode) {
				sindex++;
				if (sindex >= items.length) sindex = -1;
			}
			h.selectedItem = '';
			if (sindex < 0 || sindex >= items.length) {
				h.inputBox.value = h.lastTyped;
			} else {
				h.selectedItem = items[sindex];
			}
		}
		for (var i = 0; i < items.length; i++) {
			if (i == sindex) {
				items[i].className = selectedItemClass;
				h.inputBox.value = h.unbold(items[i].innerHTML);
			} else {
				items[i].className = itemClass;
			}
		}
	},

	queueAcRequest: function () {
		var val = h.inputBox.value;
		if (window.pjf_timer) clearTimeout(window.pjf_timer);
		if (!val) return false;
		window.pjf_timer = setTimeout(
			function () {
				var headEl = document.getElementsByTagName('head')[0];
				if (window.pjf_sc) headEl.removeChild(window.pjf_sc);
				var url = h.isLocation ? lAcScriptUrl : qAcScriptUrl;
				var fullUrl = url + encodeURIComponent(val);
				window.pjf_sc = document.createElement('script');
				window.pjf_sc.src = fullUrl;
				headEl.appendChild(window.pjf_sc);
			}, 100);
		h.lastTyped = val;
		h.selectedItem = '';
		return false;
	},

	onMOver: function () {
		var el = this;
		var txt = el.innerHTML;
		var ch = h.acdiv.childNodes;
		for (var i = 0; i < ch.length; i++) {
			var value = ch[i].innerHTML;
			if (value == txt) {
				sindex = i;
			} else {
				ch[i].className = itemClass;
			}
		}
		el.className = selectedItemClass;
	},

	onClick: function () {
		h.diac(0);
		h.inputBox.value = h.selectedItem = h.unbold(this.innerHTML);
		if (h.inputBox.autocompleteFinished) h.inputBox.autocompleteFinished();
	},

	fillAc: function (val) {
		var matches = val;
		while (h.acdiv.childNodes.length > 0) h.acdiv.removeChild(h.acdiv.childNodes[0]);
		if (matches.length == 0) {
			nomatches = 1;
			sindex = -1;
			h.diac(0);
			return;
		}
		nomatches = 0;
		sindex = -1;
		h.diac(1);
		for (var i = 0; i < matches.length; i++) {
			var u = document.createElement('div');
			u.className = itemClass;
			bindEvent(u, 'mouseover', h.onMOver);
			bindEvent(u, 'mousedown', h.onClick);
			u.innerHTML = matches[i];
			h.acdiv.appendChild(u);
			lastu = u;
		}
	}
};

h.inputBox.setAttribute('autocomplete', 'off');
h.lastTyped = h.inputBox.value;
bindEvent(h.inputBox, 'keyup', h.keyuphandler);
bindEvent(h.inputBox, 'keydown', h.keydownhandler);
return h;
};
var qacs = initAc('q', 'qacdiv');
var lacs = initAc('l', 'lacdiv');
//]]>
</script>

			
