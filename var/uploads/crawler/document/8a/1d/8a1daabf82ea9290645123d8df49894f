					function setC(c1, c2) {
						c1 = document.getElementById(c1);
						c2 = document.getElementById(c2);
						c1.className = 'active';
						c2.className = 'inactive';
						sf();
						if ((document.getElementById('q').value != '') || (document.getElementById('l').value != '')) {
							document.getElementById('search').click();
						}
						return false;
					}
