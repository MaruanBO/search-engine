            var currentInfoWindow;
            $(document).ready(function(){
                var mapCanvasObj = document.getElementById("map_canvas");

                if (mapCanvasObj) {
                    geocoder = new google.maps.Geocoder();
                    

            var address = '9655 SW Canyon Road Portland, OR';
            var LatLng;
            if (geocoder) {
                geocoder.geocode( { 'address': address}, function(results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {

                        map.setCenter(results[0].geometry.location);

                        marker = new google.maps.Marker({
                            map: map,
                            position: map.getCenter()
                            , icon: '/public/images/acurapointer.png'
                        });

                        

                        if (typeof infowindow !== 'undefined' && typeof infowindow.open === 'function') {
                            google.maps.event.addListener(marker, 'click', function() {
                                currentInfoWindow.close();
                                infowindow.open(map, marker);
                                currentInfoWindow = infowindow;
                            });
                        }

                    } else {
                        console.error("Geocode was not successful for the following reason: " + status);
                    }
                });
            }

                    var myOptions = {zoom: 8, disableDefaultUI: 1, mapTypeId: google.maps.MapTypeId.ROADMAP, mapTypeControl: 0, navigationControl: 0, scrollwheel: false, zoomControl: 1}

                    map = new google.maps.Map(mapCanvasObj, myOptions);
                    
                }
            });
