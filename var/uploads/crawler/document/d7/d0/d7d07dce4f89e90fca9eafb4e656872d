	<script type="text/javascript">
    /**
     * If the liftAjax piece doesn't exist, this will try 5x at .5 second intervals before quitting.
     *
     * @param func
     * @param n
     */
    var ensureLiftAjaxInitialized = function(func, n) {
      n = (!n) ? 0 : n;
      if (n<5) {
        if (!window.liftAjax) {
          setTimeout("ensureAjaxLiftInitialized("+func+","+ ++n +")", 500);
        } else {
          func();
        }
      } else {
        window.console&&console.log("Cant find liftAjax to override!");
      }
    };

    /**
     * Setting up the overloaded lift ajax request
     *
     * Note: Just changed dataType from "script" to "text" to not execute the returned reload
     */
    var overloadLiftRegisterGC = function() {
      var data = "__lift__GC=_"
      jQuery.ajax({
        url: liftAjax.addPageName("/ajax_request/"),
        data: data,
        type: "POST",
        dataType: "text",
        timeout: 5000,
        cache: false,
        success: liftAjax.lift_successRegisterGC,
        error: liftAjax.lift_failRegisterGC
      });
    }

    $(document).ready(function() {
      ensureLiftAjaxInitialized(function() {
        /* overloading the lift_registerGC method call */
        window.liftAjax.lift_registerGC = overloadLiftRegisterGC;
      });
    });
  </script>
