<script src="/ajax_request/liftAjax.js" type="text/javascript"></script>

	<div>
  
  <script language="JavaScript" type="text/javascript">
    function qs(key) {
      key = key.replace(/[*+?^$.\[\]{}()|\\\/]/g, "\\$&"); // escape RegEx meta chars
      var match = location.search.match(new RegExp("[?&]" + key + "=([^&]+)(&|$)"));
      return match && decodeURIComponent(match[1].replace(/\+/g, " "));
    }

    $(document).ready(function () {
      var ref = qs('ref');
      if (ref != undefined && ref != null) {
        document.cookie = 'ref=' + ref;
      } else if (document.cookie.length > 0) {
        var cookies = document.cookie.split(';');
        for (var index = 0; index < cookies.length; index++) {
          if (cookies[index].indexOf('ref=') == 1) {
            ref = unescape(cookies[index].substring(5));
          }
        }
      }

      if (ref != undefined && ref != null) {
        $(".with-ref").each(function () {
          var href = $(this).attr('href');
          if (href.indexOf('?') > 0) {
            href = href + '&ref=' + ref;
          } else {
            href = href + '?ref=' + ref;
          }
          $(this).attr("href", href);
        });
      }

      // Load refcode into Salesforce WTL.
      if (ref) {
        $('.ref-input').val(ref);
        $('.url-input').val(window.location.pathname);
      } else {
        $('.ref-input').val('Organic');
        $('.url-input').val(window.location.pathname);
      }
    });
  </script>
</div>
