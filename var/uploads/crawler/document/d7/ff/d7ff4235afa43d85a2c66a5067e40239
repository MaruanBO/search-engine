	function togglehover(hoverDivId, show){
		if(show){
			//alert("show");
			jQuery("#" + hoverDivId).css("opacity", "1");
		}else{
			//alert("hide");
			jQuery("#" + hoverDivId).css("opacity", "");
		}
	}
</script>








