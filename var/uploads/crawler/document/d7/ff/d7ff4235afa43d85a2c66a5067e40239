<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
		<h3 id="myModalLabel">Newsletter Sign up</h3>
	</div>
	<div class="modal-body">
		<form style="margin:0;" action="http://www.carbonated.tv/subscribeNewsLetter" method="post" id="subscribeNewsLetterFormHeaderSocial" name="subscribeNewsLetterFormHeaderSocial" onsubmit="return false;">
			<input name="subscriber.email" id="subscriber.emailHeaderSocial" type="text" placeholder="Enter your email address..." class="subsholder opensanslight"/>
		</form>
		<div class="successtxt" style="display: none;" id="subscribeUpdateHeaderSocial">Your subscription has been updated</div>
		<div class="successtxt" style="display: none;" id="subscribeDoneHeaderSocial">Thank You for signing up for Carbonated.tv  Newsletter You will receive email verification shortly</div>
		<div class="failuretxt" style="display: none;" id="subscribeInValidHeaderSocial">Please enter valid email</div>
		<div class="failuretxt" style="display: none;" id="subscribeExistsHeaderSocial">You are already subscribed</div>
		<div class="" style="display: none;" id="subscribeLoadingHeaderSocial">Your request is being processed</div>
	</div>
	<div class="modal-footer">
		<button class="redbtn" onclick="subscribe('HeaderSocial');">Subscribe</button>
	</div>
</div>
<!-- End of jquery modal for newsletter subscribe -->



<script>
	jQuery(document).ready(function(){
		var newsletterPopup = 'myModal';
		jQuery('#' + newsletterPopup).modal({
			keyboard: true,
			show:false
		});
		jQuery('#'+ newsletterPopup).on('hidden.bs.modal', function () {
			//empty the input field with prototype js
			jQuery('input#subscriber\\.emailHeaderSocial').val('');
			
			//hide the success/failure messages
			jQuery("#subscribeUpdateHeaderSocial").hide();
			jQuery("#subscribeDoneHeaderSocial").hide();
			jQuery("#subscribeInValidHeaderSocial").hide();
			jQuery("#subscribeExistsHeaderSocial").hide();
			jQuery("#subscribeLoadingHeaderSocial").hide();
		});
		if(window.location.href.indexOf("openNewsletterModal")!=-1){
			jQuery("#newsletterModal").click();
		}
		
		jQuery(window).on('hashchange',function(){ 
			hashNewsletterSignup(location.hash.slice(1));
		});
		if(window.location.hash){
			hashNewsletterSignup(location.hash.slice(1));
		}
	});
	
	var hashNewsletterSignup = function(hash){
		if(hash === 'NewsletterSignup'){
			jQuery('#myModal').modal('show');
		}
	}
</script>
	<!-- social links closed !-->
</div>

