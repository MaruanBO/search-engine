<script>
	var fbboxId="fbbox";
	var fbboxClosed=false;
	var fbboxEnable = false;
	var fbBox = jQuery('#'+fbboxId);
	var isShown = false;
	
	function hideFbBox(){
		fbboxClosed=true;
		fbBox.hide();
	}
	jQuery(window).scroll(function() {
		var scBottom = jQuery(document).height() - jQuery(window).height() - jQuery(window).scrollTop();
		
  		if(fbboxEnable && !fbboxClosed && (scBottom <= 900)) {//show it
  			
  			if(scBottom<(jQuery("#footerArea").height() + 10)){
  				fbBox.css({
  					bottom :  (jQuery("#footerArea").height() + 10) - scBottom
  				});
  			}else{
  				fbBox.css({
  					bottom : '0%'
  				});  				
  			}
  		
	  		if(!isShown){
	  			fbBox.animate({
					"right" : "1px"
				}, "slow");
	  		}
  			isShown = true;
  			
		} else {
			//hide it
	  		if(isShown){
	  			fbBox.animate({
					"right" : "-400px"
				}, "slow");
	  		}
  			isShown = false;
		}
	});
</script>
	
