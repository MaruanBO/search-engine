		contextPath = 'http://www.carbonated.tv';
		
		function searchKeyword(searchQuery, sortBy){
			//google analytics custom event for search
			_gaq.push(['_setCustomVar', 1, 'search', searchQuery, 1]);
			
			var q = jQuery.trim(searchQuery);
			var sortParam = (sortBy==="sortByRelevance") ? "&sortBy=sortByRelevance" : "";
			
			if(q!=null && q.length>0){
				//location =contextPath + "/topics/" + q;
				location =contextPath + "/googleSearch?q=" + q + sortParam;
			}
			return false;
		}
		
