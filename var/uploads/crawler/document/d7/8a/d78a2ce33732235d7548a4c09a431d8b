<script type="text/javascript">
/**************************************************************\
****************   Gravitate Event Tracking   ******************
\**************************************************************/



function getga_has_universal_event_tracking()
{
	var scripts = document.getElementsByTagName('script');

	for (var i = scripts.length; i--;)
	{
	    if (scripts[i].src.indexOf(String.fromCharCode(103,111,111,103,108,101,45,97,110,97,108,121,116,105,99,115,46,99,111,109,47,97,110,97,108,121,116,105,99,115,46,106,115)) > 0)
	    {
	    	return true;
	    }
	}

	return false;
}

function getga_add_event_tracking(has_universal_event_tracking, getga_selector, getga_event, getga_category, getga_action_label, getga_label)
{
	jQuery(function($){

		$(getga_selector).on(getga_event, function(e){

			var NA = 'undefined';

			
			if(getga_label.indexOf('{ITEM_TITLE}') > -1)
			{
				getga_label = getga_label.replace("{ITEM_TITLE}", ($(this).attr('title') !== undefined ? $(this).attr('title') : NA));
			}

			if(getga_label.indexOf('{PAGE_URL}') > -1)
			{
				getga_label = getga_label.replace("{PAGE_URL}", location.href);
			}

			if(getga_label.indexOf('{PAGE_RELATIVE_URL}') > -1)
			{
				getga_label = getga_label.replace("{PAGE_RELATIVE_URL}", location.pathname);
			}

			if(getga_label.indexOf('{LINK_URL}') > -1)
			{
				getga_label = getga_label.replace("{LINK_URL}", ($(this).attr('href') !== undefined ? $(this).attr('href') : NA));
			}

			if(getga_label.indexOf('{LINK_RELATIVE_URL}') > -1)
			{
				getga_label = getga_label.replace("{LINK_RELATIVE_URL}", ($(this).attr('href') !== undefined ? $(this).attr('href').replace(/^(?:\/\/|[^\/]+)*\//, '/') : NA));
			}

			if(getga_label.indexOf('{IMAGE_SRC}') > -1)
			{
				getga_label = getga_label.replace("{IMAGE_SRC}", ($(this).attr('src') !== undefined ? $(this).attr('src') : NA));
			}

			if(getga_label.indexOf('{IMAGE_ALT}') > -1)
			{
				getga_label = getga_label.replace("{IMAGE_ALT}", ($(this).attr('alt') !== undefined ? $(this).attr('alt') : NA));
			}

			if(getga_label.indexOf('{TAG_HTML}') > -1)
			{
				getga_label = getga_label.replace("{TAG_HTML}", $(this).html().replace(/(<([^>]+)>)/ig,'').replace(/(\r\n|\n|\r)/gm,'').replace(/\s{2,}/g, ' '));
			}

			if(has_universal_event_tracking)
			{
				try
				{
					ga('send', 'event', getga_category, getga_action_label, getga_label);
									}
				catch(err)
				{
									}
			}
			else
			{
				try
				{
					_gaq.push(['_trackEvent', getga_category, getga_action_label, getga_label]);
									}
				catch(err)
				{
									}
			}
		});

	});
}

function getga_add_all_event_tracking()
{

	var getga_selector, getga_event, getga_category, getga_action_label, getga_label;

	if (typeof jQuery !== 'undefined') // Check if jQuery is Loaded
	{
		(function($){

			
			var has_universal_event_tracking = getga_has_universal_event_tracking();

			if (typeof(_gaq) !== 'undefined' || has_universal_event_tracking) // Check if Google Analytics is Loaded
			{
				
							/* Contact us */
							if($('contact_us').length)
							{
								getga_add_event_tracking(has_universal_event_tracking, 'contact_us', 'click', 'contact_us', 'Default Item Clicked', 'Default_{ITEM_TITLE}_{PAGE_URL}');
															}
														else
							{
								setTimeout(function(){

									if($('contact_us').length)
									{
										getga_add_event_tracking(has_universal_event_tracking, 'contact_us', 'click', 'contact_us', 'Default Item Clicked', 'Default_{ITEM_TITLE}_{PAGE_URL}');
																			}
																		else
									{
										setTimeout(function(){

											if(!$('contact_us').length)
											{
																							}
											getga_add_event_tracking(has_universal_event_tracking, 'contact_us', 'click', 'contact_us', 'Default Item Clicked', 'Default_{ITEM_TITLE}_{PAGE_URL}');

										}, 3000);
									}
									
								}, 2000);

															}
							
							
							/* Downloads - pdf, doc(x), xls(x), txt, zip, csv */
							if($('a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]').length)
							{
								getga_add_event_tracking(has_universal_event_tracking, 'a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]', 'click', 'Downloads', 'Downloaded', 'Download_{ITEM_TITLE}_{PAGE_RELATIVE_URL}_{LINK_RELATIVE_URL}');
															}
														else
							{
								setTimeout(function(){

									if($('a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]').length)
									{
										getga_add_event_tracking(has_universal_event_tracking, 'a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]', 'click', 'Downloads', 'Downloaded', 'Download_{ITEM_TITLE}_{PAGE_RELATIVE_URL}_{LINK_RELATIVE_URL}');
																			}
																		else
									{
										setTimeout(function(){

											if(!$('a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]').length)
											{
																							}
											getga_add_event_tracking(has_universal_event_tracking, 'a[href$=".pdf"], a[href$=".doc"], a[href$=".docx"], a[href$=".ods"], a[href$=".odt"], a[href$=".xls"], a[href$=".xlsx"], a[href$=".txt"], a[href$=".zip"], a[href$=".csv"]', 'click', 'Downloads', 'Downloaded', 'Download_{ITEM_TITLE}_{PAGE_RELATIVE_URL}_{LINK_RELATIVE_URL}');

										}, 3000);
									}
									
								}, 2000);

															}
							
							
							/* All Submit Buttons */
							if($('input[type=submit]').length)
							{
								getga_add_event_tracking(has_universal_event_tracking, 'input[type=submit]', 'click', 'Form Submits', 'Form Submitted', 'Form_Submitted_{TAG_HTML}_{PAGE_RELATIVE_URL}');
															}
														else
							{
								setTimeout(function(){

									if($('input[type=submit]').length)
									{
										getga_add_event_tracking(has_universal_event_tracking, 'input[type=submit]', 'click', 'Form Submits', 'Form Submitted', 'Form_Submitted_{TAG_HTML}_{PAGE_RELATIVE_URL}');
																			}
																		else
									{
										setTimeout(function(){

											if(!$('input[type=submit]').length)
											{
																							}
											getga_add_event_tracking(has_universal_event_tracking, 'input[type=submit]', 'click', 'Form Submits', 'Form Submitted', 'Form_Submitted_{TAG_HTML}_{PAGE_RELATIVE_URL}');

										}, 3000);
									}
									
								}, 2000);

															}
							
							
							/* Download */
							if($('download').length)
							{
								getga_add_event_tracking(has_universal_event_tracking, 'download', 'click', 'download', 'Download', 'Default_{ITEM_TITLE}_{PAGE_URL}');
															}
														else
							{
								setTimeout(function(){

									if($('download').length)
									{
										getga_add_event_tracking(has_universal_event_tracking, 'download', 'click', 'download', 'Download', 'Default_{ITEM_TITLE}_{PAGE_URL}');
																			}
																		else
									{
										setTimeout(function(){

											if(!$('download').length)
											{
																							}
											getga_add_event_tracking(has_universal_event_tracking, 'download', 'click', 'download', 'Download', 'Default_{ITEM_TITLE}_{PAGE_URL}');

										}, 3000);
									}
									
								}, 2000);

															}
							
										}
			else
			{
							}

		})(jQuery)
	}
	else
	{

	
	}
}

getga_add_all_event_tracking();

/**************************************************************\
**************   End Gravitate Event Tracking   ****************
\**************************************************************/
</script>

