<script type="text/javascript" class="ezCSS">
    
$ezJQuery('#ezomat-menu > ul').dcAccordion({
    eventType: 'click',
    autoClose: false,
    saveState: true,
    disableLink: true,
    speed: 'fast',
    classActive: 'test',
    showCount: true
});
                    
                    
$ezJQuery(document).ready(function() {

    function moveScroller() {
        var move = function() {
            var top = $ezJQuery('#ezo-left-side').position().top;
            var height = $ezJQuery('#ezo-left-side').height();
            var scroll_top = $ezJQuery(window).scrollTop();
            var bottom = top+$ezJQuery('#ezo-left-side').outerHeight(true);
            var scroll_bottom = scroll_top + $ezJQuery(window).height();
            if(bottom <  scroll_bottom)
            {
                //$ezJQuery('#ezo-left-side').css('top',(scroll_bottom-bottom)+'px');
                //$ezJQuery('#ezo-left-side').animate({top:scroll_bottom-height},0,'swing');
                //$ezJQuery('#ezo-left-side').css('bottom','0px');
                if($ezJQuery('#ezo-left-side').css('position') != 'fixed')
                {
                    $ezJQuery('#ezo-left-side').css('top',($ezJQuery(window).height()-height)+'px');
                    $ezJQuery('#ezo-left-side').css('position','fixed');
                }
            }
            else if(scroll_bottom < height)
            {
                if($ezJQuery('#ezo-left-side').css('position') != 'absolute')
                {
                    $ezJQuery('#ezo-left-side').css('top','0px');
                    $ezJQuery('#ezo-left-side').css('position','absolute');
                }
            }
        };
        $ezJQuery(window).scroll(move);
        move();
    }

    $ezJQuery(function() {
        moveScroller();
    });

});

</script><script data-cfasync="false" type="text/javascript">

