<script type="text/javascript">
	var mobileOverride = false;
	var nameEQ = 'mobile_override=';
	var ca = document.cookie.split(';');

	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) mobileOverride = unescape(c.substring(nameEQ.length,c.length));
	}

	if (mobileOverride == 1 ) {
		document.write('<meta name="viewport" content="" />');
	}
	else {
		//Steve MJPRO-4586 document.write('<meta name="viewport" content="user-scalable=1, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0"/>');
		document.write('<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0"/>');
	}
</script>


