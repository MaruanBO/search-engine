	<script type="text/javascript">
		function updateCartTotal(tot)
		{
			 var elem = dojo.byId('totalCartQuantity');
 			 var elem1 = dojo.byId('totalCartItems');
			
			if (tot > 1 || tot == 0) {
				elem.innerHTML = tot;
				elem1.innerHTML = "items";
			}
			else {
				elem.innerHTML = tot;
				elem1.innerHTML = "item";
			}
			
			if (tot > 0 && (window['cartTotalColor'] != undefined)) {
				elem.style.color = countColor;
				elem.style.background = countBackground;
			}
		}
		
	</script>

