				<script type='text/javascript'>
					
					function pushProductView(productSku) {
						_a1as.push(["clear"]);
						_a1as.push(["setvar","productid_view",productSku]);
						_a1as.push(["track"]);
					}
					
					function pushProductAddToCart(productSku) {
						_a1as.push(["clear"]);
		
						
						
						var shoppingCartProducts = '';
						if (shoppingCartProducts != null && shoppingCartProducts != '') {
							shoppingCartProducts = shoppingCartProducts + ',' + productSku;
						} else {
							shoppingCartProducts = productSku;
						}
										
						_a1as.push(["setvar","productid_cart",shoppingCartProducts]);
						_a1as.push(["track"]);
					}
					
					function pushProductRemoveFromCart(productSku) {
						_a1as.push(["clear"]);
		
						
		
						var newCartProducts = '';				
						var shoppingCartProducts = '';
						if (shoppingCartProducts != null && shoppingCartProducts != '') {
		
							var str_array = shoppingCartProducts.split(',');
							
							for(var i = 0; i < str_array.length; i++)
							{
							   str_array[i] = str_array[i].replace(/^\s*/, "").replace(/\s*$/, "");
							   if (productSku != str_array[i]) {
								   	if (newCartProducts != null && newCartProducts != '') {
										newCartProducts = newCartProducts + ',' + str_array[i];
									} else {
										newCartProducts = str_array[i];
									}
							   }
							}
						}
										
						_a1as.push(["setvar","productid_cart",newCartProducts]);
						_a1as.push(["track"]);
					}
					
					function pushProductAddToWishlist(productSku) {
						_a1as.push(["clear"]);
						_a1as.push(["setvar","productid_wishlist",productSku]);
						_a1as.push(["track"]);
					}
