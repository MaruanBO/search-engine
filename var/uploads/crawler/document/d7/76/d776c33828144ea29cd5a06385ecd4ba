<script>

 function gup( name ) {

  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");

  var regexS = "[\\?&]"+name+"=([^&#]*)";

  var regex = new RegExp( regexS );

  var results = regex.exec( window.location.href );

  if( results == null )

  {

   return "";

  }

  else

  {

   return results[1];

  }

 }


 var mailingID    = gup('spMailingID');

 var recipientID  = gup('spUserID');

 var mailingJOBID = gup('spJobID');

 var reportJOBID  = gup('spReportId');

 var cookieExpiryInDays = 365;

if((mailingID != '') && (recipientID != '') && (mailingJOBID != '') && (reportJOBID != ''))

 {

   createCookie('mailingID',mailingID,cookieExpiryInDays);

   createCookie('recipientID',recipientID,cookieExpiryInDays);

   createCookie('mailingJOBID',mailingJOBID,cookieExpiryInDays);

   createCookie('reportJOBID',reportJOBID,cookieExpiryInDays);

 }

 

