  <script type="text/javascript">
      function loading_page() {
          $('#call_loading_page').click();
      }
      function performCheck() {

          callProgressBar(10);
          
          if (Page_ClientValidate("manageReservation")) {
              
              $('#cancelConfirmCheck').click();
          }
      }

      $(document).ready(function () {
          Fox.InitializeHomepageReservationItinerary();
      });


      function setUpdatedText() {
          callProgressBar(50);
          var valOftext = $("#hdnPayMesage").html();
         
          if (valOftext == "") {
          
              $(".clsPreBlueDiv").hide();
          }
          else
          {
              $(".clsPreBlueDiv").show();
          
          }
          $(".ltrCancel").html(valOftext);

          $("#hdnPayMesage").html("");
//          hdnPayMesage
          callProgressBar(80);
          $(".progressBar").hide();
//          EnableDisablePopup(false);
      }


      function EnableDisablePopup(flag) {
//          clsBtnCancelReserve
          //          clsDonot

          // Get the dialog buttons.
          var dialogButtons = $(".progressBar").dialog("option", "buttons");

          // Find and disable the "Done" button.
          $.each(buttons, function (buttonIndex, button) {

              if ($(this).attr("id") === "btnCancelReserve") {
                  button.disabled = true;
              }
          })

          // Update the dialog buttons.
              $(".progressBar").dialog("option", "buttons", dialogButtons);
         
         
          if (flag == true) {
              $(".clsBtnCancelReserve").attr("disabled", "true");
              $(".clsDonot").attr("disabled", "true");
          }
          else {

              $(".clsBtnCancelReserve").attr("disabled", "false");
              $(".clsDonot").attr("disabled", "false");
          }

      }



      function callProgressBar(step) {

          $(".progressBar").progressbar({
              value: 0
          });
      }; 

        </script>




