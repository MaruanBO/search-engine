    //// JIRA Issue 401: Fix has been applied for IE, FireFox and Chrome ENTER press.    (Ali -  Xelleration Team)
    $(document).ready(function () {
        $(".enterpress").keypress(function (e) {
            var charCode = (e.keyCode ? e.keyCode : e.which);
            if (charCode == 13) {
                $(this).blur();
            }
        });
    });    


     function FoxEncrypt() 
     {
        var _txtPassword = document.getElementById("Password").value;        
        var _value = "";
        asciiKeys = [];
        for (var i = 0; i < _txtPassword.length; i++) 
        {
            asciiKeys.push(_txtPassword[i].charCodeAt(0));
            if (_value == "")
                _value = _txtPassword[i].charCodeAt(0);
            else {
                _value += " " + _txtPassword[i].charCodeAt(0);
            }
        }
        _txtPassword = _value;                
        document.getElementById("Password").value = _txtPassword;
        return;
    }                        

</script>
