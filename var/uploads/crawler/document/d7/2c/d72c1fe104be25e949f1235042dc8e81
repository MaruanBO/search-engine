$("document").ready(function(){
  $(".tabs ul a").click(function(e){
    e.preventDefault();
    
    $(".tabs ul li").removeClass('active');
    $(this).parent().addClass('active');
    $(".tabs .tab-content").hide();
    $(".tabs ."+$(this).attr('id')+"-content").show();
  });
  $(".submit").click(function(e){
    e.preventDefault();
    
    $(this).parents("form").submit();
  });
  $("#search-form input[name=search]").focus(function(){
    if( $(this).hasClass('example') ){
      $(this).val('');
      $(this).removeClass('example');
    }
  });
  equalHeight($(".category-article-mini"));
});


$("document").ready(function(){

    // truncate teasers in the the go deeper grid
	  if ($('.textbox .teaser-text').length) {
		$('.textbox .teaser-text').truncatable({    limit: 120, more: ' ...' });
		}

    // adjust spacing on list items with no thumbnail
    $('.recent-news .entry').not(':has(img)').children('.entry-content').css("width", "440px");

	});



function mtColorlinesComments(){
  if( user && user.is_authenticated ){
    //$(".form-header").attr("style","border:1px solid red;");
    
    //remove fb signin link
    $("#signin_with_Facebook").hide();
    
    //add "Hi whatever" text and signout link
    $('<p class="signedin">Hi '+user.name+', you\'re signed in. Click here to <a href="javascript:mtSignOutOnClick();">Sign out</a></p>').insertAfter("#comment-form .form-header");
    
    //hide text fields
    $(".comment-form .hidable").hide();
  }
}
function equalHeight(group) {
    tallest = 0;      
    group.each(function() {
        thisHeight = jQuery(this).height();
        if(thisHeight > tallest) {
            tallest = thisHeight;
        }
    });
    group.height(tallest);
}

GA_googleFetchAds();

