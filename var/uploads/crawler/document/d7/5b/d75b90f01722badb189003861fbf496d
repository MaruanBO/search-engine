    var renderSearchElement = function() {
      google.search.cse.element.render(
      {
        div: "cse-search",
        tag: 'searchbox-only',
        gname: 'rm-site-search'
      });
      if (typeof(showSearchQuery) == 'function') showSearchQuery();
    }
    var cseCallback = function() {
      if (document.readyState == 'complete') {renderSearchElement();} else {
        //google.setOnLoadCallback(renderSearchElement, true);
        $(window).load(renderSearchElement);
      }
    };

    window.__gcse = {
      parsetags: 'onload',
      callback: cseCallback
    };

    (function() {
      var cx = '016446845654345809449:lqtzp0nmz3c';
      var gcse = document.createElement('script'); gcse.type = 'text/javascript';
      gcse.async = true;
      gcse.src = (document.location.protocol == 'https' ? 'https:' : 'http:') + '//www.google.com/cse/cse.js?cx=' + cx;
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(gcse, s);
    })();
    
    function searchWithGoogle(inputID) {
      var csElement = google.search.cse.element.getElement('rm-site-search');
      if (csElement) {
        csElement.execute($("#" + inputID).val());
      }
      return false;
    }
