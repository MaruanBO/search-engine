  <script type="text/javascript">

  function generateLocationUrl(is_mileage_request){
    var sites = {
      md: {
        url: "http://maps.randmcnally.com/"
      },
      tm: {
        url: "http://tripmaker.randmcnally.com/index.php/",
        prb: 0
      }
    };
  
    var prob = Math.ceil(Math.random() * 100);
    if(prob > sites.tm.prb){ 
      if(is_mileage_request){
        return sites.md.url + "mileage_calculator?";
      } else {
        return sites.md.url + "?";
      }
    } else {
      if(is_mileage_request){
        return sites.tm.url + "mileage-calculator?";
      } else {
        return sites.tm.url + "?";
      }
    }
    
  }

  function getDirections(){
    _gaq.push(['_trackEvent', 'Call to Action', 'Submit Form', 'Submit_GetDirections']);
    var get_params = "loc[]=" + $("#mnd-origin").val().trim() + "&loc[]=" + $("#mnd-destination").val().trim();  
    location.href = generateLocationUrl() + get_params;
  }

  function findLocation(){
    _gaq.push(['_trackEvent', 'Call to Action', 'Submit Form', 'Submit_ShowOnMap']);
    var get_params = "loc[]=" + $("#mnd-location").val().trim();
    location.href = generateLocationUrl() + get_params;
  }

  function getMileage(){
    _gaq.push(['_trackEvent', 'Call to Action', 'Submit Form', 'Submit_CalcMileage']);
    var get_params = "loc[]=" + $("#mnd-mile-origin").val().trim() + "&loc[]=" + $("#mnd-mile-destination").val().trim();
    location.href = generateLocationUrl(true) + get_params;
  }

  $(window).load(function() {
    
    $('#mnd-origin, #mnd-destination').keypress(function (e) {
      if (e.which == 13) {
        $('#btnDirections').click();
        return false;
      }
    });
    $('#mnd-location').keypress(function (e) {
      if (e.which == 13) {
        $('#btnLocation').click();
        return false;
      }
    });
    $('#mnd-mile-origin, #mnd-mile-destination').keypress(function (e) {
      if (e.which == 13) {
        $('#btnMileage').click();
        return false;
      }
    });
  });
</script> 

  
