              function loadVideo(index){
                if(typeof player == "object" && typeof player.destroy == "function")
                    player.destroy();

                player = new YT.Player('player', {
                  height: '540',
                  width: '960',
                  playerVars: { 'autoplay': 0, 'controls': 1, 'autohide': 1, 'modestbranding': 0, 'showinfo': 0, wmode: 'opaque', rel: 0, theme: 'dark', start: player_videos[index].start },
                  videoId: player_videos[index].id,
                  events: {
                    'onReady': onPlayerReady,
                    'onStateChange': function(event){
                        if (event.data == YT.PlayerState.ENDED) { 
//                            player.destroy();

                            $('.box1').removeClass("box1-active box1-2");
                            $('#player').hide();

                            $('.icon-dot2').removeClass("active");
                            $('.icon-dot2:first').addClass("active");

                            $('#player-holder svg').show();
                        }
                    }
                  }
                });
            }


              // 4. The API will call this function when the video player is ready.
              function onPlayerReady(event) {
                player_ready = true;

                var isMobile = false;
                if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
                    isMobile = true;
            //              alert(navigator.userAgent);
                }
                if(!isMobile) event.target.playVideo();

