function autoSuggestLocation() {

var input1 = document.getElementById('starting');

if(input1 != null)

var autocomplete1 = new google.maps.places.Autocomplete(input1);	

var input1 = document.getElementById('starting-location');

if(input1 != null)

var autocomplete1 = new google.maps.places.Autocomplete(input1);



var input2 = document.getElementById('destination');

if(input2 != null)

var autocomplete2 = new google.maps.places.Autocomplete(input2);

};

google.maps.event.addDomListener(window, "load", autoSuggestLocation);

</script>

