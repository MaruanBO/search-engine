<script type="text/javascript">
  jQuery(document).ready(function(){
    
  var clickEvent = false;
  jQuery('#views-bootstrap-carousel-1').carousel({
  }).on('click', '.list-group li', function() {
      clickEvent = true;
      jQuery('.list-group li').removeClass('active');
      jQuery(this).addClass('active');   
  }).on('slid.bs.carousel', function(e) {
    if(!clickEvent) {
      var count = jQuery('.list-group').children().length -1;
      var current = jQuery('.list-group li.active');
      current.removeClass('active').next().addClass('active');
      var id = parseInt(current.data('slide-to'));
      if(count == id) {
        jQuery('.list-group li').first().addClass('active'); 
      }
    }
    clickEvent = false;
  });
})

jQuery(window).load(function() {
    var boxheight = jQuery('#views-bootstrap-carousel-1 .carousel-inner').innerHeight();
    var itemlength = jQuery('#views-bootstrap-carousel-1 .item').length;
    var triggerheight = Math.round(boxheight/itemlength+1);
  jQuery('#views-bootstrap-carousel-1 .list-group-item').outerHeight(triggerheight);
});

</script> </div>
