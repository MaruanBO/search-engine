<script>
	function FreestyleFrontpageMainBlockTab(ATab){
		LTabCnt = 1;
		LTabSubCnt = 1;
		LHtml = '';
		while(LTabCnt<=GFreestyleFrontpageMainBlock)
		{
			if(ATab == LTabCnt)
			{
				LHtml += document.getElementById('Freestyle_FrontpageMainBlockActive'+ATab).innerHTML;
			}
			else
			{
				LHtml += '<div class="artical-off" ><a href="javascript:void(0)" onclick="';
				while(LTabSubCnt <= GFreestyleFrontpageMainBlock)
				{
					if(LTabSubCnt == LTabCnt)
						LHtml += "document.getElementById('articaltap"+LTabSubCnt+"').className = 'articaltapS"+LTabSubCnt+"';";
					else
						LHtml += "document.getElementById('articaltap"+LTabSubCnt+"').className = 'articaltapH"+LTabSubCnt+"';";
					LTabSubCnt++;
				}
				LHtml += '">'+ document.getElementById('Freestyle_FrontpageMainBlockSubHeading'+LTabCnt).innerHTML+'</a></div>';
				LTabSubCnt = 1;
			}
			LTabCnt++;
		}
		return LHtml;
	}
	function FreestyleFrontpageMainBlockTabs(){
		LTabsCnt = 1;
		while(LTabsCnt<=GFreestyleFrontpageMainBlock)
		{
			document.getElementById('Freestyle_FrontpageMainBlockTabs'+LTabsCnt).innerHTML = FreestyleFrontpageMainBlockTab(LTabsCnt);
			LTabsCnt++;
		}
	}
	FreestyleFrontpageMainBlockTabs();
</script><!-- freestyle end -->
