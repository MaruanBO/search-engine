					function searchsubmit()	{
						var strsearch = document.getElementById('searchquery').value;
						strsearch =  strsearch.replace(/^\s+|\s+$/g,"");
						if(strsearch != null && strsearch != "" ){
							return true;
						} else {
							alert('Enter Valid Search Keyword');
							return false;
						}
