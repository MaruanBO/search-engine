		<script type="text/javascript">

			$( "#optinForm" )
				.validationEngine(
					'attach',
					{
						scroll: false,
						autoHidePrompt: true,
						autoHideDelay: 2000
					}
				)
				.submit(function( event ) {
	 
				  // Stop form from submitting normally
				  event.preventDefault();
				 
				  // Get some values from elements on the page:
				  var $form = $( this ),
				    email = $form.find( "input[name='EMAIL']" ).val(),
				    source = $form.find( "input[name='SOURCE']" ).val(),
				    list_id = $form.find( "input[name='list_id']" ).val(),
				    newsletter = $form.find( "input[name='NEWSLETTER']" ).val(),
				    url = $form.attr( "action" );

				    $form.find( "input[name='EMAIL']" ).addClass("optinProccessing");
				 
				  if(email !== "" && email.indexOf("@") >= 0) {

				  // Send the data using post
				  var posting = $.post( url, { EMAIL: email, SOURCE: source, list_id: list_id, NEWSLETTER: newsletter, subscribe: "true", AJAX: "true" } );
				 
				  // Put the results in a div
				  posting.done(function( data ) {
				  	if(data == "214" || data == "success") {
				  		// $("#contents").fadeIn(100);
				  		// $(".optinForm").hide();
				  		window.location = "http://www.muscleforlife.com/signup/?sub=true&loc=frontpage";
				  	} else {
				  		$form.find( "input[name='EMAIL']" ).stop().css("background-color", "#FFFF9C").attr("placeholder", "Enter Your Email Please!").removeClass("optinProccessing").focus();
				  	}	
				  });

				  } else {

				  	$form.find( "input[name='EMAIL']" ).stop().css("background-color", "#FFFF9C").attr("placeholder", "Enter Your Email Please!").removeClass("optinProccessing").focus();

				  }

			});


		</script>

	</div>


