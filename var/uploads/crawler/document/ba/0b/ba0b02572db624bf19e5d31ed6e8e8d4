<script>
	if(getCookie("pty_visited") !== "visited" && $(window).width() >= 800){
		setTimeout(function(){

			$(".pty_overlay").fadeIn();
			setCookie("pty_visited", "visited", 30);

			$("#pty_5mistakes").click(function(e){
				e.stopPropagation();
			});

			$("#pty_close, #pty_bottom_content, .pty_overlay").click(function(e){
				$(".pty_overlay").fadeOut();
				e.preventDefault();
				e.stopPropagation();
			});

		}, 5000);
	} 

	if ( $('#pty_form').length ) {
		$('#pty_form').validationEngine(
			'attach',
			{
				scroll: false,
				autoHidePrompt: true,
				autoHideDelay: 2000
			}
		);
	}
</script>	
