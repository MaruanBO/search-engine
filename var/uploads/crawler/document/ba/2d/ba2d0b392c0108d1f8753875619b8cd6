        <script type="text/javascript">
        jQuery(window).load(function(){
            jQuery('#slider-PageContent-bxslider').nivoSlider({
                effect: "fade",
                slices: 12,
                controlNav: true,
                controlNavThumbs: false,
                directionNav: true,
                animSpeed: 700,
                pauseOnHover: true,
                pauseTime: 12000,
                randomStart: false,
                afterChange: function() {testThumbs(jQuery(".nivo-control.active"), 400) ;},
                afterLoad: function() {
                        jQuery('#slider-PageContent-bxslider').parents(".bxSlider").css("display","block");
                    jQuery(".nivo-control.active img").load(function() {
                        
                        setWidth(5, jQuery(".nivo-control.active img").width());
                        testThumbs(jQuery(".nivo-control.active"), 0);
                    });
                    
                }
                
                
            });
            
              
                var thebtn = jQuery('<a class="btn-pause" id="playpausebutton" href="#pause">pause/play</a>');
            
                    jQuery(".nivo-controlNav").append(thebtn);
                
                thebtn.click(function() {
                    if(thebtn.hasClass("btn-pause"))
                    {
                        thebtn.removeClass("btn-pause");
                        thebtn.addClass("btn-play");
                        jQuery('#slider-PageContent-bxslider').data("nivoslider").stop();
                    }
                    else
                    {
                        thebtn.removeClass("btn-play");
                        thebtn.addClass("btn-pause");
                        jQuery('#slider-PageContent-bxslider').data("nivoslider").start();
                    }
                
                });
                
            
            function setWidth(times, width)
            {
                theWidth = (width + 4 + 6) * times + 6; 
                
                if(theWidth < jQuery(".slider-wrapper").width())
                    jQuery(".thumbs-bottom-left .nivo-thumbs-enabled, .thumbs-bottom-right .nivo-thumbs-enabled, .thumbs-bottom-center .nivo-thumbs-enabled").width(jQuery(".slider-wrapper").width());
                else
                    jQuery(".thumbs-bottom-left .nivo-thumbs-enabled, .thumbs-bottom-right .nivo-thumbs-enabled, .thumbs-bottom-center .nivo-thumbs-enabled").width(theWidth);
            
            }
            
            function testThumbs(totest, speed)
            {
                    if(!jQuery(".thumbs-left .nivo-control.active img, .thumbs-right .nivo-control.active img, .thumbs-bottom-left .nivo-control.active img, .thumbs-bottom-right .nivo-control.active img,.thumbs-bottom-center .nivo-control.active img").length)
                        return;
                     a = totest;
                     t = jQuery(".nivo-thumbs-enabled");
                     s = jQuery(".slider-wrapper");
                    
                    
                    if(jQuery(".thumbs-left .nivo-control.active img, .thumbs-right .nivo-control.active img").length > 0)
                    {                 
                        a = totest.children("img").eq(0);   
                        if(a.offset().top + a.height() > t.offset().top + t.height())
                        {
                            o = a.offset().top + a.height() - (t.offset().top + t.height()) + 15;
                            jQuery(".nivo-control").animate({top:"-=" + o + "px"},speed);
                        }
                        if(a.offset().top < t.offset().top)
                        {
                            o =  t.offset().top - a.offset().top + 15;
                            jQuery(".nivo-control").animate({top:"+=" + o + "px"},speed);
                        }
                    }
                    else {
                        //for sideways
                        if(a.offset().left + a.width() > s.offset().left + s.width())
                        {
                            o = a.offset().left + a.width() + 6 - (s.offset().left + s.width());
                            jQuery(".nivo-control").animate({left:"-=" + o + "px"},speed);        
                        }
                        if(a.offset().left < s.offset().left)
                        {
                            o = s.offset().left + 6 - a.offset().left;
                            jQuery(".nivo-control").animate({left:"+=" + o + "px"},speed);
                        }
                     
                    }
            }
    
            
        });
        
        </script>
