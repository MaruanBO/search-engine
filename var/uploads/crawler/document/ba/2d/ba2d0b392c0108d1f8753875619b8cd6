    jQuery(document).ready(function() {
        if ( jQuery('#Future_Students>div').size() <= 1 ) {
            jQuery('#Future_Students>div').addClass('chosen');
        }
        else {
            while (jQuery('#Future_Students>div.chosen').size() < 1) {
                var x = jQuery('#Future_Students>div:not(".chosen")').size();
                var y = Math.floor(Math.random()*x);
                jQuery('#Future_Students>div:not(".chosen")').eq(y).addClass('chosen');
            }
        }
    });
