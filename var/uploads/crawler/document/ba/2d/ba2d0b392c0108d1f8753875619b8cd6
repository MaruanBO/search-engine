            .captions-left .nivo-prevNav
            {
               left: 250px;
               z-index: 13;
            }
            .captions-right .nivo-nextNav
            {
               right: 250px;
               z-index: 13;
            }
            
            .btn-pause
            {
                background: url(/etc/designs/uc/resources/lenses/NewsTickerSlider/images/pausebutton.png) !important;
            }
            .btn-play
            {
                background: url(/etc/designs/uc/resources/lenses/NewsTickerSlider/images/playbutton.png) !important;
            }
            
            .notsquares
            {
            }
            

            
            
            .nivo-thumbs-enabled
            {
                background-color:  !important;
            }
            
            
