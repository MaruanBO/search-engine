<script type="text/javascript">
	$("#big_search").autocomplete({
		source: "http://www.singletracks.com/ajax/st_search.php?t=all",
		minLength: 5,
		select: function( event, ui ) {
			window.location.href = ui.item.value;
			return false;
		}
	})
	.data( "autocomplete" )._renderItem = function( ul, item ) {
			return $( "<li></li>" )
				.data( "item.autocomplete", item )
				.append( "<a>" + item.label + "</a>")
				.appendTo( ul );
	};

	$("#big_search_button").click(function(){
		 $("#big_search_form").submit();
		 preventDefault();
	});
	function load_more_news(start){
		$("#load_news_div").empty().html("Loading...");
		$.get( "http://www.singletracks.com/ajax/bsv_news.php?start="+start+"&datestart=1424382119", function( data ) {
			$( "#mtb_news" ).append( data );
			var next = start + 15;
			$("#load_news_div").empty().html("<a href='javascript:load_more_news("+next+");'><img src='./ra-content/images/graphics/load_more.png' style='height: 35px; width: 620px;' /></a>");
		});	
	}

</script>

