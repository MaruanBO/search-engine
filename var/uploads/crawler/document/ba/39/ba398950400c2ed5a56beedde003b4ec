<script type="text/javascript">
    jQuery(function()
    {
       jQuery('#zip').change(function()
       {
            // Grab the state from the option's value and then get a count of matching elements
            var state = jQuery(this).val(),
                count = jQuery('.'+state).length;

                console.log(state);

            count === 0 && state !== 'ALL'
            ? jQuery('#no-list').html('<span>Sorry No Events For This Location</span>')
            : jQuery('#no-list').html('');

             // The CSS for the list gets screwed up if there aren't enough listings
            //  This compensates for that
            (count < 3)
            ? jQuery('.gigpress-table td').css('vertical-align', 'text-bottom')
            : jQuery('.gigpress-table td').css('vertical-align', 'middle');

            // If the user selects ALL just unhide all elements
            if (state === 'ALL')
            {
              jQuery('.gigpress-rowa').each(function()
                {
                    jQuery(this).show();
                });
            }
            else if (state !== '') // Else cycle through and hide non-matching elements
            {
                jQuery('.gigpress-rowa').each(function()
                {
                    if (!jQuery(this).hasClass(state))
                    {
                        jQuery(this).hide();
                    }
                    else
                    {
                        jQuery(this).show();
                    }
                });
            }
       });
    });
</script>

