<script type="text/javascript">

var galSwiper;

var preGalFunc = window.onload;
window.onload = function() {
	preGalFunc();  // whatever else had been scheduled to run on load...
	
	galSwiper = new Swiper('.galswiper-container',{
		//Your options here:
		mode:'horizontal',
		wrapperClass: 'galswiper-wrapper',
		slideClass: 'galswiper-slide',
		pagination: '.galpagination',
		slidesPerSlide:4,
		loop: true,
		//etc..
	});  
}

function nextGalSlide()
{
	var num = galSwiper.activeIndex;
	num += 1;
	if(num == 11) num = 0;

	galSwiper.swipeTo(num, 300);	
}

function prevGalSlide()
{
	var num = galSwiper.activeIndex;
	num -= 1;
	if(num == -1) num = 10;

	galSwiper.swipeTo(num, 300);	
}

</script>

