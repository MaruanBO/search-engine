<script type="text/javascript">
    $(document).ready(function () {
    
    
    $('.exploresantafenm .nav-main-item-home a').css({
        width:150,
        height:30,
        display:'block',
        overflow:'hidden'
    }).text('');
    $('.exploresantafe .nav-main-item-home a').css({
        width:150,
        height:30,
        display:'block',
        overflow:'hidden'
    }).text('');
    
    
    
                $('.sfn-columnists li').each(function(){
                        $('p.byline', this).insertBefore($("h4.blox-headline", this));
                    });
                //var browser;
                //if($.browser.mozilla){
                 //   browser="Firefox";
                 //}
                //if(browser != "Firefox"){
		 //  $("body").disableSelection();    
                 //}
                //var conductRule7 = '<li><span class="rule-number">7</span><span class="rule-text"><span class="short">Commenters</span> Must use thier real names.</span></li>';
		//$(conductRule7).insertAfter($('.rules').children().eq(5));


		
		/* make commenting rules less in your face, per ticket reqeust */

		var nOpen = 0;
		$('.rules-columns').hide();
		var sDivInsert = '<span class="rules-display" style="display:block;">Click the text above to view </span>';
		$(sDivInsert).insertAfter( $('#comment-rules h3') );
		$('#comment-rules h3').click(function() {
		  $('.rules-columns').slideToggle(1000,'linear',function() { 
			  nOpen++; 
			  if(nOpen == 1)
			  {
						$('.rules-display').css('display', 'none');
						$('.rules-display').remove();

				   var sDivInsert = '<span class="rules-display" style="display:block;">Click the text above to close </span>';      
				   $(sDivInsert).insertAfter( $('#comment-rules h3') );
				   $('.rules-display').css('display', 'block');                  
			  }
			  
			  if( nOpen >=2 ){
				  nOpen = 0;
				  $('.rules-display').remove();      
				  var sDivInsert = '<span class="rules-display" style="display:block;">Click the text above to view </span>';
				  $(sDivInsert).insertAfter( $('#comment-rules h3') );
			  }
			})
		});



		/* provide mouse over tooltip visual cues */

		$('.pasa-logo').attr('title', 'HOME');
		$('#sfnm-small').attr('title', 'NM Homepage')
		
		

		
		
		/* change the text for the similar stories to more stories */
		$('.related-by-section h3').eq(0).text("More Stories");  
		
		
		/* populate search box with text sfnm side only  */

		var sPathWithSlashes = '/';
		var sPublicationName = sPathWithSlashes.substr(0,11);
		if(sPublicationName != '/pasatiempo'){
			$('.search-bar').val('Search')
			$('.search-bar').focus(function() {  
				this.value = '';  
			}); // closes search
		}       

		
		
		$('#mostPopAccordian').children().eq(1).css('display','none');
		
		
		
		/* replace most commented/most read image flags with numbers */

		var itemIter = 1;
		$(".numbered-list .blox-list").each(function () {
			var itemIter = 1;
			$("li", this).each(function() {
				var numberedItem = '<span class="numbered-item">' + itemIter + '.</span>';
				$(numberedItem).insertBefore( $(this).find("h4") );
				if (itemIter >= 10) {
					itemIter = 1;
				} else {
					itemIter++;
				}
			});
		}); 

		
		
		//* BEGIN: get/show the section and sub section to display in header *//
                
                // get current page url
                
		var sSection = "/";
		var sSectionClean = ''; // used later
                
		// array to hold the positions of the forward slashes
                
		var aPos = [];
                
                // poss is the number of characters to the first 
                // forward slash - used to seed the while loop
                
		var poss = sSection.indexOf("/");
                
		// as one is found push the value into the array
                
		while (poss > -1) {
			aPos.push(poss);
			poss = sSection.indexOf("/", (poss + 1));
                      
		}

        // after populating array perform additional
        // use the values to extract the section name
                
                var nLength = aPos.length;
		if (nLength) {
                
                        // the length of the entire url string
			nLen = sSection.length;
                        
                        // get portion of the url string from the end (nLen-1) to the position 
                        // of the last found (aPos[(aPos.length-1)] forward slash 
                        
			sSectionClean = sSection.substr((aPos[(aPos.length-1)] + 1), (nLen - 1));
                        
		} 
                
                
                
		
                //  sections where the name is different form the url
                //  url shows no puncuation/apostrophes
                
		if(sSectionClean == 'terrells_tuneup') {
			sSectionClean = "TERRELL'S TUNE-UP";
		}     
		
		if(sSectionClean == 'readings_signings') {
			sSectionClean = "READINGS &amp; SIGNINGS";
		}
		
		if (sSectionClean == 'multimedia') {
			sSectionClean = "MEDIA CENTER";
		}

                // display NO title on this section
		if (sSectionClean == 'forms') {
			sSectionClean = "";
		}
		
		if(sSectionClean == 'letters_to_editor') {
			sSectionClean = 'LETTERS'
		}
                

		
                // after removing forward slashes
                // remove underscores
                
		underscorepos = sSectionClean.indexOf("_");
		if(underscorepos > -1) {
			sSectionClean = sSectionClean.replace(/_/g, " ");
		}

		var sSubSection = sSection.substr(0, 11);

		if(sSubSection == "/pasatiempo") {
			$('#section-name-pasa').append(sSectionClean);
		} else {
			$('#section-name').append(sSectionClean);
		}

		/* END: get/show the sub section to display in header */
		

		
		
			var sUpdated = '';

			

		


		
		$("#ud-time").insertAfter("#snm-latestupdates-para");
		var sCalendarSubTitle = "<span id='cal-sub-title'>TODAY'S CALENDAR EVENTS</span>";
		$(sCalendarSubTitle).insertAfter($('.pasa-calendar .blox-asset-content-block.event .blox-title'));

		
		
		$('.subscribemessage a').attr("href","javascript:void(0)"); 
		 $('.total_access a').attr("href","javascript:void(0)"); 
                 
                 /* #tntracking 632647 */
                 $(".users.admin.calendar.event label[for='contact_name'], .users.admin.calendar.event input[name='contact_name']").addClass("required");
                 
                 /* #tntracking 533614: calendar: event tools (gray box) */
                 $("#eventDetail #sidebar .highlight-box").append('<span class="content-partition subtle"></span><ul><li><a href="http://www.santafenewmexican.com/exploresantafe/">Back to Calendar</a></li></ul>');
                /* #tntracking 533426 2013.05.24 mgorman */                 
                $("#calendar-widget-block #calendar-widget-buttons .today").attr("href", "/calendar");
                
                /* #tntracking mgorman last updated block */
                $('.snm-latestupdates .blox-updated').each(function() {
                    $(this).prependTo($(this).parents(".blox-recent-block").find("h4.blox-headline"));
                });
                 
	});
</script>



