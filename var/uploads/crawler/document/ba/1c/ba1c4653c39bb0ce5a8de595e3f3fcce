        function onBeforeslider1col0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAfterslider1col0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#slider-1-col-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#slider-1-col-0 .item-list').cycle({
		fx: effectslider1col0,
		sync: true,
		fit: 1,
		
		timeout: 10000,
                before: onBeforeslider1col0,
		after:	onAfterslider1col0,
        pause:true,
		pager:'#slider-1-col-0 .nav',
		prev:'#slider-1-col-0 .prev',
		next:'#slider-1-col-0 .next'
	});
        
            $('#slider-1-col-0 .pause').on('click',function(){
                if($('#slider-1-col-0').hasClass('paused')){
                    $('#slider-1-col-0').removeClass('paused').find('.item-list').cycle('resume');
                }else{
                    $('#slider-1-col-0').addClass('paused').find('.item-list').cycle('pause');
                }
            });
        

        

