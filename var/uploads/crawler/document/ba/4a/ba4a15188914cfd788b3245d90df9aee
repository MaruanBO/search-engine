    <script>
        function avatarError(image){
            image.onerror = "";
            image.src = "/assets/pixel.gif";
            $(image).addClass('missingAvatar');
            return true;
        }

        $(document).ready(function() {
            //Hide on error'd images
            $("img.hoe").error(function(){
                $(this).hide();
            });
        });
    </script>
