<div id="mainPhotoSection">

<script language="javascript">

//bannerImage = {'3': "L|http://www.allaboutdance.com/search/WEAR-MOI",'4':"L|http://www.allaboutdance.com/dance-clothing/shoes/ballet-shoes/page1?brand=RUSSIANPOINTE",'1': "L|http://www.allaboutdance.com/search/MOTIONWEAR",'2': "L|http://www.allaboutdance.com/search/MOTIONWEAR?ageGroup=child"};

bannerImage = {
				'1': "L|http://www.allaboutdance.com/search.php?skey=danshuz&SortOrder=N",
				'2': "L|http://www.allaboutdance.com/dance-clothing/child/leotards/page1",
				'3': "L|http://www.allaboutdance.com/dance-clothing/bodywear/tops/page1",
				'4': "L|http://www.allaboutdance.com/search.php?skey=mylist:62092&SortOrder=N",
				'5': "L|http://www.allaboutdance.com/search.php?skey=gia_mia&SortOrder=N"	
			  };


function changeImage(imageNumber)
	{
		var noOfBanners = 5;
		$("#mainPhotoSectionLevel1").html("<img src=\"/images/holiday_banner_"+imageNumber+".jpg?569\">");
		$("#mainPhotoSectionLevel2").animate({ opacity: 0.0 }, 500, function()
			{
				arrayElement = bannerImage[""+imageNumber+""];
				linkInfo = arrayElement.split("|");
				$("#mainPhotoSectionLevel2").html("<img src=\"/images/holiday_banner_"+imageNumber+".jpg?569\">");
				if (linkInfo[0]=="L")
				{
					$("#mainPhotoSectionLevel3").html("<a onClick=\"_gaq.push(['_trackEvent', 'AAD_HomePage', 'Headerlink', '"+linkInfo[1]+"'])\" href="+linkInfo[1]+"><img src=\"/images/cover.png\" border=\"0\"></a>");
				}
				else
				{
					$("#mapInformation").html(""+linkInfo[1]+""); // place map details on the page
					$("#mainPhotoSectionLevel3").html("<img usemap=\"#Map\" src=\"/images/cover.png\" border=\"0\">");
				}
				$("#mainPhotoSectionLevel2").css({ opacity: 1});
				for(linkStyle=1;linkStyle<(noOfBanners+1);linkStyle++)
					{
						if (linkStyle==imageNumber)
							{
								$("#image"+linkStyle+"").css("text-decoration","underline");
							}
							else
							{
								$("#image"+linkStyle+"").css("text-decoration","none");
							}
					}
				$("#currentImage").val(""+imageNumber+"");	
				if (imageNumber<noOfBanners) { imageNumber++; } else { imageNumber=1; }
				
				showTimer=setTimeout(function(){changeImage(imageNumber) },3000);															
			});	
	}
function changeImageManual(imageNumber)
	{
		var noOfBanners = 5;
		clearTimeout(showTimer);
		$("#currentImage").val(imageNumber);
		$("#mainPhotoSectionLevel1").html("<img src=\"/images/holiday_banner_"+imageNumber+".jpg\">");
		
		$("#mainPhotoSectionLevel2").animate({ opacity: 0.0}, 500, function()
			{
				arrayElement = bannerImage[""+imageNumber+""];
				linkInfo = arrayElement.split("|");
				$("#mainPhotoSectionLevel2").html("<img src=\"/images/holiday_banner_"+imageNumber+".jpg\">");
				if (linkInfo[0]=="L")
				{
					$("#mainPhotoSectionLevel3").html("<a onClick=\"_gaq.push(['_trackEvent', 'AAD_HomePage', 'Headerlink', '"+linkInfo[1]+"'])\" href="+linkInfo[1]+"><img src=\"/images/cover.png\" border=\"0\"></a>");
				}
				else
				{
					$("#mapInformation").html(""+linkInfo[1]+""); // place map details on the page
					$("#mainPhotoSectionLevel3").html("<img usemap=\"#Map\" src=\"/images/cover.png\" border=\"0\">");
				}
				$("#mainPhotoSectionLevel2").css({ opacity: 1});
				for(linkStyle=1;linkStyle<noOfBanners;linkStyle++)
					{
						if (linkStyle==imageNumber)
							{
								$("#image"+linkStyle+"").css("text-decoration","underline");
							}
							else
							{
								$("#image"+linkStyle+"").css("text-decoration","none");
							}
					}																
			});	
	}
function stopShow(){
	$("#mainPhotoSectionLevel2").stop(false, true);
	clearTimeout(showTimer);	
}
function startShow(){
	imageNumber = $("#currentImage").val();
	showTimer=setTimeout(function(){ changeImage(imageNumber) },600);
}
$(document).ready(function(){
						   $("#mainPhotoSectionLevel3").bind({
													   mouseenter: function(){ stopShow(); },
													   mouseleave: function(){ startShow(); }
													   });
						   
						   
						   showTimer=setTimeout(function(){changeImage(4) },4000);
						  
						   });
</script>
