<script type="text/javascript" language="javascript">
	function displayDCError(errorMsg) {
		$("#_discountCodeDiv").before("<div id='divDCErrorSummary' class='errorTextSummary'><span class='hidden'>Error</span><div style='position:absolute; margin-left:30px;'>" + errorMsg + "</div></div>");
	}

	function showAboutContractFares() {
		if ($("#divAboutContractFares").html() == "") {
			var protocol = "http:";
			protocol = (top.location.protocol ? top.location.protocol : protocol) + "//";
			$.get(protocol + "www.alaskaair.com/shared/tips/aboutcompanyfares.aspx", function (data) {
				$("#divAboutContractFares").html(data);
				showLightBox();
			});
		}
		else {
			showLightBox();
		}
		function showLightBox() {
			$("#divAboutContractFares").showLightBox({ width: 300 }).show();
		}
	}
</script>
        <script type="text/javascript">
            function InitializePage() {

                //Check for EasyBiz cookie, if present, display end session message.  Used in the case where IE caches page.
                if (Get_Cookie('AS%5FACNT%5FCOMPANY') != null) {
                    if ((confirm("Accessing this page will end this EasyBiz session."))) {
                        deleteCookieJS("AS%5FACNT%5FCOMPANY", ".alaskaair.com", "/");
                    } else {
                        window.location.href = "https://www.alaskaair.com/content/easybiz.aspx";
                    }
                }

                if ($("#lastElementClicked").val() != '') {
                    var elementToClick = "#" + $("#lastElementClicked").val();
                    $(elementToClick).click();
                }
            }

            as.homeContent.onDisplayCalling.push(
                function(nearestAirport) {
					if(nearestAirport == null || nearestAirport == 'non')
						nearestAirport = '';
                    
					$.get("/Fare/GetLowestFare?airportCode=" + nearestAirport, function(lowestPrice) {
						$('div.hero-sale-pricing > .hero-saleprice > span').text(lowestPrice);
						$('#heroLink').append($('.hero-sale'));
						$('.hero-sale')                                
							.toggle(true);
					});
                    
                }
            );
        </script>
