							if (isOops) {
								SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetCar, "Oops", "iFrame content contains 'Oops'", "", cariframeUrl), carReferrer, carLogType);
								writeCarExceptionError();
							}
							else if($('#_carIframe').height() == 0)
							{
								// Set widget Car Error Message
								SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetCar, "FrameHeight 0", "iFrame height is 0", "", cariframeUrl), carReferrer, carLogType);
								writeCarExceptionError();
							}
							else
							{
								// Successful iframe loading
								writeStatusMessage(errorPathCar, 'no error', carReferrer, carLogType, 'success');
								$('#cariFrameError').hide();
							}

						}
						catch(e)
						{
							// Set widget Car Error Message
							SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetCar, "iFrameLoadEventHandlerException", e.message, getCallstack(e), cariframeUrl), carReferrer, carLogType);
							writeCarExceptionError();
						}
					}
				}
			</script>
		

