<script type="text/javascript">
	$(document).ready(function(e){
		//on back button, force page to refresh to avoid invalid widget state
		var isAwardFromServer = false;
		var shopAwardCalendarFromServer = false;
		var shopLowFareCalendarFromServer = false;
		var isOneWayFromServer = false
		var awardOptionFromServer = "";

		if(isOneWayFromServer && $("#oneWay").attr("checked") != true){
			shoppingWidget.SelectFlightType();
		}
		if(!isOneWayFromServer && $("#oneWay").attr("checked") != false){
			shoppingWidget.SelectFlightType();
		}
	});
</script>

