				function HandleCFrameLoaded() {
					if (document.getElementById('_carIframe') && $('#_carIframe').attr('src') != "javascript:void(0);") {

						try
						{
							ResetTimer();

							$('#carInterstitial').hide();

							try {
							    if ($('#_carIframe').height() == 0 && $('#_carIframe').contents().find("body").html() != null && $('#_carIframe').contents().find("body").html() != "") {
							        $('#_carIframe').height(200);
								}
							} catch(e) {}	//ignore exception

							$('#_carIframe').show();

   							var isOops = false;
							try {
								isOops = ($('#_carIframe').contents().find("body h2").html() == "Oops");
							}
							catch(e) {
								writeStatusMessage(errorPathCar, BuildErrorMessage(exceptionMessageWidgetCar, "CheckingForOops", e.message, getCallstack(e), $('#_carIframe').attr('src')), carReferrer, carLogType, 'Unknown');
