		<div id="hotelInterstitial" class="hide"></div>
		
			<iframe id="_hotelIframe" name="_hotelIframe" title="Hotel Shopping" src="javascript:void(0);" width="412" height="200" align="left" scrolling="no" allowTransparency="true" frameborder="0" onload="setTimeout('HandleHFrameLoaded()');"> </iframe>

			
			<script type="text/javascript">
				var hoteliframeUrl = "http://bframe.alaskaair.com/dl/hotels/search_form?&amp;Form=Small&amp;CartToken=0";
				var hotelReferrer = "HomePageNav";
				var hotelLogType = "HomePageHotelIFrameLoading";
				var hasHotelInCart = false;
				exceptionMessageWidgetHotel = "iSeatz Hotel Widget Exception | " + hotelReferrer;

				function HandleHFrameLoaded() {
					if (document.getElementById('_hotelIframe') && $('#_hotelIframe').attr('src') != "javascript:void(0);") {

						try {
							ResetTimer();

							$('#hotelInterstitial').hide();

							try {
								if ($('#_hotelIframe').height() == 0 && $('#_hotelIframe').contents().find("body").html() != null && $('#_hotelIframe').contents().find("body").html() != "") {
									$('#_hotelIframe').height(200);
								}
							} catch(e) {}	//ignore exception

							$('#_hotelIframe').show();

							var isOops = false;
							try {
								isOops = ($('#_hotelIframe').contents().find("body h2").html() == "Oops");
							}
							catch (e) {
								writeStatusMessage(errorPathHotel, BuildErrorMessage(exceptionMessageWidgetHotel, "CheckingForOops", e.message, getCallstack(e), $('#_hotelIframe').attr('src')), hotelReferrer, hotelLogType, 'Unknown');
							}
							if (isOops) {
								SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetHotel, "Oops", "iFrame content contains 'Oops'", "", hoteliframeUrl), hotelReferrer, hotelLogType);
								writeHotelExceptionError();
							}
							else if ($('#_hotelIframe').height() == 0) {
								// Set Widget Hotel Error Message
								SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetHotel, "FrameHeight 0", "iFrame height is 0", "", hoteliframeUrl), hotelReferrer, hotelLogType);
								writeHotelExceptionError();
							}
							else {
								// Successful iframe loading
								writeStatusMessage(errorPathHotel, 'no error', hotelReferrer, hotelLogType, 'success');
								$('#hoteliFrameError').hide();
							}

                        }
						catch (e) {
							// Set Widget Hotel Error Message
							SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetHotel, "iFrameLoadEventHandlerException", e.message, getCallstack(e), hoteliframeUrl), hotelReferrer, hotelLogType);
							writeHotelExceptionError();
						}
					}
				}
			</script>
		

		<div style="clear:both;"><!--  --></div>
		<a href="https://www.alaskaair.com/Shopping/Hotel/Search" style="display:none" id="RedirectHotelHREF"></a>
</div>
<script type="text/javascript">
	function assignHotelURl() {
		
		try
		{
			$('#hotelInterstitial').show();

			SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetHotel, "Timeout", "iFrame load timed out", "", hoteliframeUrl), hotelReferrer, hotelLogType);
			SetHotelTimer("30000");

			if (document.getElementById('_hotelIframe')) {
				document.getElementById('_hotelIframe').setAttribute('src', hoteliframeUrl);
				$('#_hotelIframe').height(0);
			}
		}
		catch(e)
		{
			// Set Widget Hotel Error Message
			SetErrorMessage(BuildErrorMessage(exceptionMessageWidgetHotel, "assignHotelURl", e.message, getCallstack(e), hoteliframeUrl), hotelReferrer, hotelLogType);
			writeHotelExceptionError();
		}
		
	}
</script>

