<script type="text/javascript">
    function HeroLink() {
        var _me = this;
        this.baseUrl = "http://www.alaskaair.com/content/deals/flights.aspx?lid=hp:homepage&merch=false";
        this.disclaimerBaseUrl = "http://www.alaskaair.com/content/deals/flights.aspx?lid=hp:homepage&terms=true&merch=false";

        this.setLink = function (q, o) {
            var params = "";
            if (q.length > 0 && q.toLowerCase() != "non")
                params = params + "&q=" + q.toLowerCase();
            if (o.length > 0 && o.toLowerCase() != "non")
                params = params + "&o=" + o.toLowerCase();

            $("#heroLink").attr("href", _me.baseUrl + params);
            $("#heroDisclaimerLink").attr("href", _me.disclaimerBaseUrl + params);
        }
    }
</script>
				
                    <div class="clear"></div>
                </div>			
            </div>		

            <script type="text/javascript">
    function mmcorePreCGRequest() {
        new MaxymiserRepository().SetPersCriterions();
    }

    function mmcorePostCGRequest() {
        // console.log("mmcorePostCGRequest");
        // note: maxymiser returned js code will be executed on "DOMContentLoaded", 
        // which is a bit right after $(document).ready()
    }

    $(document).ready(function () {
        as.homeContent.display();  // display default, maxymiser code will execute right after this if any to overwrite
        //console.log("$(document).ready(function () {}");
    });

    function HomeContent() {
        var _me = this;
        this.onDisplayCalling = new Array();
        this.onDisplayCalled = new Array();
        _me.contentChoice = "non";

        this.hide = function () {
            _me.contentChoice = "non";

            if (_me.contentChoice.length > 0) {
                $("body").removeClass(_me.contentChoice);
            }
        }

        function invokeAll(array) {
            return function(arg) {
                for (var idx in array) {
                    var callBack = array[idx];
                    callBack(arg);
                }
            };
        }

        this.display = function (newContentChoice) {            
            _me.hide();
			
            var params = _me.getQuerystringParams(window.location.search);
            if (params.hasOwnProperty("cc")) {
                newContentChoice = params["cc"];
            }
			
			invokeAll(_me.onDisplayCalling)(newContentChoice);

            if (typeof (newContentChoice) == "string" && newContentChoice.length > 0) {
                _me.contentChoice = newContentChoice.toLowerCase();
            }

            if (_me.contentChoice.length > 0) {
                $("body").addClass(_me.contentChoice);
            }

            if (typeof (HeroLink) == "function") {
                new HeroLink().setLink(_me.contentChoice, _me.contentChoice);
            }

            $(".footer-servername").append(" " + _me.contentChoice);
            invokeAll(_me.onDisplayCalled)(newContentChoice);
        }

        this.getQuerystringParams = function (querystring) {
            var qs = unescape(querystring);

            if (!qs) {
                return {};
            }
            if (qs.substring(0, 1) == "?") {
                qs = qs.substring(1);
            }

            var qsDictionary = {};
            var pairs = qs.split("&");
            for (var i = 0; i < pairs.length; i++) {
                var keyValuePair = pairs[i].split("=");
                qsDictionary[keyValuePair[0]] = keyValuePair[1];
            }

            return qsDictionary;
        }
    }

    if (typeof (as) != "undefined") {
        as.homeContent = new HomeContent();
    }
</script>


            

