<script type='text/javascript'>
function Interstitial() {
    this.show = function () {
        var interstitialId = 'interstitial';
        var top = ($(window).scrollTop() + 200);
        top = (top > 0 ? top : 0);
        var left = ($(window).width() - $('#' + interstitialId).outerWidth()) / 2;
        left = (left > 0 ? left : 0);
        $.showInterstitialFormFiller(false);
        if ($.browser.msie && $.browser.version <= 7) { $('html').css('overflow', 'hidden'); }
        else { $('body').css('overflow', 'hidden'); }
        $('#' + interstitialId).css({ position: 'absolute', margin: 0, top: top + 'px', left: left + 'px' });
        $('#' + interstitialId).show();
    }

    
    this.insertInterstitialImages = function () {        
        _insertAdImage();  
        _insertAnimationImage();
        _insertLogoImage();
    }
        
    function _insertAdImage() {    
        $('body').append($("<img src='http://resource.alaskaair.net/content/~/media/images/onSite/interstitial/interstitial-Hotel250BonusMiles20120402.ashx' alt=''></img>").hide());
    }
    
    function _insertAnimationImage() {        
        $("#intAnimation").append($("<img src='/content/~/media/images/onSite/interstitial/interstitial-animated-dots.ashx' alt='alaskaair.com is processing your request'></img>"));
    }

    function _insertLogoImage() {
        $('#intLogo').append($("<img src='/content/~/media/images/logos/Interstitial_logo.ashx' alt='alaskaair.com'></img>"));
    }    
}

// in case this is used in a formlet where there is no header and footer, and the global object "as" is not defined
if (typeof as === "undefined" || as == null)  {
    new Interstitial().insertInterstitialImages();
}
else {
    as.interstitial = new Interstitial();
}
</script>

<script type='text/javascript'>
    function ShowInterstitial() {        
        if (typeof as === "undefined" || as == null) {
            new Interstitial().show();
        }
        else {
            as.interstitial.show();
        }
    }
</script>
