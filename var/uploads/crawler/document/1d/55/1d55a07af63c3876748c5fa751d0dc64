                    <script language="javascript" type="text/javascript">
                     window.addEvent('load',function() {
                         var showDuration = 4000;
                         var container = $('homeSS');
                         var slides = container.getElements('div');
                         var currentIndex = 0;
                         var interval;
                         /* opacity and fade */
                         slides.each(function(slide,i){ 
                           if(i > 0) {
                             slide.set('opacity',0);
                             slide.setStyle('display','block');
                           }
                         });

                         /* worker  function*/
                         function show() {  
                           slides[currentIndex].fade('out','slow');
                           slides[currentIndex = currentIndex < slides.length - 1 ? currentIndex+1 : 0].fade('in','slow');
                         };    

                         interval = show.periodical(showDuration);
                        });
                    </script>

