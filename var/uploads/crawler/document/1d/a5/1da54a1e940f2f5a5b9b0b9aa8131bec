<script type="text/javascript">
	$(document).ready(function() {
		$("li.megamenu_flyout").mouseenter(function() {
			var id = $(this).attr("id").split("_");
			//console.log("id: "+id[0]);
			$("ul#"+id[0]+"_flyout").css({"display":"block"});
		}).mouseleave(function() {
			var id = $(this).attr("id").split("_");
			$("ul#"+id[0]+"_flyout").css({"display":"none"});
		});
	});
</script>
