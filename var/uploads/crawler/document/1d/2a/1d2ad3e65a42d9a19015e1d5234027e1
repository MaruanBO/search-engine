<script type="text/javascript">

    $(document).ready(function() {
        function validateForm() {
            var x = $('#subscribeEmail').val();
            var atpos = x.indexOf("@");
            var dotpos = x.lastIndexOf(".");
            if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
                return false;
            }
            return true;
        }

        $('#subscribeButton').click(function(){$("#subscribeForm").submit();});

        // this is the id of the submit button
        $("#subscribeForm").submit(function(e) {
            e.preventDefault();
            if (validateForm()) {
                $.ajax({
                    type: "POST",
                    url: "/new/feedback",
                    data: $("#footerSubmitForm").serialize(), // serializes the form's elements.
                    success: function(data)
                    {
                        if (data) {
                            $('#subscribeBar, #legal').css("background", "#31CE89");
                            $('#subscribeEmail').val("");
                            $('#subscribeEmail').attr("placeholder", "Thank you for signing up!");
                        } else {
                            $('#subscribeBar, #legal').css("background", "#DB5252");
                            $('#subscribeEmail').val("");
                            $('#subscribeEmail').attr("placeholder", "Error: Please Try Again...");
                        }
                    }
                });
            } else {
                var invalid = $('#subscribeEmail').val();
                $('#subscribeBar, #legal').css("background", "#DB5252");
                $('#subscribeEmail').val("");
                $('#subscribeEmail').attr("placeholder", "Invalid Email");
                $('#subscribeEmail').click(function() {
                    $('#subscribeEmail').val(invalid);
                });
            }

            return false; // avoid to execute the actual submit of the form.
        });

        $('#footerSliderContainer').on('mouseleave', function(){
            if ($('#footerSliderContainer').css('max-height') === '336px') {
                $('.footerExpandButton').click();
            }
        });

        $('.fullWidthContent').click(function(){
            if ($('#footerSliderContainer').css('max-height') === '336px') {
                $('.footerExpandButton').click();
            }
        });

        $('.footerExpandButton').click(function(){
            $('#footerSliderContainer').css({'max-height': "310px"});
            $(this).find('span').html('View Less');
            $(this).removeClass('fa-caret-up').addClass('fa-caret-down');

            if ($('#footerSliderContainer').css('max-height') === '310px') {
                $('#footerSliderContainer').css({'max-height': ""});   
                $(this).removeClass('fa-caret-down').addClass('fa-caret-up');
                $(this).find('span').html('Site Map');
            }
        });

        $('#companyHeader').click(function(){
            $(this).addClass('selected');
            $('#productHeader').removeClass('selected');
            $('#companyContainer').show();
            $('#productContainer').hide();
            $('#bottomFooterContent').show();
        });

        $('#productHeader').click(function(){
            $(this).addClass('selected');
            $('#companyHeader').removeClass('selected');
            $('#companyContainer').hide();
            $('#productContainer').show();
            $('#bottomFooterContent').hide();
        });

        $(window).resize(function(){
            if ($(window).width() > 767) {
                $('#productContainer').css({display:""});
                $('#companyContainer').css({display:""});
                $('#bottomFooterContent').css({display:""});
            }
        });
    });
</script>

