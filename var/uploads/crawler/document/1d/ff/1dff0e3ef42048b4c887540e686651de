      daysEvents = $('<ul class="maryland_today_list"/>');

      // Create and format the What's Happening calendar
      $('#maryland_today').append('<div id="maryland_today_calendar" />');

      $('#maryland_today_calendar').datepicker({
        dayNamesMin: ['S', 'M', 'T', 'W', 'T', 'F', 'S'],
        beforeShowDay: function(date) {
          tempDate = date.toString();
          if (datesArray[tempDate] !== undefined) {
            return [true, 'ui-state-has-active'];
          } else {
            return [true, ''];
          }
        },
        onSelect: function(dateText, inst) {
          pos = $('#maryland_today_calendar .ui-datepicker-calendar').offset();

          selectedDate = new Date(inst.selectedYear, inst.selectedMonth, inst.selectedDay, 0, 0, 0);
          if (datesArray[selectedDate] !== undefined) {
            daysEvents.html('');

            $.each(datesArray[selectedDate], function(i, v) {
              daysEvents.append('<li>' + $('#maryland_today_list li:eq(' + (v - 1) + ')').html() + '</li>');
            });

            daysEvents.dialog({
              title: selectedDate.toString('MMMM d, yyyy'),
              position: [pos.left - width - $(window).scrollLeft(), pos.top - $(window).scrollTop()]
            }).dialog('open');
          }
        }
      });

      // datePicker starts with the current day having the hover state. Remove.
      $('#maryland_today_calendar a.ui-state-hover').trigger('mouseout');
      width = $('#maryland_today_calendar').width();
      daysEvents.dialog({
        autoOpen: false,
        draggable: false,
        width: width,
        resizable: false,
        show: 'fade',
        hide: 'fade'
      });
    }());
  });
//]]>
</script>


  
