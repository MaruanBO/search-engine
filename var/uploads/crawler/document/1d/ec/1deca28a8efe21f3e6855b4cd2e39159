            <!-- Top Products -->
                <script type="text/javascript">

        function showHotTopic(thumb) {

            var img = $(thumb).find('img');
            
            var oldImageUrl = $('#hotTopicDetailsFullImage').attr('src');
            var newImageUrl = $(img).attr('data-imageurl');

            if (oldImageUrl == newImageUrl)
                return false;

            $('#hotTopicDetailsFullImage').stop();

            $('#hotTopicDetailsBody').html($(img).attr('data-abstract'));

            var sep = ' ';
            $(thumb).find('form').find('input').each(function() {
                var term = $(this).attr('name');

                var link = $(this).val();

                $('#hotTopicDetailsBody').append(sep + '<a class="hotTopicLink" style="color:#00C;" href="' + link + '">' + term + '</a>');
                
                sep = ', ';
            });

            $('#hotTopicDetailsFull').css('background-image', 'url(' + newImageUrl + ')');
            $('#hotTopicDetailsFullImage').fadeTo(500, 0, function() {
                $('#hotTopicDetailsFullImage').attr('src', newImageUrl).fadeTo(1, 1);
            });
            return false;
        }

        $(document).ready(function() {
            $('#hotTopicsCarousel').jsCarousel({
                autoscroll: false,
                circular: true,
                masked: false,
                itemstodisplay: 5,
                orientation: 'h',
                scrollspeed: 500
            });

            $('.hotTopicThumbnail').mouseover(function() { showHotTopic($(this)); });
        });

    </script>
