    <script type="text/javascript">

        function showVideoDetail(element, narrow) {
            var currentVideoId = $('#videoPlayer').data("videoid");

            $(".youTubeResult").removeClass("selected");

            if (element == null || ($('#videoDetailsPanel').is(':visible') && currentVideoId == $(element).data("videoid"))) {
                $("#videoDetailsPlayer").html("<iframe id=\"videoPlayer\" allowfullscreen></iframe>");

                if (narrow)
                    $('#videoDetailsPanel').hide('slide', { direction: 'right' }, 500);
                else
                    $('#videoDetailsPanel').slideUp();

                return false;
            }

            $(element).addClass("selected");

            var videoId = $(element).data("videoid");
            var title = $(element).data("title");
            var description = $(element).data("description");

            $("#videoDetailsPlayer").html("<iframe id=\"videoPlayer\" allowfullscreen></iframe>");
            $("#videoDetailsTitle a").attr("href", "//www.youtube.com/watch?v=" + videoId).text(title);
            $("#videoDetailsBody").text(description);

            var showVideo = function () {
                $("#videoPlayer").data("videoid", videoId);
                $("#videoPlayer").attr("src", "//www.youtube.com/embed/" + videoId + "?autoplay=0");
            };

            if (narrow)
                $('#videoDetailsPanel').show('slide', { direction: 'right' }, 500, showVideo);
            else
                $('#videoDetailsPanel').slideDown(showVideo);

            return false;
        }

        $(document).ready(function () {

            var itemsToShow = 5;
            var narrow = false;

            $(".youTubeResult").click(function () { showVideoDetail(this, narrow); });
            $("#videoDetailsClose").click(function () { showVideoDetail(null, narrow); });

        });
    </script>

        </div>

    </div>
</div>
<script type="text/javascript">

    var selectedTile = '';

    $(document).ready(function () {
        $('form#BigSearch input').focus(function () {
            $('form#BigSearch').fadeTo('slow', 1);
        });
        $('form#BigSearch input').blur(function () {
            $('form#BigSearch').fadeTo('slow', .7);
        });

        if (selectedTile.length > 0) {
            $("#HomepageButtons").find('li.' + selectedTile).find('a').addClass('selected').find('img').fadeTo(1, 1);
        }

        $("#HomepageButtons").find('li').find('a').hover(
            function () {
                console.log('ggg');
                $(this).find('img').fadeTo(1, 1);
            }, function () {
                $(this).find('img').fadeTo(1, .8);
            }
        );

    });

    function homepageMenuClick(link) {
        return false;
    }

</script>

