          <script type="text/javascript">
            
            $(document).ready(function() {
              //load emailPop
               _gaq.push(['_trackEvent', 'Email_Overlay_Sitewide', 'Generated', 'Sticky Footer Email Test']);
              $('#stickyFooter').hide();
              $('#emailPop').modal();              
              $('form#emailSubmit').submit(function(e) {
                    //get form data email_address
                    var formData = $(this).serialize();
                    //add loading gif 
                    $('#initialPopContent').hide();
                    $('#displayError').hide();
                    $('#displayLoading').show();
                      //send ajax call
                      $.ajax({
                        url: '/ajax/emailpop_join_scroll.php',
                        data: formData,
                        type: 'POST',
                        //handle successful ajax call
                        success: function(data) {
                          
                          //hide loading gif
                          $('#displayLoading').hide();
                          //display thank you message for new entry
                          data = $.trim(data);
                          if (data == "created") {
                             _gaq.push(['_trackEvent', 'Email_Overlay_Sitewide', 'Overlay Created', 'Sticky Footer Email Test_814']);
                            $('#displayThankYou').show();
                            $('#stickyFooter').remove();
                          }
                          //display already exists message for duplicate entry
                          if (data == 'exists') {
                             _gaq.push(['_trackEvent', 'Email_Overlay_Sitewide', 'Overlay Exists', 'Sticky Footer Email Test_814']);
                            $('#displayExists').show();
                            $('#stickyFooter').remove();
                          }
                          //handle error and give option to retry
                          if (data == 'error') {
                             _gaq.push(['_trackEvent', 'Email_Overlay_Sitewide', 'Overlay Error', 'Sticky Footer Email Test_814']);
                            $('#displayError').show();
                          }
                        }
                      });
                    $('.closeModal').click(function() {
                      $('#emailPop').modal('hide');
                    });
                    return false;
                  });
             
                            
              
              //add hover state to input group
              $('#emailPop input.form-control').on('focus', function() {
                $(this).parent().addClass('hovered');
              }).on('blur', function() {
                $(this).parent().removeClass('hovered');
              });

              /*$('.submitForm').click(function(e) {
                $('form#emailSubmit').submit();
                return false;
              });*/
            });

          </script>

