   (function() {
       var tt = document.createElement('script'); tt.type = 'text/javascript'; tt.async = true;
       tt.src = document.location.protocol + "//static.www.turnto.com/traServer4_1/trajs/" + turnToConfig.siteKey + "/tra.js";
       var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(tt, s);
   })();
   /*
			 * This function is called by TurnTo code to discern the login status of the user on the Partner site.
			 */
			function localGetLoginStatusFunction(callback) {
				console.log('Inside localGetLoginStatusFunction');
				$.ajax({
					type: 'POST',
					url: 'turnto_login_status.php',
					dataType: 'json',
					error: function() { alert("error"); },
					success: function(data) {
						console.log('at start of success function');
						if(data.length != 0) {
							if(data.loginStatus == true) {
								console.log('loginStatus is true and cust_id is'+data.cust_id);
								callback({user_auth_token: data.cust_id});
							} else {
								console.log('loginStatus is false');
								callback({user_auth_token: null});
							}
						}
							console.log('Inside localGetLoginStatusFunction.  Return JSON = ' + JSON.stringify(data));
					}
				});
			}
			/*
			 * This function is called by TurnTo code to get details about the currently logged in user on the Partner
			 * site.
			 */
			function localGetUserInfoFunction(callback) {
				console.log('Inside localGetUserInfoFunction qc');
				turntoCallbackFn = callback;
				var ttinfo = document.createElement('script');
				ttinfo.type = 'text/javascript';
				ttinfo.async = true;
				ttinfo.src = "https://www.dermstore.com/turnto_user_info.php";
                //ttinfo.src = "http://qc.dermstore.com/turnto_user_info.php";
				var s = document.getElementsByTagName('script')[0];
				s.parentNode.insertBefore(ttinfo,s);
				console.log('End of localGetUserInfoFunction');
			}
			/*
			 * This function is called by TurnTo code to log out any currently logged in user on the Partner site.
			 */
			function localLogoutFunction(callback) {
				console.log('Inside localLogoutFunction');
				$('#logoutHidden').click(function(e) {
					e.preventDefault();
					$.ajax({
						url: $(this).attr("href"),
						type: 'POST',
						success: function(result) {
							callback();
						}
					});
				}).click();
			}
			function turntoItemTeaserDisplay(data) {
				var iteaserhtml = '';
				iteaserhtml += '<div><strong>Q&A:</strong>&nbsp;<a href="#TurnToContent" <a onclick="show_TurnTo();">Ask a question</a><br>';
				if (data.counts.q > 0 || data.counts.a > 0) {
					iteaserhtml += '  <span style="color: #555"></span>  See All ';
					if (data.counts.q > 0) {
						//iteaserhtml +=  'data.counts.q + (data.counts.q > 1 ? ' Questions ' : ' Question ');
						iteaserhtml +=  'Questions(' + data.counts.q +') ';
					}
					if (data.counts.a > 0) {
						iteaserhtml +=  '& Answers(' + data.counts.a  + ')';
					}
				}
				iteaserhtml += '</div>';
				return iteaserhtml;
			}
 </script>
