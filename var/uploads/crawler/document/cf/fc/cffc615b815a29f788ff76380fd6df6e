  <script>
		function orientation_change() {
			if (navigator.userAgent.match(/iPad/i)) {
				var mw = 1024; // min width of site
       			var ww = 0;
        		var ratio = 0;

				if (window.orientation == 0 || window.orientation == 180) {
					ww = screen.width;
				} else if (window.orientation == -90 || window.orientation == 90) {
					ww = screen.height;
				}
				ratio =  (ww / mw) * 0.9;
				document.getElementById("viewport").content = 'initial-scale=' + ratio + ', maximum-scale=' + ratio + ', minimum-scale=' + ratio + ', user-scalable=yes, width=' + ww;
			}
		}
		if (window.addEventListener) {
		    window.addEventListener('orientationchange', orientation_change);
		} /*else {
		    window.attachEvent("orientationchange", orientation_change);
		}*/
		orientation_change();
	</script>
