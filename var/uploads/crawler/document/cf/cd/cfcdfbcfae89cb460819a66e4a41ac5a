<script>
$(function () {
    $('#topSearch a').click(function (e) {
        e.preventDefault();
        $('#topSearch a').removeClass('active');
        $(this).addClass('active');
        $(this).tab('show');
        if ($(this).hasClass("byName")) {
            if ($('#byName input').val() != '') {
                $('#byName input#topsearch').focus();
            } else {
                $('#byName input#city_state').focus();
            }
        }
        if ($(this).hasClass("byPhone")) {
            $('#byPhone input').focus();
        }
        if ($(this).hasClass("byAddress")) {
            $("#byAddress input[name='street']").focus();
        }
    });
  });

function topsearchClear(elem) {
    var e = $(".tab-pane.active div input:first");
    var eval = e.val();
    if (eval == "") {
    e.parent().find('.reset-field').css('display', 'none');
    }   else {
    e.parent().find('.reset-field').css('display', 'block');
    }
    var e = $(".tab-pane.active div input:last");
    var eval = e.val();
    if (eval == "") {
    e.parent().find('.reset-field').css('display', 'none');
    }   else {
    e.parent().find('.reset-field').css('display', 'block');
    }
};
topsearchClear();
</script>
