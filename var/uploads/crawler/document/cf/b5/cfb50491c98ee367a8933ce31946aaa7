<script>
	function toggle_login_messages(success, errros, processing)
	{
		if(success)
			$('#div_login_success').css('display', '');
		else $('#div_login_success').css('display', 'none');
		
		if(errros)
			$('#div_login_errors').css('display', '');
		else $('#div_login_errors').css('display', 'none');

		if(processing)
			$('#div_login_processing').css('display', '');
		else $('#div_login_processing').css('display', 'none');
	}

	function submitLogin(){
		toggle_login_messages(false, false, true);
		var form_data = $("#login-form").serialize();
		$.post("/users/login_ajax", form_data, function(data)
		{
			toggle_login_messages(true, false, false);
			refresh_user_status();
			$("#login-modal").modal('toggle');
			$.cookie("zuus_logged_in", true);
		}).error(function(data)
		{
			if(data.responseText == null)
				alert('An unexpected error ocurred, please try again');

			toggle_login_messages(false, true, false);
      $('#div_login_errors').html($.parseJSON(data.responseText).messages);
		});
	}

	$(function(){
		$("#submit-login").click(function(){
			submitLogin();
		});

		$("#login-form input").keypress(function(e){
			if(e.which == 13){
				submitLogin();
			}
		});
	});
</script>

