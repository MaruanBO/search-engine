  <script>
      var layout_type = "desktop";
      // Apply watermarks for IE instead of placeholders
      configurePlaceholders();

      function refresh_user_status() {
          url = "/users/status_bar";
          $.get(url, function(data)
          {
              $('#user-status').html(data);
          }).error(function()
          {
              alert('An error ocurred when refreshing user status bar, please try again');
          });
      }

      function destroy_session() {
          url = "/sessions/destroy";
          $.get(url, function(data)
          {
              refresh_user_status();
              $.cookie("zuus_logged_in", false);
          }).error(function()
          {
              console.log("Error when destroying session");
          });
      }

      function loadFavorites() {
          $.get( "/users/load_favorites_ajax", function(data)
          {
              $( "#menu-section-favorites" ).html(data);
          });

          cookie_value = $.cookie("zuus_logged_in");
          if(cookie_value.length > 0 && $("#section-favorites").css("display") == "none")
          {
              $("#section-favorites").show();
          } else if(cookie_value.length == 0 || cookie_value == "false")
          {
              $("#section-favorites").hide();
          }
      }

      	function check_geo()
       {
       var url = window.location.pathname
       var cookie_value = $.cookie("valid_geo");

       if(cookie_value == true || cookie_value == "true" || (url.indexOf("country-not-supported") >= 0 || url.indexOf("country_not_supported") >= 0))
       return;

       random_number = Math.random();
       $.get("/check-geo?rand=" + random_number, function(data)
       {
       $.cookie("valid_geo", true, { expires : 30 });
       }).error(function(data)
       {
       $.cookie("valid_geo", false, { expires : 1 });
       //window.location.href = "/country-not-supported";
       });
       }

      $(function()
      {
          //check_geo();
          refresh_user_status();
      });
  </script>

