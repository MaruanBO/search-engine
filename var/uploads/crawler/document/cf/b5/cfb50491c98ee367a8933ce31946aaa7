        <script>
	$(function(){
		// Clean form when register-button is clicked
		if($("#register-button").length > 0) 
		{
			$("#register-button").click(function(){
				$("#register-modal").find("input").val("");
				$("#register-modal").find("input[type=checkbox]").attr("checked", false);
			});
		}

		$("#submit-create-user").click(function()
		{
			toggle_create_messages(false, false, true);
			var form_data = $("#register-form").serialize();
			$.post("/users/create_ajax", form_data, function(data){
				toggle_create_messages(true, false, false);

				if(layout_type == "desktop"){
					refresh_user_status();
					$("#register-modal").modal('toggle');
				} 
				else if (layout_type == "mobile") 
				{
					// Open mobile app
					window.location = "zuusapp://openapp";
				}

			}).error(function(data){

				if(data.responseText == null){
					alert("An unexpected error ocurred, please try again");
				}

				toggle_create_messages(false, true, false);
				$('#div_register_errors').html($.parseJSON(data.responseText).messages);

			});
		});

		// Apply watermarks for IE instead of placeholders
  	configurePlaceholders();
	});

	function toggle_create_messages(success, errros, processing)
	{
		if(success)
			$('#div_register_success').css('display', '');
		else $('#div_register_success').css('display', 'none');
		
		if(errros)
			$('#div_register_errors').css('display', '');
		else $('#div_register_errors').css('display', 'none');

		if(processing)
			$('#div_register_processing').css('display', '');
		else $('#div_register_processing').css('display', 'none');
	}
</script>

