<div class="container content"><script type="text/javascript">
    function loadCarousel(){
        if(navigator.userAgent.match(/Firefox/i)){
            // Fix Featured shape if available
            var featured_shape_elements = $('.content-featured');
            if(featured_shape_elements.length > 0) {
                featured_shape_elements.css('padding', '0');
                var featured_shape_elements_text = $('.content-featured div');
                featured_shape_elements_text.css('position', 'absolute');
                featured_shape_elements_text.css('margin-top', '-14px');
                featured_shape_elements_text.css('margin-left', '12px');
                featured_shape_elements.height(1);
            }
        }

        if($("#content-carousel") == null) {
            return;
        }

        $('.carousel-inner').children('.item:first').addClass("active");
        $('.carousel-indicators').children('li:first').addClass("active");
        $("#content-carousel").carousel({
            interval : 10000,
            pause: false
        });

    }

    function fadeCarousel(){

        var $carousel=$("#home-carousel");
        $carousel.carousel();

        $carousel.bind('slide.bs.carousel', function (e) {
            $(".content-carousel-shapes").hide();
            $(".content-shape").hide();
        });
        $carousel.bind('slid.bs.carousel', function (e) {
            $(".content-shape").fadeIn("slow");
            $(".content-carousel-shapes").fadeIn("slow");
        });

    }

    function setCarouselCaptionOffsetY(milliseconds){
        if(milliseconds == null){
            milliseconds = 500;
        }

        // Reposition div
        var window_height = $(window).height();
        var carousel_info_bottom = 80;
        var carousel_info_height = $("#carousel-info").innerHeight();
        var carousel_info_offset_y = window_height - carousel_info_height - carousel_info_bottom;
        //$("#carousel-info").css("top", carousel_info_offset_y + "px");
        $("#carousel-info").animate({
            top: carousel_info_offset_y + "px"
        }, milliseconds);
    }

    function configureCarouselCaptionClickEvent(){
        // Make carousel-info clickable if current carousel slide
        $("#carousel-info").unbind("click");
        $("#carousel-info").css("cursor", "auto");
        if($('.carousel-inner').children('.item.active').first().attr("data-href").length > 0){
            $("#carousel-info").click(function(){
                window.location = $('.carousel-inner').children('.item.active').first().attr("data-href");
            });
            $("#carousel-info").css("cursor", "pointer");
        }
    }

    function positionCarouselCaption(){

        $("#carousel-info").hide();
        var active_caption = $('.carousel-inner').children('.item.active').children('div.carousel-caption').html();
        $("#carousel-info").html(active_caption);
        $("#carousel-info").fadeIn(1000, function(){
            setCarouselCaptionOffsetY();
        });

        configureCarouselCaptionClickEvent();

        $("#content-carousel").bind("slid", function(){
            var active_caption = $('.carousel-inner').children('.item.active').children('div.carousel-caption').html();
            $("#carousel-info").fadeOut(250, function(){
                $("#carousel-info").html(active_caption);
                $("#carousel-info").fadeIn(250, function(){
                    // Reposition div
                    var window_height = $(window).height();
                    var carousel_info_bottom = 80;
                    var carousel_info_height = $("#carousel-info").innerHeight();
                    var carousel_info_offset_y = window_height - carousel_info_height - carousel_info_bottom;
                    // $("#carousel-info").css("top", carousel_info_offset_y + "px");
                    $("#carousel-info").animate({
                        top: carousel_info_offset_y + "px"
                    }, 500);
                });
            });

            configureCarouselCaptionClickEvent();
        });
    }

    function fixIEStyles(){
        if($.browser.msie && ($.browser.version == "8.0" || $.browser.version == "9.0")){
            $(".icon-next, .icon-prev").css("line-height", "50px");
        }
    }

    $(function(){
        loadCarousel();
        fadeCarousel();
        positionCarouselCaption();
        fixIEStyles();
        $("#left-menu").hide(500);
    });
</script>

