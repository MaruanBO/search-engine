<script>
window.onload = function(){
	jQuery.fn.getStaticBlocks = function(klas){
		jQuery('#content_blocks li').each(function(){
			jQuery(this).hasClass(klas)? jQuery(this).show() : jQuery(this).hide();
		});
	}
	jQuery('#header_men, #header_women').live('click', function(){
		var cls = jQuery(this).attr('class');
		jQuery.fn.getStaticBlocks(cls);
	});
}
</script>		<!-- static blocks ends -->
				
