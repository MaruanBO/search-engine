function onYouTubeIframeAPIReady()
{ // Defined here, before all other scripts to be sure it's ready when the
  // YouTube iframe API loads.
  onYouTubeIframeAPIReady.hasRun = true
  for (var fx = 0; fx < onYouTubeIframeAPIReady.funcs.length; fx++)
    onYouTubeIframeAPIReady.funcs[fx]()
}
onYouTubeIframeAPIReady.hasRun = false
onYouTubeIframeAPIReady.funcs  = []
onYouTubeIframeAPIReady.run    = function (func)
{
  if (onYouTubeIframeAPIReady.hasRun)
    func()
  else
    onYouTubeIframeAPIReady.funcs.push(func)
}

