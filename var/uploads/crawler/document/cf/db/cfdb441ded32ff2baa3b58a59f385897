<script type="text/javascript" src="http://ftpcontent.worldnow.com/revenue/js/timeDifference.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	var $ts = $("#WNTSWContent #WNDS-20");
	var $col2 = $("#WNCol2 #WNDS-20");

	function tsr_timeAgo(fullTime){
		//1D2H45M5S252MS
		var tdays = parseInt(fullTime.substring(0, fullTime.indexOf("D")),10);
		if (tdays < 1) {
		    var thours = parseInt(fullTime.substring(fullTime.indexOf("D") + 1, fullTime.indexOf("H")),10);

		    if(thours < 1){

	    		var tmins = parseInt(fullTime.substring(fullTime.indexOf("H") + 1, fullTime.indexOf("M")),10);

	    		if(tmins === 0){
	    			tmins = "Updated moments ago";
	    		} else if (tmins === 1){
	    			tmins = "Updated 1 minute ago";
	    		} else {
	    			tmins = "Updated " + tmins + " minutes ago";
	    		}
	    		
	    		return tmins;

		    } else {
		    	return "";
		    }

		} else {

			return "";
		}
	};

	function fixDate($el){
		var tDate = $el;
		$(tDate).find("script").remove();
		$(tDate).find("noscript").remove();
		$(tDate).html($(tDate).text());
		
		var sd = $(tDate).text();
		sd = sd.slice(sd.indexOf("Updated: ") + 9, sd.length);
		sd = new Date(sd);

		var currentTime = new Date();

		if (navigator.appVersion.indexOf("Win")!=-1 && $.browser.mozilla && parseInt($.browser.version,10) >= 22) {
			sd = sd.getTime() - 3600000;
			//sd = sd.getTime();
			//console.log(sd)
		} else {
			sd = sd.getTime();
		}			

		var iTime = timeDiff(currentTime, sd);
		var timeAgo = tsr_timeAgo(iTime);

		$(tDate).text(timeAgo);			
	}	

	$("#wnHideParent").parents(".wnDVUtilityBlock").hide();
	var tsrBackupImg = "http://khq.images.worldnow.com/images/378197_G.jpg";
	var maxNum = 10;
	// FIX VIDEO INCLUDED AND DATE //
	fixDate($ts.find(".wnDVSummary .wnDate"));
	
	$("#WNCol2 #WNDS-20 .wnDVSummary:lt(" + maxNum + ") .wnDate").each(function(){
		fixDate($(this));
	});
	if ($("#WNDS-18").length){
		$("#WNDS-18 li h4").addClass("related").appendTo($("#WNTSWContent #WNDS-20 .wnDSItems-standard")).show();
		$("#WNDS-20 h4.related:first a").addClass("first");
	}
	if ($("#WNDS48").length){
		$("#WNDS48 .wnDSItems-standard ul").empty();
	} else {
		$("#WNTSWContent .wnDSItems-standard:first").append("<div class='wnDS48 wnDSContainer-standard wnDS' id='WNDS48'><div class='wnDSItems-standard'><ul class='wnDSItemsList'></ul></div></div>");
	}
	$("#WNDS48 .wnDSItemsList").html($("#WNCol2 #WNDS-20 .wnDVSummary:lt(" + maxNum + ")"));
	$("#WNDS48 .wnDSItemsList .wnDVSummary").wrap("<li class='wnDVHeadline'></li>");
	
	$("#WNDS48 .wnDSItemsList .wnDVSummary h4 a .wnVideoIncluded").attr("src","http://ftpcontent.worldnow.com/khq/custom/2011/icon_video.png");
	
	$("#WNDS48 .wnDSItemsList .wnDVSummary .wnRole-CLIP a").append("<img class='wnVideoIncluded' alt='Video included' src='http://ftpcontent.worldnow.com/khq/custom/2011/icon_video.png' border='0' />");

/* Updated XX Minutes Time Stamp */
	$("#WNDS48 .wnDSItemsList .wnDVSummary").each(function(){
		if($(this).find(".wnDate").length){
			$(this).find("h4").append("<span class='newDate'>" + $(this).find(".wnDate").text() + "</span>");
		}	
	});

	if(!($ts.find(".wnDVSummary .wnImage").length)){
		$ts.find(".wnDVSummary").prepend("<div class='wnImage wnImageLeft wnImageWidth-180'><a href='" + $ts.find(".wnDVSummary h4 a").attr("href") + "'><img src='" + tsrBackupImg + "' border='0'></a></div>");
	}
	
	if($ts.find(".wnDVSummary .wnVideoIncluded").length){
		$ts.find(".wnDVSummary .wnVideoIncluded").attr("src", "http://ftpcontent.worldnow.com/khq/custom/2011/icon_video.png");
	}

	if ($("#WNTSWContent #WNDS-20").height() > $("#WNTSWContent #WNDS-18").height()) {
		$("#WNTSWContent #WNDS48").height($("#WNTSWContent #WNDS-20").height())
	} else if ($("#WNTSWContent #WNDS48").height() > $("#WNTSWContent #WNDS-20 .wnDSItems-standard").height()) {
		$("#WNTSWContent #WNDS-20 .wnDSItems-standard").height($("#WNTSWContent #WNDS48").height())
	}

});
</script>
