    <script type="text/javascript">
        var popped = ('state' in window.history && window.history.state !== null), initialURL = location.href
        if (isAuthenticated == "False") {
            $(window).bind('popstate', function (event) {
                var initialPop = !popped && location.href == initialURL
                popped = true
                if (initialPop) return
                var state = event.state
                //alert(document.location + '-------' + initialURL);
                var loc = event.location || (event.originalEvent && event.originalEvent.location) || document.location;
                //if (document.location != initialURL) {
                var path = document.location;
                loadContent(loc.href);
                //}
            });
        }
        if (typeof history.historyAPI === "undefined" || /^(?:#[\/]?)?$/.test(document.location.hash)) {
            $("#main_content").show();
        }


        function ShowServerMessage(ErrorMessage) {
            if (!checkCookie('sm')) {
                setCookie('sm', null, null);
                ShowAlertMessage(ErrorMessage);
            }
        }
        function ShowAlertMessage(ErrorMessage) {

            $('#spnErrorMessage').html('');
            $('#spnErrorMessage').html(ErrorMessage);
            $('.alert-notification-wrap .dismiss-btns .btn').hide();
            if (timeOutms > 0) {
                $('.alert-notification-wrap').slideDown(500), setTimeout(function () {
                    $('.alert-notification-wrap').slideUp();
                }, timeOutms);

                $('.alert-notification-wrap a').click(function () {
                    $('.alert-notification-wrap').slideUp();
                });

                $('.alert-notification-wrap .dismiss-btns .btn').click(function () {
                    $('.alert-notification-wrap').slideUp();
                });
            } else {

                timeOutms = 5000;
                $('.alert-notification-wrap').slideDown(500);
            }
            ErrorMessage = '';

        }
    </script>
    



   

    <script type="text/javascript">

        //Chat function
        (function () {
            var done = false;
            var script = document.createElement('script');
            script.async = true;
            script.type = 'text/javascript';
            script.src = 'https://widget.purechat.com/VisitorWidget/WidgetScript';
            document.getElementsByTagName('HEAD').item(0).appendChild(script);
            script.onreadystatechange = script.onload = function (e) {
                if (!done && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete')) {
                    var w = new PCWidget({
                        c: '5fb3a2d0-9eae-4281-a44b-7a7cada638ae',
                        f: true
                    });
                    done = true;
                }
            };
        })();
        //Chat function



    </script>
