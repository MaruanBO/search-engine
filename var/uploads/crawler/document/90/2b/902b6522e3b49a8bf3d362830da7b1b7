    <script type="text/javascript">
        $(document).ready(function () {
            var is25Offer = window.location.pathname.indexOf("25offer") != -1;

            if (is25Offer) {
                $('#enrollPop25offer').modal();
            }
            else if (window.location.hash) {
                var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
                if (hash.toLowerCase() == "signup") {
                    $("#enrollPop").css("visibility", "visible");
                    $('#enrollPop').modal();

                }
                else if (hash.toLowerCase() == "signin") {
                    $("#loginPop").css("visibility", "visible");
                    $('#loginPop').modal();
                }
                // hash found
            }
        });

        
    </script>
