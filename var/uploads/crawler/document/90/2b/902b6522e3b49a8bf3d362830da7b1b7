    function RemovePagePreferenceCookise() {
        setCookie("PagePreferredSchool", 0, 1);
    }
    function SetNativeSignUpValue(sender) {
        $("#hdnNative").val(1);

        $.ajax({
            url: baseUrl + "General/Index.aspx?method=RemoveSession&x=" + (new Date()).getTime(),
            type: "POST",
            success: function (response, status) {

                if (checkCookie('InvitedUser')) {

                    window.location.href = baseUrl + 'Choose-Role';
                }
            }
        });


        }
        function RemoveMessage() {
            $("#spnUserNotExist").hide();
            $("#txtUserPassword").closest('li').removeClass('has-error');
            $("#txtUserEmailAddress").closest('li').removeClass('has-error');
            $("#txtForgotPassword").closest('li').removeClass('has-error');
        }
        function RemoveErrorClass() {
            $("#txtForgotPassword").closest('li').removeClass('has-error');
        }

        function onHideSearch(event) {
            var targetId = $(event.target).attr('id');
            if (targetId == 'global_search'
                || targetId == 'globalTxt'
                || targetId == 'submitSearch'
                || targetId == 'header_search'
                || $(event.target).parents('#global_search').length > 0)
                return;

            hideSearch(true);
        }

        function hideSearch(checkSearchValue) {

            $('#global_search').hide();
            if (!checkSearchValue || $('#globalTxt').val() == null || $('#globalTxt').val() == '') {
                $('#globalTxt').val('');
                //$('#submitSearch').hide();
                $('#header_search').show();
                $('.search_block .wrapp').animate({
                    width: "0px"
                }, 500);
            }
        }

        $(document).ready(function () {

            if (window.location.hash) {
                var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
                if (hash.toLowerCase() == "forgotpassword") {
                    $("#ForgotPassword").css("visibility", "visible");
                    setTimeout(function () {
                        $("html, body").animate({ scrollTop: $('#form1').position().top }, 0);
                    }, 100);
                    $('#ForgotPassword').modal();

                }
                else if (hash.toLowerCase() == "enrollpop" && window.location.href.toLowerCase().indexOf('choose-role') == -1) {
                    if (isAuthenticated == "False")
                        window.location.href = baseUrl + "Choose-Role#enrollPop";
                }
                $('#btnHClose2').bind('click', function () {
                    ClearCookieForRole();
                });
                // hash found
            }


            $(document).on('click', 'a[href!="javascript:void(0);"]', function () { hideSearch(false); });

            hideSearch(false);

            $('#header_search').click(function () {
                $('.search_block .wrapp').animate({
                    width: "100%"
                }, 500);
                $(this).hide();
                $('#submitSearch').show();
                $('#globalTxt').focus();
            });
            //$('#global_search').bind('mouseenter',GlobalSearch_Focus);
            //$('#global_search').on('mouseout', '#global_search', GlobalSearch_LostFocus);
            //$('#global_search').off('mouseout', '#global_search');

            $(document).on('click', onHideSearch);
            $(document).on('click', 'a[href="#enrollPop"]', enrollClick);
            $(document).on('click', 'input[href="#enrollPop"]', enrollClick);

            function enrollClick(e) {
                if (window.returnUrl) {
                    window.location.href = baseUrl + "Choose-Role?Returnurl=" + returnUrl + "#enrollPop";
                } else { window.location.href = baseUrl + "Choose-Role#enrollPop"; }
            }

            $(".icon-awards").unbind('click');
            $(".icon-awards").click(function () {
                if ($('.explore-wrap').length == 0) {
                    isFromOther = true;
                    loadContent(baseUrl + "Dashboard", true);
                }

            });
            //Get Current Status of tutor 
            if (isAuthenticated == "True") {
                var options = {
                    url: baseUrl + "AppCommon.ashx?method=tutoronline&x=" + (new Date()).getTime(),
                    type: "POST",
                    data: { SetReset: 2 },
                    success: function (response, status) {
                        var getTutorStatus = $.parseJSON(response);
                        if (getTutorStatus && getTutorStatus.SetReset == 1) {
                            $('.offline').addClass('online');
                            CommitLogoutTimer();
                        }

                    },
                    error: function (error) {
                        //alert('error');
                        //window.location.href = baseUrl;
                    }
                }
                $.ajax(options);
            }
            //end current tutor

            $('#imgProfileImg').bind('load', function () { setImageProportion(this); });
            GetGplusJs();  //required on gplus load
            //Start embed Home video
            $('#videoPopup').on('show', function () {      //show event used for bootstrap modal event
                setHomeVideo();
            });
            if (window.location.href == baseUrl || window.location.href == baseUrl + "#loginPop") {
                if (!$('#btnHomeVideo').is(':visible')) {
                    console.log('Player Initialized');
                    setHomeVideo();
                }
            }
            //End embed Home video


            //For tutor online
            var refresh = false;  //Control variable to control refresh access


            $(window).keydown(function (event) {
                if (event.keyCode == 116) { // User presses F5 to refresh
                    refresh = true;

                }
            });
