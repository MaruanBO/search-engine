    <script type="text/javascript">
        function UpdateSession(obj) {
            setCookie('Coockie_AppLang', $('#DLAppLang').val(), 1);

            var options = {
                url: baseUrl + "AppCommon.ashx?method=langchange&x=" + (new Date()).getTime(),
                type: "POST",
                data: { lang: $(obj).val() },
                success: function (response, status) {
                    window.location.href = window.location.href;
                },
                error: function (error) {
                }
            }
            $.ajax(options);
            return false;
      }
    </script>
