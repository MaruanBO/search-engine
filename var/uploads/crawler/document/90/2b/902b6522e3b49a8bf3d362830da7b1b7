        function setHomeVideo() {
            var vidObj = { mp4VideoURL: 'https://d1w9kw5f4q541t.cloudfront.net/enroll.mp4', webmVideoURL: 'https://d1w9kw5f4q541t.cloudfront.net/enroll.webm', flashVideoURL: 'rtmp://s2g1y847ws4w06.cloudfront.net/cfx/st/mp4:enroll.mp4', imgURL: 'https://d1w9kw5f4q541t.cloudfront.net/enroll.jpg' }
            if (!playerInitialized) {
                configHomePlayer('homeVideoPanel', vidObj);
                playerInitialized = true;
            }
        }

        //Javascript Function
        function GetApiInfo() {
            this.ContactEmailId = "";
            this.FirstName = "";
            this.LastName = "";
            this.AuthId = "";
            this.ProfilePicture = "";
            this.Gender = "";
            this.DOB = "";
            this.AuthType = "";
            this.UserEmail = "";
            this.Password = "";
        }
        //End Javascript Function

        //Javascript Function
        function GetNativeLoginInfo() {
            this.UserEmail = "";
            this.Password = "";
            this.UserIp = "";
        }
        //End Javascript Function

        function GetGplusJs() {
            var po = document.createElement('script');
            po.type = 'text/javascript'; po.async = true;
            po.src = 'https://apis.google.com/js/client:plusone.js?onload=google_init' + "&x=" + (new Date()).getTime();
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(po, s);
        }
        function google_init() {
            g_obj1 = new GoogleLogin(google_client_id, google_scope,
      google_api_key);

            g_obj1.SignInButtonRender('google-btn');
            g_obj1.callback = 'onSignInCallback';

            g_obj2 = new GoogleLogin(google_client_id, google_scope,
    google_api_key);

            g_obj2.SignInButtonRender('google-login-btn');
            g_obj2.callback = 'onSignInCallback';
        }


        function onSignInCallback(authResult) {  //Calls on clicking Enroll button    
            CheckUserExist(authResult, '1'); //set authtype as g+
        }


        function CheckUserExist(authResult, authType) {
            $('body').css({ 'cursor': 'progress' });
            var getDataApi = new Array();
            var getInfo = new GetApiInfo();

            var Id;
            if (authType == '1') { //Gmail
                getInfo.FirstName = authResult.first_name;
                getInfo.LastName = authResult.last_name;
                getInfo.ProfilePicture = authResult.image;
                getInfo.AuthId = authResult.id;
                try { getInfo.DOB = authResult.birthday; } catch (e) { }
                try { getInfo.Gender = authResult.gender; } catch (e) { }
                getInfo.AuthType = authType;
                getInfo.ContactEmailId = authResult.email;
                CallAjaxIsExists(getInfo);

            } else { //i.e FB called from facebookjs.js  
                getInfo.FirstName = authResult[0].first_name;
                getInfo.LastName = authResult[0].last_name;
                getInfo.ContactEmailId = authResult[0].contact_email;
                getInfo.AuthId = authResult[0].uid;
                getInfo.ProfilePicture = authResult[0].pic_big;
                //getInfo.current_location = authResult[0].current_location;
                getInfo.DOB = authResult[0].birthday;
                getInfo.Gender = authResult[0].sex;
                getInfo.AuthType = authType;
                // getDataApi.push(getInfo);  //as we desearialize it on server side we need an array to push
                CallAjaxIsExists(getInfo);

            }
        }

        function CallAjaxIsExists(getInfo) {
            $("#spnUserNotExist").hide();
            if (getInfo.AuthType == "") {

                if ($.trim($("#txtUserEmailAddress").val()) != "" && $.trim($("#txtUserPassword").val()) != "" && validateEmail($.trim($("#txtUserEmailAddress").val()))) {
                    getInfo.UserEmail = $("#txtUserEmailAddress").val();
                    getInfo.Password = $("#txtUserPassword").val();
                }
                else {
                    $("#txtUserEmailAddress").closest('li').removeClass('has-error');
                    $("#txtUserPassword").closest('li').removeClass('has-error');

                    if ($.trim($("#txtUserEmailAddress").val()) == "") {
                        $("#txtUserEmailAddress").closest('li').addClass('has-error');
                    } else {
                        if (validateEmail($.trim($("#txtUserEmailAddress").val()))) {
                            $("#txtUserEmailAddress").closest('li').removeClass('has-error');

                        } else {
                            $("#spnUserNotExist").text('Please enter a valid email.');
                            $("#spnUserNotExist").show();
                            $("#txtUserEmailAddress").closest('li').addClass('has-error');
                        }
                    }
                    if ($.trim($("#txtUserPassword").val()) == "") {
                        $("#txtUserPassword").closest('li').addClass('has-error');
                    }
                    else {
                        $("#txtUserPassword").closest('li').removeClass('has-error');
                    }
                    $('body').css({ 'cursor': 'default' });
                    $('body a').css({ 'cursor': 'pointer' });
                    return false;
                }

            }

            var options =
                {
                    url: baseUrl + "General/Index.aspx?method=CheckUserExists&x=" + (new Date()).getTime(),
                    data: { authdata: JSON.stringify(getInfo) },
                    dataType: "text",   //json
                    type: "POST",
                    success: function (response, status) {
                        //response set on index.aspx.cs page

                        if (response == 1) {
                            //Start To erase cookie if it is set on choose-role page 
                            eraseCookie(cookieName, "", -1);
                            //End To erase cookie if it is set on choose-role page 
                            // alert(getCookie('CookieRoleIDs'));
                            if (window.location.href.toLowerCase() != (baseUrl + 'Choose-Role').toLowerCase()) {
                                window.location.href = baseUrl + 'Choose-Role';
                            } else {
                                $('#btnHClose1').trigger('click');
                                $('#btnHClose2').trigger('click');
                                if (window.haveData)
                                    haveData = 'y';
                                if (window.clickChk) {
                                    if (clickChk == "user")
                                        window.location.herf = baseUrl + "Complete-Profile";
                                    else { window.location.herf = baseUrl + "ClaimOrganization"; }
                                }
                            }
                            $('body').css({ 'cursor': 'default' });
                            $('body a').css({ 'cursor': 'pointer' });
                        }
                        else if (response == 2) {
                            var cUrl = window.location.href.toLowerCase();
                            if (cUrl.indexOf('returnurl=') > -1) {
                                cUrl = cUrl.substring(cUrl.indexOf('returnurl=')).replace('returnurl=', '');
                                window.location.href = baseUrl + cUrl.replace('%2f', '');
                            }
                            else if (window.returnUrl) {
                                window.location.href = baseUrl + window.returnUrl;
                            }
                            else {
                                if (window.location.hash) {
                                    window.location.href = window.location.href.replace('#' + window.location.hash.substring(1), '');
                                } else { window.location.href = window.location.href; }
                            }

                        }
                        else if (response == 0) {
                            window.location.href = baseUrl + 'ClaimConfirmation';
                            //window.location.href = baseUrl + 'OrganizationLander';
                        }
                        else if (response == 3) {
                            $("#spnUserNotExist").show();
                            $("#spnUserNotExist").closest('li').addClass('error-msg');
                            $("#txtUserPassword").val('');
                            $("#spnUserNotExist").text('Email or Password is incorrect.')

                            $('body').css({ 'cursor': 'default' });
                            $('body a').css({ 'cursor': 'pointer' });
                            //window.location.href = baseUrl + 'OrganizationLander';
                        }
                        else {
                            window.location.href = 'App-Error';
                        }
                    }
                }
                $.ajax(options);
            }
            function CheckAlreadyExistsEmailID(sender, id) {

                if ($.trim($(sender).val()) != "") {
                    if (validateEmail($.trim($(sender).val()))) {
                        if (id == 2) {

                            $("#smlforgot").hide();

                        }
                        $("#txtUserEmailAddress").closest('li').removeClass('has-error');
                        $("#spnUserNotExist").hide();
                        return true;
                    }
                    else {
                        $("#spnUserNotExist").text('Please enter a valid email.');
                        $("#spnUserNotExist").show();
                        $("#txtUserEmailAddress").closest('li').addClass('has-error');
                        if (id == 2) {
                            $("#smlforgot").text('Please enter a valid email.');
                            $("#smlforgot").show();

                        }
                        return false;

                    }


                }
                return false;
            }
            function validateEmail(email) {
                var reg = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/
                if (reg.test(email)) {
                    return true;
                }
                else {
                    return false;
                }
            }
            function fnForgotPassword() {

                if ($.trim($("#txtForgotPassword").val()) != "" && validateEmail($.trim($("#txtForgotPassword").val()))) {
                    $("#txtForgotPassword").closest('li').removeClass('has-error');
                    $.ajax({
                        url: baseUrl + "General/Index.aspx?method=ForgotPassword&x=" + (new Date()).getTime(),
                        data: { ForgotPassword: $.trim($("#txtForgotPassword").val()) },
                        type: "POST",
                        success: function (response, status) {
                            if (response == "success") {

                                $('#ForgotPassword').modal('hide');
                                ShowAlertMessage("Reset password link has been sent to " + $.trim($("#txtForgotPassword").val()));
                                $("#txtForgotPassword").val('');
                            } else {
                                $("#smlforgot").text(response);
                                $("#smlforgot").show();
                            }
                            //response set on index.aspx.cs page
                            // $('#dvMsgPopup').modal();
                        },
                        error: function (error, ajaxOptions, thrownError) {
                            $("#smlforgot").text(error.responseText);
                            $("#smlforgot").show();
                        }
                    });

                } else {

                    $("#txtForgotPassword").closest('li').addClass('has-error');

                    return false;
                }
            }

            function SysLogout() {
                var options = {
                    url: baseUrl + "AppCommon.ashx?method=Logout&x=" + (new Date()).getTime(),
                    type: "POST",
                    success: function (response, status) {
                        if (response.toLowerCase() == "success") {
                            if (window.FacebookLogout) {
                                FacebookLogout();
                            }

                            setCookie("Save_SchoolSearchParameters", "", 1);
                            setCookie("Save_SearchParameters", "", 1);

                            var newUrl = baseUrl.replace("https", "http");
                            $(location).attr('href', newUrl);
                            return false;
                        }
                    },
                    error: function (error) {
                        var newUrl = baseUrl.replace("https", "http")
                        $(location).attr('href', newUrl);
                        return false;
                    }
                }
                $.ajax(options);
                return false;
            }


            //$('.offline').click(function () {
            //    $(this).toggleClass('online');
            //});

            function TutorOnline(Sender) {
                var online = "";
                if ($(Sender).hasClass("online")) {
                    online = 0;
                    $(Sender).removeClass('online');
                    RemoveTimer();
                } else {
                    $(Sender).addClass('online');
                    online = 1;
                    setCookieM("CommitTimes", new Date().getTime(), 1);
                    CommitLogoutTimer();
                }
                SetResetTutor(online, false);
            }

            function CheckNegativeBalance() {

                var options = {
                    url: baseUrl + "AppCommon.ashx?method=checknegativebalance&x=" + (new Date()).getTime(),
                    type: "POST",
                    success: function (response) {

                        var UserData = $.parseJSON(response);
                        if (UserData.BalanceAmount < 0) {

                            timeOutms = 0;
                            $('.dismiss-btns').find('a').html('Dismiss').attr('onclick', '$(\'.alert-notification-wrap\').slideUp();');
                            ShowAlertMessage('You can not close your account since you have negative balance.');
                            return false;

                        } else {
                            //alert(13);
                            loadContent(baseUrl + "CloseAccount", true);
                        }
                    },
                    error: function (error) {
                        alert(error);
                    }
                }
                $.ajax(options);
                return false;
            }
            function SetResetTutor(online, dopostback) {

                var options = {
                    url: baseUrl + "AppCommon.ashx?method=tutoronline&x=" + (new Date()).getTime(),
                    type: "POST",
                    data: { SetReset: online },
                    success: function (response, status) {

                    },
                    error: function (error) {
                    }
                }
                $.ajax(options);
            }

            function GlobalSearchParameterBO() {
                this.GlobalSearchKey;
            }
            function MakeGlobalSearch() {
                if ($('#globalTxt').val().length == 0) return false;
                var GSPBO = new GlobalSearchParameterBO();
                GSPBO.GlobalSearchKey = $('#globalTxt').val();
                var dataToSend = { SearchParam: JSON.stringify(GSPBO) }
                //alert(JSON.stringify(SSPBO));
                $.ajax({
                    type: "POST",
                    url: baseUrl + 'ScholarshipSearch?method=makeglobalsearch',
                    data: dataToSend,
                    success: function (response) {
                        $('#global_search').show();
                        var SearchData = $.parseJSON(response);

                        if (SearchData.Scholarships.length == 0 && SearchData.Courses.length == 0 && SearchData.Schools.length == 0 && SearchData.Tutors.length == 0) {
                            $('#global_search').children('.wrapp').hide();
                            $('#global_search_no_results').show();
                        }
                        else {

                            $('#global_search').children('.wrapp').show();
                            $('#global_search_no_results').hide();

                            if (SearchData.Scholarships.length > 0) {
                                $('#globalScholarships').parent('.wrapp').show();
                                $('#globalScholarships').html("");
                                var template = Handlebars.getTemplate("GlobalScholarshipSearch");
                                //alert(template);
                                var html = template({ GlobalScholarshipSearch: SearchData.Scholarships });
                                //alert(html);
                                $('#globalScholarships').append(html);
                            }
                            else {
                                $('#globalScholarships').html('');
                                $('#globalScholarships').parent('.wrapp').hide();
                            }

                            if (SearchData.Courses.length > 0) {
                                $('#globalCourses').parent('.wrapp').show();
                                $('#globalCourses').html("");
                                template = Handlebars.getTemplate("GlobalCourseSearch");
                                //alert(template);
                                html = template({ GlobalCourseSearch: SearchData.Courses });
                                //alert(html);
                                $('#globalCourses').append(html);
                            }
                            else {
                                $('#globalCourses').html('');
                                $('#globalCourses').parent('.wrapp').hide();
                            }

                            if (SearchData.Schools.length > 0) {
                                $('#globalSchools').parent('.wrapp').show();
                                $('#globalSchools').html("");
                                template = Handlebars.getTemplate("GlobalSchoolSearch");
                                //alert(template);
                                html = template({ GlobalSchoolSearch: SearchData.Schools });
                                //alert(html);
                                $('#globalSchools').append(html);
                            }
                            else {
                                $('#globalSchools').html('');
                                $('#globalSchools').parent('.wrapp').hide();
                            }

                            if (SearchData.Tutors.length > 0) {
                                $('#globalTutors').parent('.wrapp').show();
                                $('#globalTutors').html("");
                                template = Handlebars.getTemplate("GlobalTutorSearch");
                                //alert(template);
                                html = template({ GlobalTutorSearch: SearchData.Tutors });
                                //alert(html);
                                $('#globalTutors').append(html);
                            }
                            else {
                                $('#globalTutors').html('');
                                $('#globalTutors').parent('.wrapp').hide();
                            }
                        }
                        $('#global_search .profile-pic img').each(
                            function () {
                                setImageProportion(this);
                            }
                            );
                        //$('#dvLoader').hide();
                        //$('').hide();
                        $.fn.raty.defaults.path = 'img/stars';
                        $('.overall-rating').raty({
                            half: true,
                            readOnly: true,
                            score: 0,
                            starHalf: 'overall-star-half.png',
                            starOff: 'overall-star-off.png',
                            starOn: 'overall-star-on.png'
                        });

                    },
                    error: function (error) {
                        alert('SEARCH - ERROR!');
                    }
                });
            }

            function gSearchPress(e) {
                if (e.keyCode == 13 || e.which == 13) {
                    MakeGlobalSearch();
                }
            }
</script>


<script type="text/javascript">
    var CommitTimesToWarning = 15 * 60, CommitTimesToLogout = 20 * 60, LogTimer = 0, CheckInterval = 1000;
    setCookieM("CommitTimes", new Date().getTime(), 1);

    function DoUnvavailable() {
        $('#divWarning .icon-popClose').click();

        $('#aTutorStatus').removeClass('online');
        SetResetTutor(0, true);
        RemoveTimer();
    }

    function ShowWarning() {
        $('#divWarning').modal();
    }

    function getCookieM(c_name) {
        var i, x, y, ARRcookies = document.cookie.split(";");
        for (i = 0; i < ARRcookies.length; i++) {
            x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
            y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
            x = x.replace(/^\s+|\s+$/g, "");
            if (x == c_name) {
                return unescape(y);
            }
        }
    }
    var exdate = new Date();
    var c_value = '';

    function setCookieM(c_name, value, exdays) {
        exdate = new Date();
        exdate.setDate(exdate.getDate() + exdays);
        c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString() + "; path=/");
        document.cookie = c_name + "=" + c_value;
    }

    function CommitLogoutTimer() {
        if ($('.offline').hasClass('online')) {
            document.onkeypress = new Function("KeepAlive();");
            document.oncontextmenu = new Function("KeepAlive();");
            document.onmousemove = new Function("KeepAlive();");
            var CommitTimes = getCookieM("CommitTimes");
            var currentTime1 = new Date();
            if (CommitTimes != null || CommitTimes != "") {
                var diff = parseInt((currentTime1.getTime() - CommitTimes) / 1000);
                //  console.log('commit');
                //console.log(CommitTimes);
                // console.log(currentTime1.getTime());
                try { clearTimeout(LogTimer); } catch (Ex) { }
                if (diff >= CommitTimesToWarning && diff < CommitTimesToLogout) {
                    if (diff == CommitTimesToWarning)
                        ShowWarning();
                }
                else if (diff >= CommitTimesToLogout) {
                    DoUnvavailable();
                }
            }
            LogTimer = setTimeout("CommitLogoutTimer()", CheckInterval);
        }
    }

    function KeepAlive() {
        if ($('#divWarning').is(':visible'))
        { }
        else
        {
            setCookieM("CommitTimes", new Date().getTime(), 1);
            CommitLogoutTimer();
        }
    }

    function KeepAliveButton() {
        setCookieM("CommitTimes", new Date().getTime(), 1);
        CommitLogoutTimer();
    }




    if ($('.offline').hasClass('online')) {
        CommitLogoutTimer();
    }


    function DoNothing() {

    }

    function RemoveTimer() {
        window.clearInterval(LogTimer)

        $(document).unbind("onkeypres", KeepAlive);
        $(document).unbind("oncontextmenu", KeepAlive);
        $(document).unbind("onmousemove", KeepAlive);




    }
    function RemoveSchoolSearchCookies() {

        setCookie("Save_SchoolSearchParameters", "", 1);
        setCookie("Save_IsCliked", "0", 1);
        setCookie("PageSize_SchoolSearch", "", 1);
        setCookie("Save_ScrollPositionSchoolSearch", "", 1);
    }
    function RemoveScholarshipSearchCookies() {

        setCookie("Save_ScholarshipSearchParameters", "", 1);
        setCookie("PageSize_ScholarshipSearch", "", 1);
        setCookie("Save_ScrollPositionScholarshipSearch", "", 1);
    }
    

    function ClearCookieForRole() {
        var cookieName = "CookieRoleIDs";
        setCookie(cookieName, "1", 1);
    }
</script>



