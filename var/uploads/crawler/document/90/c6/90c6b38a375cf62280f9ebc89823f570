<script>
    $(document).ready(function(){
        $("#lead-capture #lead-submit").live("click", function() {
            $.ajax({
                url: "/register/emaillead",
                type: "post",
                dataType: "json",
                data: "register[email]=" + $("#lead-email").val() + "&register[zipCode]=" + $("#lead-zip").val() + "&placement=homepage",
                success: function(errors) {
                    $('#lead-capture input[type="text"]').removeClass('error-border');
                    if (errors.length == 0) {
                        $('#lead-capture').html('Thank you! You will now begin receiving job alerts.').css({
                            "font-size": "larger",
                            "font-weight": "bolder"
                        });
                    }else{
                        for (index = 0; index < errors.length; ++index) {
                            if (errors[index].indexOf('email') != -1){
                                $('#lead-email').addClass('error-border');
                            }
                            if (errors[index].indexOf('code') != -1){
                                $('#lead-zip').addClass('error-border');
                            }
                        }
                    }
                }
            });
            return false;
        });
    });
</script>
