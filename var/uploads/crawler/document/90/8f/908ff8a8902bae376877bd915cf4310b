    <script>

        var minimizedHeaderOpen = false;
        var headerHeight;

        $(function() {

            
            mmodal.enable_anchors("a.mmodal");

            utils.is_logged_in = false;
            utils.log_user_in();
            utils.total_items_in_cart = 0;

            //--- Initialize YourCart
            yourCart.init();

            //--- Initialize login/register modal
            login_register.init();

            headerHeight = $("header").height() - $("#categories").height();

            $(window).bind('scroll', function() {

                if ($(document).scrollTop() >= headerHeight) {

                    if (minimizedHeaderOpen == false) {
                        minimizedHeaderOpen = true;
                        $("header").addClass('minimized');
                        $("#urgencyDiv").addClass('minimized');
                    }

                } else {

                    if (minimizedHeaderOpen == true) {
                        minimizedHeaderOpen = false;
                        $("header").removeClass('minimized');
                        $("#urgencyDiv").removeClass('minimized');
                    }

                }

            });

            //--- Highlight search on field focus
            $(".share_search .field").focus(function() {
                if (!$(".share_search .search").hasClass('focused')) {
                    $(".share_search .search").addClass('focused');
                }
            }).blur(function() {
                if ($(".share_search .field").val() == '') {
                    $(".share_search .search").removeClass('focused');
                }
            });

        });

    </script>

