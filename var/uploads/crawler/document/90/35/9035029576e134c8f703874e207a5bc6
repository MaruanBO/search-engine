<script type="text/javascript">
	function setCookie(c_name,value,expiredays){
		var exdate=new Date();exdate.setDate(exdate.getDate()+expiredays);
		document.cookie=c_name+ "=" +escape(value)+ ";path=/"+
		((expiredays==null) ? "" : ";expires="+exdate.toGMTString());
	}
	function setLocaleCookie(c_name,value1,value2,expiredays){
		if(c_name == 'currentStoreId'){
			setCookie('currentStoreId',value1,expiredays);
			setCookie('currentLangId',value2,expiredays);
			setCookie('currentLegacyStoreURL','',-1);
		}else{
			setCookie('currentLegacyStoreURL',value1,expiredays);
			setCookie('currentStoreId','',-1);
			setCookie('currentLangId','',-1);
		}
	}
</script>

