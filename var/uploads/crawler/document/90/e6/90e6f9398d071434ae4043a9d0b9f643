<script>
var siteDebug = "";
$(function() {
    DEFAULT_IMAGE = "src=\"http://local.dailybreeze.com/assets/images/defaultBlog.png\"";
    var blog_links = [];
    var error_blogs = []; 
    //feed_url format: "feed link, logo link | feed link, logo link | feed link | feed link"   
    //TAXONOMYSearch="Frontpage - Section Configuration Use Only"     
    FEED_URL = "http://blogs.dailybreeze.com/prepsports/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=458&Element=1|http://blogs.dailybreeze.com/southbay/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=456&Element=1|http://blogs.dailybreeze.com/history/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=459&Element=1|http://insidesocal.com/beer/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=447&Element=1|http://blogs.dailynews.com/politics/feed/,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20140317&ID=722&Element=1|http://www.insidesocal.com/soccer/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=434&Element=1|http://www.insidesocal.com/lakers/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=432&Element=1|http://www.insidesocal.com/USC/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=429&Element=1|http://www.insidesocal.com/UCLA/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=430&Element=1|";
    ALT_URL = "http://blogs.dailybreeze.com/prepsports/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=458&Element=1|http://blogs.dailybreeze.com/southbay/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=456&Element=1|http://blogs.dailybreeze.com/history/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=459&Element=1|http://insidesocal.com/beer/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=447&Element=1|http://blogs.dailynews.com/politics/feed/,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20140317&ID=722&Element=1|http://www.insidesocal.com/soccer/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=434&Element=1|http://www.insidesocal.com/lakers/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=432&Element=1|http://www.insidesocal.com/USC/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=429&Element=1|http://www.insidesocal.com/UCLA/feed,http://www.dailybreeze.com/cceimg?Site=LI&Module=2&Class=2&Type=1&Date=20130701&ID=430&Element=1|";
    if(FEED_URL == 'NOREC') FEED_URL = ALT_URL;
  //if no blogs, don't display the rotator and no further processing.
    var mediacentercheck = document.getElementById('mc-widget-container-large');
    if(FEED_URL == 'NOREC') {
        $('#blog-rotator-title').hide();    
        $('#blog-widget-container-large').hide();
		if (mediacentercheck == null) {
			$('.blogCheck').hide();
		}
        return;
    }
    // site debug information
    if(siteDebug) {
        console.log("Feed URLs: ");console.log(FEED_URL);
    }
    // blog set is separated by "|", feed link and logo link is separated by comma;
    var blog_urls = FEED_URL.split("|");
    //max blog url is 10
    max_url = (blog_urls.length <= 9) ? blog_urls.length : 9;    
    var logo_url = "";
    var feed_url = "";
    var promises=[];
    //for each blog feed url, grab the first post
    for (var i = 0; i < max_url; i++) {        
        (function(key, logo_url) {                   
            feed_set = blog_urls[key].split(",");
            // site debug information
                if(siteDebug) {
                    console.log("Feed set: ");console.log(feed_set);
                } 
            feed_url = feed_set[0];
            //no processing further for empty urls, empty urls are byproduct of "split" function
            if(feed_url.length < 1) return;
            if(typeof feed_set[1] != 'undefined') { 
                logo_url = 'src="'+feed_set[1]+'"'; 
                //need to insert 'apps/pbcsi.dll' before 'cceimg
                logo_url = logo_url.replace("/cceimg","/apps/pbcsi.dll/cceimg");
            } 
            var request = $.ajax({
                url: document.location.protocol + '//ajax.googleapis.com/ajax/services/feed/load?v=1.0&num=2&callback=?&q=' + encodeURIComponent(feed_url),
                dataType: 'json',                    
                success: function(data) {
                    // log error blog links
                    if(data.responseStatus != 200 && feed_url.length > 1) {
                        error_blogs[key] = [];
                        error_blogs[key]['blog_link'] = feed_url;                        
                        error_blogs[key]['reason'] = 'Response Status: ' + data.responseStatus;   
                    }
                    if (data.responseData) {var post = data.responseData.feed.entries[0];} else {
                        error_blogs[key] = [];
                        error_blogs[key]['blog_link'] = feed_url;   
                        error_blogs[key]['reason'] = 'Empty Response';   
                        return;
                    }     
                    //post  image
                    if(typeof post != 'undefined') { 
                        var imgArray = post.content.match( /src=('|")(.+?\.(jpg)(|\?.+?))('|")/i );    
                    }
                    if(post) {
                        blog_links[key] = [];  
                        blog_links[key]['link'] = [];
                        blog_links[key]['title'] = [];
                        blog_links[key]['image'] = [];
                        blog_links[key]['link'].push(post.link);
                        blog_links[key]['title'].push(post.title);
                        if(imgArray) {
                            blog_links[key]['image'].push(imgArray[0]);
                        } else {
                            if(logo_url) {
                                blog_links[key]['image'].push(logo_url);
                            } else {
                                blog_links[key]['image'].push(DEFAULT_IMAGE);
                            }
                        }              
                    } else {
                        error_blogs[key] = [];
                        error_blogs[key]['blog_link'] = data.responseData.feed.feedUrl;   
                        error_blogs[key]['reason'] = 'Empty Post';   
                    }          
                }
            });
            promises.push(request);
        }(i, logo_url));
    }
    //once all ajax calls are complete, process further
    $.when.apply(null, promises).done(function() {
    //renumbering array elements, if any
    var i=0;
    var blog_links_new = new Array();
    for(key in blog_links) {
        if(typeof blog_links[key] != 'undefined') { 
            blog_links_new[i] = new Array();
            blog_links_new[i]['image'] = blog_links[key]['image'];
            blog_links_new[i]['link'] = blog_links[key]['link'];
            blog_links_new[i]['title'] = blog_links[key]['title'];
            i++;
        }
    }
    blog_links = blog_links_new;
    // site debug information
    if(siteDebug) {
        console.log("Blog Links: ");console.log(blog_links);
        console.log("Error Links: ");console.log(error_blogs);
    }    
        var output = '<li class="item"><div class="slide">';                
        for(key in blog_links) {            
            // if no post image, use default from cce objects..
            if(blog_links[key]['image'] != "") {
                IMAGE = blog_links[key]['image'][0];                        
            } else {
                IMAGE = DEFAULT_IMAGE;
            }                
            if ((key%3 === 0) && key != 0) { output += '</div></li><li class="item"><div class="slide">'; }
            output += '<div class="was-figure">';
            output += '	  <a href="' + blog_links[key]['link'] + '" title="' + blog_links[key]['title'] + '">';
            output += '      <div style="width:151px; "></div>';
            output += '      <img ' + IMAGE + ' alt="' + blog_links[key]['title'] + '" style="width:151px;" onError="this.onerror=null;this.src=\'http://local.dailybreeze.com/assets/images/defaultBlog.png\';">';
            output += '	  </a>';
            output += '   <h3>';
            output += '      <a href="' + blog_links[key]['link'] + '" title="' + blog_links[key]['title'] + '">' + blog_links[key]['title'] + '</a>';
            output += '   </h3>';
            output += '</div>';
           //console.log(blog_links[key]);            
        }
        output += '</div></li>';
        $('#blog-widget-items').html(output);
    })
});
</script>

                    </div>
                    
                </section>
