                case 'DFM-adPos-nav-8':       DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dnav_8&sz=300x250&c="+tagOrd+"'></iframe>"; break;
                case 'DFM-adPos-nav-9':       DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dnav_9&sz=300x250&c="+tagOrd+"'></iframe>"; break;
