/*====URL parsing==========================================*/
		
	function countPath(){
	
	s.server="nfo";
	s.contextData["pageType"]="nfo";
	s.pageName="nfo:" + s.getPageName();
		
	if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName ="nfo:home";
			var l1 = "nfo";	
			var l2 = "nfo:home";
			}
		else{
		var pos=s.pageName.indexOf('index.php');   /*for the pages ending with index.php*/
		if (pos>0) {s.pageName= s.pageName.substring(0, pos);}  
			
		var pos= s.pageName.indexOf(':');
		if (pos >= 0) {
			var l1= s.pageName.substring(0, pos);
			var parse2 = s.pageName.substring(pos + 1);
			
			var pos= parse2.indexOf(':');
			if (pos >= 0) {
				var l2 = "nfo:" + parse2.substring(0, pos);
				var parse3 = parse2.substring(pos+1);
				var l3=s.pageName;
				}
			else {l2=parse2;}		
			}
			
		}  /* end of else / if it's not a homepage*/
				
		s.contextData['l1']=l1;	
		s.contextData['l2']=l2;
		s.contextData['l3']=l3;
		var s_code=s.t();if(s_code)document.write(s_code)
	}
		
window.onload=setUp;
function setUp() {
	var tc = document.getElementById("conds");
	var hp = document.getElementById("aaSignOn");
	var al = document.getElementById("calc");
	if (tc == null && hp == null && al == null){
		countPath();
		}
		else if(tc !== null && hp == null && al == null){
			start();
			countPath();
		}
		else if(tc == null && hp !== null && al == null){
			setAACookie();
			showHeros();
			countPath();
		}
		else if(tc == null && hp == null && al !== null){
			setSliders();
			countPath();
		}
}	
/*====Downloads==========================================*/
		
	function countDownload(fileName){
		
		s.linkTrackVars='events,eVar6';
		s.linkTrackEvents='event4';
		s.eVar6=s.prop6=fileName;
		s.trackInlineStats='true';
		s.tl(this, 'd', fileName,null,'navigate');
		return false;
		
	}

/*====Tabs==========================================*/
	
	function countTab(tabName){
		s.pageName="nfo:" + s.getPageName();
		if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName="nfo:home:" + tabName;
		}
		else {
			s.pageName="nfo:" + s.getPageName() + ":" + tabName;
		}
		s.t();
	}

/*====Modals==========================================*/
		
	function countModal(modalName){
		s.pageName="nfo:" + s.getPageName();
		if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName="nfo:home:" + modalName;
		}
		else {
			s.pageName="nfo:" + s.getPageName() + ":" + modalName;
		}
		s.t();
	}
	
/*====Buttons==========================================*/
		
	function countButton(buttonName){
		s.pageName="nfo:" + s.getPageName();
		if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName="nfo:home:" + buttonName;
		}
		else {
			s.pageName="nfo:" + s.getPageName() + ":" + buttonName;
		}
		s.linkTrackEvents="None";
		s.linkTrackVars="None";
		s.events = "";
		s.t();
	}

/*====Views (tile, list, compare)==========================================*/	
	function countLink(linkName){
		s.pageName="nfo:" + s.getPageName();
		if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName="nfo:home:" + linkName;
		}
		else {
			s.pageName="nfo:" + s.getPageName() + ":" + linkName;
		}
		s.events = "";
		s.t();
	}
	
	function countView(viewName){
		s.pageName="nfo:" + s.getPageName();
		if ((s.pageName == "nfo:index.php")||(s.pageName == "nfo:")) {
			s.pageName="nfo:home:" + viewName;
		}
		else {
			s.pageName="nfo:" + s.getPageName() + ":" + viewName;
		}
		s.events = "";
		s.t();
	}
