function createWindow(cUrl,cName,cFeatures) {
	var features = cFeatures.split(',');
	var winWidth = features[0];
	var winWidth = winWidth.length;
	var winHeight = features[1];
	var winHeight = winHeight.length;
	if(winWidth == 9 && winHeight == 10){
		var w = cFeatures.substring(6,9);
		var h = cFeatures.substring(17,20);
	}
	else if (winWidth == 10 && winHeight == 10){
		var w = cFeatures.substring(6,10);
		var h = cFeatures.substring(18,21);
	}
	else if (winWidth == 10 && winHeight == 11){
		var w = cFeatures.substring(6,10);
		var h = cFeatures.substring(18,22);
	}
	else if (winWidth == 9 && winHeight == 11){
		var w = cFeatures.substring(6,9);
		var h = cFeatures.substring(17,21);
	}
	var l1 = ",left=";
	var t1 = ",top=";
	var l2 = (screen.width/2)-(w/2);
  var t2 = (screen.height/2)-(h/2);
	var display = cFeatures.concat(l1,l2,t1,t2);
  //var xWin = window.open(cUrl, cName, 'toolbar=no, status=no, menubar=no, scrollbars=no, resizable=no, width='+w+', height='+h+', top='+top+', left='+left);
	var xWin = window.open(cUrl, cName, display);
}
