    <script type="text/javascript" src="/info/presto.cookie-handler.js"></script>
    <script>
    var dateAux = new Date();
    var dayVar = dateAux.getDate();
    var dayString = '' + dayVar;
    if(dayVar < 10){
    	dayString = '0' + dayString;
    }
    var monthVar = dateAux.getMonth() + 1;
    var monthString = ''+ monthVar;
    if(monthVar < 10){
    	monthString = '0' + monthString;
    }
    var hourVar = dateAux.getHours();
    var hourString = '' + hourVar;
    if(hourVar < 10){
    	hourString = '0' + hourString;
    }
    var minuteVar = dateAux.getMinutes();
    var minuteString = '' + minuteVar;
    if(minuteVar < 10){
    	minuteString = '0' + minuteString;
    }
    var secondVar = dateAux.getSeconds();
    var secondString = '' + secondVar;
    if(secondVar < 10){
    	secondString = '0' + secondString;
    }
    var dateAuxString = dateAux.getFullYear() + '-' + monthString + '-' + dayString + ' ' + hourString + ':' + minuteString + ':' + secondString + '.' + dateAux.getMilliseconds(); 
    
    $(window).load(function() {
		var splash = $('#splash-overlay1').clone(),
			cookieHandler = new jQuery.PRESTO.CookieHandler(),
			cookieName = "sp_uj43zo3ui326hydj",
			cookieNameLastUpdate = "splu_uj43zo3ui326hydj",
			lastUpdateCookie,
			lastUpdatePage,
			backgroundColorAux = "#045596",
			opacityAux = "0.75",
			fontLinksColorAux = "#FFFFFF",
			overlayApi = null,
			canBeFixed = false,
			splashHeight = $('#splash-overlay1').outerHeight(),
			splashImageWidth = 0,
			splashVideoWidth = 0;
		
		//calculating image and video width
		if ($('#splash-overlay1').find('.splash-image').length && $('#splash-overlay1').find('.splash-image').outerWidth()) {
			splashImageWidth = $('#splash-overlay1').find('.splash-image').outerWidth();
		} else if ($('#splash-overlay1').find('.splash-video').length && $('#splash-overlay1').find('.splash-video').outerWidth()) {
			splashVideoWidth = $('#splash-overlay1').find('.splash-video').outerWidth();
		}
			
		//moving splash to the body root
		$('#splash-overlay1').remove();
		splash.prependTo('body');
		
		//checking if we can make overlay fixed
		if ($(window).height() >= splashHeight) {
			canBeFixed = true;
		}
		
		//setting splash width
		if (splashImageWidth) {
			splash.width(splashImageWidth);
		} else if (splashVideoWidth) {
			splash.width(splashVideoWidth);
		}
		
  		if (cookieHandler.readCookie(cookieNameLastUpdate)) {
			lastUpdateCookie = cookieHandler.readCookie(cookieNameLastUpdate);
			lastUpdatePage = "2015-01-29 23:34:06.006";
			if(lastUpdateCookie < lastUpdatePage){
				cookieHandler.eraseCookie(cookieName);
				cookieHandler.eraseCookie(cookieNameLastUpdate);
			}
    	}	
	
		//positioning video
				
		splash.addClass('none').removeClass('splash-overlay-none');
		
					if (!cookieHandler.readCookie(cookieName)) {
		    	
		setTimeout(function() {
			overlayApi = splash.overlay({
				mask: {
					color: "#045596",
					opacity: 0.75
				},
				closeOnClick: false,
				load: true,
				speed: 1000,
				top: 'center',
				fixed: canBeFixed
			}).data('overlay');
		}, 1000);
		
		splash.find('.splash-close').click(function(){
			overlayApi.close();
			return false
		})
		
		splash.find('.perma-bypass').click(function(e) {
			e.preventDefault();
			overlayApi.close();
			cookieHandler.createCookie(cookieName, 'p', 365);
			cookieHandler.createCookie(cookieNameLastUpdate, dateAuxString , 365);
		}); 
		
					}
			});
    </script>
    
    	
