<script>
$(function() {

setTimeout(function() {

var aotwSlider = $('#aotw-slider').eq(0);
var scroller = aotwSlider.children('.slider').scrollable({ circular: false, touch:false }).data('scrollable');

var activeIndex = aotwSlider.find('.item.active').index();
if (activeIndex >= 0) {
  scroller.seekTo(activeIndex, 0);
}

aotwSlider.children('.pp').on('click', function(e) {
  e.preventDefault();

  if ($(this).hasClass('pause')) {

    $(this).removeClass('pause');
    $(this).addClass('play');
    scroller.stop();

  } else if ($(this).hasClass("play")) {

    $(this).removeClass('play');
    $(this).addClass('pause');
    scroller.play();

  }
});

}, 200);

});
</script>

 

</div> 


<script>
$(function() {

setTimeout(function() {

var switcher = $('#aotw-switcher > .switcher').eq(0);
var scroller = $('#aotw-switcher .aotw-slider > .slider').data('scrollable');

if (scroller) {

var itemsWrapper = scroller.getItemWrap();
 
switcher.on("click", function() {
  $(this).toggleClass('active');
  return false;
});

switcher.on('click', 'a[data-fetch-url]', function(e) {
  e.preventDefault();
  var el = $(this);

  if (!el.hasClass("active")) {
    var url = $(this).attr("href");
    if (url.length > 1) {

      itemsWrapper.fadeTo(400, 0.3);

      $.get(url, {"tmpl": "/aotw-slider-template", "ajax": true}, function(data) {

        var html = $('<div />').html(data);
          
        var newItems = html.find('.item');
        scroller.seekTo(0, 0).getItems().remove();
        itemsWrapper.append(newItems);

        var activeIndex = newItems.filter('.active').index();
        if (activeIndex >= 0) {
          scroller.seekTo(activeIndex, 0);
        }

       itemsWrapper.fadeTo(400, 1);

        switcher.find('a[data-fetch-url].active').removeClass('active');
        el.addClass('active');

        switcher.children('.selection').html(el.html());

      });     }
  }
});

} 
}, 300);

});
</script>

