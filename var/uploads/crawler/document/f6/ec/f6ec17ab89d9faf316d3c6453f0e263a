<script>
$(function() {
	$('#video-carousel').presto_Carousel(660, 400, {
		auto: true,
		animation: 3, 
		interval: 5000, 
		animationSpeed: 400,
		control: '.control'
	});
		
		
  $('#video-carousel .player-wrapper').hover(function() {
  	$(this).children('.close-player').show();
  }, function() {
  	$(this).children('.close-player').hide();
  });
	
$('#video-carousel .close-player').click(function(e) {
  e.preventDefault();
  jwplayer('video-carousel-player').stop();
  $('#video-carousel .player-wrapper').hide();
  $('.player-wrapper > div', '#video-carousel').remove();
  $('.player-wrapper', '#video-carousel').prepend('<div id="video-carousel-player" />');
  $('#video-carousel').getPRESTO_Carousel().play();
});
		
$('#video-carousel .play-video').click(function(e) {
	jwplayer('video-carousel-player').setup({
  		width: $('#video-carousel').outerWidth(),
  		height: $('#video-carousel').outerHeight(),
		file: $(this).attr('href'),
		provider: $(this).data('provider'),
		modes: [
			{ type: "flash", src: "/info/mediaplayer.swf" },
			{ type: "html5" }
		],
		autostart: true
  	});
  	e.preventDefault();
  	$('#video-carousel').getPRESTO_Carousel().stop();
	$('#video-carousel .player-wrapper').show();
}).hover(function() {
	$(this).css({'background-position': '0 -42px'});
}, function() {
	$(this).css({'background-position': '0 0'});
});
		

});
</script>

