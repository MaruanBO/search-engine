<script>
$(function() {

var api;

api = $('#main-tabs > .tabs').tabs($('#main-tabs > .panes > .pane'), {
    current: 'active',
    initialIndex: 0,
    onBeforeClick: function(e, i) {
        $('#main-tabs > .tabs').removeClass('state' + (this.getIndex() + 1)).addClass('state' + (i + 1));
        $('#main-tabs > .panes > .tab_title').removeClass('active').eq(i).addClass('active');
    },
	onClick: function() {
		$(document).trigger("ui_updated");
	}
}).data('tabConfig', { 'initialIndex': 0 }).data('tabs');


var tabTitles = $('#main-tabs > .panes > .tab_title');
tabTitles.click(function(e) {
  e.preventDefault();
  if (typeof api !== 'undefined') {
    api.click(tabTitles.index(this));
  }
});


});
</script>

        





