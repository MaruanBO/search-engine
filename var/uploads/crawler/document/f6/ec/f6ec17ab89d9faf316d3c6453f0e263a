<div class="none close">Close</div></nav>

<script>
(function($) {
  $(function() {

    $('#jump-to-nav').click(function(e) {
      e.preventDefault();
      $(this).toggleClass('active');
      $('#nav').toggleClass('active');
    });

    $('#nav .close').click(function(e) {
      $('#jump-to-nav').removeClass('active');
      $('#nav').removeClass('active');
      return false;
    });

        $('.has-submenu > a', '#nav').on('click', function() {
      $(this).parent().toggleClass('active');
      return false;
    });
  
    $(document).on('click', function(e) {
      $('.has-submenu.active').removeClass('active');
    });

  });
})(jQuery);
</script>

 



