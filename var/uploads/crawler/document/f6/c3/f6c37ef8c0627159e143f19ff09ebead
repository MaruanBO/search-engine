function setAgentToOffline() {
    jQuery('#livechat').addClass('offline')
    jQuery('#livechat .livechat-text')
      .html("Sorry, we&#039;re offline <br/>" +
        "Please leave a message");
    jQuery('#livechat .chat-with-us .chat-bubble .bubble-text').html('HELP');
    jQuery('#livechat #agentpic').css('background-image','url("//snapengage.com/wp-content/themes/snapengage_car-parent/img/agent/offline-message-icon-2.png")');

    jQuery('#text-to-chat-headline').hide();
    jQuery('#text-to-chat-number').hide();
}

