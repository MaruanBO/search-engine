<script type="text/javascript">
var jQ = jQuery.noConflict();

/* BEGIN PREFERRED LANGUAGE REDIRECTION SCRIPT */
function LanguageObject () {
	// Establish vars (alphabetical)
	//this.ajaxURL;
	this.ajaxRequestedLang;
	this.currentSubdomainLang;
	this.cookieDomain;
	this.langCookie;
	this.newPreferredLang;
	this.onNakedDomain;
	this.onFrontPage;
	this.saveLavePref;
	this.targetLang;

	// Value them vars up, if possible;
	this.cookieDomain = '.snapengage.com';
	this.onNakedDomain = (window.location.hostname == 'snapengage.com') ? true : false;
	this.saveLangPref = (simulateGet('saveLangPref')) ? true : false;
};
var langObj = new LanguageObject(); // container object for Language Redirect stuff.

// Save the Javascript resources by ignoring all of this if we're not going to use it
	langObj.currentSubdomainLang = 'en';
	langObj.langCookie = readCookie('snapengage.language');

// First, get preferred lang from AJAX php.
jQuery(document).ready(function() {
		jQuery.ajax({
			type: "POST",
			url: '//snapengage.com/wp-content/themes/snapengage_car-parent/header/ajaxLangRequest.php',
			//data: "acceptLang="+ajaxData,
			cache: false,
			processData: false,
			dataType: 'text',
			success: function(data) {
				// console.log('AJAX Language Data:: ', data); // Helpful for debugging language-redirect issues in future.
				var reqLang = data.substr(0, 2);
				beginRedirectAttempt(reqLang, langObj);
			}
		});
});
/* END PREFERRED LANGUAGE REDIRECTION SCRIPT */
</script>

<script type="text/javascript">		
// do jQuery stuff once document is ready.
var sePricingPlan;
var logolink = false;

jQuery(document).ready(function() {
	if (typeof simulateGet('logolink') === 'string') { // ?logolink parameter is present
		logolink = true;
	}

	sePricingPlan = readCookie('snapengage.plans');
	if( sePricingPlan === null || sePricingPlan === undefined ) {
		sePricingPlan = "Not yet defined";
	}

	// After 2 seconds, check scroll level.
	scrollObj.tid = setTimeout(checkScrollTop,5000);
	
	jQuery('#livechat').fadeOut(0);

	jQuery('.sign-up-box>h1.oversize').click(function() {
		jQuery('#sign-up-email').focus().tipsy('show');
	});
	jQuery('#sign-up-email').focusout().tipsy('hide');
		
	// These affect the background-image css propety
	// set #agentpic
	setRandomImage('#agentpic','agent','//snapengage.com/wp-content/themes/snapengage_car-parent'); // These images come from parent theme
	// set .point-img
	setRandomImage('.point-img','point','//snapengage.com/wp-content/themes/snapengage_car-marketing'); // These images come from child theme
	
	//backup to make sure the #livechat div shows up
	setTimeout(function() {
		// if #livechat is still invis 3 seconds after (document).ready(), reveal it now.		
		if (jQuery('#livechat').css('display') == 'none') {
			jQuery('#livechat').fadeIn(0);
			}},3000);
});
	
</script>

