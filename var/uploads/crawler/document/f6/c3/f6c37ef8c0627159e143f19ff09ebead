  (function() {
    var se = document.createElement('script'); se.type = 'text/javascript'; se.async = true;
    se.src = '//commondatastorage.googleapis.com/code.snapengage.com/js/' + seWidgetId + '.js';
    var done = false;
    se.onload = se.onreadystatechange = function() {
        if (!done && (!this.readyState||this.readyState==='loaded'||this.readyState==='complete')) {
            done = true;
            // We want the #livechat div to fade in on the front page only.
            // On other pages it should pop up as soon as (online) is true
            var fadeDelay = (wpPageStatus.is_front_page) ? '1000' : '0';
      
            // If we're using an onscroll-reveal, we'll want this hidden like so:
            jQuery('#SnapABug_Button').css('display', 'none');
            jQuery('#livechat .chat-bubble p>a').attr("onclick", "liveChatClick();");
            jQuery('#livechat #agentpic>a').attr("onclick", "liveChatClick();");
             
            SnapEngage.getAgentStatusAsync(function(online) {
                if (online) {
                    jQuery('#livechat').fadeIn(fadeDelay);
    
                    var chatParam = new Boolean();
                    //search the ?querystring for 'chat'    

                    chatParam = searchQueryString('chat');
                    //if chatParam exists, fire proactive immediately.
                    if (chatParam) {
                        if (SnapEngage.isMobile()) { // TEMP Mobile hack workaround
                          SnapEngage.startLink();
                        } else {
                          SnapEngage.openProactiveChat(true,true);
                        }
                    }
                } else {
                    setAgentToOffline();
                    jQuery('#livechat').fadeIn(fadeDelay);
                }
        
            });

            var seAgent;
            SnapEngage.setCallback('OpenProactive', function(agent, msg) {
                seAgent = agent;
                _gaq.push(['_trackEvent', 'SnapEngage', 'proactivePrompt', agent]);
            });

            SnapEngage.setCallback('StartChat', function(email, msg, type) {
                if (type == 'proactive') {
                    _gaq.push(['_trackEvent', 'SnapEngage', 'proactiveEngaged', seAgent]);
                }
            });
      
        }
    };
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(se, s);
  })();

</script>
