<script type="text/javascript" src="/scripts/home/vovici/vovicy_hm_survey_trigger.js"></script>
	<script type="text/javascript">
		var	voviciEnabledCountries = ',,';
		var vsite = 'HSeu';
		var processVovicy = false;
		// if any of the cookies 'geo_country_code' & 'choose_country' exists
		if(vovicyreadcookie("geo_country_code") || vovicyreadcookie("choose_country"))
		{
			var isCountryCodeMatch = false;
			var isChooseCountryMatch = false;

			// if any of the cookies 'geo_country_code' is present in enabledCountries property
			if((vovicyreadcookie("geo_country_code") && (voviciEnabledCountries.indexOf(","+vovicyreadcookie("geo_country_code")+",") > -1)) || (voviciEnabledCountries.indexOf("All") > -1)) {
				isCountryCodeMatch = true;
			}
			// if any of the cookies 'choose_country' is present in enabledCountries property
			if((vovicyreadcookie("choose_country") && (voviciEnabledCountries.indexOf(","+vovicyreadcookie("choose_country")+",") > -1)) || (voviciEnabledCountries.indexOf("All") > -1)) {
				isChooseCountryMatch = true;
			}
			//alert("voviciEnabledCountries:: "+voviciEnabledCountries+"\ngeo_country_code :: "+vovicyreadcookie("geo_country_code")+"\nisCountryCodeMatch :: "+isCountryCodeMatch+"\nisChooseCountryMatch :: "+isChooseCountryMatch);
			if((isCountryCodeMatch && !vovicyreadcookie("choose_country")) || ((isCountryCodeMatch && isChooseCountryMatch)) || (!isCountryCodeMatch && isChooseCountryMatch))
			{
				processVovicy = true;
				if(vsite != null || vsite != '')
				{
					vsite = vsite.substring(2,vsite.length);	
					vsite = vsite.toUpperCase();	
				}
				else
				{
					vsite = "US";
				}
				vovicy_hitcount_url = getvovicyhitcountturl('HSeu');	  
				document.write('<script type="text/javascript" language="JavaScript" src="'+vovicy_hitcount_url+'"><\/script>');
			}
		}
			</script>
	
			<script type="text/javascript">
			if(processVovicy)
			{
				var vov_productcategory = "";
				var vcategory = "";
				var cattype = "";
				var productsku = "";

				setvocicyurlparameters('en', vsite, productsku, vov_productcategory, vcategory);
			}
	</script> 
