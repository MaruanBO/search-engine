<div style="display: none; height: 27px; width: 690px; margin-top: 3px; background-image: url(images/Buttons/rotator/controlbackground_foo.jpg); background-repeat: repeat-x;">
    <div style="float: right;">
        <a id="lnkPlay" target="_self"><img id="imgPlay" src="Images/Buttons/rotator/play_select.jpg" border="0" alt="Play Button" /></a>
        <a id="lnkPause" href="javascript:Pause();" target="_self"><img id="imgPause" src="Images/Buttons/rotator/pause.jpg" border="0" alt="Pause Button" style="margin-right: 15px;" /></a>
    </div>
