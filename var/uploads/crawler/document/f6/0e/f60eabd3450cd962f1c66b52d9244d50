<script>
    $(function () {
        $(".lnkClose").click(function () {
            hideAlert($(this).parent());
        });
        $(".divAlert").each(function (index, element) {
            setTimeout(
                function() {
                    showAlert($(element));
                },
                500
            );
        });
    });
    function showAlert(theAlert) {
        $(theAlert).slideDown();
        var delay = parseInt($(theAlert).attr("data-delay"), 10);
        if (delay > 0) {
            var timerId = setTimeout(function() {
                hideAlert($(theAlert));
            }, delay)
            $(theAlert).attr("data-timerId", timerId);
        }
    }
    function hideAlert(theAlert) {
        var timerId = $(theAlert).attr("data-timerId");
        if (timerId) window.clearTimeout(timerId);
        $(theAlert).find(".lnkClose").hide();
        $(theAlert).slideUp();
    }
</script>
<style>
    .divAlert {
        width: 100%;
        padding: 10px;
        border-bottom: 1px dotted red;
        background-color: #e7e7e7;
        color: red;
        display: none;
        position: relative;
        text-align: center;
    }
    a.lnkClose {
        font-size: 10pt;
        color: red;
        position: absolute;
        right: 40px;
    }
</style>


