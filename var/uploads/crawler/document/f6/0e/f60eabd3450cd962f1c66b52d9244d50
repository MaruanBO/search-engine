<script type="text/javascript">
    function openTieSchoolSelector() {
        window.open("/includes/popup/schoolpicker.aspx?detail=true", "schoolpicker", "height=500,width=500,status=no,toolbar=no,menubar=no,location=no,scrollbars=yes,resize=yes");
    }

    function headerSearchText_onkeypress() {
        if (window.event.keyCode == 13) {
            doHeaderSearch();
            return false;
        }
        return true;
    }

    function headerSearchText_onfocus() {
        var srch = document.getElementById("headerSearchText");
        if (srch) {
            var srchTxt = srch.value;
            if (srchTxt == "Search")
                srch.value = "";
        }
    }
    $(function () {
        $("#headerSearchText").watermark("Search");
    });

    function doHeaderSearch() {
        var srch = document.getElementById("headerSearchText");
        if (srch) {
            var srchTxt = srch.value;
            if ((srchTxt.length > 0) /*&& (srchTxt != "Search")*/) {
                var re = /[^A-Za-z0-9_ +-?"]+/g;
                if (!re.test(srchTxt))
                    window.location.href = "/search.aspx?srchTxt=" + srchTxt;
            }
        }
        return false;
    }

    function ShowRecipe(recipeId) {
        window.location.href = "/meal_planning/recipes/recipe.aspx?id=" + recipeId;
        //window.open("/meal_planning/recipes/recipe.aspx?id=" + recipeId, "ViewRecipe");
        //        window.open("/includes/popup/ViewRecipe.aspx?id=" + recipeId, "ViewRecipe", "height=600px,width=620px,status=no,toolbar=no,menubar=no,location=no,scrollbars=yes");
        //        var w = window.open("/includes/popup/ViewRecipe.aspx?id=" + recipeId, "ViewRecipe", "height=650px,width=615px,scrollable=yes,resize=yes");
        //        w.focus();
    }

    function ViewCart(dropdown) {
        var url = dropdown.options[dropdown.selectedIndex].value;

        if (url.length > 0)
            window.location = url;
    }

    function GetControl(name) {
        return document.getElementById(name);
    }
</script>
