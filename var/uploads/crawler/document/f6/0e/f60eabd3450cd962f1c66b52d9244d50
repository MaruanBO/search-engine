<script language="javascript" type="text/javascript">
    var is_paused = false; // set to true to start off paused
    var current_timer;
    var current_index = 0;
    var tabs = $("#ctl00_ImageRotator1_tabs").val().split(';');
    var images = $("#ctl00_ImageRotator1_images").val().split(';');
    $(".imgPointer").attr("src", $("#ctl00_ImageRotator1_imgPointer").attr("src"));
    function GetTab(index) { return $('#' + tabs[index]); }
    function GetImage(index) { return $('#' + images[index]); }

    // hide all images 
    jQuery.each(images, function () { if (this.length > 0) { $('#' + this).css('display', 'none'); } });
    // show the first image
    //GetImage(0).show();
    // highlight the first tab
    //ActivateTab(0);
    // start timer when the page is loaded
    $(document).ready(function () { GetImage(0).show(); ActivateTab(0); StartTimer(); });

    function StartTimer(time) {
        if (!is_paused) {
            var timer = (time == null ? 5000 : 0)
            var index = (current_index == 2 ? 0 : current_index + 1)
            var call = "ShowImage(" + index + ")"
            current_timer = setTimeout(call, timer);
        }
    }

    function ShowImage(index) {
        GetImage(current_index).fadeOut("slow");
        GetImage(index).fadeIn("slow");
        ActivateTab(index)
        current_index = index;
        StartTimer();
    }

    function ActivateTab(index) {
        
        $(".imgPointer").hide();
        //ptr.hide();

        // gray out all tabs
        jQuery.each(tabs, function () { if (this.length > 0) { $('#' + this).css('color', 'gray').css('background-image', 'none'); } });
        // set the given tab to green
        var tab = GetTab(index);
        tab.css('color', 'white').css('background-image', 'url(/images/UI/homepage/rotator_background.png)').css('background-repeat', 'repeat');
        var pos = tab.offset();
        var cntnr = $("#image_container").offset();
        $(tab).find(".imgPointer").css("left", cntnr.left + "px").show();
        //ptr.css({ "left": cntnr.left + "px", "top": pos.top + "px" }); //
        offsetLeft = 0;
        //ptr.show();
    }

    function TabClick(index) {
        if (!is_paused && current_index == index)
            Pause();
        else if (!(is_paused && current_index == index)) {
            // try to cancel the current timer befoer it starts the nex transition
            clearTimeout(current_timer);
            // shows the image and set the tab
            ShowImage(index);
            // pause after image is shown
            Pause();
        } else if (is_paused && current_index == index) Play();
    }

    function Play() {
        PlayPause(false);
    }

    function Pause() {
        PlayPause(true);
    }

    function PlayPause(isStop) {
        // try to cancel the current timer befoer it starts the nex transition
        clearTimeout(current_timer);
        // set global status
        is_paused = isStop;
        // call the timer function to getting going quickly if this is a play
        StartTimer(0);

        var path = 'Images/Buttons/rotator/';
        var select_text = '_select';
        var attrib_text = 'href';
        var play = $('#lnkPlay');
        var pause = $('#lnkPause');


        // start/stop and enable/disable links
        if (isStop) {
            play.attr(attrib_text, 'javascript:Play();');
            pause.removeAttr(attrib_text);
        }
        else {
            play.removeAttr(attrib_text);
            pause.attr(attrib_text, 'javascript:Pause();')
        }

        // flip images
        $('#imgPlay').attr('src', path + 'play' + (isStop ? '' : select_text) + '.jpg');
        $('#imgPause').attr('src', path + 'pause' + (isStop ? select_text : '') + '.jpg');
    }
</script>

