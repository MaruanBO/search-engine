<script>
  // Load the SDK Asynchronously
  (function (d) {
    var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
    if (d.getElementById(id)) {
      return;
    }
    js = d.createElement('script');
    js.id = id;
    js.async = true;
    js.src = "//connect.facebook.net/en_US/all.js";
    ref.parentNode.insertBefore(js, ref);
  }(document));

  if(AT.Globals.loginFacebook == undefined) {
    AT.Globals.loginFacebook = function(response, omniauth_endpoint) {
      window.location = omniauth_endpoint;
    };
  }

  if(AT.Globals.canLoginFacebook == undefined) {
    AT.Globals.canLoginFacebook = function(target) {
      return true;
    };
  }

  window.fbAsyncInit = function () {
    FB.init({appId:'18128749820', channelUrl:'//alltrails.com/channel.html', status:true, cookie:true, xfbml:true});
    var required_perms = 'email,user_birthday,offline_access,read_stream,publish_stream,publish_actions';
    var omniauth_endpoint = '/users/auth/facebook/callback?autologin=1&redirect_to=%2F';
    var omniauth_endpoint_no_autologin = '/users/auth/facebook/callback?redirect_to=%2F';


    var checkPermissions = function () {
      if(AT.Globals.canLoginFacebook()) {
        FB.api('/me/permissions', function (response) {
          var permsArray = response.data[0];
          var permsNeeded = required_perms.split(',');
          var permsToPrompt = [];
          for (var i = 0, item; item = permsNeeded[i]; i++) {
            if (permsArray[item] == null) {
              permsToPrompt.push(item);
            }
          }
          if (permsToPrompt.length > 0) {
            promptForPerms(permsToPrompt);
          } else {
            AT.Globals.loginFacebook(response, omniauth_endpoint_no_autologin);
          }
        });
      }
    };

    var promptForPerms = function (perms) {
      joined_perms = perms.join(',');
      if(AT.Globals.canLoginFacebook()) {
        FB.login(function (response) {
          if (response.authResponse)
            AT.Globals.loginFacebook(response, omniauth_endpoint_no_autologin);
        }, {scope:joined_perms});
      }
    };

    jQuery(function () {
      jQuery("body").delegate(".new-fb-btn", "click", function (e) {
        e.preventDefault();

        if(AT.Globals.canLoginFacebook(e.target)) {
          FB.getLoginStatus(function (response) {
            if (response.status === 'connected') {
              //checkPermissions();
              AT.Globals.loginFacebook(response, omniauth_endpoint_no_autologin);
            } else {
              FB.login(function (response) {
                if (response.authResponse) {
                  AT.Globals.loginFacebook(response, omniauth_endpoint_no_autologin);
                }
              }, {scope:required_perms });
            }
          });
        }
      });
    });
    FB.Event.subscribe('auth.login', function (response) {
      if (response.authResponse) {
        //checkPermissions();
        if(AT.Globals.canLoginFacebook()) {
          AT.Globals.loginFacebook(response, omniauth_endpoint);
        }
      }
    });

  };

</script>

