  	<script type="text/javascript" language="Javascript">		
	
          	carouActive    = 5559;
		
		var carouArrMargin=new Array();
		carouArrMargin[5559] = [1];
		carouArrMargin[5560] = [111];
		carouArrMargin[5561] = [224];
		carouArrMargin[5562] = [338];
		carouArrMargin[5563] = [448];	
	
		carouTimer	   = true; // don't switch product while the user is looking at them
	
		
	
		function carouMouseOver(elem,num)
	
		{		
	
			for(i = 5559; i <= 5563; i++)
	
			{
	
                          document.getElementById('car'+i).style.color='#666';
	
				document.getElementById('zone'+i).style.display = "none";
	
			}		
	
			elem.style.color='#fff';
	
			document.getElementById('carou_menu_active').style.marginLeft = carouArrMargin[num] + "px";		
	
			$('#zone'+num).show();
	
			carouActive = num;
	
		}////////mouseOver
	
		
	
		function carouMouseOut(elem,num)
	
		{		
	
			return;		
	
		}////////mouseOut	
	

	
		function switchProducts()
	
		{		
	
			if (carouTimer == true)
	
			{
	
				if (carouActive + 1 == carouArrMargin.length)
	
				{
	
					carouActive = 5559;
	
				}
	
				else
	
				{
	
					carouActive = carouActive + 1;
	
				}
	
				carouMouseOver(document.getElementById('car'+carouActive), carouActive);
	
			}
	
		}
	
		
	
		window.setInterval("switchProducts()", 6000);
	
	</script>
	
