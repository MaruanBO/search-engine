$(document).ready(function(){
  // Hide all video containers
  $('div.fvideo-container').hide();
  // Show the first container
  $('div#video0').show();
  // Set the first thumbnail to active
  $('li#thumb0').addClass('active');
  // Set the current div and list item targets
  var currentVideoDiv = 'div#video0';
  var currentListItem = 'li#thumb0';
  // On click event of video thumbnails
  $('li.thumb').click(function(event) {
    // Get the id of the thumbnail and translate to div target
    var thumbId = $(this).attr('id').substr(5);
    var targetVideoDiv = ('div#video' + thumbId).toString();
    var targetListItem = ('li#thumb' + thumbId).toString();
    // Hide the previous div, show the new one
    $(currentVideoDiv).hide();
    $(targetVideoDiv).show();
    // Highlight the new active thumbnail
    $(currentListItem).removeClass('active');
    $(targetListItem).addClass('active');
    // Update the new current div and list item targets
    currentVideoDiv = targetVideoDiv;
    currentListItem = targetListItem;
  });
});
</script>	
