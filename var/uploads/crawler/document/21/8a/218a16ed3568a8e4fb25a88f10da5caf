<script type="text/javascript">
    if ($('body.has-sub-header').length) {
        scrollDir = 'down';
        scrollPos = 0;
        $(window).scroll(function() {
            if ($(this).scrollTop() < scrollPos) {
                scrollDir = 'up';
            } else {
                scrollDir = 'down';
            }
            scrollPos = $(this).scrollTop();
            if (scrollDir == 'down' && scrollPos > 200) {
                $('body').addClass('compact');
            }
            if (scrollDir == 'up' && scrollPos < 400) {
                $('body').removeClass('compact');
            };
        });
        $('body.has-sub-header.compact .site-header-container #site_header').live('mouseenter', function() {
            $('body.has-sub-header').removeClass('compact').addClass('site-header-reveal');
        });
        $('body.has-sub-header.site-header-reveal .site-header-container').live('mouseleave', function() {
            $('body.has-sub-header').addClass('compact').removeClass('site-header-reveal');
        });
    }
        </script>

