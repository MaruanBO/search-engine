<script>
$(document).ready(function () {
    $('#stateCaption').click(function() {
        if ($('#stateCaption').hasClass('toggleOn')) {
            $('#stateCaption')
                .removeClass('toggleOn')
                .addClass('toggleOff');
        } else {
            $('#stateCaption')
                .removeClass('toggleOff')
                .addClass('toggleOn');
        }

        $('#stateList .column').toggle();
        $('.frontpageLocationFooterState').toggle();
    });
});
</script>

