    $(".noTy").click(function(e) {
        $('#newsletterPopup').dialog("close");
        window.open($('#tyUrl').val());
        setpopcookie();
        e.preventDefault();
    });

    function setpopcookie() {
        $.cookie('subscribePopup', '1', { path: '/', expires: 365 });
    }

    $('#externalPopupOptin').submit(function () {

        $('#newsletterPopup').dialog('close');

        $.post(
            '/opt-in/popout.php',
            {
                email : $(this).find('#Email1').val(),
                search : $('#pagemsg h1').html()
            }
        );
    });

});
//-->
</script>
