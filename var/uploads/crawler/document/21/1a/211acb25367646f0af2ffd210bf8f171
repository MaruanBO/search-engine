<script language="JavaScript" type="text/javascript">
/* You may give each page an identifying name, server, and channel on the next lines. */

/************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/

window.onpageshow = function(evt) {
    // If persisted then it is in the page cache, force a reload of the page.
    if (evt.persisted) {
        document.body.style.display = "none";
		location.reload();
    }
};
$(function(){
	var cookiePath = $.cookie('myPath'); 
	var domain = location.hostname;
	var prevUrl = "";
	prevUrl = document.referrer; 
	
	if(prevUrl != ""){
		prevDomain = prevUrl.split("/")[2];
		prevAudience = prevUrl.split("/")[3];
	}
	else{
		prevDomain = "";
		prevAudience = "";
	}
	if((prevAudience == 'individuals-families' || prevAudience == 'individuals-families.html') || prevDomain != domain){
		redirectPath(cookiePath);		
	}
	else{
		var iOS = ( navigator.userAgent.match(/(iPad|iPhone|iPod)/g) ? true : false );
		//if in ios device, back button will detect index.html as the referrer, on back press check the cookie and redirect.
		if(iOS){
			if(prevAudience == "index.html" || prevAudience == ""){
				redirectPath(cookiePath);
			}
			else{
				$('.routerCover').hide();
				$.removeCookie('myPath');	
			}
		}
		else{
			$('.routerCover').hide();
			$.removeCookie('myPath');            
		}
		
	}
	
});

function redirectPath(cookiePath){
	if (cookiePath && cookiePath.indexOf('|') != -1) {
		var isIE7 = navigator.userAgent;
		cookiePath = cookiePath.replace(/\|/g, '');
		if(isIE7.indexOf('MSIE 6.0') != -1){
			//disables redirect for ie6
			null;
		}else if(isIE7.indexOf('MSIE 7.0') > -1 && isIE7.indexOf('Trident') == -1){
			$('.routerCover').hide();
		}
		else{
			if(cookiePath == 'if'){
				window.location.assign("individuals-families.html")
			}
			else if(cookiePath == 'producers'){
				window.location.assign("insurance-producer.html");
			}
			else if(cookiePath == 'hcp'){
				window.location.href = "health-care-professionals.html";
			}
			else if(cookiePath == 'mobileRouter'){
				window.location.href = "http://mobile.aetna.com";
			}
			else if(cookiePath == 'employers'){
				window.location.href = "employers-organizations.html";
			}
			else if(cookiePath == 'about-us'){
				window.location.href = "about-us.html";
			}
			else{
				$('.routerCover').hide();
				var s_code=s.t();if(s_code)document.write(s_code);
			}
	
		}
	} else{
		$('.routerCover').hide();
		var s_code=s.t();if(s_code)document.write(s_code);
	}
};
</script>
