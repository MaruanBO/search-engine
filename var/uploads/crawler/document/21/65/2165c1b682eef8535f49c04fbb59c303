	$( ".sub-info-section" ).toggle();
	$( "#info-up-arrow" ).toggle();
	$( "#info-down-arrow" ).toggle();
});

//author dropdown widget jquery code
$( "#author-dropdown-down-arrow" ).click(function() {
	$( "#the-article-author-dropdown" ).toggle();
	$( "#author-dropdown-down-arrow" ).toggle();
	$( "#author-dropdown-up-arrow" ).toggle();
});
$( "#author-dropdown-up-arrow" ).click(function() {
	$( "#the-article-author-dropdown" ).toggle();
	$( "#author-dropdown-down-arrow" ).toggle();
	$( "#author-dropdown-up-arrow" ).toggle();
