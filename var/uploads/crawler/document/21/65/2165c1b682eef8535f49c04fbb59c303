jQuery(document).ready(function($){


//article references
$( "#references-up-arrow" ).click(function() {
	$( "#references-down-arrow" ).toggle();
	$( "#references-up-arrow" ).toggle();
	$( "#article-references-body" ).hide();

});
$( "#references-down-arrow" ).click(function() {
	$( "#references-down-arrow" ).toggle();
	$( "#references-up-arrow" ).toggle();
	$( "#article-references-body" ).show();
});


//sidebar more info widget jquery code
$( "#sds-more-information" ).click(function() {
	$( ".sub-info-section" ).toggle();
	$( "#info-up-arrow" ).toggle();
	$( "#info-down-arrow" ).toggle();
});
