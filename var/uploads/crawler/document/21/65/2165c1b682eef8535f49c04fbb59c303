    <script type="text/javascript">
        var i = 0;
	jQuery(document).ready(function($){
       // $(document).ready(function () {

            //Will check for currently handheld avialable media device
            var isPhone = /Android|webOS|iphone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent.toLowerCase());
            if (isPhone) {
                $("#leftmenu").css('left', '-200px');
                $('#right-menu').css('right', '-200px');
            }
            //Swipe Gestures(left/right) function on media devices
            $(document).on("swipeleft", backslideleft);

            $(document).on("swiperight", backslideright);
            //End of Swipe Gestures(left/right)

            //callback functions for slide back left menu and right menu

            function backslideleft() {
                if (isPhone) {
                    var hidden = $("#leftmenu");
                    if (hidden.css('left') == '0px' && hidden.css('display', 'block')) {
                        hidden.animate({ 'left': '-200px' }, 500).hide(400);
                    }
                }
            }
            function backslideright() {
                if (isPhone) {
                    var hidden = $("#right-menu");
                    if (hidden.css('right') == '0px' && hidden.css('display', 'block')) {
                        hidden.animate({ 'right': '-200px' }, 500).hide(400);
                    }
                }
            }

            //End of callback functions slide back 

            //End of Swipe Gestures(left/right) function on media devices

            $(document).click(function (e) {
                if ($(e.target).is(".nvg *"))
                { }
                else
                {
                    $('div.sub').hide();
                    i = 0;
                }
            });

            $(".nvg").click(function (ev) {
                //debugger;
                /*If available media is phone will run iphone navigation slides else will run toggle effects*/
                if (isPhone) {

                    if ($(ev.target).is("#left-navigation *")) {
                        backslideright();
                        $("#middle-menu").hide();
                        var hidden = $("#leftmenu");
                        hidden.css('left') == '-200px' ? hidden.animate({ "left": "0px" }, 500).show() : hidden.animate({ "left": "-200px" }, 500, function () { hidden.hide(); }).hide(500);
                    }
                    if ($(ev.target).is("#right-navigation *")) {
                        backslideleft();
                        $("#middle-menu").hide();
                        var hidden = $("#right-menu");
                        hidden.css('right') == '-200px' ? hidden.animate({ "right": "0px" }, 500).show() : hidden.animate({ 'right': '-200px' }, 500, function () { hidden.hide(); }).hide(500);
                    }
                    if ($(ev.target).is("#mid-navigation *")) {
                        backslideright();
                        backslideleft();
                        $("#middle-menu").toggle();
                    }
                }
                    //applicable for desktop screens
                else {
                    var prevAttr = $(".selected").find('div.sub').attr("data-id");
                    $(".selected").removeClass("selected");
                    $(this).addClass("selected");
                    var obj = $(this).find('div.sub');
                    var Currattr = obj.attr("data-id");
                    $('div.sub').each(function () {
                        $('div.sub').hide();
                    });
                    if (prevAttr == Currattr) {
                        $(obj).hide();
                        if (i == 0) {
                            $(obj).show();
                            i = 1;
                        }
                        else {
                            i = 0;
                            $(obj).hide();
                        }
                    }
                    else {
                        $(obj).show();
                        i = 1;
                    }
                    ev.stopPropagation();
                }

            });
            /*$("#left-navigation").click(function(){
                $("#leftmenu").toggle();
                });		
            /*$("#mid-navigation").click(function(){
                $("#middle-menu").toggle();
                });
            $("#right-navigation").click(function(){
                $("#right-menu").toggle();
                });*/
            //$("#middle-menu li a").click(function () {
            //    var selectedmenu = $(this).text().trim();
            //    $("#mid-menu-text").text(selectedmenu);
            //    var check = "<img src='/wp-content/themes/sds-3/images/check-icon.png' />";
            //    $("#middle-menu li").each(function () {
            //        $(this).find('img').remove();
            //    });
            //    $(this).parent().append(check);
            //});

          //  $("#middle-menu li").click(function () {                
            //    var selectedmenu = $(this).children('a').text().trim();
              //  var check = "<img src='/wp-content/themes/sds-3/images/check-icon.png' />";
                //$("#middle-menu li").each(function () {                  
                  //  if ($(this).hasClass('selected_li')) {
                  //      $(this).removeClass('selected_li');
                  //      $(this).find('img').remove();
                  //  }
               // });
               // $(this).append(check);
               // $(this).addClass('selected_li');
               // $("#mid-menu-text").text(selectedmenu);
