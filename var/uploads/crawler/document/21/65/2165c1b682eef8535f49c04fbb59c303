//solution from http://jsfiddle.net/7ey9g/76/
var $sidebar = $("#sidebar"),
    $window = $(window),
    sidebartop = $("#sidebar").position().top,
    windowsize = $window.width();

 var timer;

 if (windowsize > 650) {

   $window.scroll(function() {


	if (windowsize > 1010) {
		var ml = 688;
	} else {
		var ml = windowsize - 320;
	}

    if ($window.height() > $sidebar.height()) {
	   $sidebar.css({"top":"0","bottom":"auto","margin-left":"0","position":"absolute","right":"0"});
        if($sidebar.offset().top <= $window.scrollTop() && sidebartop <= $window.scrollTop()) {
//            $sidebar.addClass('fixedTop');        
        } else {
		$sidebar.css({"top":"0","bottom":"auto","margin-left":"0","position":"absolute","right":"0"});
        }
    } else {
        $sidebar.removeClass('fixedTop');
        if ($window.height() + $window.scrollTop() > $sidebar.offset().top + $sidebar.height()+20) {
		$sidebar.css({"top":"auto","bottom":"0","margin-left" : ml + "px","position":"fixed","right":"auto"});
        }
                   
        if ($sidebar.offset().top < 0) {
		$sidebar.css({"top":"0","bottom":"auto","margin-left":"0","position":"absolute","right":"0"});
        }
    }

   }); //end $window.scroll(function())

 }

});

</script>
