<body id="onstar-connections" class="home blog  has-masthead has-mainnav has-sidebar has-footer theme-onstar-connections role-guest">  <script>

  /* -------------------------------------------------------------------------
   * YouTube iFrame API
   * -------------------------------------------------------------------------
   * Reference: https://developers.google.com/youtube/iframe_api_reference
   * Reference: https://developers.google.com/youtube/js_api_reference
   * ---------------------------------------------------------------------- */
  
  try { 

    // API async
    // ---------
  
    var tag = document.createElement('script');
    tag.src = "https://www.youtube.com/iframe_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
    
  
    // Create Objects
    // --------------
  
    var ytPlayers = {};
  
    var currentVideo = {};
    var lastVideo = {};
    
    // set default
    currentVideo.index = 'default';
    currentVideo.id = 'default';
    currentVideo.playerState = 'default';
    
    
    // Create Player Objects
    // ---------------------
    
    function onYouTubeIframeAPIReady() {
                  
      // Flag YT API as true
      jQuery('body').addClass('has-ytapi');
  
      // if youtube.com is in the iframe's url, assumpe it's a video      
      jQuery('iframe[src*="youtube.com"]').each(function() {
        
        // youtube iframe api will create a player for a div that uses the hyphen-format-ids
        // so your id on the players need to be distinguished from all other makrup
        // i've tried and failed to automate this. adding strings and converting characters all breaks the api
        // ps, i hate youtube
                
        // get the youtube id from the embed url
        var ytID = jQuery(this).attr('src').split('/')[4]; ytID = ytID.substring( 0, ytID.indexOf('?') );
                
        // if there's no html id, add one using the ytID
        if ( typeof jQuery(this).attr('id')  === 'undefined' ) jQuery(this).attr('id', ytID);
        
        // use the html id as the slug for the api object
        var slug = jQuery(this).attr('id');
        
        // classify this as a youtube player for event detection purposes
        jQuery(this).addClass('youtube');
        
        // construct the player API objects                
        currentVideo[slug] = new Object();
        lastVideo[slug] = new Object();
  
        ytPlayers[slug] = new YT.Player(slug, { events: {'onReady': onPlayerReady, 'onStateChange': onPlayerStateChange} });
                                
      }); 
  
    }
    
    // Helpers
    // -------
    
    function updateVideoName( slug, index ) {
    
      // default is unknown
      var index = typeof index !== 'undefined' ? index : 'unknown';
    
      // set last video based on last current video
      lastVideo.name = currentVideo.name;
      
      // update video name 
      currentVideo.name = slug.replace(/_|-|player/g, ' ').toUpperCase();
      
      // console.log('Current video is: ' + currentVideo.id);
    
    }  
  
    // Event Handlers
    // --------------
  
    function onPlayerReady(event) { /* player is ready */ }
  
    function onPlayerStateChange(event) {
  
      // currentVideo.index = event.target.getPlaylistIndex();
      currentVideo.id = event.target.a.id;
      currentVideo.playerState = event.data;
  
      // Handle Video Start
      // ------------------
  
      if ( event.data == YT.PlayerState.PLAYING ) { 
       
        // update status first
        updateVideoName( currentVideo.id, currentVideo.index ); 
  
        // only fire analytics if it's not the same video playing
        if ( currentVideo.name != lastVideo.name && typeof trackVideoStart == 'function' ) trackVideoStart( event, currentVideo.name ); 
  
      }
      
      // Handle Video End
      // ----------------
      
      if ( event.data == YT.PlayerState.ENDED ) {
        
        // fire ENDED
        if ( typeof trackVideoEnd == 'function') trackVideoEnd( event, currentVideo.name );
        
        // update status
        updateVideoName( currentVideo.id, currentVideo.index );
        
      }
  
    }
    
  } catch (error) { if ( typeof console !== undefined || typeof console.log !== undefined) console.log('There was a problem loading the YouTube iFrame API'); console.log(error); }

</script>
