    function buttonOnClick(){
        s.linkTrackVars='events';

        if (0 == 0){
            s.linkTrackEvents='scCheckout,scOpen,event80';
            s.events='scCheckout,scOpen,event80';
        } else if (0 >0){
            s.linkTrackEvents='event80';
            s.events='event80';
        }

        s.tl(this,'o','Home page scheduler');
    }
</script>


