<script language="JavaScript">
  var ImageArrManagers = new Array("/assets/gasc/assets/cms/homepage/managers/m1.png","/assets/gasc/assets/cms/homepage/managers/m2.png","/assets/gasc/assets/cms/homepage/managers/m3.png","/assets/gasc/assets/cms/homepage/managers/m4.png");
  var ImageHolderManagers = document.getElementById('RotatingManager');
  
  function RotateManagers(Start)  {
  	var a = eval("ImageArrManagers");
  	var b = eval("ImageHolderManagers");
  	if(Start>=a.length)
  		Start=0;
  	b.src = a[Start];
  	window.setTimeout("RotateManagers("+(Start+1)+")",50000);
  }
  
  RotateManagers(0);  
</script>
