<script language="JavaScript">

var ImageArrPromos = new Array("/assets/gasc/assets/cms/homepage/Homepage-promoarea-1995.png");
var UrlArrPromos = new Array("/catalog/getStoreServices.cmd?parentCategoryId=668&subCategoryId=696&icid=GASC_May1_2495LOF_Home_promo2");

if (Quova.Location.DMA.Id == '528' || Quova.Location.DMA.Id == '534'){
ImageArrPromos = new Array("/assets/gasc/assets/cms/homepage/Homepage-promoarea-1995.png","/assets/gasc/assets/cms/homepage/home-promo-FL-flat-repair.png");
UrlArrPromos = new Array("/catalog/getStoreServices.cmd?parentCategoryId=668&subCategoryId=696&icid=GASC_May1_2395LOF_Home_promo2","/checkout/add_services_to_order.cmd?services=153095");
}
if (Quova.Location.DMA.Id == '744'){
ImageArrPromos = new Array("/assets/gasc/assets/cms/homepage/May1-homepage-promo-27.95lof-HI.png");
UrlArrPromos = new Array("/catalog/getStoreServices.cmd?parentCategoryId=668&subCategoryId=696&icid=GASC_May1_2395LOF_Home_promo2","/home.jsp");
}

  var ImageHolderPromos = document.getElementById('RotatingPromo');
  var UrlHolderPromos = document.getElementById('RotatingURL');
  
  function RotatePromos(Start)  {
  	var a = eval("ImageArrPromos");
  	var b = eval("ImageHolderPromos");
  	if(Start>=a.length)
  		Start=0;
  	b.src = a[Start];
  	window.setTimeout("RotatePromos("+(Start+1)+")",50000);
  }

function RotateUrls(Start)  {
  	var a = eval("UrlArrPromos");
  	var b = eval("UrlHolderPromos");
  	if(Start>=a.length)
  		Start=0;
  	b.href = a[Start];        
  	window.setTimeout("RotateUrls("+(Start+1)+")",50000);
  }
  
  RotatePromos(0);
  RotateUrls(0);  
</script>
