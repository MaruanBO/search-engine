 <script type="text/javascript">
jQuery(function(){
  jQuery('#infotab, .help_box').find('#lpButtonDiv').attr('id', 'lpButtonDiv2');
  jQuery('#checkout .help_box').find('#lpButtonDiv2').attr('id', 'lpButtonDiv4');
});

    // ########################################################################
    // ================== 1: VARIABLE DEFINITION =============================
    // ########################################################################
    // Variables are JavaScript objects that are passed in an array
    // Each object must contain: scope, name, value
    // Names and values should be strings
    // A LivePerson variable can be defined in one of three different scopes:
    //   'page' - exists on the current page only
    //   'session' - exists for the current visitor session
    //   'visitor' - persists between browser sessions
    // Not all variables will be required by the LP Mobile deployment, use the mobile:true/false flag to specificy this

var arrLPvars = [
    { scope:'page', name:'unit', value:'sales', mobile:true }, // pass [sales] or [service] depending on page. For Help and customer service related pages, this needs to be set to 'service' please pass in lower case unlike other variables

    { scope:'session', name:'language', value:'english', mobile:true },// For now only pass [english]. please pass in lower case unlike other variables

    { scope:'page', name:'Section', value:'Marketing', mobile:true }, // [Marketing, Checkout, MyAccount,Help]

    

    { scope:'session', name:'Country', value:'UK', mobile:true },// Pass the used country - currently UK

    { scope:'page', name:'Currency', value:'GBP', mobile:true }, // pass the used currency on confirmation page

    { scope:'page', name:'HelpHover', value:false, mobile:false }, // pass true if link 'Help' is mouse hovered or false if not.

    { scope:'page', name:'Category', value:'', mobile:false }, // pass different values as shown under [NEW ARRIVALS, HANDBAGS, CLOTHING, SHOES, JEWELRY, ACCESSORIES, Home, Featured Shops, SALE, OUR BLOG]



    { scope:'page', name:'CartTotal', value:'0.00', mobile:false },// pass value of cart total on all pages
    { scope:'page', name:'ErrorCounter', value:'', mobile: false }, // pass the number of errors which occured on the page
    { scope:'page', name:'SizingCounter', value:'', mobile:false } // pass number of times the link 'Size guide' is clicked

    ];



    // ########################################################################
    // ==================== 2: LP MOBILE CONFIG ===============================
    // ########################################################################
    var _LP_CFG_ = {
      // app_id : 'YOUR_LP_MOBILE_APP_ID', // Only required for accounts that already have an app_id, it is not required for new LP Mobile deployments
      options: {
        // skill: chatSkill, // set the mobile chat skill
        triggerSelector: "#lp-mobile-chat-button", // optional CSS selector to provide a button on the page

        // the following function is for sending custom variables + values to the LivePerson Servers when the chat begins
        extras: function() { // A static dictionary or a function that returns a dictionary of custom variables

          var extras = {};
          var i, varsCount = arrLPvars.length;

          // This will run through the array of variables and add those flagged with mobile:true to the dictionary
          if (arrLPvars) {
            for (i = 0; i < varsCount; i++) {
              if (arrLPvars[i].mobile) {
                extras[arrLPvars[i].name] = arrLPvars[i].value;
              }
            };
          }

          return extras;
        },
        onChatNotAnswered: function(messages) {
          console.log('chat not answered');

        },
        onChatDisabled: function(els) {
          console.log('chat disabled!');
          // Change the LP Mobile button to an offline state, for example:
          for(var i = 0; i < els.length; i++) {
            els[i].style.display = "none";
            els[i].setAttribute("disabled","true");
          }
          $('.available-help').hide();
        /*var lpMobileButton = document.getElementById('lp-mobile-chat-button');
          lpMobileButton.style.display = "block";
          lpMobileButton.setAttribute("disabled","true");
          lpMobileButton.innerHTML = "Chat unavailable";*/
        },
        onChatEnabled: function(els) {
          // Change the LP Mobile button to an online state, for example
          console.log('chat enabled!');
          for(var i = 0; i < els.length; i++) {
            els[i].style.display = "inline";
            els[i].removeAttribute("disabled");
          }
          $('.available-help').show().css('margin-top', '5px');
       /* var lpMobileButton = document.getElementById('lp-mobile-chat-button');
          lpMobileButton.style.display = "block";
          lpMobileButton.removeAttribute("disabled");
          lpMobileButton.innerHTML = "Chat now";*/
        }
      }
    };

    // ########################################################################
    // ==================== 2: LP DESKTOP SETUP ===============================
    // ########################################################################
    // Start by defining the required objects
    var lpTag = lpTag || {}, lpMTagConfig = lpMTagConfig || {}; lpTag.vars = lpTag.vars  || []; lpTag.dbs = lpTag.dbs || [];
    lpTag.section = 'default';


    // ---------- i: Sending variable array when the page loads --------------
    lpTag.vars.push(arrLPvars);

    // ======================= DYNAMIC BUTTONS ============================
    // Define a default dynamic button on the page, specifying the following parameters
    //   name: the name of the dynamic button in the LivePerson Admin Console
    //   pid: the id of the div that the button will be injected into
    lpTag.dbs.push({ name:'chat-sales-english-default', pid:'lpButtonDiv' });


    // ########################################################################
    // ===================== 3: IN PAGE ACTIONS ===============================
    // ########################################################################


    // -------- Sending variables on demand (e.g. after a click event) --------
    var sendVarsOnClick = function (scope, name, value, mobile) {
      // create the variables array the same as for the onload variables
      var arrNewVars = [
        { scope:scope, name:name, value:value, mobile:mobile }
      ];
      //console.log(' arrNewVars   '+ name +'        '+ value);
      // The push methods are different for Mobile and Standard chat for technical reasons
      // For LP Mobile loop through and send the variables as events
      if (typeof(LPMobile) !== "undefined") {
        for (var i = 0; i < arrNewVars.length; i++) {
          if (arrNewVars[i].mobile) {
            LPMobile.reportEvent(arrNewVars[i].name,arrNewVars[i].value);
          }
        }

      } else {
        // For stand monitoring push to the vars queue
        lpTag.vars.push(arrNewVars);
        // This 'try' block is here because the LP code must be instantiated on the page
        // before the lpTag.vars.send() method can be called
        // If the method fails for this reason the variables in the queue will be picked
        // up when the LP code initiates
        try{
          // Send waiting variables immediately
          lpTag.vars.send();
        } catch(e) {}
      }
    };
    var currcat;
    for (var i = 0; i < arrLPvars.length; i++) {
      if (arrLPvars[i].name == 'Category') {
          currcat = arrLPvars[i].value;
      }
    }
