                <script type="text/javascript">
                var recentHelper = {
                    listing: [],
                    current: 0,
                    
                    timeKeeper: false,
                    start: function() {
                        for(var i = 0; i < 5; i++) {
                            $('#rlSD'+i).html(recentHelper.listing[i]['sellDate']);
                            $('#rlSN'+i).html(recentHelper.listing[i]['sld']+'<span class="txt-tld">.'+recentHelper.listing[i]['tld']+'</span>');
                            $('#rlSP'+i).html(recentHelper.listing[i]['price']);
                            
                            recentHelper.current = i;
                        }

                        recentHelper.timeKeeper = setTimeout(function() {
                            recentHelper.next();
                        },1000);
                    },

                    next: function() {
                        var curId = recentHelper.current+1;
                        if (recentHelper.listing.length <= curId) {
                            var rhl = recentHelper.listing.length;
                            for(var rhi = 0; rhi < rhl; rhi++) {
                                recentHelper.listing.push(recentHelper.listing[rhi]);
                            }
                        }
                        
                        $('#rlID0').animate({
                            'opacity':0
                        },1000,function() {
                            for(var ir = 1; ir < 5; ir++) {
                                var irp = ir-1;

                                var ir1 = $('#rlSD'+ir).html();
                                var ir2 = $('#rlSN'+ir).html();
                                var ir3 = $('#rlSP'+ir).html();

                                $('#rlSD'+irp).html(ir1);
                                $('#rlSN'+irp).html(ir2);
                                $('#rlSP'+irp).html(ir3);
                            }

                            $('#rlID4').css('opacity',0);
                            $('#rlID0').css('opacity',1);

                            $('#rlSD4').html(recentHelper.listing[curId]['sellDate']);
                            $('#rlSN4').html(recentHelper.listing[curId]['sld']+'<span class="txt-tld">.'+recentHelper.listing[curId]['tld']+'</span>');
                            $('#rlSP4').html(recentHelper.listing[curId]['price']);

                            $('#rlID4').animate({
                                'opacity':1
                            },400,function() {
                                recentHelper.current++;

                                clearTimeout(recentHelper.timeKeeper);
                                recentHelper.timeKeeper = setTimeout(function() {
                                    recentHelper.next();
                                },1000);
                            });
                        });
                    }
                };

