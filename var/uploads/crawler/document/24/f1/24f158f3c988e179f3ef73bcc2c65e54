<script type="text/javascript">
var HomeSearch = {
    tlds: ['com'],

    addTLD: function(tld) {
        HomeSearch.tlds.push(tld);

        if (HomeSearch.tlds.length == 1) $('#tldShow').html('.'+HomeSearch.tldString());
        else $('#tldShow').html('...');
    },
    
    removeTLD: function(tld) {
        if (HomeSearch.tlds.indexOf(tld) > -1) {
            HomeSearch.tlds.splice(HomeSearch.tlds.indexOf(tld),1);
            
            if (HomeSearch.tlds.length == 1) $('#tldShow').html('.'+HomeSearch.tldString());
            else $('#tldShow').html('...');
        }
    },
    
    tldString: function() {
        if (HomeSearch.tlds.length > 0) return HomeSearch.tlds.join(',');
        else return false;
    },
    
    sendAlong: function() {
        if ($('#homeSearch').val() !== '') {
            $('#homeSearch').val($('#homeSearch').val().replace(/@/g,''));
            $('#homeSearch').val($('#homeSearch').val().replace(/ /g,''));
            $('#homeSearch').val($('#homeSearch').val().replace('www.',''));
            
            var sval = $('#homeSearch').val();
            
            if (sval.indexOf('.') > -1) {
                nTLD = sval.substring(sval.indexOf('.')+1);
                sval = sval.substring(0,sval.indexOf('.'));

                if ($('#selector-'+nTLD).length !== 0) HomeSearch.tlds = [nTLD];
            }

            if (sval !== '') {
                var rule = /^[a-zA-Z\d]+$/;
                if (rule.test(sval.replace(/-/g,''))) {
                     var uri = '/search/'+sval;

                    var tldString = HomeSearch.tldString();
                    if (tldString !== false) uri =  uri+'?tlds='+tldString;

                    BuyDomains.eventTrack('Search Initiation','Search on Splash',sval);
                    setTimeout(function() {
                        window.location = uri;
                    },500);
                } else popup.trigger('An error has occured!','/generic-error/');
            } else popup.trigger('An error has occured!','/generic-error/');
        } else popup.trigger('An error has occured!','/generic-error/');
    }
};

$('#homeSearch').keyup(function(e) {
    if (e.keyCode == 13) {
        HomeSearch.sendAlong();
    }
});

$('#homeSearchSubmit').click(function() {
    HomeSearch.sendAlong();
});

$('#nav-home-tld-drop').click(function() {
    if ($('#home-tld-drop').css('display') == 'none') $('#home-tld-drop').hide();
    $('#home-tld-drop').fadeToggle();
    return false;
});

$('.tld-choice').click(function() {
    if ($(this).attr('id').indexOf('head-') < 0) {
        if ($(this).hasClass('selected')) {
                HomeSearch.removeTLD($(this).attr('id').replace('selector-',''));
                $(this).removeClass('selected');
                $(this).find('i.icon-check').attr('class','icon-check-empty');
        }
        else {
                HomeSearch.addTLD($(this).attr('id').replace('selector-',''));
                $(this).addClass('selected');
                $(this).find('i.icon-check-empty').attr('class','icon-check');
        }
    }
});
</script>

