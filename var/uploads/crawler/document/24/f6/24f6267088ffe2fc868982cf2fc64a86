        //function that creates posts
        var postHandler = function(postsJSON) {
            var template = '/wp-content/themes/surfermag.com/templates/content-browse.mustache';

            // Load the HTML template
            /*$.get( template, function(tmpl) {
                var renderedPage = Mustache.to_html( tmpl, {'data' : postsJSON} );
                $('#posts').html( (renderedPage != '') ? renderedPage : '<span class="alert">Sorry: no more posts to be loaded!</span>' );
            });*/

            $.each(postsJSON,function(i,post) {
                //post url
                var id = 'post-' + post.post_id;
                //create the HTML
                $('<section></section>')
                //.attr('id', id)
                //generate the HTML
                .html('<a href="'+post.post_url+'"><img src="'+post.post_image+'"></a><div class="box-shadow height-90"><p class="p1">'+post.post_category+'</p><p class="p2"><a href="'+post.post_url+'" title="'+post.post_title+'">'+post.post_title+'</a></p><p class="p3">'+post.post_excerpt+'</p></div>')
                //inject into the container
                .appendTo($('#posts'))
                .hide()
                .slideDown(250,function() {
                    if(i == 0) {
                        $.scrollTo($('section#' + id));
                    }
                });
            });
        };

        //first, take care of the "load more"
        //when someone clicks on the "load more" DIV
        var start = 9;
        var desiredPosts = 9;
        var loadMore = $('.js-more-button');
        var loadMoreText = $('.js-more-button').html();

        //load event / ajax
        loadMore.click(function(){
            //add the activate class and change the message
            loadMore.addClass('activate').html('<img src="/wp-content/themes/surfermag.com/images/loading.gif" />');
            //begin the ajax attempt
            $.ajax({
                url: '',
                data: {
                    'start': start,
                    'desiredPosts': desiredPosts
                },
                type: 'get',
                dataType: 'json',
                cache: false,
                success: function(responseJSON) {
                    //reset the message
                    loadMore.text(loadMoreText);
                    //increment the current status
                    start += desiredPosts;
                    //add in the new posts
                    postHandler(responseJSON);
                },
                //failure class
                error: function() {
                    //reset the message
                    loadMore.text('Error: There are no more posts to load.');
                },
                //complete event
                complete: function() {
                    //remove the spinner
                    loadMore.removeClass('activate');
                    //destroy stickem
                    //sticky.destroy();
                    //$('.js-ad-200-big').removeClass('stickit-end').addClass('stickit');
					//$('.ad-300x250').removeClass('stickit-end').addClass('stickit');
                    /* $('.stickem-container').stickem({
                      offset: 60,
                      start: 147
                    });
					 $('.stickem-container-videos').stickem({
                      offset: 0,
                      start: 420
                    }); */
                }
            });
        });	
		
		//Lock in top navigation after campaign loads.
		//setTimeout(function() {lockNavigation()}, 0);
    });
</script>

<script>
    var sliderMantleVideo = new Swipe( document.getElementById('slider__mantle-video'), {
      callback: function(index, elem) {
        i = videos.length;
        while (i--) {
          id = $(videos[i]).attr('id');
          $('#'+id).html(videosContent[i]);
        }
      }
    });
    var sliderSurferGuide = new Swipe( document.getElementById('slider__surfer-guide'));
    var sliderMagazine = new Swipe( document.getElementById('slider__magazine'));
    var sliderMantleImage = new Swipe( document.getElementById('slider__mantle-image'), {
      auto: 4000
    });
    var sliderFeaturedPosts = new Swipe( document.getElementById('slider__featured-posts'), {
      auto: 3000,
      continuous: true,
      callback: function(index, elem) {
        var i = bullets.length;
        while (i--) {
          bullets[i].className = ' ';
          bullets[i].setAttribute("onclick","sliderFeaturedPosts.slide("+i+")");
        }
        bullets[index].className = 'on';
        }
    });
    if($('.js-mantle-video .swipe-wrap').length != 0) {
      var videos = $('#slider__mantle-video .swipe-wrap div');
      var videosContent = $('#slider__mantle-video .swipe-wrap div a');
    }
    if($('#bullets').length !== 0) {
      var bullets = document.getElementById('bullets').getElementsByTagName('li');
    }
</script>

