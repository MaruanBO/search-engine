        $(function(){
            $('#rtb-carousel').on('swipeleft', function() {
                if($(window).innerWidth() >= 768){
                    $(this).carousel('next');
                }
            })
            $('#rtb-carousel').on('swiperight', function() {
                if ($(window).innerWidth() >= 768) {
                    $(this).carousel('prev');
                }
            });
        });

    </script>

        
        
