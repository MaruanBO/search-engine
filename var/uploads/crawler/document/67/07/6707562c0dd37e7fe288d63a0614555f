<script>
	//This is a way to juggle the problem to know who happened before, if the ready or the sdk callback.
	var websdkCallbackHappenedFirst = false;

    window.incomingAamAudCookie = jQuery.cookie("aam_aud");
        try {
            var envVar="prod";

            window.webAnalyticsSDKTracker = new intuit.web.analytics();

                if (typeof(xhrData) == "string") {
                    xhrData = JSON.parse(xhrData);
                }

                analyticsUtility.setEnvironmentSuffix(envVar, xhrData);
                analyticsUtility.setAppStack("EMS","2.0");
                analyticsUtility.setEndpoints(xhrData);

                window.webAnalyticsSDKTracker.init(xhrData);

                // since landing pages may be created with test and target, we cannot beacon this early in the page lifecyle
                if (analyticsUtility.getIsLP() !== true) {
                    analyticsUtility.triggerEvent("load_screen_success", []);
                }
                websdkCallbackHappenedFirst = true;

        }catch(ex){
            //console.dir(ex);
        }
        
    $(function () {

		
		//If the callback and therefore the listeners setup happened first, we need to reattach the listeners
		//as we do not know which ones were not attached.
		if (websdkCallbackHappenedFirst) {
			analyticsUtility.resetListeners();
		}
		analyticsUtility.triggerEvent("doubleClick_load_screen", []);

        if (jQuery && jQuery.cookie) {
            var aam_aud_cookie = jQuery.cookie("aam_aud") ? jQuery.cookie("aam_aud") : "";
            var incoming_aam_aud_cookie = window.incomingAamAudCookie ? window.incomingAamAudCookie : "";
            // If the incoming aam_aud cookie is different than the aam_aud sent from websdk, then beacon it
            if (incoming_aam_aud_cookie !== aam_aud_cookie) {
                analyticsUtility.triggerEvent("incoming_segment_different_event", []);
            }
        }

        if (intuit.web.analytics && jQuery && jQuery.cookie) {
            var priorityCodeUrlParam = intuit.web.analytics.prototype.util.getQueryStrParam("priorityCode");
            var priorityCodeCookie = jQuery.cookie("priorityCode") ? jQuery.cookie("priorityCode") : "";
            var priorityCode = priorityCodeUrlParam ? priorityCodeUrlParam : priorityCodeCookie;

            if (window.incomingPriorityCode !== "" && window.incomingPriorityCode !== priorityCode) {
                analyticsUtility.triggerEvent("incoming-priority-code-different-event", []);
            }
        }

    });
</script>

