var browserMake = '';
function isBrowserCompatible(){
if($j.browser.msie){
browserMake = 'IE';
if($j.browser.version < 8){
return false;
}
}
if($j.browser.mozilla){
browserMake = 'Firefox';
if($j.browser.version < 5){
return false;
}
}
if($j.browser.opera){
browserMake = 'Opera';
if($j.browser.version < 10.5){
return false;
}
}
if($j.browser.safari){
browserMake = 'Safari';
if($j.browser.version < 400){
return false;
}
}
return true;
}
function hide_brch_banner(){
setCookie('hbck', '1', null, '/');
$j('#browser_compat_warning').hide();
return false;
}
