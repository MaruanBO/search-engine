	<link rel="stylesheet" type="text/css" href="//homepath-activedt.netdna-ssl.com/lib/jquery-ui-1.8.19.custom/css/smoothness/jquery-ui-1.8.19.custom.css" />
	<script type="text/javascript" src="//homepath-activedt.netdna-ssl.com/lib/jquery-ui-1.8.19.custom/js/jquery-ui-1.8.19.custom.min.js"></script>
	
	<script type='text/javascript'>
	var bootstrap = {};
	bootstrap.appPath = "";
	
	function initLocationAutoComplete(){
		$j( "#oneBoxSearchInp, #oneBoxSearchInp2" ).autocomplete({
			source: function( request, response ) {
				$j.ajax({
					url: bootstrap.appPath+"/location/select",
					dataType: "json",
					data: {
						limit: 10,
						q: request.term
					},
					success: function( data ) {
						if(data != null){
							response( $j.map( data, function( item ) {
								return {
									label: item.label,
									value: item.label 
								}
							}));
						}
					}
				});
			},
			minLength: 3,
			select: function( event, ui ) {
				
			},
			open: function() {
				$j( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
			},
			close: function() {
				$j( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
			}
		});
	}
	
	$j(document).ready(function(){ 
		initLocationAutoComplete();
		
		$j("#searchOneBox2").click(function() {
			if ($j("#oneBoxSearchInp2").val() == "Address, County or City, State or Zip Code(s) or MLS#"){
				if( $j('#src_ref').val() == "" && $j('#mlsid').val() == ""){
					$j("#errorOneboxSearch2").text("Please enter Address, County or City, State or Zip Code(s) or MLS#.");
					return false;
				} else {
					$j("#oneBoxSearchInp2").val("");
					fixZipsField();
					return true;
				}
			}else{
				fixZipsField();
				return true;
			}
		});	
	});
</script> 




<script type='text/javascript'>	
	$j(document).ready(function(){	
		$j("#searchOneBox").click(function() {
			if ($j("#oneBoxSearchInp").val() == "Address, County or City, State or Zip Code(s) or MLS#"){
				$j("#errorOneboxSearch").text("Please enter Address, County or City, State or Zip Code(s) or MLS#.");
				return false;
			}else{
				return true;
			}
		});
		
	});
		
		function fixZipsField(){
			var zips = document.getElementById('hp_zip').value.split(/[\s,\n]+/);
			$j('#hp_zip').val("");
			$j("#advancedSearchForm").append("<input type='hidden' name='zzs' value='1'/>");
			$j.each(zips, function(i, item){
				$j("#advancedSearchForm").append("<input type='hidden' name='z' value='" + item + "'/>");
			});
		}
	
	
	function clearBlur(input, valInput){
		if($j(input).val() == ''){
			$j(input).val(valInput);
		}
	}
	
	function clearClick(input, valInput){
		if($j(input).val() == valInput){
			$j(input).val('');
			$j("#errorOneboxSearch").text("");
		}
	}
	
	function expandMultipleZip(){
		$j('#hp_zip').height(80);
		$j('#expandMultipleZip').hide();
		$j('#decreaseMultipleZip').show();
	}
	
	function decreaseMultipleZip(){
		$j('#hp_zip').height(14);
		$j('#expandMultipleZip').show();
		$j('#decreaseMultipleZip').hide();
	}
</script>

<script type="text/javascript">
	$j(document).ready(function(){ 
		loadFeatureListing();
	});
	
	(function($j){
		$j.fn.shuffle = function() {
			return this.each(function(){
				var items = $j(this).children();
				return (items.length) ? $j(this).html($j.shuffle(items)) : this;
			});
		}
		
		$j.shuffle = function(arr) {
			for(var j, x, i = arr.length; i; j = parseInt(Math.random() * i), x = arr[--i], arr[i] = arr[j], arr[j] = x);
			return arr;
		}
	})(jQuery);
	
	function loadFeatureListing(){
		$j.getJSON("/featuredListings.json",   // url
	             { 
	             	type: "homepath",
	             	ci: "-",
	             	st: "",
	             	zip: "-"
	             },   // request params
	             function(json){        // callback
	                var options = '';
					var countItem = 0;
					var siteLanguage = 'en';
	                $j(json.list).each(function(i, item) {
	                     if(item.length != 0){
	                     	var countItem = item.featureListing.length;
	                     }
	                     
	                     if(countItem >= 4){
		                    var sh = item.featureListing;  
		                     $j.each(sh, function(i, item) {
		                     	var last = '';
		                     	if(i == "4" || i == "9" || i == "14" || i == "19" || i == "24" || i == "29" || i == "34" || i == "39" || i == "44" || i == "49"){
		                     		last = "last";
		                     	}
		                     	
		                     	if ( i <= 4 ) {
			                     	options = options + '<div class="featured_home '+ last +'" style="width: 160px; height: 168px;">'; 
			                     	options = options + '<div style="border: 1px solid #C8C8C8; width: 155px; height: 162px; overflow: hidden;">'; 
			                     	options = options + '<div><a style="display:block; height:100px;" href="/listing?listingid=' + item.listingId + '"><img src="//homepath-activedt.netdna-ssl.com/homepathlistingphoto.html?thumbnail=true&w=155&h=100&s=' + item.lse + '&pse=' + item.pse + '"/></a></div>';
			                     	options = options + '<div class="featured_home_desc" style="height: 51px; font-size: 12px; padding: 6px; line-height: 16px; background: #ffffff URL(//homepath-activedt.netdna-ssl.com/css/cobrands/homepath/featured_home_desc.gif) bottom left repeat-x; white-space: nowrap; overflow:hidden;">'; 
			                     	options = options + '<div style="color: #01467F; font-weight: bold; font-size: 12px; margin-bottom: 2px;"><span style="text-transform: capitalize; width:100px;" class="fhcity">'+ item.city + '</span>, <span>' + item.state.toUpperCase() + '</span></div>'; 
			                     	options = options + '<div style="margin-bottom: 2px;">';
			                     	if(typeof item.beds !== '' && item.beds !== null && typeof item.beds != 'undefined'){
			                     		if(siteLanguage == 'es'){
			                     			options = options + ' ' + item.beds + ' dorm ';
			                     		}else{
			                     			options = options + ' ' + item.beds + ' bd ';
			                     		}
			                     	}
			                     	if(typeof item.baths !== '' && item.baths !== null && typeof item.baths != 'undefined'){
			                     		options = options + ' / ';
										options = options + ' ' + item.baths + ' ba ';
									}
									options = options + '</div>';
									if(typeof item.price !== '' && item.price !== null && typeof item.price != 'undefined'){
										options = options + '<span style="float:right; font-weight: bold; color: #ED0D27;">'+ item.price + '</span>';
									}
									options = options + '<div>';
									if(typeof item.size !== '' && item.size != '' && typeof item.size != 'undefined' && item.size != null ){
										if(siteLanguage == 'es'){
											options = options + ' '+ item.size +' p/c ';
										}else{
											options = options + ' '+ item.size +' sq.ft ';
										}
									}
									options = options + '</div>';
			                     	options = options + '</div>';
									options = options + '</div>';
									options = options + '</div>';
								}
								
		                	});
		                	
		                	if(countItem != 5 && countItem <= 4){
								var ads=countItem;
								for (ads=countItem;ads<=4;ads++){
							 		options = options + '<div class="featured_home last" style="width: 160px; height: 168px;">';
									options = options + '<img src="//homepath-activedt.netdna-ssl.com/css/cobrands/homepath/hp1_155x162.gif" style="margin-left: 2px; margin-top: 2px;">';
									options = options + '</div>';
								}
							}
							
	                	}else{
							$j("#conFeaturedHomes").hide();
	                	}
	                });
	               
	              $j("#showFeaturedListings").html(options);
	             }
	          );
          }
</script>




