	$j(document).ready(
		function() {
			$j('#banners').cycle({
				fx : 'fade',
				speed : '700',
				timeout : 10000,
				pause : 1,
				pager : '#pager'
			});
			
			$j('#pause-resume').click(function(){
				if($j(this).hasClass('paused')){
					$j('#banners').cycle('resume');
					$j(this).removeClass('paused');
					$j('#pager-pause-button').show();
					 $j('#pager-resume-button').hide();
				} else {
					$j('#banners').cycle('pause');
					$j(this).addClass('paused');
					$j('#pager-pause-button').hide();
					 $j('#pager-resume-button').show();
				}
			});
	
			$j("#calculator1").fancybox({
				'width'			: '7',
				'height'			: '6',
