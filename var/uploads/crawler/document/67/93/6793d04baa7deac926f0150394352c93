<script type="text/javascript">
	var loginURL='';
	function user_login(backUrl)
	{
		show_gray_bg();
		loginURL=backUrl;
		var l=document.getElementById('login');
		l.style.display="block";
		document.getElementById('email').focus();
		center_element(l,100,220);
		return false;
	}
	function user_login_restore()
	{
		var arr=Array('email','pwd','space','submit');
		for(i=0;i<arr.length;i++) document.getElementById(arr[i]+'_tr').style.display='';
		document.getElementById('resetend_tr').style.display='none';
		
		document.getElementById('login_error').innerHTML='<a href="#" onclick="user_login_prereset();return false">Forgot your password?</a>';
		
		var f=document.forms.loginform;
		f.email.value=f.pwd.value='';
		f.upload.value="Login";
		f.upload.onclick=user_login_submit;
	}
	function user_login_cancel()
	{
		hide_gray_bg();
		user_login_restore();
		document.getElementById('login').style.display="";
		return false;
	}
	function user_loading()
	{
		document.getElementById('resetend_tr').style.display='';
		document.getElementById('resetend_td').innerHTML='<br><br><br><table><tr><td><img src="/img/icn_loading_animated.gif" width="20" height="20" style="vertical-align:bottom"></td><td class="compl-text">&nbsp;Loading, please wait...</td></tr></table>';
		document.getElementById('login_error').innerHTML='';
		var arr=Array('email','pwd','space','submit');
		for(i=0;i<arr.length;i++) document.getElementById(arr[i]+'_tr').style.display='none';
	}
	function user_loading_restore()
	{
		document.getElementById('resetend_tr').style.display='none';
		document.getElementById('resetend_td').innerHTML='';
		document.getElementById('login_error').innerHTML='<a href="#" onclick="user_login_prereset();return false">Forgot your password?</a>';
		var arr=Array('email','pwd','space','submit');
		for(i=0;i<arr.length;i++) document.getElementById(arr[i]+'_tr').style.display='';
	}
	function user_login_reset()
	{
		var f=document.forms.loginform;
		if(f.email.value.length==0)
		{
			document.getElementById('login_error').innerHTML='The email address you entered has not been registred';
		}else{
			user_loading();
			
			var data=mainPOSTRequest('/','resetlogin=true&email='+f.email.value,'',user_login_ie);
		}
	}
	function user_login_ie(data)
	{
		show_gray_bg();
		if(data=='DONE')
		{
			document.getElementById('resetend_td').innerHTML='<br><br><br><table><tr><td class="compl-text">Your password has been sent to the specified email address | <a href="#" onclick="user_login_restore();return false">Login</a></td></tr></table>';
			document.getElementById('login_error').innerHTML='';
		}else{
			var arr=Array('email','space','submit');
			for(i=0;i<arr.length;i++) document.getElementById(arr[i]+'_tr').style.display='';
			document.getElementById('resetend_tr').style.display='none';
			document.getElementById('login_error').innerHTML='The email address you entered has not been registred';
		}
	}
	function user_login_prereset()
	{
		var f=document.forms.loginform;
		document.getElementById('pwd_tr').style.display='none';
		document.getElementById('login_error').innerHTML='';
		
		f.upload.value="Reset";
		f.upload.onclick=user_login_reset;
	}
	function user_login_submit()
	{
		var f=document.forms.loginform;
		var data='';
		if(f.email.value.length>0 && f.pwd.value.length>0)
		{
			user_loading();
			mainPOSTRequest('/','submitlogin=true&email='+encodeURIComponent(f.email.value)+'&pwd='+encodeURIComponent(f.pwd.value),'',user_login_submit_response);
		}
	}
	function user_login_submit_response(data)
	{
		var f=document.forms.loginform;
		
		if(data=='DONE')
		{
			user_login_cancel();
			if(typeof(loginURL)!='undefined' && loginURL.length) 
				location.href=loginURL;
			else
				location.reload();
		}else{
			show_gray_bg();
			user_loading_restore();
			document.getElementById('login_error').innerHTML='Incorrect Email or Password <span class=small>|</span> <a href="#" onclick="user_login_prereset();return false">Reset your password</a>';
		}
	}
</script>
<img src="/img/icn_loading_animated.gif" style="width:20px;height:2px;display:none" alt="loading">
