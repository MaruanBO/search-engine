    <script type="text/javascript">
        $(document).ready(function () {
            $("#indSel").sb({ maxHeight: 325 });
            $("#indSel").change(function () { ChangeIndustry(); });

            $.each($('.trial_form :input'), function () {
                $(this).attr("name", $(this).attr("id"));
            });

            /*for awards*/
            divs = ['d1', 'd2', 'd3'];
            function hideDivs() {
                for (var i = 0; i < divs.length; i++)
                    document.getElementById(divs[i]).style.display = 'none';
            }
            function showDiv() {
                hideDivs(); //hide them all before we show the next one.
                var randomDiv = divs[Math.floor(Math.random() * divs.length)];
                var div = document.getElementById(randomDiv).style.display =
                'block';

            }

            showDiv();



        });

        function sendgaevttrack(cat, act, lbl) {
            dataLayer.push({ 'event': 'GAevent', 'eventCategory': cat, 'eventAction': act, 'eventLabel': lbl });
        }
        function FormIsValid() {
            var ErrorMessages = new Array();
            var sError = "";
            if ($("#FirstName").val().length < 1) {
                ErrorMessages.push("First Name is required");
                $("#FirstName").attr("class", "textfielderror");
                sendgaevttrack("form errors", "homepage", "First Name");
            }

            if ($("#FirstName").val().length > 1) {
                $("#FirstName").attr("class", "");
            }

            if ($("#LastName").val().length < 1) {
                ErrorMessages.push("Last Name is required");
                $("#LastName").attr("class", "textfielderror");
                sendgaevttrack("form errors", "homepage", "Last Name");
            }

            if ($("#LastName").val().length > 1) {
                $("#LastName").attr("class", "");
            }

            if (!validateEmail($("#Email").val())) {
                ErrorMessages.push("Invalid Email");
                $("#Email").attr("class", "textfielderror");
                sendgaevttrack("form errors", "homepage", "Email");
            }

            if (validateEmail($("#Email").val())) {
                $("#Email").attr("class", "");
            }

            if (!IsValidPassword($("#Password").val())) {
                ErrorMessages.push("Password must contain at least 8 characters, at least 1 number, 1 upper case, and 1 lower case letter.");//TODO
                $("#Password").attr("class", "textfielderror");
                sendgaevttrack("form errors", "homepage", "Password");
            }

            if (IsValidPassword($("#Password").val())) {
                $("#Password").attr("class", "");
            }

            if (ErrorMessages.length > 0) {
                sError = "<strong>There was an error submitting your form:</strong> <br /><br />";
                for (var i = 0; i < ErrorMessages.length; i++)
                    sError += "- " + ErrorMessages[i] + "<br />";
            }

            return sError;
        }

        function IsValidPassword(password) {
            var re = /^.*(?=.{8,})(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).*$/;
            return re.test(password);
        }


        function validateEmail(email) {
            var kk = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
            return kk.test(email);
        }

        function OnClientSubmit() {
            var sErr = FormIsValid();
            if (sErr != "") {
                $("#ErrorText").html(sErr);
                    $("#ErrorBox").show();
                    return false;
                }

                $("#RegSubmitButton").hide();
                $('#spnWaitText').show();
                $('#spnWaitText').css("display", "block");
                return true;
        }
        //Change the industry, set cookie, redirect to industry microsite
        function ChangeIndustry() {

            var sel = $('#indSel').sb("getSelected");
            

            if (sel.val() == 'Chooseyourindustry')
                return;
            createCookie('ind_main', sel.val(), 604800000);
            var currLocation = document.location.href.replace("index.aspx", "");            

            /*Redirect Code*/
            //if live site, we use pretty redirect, URL Rewrite
            currLocation = window.location.protocol + "//" + document.location.host;
            window.location.href = currLocation + "/industries/" + sel.val() + "?src=direct&v=e&cat=1";
        }
    </script>

