//This function is used to set the pickup and return date when user clicks on Reserve with CustomerId on home page
//It does a form submit to get the pick up and return date at the backend.
function setPickupAndReturnDate()
{

	if(document.reservationForm == null){
		var newCustomerIdForm = document.createElement("form");
		newCustomerIdForm.setAttribute("method", "POST");
		newCustomerIdForm.setAttribute("action", "/budgetWeb/reservation/initializer.ex");
		document.body.appendChild(newCustomerIdForm);
		newCustomerIdForm.submit();
		return true;
	}	
	if(null != document.reservationForm.pulDesc){
	var chkPul = document.reservationForm.pulDesc.value;
		if(chkPul.indexOf("zip or airport") != -1){
		document.reservationForm.pulDesc.value = "";
		}
	}
	if(null != document.reservationForm.dolDesc){
	var chkDol = document.reservationForm.dolDesc.value;
		if(chkDol.indexOf("zip or airport") != -1){
		document.reservationForm.dolDesc.value = "";
	 }
	}
    if(null != document.getElementById("chkReturn")){
	if(!document.getElementById("chkReturn").checked){
		document.reservationForm.dolDesc.value = document.reservationForm.pulDesc.value;	}
	}
	if(null != document.reservationForm.txtPUDate){
	var newPickupDate = document.reservationForm.txtPUDate.value;
	}
	if(null != document.reservationForm.txtDODate){
	var newReturnDate = document.reservationForm.txtDODate.value;
	}
	if(null != document.reservationForm.puHourMinute){
	var newPuHrMin = document.reservationForm.puHourMinute.value;
	}
	if(null != document.reservationForm.doHourMinute){
	var newDoHrMin = document.reservationForm.doHourMinute.value;
	}
	if(null != document.reservationForm.pulDesc){
	var newPulLoc = document.reservationForm.pulDesc.value;
	}
	if(null != document.reservationForm.dolDesc){
	var newDolLoc = document.reservationForm.dolDesc.value;
	}
	var customerIdForm = document.createElement("form");
    customerIdForm.setAttribute("method", "POST");
    customerIdForm.setAttribute("action", "/budgetWeb/reservation/initializer.ex");
	var actionparam = document.createElement("input");
    actionparam.setAttribute("type", "hidden");
    actionparam.setAttribute("name", "actionParam");
    actionparam.setAttribute("value", "reservewithCustId");
    customerIdForm.appendChild(actionparam);
    var pickDtHidden = document.createElement("input");
    pickDtHidden.setAttribute("type", "hidden");
    pickDtHidden.setAttribute("name", "newPickupDate");
    pickDtHidden.setAttribute("value", newPickupDate);
    customerIdForm.appendChild(pickDtHidden);
    var retDtHidden = document.createElement("input");
	retDtHidden.setAttribute("type", "hidden");
    retDtHidden.setAttribute("name", "newReturnDate");
    retDtHidden.setAttribute("value", newReturnDate);
    customerIdForm.appendChild(retDtHidden);
	var puHrMin = document.createElement("input");
	puHrMin.setAttribute("type", "hidden");
    puHrMin.setAttribute("name", "newPuHrMin");
    puHrMin.setAttribute("value", newPuHrMin);
	customerIdForm.appendChild(puHrMin);
	var doHrMin = document.createElement("input");
	doHrMin.setAttribute("type", "hidden");
    doHrMin.setAttribute("name", "newDoHrMin");
    doHrMin.setAttribute("value", newDoHrMin);
	customerIdForm.appendChild(doHrMin);
	var newPuHid = document.createElement("input");
	newPuHid.setAttribute("type", "hidden");
    newPuHid.setAttribute("name", "newPulLoc");
    newPuHid.setAttribute("value", newPulLoc);
	customerIdForm.appendChild(newPuHid);
	var newRtHid = document.createElement("input");
	newRtHid.setAttribute("type", "hidden");
    newRtHid.setAttribute("name", "newDolLoc");
    newRtHid.setAttribute("value", newDolLoc);
	customerIdForm.appendChild(newRtHid);
	document.body.appendChild(customerIdForm);
    customerIdForm.submit();
	
}
