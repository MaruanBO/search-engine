<script type="text/javascript">
if(typeof(autoRedirectOnTimeout) != "undefined" && autoRedirectOnTimeout != ''){
    function timeOutAutoRedirect(){
	var url;
	if(autoRedirectOnTimeout == "reservationTimeout"){  	
       url ="/budgetWeb/reservation/timeout.ex?flow=reservation&timeoutRedirect=true";
	   }
	else if(autoRedirectOnTimeout == "profileTimeout") {
	  url="/budgetWeb/reservation/timeout.ex?flow=profile&timeoutRedirect=true";
	  }
         
	else {
		url="/budgetWeb/reservation/timeout.ex?flow=home";
		}
		window.location = url;
    }
	
	
    var oneMinute = 60 * 1000;
    setInterval("timeOutAutoRedirect()",28*oneMinute);  
    }
</script> 


