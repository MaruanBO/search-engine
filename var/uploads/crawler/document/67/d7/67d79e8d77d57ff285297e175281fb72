	$ = jQuery.noConflict();
	jQuery(document).ready( function() {
		jQuery(".utinavcl-second .utinavspn img").click(function(){
			jQuery(".utinavcl-second").hide();
		});

		jQuery(".clpopup").click(function(){
			jQuery(".utinavcl-second").show();
			jQuery(".utinavcl-second").hide();
			jQuery(".utinavcl-second").show();
		});
		
		jQuery("a.searchButton img").click(function(){
			jQuery(this).attr('src','/budgetWeb/images/dashboard-images/budget/search_fld.png')
			jQuery("#question_box").show();
			jQuery(".mainMenu .search").css('right','5px');
			jQuery(".mainMenu .search a.searchButton").css('right','9px');	
		});


		if (navigator.userAgent.indexOf('MSIE') > -1 || navigator.appVersion.indexOf('Trident/') > 0) {
        jQuery(".txtplaceholder").each(function() {                                  
            var input = jQuery(this);
            jQuery(input).val(input.attr('placeholder'));
            jQuery(input).focus(function() {
				if (input.val() == input.attr('placeholder')) {
					input.val('');
                }
            });

            jQuery(input).blur(function() {
                    if (input.val() == '' || input.val() == input.attr('placeholder')) {
                         input.val(input.attr('placeholder'));
                     }
            });
        });
			
		jQuery('#pwordClear').show();
		jQuery('#pword').hide();
		jQuery('#pwordClear').focus(function() {
			jQuery('#pwordClear').hide();
			jQuery('#pword').show();
			jQuery('#pword').focus();
		});
		jQuery('#pword').blur(function() {
			if(jQuery('#pword').val() == '') {
				jQuery('#pwordClear').show();
				jQuery('#pword').hide();
			}
		});
}


	});	
