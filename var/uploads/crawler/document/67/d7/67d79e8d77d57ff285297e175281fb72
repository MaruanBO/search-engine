function XSSSafe(str){

var Regex = new RegExp("^.*(>|<|(&lt;)|(&gt;)|(%3E)|(%3C)|(script)|(onclick)|(onmouseover)|(onmouseout)|(onblur)|(onerror)|(onfocus)|(onload)|(onunload)|(onsubmit)|(onreset)|(onabort)|(onevent)|(//)|(%2F)|(&#47;)|(redirect)).*$");


 if(Regex.test(str)){
            
           return false;


}else{
            return true;
}


}
						
					
						
						
						
						function trimString (str) {
                          str = this != window? this : str;
                          return str.replace(/^\s+/g, '').replace(/\s+$/g, '');
                        }

                        // This function is to submit the form when user clicks on "Enter"
                        function submitenter(e)
                        {
                                var keycode;
                                if (window.event) {
                                        keycode = window.event.keyCode;
                                } else if (e) {
                                        keycode = e.which;
                                } else {
                                        return true;
                                }
                                if (keycode == 13)
                            {
                                   submitSearch();
                                   return false;
                            }
                            else
                                   return true;
                        }

                      function submitSearch(obj)
                        {
							var query= document.getElementById("question_box").value;
							query = trimString(query);

							if(!XSSSafe(query)){
								document.getElementById("question_box").value='';
								return false;
							}
					
							if (query == null || query.length==0) {
							
								document.getElementById("question_box").value='search budget.com';
							}else if(query != null){							
								doInquiraTrackingEvent(query, obj);
								delete_cookie('SIGNOUT_COOKIE');
								if(query == '')
								query = 'search budget.com';
								location="/budgetSearch/sitesearch/performSearch.ex?isSearchSelect=yes&ui_mode=question&charset=UTF-8&language=en-US&site=Budget&searchtype=WebSearch&question_box="+query;
							}
                               
                        }
                                             
                        function doInquiraTrackingEvent(kw, obj)
                        {
                                s.linkTrackEvents='event17';
                                s.linkTrackVars='events,eVar29,prop3,prop30';
                                s_linkName=s.pageName+' '+'INQUIRA';
                                s.eVar29=kw;
                                s.events='event17';
                                s.prop3=kw+' - '+s.pageName;    
                                s.prop30='SEARCH: '+kw+' '+s.pageName;
                                s.tl(this,"o",s_linkName);
                        }

                        

                        function clickSearchBox()
                        {
                                document.getElementById("question_box").value='';
                        }

                        function delete_cookie ( cookie_name )  {
                                var cookie_date = new Date ( );  // current date & time
                                cookie_date.setTime ( cookie_date.getTime()+(-1*24*60*60*1000) );
                                // fix for 4263 clear the home page cookie
                                var expires = "; expires="+cookie_date.toGMTString();
                                document.cookie = name+"="+expires+"; path=/";
                        }

                  function delete_signout_cookie (name) {
                          //fix to clear sign out cookie for site search
                          var cookie_date = new Date ( );  // current date & time
                                cookie_date.setTime ( cookie_date.getTime()+(-1*24*60*60*1000) );
                                var expires = "; expires="+cookie_date.toGMTString();
                                document.cookie = name+"="+expires+"; path=/";
                        }
                   
                        function checkCookieForBackButton() {
                                var getcookie = document.cookie;
                                getcookie = unescape(getcookie);
                                documentCookieValues = getcookie.split(";")
                                documentCookieValues1 = getcookie.split(";")

                                // fix for 4263 clear the home page cookie
                                

                                for(i=0;i<documentCookieValues.length;i++){
                                        if(documentCookieValues[i].indexOf('SIGNOUT_COOKIE=')!=-1 ){
                                                for(j=0;j<documentCookieValues1.length;j++){
                                                        if(documentCookieValues1[j].indexOf('HomePage=')!=-1 ){
                                                                
                                                                        window.location = "/budgetWeb/home/home.ex?action=logout&history=true";
                                                                
                                                        }
                                                }
                                        }
                                }
                                delete_cookie('HomePage');
                        }
          
                        
