<script>

function cleardata(){
	var recognized = 'false';
	
	if(recognized == null || recognized == '' || recognized == 'false' || recognized == false){
		document.reservationForm.budgetComId.value = '';
		
	}
	if(document.reservationForm.passcode){
		document.reservationForm.passcode.value = '';
	} 
	if(document.reservationForm.rentFavCar){
		document.reservationForm.rentFavCar.checked = false;
	}
} 
function signInLink(){
	if(document.reservationForm.signClick){
	document.reservationForm.signClick.value = 'true';
	}
}

function closeSignInModalWindow() {
			document.getElementById('dialogForHomePage').style.display= "none";  
			document.getElementById('maskSignIn').style.display="none"; 
			
		}
		
function removeBorder(fieldId,valErrorFieldId){
  document.getElementById(fieldId).style.border = "1px solid #9CBBD2";
  document.getElementById(fieldId).style.background=""; 
  if( document.getElementById(valErrorFieldId)){
	var valErrorStyle = document.getElementById(valErrorFieldId).className;
	if( valErrorStyle == "fieldBlock"){
		document.getElementById(valErrorFieldId).className="fieldNone";
	} 
  }
}

 function validateDataOnBlur(form,fieldName){
			
            var url = '/budgetWeb/home/ajaxValidationAction.ex?fieldName='+fieldName;
            var data = $(form).serialize()
		
            $.ajax({
                type: "POST",
                url: url,
                data: data,
                success: function (resp) {
					var result = resp;
					var errorFieldId =fieldName+"ValError";
					if(result != ""){
					//	document.getElementById(errorFieldId).style.display = "block";
						document.getElementById(errorFieldId).innerHTML = result;
						document.getElementById(errorFieldId).className = "fieldBlock";
						document.getElementById(fieldName).style.backgroundColor = "#fff3dc";
						document.getElementById(fieldName).style.border = "1px solid #D4002A";
						
					}
                   
                },
                error: function (xhr, status, error) {
 
					//alert('Error');
                }
        });
		
} 
</script>

