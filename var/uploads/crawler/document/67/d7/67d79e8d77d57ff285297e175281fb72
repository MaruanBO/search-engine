	<script type="text/javascript">

	var countryLanguage = [];

	function initCountrySelect()
	{
		//
		// process country select and compress into 1 country select
		// for each supported language
		//

		// first sort them
		var mylist = jQuery('#ddCountry');
		var listitems = mylist.children('option').get();
		listitems.sort(function(a, b) {
		   var compA = jQuery(a).html().toUpperCase();
		   var compB = jQuery(b).html().toUpperCase();
		   return (compA < compB) ? -1 : (compA > compB) ? 1 : 0;
		});
		jQuery.each(listitems, function(idx, itm) { mylist.append(itm); });

		// get the updated sorted list
		listitems = mylist.children('option').get();

		// now clear the list and only add each unique country
		mylist.html('<option value="">select</option>');

		// process the newly sorted list and dump each country's
		// supported language into its own bucket
		var currentCountry = null;
		for (var i=0; i < listitems.length; i++)
		{
			var tmp = (listitems[i].innerHTML).replace(" ", "_");
			if (currentCountry == null || currentCountry != tmp)
			{
				currentCountry = tmp;

				countryLanguage[currentCountry] = [];

				mylist.append('<option value="' + currentCountry + '">' + listitems[i].innerHTML + '</option>');
			}

			countryLanguage[currentCountry].push([listitems[i].lang, listitems[i].value]);

			//alert(currentCountry + " + [" + (countryLanguage[currentCountry])[countryLanguage[currentCountry].length-1] + "]");
		}

		/*
			handle country select change
		*/
		jQuery('#ddCountry').change( function() {

				var selectedCountry = jQuery(this).val();

				if (selectedCountry != null && selectedCountry != "")
				{

					var langSelect = jQuery('#ddLanguage');
					langSelect.html('');

					// build out the language select based on
					// selected country
					var languages = countryLanguage[selectedCountry];

					for (var i=0; i < languages.length; i++)
					{
						langSelect.append('<option value="' + languages[i][1]+ '">' + languages[i][0] + '</option>');
					}

					// if more than 1 language, unhide the language drop down
					// and default the button to having no href and disable it
					if (languages.length > 1)
					{
						langSelect.prepend('<option selected="selected" value="">select</option>');
						langSelect.selectedIndex = 0;

						jQuery('#lblLanguage').css('display', '');
						jQuery('#btnContryLang').attr("href", "javascript:alert('Please select a language');");
						jQuery('#btnContryLang').attr('disabled', true);
						jQuery('#btnContryLang').attr("class", "counlang_grey_btn_arws");
						jQuery('#btnContryLang').removeAttr("target");

						langSelect.val('');
					}
					else
					{
						// this country only has 1 language and its
						// already selected in the language drop down
						// so just enable the continue button
						// after setting its href.
						jQuery('#lblLanguage').css('display', 'none');
						jQuery('#btnContryLang').attr('href', jQuery('#ddLanguage').val());
						jQuery('#btnContryLang').removeAttr('disabled');
						jQuery('#btnContryLang').attr("class", "orng_btn_arws_1");
						jQuery('#btnContryLang').attr("target", "_blank");
					}

					// enable the select
					langSelect.removeAttr('disabled');
				}
				else
				{
					jQuery('#btnContryLang').attr("href", "javascript:alert('Please select a country');");
					jQuery('#btnContryLang').attr("disabled", true);
					jQuery('#btnContryLang').attr("class", "counlang_grey_btn_arws");
					jQuery('#btnContryLang').removeAttr("target");
				}
		});

		/*
			handle language select
		*/
		jQuery('#ddLanguage').change( function() {

			if (jQuery(this).val() != null && jQuery(this).val() != "")
			{
				jQuery('#btnContryLang').attr('href', jQuery(this).val());
				jQuery('#btnContryLang').removeAttr('disabled');
				jQuery('#btnContryLang').attr("class", "orng_btn_arws_1");
				jQuery('#btnContryLang').attr("target", "_blank");
			}
			else
			{
				jQuery('#btnContryLang').attr("href", "javascript:alert('Please select a language');");
				jQuery('#btnContryLang').attr('disabled', true);
				jQuery('#btnContryLang').attr("class", "counlang_grey_btn_arws");
				jQuery('#btnContryLang').removeAttr("target");
			}
		});

		// unhide country select
		jQuery('#ddCountry').css('display', '');

	}

	initCountrySelect();

	</script>

</html><!-- START HomeRes.jsp -->












  



