// 0705-2959 
// This is replicated in all tracking files
var is_weekend = 0; 
var myDate=new Date(); 
strDate = myDate.toString(); 

if ( (strDate.toLowerCase().indexOf('sat') != -1) || (strDate.toLowerCase().indexOf('sun') != -1) ) 
{
	is_weekend = 1;
} 

else if(strDate.toLowerCase().indexOf('fri') != -1) 
{
	if (myDate.getHours() >= 17) 
		is_weekend = 1;
} 

// <!-- TC 3: Resconfirm number = -->

var optrial = new Object; 
optrial.isWeekend = is_weekend;

// <!-- TC 4: Resconfirm number = -->

