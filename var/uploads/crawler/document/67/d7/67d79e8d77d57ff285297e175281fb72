<!-- Start Big Boy Upgrade cookie handling code -->
<script type="text/javascript">

s.pageName="Home";      // set the page name for omniture

var discountCode = getURLParam(window.location.href, 'discountNumber');
var bcdCode = getURLParam(window.location.href, 'BCD');
var couponCode = getURLParam(window.location.href, 'CPN');

// test for a discount number on the url
if (discountCode !== '')
{
        // try and get the bbuDiscountcode url param (true of false)
        var isbbuDiscountCode = (getURLParam(window.location.href, 'isBBUDiscountCode')).toUpperCase();

        if (isbbuDiscountCode == 'TRUE')
        {
                // store a cookie for 1 year
                setcookie('bbuDiscountCode', discountCode, 365);                        
        }       
}
else
{
        // try a read in the big boy upgrade cookie
        var bbuDiscountCode = getcookie('bbuDiscountCode');

        // only use the cookied big boy code if no bcds are on the url
        if (bbuDiscountCode != null && bcdCode == '' && discountCode == '' && couponCode == '')
        {               
                // add the discount code to the BCD box on the page
                if (document.getElementById('discountNumber'))
                {                                       
                        document.getElementById('discountNumber').value = bbuDiscountCode;
                        document.getElementById('discountNumber').style.background="#fdbe66";
                }
                // add the discount code to the BCD box on the page
                if (document.getElementById('discountNumber1'))
                {                                       
                        document.getElementById('discountNumber1').value = bbuDiscountCode;
                        document.getElementById('discountNumber1').style.background="#fdbe66";
                }
                // add the discount code to the BCD box on the page
                if (document.getElementById('discountNumber2'))
                {                                       
                        document.getElementById('discountNumber2').value = bbuDiscountCode;
                        document.getElementById('discountNumber2').style.background="#fdbe66";
                }
                // add the discount code to the BCD box on the page
                if (document.getElementById('discountNumber3'))
                {                                       
                        document.getElementById('discountNumber3').value = bbuDiscountCode;
                        document.getElementById('discountNumber3').style.background="#fdbe66";
                }
        }
}

