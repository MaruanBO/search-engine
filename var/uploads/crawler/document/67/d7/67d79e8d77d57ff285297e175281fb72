<script>
	function retain() {
	document.reservationForm.pulDesc.value = document.forms[0].pickUpLocationVariable.value;
	document.reservationForm.dolDesc.value = document.forms[0].dropOffLocationVariable.value;
	}

	if(window.addEventListener) {
       window.addEventListener("load",doOnloadTasks,true);
	}
	
function doOnloadTasks(){
              
       var arr = document.getElementsByClassName("fieldError");
       

for(var i = 0; i<arr.length; i++){
       $(".fieldError").parent().find('input').css('border', '1px solid #d4002a');
       $(".fieldError").parent().find('input').css('backgroundColor', '#fff3dc');
		}
if(arr.length != 0){
	if(arr[0].parentNode.children[1].nodeName=="INPUT"){
   arr[0].parentNode.children[1].focus()
    }else{
    arr[0].parentNode.children[2].focus()
		}
}
}
</script>
<!-- Budget Release 5.3.13: Start -->
<script type="text/javascript">

//var optimostCreativeId = null;

jQuery(document).ready(function() 
{ 
	
	//Start - Budget Release 6.2.7 : Tracker 307 retain data on click of back button
	var retainDataOnBackButton =  document.reservationForm.retainDataOnBackButtonForHomepage.value;
       
	if(retainDataOnBackButton == 'true'){
		retain();
	}
	//End - Budget Release 6.2.7 : Tracker 307 retain data on click of back button
	$('#dropOffLbl, #dolDesc' ).css({'display':'none'});
	$('#cpn,#bcd').css({'display':'none'});

   //start of 6.4.1
	var dolCode = document.reservationForm.dolDesc.value;
  var pulCode = document.reservationForm.pulDesc.value;
        if(dolCode.length > 3){
    //Incase where dolDesc has complete location description then extract the location code
    dolCode = dolCode.substring(dolCode.length-3);
   }
   if(pulCode.length > 3){
    //Incase where pulDesc has complete location description then extract the location code
    pulCode = pulCode.substring(pulCode.length-3);
   }
	
    // end of 6.4.1
	//Start - Budget Release 6.2.7 : Tracker 307 retain data on click of back button
	
	if(retainDataOnBackButton == 'false'){


    if($('#pulDesc').val() != 'Airport, city, zip, address, attraction'){
		$('#pulDesc').val(document.reservationForm.pulDesc.value);
	}
    if($('#dolDesc').val() != 'Airport, city, zip, address, attraction'){
		$('#dolDesc').val(document.reservationForm.dolDesc.value);
    }
	}else{
		
		$('#pulDesc').val(document.reservationForm.pulDesc.value);
		$('#dolDesc').val(document.reservationForm.dolDesc.value);
	}
	//End - Budget Release 6.2.7 : Tracker 307 retain data on click of back button
	//var ischecked = $('#chkReturn').attr('checked');


	if($('#dolDesc').val() != 'Airport, city, zip, address, attraction' && $('#dolDesc').val()!='' && $('#dolDesc').val()!=null && dolCode!=pulCode){


		$('#dropOffLbl, #dolDesc' ).css({'display':''});
		$('#chkReturn').attr('checked','checked');
	} 
	else
	{
		$('#dropOffLbl, #dolDesc' ).css({'display':'none'});
		$('#chkReturn').removeAttr('checked');
	}

	if(($('#couponNumber').val() != '' && $('#couponNumber').val() != null) || ($('#discountNumber').val() != '' && $('#discountNumber').val() != null) ){
		$('#offerChk').attr('checked','checked');
		$('#cpn,#bcd').css({'display':''});
	}
	else
	{
		$('#offerChk').removeAttr('checked');
		$('#cpn,#bcd').css({'display':'none'});
	}
	
});


$('#chkReturn, #offerChk, #rentFavCar').click(function(){hasCheck(this.id);});
function hasCheck(checkId){
	var ischecked = $('#'+checkId).attr('checked');
	if(checkId == 'chkReturn'){
		if(ischecked == true){
			$('#dropOffLbl, #dolDesc' ).css({'display':'block'});
			$('#dolDesc').text($('dolDesc').text);
		}else{
			$('#dropOffLbl, #dolDesc' ).css({'display':'none'});
			$('#dolDesc').val('Airport, city, zip, address, attraction');
			$('#dolDesc').text('Airport, city, zip, address, attraction');
			$('#dolDescValError').attr('class','fieldNone');
			$('#dolDescError').attr('class','fieldNone');
			$('#dolDesc').css({'border':'1px solid #c2c2c2'});
			$('#dolDesc').css({'backgroundColor': '#FFFFFF'});
		}
	}else if(checkId == 'offerChk'){
		if(ischecked == true){			
			$('#cpn,#bcd,#multiInstanceCoupondiv').css({'display':''});
		}else{
			$('#cpn,#bcd,#multiInstanceCoupondiv').css({'display':'none'});
		}
	}	
	else if(checkId == 'rentFavCar'){
		if(ischecked == false){			
			$('#oneClickBtn').css({'display':'none'});
			$('#cntBtn').css({'display':'block'});
		}else{
			$('#cntBtn').css({'display':'none'});
			$('#oneClickBtn').css({'display':'inline-block'});
		}
	}
}
if(document.getElementById('cntnBtn')){
document.getElementById('cntnBtn').onclick = new Function("handleBcdCoupon();chkLocAndDate();hideBudgetMessageTag();");
}
if(document.getElementById('oneClickCntnBtn')){
document.getElementById('oneClickCntnBtn').onclick = new Function("handleBcdCoupon();chkLocAndDate();");
}

if(document.getElementById('continueAsGuest')){
document.getElementById('continueAsGuest').onclick = new Function("handleBcdCoupon();closeContinueMM();");
} 

/********************/
function killCoverDiv(){
 	var loadingCoverDiv=document.getElementById('loading');
	
 	if(loadingCoverDiv){
		//alert(loadingCoverDiv);;
 		loadingCoverDiv.style.display='none';
		loadingCoverDiv.style.visibility ='hidden';		
 		}
 	}
 	var timeout=setTimeout(function(){
 										killCoverDiv();},30000
 						  );
 		jQuery(document).ready(function(){
 			killCoverDiv();
 		});



/**
	This function takes care of the following: 
	1. used to show the datepicker on click of text/calender.
	2. disabling all dates prior to pick-up date in the return date calendar.
	3. Drop off date value should not be altered based on pick-up date change.
	4. One of the div tag issue with IE 8 browser
*/

/**
*	This function is called when user changes the date by selecting another date from the calender.
*	The function takes care of setting the user preferred date to respective pickup and drop of objects of *	the form to retain the value in the session.
*	It also takes care of setting the date field to deault value when user tries to enter a NaN.
*/
function hideBudgetMessageTag() {
	if(document.getElementById("errorDiv").visiblity="visible") {
		$('#messageDivErr').css({'display':'none'});
	}
}
function closeContinueMM(){
	if(document.getElementById('pulDescError').style.display != 'block'){
	$('#continueAsMember').css({'display':'none'});
	}	
}

