	//LRC UI elements
	jQuery(document).ready(function($){
		$("#menu-main-menu").prepend('<li id="menu-item-podcasts" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-mobile-only"><a href="http://www.lewrockwell.com/podcast/">Podcast</a></li>');
		$(".sub-menu").parent().addClass("accordion");
		$(".accordion > a").click(function(){ 
			if($(this).hasClass("active")){
				//if clicking on an open menu item, close its submenu
				$(".accordion > a").removeClass("active");
				$(".sub-menu:visible").slideUp("slow");
			}else{
				//if clicking on another menu item, close other submenus and open this one
				$(".accordion > a").not(this).removeClass("active");
				$(this).addClass("active");
				$(".accordion > a").not(this).not(".active").next(".sub-menu:visible").slideUp("slow");
				$(this).next(".sub-menu").slideDown("slow");
			}
		});	
		$(".nav").mouseleave(function(){
			//if mouse leaves navigation, close any open submenus
			$(".accordion > a").removeClass("active");
			$(".sub-menu:visible").slideUp("slow");
		});
		
		//Google search image placeholder
		$('input.google-search').keyup(function(){
			//console.debug("--" + $(this).val() + "--");
			if($(this).val()=="") $(this).css("background-image", "url(//www.google.com/cse/intl/en/images/google_custom_search_watermark.gif)");
			else ($(this).css("background-image",""));
		});
		$('input.google-search').keyup();
		$('.toggle-menu a').click(function(){
			toggleMenu();
		});
		function toggleMenu(){
			var button = $('.toggle-menu a');
			var callback = function(){
				button.text(($(this).is(':visible'))? 'Hide Menu': 'Show Menu');
			};
			$('#main-menu-container').slideToggle('400', callback);
		}
		if(jQuery.browser.mobile == true) $("body").addClass('mobile');
				$('.print-page').click(function(){
			if($(this).attr('href')=='#'){
				window.print();
				return false;
			}
		});
	});
</script>
