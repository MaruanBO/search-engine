<script type="text/javascript">
  $(document).ready(function(){
  	  /*========================================================================== 
	  
					  	@@@ Marketo Form Settings @@@
					  	
		  !Pass these ids into the removeFieldsArray variable to remove the field
		  
					#first_name - First Name
					#last_name - Last Name
					#email - Email
					#company - Company
					#phone - Phone Number
					#job_title - Job Title
					#which-product - Which Product?
					#countrySelect - Country dropdown
					#stateSelect - State dropdown
					#txtPostalCode - Zip Code
					
			
			!Product specific thank you page settings
					"General" - Generic
					"Event" - Event Management
					"Survey" - Survey
					"Mobile" - Mobile Apps
					"Ticketing" - Ticketing
					"CSN" - CSN
					"SMM" - SMM
					"Other" - Unique thank you pages (Ex. Adobe Connect)
			
			!Submit button settings
					@@@Colors@@@
					
					-"green"
					-"blue"
					-"lightblue"
					-"darkgreen"
					-"grey"
					-"purple"
					
					@@@Size@@@
					
					-"small"
					-"medium"
					-"large"
					
			!Adobe Connect
					@@@Country@@@
					
					-"US"
					-"UK"
					-"AUS"
					-"DE"
					-"SING"
					-"UAE"
					
					@@@Product@@@
					
					-"Event"
					-"Survey"
					-"Mobile"
					
			!Layout (Must add custom CSS)
				- "1": 1 column layout
				- "2": 2 column layout 		
															
		==========================================================================*/
		
		
		
		/***** !Initialize/set default variables here (Prevents undefined errors) ******/
		
		/*========================================================================== 
								!Layout type variables
		==========================================================================*/
		var cols = "1";
	
		/*========================================================================== 
								!Comments variables
		==========================================================================*/		
		var title = "";
		var label = "";
		
		/*========================================================================== 
								!Adobe Connect variables
		==========================================================================*/
		var adobe = false;
		var country = "US";
		var productName = "";
		
		/*========================================================================== 
								!Hidden fields object
		==========================================================================*/
		var fieldsObj = {"mk_ls": "", "product_name": ""};
				
				/*==========================================================================
				  					!Customize the form by removing fields
				==========================================================================*/
				var removeFieldsArray = ["#txtPostalCode", "#which-product", "#stateSelect", "#countrySelect", "#comments" , "#job_title"];
								
				/*==========================================================================
				  					!Product specfic thank you pages
				==========================================================================*/
				/* !Leave both variables blank if you don't want a redirect. For unique
					urls, set the product to "Other" and place the url in the "unique_url"
					variable, otherwise leave that variable blank. 							
				 */
				var ty_product = "Other"; 
				var unique_url = "http://www.cvent.com/en/get-in-touch-thank-you.shtml";
														
				/*==========================================================================
				  						!Marketo/Munchkin settings
				==========================================================================*/
				/* !Dev/Sandbox links */
				var munchkinID_dev = "006-LRT-285";
				var formID_dev = "1051";
				
				/* !Production links */
				var munchkinID_prod = "006-LRT-285";
				var formID_prod = "1051";
								
				/*==========================================================================
				  						!Generate submit button
				==========================================================================*/
				var submitText = "Get Pricing";
				var color = "green";
				var size = "medium";
				var onclickTxt = "";
                
                /*==========================================================================
                						!Hidden form fields
                ==========================================================================*/
                var fieldsObj = {"mk_ls":"WEB-Contact Us form", "product_name":"", "formProgramType":"quote"};
                
	/*==========================================================================
	  					!Pass the data into the modules
	==========================================================================*/
		
	removeFields( removeFieldsArray );
			
	thankYou( ty_product, unique_url );
									
	mktoSettings( munchkinID_dev, formID_dev, munchkinID_prod, formID_prod );
	
	layoutType( cols );
	
	comments( title, label );
	
	adobeConnect( adobe, country, productName );
	
	submitButton( submitText, color, size, onclickTxt );
	
	hiddenFields ( fieldsObj );
		
});/* !End document ready state */
</script>

