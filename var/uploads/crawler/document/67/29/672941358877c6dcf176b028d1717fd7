        <script type="application/javascript">
			function resetNotificationFlag(){
				var sEmail = document.getElementById("email").value;
				if(sEmail.indexOf("@cvent.com")>=0 || sEmail.indexOf("@gmail.com")>=0 || sEmail.indexOf("@hotmail.com")>=0 || sEmail.indexOf("@test.com")>=0 || sEmail.indexOf("@example.com")>=0)
					document.getElementById("notification_flag").value="0";
				else
					document.getElementById("notification_flag").value="1";
			}
		</script> 
