<script language="javascript" src="http://www.cvent.com/g/scripts/prod/s_code.js" type="text/javascript"></script>
<!-- Site Catalyst (Santosh 6/5/2013) -->
<script type="text/javascript">
	var url = document.URL;
	var orgurl=url.split('?');
	var urlarray = orgurl[0].split('/');
	var titleString=urlarray[urlarray.length-1];
	var sectionString=urlarray[urlarray.length-2];
	sectionString=sectionString.toUpperCase().charAt(0)+sectionString.substring(1);
	var sTst=urlarray[3];
	if (titleString === "") {
		if(sTst.toLowerCase() == "en")
			(urlarray.length)>5 ? titleString = 'Overview' : titleString = 'Homepage';
		else
			(urlarray.length)>=5 ? titleString = 'Overview' : titleString = 'Homepage';
	}
	titleString = titleString.replace(".shtml", "");
	if(titleString=='index')titleString='Homepage';
	if (sectionString === "" || sectionString.toLowerCase() == "en" || sectionString.toLowerCase() == "www.cvent.com" || sectionString.toLowerCase() == "hqwww02") {
		s.pageName= titleString;
	}
	else
	{
