                function finalCountdown(date) {
                                var currentDateTime = new Date();
                                var rawRange = countdownToDateTime - currentDateTime;
                                var rawRangeDays = rawRange / 86400000;
                                var rangeDays = Math.floor(rawRangeDays);
                                var remainingRange = rawRangeDays - rangeDays;
                                var rawRangeHours = remainingRange * 24;
                                var rangeHours = Math.floor(rawRangeHours);
                                remainingRange = rawRangeHours - rangeHours;
                                var rawRangeMinutes = remainingRange * 60;
                                var rangeMinutes = Math.floor(rawRangeMinutes);
                                remainingRange = rawRangeMinutes - rangeMinutes;
                                var rawRangeSeconds = remainingRange * 60;
                                var rangeSeconds = Math.floor(rawRangeSeconds);
                                document.getElementById('countdownDays').innerHTML = (rangeDays);
                                document.getElementById('countdownHours').innerHTML = (rangeHours);
                                document.getElementById('countdownMinutes').innerHTML = (rangeMinutes);
                                document.getElementById('countdownSeconds').innerHTML = (rangeSeconds);
                }
/*]]>*/</script></p> <center> </center> <p></p> <center> <div id="countdown1"><div class="countdownIncrement" id="countdownDays">0</div><div class="countdownIncrement" id="countdownHours">0</div><div class="countdownIncrement" id="countdownMinutes">0</div><div class="countdownIncrement" id="countdownSeconds">0</div><a href="http://www.abcactionnews.com/oscars"><span class="countdownIncrement"><img alt="Fiscal Cliff" border="1" height="250" src="http://media2.abcactionnews.com/image/2015/oscars-countdown_2015_sunday.jpg" width="300" /></span></a></div></center> <p><a href="http://media2.abcactionnews.com/oscars"><script type="text/javascript">/*<![CDATA[*/
                var theFinalCountdown = window.setInterval("finalCountdown(countdownToDateTime)",1000);
/*]]>*/</script></a></p> </div> </div> <footer class="module__footer"> </footer> </div> </article> </div> </div> </div> </div> </div> </div> </div> </div> <div class="layout__container area__full-col--middle portlet-area portlet-area--3 area__break--second" id="column-3"> <div class="portlet-dropzone portlet-column-content" id="layout-column_column-3"> <div id="p_p_id_56_INSTANCE_2ZQU4Oe1d7yx_" class="portlet-boundary portlet-boundary_56_ portlet-static portlet-static-end portlet-borderless portlet-journal-content " > <span id="p_56_INSTANCE_2ZQU4Oe1d7yx"></span> <div class="portlet-body"> <div class="portlet-borderless-container" style=""> <div class="portlet-body"> <div class="journal-content-article" id="article_10155_130949050_251758201_1.0"> <article id="m251758201" class="module module--triple-box triple-box--triple " > <div class="inner"> <header class="header--dark">MORE TOP STORIES</header> <div class="module__body"> <div class="inner"> <div class="blocks--keyline"> <div class="blocks"> <!-- 320 x 240 --> <article class="block "> <a href="/news/state/florida-is-middle-of-the-pack-for-the-state-of-american-well-being-index" class="inner" > <div class="headline">Florida middle of the pack in well-being</div> </a> </article> <!-- 320 x 240 --> <article class="block has-video "> <a href="/news/hillsborough-schools-warns-parents-about-incoming-cold-weather" class="inner" > <div class="headline">Hillsborough Schools send cold weather warning</div> </a> </article> <!-- 320 x 240 --> <article class="block has-video "> <a href="/news/local-news/la-france-in-ybor-city-to-donate-sarina-fazan-oscar-dress-viewers-cast-vote-to-choose-the-gown" class="inner" > <div class="headline">Vote for Sarina Fazan's Oscar dress</div> </a> </article> <!-- 320 x 240 --> <article class="block has-video "> <a href="/money/consumer/dirty-dining/dirty-dining-inspectors-find-live-and-dead-roaches-and-dangerous-food-temperatures" class="inner" > <div class="headline">Dirty Dining: Country H