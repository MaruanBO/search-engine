jQuery(document).ready(function() {
    jQuery('.hitbox').click(function(e) {
        e.preventDefault();
        if (jQuery(this).hasClass("open")) {
            jQuery(this).removeClass("open");
            jQuery('#info').slideToggle();
        } else if (!jQuery(this).hasClass("open")) {
            jQuery(this).addClass("open");
            jQuery('#info').slideToggle();
        }
    });

});

