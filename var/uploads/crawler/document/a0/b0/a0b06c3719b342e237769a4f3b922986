<div id="lastmenuId"></div>
<script type="text/javascript">

jQuery(document).ready(function() { 
	
	jQuery(".level0").find('div').hide();
	
	var timer;
	var start;
	var lastTopMenuId = '';	
	var threshold = 450;
	
	//Enter tabmenu	
	jQuery(".level0").mouseover(function (e) {

		if(timer) {
			clearTimeout(timer);
		}
		
		start = Date.now();
		
		if( e.target.id ) {
			var currentEleIdStr = e.target.id.split('-');	
			var eleName = currentEleIdStr[0];
			lastTopMenuId = currentEleIdStr[1];				
			timer = setTimeout(function() {
				jQuery('#L1menu-'+lastTopMenuId).css('display', 'block'); 
			}, 200);		
		}		
	});
	
	//Leave tabmenu	
	jQuery(".level0").mouseleave(function (e) {

		if(timer) {
			clearTimeout(timer);
		}
		
		var end = Date.now();
		visitTime = (Date.now()) - start;
		
		if( visitTime >  threshold ) {
			jQuery('#L1menu-'+lastTopMenuId).css('display', 'block'); 			
		}
		else {		 
		  visitTime = '';		 
		}
		
		jQuery('#L1menu-'+lastTopMenuId).hide();
	});	
	
	
});
		
</script>

