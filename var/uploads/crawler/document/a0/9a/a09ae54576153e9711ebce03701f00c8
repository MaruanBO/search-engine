    function HomePopup(xml) {
        var StartDateArr = new Array;
        var EndDateArr = new Array;

        var StartDate = new Date($("StartDate", xml).text());
        var EndDate = new Date($("EndDate", xml).text());
        var NowDate = new Date();

        if (NowDate >= StartDate && NowDate <= EndDate) {
            if ($("HTML_Content", xml).text() != "") {
                $("#PopupModel p").html($("HTML_Content", xml).text().replace("http://", SecureHost));
            }
            var pop_width = (Number(documentWidth) < Number($("Width", xml).text())) ? documentWidth : $("Width", xml).text();
            var pop_height = (Number(documentHeight) < Number($("Height", xml).text())) ? documentHeight : $("Height", xml).text();

            $("#PopupModel").css({ "height": Number(pop_height) + 40, "width": Number(pop_width) + 40 });

            if ($("URL", xml).text() != "") {
                $('#PopupModel .modal-body').append("<iframe width=\"" + pop_width + "\" height=\"" + pop_height + "\" frameborder=\"0\" scrolling=\"no\" allowtransparency=\"true\" src=\"" + $("URL", xml).text() + "\"></iframe>");
            }
            var temp_left = (Number(pop_width) + 40) / 2
            $('#PopupModel').css({ "top": "50%", "left": "50%", "margin-left": "-" + temp_left + "px" });
            $('#PopupModel .modal-body').css({ "overflow-y": "visible" });

            $('#PopupModel a').click(function () { $('#PopupModel').modal('hide'); });
            $("#popupmodel").click();
        }
    }
</script>
