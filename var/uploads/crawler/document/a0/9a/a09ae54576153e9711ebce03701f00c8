    function getLocationMapping(location, LangSite) {
        $.ajax({
            datatype: 'xml',
            url: 'GetData.asmx/getLocationMapping',
            type: 'GET',
            data: { location: location, LangSite: LangSite },
            success: function (xml) {
                window.parent.location = $("url", xml).text();
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                $("#main_container").show();
                $("#memberbar-zone").show();
            }
        });
    }


    $(document).ready(function () {
        $("#asus-api-header").addClass("aai-pot");
        setBusinessCookie("");
        setClickRelationIdCookie("0");
        $("body").addClass("home");
        window.name = "";

        //:[[2013/11/29 Angel, to check location and redirect to default local site]]
        if (EntryPageCookie == "1" && getWebsite().toUpperCase() == "GLOBAL" && window.location.hostname != "test.asus.com") {
            var url = window.location.pathname;
            var urlsplit = url.split("/");
            var action = urlsplit[urlsplit.length - 1];
            if (action != "empty.htm" &&
                action != "entry.htm" &&
                window.location.pathname.split("/")[1].toLowerCase() != "gr" &&
                window.location.pathname.split("/")[1].toLowerCase() != "bg") {

                var redirect_flag = false;
                var default_global = (getcookiedata("GlobalNoFlag") != "" && getcookiedata("GlobalNoFlag") == "Y");
                if (getcookiedata("country_setting") != "" && !default_global) {
                    redirect_flag = (
                        getcookiedata("country_setting") != "http://www.asus.com/" &&
                        getcookiedata("country_setting") != "https://www.asus.com/" &&
                        getcookiedata("country_setting") != "http://www.asus.com" &&
                        getcookiedata("country_setting") != "https://www.asus.com" &&
                        getcookiedata("country_setting") != "\/\/www.asus.com" &&
                        getcookiedata("country_setting") != "\/\/www.asus.com/")
                }

                var pre_url = (typeof (document.referrer) == "undefined") ? "" : document.referrer;
                if (redirect_flag || (pre_url != SecureHost + "www.asus.com/entry.htm" && pre_url != SecureHost + "www.asus.com/empty.htm")) {
                    $("#main_container").remove();
                    $("#memberbar-zone").remove();
                    $("#iframeEntry").attr("src", SecureHost + "www.asus.com/empty.htm");
                    $("#iframeEntry").show();
                }
                else {
                    $("#iframeEntry").hide();
                    $("#main_container").show();
                    $("#memberbar-zone").show();
                    Global_Tracking_Flag = "1";
                }
            }
            else {
                if (getcookiedata("Popup") != "Y") {
                    add_cookie("Popup", "Y", true);
                    if (!jQuery.browser.mobile) XML_ReaderByWebservice("getHomePopup_Xml", "HomePopup", "");
                }

                $("#iframeEntry").hide();
                $("#main_container").show();
                $("#memberbar-zone").show();
                Global_Tracking_Flag = "1";
            }
        }
        else {
            if (getcookiedata("Popup") != "Y") {
                add_cookie("Popup", "Y", true);
                if (!jQuery.browser.mobile) XML_ReaderByWebservice("getHomePopup_Xml", "HomePopup", "");
            }
            $("#iframeEntry").hide();
            $("#main_container").show();
            $("#memberbar-zone").show();
            Global_Tracking_Flag = "1";
        }

