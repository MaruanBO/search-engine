<script>
    var intControlstopSysWordingCheckId;
    intControlstopSysWordingCheckId = setInterval("controlstopSysWordingCheck()", intSysWordingCheckSpeed);

    function controlstopSysWordingCheck() {
        if (sysWordingRead) {
            $("#aai-search-btn").val(sysWording.SearchTitle);
            $("#minicart_panel p.aai-item-titl").html(sysWording.ShoppingCart);

            clearTimeout(intControlstopSysWordingCheckId);
        }
    }

    $(document).ready(function () {
        $("div.aai-menu ul li").removeClass("aai-focus");
    });

    function setTopmenuTab(tab_name) {
        $("div.aai-menu ul li").removeClass("aai-menu-focus");
        $("div.aai-menu ul").find("li[data-name='" + tab_name + "']").addClass("aai-menu-focus");
    }

    $("#searchinput").keypress(function (event) {
        if (event.keyCode == 13) {
            $("#aai-search-btn").click();
        }
    });

    $("#searchinput").click(function () {
        if (this.value == $('div.aai-search-group input:eq(1)').val()) { this.value = '' }
        else {
        }
    });

    $("#searchinput").blur(function () {
        if (this.value == '') { this.value = $('div.aai-search-group input:eq(1)').val() };
    });

    function Search() {
        var keyword = $("#searchinput").val();
        var DefaultKeyword = $("#searchinputDefault").val();
        if (keyword != DefaultKeyword) {
            //:2014/06/19 Angel, remove shop re-search function
            var searchType = "";
            if (isApplicationPathSite()) window.location.href = '/' + getWebsite() + '/Search/Search?SearchKey=' + encodeURI(keyword);
            else window.location.href = '/Search/Search?SearchKey=' + encodeURI(keyword);
        }
    }

    var CompareJson;
    function fn_compare(obj) {
        CompareJson = obj;
        setCompareList();

        var topmenu_unm = (typeof ($("#num_shoppingcart")) == "undefined") ? Number($("#num_compare").html()) : (Number($("#num_compare").html()) + Number($("#num_shoppingcart").html()));
        if (Number(topmenu_unm) > 0) {
            $(".aai-right-more a.aai-more .aai-circle-num").html(topmenu_unm);
            $(".aai-right-more a.aai-more .aai-circle-num").css({ display: 'inline-block' });
        }
        else {
            $(".aai-right-more a.aai-more .aai-circle-num").css({ display: 'inline-block' });
            $(".aai-right-more a.aai-more .aai-circle-num").css({ display: 'none' });
        }
    }
</script>
            
