<script type="text/javascript">
    var intPageindexMasterSysWordingCheckId;
    intPageindexMasterSysWordingCheckId = setInterval("pageindexMasterSysWordingCheck()", intSysWordingCheckSpeed);
    function pageindexMasterSysWordingCheck() {
        if (sysWordingRead && Global_Tracking_Flag == "1") {
            try {
                ga('create', sysWording.GoogleTrackingCode, 'auto');
                ga('require', 'displayfeatures');
                
                ga('set', 'anonymizeIP', true);
                if (window.location.hostname != "athena.asus.com") {
                    if (isProductPage == "") ga('send', 'pageview');
                }
                else {
                    ga('send', 'pageview', TrackerWording);
                }
            } catch (err) { }
            clearTimeout(intPageindexMasterSysWordingCheckId);
        }
    }
</script>
