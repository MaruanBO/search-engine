<script>
var slideDown = 'Y';

	$('.uni-m').mouseenter(function(e){
		var idx = $(this).index();
		if(idx == 0 || idx == 7 ) {
			slideDown = "Y";
			$('.bk-showmore').slideUp("slow");
			return;
		}
		var box_Idx = $('#box'+idx);
	
		if(slideDown == "N") {
			$('.bk-showmore').css("display","none");
			$('#box'+idx).css("display","block");			
		}else {
			$('#box'+idx).slideDown("slow");	
			slideDown = "N"
		}

	});

	$('.uni-nav').mouseleave(function(e){
		slideDown = "Y";
		$('.bk-showmore').slideUp("slow");
	});
	
</script>
