$(document).ready(function(){
    var options = {
        success:        showJsonResponse,
        dataType:       'json'
    };
    $('#firstVisitForm').ajaxForm(options);

});


 // Our callback function
function showJsonResponse(data) {
	  
    	if(data.codes[2].error=="false") {
        	$("#firstVisitEmail")[0].value=data.codes[0].email
        	$("#firstVisitCode")[0].innerHTML=data.codes[1].code;
    		showFirstVisitCode();
    	} else {
        	$("#firstVisitError")[0].innerHTML=data.codes[1].code;    		
    	}
        // No errors, show the server response which is a Json object.
        // e.g. the following line shows the country JSON parameter that was returned by the server
        // alert(data.country);
       
    
}


 
 
