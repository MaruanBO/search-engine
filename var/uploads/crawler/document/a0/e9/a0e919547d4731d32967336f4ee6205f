<script type="text/javascript">
	// Breaking news enhnaced trumps breaking news
	if (trim($("#breaking-news-enhanced-wrapper").html())!='')
	{
		renderBreakingNewsEnhanced();
	}
	else if(trim($("#breaking-news-wrapper").html())!='')
	{
		renderBreakingNews();
		breakingNewsPager();
	}
	function renderBreakingNewsEnhanced()
	{	
		$("#breaking-news-wrapper").hide();
	}
	function renderBreakingNews()
	{
		$("#breaking-news-enhanced-wrapper").hide();
	}
	function breakingNewsPager()
	{
	    itemPager({
			indvitem: $('.brnews_content_carousel_indvitem-1'),
			navcont: $('.brnews_content_nav_cont'),
			nextbtn: $('#brnews_content_nextbtn-1'),
			prevbtn: $('#brnews_content_prevbtn-1')
	    });
	}
</script>

