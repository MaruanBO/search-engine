function addFavorite(){
	url = 'http://www.latinopost.com/';
	title = 'META_TITLE';
	//ff
	if (window.sidebar) {
		window.sidebar.addPanel(title, url,"");
	}
	//ie
	else if( window.external ) {
		window.external.AddFavorite(url, title);
	}
	return false;
}
</script>
