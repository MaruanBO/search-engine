<script language="javascript" type="text/javascript">
$(document).ready(function(){
	$('div.headlines div.tabs div').hover(
		function(){
			if (!$(this).hasClass('active')) {
				$(this).css({ 'text-decoration' : 'underline', 'cursor' : 'pointer' });
			}
		},
		function(){ $(this).css({ 'text-decoration' : 'none', 'cursor' : 'default' }); }
	);
	$('div.headlines div.tabs div').click(function(){
		if ($(this).hasClass('active')) return false;
		$(this).css({ 'text-decoration' : 'none', 'cursor' : 'default' });
		$('div.headlines div.tabcontainer-content').hide();
		var tab = $(this).attr('id').replace('tab-', '');
		$('div.headlines #tabcontainer-'+tab).show();
		$('div.headlines div.tabs div.active').removeClass('active').addClass('inactive');
		$(this).removeClass('inactive').addClass('active');
	});
});
</script>

