<script type="text/javascript">
    PbInstallerWidth();

    $(document).ready(function () {
        showInstallerDiv();
        SearchFitmentShow();
        SearchFitmentHide();
        ValidatePersonalBarZipCode();
    });

    $('#pbfrontWidth').change(function() {
        var width = $('#pbfrontWidth :selected').text();
        var url = "/rest/fitment/tires/" + width;
    });

    $('#pbfrontAspectRatio').change(function() {
        var width = $('#pbfrontWidth :selected').text();
        var aspect = $('#pbfrontAspect :selected').text();
        var url = "/rest/fitment/tires/" + width + "/" + aspect;
    });

    $('#pbrearWidth').change(function() {
        var width = $('#pbrearWidth :selected').text();
        var url = "/rest/fitment/tires/" + width;
    });

    $('#pbrearAspectRatio').change(function() {
        var width = $('#pbrearWidth :selected').text();
        var aspect = $('#pbrearAspect :selected').text();
        var url = "/rest/fitment/tires/" + width + "/" + aspect;
    });

    $(document).on('click','a#saveZip', function(){
        $.post(url, function(html) {
        });
    });

    $(function () {
        function activate_installer_button() {
            updatedZip = $('#installerSearchForm .installer-zip-code').val();
            $('#installerSearchForm button').attr('disabled', 'disabled');
            IsPostalCodeExists(updatedZip, function(value) {
                if (value) {
                    $('#installerSearchForm button').removeAttr('disabled');
                }
            });
        }
        $('#installerSearchForm').submit(function (ev) {
            ev.preventDefault();
            updatedZip = $('#installerSearchForm .installer-zip-code').val();
            IsPostalCodeExists(updatedZip, function(value) {
                if (value) {
                    $.ajax({
                        cache:false,
                        type:"POST",
                        url: '/updatezipcode/' + updatedZip,
                        success:function (html) {
                            $('#installerSearchForm').off("submit");
                            $('#installerSearchForm').submit();
                        }
                    });
                }
            });
        });
        $('#installerSearchForm .installer-zip-code').keyup(activate_installer_button);
    });

    function showInstallerDiv() {
        if(false){
            $('div#installerDescription').removeClass();
            $('div#chooseInstaller').addClass('hidden');
        }
        else{
            $('div#installerDescription').addClass('hidden');
            $('div#chooseInstaller').removeClass();
        }
    }


</script>

