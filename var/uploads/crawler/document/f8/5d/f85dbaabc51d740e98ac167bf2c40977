<script type="text/javascript">
    function submitLoginForm(){
        $('#loginForm').submit();
    }
    $(document).ready(function () {
        try{
            if( $('#loginErrorFromServer').val() != null )
            {
                $('#signInFlyoutContainer').show();
                $('.loginError').show();

            }

            SignInFlyout.Init();
            (function($){
                var termTemplate = "<span class='ui-autocomplete-term'>%s</span>";
                var autoOpts = {
                    minLength : 3,
                    source: function(req, resp){
                        $.getJSON("/suggest/search", req,
                                function(data) {
                                    var suggestions = [];
                                    var keywords = JSON.parse(data);
                                    $.each(keywords, function(i, val){
                                        var obj = {};
                                        obj.value = val.text;
                                        suggestions.push(obj);
                                    });
                                    resp(suggestions);
                                });
                    },
                    open: function(e, ui){
                        var resultsList = $("ul.ui-autocomplete > li.ui-menu-item > a");
                        var term    = $.trim($(this).val()).split(/\s+/).join('|');
                        resultsList.each ( function () {
                            var jThis   = $(this);
                            var regX    = new RegExp ('(' + term + ')', "ig");
                            var oldTxt  = jThis.text ();

                            jThis.html (oldTxt.replace (regX, '<b>$1</b>') );
                        } );
                    },

                    select: function(e, ui) {
                        window.location.href = '/searchpage?text='+ui.item.value;
                    }
                };
                if( $('#search-mobile').length > 0 || $('#search').length > 0 ){
                    if( tbMq.isMobile() ){
                        $("#search-mobile").autocomplete(autoOpts);
                        $("#search-mobile").siblings(".icon-search").click(function () {
                            window.location.href = '/searchpage?text=' + $("#search-mobile").val();
                        });
                        $("#search-mobile").keypress(function (e) {
                            if ( e.which == 13 ) {
                                window.location.href = '/searchpage?text=' + $("#search-mobile").val();
                            }
                        });
                    }else{
                        $("#search").autocomplete(autoOpts);
                    }
                }
            })(jQuery);
            PerformGlobalSearch();
        } catch( error ){
        }
    });      // docuemnt.ready

</script>
