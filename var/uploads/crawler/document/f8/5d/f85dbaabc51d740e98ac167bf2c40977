<script type="text/javascript">
    var contextPath = "";
    var miniCartComponentId = "TB-MiniCart";
    var activateMiniCart;
    function refreshMiniCart() {
        $.get('/tirebuyer/cart/miniCart/TOTAL', function(result) {
            $('#minicart_data').html(result)
        });
    }

    function EnableHoverCart() {
        //Hover Cart
        var timeEvent, timeMouseOut;
        var hoverTimer = 500;
        var closeTimer = 200;
        $(".cartInfo, .hoverCart").hover(function () {
                   activateMiniCart = true;
                   if( $(this).hasClass('cartInfo')){
                        timeEvent = setTimeout(GetMiniCartContent, hoverTimer);
                    }
                    //console.log('cartInfo hoverCart mouseIn');
                    clearTimeout(timeMouseOut);
                },
                function (e) {
                    activateMiniCart = false;
                    if ($(this).hasClass("hoverCart")) {
                        if ((e.type == "mouseleave")) {
                            //$(this).removeClass("active");
                            clearTimeout(timeMouseOut);
                            timeMouseOut = setTimeout(' $(".hoverCart").removeClass("active") ', closeTimer);
                            printf('hoverCart mouseOut: ' + e.type);
                        }
                    }
                    if ($(this).hasClass("cartInfo")) {
                        if ((e.type == "mouseleave") && ($(".hoverCart").hasClass("active"))) {
                            //console.log('cartInfo mouse out');
                            clearTimeout(timeMouseOut);
                            timeMouseOut = setTimeout(' $(".hoverCart").removeClass("active") ', closeTimer);
                        }
                    }
                    //console.log('clear timeEvent');
                    clearTimeout(timeEvent);
                }
        );
    }
    function GetMiniCartContent(){

            printf("GetMiniCartCo.....");
            $.ajax({
                url:contextPath + '/tirebuyer/cart/rollover/' + miniCartComponentId ,
                cache:false,
                type:"GET",
                success: function(result){
                            $('#rollover_cart_popup').html(result);
                            $('#ajax_cart_close').click(function(e) {
                                e.preventDefault();
                                $('#rollover_cart_popup').hide();
                            });
                            if(activateMiniCart){
                                $(".hoverCart").addClass("active");
                            }
                            //$('#rollover_cart_popup').fadeIn();
                            //gotCartContent = true;
                        }
          });

    }

    $(document).ready(function() {
        EnableHoverCart();
    });
</script>
