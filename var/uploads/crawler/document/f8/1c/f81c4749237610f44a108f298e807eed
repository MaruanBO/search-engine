(function($) { 

$(document).ready(function(){
$(".view-header > h3").hover(function(){

 var test = $(".view-header > h3 > a").attr("href");
if(test == "http://F1 Race Day Report.nbcsports.com/"){
 $(".view-header > h3 > a").attr("href","http://motorsportstalk.nbcsports.com/tag/race-day/");
}
if(test == "http://F1 Weekend Preview.nbcsports.com/"){
 $(".view-header > h3 > a").attr("href","http://motorsportstalk.nbcsports.com/tag/weekend-preview/");

}

if(test == "http://F1 Weekend Review.nbcsports.com/"){
 $(".view-header > h3 > a").attr("href","http://motorsportstalk.nbcsports.com/tag/weekend-review/");

}
});
});
})(jQuery)


