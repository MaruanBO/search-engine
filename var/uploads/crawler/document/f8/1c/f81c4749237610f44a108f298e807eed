     <script>
(function($){
  $(document).ready(function(){
    var iframeSrc = $('#tp-global-player').attr('src');
      if (typeof iframeSrc !== "undefined") {
        var arr = iframeSrc.split('autoPlay=false&');
        if(arr[0]){
          //alert(arr[0]);//this is required
          //var var1 = arr[1];
          //var fullURL = arr[0]+arr[1];
          var finalRes = arr[0];
          if (typeof arr[1] !== "undefined") {
            var var2 = arr[1].split('parentUrl=');
            var var4 = arr[1].split('&params=');
            finalRes = finalRes + var4[0];
          }
          if (typeof var2[1] !== "undefined") {
            var var3 = var2[1].split('&params=');
            finalRes = finalRes + var3[0];
          }
          //alert(var3[0]);//this is required
          
          //alert(var4[0])//this is required
          if(typeof finalRes !== "undefined"){
            var iframeSrc = $('#tp-global-player').attr('src',finalRes);
          }else{
            console.log("finalRes is undefined");
          }
        }else{
          console.log("autoPlay=false doesn't exist");
      }
    }
  });
})(jQuery)
</script>
