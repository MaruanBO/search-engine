<SCRIPT LANGUAGE=javascript>
function getMore(max)  {
var count=1;
var group=12;
var flag="";
for(var i=1;i<=max;i++)
   {
    var name="recent["+i+"]";//alert(name + i + max);
    element=document.getElementById(name);
    if(element.style.display != "block" && count <= group)
      {
       if(!flag)
         {
         var pos=i;
         flag=1;
         }
       element.style.display="block";
       count++;
       if(i==max)
         {
         element2=document.getElementById("More");
         element2.style.display="none";
         }
      }
    
    
   }


return false;
}

</script>


