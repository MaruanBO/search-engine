<script type="text/javascript"> 
var lede_current = 1;
var lede_max = 3;
var lede_delay = 8000;   /* 8 second delay between slides. */
var lede_timer = '';
 
function Lede_Slide(slide)
{
  if ( lede_timer != '' )
  {
    clearInterval(lede_timer);
    lede_timer = '';
  }
 
  if ( slide && slide != lede_current )
  {
    var eleId = 'kip_lede' + lede_current;
    var ele = document.getElementById(eleId);
    ele.style.display = 'none';
    /* ele.className = ''; */
 
    /* 
    eleId = 'kip_lede_button' + lede_current;
    document.getElementById(eleId).style.color = '#999';
    document.getElementById(eleId).style.backgroundColor = '#fff';
    */
 
 
    /* Fix IE8 pixilated ClearType font problem */
    /*
    if ( navigator.appName == 'Microsoft Internet Explorer' )
    {
      ele = document.getElementById('kip_lede_bold1');
 
      if ( ele.style.filter && ele.style.removeAttribute )
      {
        ele.style.opacity = 1;
        ele.style.removeAttribute('filter');
      }
 
      ele = document.getElementById('kip_lede_text1');
 
      if ( ele.style.filter && ele.style.removeAttribute )
      {
        ele.style.opacity = 1;
        ele.style.removeAttribute('filter');
      }
    }
    */
 
    eleId = 'kip_lede' + slide;
    ele = document.getElementById(eleId);
    /* ele.className = 'kip_lede_button_active'; */
 
    /*
    eleId = 'kip_lede_button' + slide;
    document.getElementById(eleId).style.color = '#fff';
    document.getElementById(eleId).style.backgroundColor = '#999';
    */
 
    ele.style.display = 'block';
    lede_current = slide;
  }
}

function Lede_RotationInit()
{
  var slide = 0;
 
  if ( lede_delay > 0 )
  {
    if ( lede_current < lede_max )
      slide = lede_current;
 
    Lede_Slide(slide);
    lede_timer = setInterval(function(){Lede_Rotation()}, lede_delay);
  }
} 

function Lede_Rotation()
{
  var slide = 0;
 
  if ( lede_delay > 0 )
  {
    if ( lede_current < lede_max )
    {
      slide = lede_current + 1;
    }
    else
    {
      slide = 1;
    }
    Lede_Slide(slide);
    lede_timer = setInterval(function(){Lede_Rotation()}, lede_delay);
  }
}
 
function removeFilter(element)
{
  if ( element.style.filter && element.style.removeAttribute )
  {
    element.style.removeAttribute('filter');
  }
}
</script>
