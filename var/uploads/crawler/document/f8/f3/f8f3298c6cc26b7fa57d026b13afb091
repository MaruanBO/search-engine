        function loadLogin(json) {
        if (json.loginStatus.loggedIn) {
            $('#memberInfo').empty().append(
                '<a href="http://store.gulfnews.com" title="Subscribe" style="color: white;background-color: red;padding: 3px 5px 3px 5px;font-weight: bold;">Subscribe</a> | Welcome back, ' + json.loginStatus.userName + ' <a href="/profile">My Profile</a> | <a href="/dologout?referrer=http://gulfnews.com/">Sign Out</a>');
        } else {
            $('#memberInfo').empty().append(
                '<a href="http://store.gulfnews.com" title="Subscribe" style="color: white;background-color: red;padding: 3px 5px 3px 5px;font-weight: bold;">Subscribe</a> | <a href="/login" title="Register">Register</a> | <a href="/login?referrer=http://gulfnews.com/" title="Sign In">Sign In</a>');
        }
    }

    $(document).ready(function() {
        $.getJSON('/ajaxservlet?action=loginStatus', loadLogin);
    });
