<script type="text/javascript">
$.post("/latestcommit", function(data){
  process(data);
});

function process(data) {
	var random;
    var lastupdated;
	if(data.indexOf("1970") != -1){
	lastupdated = "few minutes back"
	data = lastupdated;
	}
	var tag = document.getElementById('updated'); 
	if (tag.innerHTML) { 
		tag.innerHTML = data; 
	} 
	else if (tag.innertext) { 
		tag.innertext = data; 
	} 
}
</script>

    
