		kvUser = 'kvcountry=;kvgender=;kvnationality=;kvyob=;kvprof=;kvindustry=;kvinterests=;';
	function getUser(json) {		
        if (json.loginStatus.loggedIn) {	
			kvUser = '';	
			if (json.loginStatus.country) {
				kvUser = kvUser+'kvcountry='+json.loginStatus.country+';';
			}
			else{
				kvUser = kvUser+'kvcountry=;';
			}				
			if (json.loginStatus.gender){ 
				kvUser = kvUser+'kvgender='+json.loginStatus.gender+';';
			}
			else{
				kvUser = kvUser+'kvgender=;';
			}		
			if (json.loginStatus.nationality){ 
				kvUser = kvUser+'kvnationality='+json.loginStatus.nationality+';';
			}
			else{		
				kvUser = kvUser+'kvnationality=;';
			}	
			if (json.loginStatus.dob){ 
				kvUser = kvUser+'kvyob='+json.loginStatus.dob.substring(json.loginStatus.dob.length-4)+';';
			}
			else{
				kvUser = kvUser+'kvyob=;';
			}		
			if (json.loginStatus.prof){ 
				kvUser = kvUser+'kvprof='+json.loginStatus.prof+';';
			}
			else{
				kvUser = kvUser+'kvprof=;';
			}	
			if (json.loginStatus.industry){ 
				kvUser = kvUser+'kvindustry='+json.loginStatus.industry+';';
			}
			else{
				kvUser = kvUser+'kvindustry=;';
			}	
			if (json.loginStatus.interests){ 
				kvUser = kvUser+'kvinterests='+json.loginStatus.interests+';';
			}
			else{
				kvUser = kvUser+'kvinterests=;';
			}			
			kvUser = kvUser + tempAdtech;
		}
		else{		
			kvUser = 'kvcountry=;kvgender=;kvnationality=;kvyob=;kvprof=;kvindustry=;kvinterests=;';	
			kvUser = kvUser + tempAdtech;
		}
		kvUser = 'KVabbr='+window.adtechCCvals+';'+kvUser;

