<script>barton.cart.setupCart=function(){barton.cart.getCountries();$("input#billToState").blur(function(){updateCart();});$("input#shipToState").blur(function(){updateCart();});setTimeout(function(){cart.customField2='Wholetones';cart.customField5='rootpage_wtone_02-XX-15';cart.customField3='wholetones.com%2F';cart.customField7='';cart.screenBrandingThemeCode='whole';cart.upsellPathCode='';bp(function(tracker){barton.page['client_id']=bp.getByName('bpTracker').get('clientId');cart.customField4=barton.page['client_id'];});},5000);}
barton.cart.isInCart=function(itemId){var itemIsInCart=false;if(cart.items.length){for(itemPosition=0;itemPosition<cart.items.length;itemPosition++){if(cart.items[itemPosition]['itemId']===itemId){itemIsInCart=true;}}
if(itemIsInCart==true){return true;}else{return false;}}else{return false;}}
barton.cart.addItem=function(itemId,qty){$('link[rel="icon"]').attr('href',barton.page['image_path']+'/'+barton.page['favicon_cart']+'?v'+barton.page['file_version']);if(barton.cart.isInCart(itemId)==false){cart.items.push({itemId:itemId,quantity:qty});jQuery.ajax({url:barton.cart['uc-rest_url']+'/cart',type:'POST',headers:{"cache-control":"no-cache"},contentType:'application/json; charset=UTF-8',data:JSON.stringify(cart),dataType:'json'}).done(function(cart){});}else{}}
barton.cart.removeItem=function(itemId,qty){if(cart.items.length){for(itemPosition=0;itemPosition<cart.items.length;itemPosition++){if(cart.items[itemPosition]['itemId']===itemId){cart.items.splice(itemPosition,qty);}}}}
barton.cart.addTracking=function(affId,subId){cart.affiliateId=affId;cart.affiliateSubId=subId;jQuery.ajax({url:barton.cart['uc-rest_url']+'/cart',type:'POST',headers:{"cache-control":"no-cache"},contentType:'application/json; charset=UTF-8',data:JSON.stringify(cart),dataType:'json'}).done(function(cart){});}
barton.cart.addPath=function(theme,upsell){cart.screenBrandingThemeCode=theme;cart.upsellPathCode=upsell;jQuery.ajax({url:barton.cart['uc-rest_url']+'/cart',type:'POST',headers:{"cache-control":"no-cache"},contentType:'application/json; charset=UTF-8',data:JSON.stringify(cart),dataType:'json'}).done(function(cart){});}
barton.cart.listCartItems=function(){if(cart.items.length){for(itemPosition=0;itemPosition<cart.items.length;itemPosition++){}}
else{}}
barton.cart.getCountries=function(){if(barton.cart['allowed_countries']){$.each(barton.cart['allowed_countries'],function(key,value){$('#shipToCountry').append($("<option></option>").attr("value",value).text(value));$('#billToCountry').append($("<option></option>").attr("value",value).text(value));});}else{jQuery.ajax({url:barton.cart['uc-rest_url']+'/site/allowedCountries',type:'GET',headers:{'X-UC-Merchant-Id':'BARTN','cache-control':'no-cache'},dataType:'json'}).done(function(countries){$.each(countries,function(key,value){$('#shipToCountry').append($("<option></option>").attr("value",value).text(value));$('#billToCountry').append($("<option></option>").attr("value",value).text(value));});});}
if(barton.geo['country_name']){$("#shipToCountry").val(barton.geo['country_name']);$("#billToCountry").val(barton.geo['country_name']);}else{$("#shipToCountry").val("United States");$("#billToCountry").val("United States");}}
barton.cart.addOption=function(itemID,productID){barton.page.showLoad();barton.player.pauseVideo();$('#blocker').hide();barton.cart.addItem(itemID,1);if(barton.cart['item1a']&&barton.cart['item1a']!==itemID){barton.cart.removeItem(barton.cart['item1a'],1);barton.cart.removeItem(barton.cart['item1b'],1);}
if(barton.cart['item2a']&&barton.cart['item2a']!==itemID){barton.cart.removeItem(barton.cart['item2a'],1);barton.cart.removeItem(barton.cart['item2b'],1);}
if(barton.cart['item3a']&&barton.cart['item3a']!==itemID){barton.cart.removeItem(barton.cart['item3a'],1);barton.cart.removeItem(barton.cart['item3b'],1);}
if(barton.lead['lead_gen']==true){barton.lead.leadGen('open');barton.cart.showCheckout();}
else{barton.cart.showCheckout();$('html, body').animate({scrollTop:$('#checkout').offset().top},1000);barton.player.showControlsWhenInView();}
updateCart();barton.page.hideLoad();}
barton.cart.bonusItem=function(optionId,itemId){if($(optionId).is(':checked')){barton.cart.addItem(itemId,1);}
else if(!$(optionId).is(':checked')){barton.cart.removeItem(itemId,1);}
updateCart();}</script>

