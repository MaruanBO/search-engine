<script>
	$(document).ready(function() {
		$('.video-play').on('click', function(e) {
			e.preventDefault();
			var $this = $(this);
			
			//grab the entry (content) id from the data-vid-entry attribute on the link
			var vidEntry = $this.attr('data-vid-entry');
			
			//pop open the modal
			vidBPopup = $('#videoPop').bPopup({
				onOpen: function() {
					//set the kaltura parameters and use the vidEntry variable
					kWidget.embed({
						"targetId": "kalturaPlayerWrapper",
						"wid": "_1355991",
						"uiconf_id": 26409621,
						"flashvars": {},
						"cache_st": 1414177791,
						"entry_id": vidEntry
					});
				}, 
				onClose: function() { 
					document.getElementById('kalturaPlayerWrapper').sendNotification('doStop');
				}
			});
			
		});
	});
</script>



