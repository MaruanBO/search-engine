	(function($,d){$.each(readyQ,function(i,f){$(f)});$.each(bindReadyQ,function(i,f){$(d).bind("ready",f)})})(jQuery,document)
</script>

<script src="/_assets/js/init.js"></script>

<script>
	/* START: Contact Me         */


	$(document).ready(function() {
		var showContactMeError=function(json){
			var errHtml = "";
			if(typeof json !== typeof undefined && json !== false){
				$.each(json.errors, function(i, item) 		  { errHtml += "<li>" + json.errors[i] + "</li>"; });
				$.each(json.requiredFields, function(i, item) { errHtml += "<li>Enter a value for " + json.requiredFields[i] + "</li>"; });
				$.each(json.invalidValues, function(i, item)  { errHtml += "<li>" + json.invalidValues[i] + "</li>"; });
				if(errHtml>"") { errHtml = "<p><ul>" + errHtml + "</ul></p>"; }
			}
			if(errHtml<="")	{  errHtml = "<p>An error has occurred.  Please try again later.</p>"; }
			$('#contactmePop .contactmeErrorMessage').addClass("error").html(errHtml);
			$("#contactmePop .contactmeErrorMessage").show();
			$('#contactmePop .contactmeErrorMessage').focus();
		};

		$("#contactmePop .contactmeErrorMessage").hide();
		$("#contactmeResult").hide();

		/* For help with this script, go here: http://jqueryvalidation.org/documentation/ */
		$("#contactMe").validate({
			debug: false, //change to true to debug this validator
			submitHandler: function(form) {
				var params = $(form).serialize();
				$.ajax({
					type: 'post',
					url: '/_assets/php/form-processor.php',
					data: params,
					dataType: 'json',
					success: function(json){
						if(json.result === "y"){
							$('#contactmeResult').removeClass("error");
							$("#contactmeResult").show();
							$('#contactmeForm').hide();
							$('#contactmePop').bPopup().reposition();

							try{
								_tfa.push({notify:"action", name:"contact-me-thank-you"});
								//Outbrain
							}catch(e){}
							$('#contactMe').trigger("reset");
						}else{
							showContactMeError(json);
						}
					},
					error: function(e) {
					  showContactMeError();
					}
				});
				return false;
			},
			invalidHandler: function(event, validator) {
				$('html, body').animate({
					scrollTop:   $("#contactMe").offset().top
				}, 500); //this is the speed of animation
			},
			errorContainer:      $('div.errorcontainer'),
			errorLabelContainer: $("#contactMe .errorcontainer ol"),
			wrapper:             "li",
			errorPlacement: function(error, element) {},
			errorClass:          "formerror",
			onfocusout: false, //controls onblur validation - use comment code to turn on or off ... a true value does not work!
			onkeyup: false, //controls onkeyup validation - use comment code to turn on or off ... a true value does not work!
			onclick: false, //controls onclick validation of checkboxes and radio buttons - use comment code to turn on or off ... a true value does not work!
			focusInvalid: true, //controls focusing of invalid elements - change to either true or false
			focusCleanup: false, //controls cleanup when focusing elements (removing the error class and hiding error messages) - change to either true or false
			rules: {
				r_First_Name:   "required",
				r_Last_Name:    "required",
				r_Street: 		"required",
				r_City:         "required",
				r_State:        "required",
				r_ZIP_Code: {
					required:  true,
					number:    true,
					minlength: 5
				},
				r_Contact_Type: {
					required:  true
				},
				o_Phone: {
					required:	"#Contact_Type_Phone:checked",
					phoneUS : true
				},
				o_Email: {
					required:	"#Contact_Type_Email:checked",
					email:		true
				}
		  }
		});

		$(document).on('click', '.contactme-link', function(event) {
			event.preventDefault();
			ntptEventTag('ev=contactme.clicked');
			$("#contactmePop .contactmeErrorMessage").hide();
			$("#contactmeResult").hide();
			$("#contactmeForm").show();
			$('#contactmePop').bPopup();

			try{
				_tfa.push({notify:"action", name:"contact-me-pop-up"});
			}catch(e){}
		});
	});
/* END:   Contact Me         */
</script>

