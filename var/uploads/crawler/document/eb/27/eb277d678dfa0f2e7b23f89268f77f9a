<script type="text/javascript">
$(function(){
    $('#first_name').autotab({ target: 'last_name', format: 'alpha' });
    $('#last_name').autotab({ target: 'state_list', format: 'alpha', previous: 'first_name' });
    $('#state_list').autotab({ previous: 'last_name' });

    //$('#first_name').focus();


    $('<i class="icon-remove"></i><i class="icon-ok"></i>').appendTo($('#people_search_form .input_field'));
    $("#people_search_form").validate(
    {
        // Rules for form validation
        rules:
        {
            first_name:
            {
                required: true
            },
            last_name:
            {
                required: true
            }
        },
        
        // Messages for form validation
        messages:
        {
            first_name:
            {
                required: 'Please enter a First Name'
            },
            last_name:
            {
                required: 'Please enter a Last Name'
            }
        },                 
        
        // Do not change code below
        errorPlacement: function(error, element)
        {
            error.appendTo(element.parent());
        },

        submitHandler: function(form)
        {
            form.submit();
            // $('#search-city-name').text($('#state_list').val());
            // $('#search-user-name').text($('#first_name').val() + ' ' + $('#last_name').val());
            // $('#progress1').anim_progressbar({form: form});
            // openPopDiv('searchingPopup');
        }
    });
});
</script> 

