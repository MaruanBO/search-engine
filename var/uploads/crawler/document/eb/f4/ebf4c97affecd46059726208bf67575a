	jQuery(document).ready(function($){
	$("form").on("submit", function(){   //catch form submission event
        var formId = $(this).attr("id");  //bucket value of id if present on current form
        var formName = $(this).attr("name");  //bucket value of name if present on current form
        var formCat = " ";
        var formAct = " ";
        /*************************************************/
        /*Added logic to search formId and assign Category*/
        /*************************************************/
            if(formId.indexOf("email")>-1 || formName.indexOf("email")>-1){
                    formCat = "Conversion Form";
                    formAct = "_Signup";
                }
                else if(formId.indexOf("hostage")>-1 || formName.indexOf("hostage")>-1){
                    formCat= "Conversion Form";
                    formAct= "_Signup";
                }
                else if(formId.indexOf("popup")>-1 || formName.indexOf("popup")>-1){
                    formCat= "Conversion Form";
                    formAct= "_Signup";
                }
				else if(formId.indexOf("main-popup-generic-plain")>-1 || formName.indexOf("main-popup-generic-plain")>-1){
                    formCat= "Conversion Form";
                    formAct= "_Signup";
                }
				 else if(formId.indexOf("pu")>-1 || formName.indexOf("pu")>-1){
                    formCat= "Conversion Form";
                    formAct= "_Signup";
                }
                else if(formId.indexOf("Web")>-1 || formName.indexOf("Web")>-1 ){
                    formCat= "Conversion Form";
                    formAct= "_Signup";
                }
                else if(formId.indexOf("single-click")>-1 || formName.indexOf("single-click")>-1){
                    formCat= "Conversion Form";
                    formAct = "_Signup";
                }
                else if(formId.indexOf("first")>-1 || formName.indexOf("first")>-1){
                    formCat= "Conversion Form";
                    formAct = "_Signup";
                }
                else if(formId.indexOf("second")>-1 || formName.indexOf("second")>-1){
                    formCat= "Conversion Form";
                    formAct = "_Signup";
                }
                else{
                    formCat= "Non Conversion Form";
                    formAct= "_Click";
            }
        
            if(formId != null && formName == null){ //if form has only ID attr - continue
                //alert(inputVal);
                ga('send', 'event', formCat, formAct, formId);
            }
            else if(formName != null && formId == null){  //if form has only Name atr - continue
                //alert(inputVal);
                ga('send', 'event', formCat, formAct, formName);
            }
            else if(formId!=null && formName!= null){   //if form has both ID and Name attr - send ID attr value
                //alert(inputVal);
                ga('send', 'event', formCat, formAct, formId);
              
            }
      })

/***********************************/   
  /*Outbound link tracking script*/ 
/***********************************/
	
	$("a").on("click", function(){
		var evtCat, evtAct, evtLab;
		var outLinkhref = $(this).attr("href"); //capture href value of current link clicked
		var currentUrl = window.location.href;  //capture current url
		var thisLab = window.location.pathname; //capture current pathname of URL
    var pubParams = window.location.search; //capture pub name on outbound link 
		var splitUrl = thisLab.split('/'); 
    var splitPub = outLinkhref.split('?');
    var pubName = splitPub[1];

    //alert(splitPub);
          
		//Send private-briefing analytics
		if(thisLab.indexOf("private")>-1){
      if(outLinkhref.indexOf("adshost")>-1){
        var adhostSplit = outLinkhref.split("u="); 
        var adhostPubname = adhostSplit[1];
        var adPubsplit = adhostPubname.split("?");
                 
        //alert(adhostPubname);
        evtCat = "Promo Link - PB Post";
				evtAct = "click to promo - " + adPubsplit[1];
				evtLab = adhostPubname;
				
				//alert(evtLab);
				ga('send', 'event', evtCat, evtAct, evtLab);
      }
			else if(outLinkhref.indexOf("pro")>-1){
        if(outLinkhref.indexOf("moneymorning")===-1){
			    evtCat = "Promo Link - PB Post";
			    evtAct = "click to promo - " + pubName;
			    evtLab = outLinkhref;
			    //alert(evtLab);
			    ga('send', 'event', evtCat, evtAct, evtLab);
        }
			}
	  }
		//Send report page analytics
		else if(thisLab.indexOf("premium")>-1){
      if(outLinkhref.indexOf("pro")>-1){
        if(outLinkhref.indexOf("moneymorning")===-1){
   				evtCat = "Promo Link - Report Upsell";
   				evtAct = "click to promo - " + pubName;
          evtLab = outLinkhref;
          //alert(evtLab);
   				ga('send', 'event', evtCat, evtAct, evtLab);
      	}
			}
	  }
		//send editorial upsell analytics
		else if(thisLab.indexOf("2014")>-1 || (thisLab.indexOf("2013")>-1)){
      if(outLinkhref.indexOf("adshost")>-1){
	      var adhostSplit = outLinkhref.split("u="); 
	      var adhostPubname = adhostSplit[1];
	      var adPubsplit = adhostPubname.split("?");
	         
	      //alert(adhostPubname);
				//evtCat = "Promo Link - Editorial Upsell";
					
				// If ad is in sidebar
				if ($(this).parents("#sidebar").length) {
					//alert("Promo Link - Editorial Upsell");
					evtCat = "Promo Link - Editorial Upsell";
				}
				// If ad is in story
				if ($(this).parents("#single-content").length) {
					//alert("Promo Link - Editorial Upsell2");
					evtCat = "Promo Link - Editorial Upsell2";
				}
				 
				evtAct = "click to promo - " + adPubsplit[1];
				var storyLab = adhostPubname;
				//alert(evtLab);
				ga('send', 'event', evtCat, evtAct, storyLab);
      }
			else if(outLinkhref.indexOf("pro")>-1){
        if(outLinkhref.indexOf("moneymorning")===-1){
          //alert("not moneymorning link");
			 		//evtCat = "Promo Link - Editorial Upsell";
					 
					// If ad is in sidebar
					if ($(this).parents("#sidebar").length) {
						//alert("Promo Link - Editorial Upsell");
						evtCat = "Promo Link - Editorial Upsell";
					}
					// If ad is in story
					if ($(this).parents("#single-content").length) {
						//alert("Promo Link - Editorial Upsell2");
						evtCat = "Promo Link - Editorial Upsell2";
					}
	        evtAct = "click to promo - " + pubName;
	        //evtLab = splitUrl[1];
	        var storyLab = outLinkhref;
	        ga('send', 'event', evtCat, evtAct, storyLab);
        }
			}
		}
		else{
      if(outLinkhref.indexOf("adshost")>-1){
        var adhostSplit = outLinkhref.split("u="); 
        var adhostPubname = adhostSplit[1];
        var adPubsplit = adhostPubname.split("?");
           
        //alert(adhostPubname);
        evtCat = "Promo Link - Web Ad";
				evtAct = "click to promo - " + adPubsplit[1];
				evtLab = adhostPubname;
				//alert(evtLab);
				ga('send', 'event', evtCat, evtAct, evtLab);
      }
			else if(outLinkhref.indexOf("pro")>-1){
        if(outLinkhref.indexOf("moneymorning")===-1){  
		      evtCat = "Promo Link - Web Ad";
		      evtAct = "click to promo - " + pubName;
		      evtLab= outLinkhref;	 
		      //alert(evtCat);
		      ga('send', 'event', evtCat, evtAct, evtLab);
				}
      }
		}
  })
	
//sidebar impression with current page url   
if($("aside#sidebar").length) {
