		// Store Author ID in JS Variable
		var tabs_author = "";

		$(document).ready(function() {

		// Change active tab based on author ID
		if (tabs_author == "11") { // Kent Moors
			$("#tabOne,#simple1Tab").addClass('active');
			$("#simple1Tab").css({"display":"block"});
			$("#tabTwo,#tabThree,#tabFour").removeClass('active');
			$("#simple2Tab,#simple3Tab,#simple4Tab").css({"display":"none"});
		}
		if (tabs_author == "12405") { // Michael Robinson
			$("#tabTwo").addClass('active');
			$("#simple2Tab").css({"display":"block"});
			$("#tabOne,#tabThree,#tabFour").removeClass('active');
			$("#simple1Tab,#simple3Tab,#simple4Tab").css({"display":"none"});
		}
		if (tabs_author == "12") { // Shah Gilani
			$("#tabThree").addClass('active');
			$("#simple3Tab").css({"display":"block"});
			$("#tabOne,#tabTwo,#tabFour").removeClass('active');
			$("#simple1Tab,#simple2Tab,#simple4Tab").css({"display":"none"});
		}
		if (tabs_author == "33") { // Kieth Fitz-Gerald
			$("#tabFour").addClass('active');
			$("#simple4Tab").css({"display":"block"});
			$("#tabOne,#tabTwo,#tabThree").removeClass('active');
			$("#simple1Tab,#simple2Tab,#simple3Tab").css({"display":"none"});
		}

		// Tabs
			var tabs = $('dl.sidebar-top-content-tabs');
			tabsContent = $('ul.sidebar-top-content-tabs-content')

			tabs.each(function(i) {
				//Get all tabs
				var tab = $(this).children('dd').children('a');
				tab.click(function(e) {

					//Get Location of tab's content
					var contentLocation = $(this).attr("href")
					contentLocation = contentLocation + "Tab";

					//Let go if not a hashed one
					if(contentLocation.charAt(0)=="#") {

						e.preventDefault();

						//Make Tab Active
						tab.removeClass('active');
						$(this).addClass('active');

						//Show Tab Content
						$(contentLocation).parent('.sidebar-top-content-tabs-content').children('li').css({"display":"none"});
						$(contentLocation).css({"display":"block"});

					}
				});
			});

		}); // end (document).ready(function()
