<script type="text/javascript">
// Omniture click tracking for SPOTLIGHT and TOP-STORIES
// SPOTLIGHT LINKS
var spotlightLinks = jQuery('#spotlight .spotlight-item');
// TOP STORIES LINKS
var topStoriesLead = jQuery('#top-stories-thumb');
var topStoriesLinks = jQuery('#top-stories #top-stories-list li');
var detectBrowser = navigator.userAgent.toLowerCase();
if(detectBrowser.indexOf('firefox')!=-1){ // Firefox
    jQuery('#spotlight .spotlight-item').click(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars='prop56';
        thisPlacement = (jQuery.inArray(this,spotlightLinks) + 1);
        if(this.id.indexOf('sponsored') > 1) {
            sponsorFlag = 'SPON';
        } else {
            sponsorFlag = '';
        }
        s.prop56='City Site :' +nbc.brand+ ' : Module Clicked : Spotlight : Placement : '+thisPlacement+' : '+this.id+' : '+sponsorFlag;
        s.tl(this,'o','City Site : '+nbc.brand+ ' : Module Clicked : Spotlight : Placement : '+thisPlacement+' : '+this.id+' : '+sponsorFlag,null,'navigate');
    });
    jQuery('#top-stories #top-stories-list li').click(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars="prop56";
        thisPlacement = (jQuery.inArray(this,topStoriesLinks) + 1); 
        s.prop56="City Site :" +nbc.brand+ " : Module Clicked : Top Stories : Placement : "+thisPlacement;
        s.tl(this,'o','City Site : '+nbc.brand+ ' : Module Clicked : Top Stories : Placement : '+thisPlacement,null,'navigate');
    });
    jQuery('#top-stories-thumb').click(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars="prop56";
        s.prop56="City Site :" +nbc.brand+ " : Module Clicked : Top Stories : Placement : 0";
        s.tl(this,'o','City Site : '+nbc.brand+ ' : Module Clicked : Top Stories : Placement : 0',null,'navigate');
    });
}else{ // All other browsers
    jQuery('#spotlight .spotlight-item').mousedown(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars='prop56';
        thisPlacement = (jQuery.inArray(this,spotlightLinks) + 1);
        if(this.id.indexOf('sponsored') > 1) {
            sponsorFlag = 'SPON';
        } else {
            sponsorFlag = '';
        }
        s.prop56='City Site :' +nbc.brand+ ' : Module Clicked : Spotlight : Placement : '+thisPlacement+' : '+this.id+' : '+sponsorFlag;
        s.tl(this,'o','City Site : '+nbc.brand+ ' : Module Clicked : Spotlight : Placement : '+thisPlacement+' : '+this.id+' : '+sponsorFlag,null,'navigate');
    });
    jQuery('#top-stories #top-stories-list li').mousedown(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars="prop56";
        thisPlacement = (jQuery.inArray(this,topStoriesLinks) + 1); 
        s.prop56="City Site :" +nbc.brand+ " : Module Clicked : Top Stories : Placement : "+thisPlacement;
        s.tl(this,'o','City Site : '+nbc.brand+ ' : Module Clicked : Top Stories : Placement : '+thisPlacement,null,'navigate');
    });
    jQuery('#top-stories-thumb').mousedown(function() {
        //nbcu.un = nbcu_account;
        //nbcu.tl(this,'o',' link tracking',null,'navigate');
        s.un = s_account;
        s.linkTrackVars="prop56";
        s.prop56="City Site :" +nbc.brand+ " : Module Clicked : Top Stories : Placement : 0";
