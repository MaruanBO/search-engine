            //<![CDATA[
            function createCookieHour(name, value) {
              var date = new Date();
              date.setTime(date.getTime() + (1 * 60 * 60 * 1000));
              var expires = "; expires="+date.toGMTString();
              document.cookie = name + "=" + value+expires + "; path=/";
            }
            $(document).ready(function() {
              // U.log(" cookie for full site "+U.getQueryParam("fullSite"));
              if(U.getQueryParam("fullSite") == "y") {
                if(U.readCookie("mobileFullSite")) {
                  // dont store cookie
                }
                else {
                  createCookieHour('mobileFullSite', 'y');   
                }
              }
            });   
            //]]>  
    </script>
