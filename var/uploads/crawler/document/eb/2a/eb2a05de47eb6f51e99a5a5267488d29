	<script>
		function setNonSecureUrl(link){
			var linkVal = link.href;
			if(linkVal.indexOf("https://")>=0) { 
				linkVal = linkVal.replace("https://","http://");
				link.href = "javascript:;";
				document.location = linkVal;
			}
		}
	</script>
