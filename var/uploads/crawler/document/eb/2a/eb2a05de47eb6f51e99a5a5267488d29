	<script> 
    setInterval(fadeItHp, 6000); 
    function fadeItHp(){ 
    if ($('#sub1a').hasClass('active')){ 
    $('#sub1a').fadeOut().removeClass('active'); 
    $('#sub1b').fadeIn().addClass('active');	
    }else if ($('#sub1b').hasClass('active')){ 
    $('#sub1b').fadeOut().removeClass('active'); 
    $('#sub1c').fadeIn().addClass('active');	
    }else if ($('#sub1c').hasClass('active')){ 
    $('#sub1c').fadeOut().removeClass('active'); 
    $('#sub1a').fadeIn().addClass('active'); 
    } 
    } 
    </script> 


