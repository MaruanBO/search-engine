$("#template-contactform").validate();

function MainPADL(which)
{

var Selected7 = document.form.which_county.selectedIndex;
var selectedOption7 = document.form.which_county.options[Selected7].value;


if (selectedOption7.length > 0 && selectedOption7 != 'Lackawanna' && selectedOption7 != 'Dauphin' && selectedOption7 != 'Blair' )

 {
  if(confirm('\n\n\n Is ' + selectedOption7 + ' County Your County Of Residence??? \n\n Click OK if it is.\n\n Otherwise Click CANCEL to choose your correct County of Residence.\n\n\n\n'))

{

return true;

}

else

{

     return (false);

}

}

else if (selectedOption7.length > 0 && selectedOption7 == 'Lackawanna' )

 {
  


if(confirm('\n\n\n Is ' + selectedOption7 + ' County Your County Of Residence??? \n\n Click OK if it is.\n\n Otherwise Click CANCEL to choose your correct County of Residence.\n\n********************\n\n If you are a resident of \n\n the City of Scranton\n\n you must purchase your dog license from the City Treasurer.\n\n Please DO NOT purchase your license through \n\n PADogLicense.com. Otherwise your \$2.00 convenience fee \n\n WIL NOT BE REFUNDED.\n\n********************'))

{

return true;

}

else

{

     return (false);

}

}


else if (selectedOption7.length > 0 && selectedOption7 == 'Dauphin' )

 {
  


if(confirm('\n\n\n Is ' + selectedOption7 + ' County Your County Of Residence??? \n\n Click OK if it is.\n\n Otherwise Click CANCEL to choose your correct County of Residence.\n\n ********************\n\n If you are a resident of \n\n the City of Harrisburg\n\n you must purchase your dog license from the City Treasurer.\n\n Please DO NOT purchase your license through \n\n PADogLicense.com. Otherwise your \$2.00 convenience fee \n\n WIL NOT BE REFUNDED.\n\n********************'))

{

return true;

} 

else

{

     return (false);

}

}

else if (selectedOption7.length > 0 && selectedOption7 == 'Blair')
 {
  
if(confirm('\n\n\n Is ' + selectedOption7 + ' County Your County Of Residence??? \n\n Click OK if it is.\n\n Otherwise Click CANCEL to choose your correct County of Residence.\n\n********************\n\n If you are a resident of \n\n the City of Altoona \n\n you must purchase your dog license from the City Code Office.\n\n Please DO NOT purchase your license through \n\n PADogLicense.com. Otherwise your \$2.00 convenience fee \n\n WIL NOT BE REFUNDED.\n\n********************'))
{

