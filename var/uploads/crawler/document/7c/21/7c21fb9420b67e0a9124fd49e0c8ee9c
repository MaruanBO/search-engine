<script type="text/javascript">
    function createCookie(name) {
        var date = new Date();
        date.setTime(date.getTime() + (3650 * 24 * 60 * 60 * 1000));
        var expires = "; expires=" + date.toGMTString();
        document.cookie = name + "=" + 'y' + expires + "; path=/";
    }
    function readCookie(name) {
        var ca = document.cookie;
        if (ca.indexOf(name) >= 0) return 'y';
        else return null;
    }
    $(document).ready(function () {
        var x = readCookie('showSignup');
        if (x == null) {
            createCookie('showSignup');
            ShowDivSignUptini();
        }
    });

    function ShowDivSignUptini() {
        DisableBackground();
        var html = $("#__dvSignUPtini").html();
        $("#__dvSignUPtini").remove();
        $("body").append(html);
        $("#divEmailSignUpTini").center();
        $("#divEmailSignUpTini").css("top", "350px");
        $("#divEmailSignUpTini").show();
        $("#txtEmailAddressSignUpTini").focus();
    }
    function HideDivSignUp() {
        $('#divEmailSignUpTini').hide();
        EnableBackground();
        return false;
    }
    function HideDivThankyou() {
        $('#divSignupThankyouTini').hide();
        EnableBackground();
        return false;
    }
    function HideDivSignUpSuccess() {
        $('#divSignupThankyouTini').hide();
        EnableBackground();
        return false;
    }
    function SubscribeToPromotion() {

        var emailAddress = trim($("#txtEmailAddressSignUpTini").val());
        var msg = "";

        if (emailAddress == "") {
            msg = 'Enter your email address';
        }
        else if (isEmailValid(emailAddress) == false) {
            msg = 'Enter a valid email address';
        }
        if (msg == "") {

            var img = new Image();
            img.src = "Subscribe.aspx?EmailAddress=" + emailAddress + "&Section=8";
            HideDivSignUp();
            DisableBackground();
            $("#divSignupThankyouTini").center();
            $("#divSignupThankyouTini").css("top", "350px");
            $("#divSignupThankyouTini").show();
        }
        else {
            emailAddress = "";
            alert(msg);
        }

        return false;
    }
    function isEmailValid(str) {
        if ($("#txtEmailAddressSignUpTini").val() == "" || $("#txtEmailAddressSignUpTini").val() == $("#txtEmailAddressSignUpTini").attr("placeholder")) {
            return false;
        }
        else {
            return /^[a-zA-Z0-9\.\-\_]+\@[a-zA-Z0-9](?:[\.\-]{0,1}[a-zA-Z0-9]+)*\.[a-zA-Z]{2,4}$/.test(str);
        }
    }

    function SignUpPressed(e) {
        var charCode = (e.which) ? e.which : e.keyCode;
        if (charCode == '13') {
            SubscribeToPromotion();
            e.cancelBubble = true;
            if (e.stopPropagation) e.stopPropagation();
            return false;
        }
    }


    function EmailSignup() {
        if ($("#txtEmailAddressSignUpTini").val() == "" || $("#txtEmailAddressSignUpTini").val() == $("#txtEmailAddressSignUpTini").attr("placeholder")) {
            $("#txtEmailAddressSignUpTini").addClass("hasPlaceholder").css("color", "#aaaaaa").val($("#txtEmailAddressSignUpTini").attr("placeholder"));
        }
        else
        {
            $("#txtEmailAddressSignUpTini").removeClass("hasPlaceholder").css("color", "#000000");

        }

    }

    function EmailSignupfocus() {
        if ($("#txtEmailAddressSignUpTini").val() == "" || $("#txtEmailAddressSignUpTini").val() == $("#txtEmailAddressSignUpTini").attr("placeholder")) {
            $("#txtEmailAddressSignUpTini").removeClass("hasPlaceholder").css("color", "#000000").val("");
        }
        else
        {
            $("#txtEmailAddressSignUpTini").removeClass("hasPlaceholder").css("color", "#000000");
        }
    }   

        
</script>
