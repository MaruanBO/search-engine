        <script language="javascript" type="text/javascript">
    var globalAvailableTime;
    var IntervalChangeMin2;    
    var MondayToFriday = 19;    
    var Sunday = 15;
    var Saturday  = 17;
    var closingTime;
    var NextDay = false;
    var ___uTickerQ;
    Date.prototype.addHours= function(h){
        this.setHours(this.getHours()+h);
        return this;
    }
    $(function() { 
        $("#divTime").easySlider({
            vertical: true,
            speed: 900,
            auto: true,
            pause: 10000,
            continuous: true,
            showPager: false            
    });
    });
    
    function InitializeClock(){
        
        var date = new Date();

        if (date.getDay() == 0){
            if(date.getHours() >= Sunday){
                closingTime = MondayToFriday 
                NextDay = true;
            }
            else{
                closingTime=Sunday;
            }
        }
        else if(date.getDay()== 6){
            if(date.getHours() >= Saturday){
                closingTime = Sunday
                NextDay = true; 
            }
            else{
                closingTime=Saturday;
            }
        }
        else if(date.getDay() > 0 && date.getDay() < 6){
           if(date.getDay() == 5 && date.getHours() >= MondayToFriday){
                closingTime = Saturday;
                NextDay = true;
            }
            else{
                closingTime=MondayToFriday;
            }
        }
        ___uTickerQ = setInterval("uTickerQ()",1000);
        }
    function uTickerQ(){
        var elmTicker = $("#Ticker");
        var elmTickerQ = $("#TickerQ");
        var date = new Date();
        var Day = date.getDay();
        var Hours = date.getHours();
        if (NextDay){
            Hours = Hours + 24;
            closingTime = closingTime + (24-date.getHours());
        }
        if (Hours<closingTime-2 && NextDay == false){
            var rHours = closingTime - date.getHours()-3;
            var rMins = 60 - (date.getMinutes()+1);
          
            elmTickerQ.html('SAME-DAY SERVICE!');
            elmTicker.html('ORDER IN THE NEXT ' + rHours +  ' HOURS '  + (rMins==0?'':rMins + ' MINUTES '));
            if (rHours == 1){
            if(rMins == 0)
                elmTicker.html(' ORDER IN THE NEXT HOUR ');
            else
                elmTicker.html(' ORDER IN THE NEXT '  + rHours +  ' HOURS ' + (rMins==0?'':rMins + ' MINUTES ') );
            }
            if (rHours == 0){
                if(rMins == 0){
                    if(closingTime >= 12)
                        elmTicker.html('STORES ARE OPEN UNTIL ' + (closingTime-12) + 'PM' );
                    else
                        elmTicker.html('STORES ARE OPEN UNTIL ' + (closingTime) + 'AM' );
                }
                else{
                    elmTicker.html(' ORDER IN THE NEXT '  +rMins +  ' MINUTES ' );
                }
            }
        }
        else if(Hours<closingTime && NextDay == false){
            elmTickerQ.html('SAME-DAY SERVICE!');

            if(closingTime >= 12)
            elmTicker.html('STORES ARE OPEN UNTIL ' + (closingTime-12) + 'PM' );
            else
            elmTicker.html('STORES ARE OPEN UNTIL ' + (closingTime) + 'AM' );
    }
    else{
            if(new Date().getHours()==0){
                NextDay = false;
                if (new Date().getDay()==0) closingTime = Sunday;
                else if(new Date().getDay()==6) closingTime = Saturday;
                else closingTime = MondayToFriday;                
                return 0;
            }
            if (date.getDay() == 0){
                if(date.getHours() >= Sunday){
                    closingTime = MondayToFriday 
                }
                else{
                    closingTime=Sunday;
                }
            }
            else if(date.getDay() == 6){
                 if(date.getHours() >= Saturday){
                    closingTime = Sunday 
                 }
                 else{
                    closingTime=Saturday;
                }
            }
            else if(date.getDay() > 0 && date.getDay() < 6){
                 if(date.getDay() == 5 && date.getHours() >= MondayToFriday){
                    closingTime = Saturday;
                 }
                else{
                    closingTime=MondayToFriday;
                }
            }
            var rHours = 24 - (date.getHours() + 3 - closingTime);
            var rMins = 60 - (date.getMinutes()+1);
            elmTickerQ.html('WANT IT TOMORROW?');
            elmTicker.html('ORDER IN THE NEXT '  + rHours + ' HOURS ' +(rMins==0?'':rMins + ' MINUTES ') );            
    }
      
    }
    InitializeClock();
    $("#dvMainTimings").show();
        </script>
    
</div>

  
