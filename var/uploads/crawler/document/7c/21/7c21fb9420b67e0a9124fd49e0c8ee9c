<script type="text/javascript">
    function EventHandler(e) {
        if (!e)
            e = window.event;
        if (e.stopPropagation) {
            e.stopPropagation();
        }
        else {
            e.cancelBubble = true;
        }

    }

    function isIE8() {
      var myNav = navigator.userAgent.toLowerCase();
      return (myNav.indexOf('msie 8.0') != -1) ? true : false;
    }

    function changeURL(elm) {
        if (typeof ContactID === "undefined") {
            ContactID = 0;
        }
        var url = '';
        if (typeof RID === 'undefined') {
            url = 't=' + new Date().getTime() + '&ContactID=' + ContactID;
        } else {
            url = 't=' + new Date().getTime() + ((RID != null && RID != 0) ? "&RID=" + RID : "");
        }
        if ($(elm).attr("href1").indexOf("ArrangementID") == -1)
            url = "?" + url;
        else
            url = "&" + url;
        if (isIE8() == true) { location.href = $(elm).attr("href1") + url; }
        else { elm.href = $(elm).attr("href1") + url; }

    }
    function sQV(ArrID, elem) {

        $("#___dvQV").remove();
        $("body").append("<div id='___dvQV'></div>");
        if (typeof ContactID === "undefined") {
            ContactID = 0;
        }
        if (typeof RID === 'undefined') {
            var url = 'https://www.ediblearrangements.com/ajax/QuickView.aspx?t=' + new Date().getTime() + '&ContactID=' + ContactID + '&ArrID=' + ArrID + '&PageURL=' + $(elem).parent("a.aArrImg").attr("href1");
        } else {
            var url = 'https://www.ediblearrangements.com/ajax/QuickView.aspx?t=' + new Date().getTime() + '&ArrID=' + ArrID + ((RID != null && RID != 0) ? "&RID=" + RID : "") + '&PageURL=' + $(elem).parent("a.aArrImg").attr("href1");
        }
        $("body #___dvQV").load(url);
    }
</script>

<script type="text/javascript">

    $(document).ready(function () {
        $(".ArrangementList .arr img:visible").each(function () {
            var t = $(this);
            var attr = $(this).attr('isrc');
            if (typeof attr !== 'undefined' && attr !== false) {
                t.attr("src", t.attr("isrc")).removeAttr("isrc");
            }
        });
        if ($("#divArrangementList .arr").lenght > 4) {
            $("#divArrangementList").easySlider({
                vertical: false,
                speed: 400,
                auto: false,
                pause: 4000,
                continuous: true,
                showPager: true,
                prevId: 'prevBtnArr',
                nextId: 'nextBtnArr',
                PagerID: 'pagerArr',
                width: "1000px",
                stopBefore: 2
            });
        }
    });
</script>

