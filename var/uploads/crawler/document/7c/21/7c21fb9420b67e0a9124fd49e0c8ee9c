        <script type="text/javascript">
            function ShowLocalizeDiv(elem) {
                if ($("#divLocalize").is(":visible") && $("#divLocalize").attr("caller") == "localize") {
                    $("#divLocalize").slideUp();
                }
                else {
                    $("#divLocalize").attr("caller", "localize");
                    $("#divLocalize").css("top", "26px");
                    $("#divLocalize").css("left", "");
                    $("#divLocalize").css("right", $(".head").width() - ($("#spanLocalize").position().left + $("#spanLocalize").outerWidth()));
                    if ($("#divLocalize").is(":visible") == false) {
                        $("#divLocalize").css("right", $(".head").width() - ($("#spanLocalize").position().left + $("#spanLocalize").outerWidth()));
                        $("#divLocalize").slideDown();
                    }
                }
            }
        </script>

