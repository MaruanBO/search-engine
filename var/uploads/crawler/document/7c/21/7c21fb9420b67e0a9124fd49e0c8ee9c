        <script type="text/javascript">
            var PreviousID = "";
            var PreviousLocation = "";
            var NewLocation = "";
            var objArrangements;
            var GSXHR;
            var _timer;
            
            function EscapeChars(txt) {
                var escapedName = txt.replace(/\\['\\]|'/g, function(s) {
                    if (s == "'") return "&apos;";
                    else return s;
                });
                return escapedName;
            }
            function searchArrangement(searchText) {
                if (searchText == '' || searchText == $("#txtArrangementSearch").attr("placeholder")) {
                    $("#txtArrangementSearch").css("border", "solid 1px #cc0000");
                }
                else {
                    $("#txtArrangementSearch").css("border", "solid 1px #999999");
                }
                location.href = encodeURI('https://www.ediblearrangements.com/fruit-arrangements?SearchText=' + searchText);
            }
            function SearchByButton() {
                if ($("#txtArrangementSearch").val() == "" || $("#txtArrangementSearch").val() == $("#txtArrangementSearch").attr("placeholder")) {
                    $("#txtArrangementSearch").css("background-color", "#FFDDDD");
                    $("#txtArrangementSearch").focus();
                }
                else {
                    var DefaultCountryID = '1';
                    var regExpAreaUS = /^\d{5}(-\d{4})?$/;
                    var regExpAreaCan = /(^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]{1}\d{1}[A-Za-z]{1} *\d{1}[A-Za-z]{1}\d{1}$)/;
                    if (DefaultCountryID == 1) {
                        if (regExpAreaUS.test($("#txtArrangementSearch").val()) == true) {
                            window.location = "https://www.ediblearrangements.com/Stores/StoreLocator.aspx?q=" + $("#txtArrangementSearch").val() + "&currloc=false";                            
                        }
                        else {
                    searchArrangement($('#txtArrangementSearch').val());
                        }
                    }
                    else if (DefaultCountryID == 2) {
                        if (regExpAreaCan.test($("#txtArrangementSearch").val()) == true) {
                            window.location = "https://www.ediblearrangements.com/Stores/StoreLocator.aspx?q=" + $("#txtArrangementSearch").val() + "&currloc=false";                            
                        }
                        else {
                            searchArrangement($('#txtArrangementSearch').val());
                        }
                    }                    
                }
            }
            
            
            //Response.Redirect(RootPath & "fruit-baskets.aspx?SearchText=" & searchBy & "&CategoryID=" & ConfigurationManager.AppSettings("DefaultCategoryID"))
            $("#txtArrangementSearch").keyup(function (e) {
                clearTimeout(_timer);
                var code = (e.keyCode ? e.keyCode : e.which);
                var txtSearch = $("#txtArrangementSearch");
                txtSearch.removeClass("validatorTxt");
                txtSearch.css("background-color", "");
                
                if (code == 13) {
                    if (txtSearch.val() == "") {
                        txtSearch.addClass("validatorTxt");
                        txtSearch.css("background-color", "#FFDDDD");
                        return false;
                    } else {
                    HideSuggestion();
                    $("#imgbtnArrangementSearch").click();
                        return false;                     
                    }
                }
                else if (code == 40 || code == 38) {
                    txtSearch.focus();
                    var Elems = $("#divSuggestion ul li");
                    var total = Elems.length;
                    var i = 0;
                    if (total > 0) {
                        var selected = $("#divSuggestion ul li.hoverClass");
                        if (selected.length == 0) {
                            if (code == 40) {
                                $($("#divSuggestion ul li")[0]).css("padding","2px 7px 2px 15px");		
				                $($("#divSuggestion ul li")[0]).addClass("hoverClass");
                                txtSearch.val($($("#divSuggestion ul li")[0]).text());
                            } else if (code == 38) {
                                var lastElm = $("#divSuggestion ul li");
				                $((lastElm)[lastElm.length - 1]).css("padding","2px 7px 2px 15px");
                                $((lastElm)[lastElm.length - 1]).addClass("hoverClass");
                                txtSearch.val($((lastElm)[lastElm.length - 1]).text());
                            }
                        } else {
                            $(Elems).each(function() {
                                selected.removeClass("hoverClass");
				                selected.css("padding","4px 7px 4px 15px");
                                if (code == 40) {
                                    var next = $(selected.next());
                                    if (next.length == 0) {
                                        txtSearch.val(txtSearch.attr("SearchText"));
                                    }
                                    else {
				                        next.css("padding","2px 7px 2px 15px");
                                        next.addClass("hoverClass");
                                        txtSearch.val(next.text());
                                    }
                                } else if (code == 38) {
                                    var prev = $(selected.prev());
                                    if (prev.length == 0) {
                                        txtSearch.val(txtSearch.attr("SearchText"));
                                    }
                                    else {
				                        prev.css("padding","2px 7px 2px 15px");
                                        prev.addClass("hoverClass");
                                        txtSearch.val(prev.text());
                                    }
                                }

                            });
                        }
                    }
                    txtSearch.focus();
                } else {
                    txtSearch.attr("SearchText", txtSearch.val());
                    if (txtSearch.val() != '') {
                        _timer = setTimeout("GetSuggestions('"+txtSearch.val()+"')", 10);
                    } else {
                        HideSuggestion();
                    }
                }
            });
            
            function GetSuggestions(SearchText) {
                var param = "{'SearchText':'" + SearchText + "'}";
                if (GSXHR) GSXHR.abort();
                GSXHR = $.get(encodeURI('https://www.ediblearrangements.com/ajax/searchcatalog.aspx?s=' + SearchText), function (data) {
                    var JsonObj = $.parseJSON(data);

                            if (JsonObj._Status == "OK") {
                                objArrangements = JsonObj._Data;
                                var TotalRec = objArrangements.length;
                                var container = $("#divSuggestion ul");
                                container.empty();

                                var i = 0;
                                $(objArrangements).each(function() {
                                    var txt = SearchText.trim().replace(/\s{2,}/g, ' ');
                                    var splitTxt = txt.split(' ');
                                    var suggestion = objArrangements[i].Name;
                                    var replacement = "***$&###";
                                    for (var j = 0; j < splitTxt.length; j++) {
                                        var regex = new RegExp('(' + splitTxt[j] + ')', 'i');
                                        suggestion = suggestion.replace(regex, replacement);
                                    }
                                    var reg2 = new RegExp('(###)', 'gi');
                                    suggestion = suggestion.replace(/\*{3}/g, "<span style='color:#000;'>").replace(/#{3}/g, "</span>");
                                    var elm = $("<li>" + suggestion + "</li>");
                                    elm.hover(function() {
                                        var e = $("#divSuggestion ul li");
                                        $(e).each(function() {
					                        $(this).css("padding","4px 7px 4px 15px"); 
                                            $(this).removeClass("hoverClass");
                                        });
					                    $(this).css("padding","2px 7px 2px 15px"); 	
                                        $(this).addClass("hoverClass");
                                    });
                                    elm.click(function() {
                                    $("#txtArrangementSearch").val($(this).text());
                                    $("#txtArrangementSearch").attr("SearchText", $("#txtArrangementSearch").val());
                                        $("#divSuggestion ul").empty();
                                        HideSuggestion();

                                        $("#imgbtnArrangementSearch").click();
                                    });
                                    container.append(elm);
                                    i++;
                                });

                                if (TotalRec > 0) {
                                    $("#divSuggestion").show();
                                }
                                else {
                                    HideSuggestion();
                                }
                            }
                });
                       
            }

            $("body").click(function() {
                HideSuggestion();
            });

            function HideSuggestion() {
                $("#divSuggestion").hide();
            }
            
            function ShowHloginDiv(){
                
                $("#divHLogin").remove();
                $("body").append("<div id='divHLogin'></div>");
                $("#divHLogin").load("https://www.ediblearrangements.com/Ajax/LoginUser.aspx?t="+new Date().getTime());
                
            }

            function ShowMyAccountMenu() {
                var windowwidth = $(window).width() -  $("#hd").width();
                $('#divMyAccMenu').css("right", "132px" );
                $('#divMyAccMenu').css("top", "26px" );
                $("#divMyAccMenu").slideToggle();
            }

            function SearchStore() {
                var txtStoreSearch = $("#txtHDSearch");
                if (txtStoreSearch.val() == "" || txtStoreSearch.val() == txtStoreSearch.attr("placeholder")) {
                    txtStoreSearch.addClass("validatorTxt");
                    txtStoreSearch.focus();
                    return false; 
                }
                else {
                    
                    if ('1' == '1') {
                    
                        FloodLightTagForStoreSearchinHeaderMenu();
                        
                    }
                    
                    setTimeout(function () { window.location = "https://www.ediblearrangements.com/stores/StoreLocator.aspx?q=" + txtStoreSearch.val() + "&currloc=false";}, 1000)                    
                }
            }
            function FloodLightTagForStoreSearchinHeaderMenu() {

                try {
                    var axel = Math.random() + "";
                    var a = axel * 10000000000000;
                    var imagTagsrc = 'https://4183063.fls.doubleclick.net/activityi;src=4183063;type=edibl198;cat=finda731;ord=' + a + '?';
                    var imgTag = new Image();
                    imgTag.src = imagTagsrc;

                }
                catch (ex) { }
            }   
                
                      
            $(document).ready(function() {
                $("#dvNeedHelp").load("https://www.ediblearrangements.com/Ajax/NeedHelp/Default.aspx");
                $("#txtHDSearch").keydown(function(e) {
                    var code = (e.keyCode ? e.keyCode : e.which);
                    if (code == 13) {
                        SearchStore();
                        return false;
                    }
                    else {
                        $("#txtHDSearch").removeClass("validatorTxt");
                    }
                });
            });           
            
            function ChangeCulture(elm) {
                var val = $(elm).html();
                var culture = "";
                if (val == "French") {
                    culture = "fr-ca";
                } else if (val == "English") {
                    culture = "en-us";
                }

                if (GSXHR) GSXHR.abort();
                GSXHR = $.ajax({
                    type: "POST",
                    url: "https://www.ediblearrangements.com/default.aspx/CultureChange",
                    data: "{'culture':'" + culture + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: 'true',
                    success: function (response) {
                        if (response.d != "null") {
                            var JsonObj = $.parseJSON(response.d);
                            if (JsonObj._Status == "OK") {
                                window.location = location.href;
                            }
                        }
                    },
                    failure: function (msg) {
                        alert(msg);
                    }
                });
            }

            function AccountSignOut() {
                if (GSXHR) GSXHR.abort();
                GSXHR = $.ajax({
                    type: "POST",
                    url: "https://www.ediblearrangements.com/default.aspx/AccountSignOut",
                    data: "",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: 'true',
                    success: function (response) {
                        if (response.d != "null") {
                            var JsonObj = $.parseJSON(response.d);
                            if (JsonObj._Status == "OK") {
                                window.location = "https://www.ediblearrangements.com/MyAccount/SignIn.aspx?ReturnUrl=%2fdefault.aspx";
                            }
                        }
                    },
                    failure: function (msg) {
                        alert(msg);
                    }
                });
            }

            function loadNeedHelp(){
                $("#dvNeedHelp").css("display", "block");
            }
        </script>
            
        

