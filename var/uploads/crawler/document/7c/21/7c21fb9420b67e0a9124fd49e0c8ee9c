    function SetCartQty(Qty) {
        if(Qty == 0){
            $("#ItemsInCart").html("");
        }
        else{
            $("#ItemsInCart").html("" + Qty);
        }
    }

    function ShowLocalizeDivFromLocalize(elem){
        if ($("#divLocalize").is(":visible") && $("#divLocalize").attr("caller") == "header") {
            $("#divLocalize").slideUp();
        }
        else{
            $("#divLocalize").attr("caller", "header");
            $("#divLocalize").css("left", "-80px");
            $("#divLocalize").css("top", $(elem).position().top + 130 + $(elem).outerHeight());
            if($("#divLocalize").is(":visible") == false){            
                $("#divLocalize").slideDown();
            }
        }
    }
    

</script>

    
