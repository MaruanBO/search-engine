            <script type="text/javascript">
                $(function() {
                    var fcol = $("#divArrMenu .first-col");
                    var lcol = $("#divArrMenu .last-col").html();
                    var height = 0;
                    var colnum = 1
                    var html = "<div class='col'>"
                    fcol.find(".unit").each(function() {
                        if (height >= 280 && colnum < 4) {
                            height = 0;
                            html += "</div><div class='col'>";
                            colnum += 1;
                            html += $(this).html();
                            var subitems = $(this).find("a.s").length;
                            var unitHeight = ((subitems * 20) + 40);
                            height += unitHeight;
                        }
                        else {
                            var subitems = $(this).find("a.s").length;
                            var unitHeight = ((subitems * 20) + 40);
                            if (height + unitHeight > 280 && colnum < 4) {
                                height = 0;
                                html += "</div><div class='col'>";
                                colnum += 1;
                                html += $(this).html();
                            }
                            else {
                                if (colnum == 4) {
                                    if (height + unitHeight > 280) {
                                        html += $(this).html();
                                    }
                                    else {
                                        html += $(this).html();
                                    }
                                }
                                else {
                                    html += $(this).html();
                                }
                            }
                            height += unitHeight;
                        }
                    });
                    html += "</div><div class='col last-col'>" + lcol + "</div>"
                    $("#divArrMenu").html(html);
                    $("#divArrMenuWrap").find(".col").each(function() {
                        if ($(this).html().replace(/\s+/g, '') == "") {
                            $(this).remove();
                        }
                    });
                    var collen = $("#divArrMenuWrap").find(".col").length;
                    if (collen <= 4) {
                        $("#divArrMenuWrap").width("767px");
                    }
                    else{
                        $("#divSubmenuAd").css("display", "none");
                    }

                });
            </script>      
