<div id="dvMainTimings" style="display: none; cursor: pointer; overflow: hidden; height: 63px;" onclick="TrackItem(4,null); window.location = 'https://www.ediblearrangements.com/fruit-arrangements'">
    
        <div id="divTime">
            <ul>
        
            
                <li id="fTimer">
                    <div class="h1Timer">
                            <span class="s1" id="TickerQ"></span> 
