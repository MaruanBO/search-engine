<script>

  jQuery(document).ready(function() {

    //  When user clicks on tab, this code will be executed
    jQuery("#tabset li").click(function(e) {
	e.preventDefault();

        //  First remove class "active" from currently active tab
        jQuery("#tabset li").removeClass('active');
 
        //  Now add class "active" to the selected/clicked tab
        jQuery(this).addClass("active");
 
        //  Hide all tab content
        jQuery(".tab_content").hide();
 
        //  Here we get the href value of the selected tab
        var selected_tab = jQuery(this).find("a").attr("href");
 
        //  Show the selected tab content
        jQuery(selected_tab).fadeIn();
 
        //  At the end, we add return false so that the click on the link is not executed
        return false;
    });
});
</script>
<script>
//script to make left hand menu collapsible
jQuery( document ).ready(function() {

//adds a div and click on the div to hide and show the children of the particular element
jQuery("#sidebar-left li.menu-item-type-post_type.menu-item-has-children").prepend("<div class='collapse'></div>");

//keep children of left hand menu hidden or open depending on which page is open
jQuery('.menu-item-type-post_type').children('.sub-menu').hide();
jQuery('.current_page_parent').children('.sub-menu').show();
jQuery('.current_page_item').children('.sub-menu').show();
jQuery('.current_page_ancestor').children('.sub-menu').show();

//add class active as a unique identifier
jQuery('.menu-item-type-post_type').each(function(){
   if ( jQuery(this).hasClass('current_page_item')  || jQuery(this).hasClass('current-menu-parent')  || jQuery(this).hasClass('current_page_ancestor') ){
        jQuery(this).find('.collapse').first().addClass('active');
   }
});

//if collapse div of current menu is clicked, then show/hide the children of the div with active class
jQuery(".collapse").click(function(e){    
       jQuery(this).toggleClass('active');
   if ( jQuery(this).hasClass('active') ){
    jQuery(this).parent().find('ul.sub-menu').first().slideDown('slow');
   } else { 
    jQuery(this).parent().find('ul.sub-menu').first().slideUp('slow');
   }
});

});
</script>
