<script type='text/javascript'>
var LocationErrorString = "Please double check the location format you entered and try again.";
var selectedIndex = 0;
var CitySuggestBlurred = true;
var SelectingItems = false;
var EnterPressed = false;
(function($) {
    $(document).ready(function() {
        //Performance optimization, decorates an existing function call with a debounce throttle.
        //Allow's the user to rapid-fire keystroke without bombarding auto-suggest server.
        var debounceAjax = $.debounce(function(el){
            SendAndFillSuggestions($(el).val());
        }, 300);

        $("#Location").keyup(function(key) {
            if (!SelectingItems && key.which != 13) { //Not the "Enter" button and we're not selecting any suggestions
                if ($(this).val().length >= 3) {
                    //SendAndFillSuggestions($(this).val());
                    debounceAjax(this);
                    selectedIndex = -1;
                }
            }
        });

        $("#Location").keydown(function(key) {
            if (key.which == 13)
                $("#citySuggest").hide();

            if (String.fromCharCode(key.which) == "&") // Up key
            {
                if (selectedIndex > 0)
                    selectedIndex--;

                SelectingItems = true;
            }
            else if (String.fromCharCode(key.which) == "(") // Down key
            {
                selectedIndex++;
                SelectingItems = true;
            }
            else
                SelectingItems = false;

            HoverSelection();
        });

        $("#Location").keypress(function(key) {
            // if it's the "Enter" button
            if (selectedIndex != -1 && key.which == 13) {
                $("#Location").val(cleanSuggestion($("#citySuggest li[index=\"" + selectedIndex + "\"]").find("span:first").text()));
                $("#aSearch").trigger("click");
                return false;
            }
        });

        $(this).click(function(){
            $("#citySuggest").hide();
        });
    });

    function stringToRegExp(pattern, flags) {
        return new RegExp(
            pattern.replace( /[\[\]\\{}()+*?.$^|]/g , function(m) { return '\\' + m; }),
            flags);
    }

    function makeBold(m) {
        return '<b>' + m + '<\/b>';
    }

    function cleanSuggestion(value) {
        //Begin ugly hack/strip
        //1. When pasting either us zip or ca zip do not paste in paranthesis portion
        return value.replace(/\s\(.*\)/, '');
    }

    function SendAndFillSuggestions(Value) {

        var data = {"i":Value,"u":"false"};

        //check if we need to wrap the data ourselves or if ajaxsetup.js is doing it for us
        //assume if $.ajaxSettings.complete has been set that things are setup already
        if(!$.ajaxSettings.complete) {
            //this page isn't using ajaxsetup.js yet, we need to wrap the JSON
            data = {"d":data};
        }

        //set up request for data
        var suggestionRequest = {
            type: "POST",
            url: "/xNet/ExternalServices/Address/GeoLookup.svc/GetJSONLocationSuggestions",
            data: $.toJSON(data),
            contentType: "application/json; charset=utf-8",
            success: function(data) {
                var Address = data.r.l;
                if (typeof(data.r.l) != 'undefined' && data.r.l != null &&
                    data.r.l.length > 0) {

                    var container = $("#citySuggest");

                    container[0].innerHTML = '';
                    container.show();

                    var uLContainer = document.createElement("ul");

                    container.append(uLContainer);

                    pattern = stringToRegExp(Value, "gi");

                    for (i = 0; i < data.r.l.length; ++i) {
                        var liContainer = document.createElement("li");
                        var spanContainer = document.createElement("span");
                        var suggestion = data.r.l[i].s;

                        $(uLContainer).append(liContainer);

                        $(spanContainer).attr("title", suggestion);

                        suggestion = suggestion.replace(pattern, makeBold);

                        $(liContainer).append(spanContainer);

                        $(spanContainer).append(suggestion);

                        $(liContainer).click(function() {
                            $("#Location").val(cleanSuggestion($(this).find("span:first").text()));
                            $(uLContainer).hide();
                            $("#Location").css("color", "#000");
                        });

                        $(liContainer).mouseover(function() {
                            selectedIndex = $(this).attr("index");
                            HoverSelection();
                        });

                        $(liContainer).mouseout(function() {
                            $(this).attr("class", "");
                        });
                    }

                    $("#citySuggest li").each(function(i) {
                        $(this).attr("index", i);
                    });
                }
            },
            error: function(xhr, options, errorMessage) {
                document.write(xhr.responseText);
            },
            complete: function(oResp, Status) {

                var text = (!oResp || typeof oResp.responseText === "unknown") ? "" : oResp.responseText;

                if (typeof(ErrorCodes) != 'undefined' && ErrorCodes != null) {
                    if (text.indexOf("\"err\":{\"c\":" + ErrorCodes.ab) !== -1) {
                        //abuse error!
                        window.location = '/xnet/mainsite/accessdenied.aspx';
                    }
                }
            }
        };

        //if the page supports ajax queuing use it
        if($.ajaxQ && window.PAGE_AjaxQueueName)
        {
            $.ajaxQ.add(PAGE_AjaxQueueName, suggestionRequest);
        }
        else
        {
            $.ajax(suggestionRequest);
        }
    }

    function HoverSelection() {
        $("#citySuggest li").attr("class", "");
        $('#citySuggest li[index="' + selectedIndex + '"]').attr("class", "selected");
    }

    $("body").bind('click', function (Event) {
        var target = $(Event.target);

        if (target.hasClass('button') ||
            target.hasClass('typeSelect') ||
            target.hasClass('quickSearchType'))
        {
            return;
        }

        if ($('#multiLevelOptionsWrapper:visible').length > 0) {
            $('#multiLevelOptionsWrapper:visible').hide();
        }
    });
})(jQuery);

function showDrop(sId, sTypeId){
    if (document.getElementById(sTypeId).style.display == '' || document.getElementById(sTypeId).style.display == 'block') {//now hide
		document.getElementById(sId).className = 'quickSearchType';
		document.getElementById(sTypeId).style.display = 'none';
		document.getElementById(sTypeId).style.visibility = 'hidden';
	} else {//now show
		if( sId === 'quickSearchPRUpsell' && !quickSearchPRUpsellShown)
		{
		    LogActionCode(1598, PageExecutionGuid);
		    quickSearchPRUpsellShown = true;
		}
		document.getElementById(sId).className = 'quickSearchType quickSearchHover';
		document.getElementById(sTypeId).style.display = 'block';
		document.getElementById(sTypeId).style.visibility = 'visible';
		$('#' + sTypeId).css('visibility', 'visible'); //--- IE fix
	}
}
</script>

		
<script type="text/javascript">
(function($) {
    var quickSearchPRUpsellShown = false;

    var PageExecutionGUID = 'be467613-17c5-434e-8e89-456e8652a436';
    var UniversalQuickSearch = ({
    	DropDownID: 'aTabbedDropDown',
    	CategoryName: '',
    	OptionsWrapperID: 'tabbedOptionsWrapper',
    	SearchType: 'FS', //FS: default (FS, FL, RS, MP)
    	CategoryID: '',
    	Location: "Enter a location",
    	DefaultLocationWhenEmptyOnSubmit: "Singapore, SI",
    	BlurString: 'Enter a location',
    	TempDontHide: false,
    	LocationInputField: document.getElementById('Location'),
    	HelpSuggest: document.getElementById('searchSuggest'),
    	HelpHover: false,

    	initialize: function () {
    	    
    		$(this.LocationInputField).val(this.Location);

    		if ($("#Location").val() == "Enter a location")
    			$("#Location").css("color", "#999");
    		else
    			$("#Location").css("color", "#000");

    		$('#AdditionalSearchOptions').show();

    		if (this.SearchType == "FL") {
    			if (searchDisplayType == 'Tabbed') {
    				toggleTabs(document.getElementById('tabFL'), null, 'selected', 'a', this.SearchType, 'TabbedFLOptions');
    			}
    			else if (searchDisplayType == 'MultiLevel') {
    				this.SelectPropertyType(this.CategoryName, this.CategoryID, this.SearchType);
    			}

    			$('#FS_MoreOptions').hide();
    			$('#FL_MoreOptions').removeAttr("style");
    			$('#RS_MoreOptions').hide();
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').hide();
    			$('#HelpAdvancedSearchFL').show();
    			$('#HelpAdvancedSearchRS').hide();
    			$('#orCanadian').html(' or Canadian');
    		}
    		else if (this.SearchType == "FS") {
    		    if (searchDisplayType == 'Tabbed') {
    		        toggleTabs(document.getElementById('tabFS'), null, 'selected', 'a', this.SearchType, 'TabbedFSOptions');
    		    }
    			else if (searchDisplayType == 'MultiLevel') {
    				this.SelectPropertyType(this.CategoryName, this.CategoryID, this.SearchType);
    			}
    			$('#FS_MoreOptions').removeAttr("style");
                $('#FL_MoreOptions').hide();
    			$('#RS_MoreOptions').hide();
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').show();
    			$('#HelpAdvancedSearchFL').hide();
    			$('#HelpAdvancedSearchRS').hide();
    			$('#HelpAdvancedSearchMP').hide();
    			$('#orCanadian').html(' or Canadian');
    		}
    		else if (this.SearchType == "RS") {
    			if (searchDisplayType == 'Tabbed') {
    				toggleTabs(document.getElementById('tabRS'), null, 'selected', 'a', this.SearchType, 'TabbedRSOptions');
    			}
    			else if (searchDisplayType == 'MultiLevel') {
    				this.SelectPropertyType(this.CategoryName, this.CategoryID, this.SearchType);
    			}
    			$('#FS_MoreOptions').hide();
    			$('#FL_MoreOptions').hide();
                $('#RS_MoreOptions').removeAttr("style");
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').hide();
    			$('#HelpAdvancedSearchFL').hide();
    			$('#HelpAdvancedSearchRS').show();
    			$('#HelpAdvancedSearchMP').hide();
    			$('#orCanadian').html('');
    		}
    		else if (this.SearchType == "MP") {
    			if (searchDisplayType == 'Tabbed') {
    				toggleTabs(document.getElementById('tabMP'), null, 'selected', 'a', this.SearchType, 'TabbedMPOptions');
    			}
    			else if (searchDisplayType == 'MultiLevel') {
    				this.SelectPropertyType(this.CategoryName, this.CategoryID, this.SearchType);
    			}
    			$('#FS_MoreOptions').hide();
    			$('#FL_MoreOptions').hide();
    			$('#RS_MoreOptions').hide();
    			$('#MP_MoreOptions').removeAttr("style");
    			//hide search options for MP
    			$('#AdditionalSearchOptions').show();
    			$('#HelpAdvancedSearchFS').hide();
    			$('#HelpAdvancedSearchFL').hide();
    			$('#HelpAdvancedSearchRS').hide();
    			$('#HelpAdvancedSearchMP').show();
    			$('#orCanadian').html('');
    		}

    		this.addEvent(this.LocationInputField, 'focus', this.locationFocusHandler);
    		this.addEvent(this.LocationInputField, 'blur', this.locationBlurHandler);
    		this.addEvent(this.LocationInputField, 'keydown', this.locationKeyDownHandler);
    		this.addEvent(this.DropDownID, 'blur', this.propertyTypeBlurHandler);
    		this.addEvent(this.DropDownID, 'focus', this.propertyTypesFocusHandler);
    		this.addEvent(this.DropDownID, 'click', this.propertyTypesFocusHandler);

            //horrible code; please change to use a common class for this!
    		this.addEvent($('#HelpAdvancedSearchMP')[0], 'mouseover', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchMP')[0], 'mouseout', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchFS')[0], 'mouseover', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchFS')[0], 'mouseout', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchFL')[0], 'mouseover', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchFL')[0], 'mouseout', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchRS')[0], 'mouseover', this.helpHoverHandler);
    		this.addEvent($('#HelpAdvancedSearchRS')[0], 'mouseout', this.helpHoverHandler);
    	},

    	helpHoverHandler: function (event) {
    		UniversalQuickSearch.HelpHover = !UniversalQuickSearch.HelpHover;
    	},


    	propertyTypesFocusHandler: function (event) {
    	},

    	locationFocusHandler: function (event) {
    		LocationInputjQ = $(UniversalQuickSearch.LocationInputField)
    		LocationInputjQ.css('color', '#000');
    		if (LocationInputjQ.val() == UniversalQuickSearch.BlurString) {
    			LocationInputjQ.val('');
    			if (UniversalQuickSearch.HelpSuggest)
    				$(UniversalQuickSearch.HelpSuggest).show();
    		}

    	},

    	locationBlurHandler: function (event) {
    		LocationInputjQ = $(UniversalQuickSearch.LocationInputField);
    		if (LocationInputjQ.val() == '') {
    			LocationInputjQ.css('color', '#999');
    			LocationInputjQ.val(UniversalQuickSearch.BlurString);
    			if (!UniversalQuickSearch.HelpHover) {
    				$(UniversalQuickSearch.HelpSuggest).hide();
    			}
    		}
    	},

    	locationKeyDownHandler: function (event) {
    		if (UniversalQuickSearch.HelpSuggest)
    			$(UniversalQuickSearch.HelpSuggest).hide()
    	},

    	propertyTypeBlurHandler: function (event) {
    		setTimeout('hidePropertyTypes()', 200);
    	},

    	addEvent: function (obj, ev, handler) {
    		if (!obj) return;

    		if (obj.addEventListener)
    			obj.addEventListener(ev, handler, false);
    		else if (obj.attachEvent)
    			obj.attachEvent('on' + ev, handler);
    	},

    	SelectPropertyType: function (categoryname, categoryId, searchtype) {            

    		$("#" + this.DropDownID).css('color', '#3765B7');
    		if (searchDisplayType == 'MultiLevel') { $('#multiLevelOptionsWrapper_ErrorAlert').hide(); }
    		var valueHasChanged = false;

    		if (
                searchtype != UniversalQuickSearch.SearchType ||
                categoryId != UniversalQuickSearch.CategoryID) {
    			valueHasChanged = true;
    		}


    		this.CategoryName = categoryname;
    		var selectedItemDisplay = categoryname;

    		$('#AdditionalSearchOptions').show();
    		if (searchtype == "FS") {
    			$('#FS_MoreOptions').removeAttr("style");
    			$('#FL_MoreOptions').hide();
    			$('#RS_MoreOptions').hide();
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').show();
    			$('#HelpAdvancedSearchFL').hide();
    			$('#HelpAdvancedSearchRS').hide();
    			$('#HelpAdvancedSearchMP').hide();
    			$('#orCanadian').html(' or Canadian');
    		}
    		if (searchtype == "FL") {
    			$('#FS_MoreOptions').hide();
    			$('#FL_MoreOptions').removeAttr("style");
    			$('#RS_MoreOptions').hide();
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').hide();
    			$('#HelpAdvancedSearchFL').show();
    			$('#HelpAdvancedSearchRS').hide();
    			$('#HelpAdvancedSearchMP').hide();
    			$('#orCanadian').html(' or Canadian');
    		}
    		if (searchtype == "RS") {
    			$('#FS_MoreOptions').hide();
    			$('#FL_MoreOptions').hide();
    		    $('#RS_MoreOptions').removeAttr("style");
    			$('#MP_MoreOptions').hide();
    			$('#HelpAdvancedSearchFS').hide();
    			$('#HelpAdvancedSearchFL').hide();
    			$('#HelpAdvancedSearchRS').show();
    			$('#HelpAdvancedSearchMP').hide();
    			$('#orCanadian').html('');
    		}
    		if (searchtype == "MP") {
    			$('#AdditionalSearchOptions').show()
    			$('#FS_MoreOptions').hide()
    			$('#FL_MoreOptions').hide()
    			$('#RS_MoreOptions').hide()
    			$('#MP_MoreOptions').removeAttr("style");
    			$('#HelpAdvancedSearchFS').hide()
    			$('#HelpAdvancedSearchFL').hide()
    			$('#HelpAdvancedSearchRS').hide()
    			$('#HelpAdvancedSearchMP').show()
    			$('#orCanadian').html('');
    		}

    		if (searchtype == "FS" && searchDisplayType == 'MultiLevel') {
    		    

    		    if(categoryId === '0'){

    		        selectedItemDisplay = 'All properties for sale';

    		    } else {

    		        selectedItemDisplay += ' For Sale';
    		    }


    		}
    		if (searchtype == "FL" && searchDisplayType == 'MultiLevel' && categoryId != '') {
    		    

    		    if(categoryId === '0'){

    		        selectedItemDisplay = 'All properties for lease';

    		    } else {

    		        selectedItemDisplay += ' For Lease';
    		    }


    		}
    		if (searchtype == "RS" && searchDisplayType == 'MultiLevel' && categoryId != '') {
    		    if(categoryId === '0'){

    		        selectedItemDisplay = 'All sales comps';

    		    } else {

                    selectedItemDisplay = 'Sold ' + selectedItemDisplay;
    		    }
    			
    		}
    		if (searchtype === "MP" && searchDisplayType == 'MultiLevel' && categoryId != '') {
    			switch (categoryId) {
    				case '0': /*All*/
    					selectedItemDisplay = 'All Property Record Types';
    					break;
    				case 100: /*Senior Housing*/
    					selectedItemDisplay = 'Sr. Housing Property Records';
    					break;
    				case 110: /*Shopping Center*/
    					selectedItemDisplay += ' Records';
    					break;
    				case 120: /*Special Purpose*/
    					selectedItemDisplay = 'Special Purp. Property Records';
    					break;
    				case 130: /*Sport & Ent*/
    					selectedItemDisplay = 'Sport & Ent. Property Records';
    					break;
    				//                    case 140:/*Res. Income*/                                                                     
    				//                        selectedItemDisplay = 'Res. Income Property Records';                                                                     
    				//                    break;                                                                     
    				case 150: /*Commercial Other*/
    					selectedItemDisplay = 'Other Property Records';
    					break;
    				default:
    					selectedItemDisplay += ' Property Records';
    					break;
    			}
    		}

    		// hack to trim the name so it looks nice in the textbox
    		if (searchtype == "FS" && categoryId == 130 && searchDisplayType == 'MultiLevel') {
    			selectedItemDisplay = 'Sport & Ent. For Sale';
    		}

    		UniversalQuickSearch.SearchType = searchtype;
    		UniversalQuickSearch.CategoryID = categoryId;

    		if (valueHasChanged && this.onchange && typeof (this.onchange) == "function") {
    			this.onchange();  // probably not the best way to "fire an event"
    		}

    		$("#" + this.DropDownID).html(selectedItemDisplay);

            //--- Weird IE fix
            $('#MultiLevelFSOptions').css('visibility', 'hidden');
    		$('#MultiLevelFLOptions').css('visibility', 'hidden');
    		$('#MultiLevelRSOptions').css('visibility', 'hidden');
    		$('#MultiLevelMPOptions').css('visibility', 'hidden');

    		hidePropertyTypes();
    	},



    	DoSearch: function (event) {
    		if ($("#" + this.DropDownID).html() != null && typeof ($("#" + this.DropDownID).html()) != 'undefined' &&
				$("#" + this.DropDownID).html().indexOf("Select a Property Type") > -1) {
    			UniversalQuickSearch.TempDontHide = true;
    			if (document.getElementById(activeOptionList).style.display == 'none') {
    				UniversalQuickSearch.togglePropertyTypesFromLink();
    			}
    			else {
    				$("#" + this.DropDownID).focus();
    			}
    			if (searchDisplayType == 'MultiLevel') { $('#multiLevelOptionsWrapper_ErrorAlert')[0].style.display = 'block'; }

    			setTimeout("UniversalQuickSearch.TempDontHide=false;", 203);
    			if ($.lnlogging) { $.lnlogging.LogActionCode({ ActionCode: 1130, Queueable: false }); }
    			return false;
    		}

    		var QS_Form = document.createElement("FORM");
    		document.body.appendChild(QS_Form);

    		var FromUniversalInput = document.createElement("INPUT");
    		FromUniversalInput.type = "hidden";
    		FromUniversalInput.name = "FromUniversalSearch";
    		FromUniversalInput.value = "true";

    		var QuickSearchBlurString = document.createElement("INPUT");
    		QuickSearchBlurString.type = "hidden";
    		QuickSearchBlurString.name = "UniversalSearchBlurString";
    		QuickSearchBlurString.value = UniversalQuickSearch.BlurString;

    		var LocationInput = document.createElement("INPUT");
    		LocationInput.type = "hidden";
    		LocationInput.name = "UniversalSearchLocation";


    		if (UniversalQuickSearch.LocationInputField.value == '' || UniversalQuickSearch.LocationInputField.value == UniversalQuickSearch.BlurString) {
    			LocationInput.value = UniversalQuickSearch.DefaultLocationWhenEmptyOnSubmit;
    		} else {
    			LocationInput.value = UniversalQuickSearch.LocationInputField.value;
    		}

    		var PropertyTypeInput = document.createElement("INPUT");
    		PropertyTypeInput.type = "hidden";
    		PropertyTypeInput.name = "UniversalSearchPropertyType";
    		PropertyTypeInput.value = UniversalQuickSearch.CategoryID;

    		var doSearchLinkCode = "19690";
    		var linkCodeQS = "";

    		if (parseInt(doSearchLinkCode) > 0)
    			linkCodeQS = "?linkcode=" + doSearchLinkCode;

    		if (UniversalQuickSearch.SearchType == "FS") {
    			QS_Form.action = "http://www.loopnet.com/forsale/" + linkCodeQS;
    		}
    		else if (UniversalQuickSearch.SearchType == "FL") {
    			QS_Form.action = "http://www.loopnet.com/forlease/" + linkCodeQS;
    		}
    		else if (UniversalQuickSearch.SearchType == "RS") {
    			QS_Form.action = "http://www.loopnet.com/salescomps/" + linkCodeQS;
    		}
    		else if (UniversalQuickSearch.SearchType == "MP") {
    			QS_Form.action = "http://www.loopnet.com/propertyrecords/" + linkCodeQS;
    		}

    		QS_Form.appendChild(PropertyTypeInput);
    		QS_Form.appendChild(LocationInput);
    		QS_Form.appendChild(FromUniversalInput);
    		QS_Form.appendChild(QuickSearchBlurString);
    		QS_Form.method = "POST";

    		QS_Form.submit();

    		return false;
    	},

    	togglePropertyTypesFromLink: function () {
    		if (document.getElementById(activeOptionList).style.display != 'none') {
    			togglePropertyTypes();
    		}
    		else {
    			UniversalQuickSearch.TempDontHide = true;
    			togglePropertyTypes();

    			var actionCode = $("#"+activeOptionList).attr('actioncode');

					if (typeof (actionCode) != 'undefined' && !isNaN(actionCode)) {
						var requestData = {"d":{"ActionCode":-2147483648,"ActionCodeList":null,"ActionCodeValue":"-2147483648","LinkCodeList":null,"PageGuid":"be467613-17c5-434e-8e89-456e8652a436"},"r":{"ci":-2147483648,"fn":"","ln":"","pg":"be467613-17c5-434e-8e89-456e8652a436","rai":-2147483648,"sg":"9f4b870d-5f90-45fd-889e-66675e267fdd","sid":1,"ai":"JIy5vYI6w","si":"JIy5vYI6w","f":0,"c":"J759pGc0Q"}};
						requestData.d.ActionCode = actionCode;
						$.sendWCFRequest("/xNet/ExternalServices/Common/Logging.svc/UpdateActionCode", "POST", $.toJSON(requestData), null, null, null);
					}

    			setTimeout("UniversalQuickSearch.TempDontHide=false;", 203);
    		}
    	}
    });


    // wrapper for RemoteUniversalSuggest .. onReturn()
    var QuickSearch = ({
        createAndSendFormHandler: function (event) {
            UniversalQuickSearch.DoSearch(event);
        }
    });

    function togglePropertyTypes() {
        toggle(activeOptionList);
        //changeclass();

        if (document.getElementById(activeOptionList).style.display != 'none') {
            $("#"+UniversalQuickSearch.DropDownID).focus();
        }
    }

    function hidePropertyTypes() {
        if (!UniversalQuickSearch.TempDontHide) {
            document.getElementById(activeOptionList).style.display = 'none';
            //  document.getElementById("propTypesInitialDropDown").className = 'quickSearchMain';
        }
    }

    function toggleActiveOptionList() {
        if (activeOptionList != null) {
            var e = $("#"+activeOptionList);
            if (e.css('display') == 'none')
                e.show();
            else
            	e.hide();
        }
    }
    // simple function to toggle a div visible or not.
    function toggle(id) {
        var e = document.getElementById(id);
        if (e.style.display == 'none')
            e.style.display = '';
        else
            e.style.display = 'none';
    }

    toggleTabs = function(element, actionCode, activeClass, desiredTag, searchType, optionList) {
        if (actionCode != null && typeof (actionCode) != 'undefined' && !isNaN(actionCode)) {
			var RequestData = {"d":{"ActionCode":-2147483648,"ActionCodeList":null,"ActionCodeValue":"-2147483648","LinkCodeList":null,"PageGuid":"be467613-17c5-434e-8e89-456e8652a436"},"r":{"ci":-2147483648,"fn":"","ln":"","pg":"be467613-17c5-434e-8e89-456e8652a436","rai":-2147483648,"sg":"9f4b870d-5f90-45fd-889e-66675e267fdd","sid":1,"ai":"JIy5vYI6w","si":"JIy5vYI6w","f":0,"c":"J759pGc0Q"}};
			RequestData.d.ActionCode = actionCode;
    	    $.sendWCFRequest("/xNet/ExternalServices/Common/Logging.svc/UpdateActionCode", "POST", $.toJSON(RequestData), null, null, null);
    	}

		var elements = element.parentNode.parentNode.getElementsByTagName(desiredTag);

        for (var i = 0; i < elements.length; i++)
            elements[i].className = "";

        element.className = activeClass;
        activeTabSearchType = searchType;
        activeOptionList = optionList;
        $tabbedDropdown = $('#aTabbedDropDown');
        $('#TabbedFSOptions').hide();
        $('#TabbedFLOptions').hide();
        $('#TabbedRSOptions').hide();
        $('#TabbedMPOptions').hide();

        switch (searchType) {
            case "FS":
                $tabbedDropdown.attr('title','All properties for sale');
                setCorrespondingListItem(FSCategories, searchType, 'All properties for sale');
                break;
            case "FL":
                $tabbedDropdown.attr('title','All properties for lease');
                setCorrespondingListItem(FLCategories, searchType, 'All properties for lease');
                break;
            case "RS":
                $tabbedDropdown.attr('title','All properties comps');
                setCorrespondingListItem(RSCategories, searchType, 'All sales comps');
                break;

            case "MP":
                $tabbedDropdown.attr('title','All properties records');
                setCorrespondingListItem(MPCategories, searchType, 'All property records');
            default:
        }
    };
    window.toggleTabs = toggleTabs;

    function setCorrespondingListItem(categoryIDArray, searchType, defaultCategoryName)
    //select an item based on previous list for Tabbed QuickSearch
    {
        var len = categoryIDArray.length;
        var itemFound = false;
        for (var i = 0; i < len; i++) {
            if (categoryIDArray[i] == UniversalQuickSearch.CategoryID) //found
            {
                itemFound = true;
                UniversalQuickSearch.SelectPropertyType(UniversalQuickSearch.CategoryName, categoryIDArray[i], searchType);
                break;
            }
        }

        if (!itemFound) {
            UniversalQuickSearch.SelectPropertyType(defaultCategoryName, '', searchType);
        }
    }

    // class change on the "dropdown anchor" to add a small "lip" 
    // to connect with the large dropdown dialogue.
    function changeclass() {
        // tclass = document.getElementById("propTypesInitialDropDown");
        //if (tclass.className == 'quickSearchMain') {
        //    tclass.className = "quickSearchMain dropdownActive";
        //} else {
        //    tclass.className = 'quickSearchMain';
        //}
    }

    UniversalQuickSearch.initialize();

    $("body").bind('click', function (Event) {
        var target = $(Event.target);
        if (target.hasClass('singleDropdown') || 
            target[0].id === 'propTypesInitialDropDown' || 
            target.parent()[0].id === 'propTypesInitialDropDown')
        {
            return;
        }

        if ($('.singleDropdown:visible').length > 0) {
            $('.singleDropdown:visible').hide();
        }
    });

    window.UniversalQuickSearch = UniversalQuickSearch;
})(jQuery)
</script>

<!--Quick Search Control END-->

