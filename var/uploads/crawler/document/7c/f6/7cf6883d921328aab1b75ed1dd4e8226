<script type="text/javascript">
    function AsyncCall(url, redirect) {
        var xmlHttp = new XMLHttpRequest();
        if (redirect) {
            xmlHttp.onreadystatechange = function () {
                if (this.readyState === 4) {
                    window.location = redirect;
                }
            };
        }
        xmlHttp.open("GET", url, true);
        xmlHttp.send(null);
    }
</script>

