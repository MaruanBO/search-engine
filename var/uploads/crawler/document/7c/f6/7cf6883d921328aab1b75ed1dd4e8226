<script language="javascript">

    function ShowSiteMap(){
        SeoResize();
        document.getElementById("BottomSEOLinks_divSEOLinks").style.visibility = "visible";
    }

    function HideSiteMap(){
        document.getElementById("BottomSEOLinks_divSEOLinks").style.visibility = "hidden";
    }

    if (window.attachEvent){
        window.attachEvent("onresize", SeoResize);
    }
    else if (window.addEventListener){
        window.addEventListener("resize", SeoResize, false);
    }

    function SeoResize(){
        var myWidth = 0, myHeight = 0;
        var seoDiv = document.getElementById("BottomSEOLinks_divSEOLinks");
        if(!seoDiv){return;}
		 if( document.documentElement &&
			( document.documentElement.clientWidth || document.documentElement.clientHeight ) ) {
			//IE 6+ in 'standards compliant mode'
			myWidth = document.documentElement.clientWidth;
			myHeight = document.documentElement.clientHeight;
		} else if( document.body && ( document.body.clientWidth || document.body.clientHeight ) ) {
			//IE 4 compatible
			myWidth = document.body.clientWidth;
			myHeight = document.body.clientHeight;
		}

		var myDivWidth = parseInt(seoDiv.offsetWidth.toString().replace("px", ""),10);
		var myDivHeight = parseInt(seoDiv.offsetHeight.toString().replace("px", ""),10);

		seoDiv.style.left = (((myWidth - myDivWidth) - 1) / 2).toString() + "px";  // - 2 is added for shadow offset
		seoDiv.style.top = ((myHeight - myDivHeight) / 2  + getScrollTop()).toString() + "px";
    }

    function getScrollTop(){
        if(typeof pageYOffset != 'undefined'){
            //most browsers
            return pageYOffset;
        }
        else{
            var B = document.body; //IE 'quirks'
            var D = document.documentElement; //IE with doctype
            D = (D.clientHeight) ? D: B;
            return D.scrollTop;
        }
    }


</script>
</form>


