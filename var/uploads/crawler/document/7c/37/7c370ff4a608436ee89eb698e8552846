<script type="text/javascript">

	$(window).load(function () {
	
		$('.new-releases-carousel-holder').lemmonSlider({
			infinite: true,
			loop: true
		});
		
		
//		if ("ontouchstart" in document.documentElement) {
//		
//			$(".new-releases-carousel-holder").swipe({
//			    swipeLeft: function () {
//			        $('.controls .next-slide').click();
//			    },
//			    swipeRight: function () {
//			        $('.controls .prev-slide').click();
//			    },
//			    threshold: 0
//			});
//		}
	
	

//		if ("ontouchstart" in document.documentElement == false) {
			
			$(".new-releases-carousel .img-wrapper img" ).mouseover(function(event){
				event.preventDefault();
				
				$(".new-releases-carousel li").removeClass('active');
				var currentisbn = $(this).closest("li").addClass('active').attr("data-id");
				
				$('.pop-up').removeClass('rightSide').hide();
				var lPos= $(".new-releases-carousel li.active").position().left;
				
				if(lPos > 400){
					lPos = lPos - 350;
					$("#pop-up-"+currentisbn).css('left',lPos).addClass('rightSide').show();
				}else{
					lPos = lPos + 225;
					$("#pop-up-"+currentisbn).css('left',lPos).show();
				}
				return false
			});
			
			$(".new-releases-carousel .img-wrapper img" ).mouseout(function(event){
				closePopup();
			});
			
			$('.next-slide, .prev-slide').mouseout(function () {
				closePopup();
			});
			
//		}
	});	
	

	function closePopup(){
		$('.pop-up').removeClass('rightSide').hide();
		
		$(".new-releases-carousel li").removeClass('active');
	}
	
	 

	

</script>







