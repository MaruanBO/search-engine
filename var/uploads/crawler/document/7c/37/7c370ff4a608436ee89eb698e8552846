<script>
$('.cycle-slideshow-ticker').cycle({
    speed: 1000,
    sync:false,
    slides: '.slide-ticker'
});

jQuery(document).ready(function($){

    $("a[_cke_saved_href]").each(function(){
        $(this).attr('href',$(this).attr('_cke_saved_href'));
        $(this).removeAttr('_cke_saved_href');
    });

    var slideshows = $('.cycle-slideshow-ticker').on('cycle-next cycle-prev', function(e, opts) {
        // advance the other slideshow
        slideshows.not(this).cycle('goto', opts.currSlide);
    });

});
</script>




