<!-- MOBILE VERSION -->

<script type="text/javascript" src="/jslibrary/cycle2"></script>
<script>
$(function(){
	$('body').addClass('slider0');

	$('.cycle-pager span').click(function () {
		dataLayer.push({ 'event_category': 'Homepage', 'event_action': 'Billboard', 'event_label' : 'Tile Button', 'event_value' : ''+ $(this).index()+''});
	});

});

$('.cycle-slideshow').cycle({
    	speed: 1000,
    	sync:false,
		slides: '.slide'
	});


	$( '.cycle-slideshow' ).on( 'cycle-before', function( event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag ) {
		$('body').removeClass('slider'+optionHash.currSlide);
		$('body').addClass('slider'+optionHash.nextSlide);

	});



</script>

