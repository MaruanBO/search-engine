<div class="snippet type-apps" id="snippet841440" >
    
	
	<script src="/js/jquery/lemmon-slider.js" type="text/javascript"></script>
	<script type="text/javascript">
		$(window).load(function(){
			$( '.next-week-events-wrapper, .this-week-events-wrapper' ).lemmonSlider({
				'infinite' : true
			});
		});
		
		
		$(document).ready(function(){

		//Show & Hide Tabs
		$('.events-tabs a').click(function(e){
			var eventspanel = $(this).attr('id');
			var type = $(this).data('type'); 
			
			$('.events-tabs a').removeClass('selected');
			$(this).addClass('selected');
			$('.thisweek, .nextweek').css('visibility', 'hidden');
			$('.'+type).css('visibility', 'visible');
			
			e.preventDefault();
		});
	});
	</script>




