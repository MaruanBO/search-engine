<div class="snippet type-apps" id="snippet891598" >
    
	<link rel="stylesheet" href="//code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css">
	<script src="//code.jquery.com/jquery-1.8.3.js"></script>
	<script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
	<script src="/js/jquery/jquery.dropkick.js"></script>
	<script type="text/javascript" charset="utf-8">
      $(window).load(function() {

           $('.hc-dropdowns').dropkick();
		   	//$('.dk_toggle').css('width','227px');
       });
  </script>


	<script>
		$(function() {
			if($('.keyword')){
				$keyword = $('.keyword');
	
				$keyword.autocomplete({
					minLength: 3,
					source: function(request, response) {
						$.ajax({
							url: "/folio/getpredictivesearch/id/891598/pageid/31140/siteid/687/keyword/"+request.term,
							dataType: "json",
	
							success: function(data) {
	
								var subHeading = null;
	
								response($.map(data, function(obj)
								{
									var seoLink = '';
									if(obj.type == 'Products') {
										obj.type = 'Books';
										seoLink ="/"+obj.id+"/"+obj.seo;
									} else if(obj.type == 'Contributors') {
										obj.type = 'Authors';
										seoLink ="/cr-"+obj.id+"/"+obj.seo;
									} else if(obj.type == 'Illustrator') {
										obj.type = 'Illustrators';
										seoLink ="/cr-"+obj.id+"/"+obj.seo;
									}
	
	
	
									var item = {
										label: obj.name,
										value: seoLink,
										type: obj.type
									};
	
									if(subHeading != obj.type)
									{
										subHeading = obj.type;
										item.subHeading = subHeading;
									}
	
									return item;
								}));
							}
	
	
						});
					},
	
					open: function (event, ul) {
						$('.ui-autocomplete').append('<li><a class="view-more" href="/search-results?search-term='+$keyword.val()+'">VIEW MORE</a></li>');
					},
	
					select: function(event, ui) {
						event.preventDefault();
						$keyword.val(ui.item.label);
	                    window.location.href = ui.item.value;
	                },
	
	
	                focus: function( event, ui ) {
	                	event.preventDefault();
		               // $keyword.val(ui.item.label);
	                }
				});
	
	
				$('.ui-autocomplete-input').data('uiAutocomplete')._renderItem = function(ul, item, request)
				{
					var predictiveItem = "<a>" + item.label + "</a>";
	
					if(item.subHeading !== undefined)
						$("<li class='predictive-subheading'></li>").append(item.subHeading).appendTo(ul);
	
					return $("<li></li>").data('item.autocomplete', item).append(predictiveItem).appendTo(ul);
				};


			}
		});
	</script>






 
<div id="search-wrapper" data-t="test2">
	<form id="basic-search" action='/search-results/' method='GET'>
		<label>SEARCH BOOKS</label>
		<input type='text' class="keyword" id='search-term' placeholder="Search" name='search-term'/>
		<input type='submit' class="go" value='submit'/>
	</form>
</div>
