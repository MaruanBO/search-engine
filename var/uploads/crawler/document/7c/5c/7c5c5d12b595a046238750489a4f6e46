        jQuery(window).load(function () {

          function thLoadPopup() {


            if (jQuery('#th-pop').length > 0 ){
              return 'hide';
            }

            var exitPopup = jQuery.cookie('th_fb_hidepopup2');
            thCleanupPopups();
            if (exitPopup === null) {
              return 'show';
            }
            return 'hide';
          }

          function thWriteExitPopup() {
            jQuery.cookie("th_fb_hidepopup2", "hidepopup", { expires: 365, path: '/' });
          }

          function thCleanupPopups() {
            jQuery.cookie("th_fb_hidepopup", null, { path: '/' });
            jQuery.cookie("th_fb_hidepopup1", null, { path: '/' });
          }


          jQuery("#th-modal-fb").on('hidden.bs.modal', function (e) {
            thWriteExitPopup();
          });

          jQuery("#th-modal-fb").on('shown.bs.modal', function (e) {
            thLoadPopup();
          });

          setTimeout(function(){
            jQuery("#th-modal-fb").modal(thLoadPopup());
          },4000)

        });
      </script>
