<script type='text/javascript'>
var preval='no';
var layout_vals = new Object();
var q_done = new Array();
var nocache='0';
function validate(f,mode){
try{
   q_done = new Array();
   document.forms[0].js.value='yes';
   if(preval=='no'){
   validation_error = new Array();
   cuss_set = 0;
   if(scrub_words){
      for(c=0;c<scrub_words.length;c++){
         for(l=0;l<document.forms[0].length;l++){
            if(document.forms[0][l].name){
               if(document.forms[0][l].name.match(/^q/)){
                  if(scrub_words[c].test(document.forms[0][l].value.toLowerCase()) && !cuss_set){
                     validation_error[validation_error.length] = 'Sorry, we cannot accept submissions containing vulgar words';   
                     cuss_set = 1;
                  }
               }
            }
         }
      }
   }


      for (func in layout_vals) layout_vals[func]();
      
      if(validation_error.length){
      if(mode=='alert') {
         alert('Please correct the following Errors:\n\n\n' + validation_error.join('\n--------\n'));
      }
      hide_spinner();
      document.forms[0].js.value='';
      sup_popval = 1;
      return false;
   }}

   xpc=1;
   preval='yes';
   sup_popval = 0;
   return true;
   }//end try
   catch(err){report_error(err);return true;}
}

</script>

