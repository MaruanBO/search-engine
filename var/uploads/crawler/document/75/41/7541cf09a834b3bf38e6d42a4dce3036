    window.fbAsyncInit = function() {
        try {
            initFacebookAndApps();
            if (typeof dojo != 'undefined')
                dojo.publish('agi/facebook/init');
            FB.Event.subscribe('edge.create', function(href) { _gaq.push([ '_trackSocial', 'Facebook', 'Like_afu', href]); });
            FB.Event.subscribe('edge.remove', function(href) { _gaq.push([ '_trackSocial', 'Facebook', 'Unlike_afu', href]); });
            FB.Event.subscribe('edge.create', function(response) {
              FB.getLoginStatus(function(response) {
                if (response.status === 'connected') {
                  FB.api('/me/permissions', function(response){
                    if(typeof response.data[0].publish_actions === "undefined")
                    {
                      FB.login(function(response){ FB.XFBML.parse(); }, {scope:'publish_actions'});
                    }
                  });
                } 
                else if (response.status === 'not_authorized') {
                  FB.login(function(response){ FB.XFBML.parse(); }, {scope:'publish_actions'});
                  }
                });
              });
        } catch(err) { console.error(err); }
    };
