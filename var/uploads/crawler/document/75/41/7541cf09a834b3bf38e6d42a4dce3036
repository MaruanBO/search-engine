    var facebookBirthdays;
    function initFacebookAndApps() {
        
        facebookBirthdays = AGI_FB.AG.BirthdayReminders(
                    { birthdayPage: 'http://www.americangreetings.com/ecards/birthday?lpage=fbbdayhome',
                      numFriendsToShow: 3,
                      numCols: 1,
                      pageIds: {
                            signedIn: 'agi-hp-fbreminders',
                            signedOut: 'agi-hp-fbconnect',
                            friendListContainer: 'agi-fbbirthdays' } },
                    {scope: 'friends_birthday'}
            );

        facebookBirthdays.initFacebook('158457776154');
        
        facebookHeader = AGI_FB.AG.Header(
                { pageIds: {
                        signedIn: 'agi-fbheader-in',
                        signedOut: 'agi-fbheader-out',
                        userContainer: 'agi-fbuser' } },
                { scope: 'friends_birthday'}
        );

        facebookHeader.initFacebook('158457776154');

    }
</script>
