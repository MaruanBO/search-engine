		    var load = function (url,divElementId) {
		        var jsPlaceholder = document.createElement('script');
		 
		        jsPlaceholder.type = 'text/javascript';
		        jsPlaceholder.async = true;
		        jsPlaceholder.src = url;
		 
		        if (divElementId === undefined)
