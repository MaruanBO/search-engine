		<script type="text/javascript">
				function base64Encode(text){
			if (/([^\u0000-\u00ff])/.test(text)){
				throw new Error("Can't base64 encode non-ASCII characters.");
			}
			var digits = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/",
			i = 0,
			cur, prev, byteNum,
			result=[];
			while(i < text.length){
				cur = text.charCodeAt(i);
				byteNum = i % 3;
				switch(byteNum){
							case 0: //first byte
							result.push(digits.charAt(cur >> 2));
							break;
							case 1: //second byte
							result.push(digits.charAt((prev & 3) << 4 | (cur >> 4)));
							break;
							case 2: //third byte
							result.push(digits.charAt((prev & 0x0f) << 2 | (cur >> 6)));
							result.push(digits.charAt(cur & 0x3f));
							break;
						}
						prev = cur;
						i++;
					}
					if (byteNum == 0){
						result.push(digits.charAt((prev & 3) << 4));
						result.push("==");
					} else if (byteNum == 1){
						result.push(digits.charAt((prev & 0x0f) << 2));
						result.push("=");
					}
					return result.join("");
				}
					jQuery(document).ready(function(){
					jQuery.getJSON('http://www.ford-trucks.com/forums/global_navbar.php?display=1&loc=defaultLoc&search_type=wp&callback=?&jsonp=1&ajax_load=1',{},function(res){
						domObject = jQuery(res.navbar);
						jQuery('.nav-holder .vbgn_search').remove();
						jQuery('.nav-holder .vbgn-holder').replaceWith(res.navbar);
						jQuery('.vbgn_right .vbgn_login').show();
						jQuery.getJSON('http://www.ford-trucks.com/forums/global_navbar.php?display=1&loc=defaultLoc&search_type=wp&callback=?&jsonp=1&get_facebook_js=1',{},function(res2){
							jsCodeToInsert= res2.navbar.replace('loc=defaultLoc','loc='+base64Encode(window.location.href));
							jQuery('.nav-holder').append(jQuery(jsCodeToInsert));
										function base64Encode(text){
			if (/([^\u0000-\u00ff])/.test(text)){
				throw new Error("Can't base64 encode non-ASCII characters.");
			}
			var digits = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/",
			i = 0,
			cur, prev, byteNum,
			result=[];
			while(i < text.length){
				cur = text.charCodeAt(i);
				byteNum = i % 3;
				switch(byteNum){
							case 0: //first byte
							result.push(digits.charAt(cur >> 2));
							break;
							case 1: //second byte
							result.push(digits.charAt((prev & 3) << 4 | (cur >> 4)));
							break;
							case 2: //third byte
							result.push(digits.charAt((prev & 0x0f) << 2 | (cur >> 6)));
							result.push(digits.charAt(cur & 0x3f));
							break;
						}
						prev = cur;
						i++;
					}
					if (byteNum == 0){
						result.push(digits.charAt((prev & 3) << 4));
						result.push("==");
					} else if (byteNum == 1){
						result.push(digits.charAt((prev & 0x0f) << 2));
						result.push("=");
					}
					return result.join("");
				}
		jQuery(document).ready(function(){
		jQuery("[href*='register.php']").each(function(){
			regLink = jQuery(this);
			targetUrl = regLink.attr('href');
			if (targetUrl.indexOf('?') == -1){
				concatWith = '?'
			}else{
				concatWith = '&'
			}
			regLink.attr('href',targetUrl+concatWith+'sr_step='+base64Encode(window.location.href))
		});
	});
							});
					});
					jQuery(document).on("click", ".vbgn_logout_button", function(event){ return confirm('Are you sure you want to log out?') });
				});
