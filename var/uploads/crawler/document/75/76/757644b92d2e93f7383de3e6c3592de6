<script language="javascript">
	function trim(str)
	{
		return str.replace(/^\s+|\s+$/g, '');
	}
	function verifyRequired() {
		if (trim(document.newsletter_signup.email.value) == "" || trim(document.newsletter_signup.email.value) == "Your Email") 
		{
			alert("Please enter a valid email to subscribe to the newsletter. (E.g. email@domain.com)");
			return false;
		}
		else if(validateEmail(trim(document.newsletter_signup.email.value)))
			return true;
		else
		{
			alert("Please enter a valid email to subscribe to the newsletter. (E.g. email@domain.com)");
			return false;
		}
	}
	function validateEmail(email) { 
		var emailPattern = /^([a-z0-9\+_\-]+)(\.[a-z0-9\+_\-]+)*@([a-z0-9\-]+\.)+[a-z]{2,6}$/;
		return emailPattern.test(email); 
	}
	
	$(document).ready(function(){  
		$("#newsletter_signup").submit(function(){ 
			if(verifyRequired())
			{
				var form_data = $("#newsletter_signup").serialize();
				$('#content').html('<div style="display:table;"><div style="display:table-cell; font-weight: bold; height:128px; margin:0 auto; text-align:center; vertical-align: middle; width:290px; ">&nbsp;<img src="http://static.ibsrv.net/autocomm/gnr/2.1/ajax-loader.gif" alt="Subscribing..." align="absmiddle" border="0"/>&nbsp;</div></div>');  			
				$.post(  
					"http://www.ford-trucks.com/newsletter_widget/newsletter_subscribe.php",  
					form_data,  
					function(data){  
						$('#content').html('<div style="display:table;"><div style="display:table-cell; font-weight: bold; height:128px; margin:0 auto; text-align:center; vertical-align: middle; width:290px; ">' + data + '</div></div>' );  
					}  
				);
			}
			return false;  
		});
	});
</script></div>
