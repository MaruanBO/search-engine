<script type='text/javascript'>

    /**
     * Periodically rotate ads.
     */
    googletag.sp_rotate = function() {
        try {
            if (!Ext || !googletag.sp_slots) {
                return;
            }

            for (var id in googletag.sp_slots) {
                if (!googletag.sp_slots.hasOwnProperty(id)) {
                    continue;
                }

                // Check if it's currently displayed
                if (!Ext.get(id)) {
                    continue;
                }

                // Rotate
                googletag.pubads().refresh([googletag.sp_slots[id]]);
            }
        } catch(ignored) {}
    };

