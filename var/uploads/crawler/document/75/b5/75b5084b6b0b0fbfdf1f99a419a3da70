<script type="text/javascript">

jQuery(document).ready(function() {
	
	var width = $('.main-content').width();
	if ( parseInt( width ) < 480 )
	{
		jQuery('#itemlist4').jcarousel({
			vertical: true,
			size: 12,
			scroll: 4,
			visible: 4,
	        initCallback: itemlist4_initCallback,
			buttonNextHTML: null,
	        buttonPrevHTML: null,
			itemVisibleInCallback: {
	            onAfterAnimation: function(c, o, i, s) {
					var activeIndex = i - 4;
	              	jQuery('.jcarousel-control .page').removeClass('active').addClass('inactive');
	              	jQuery('.jcarousel-control #' + activeIndex).removeClass('inactive').addClass('active');				
	            }
	          }
	
	    });
   	}
	else
	{
		jQuery('#itemlist4').jcarousel({
			vertical: true,
			size: 12,
			scroll: 4,
			visible: 4,
	        initCallback: itemlist4_initCallback,
			buttonNextHTML: null,
	        buttonPrevHTML: null,
			itemVisibleInCallback: {
	            onAfterAnimation: function(c, o, i, s) {
					var activeIndex = i - 4;
	              	jQuery('.jcarousel-control .page').removeClass('active').addClass('inactive');
	              	jQuery('.jcarousel-control #' + activeIndex).removeClass('inactive').addClass('active');				
	            }
	          }
	
	    });
	}
    
});

function itemlist4_initCallback(carousel) {
    jQuery('.jcarousel-control div').bind('click', function() {
        carousel.scroll(jQuery.jcarousel.intval(jQuery(this).attr("title")));
        return false;
    });
	
	jQuery('#mycarousel-next').bind('click', function() {
        carousel.next();
        return false;
    });

    jQuery('#mycarousel-prev').bind('click', function() {
		
        carousel.prev();
        return false;
    });
};


</script>
	
