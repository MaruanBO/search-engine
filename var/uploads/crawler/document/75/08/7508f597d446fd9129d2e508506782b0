	<script type="text/javascript">
			// set the utm_ data as javascript vars so they can be accessed by the Revive ad server
		// default to no_data for all utm_ data
		var utm_medium = 'no_data';
		var utm_source = 'no_data';
		var utm_campaign = 'no_data';
		var utm_term = 'no_data';

		// look in the querystring for the data
		if ( '' != getQueryString('iid') ) {
			try {
				var pgoadata = JSON.parse( Base64.decode( getQueryString('iid') ) );
				if ( pgoadata !== null ) {
					utm_medium = pgoadata.utm_medium;
					utm_source = pgoadata.utm_source;
					utm_campaign = pgoadata.utm_campaign;
					utm_term = pgoadata.utm_term;
				}
			} catch(e) {}
		} else {
			if ( '' != getQueryString('utm_source') ) {
				utm_source = getQueryString('utm_source');
			}
			if ( '' != getQueryString('utm_medium') ) {
				utm_medium = getQueryString('utm_medium');
			}
			if ( '' != getQueryString('utm_campaign') ) {
				utm_campaign = getQueryString('utm_campaign');
			}
			if ( '' != getQueryString('utm_term') ) {
				utm_term = getQueryString('utm_term');
			}
		}

		// if the query string does not have utm_ data, look in the cookie
		var pgoa_utm_cookie = jQuery.cookie('pgoa_utm_data');
		if ( pgoa_utm_cookie !== null ) {
			pgoadata = JSON.parse( pgoa_utm_cookie );
			utm_medium = pgoadata.utm_medium;
			utm_source = pgoadata.utm_source;
			utm_campaign = pgoadata.utm_campaign;
			utm_term = pgoadata.utm_term;
		} else {
			// make cookie
			pgoadata = {};
			pgoadata.utm_medium = utm_medium;
			pgoadata.utm_source = utm_source;
			pgoadata.utm_campaign = utm_campaign;
			pgoadata.utm_term = utm_term;
			jQuery.cookie( 'pgoa_utm_data', JSON.stringify( pgoadata ) );
		}

		var traffic_source = jQuery.cookie('traffic_source');
		// utm_medium was set earlier
		if ( null == traffic_source ) {
			switch( utm_source ) {
				case 'search': traffic_source = 'TRA001'; break;
				case '65687978': traffic_source = 'TRA002'; break;
				case 'bl': traffic_source = 'TRA003'; break;
				case 'cpz': traffic_source = 'TRA004'; break;
				case '2000006pr': traffic_source = 'TRA005'; break;
				case 'sedo': traffic_source = 'TRA006'; break;
				case 'adup': traffic_source = 'TRA007'; break;
				case 'ob': traffic_source = 'TRA008'; break;
				case 'tb': traffic_source = 'TRA009'; break;
				case 'nr': traffic_source = 'TRA010'; break;
				case 'dq': traffic_source = 'TRA011'; break;
				case 'ez': traffic_source = 'TRA012'; break;
				case 'blp': traffic_source = 'TRA013'; break;
				case '65687978p': traffic_source = 'TRA014'; break;
				case 'searchp': traffic_source = 'TRA015'; break;
			}
			if ( 'email' == utm_medium ) { traffic_source = 'TRANEWS'; }

			// check referrer if utm_source is empty
			if ( '' == utm_source && '' != document.referrer ) {
				var ref = document.referrer.toLowerCase();
				if ( ref.indexOf('google.com') !== -1 ) { traffic_source = 'TRASER'; }
				if ( ref.indexOf('bing.com') !== -1 ) { traffic_source = 'TRASER'; }
				if ( ref.indexOf('pinterest.com') !== -1 ) { traffic_source = 'TRASOC'; }
				if ( ref.indexOf('facebook.com') !== -1 ) { traffic_source = 'TRASOC'; }
				if ( ref.indexOf('twitter.com') !== -1 ) { traffic_source = 'TRASOC'; }
			}

			jQuery.cookie('traffic_source', traffic_source, { expires: 1, path: '/' });
		}

		var keyword_target='home,relish,' + traffic_source; // Targeting
	
		// check for utm_medium, without cookie, for glam
		if ( 'cpc' != utm_medium ) {
			keyword_target += ",nocpc";
		}
