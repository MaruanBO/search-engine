<script language="javascript" type="text/javascript">
    var geocoder = null;
    var map = null;

    function loadMap () {
        var myOptions = {
            zoom: 11,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        map = new google.maps.Map (document.getElementById("trafficmap"), myOptions);

        geocoder = new google.maps.Geocoder();

        var trafficLayer = new google.maps.TrafficLayer();
        trafficLayer.setMap(map);
    }

    function show_traffic_map (city_id) {
        if (document.getElementById('googlemaptraffic') && document.getElementById('trafficmap')) {
            showLayer ('googlemaptraffic', 700, 450);
            if(!map) {
                loadMap ();
            }
            change_city (city_id);
        }
        return false;
    }

    function change_city (city_id) {
        if(city_id == 27612) {
            geocoder.geocode( { 'address': 'Moscow, Russia'}, function(results, status) {map.setCenter(results[0].geometry.location)});
        } else if (city_id == 26063) {
            geocoder.geocode( { 'address': 'St. Petersburg, Russia'}, function(results, status) {map.setCenter(results[0].geometry.location)});
        } else if (city_id == 28440) {
            geocoder.geocode( { 'address': 'Yekaterinburg, Russia'}, function(results, status) {map.setCenter(results[0].geometry.location)});
        } else if (city_id == 37099) {
            geocoder.geocode( { 'address': 'Sochi, Russia'}, function(results, status) {map.setCenter(results[0].geometry.location)});
        }
    }
</script>

