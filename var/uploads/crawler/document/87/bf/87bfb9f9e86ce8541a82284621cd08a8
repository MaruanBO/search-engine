function myUrlParam(name) {
	var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
	if(results==null)
		return null;
	else
		return results[1] || 0;
}
jQuery(document).ready(function(){
	//var sfopts = { dropShadows: false };
	//jQuery("ul.sf-menu").superfish(sfopts);
	jQuery('a').each(function (){
		if(jQuery(this).attr("href") && jQuery(this).attr("href").match(/products\/ingredients/)) {
			var ing = jQuery(this).attr("href").replace(/^.+products\/ingredients\//, "");
			jQuery(this).click(function(){
				var url = "http://www.stopagingnow.com/products/ing-quick.html?blog=1&cid=" + ing;
				$.fancybox.open({
					href: url,
					type: 'iframe',
					fitToView       : false,
					width           : '450',
					height          : '440',
					autoSize        : false,
					closeClick      : false,
					openEffect      : 'none',
					closeEffect     : 'none'
				});
				return false;
			});
		}
	});
	jQuery('#joinTheListForm').bind('submit', function(){
		var url = jQuery('#joinTheListForm').attr('action');
		url = 'http://www.stopagingnow.com/about_us/join_the_list?shadowbox=1';
		var email = jQuery('#frm_email').val();
		if(email == '') {
			alert('Please provide a valid email');
			return false;
		}
		jQuery('body').css('cursor', 'wait');
		jQuery('#emailGo img').attr("src", "http://cdn3.stopagingnow.com/images/ajax-loader.gif");
		jQuery('#emailGo').attr('disabled', 'disabled');
		jQuery.ajax({
			url: url, 
			dataType: 'html',
			// async: false,
			cache: false,
			data: 
			{ 
				frm_email: 	email,
				from_blog: 1,
				shadowbox: "1"
			},
			success: function(data){
				window.location = '/thank-you';
			},
			error: function (){
			}
		});
		return false;
	});
	jQuery('#frm_email_head').keypress(function(){
		jQuery(this).css("color", "#000");
	}).focus(function () { jQuery(this).val(''); });
	jQuery('#email_head_form').bind('submit', function(){
		var url = 'http://www.stopagingnow.com/about_us/join_the_list?shadowbox=1';
		var email = jQuery('#frm_email_head').val();
		if(email == '') {
			alert('Please provide a valid email');
			return false;
		}
		jQuery('body').css('cursor', 'wait'); 
		// jQuery('#emailGo img').attr("src", "http://cdn3.stopagingnow.com/images/ajax-loader.gif"); 
		jQuery('#emailGo').attr('disabled', 'disabled');
		jQuery.ajax({
			url: url, 
			dataType: 'html',
			cache: false,
			data: 
			{ 
				frm_email: 	email,
				from_blog: 1,
				shadowbox: "1"
			},
			success: function(data){
				window.location = '/thank-you';
			},
			error: function (){
			}
		});
		return false;
	});
	jQuery(".hrsr").fancybox({
		maxWidth	: 800,
		maxHeight	: 600,
		fitToView	: false,
		width		: '590',
		height		: '534',
		autoSize	: false,
		closeClick	: false,
		openEffect	: 'none',
		closeEffect	: 'none'
	});
	if(jQuery("#toggleText").is(':visible')) {
		jQuery('.masthead').addClass('banner-margin');
	}
	jQuery('.open-inter').on('click', function(e){
		e.preventDefault();
		delayed_popup(true);
		return false;
	});
});
function toggle() {
	var ele = document.getElementById("toggleText");
	var text = document.getElementById("displayText");
	if(ele.style.display == "block" || ele.style.display == "") {
		ele.style.display = "none";
		text.innerHTML = "show";
		jQuery('.masthead').removeClass('banner-margin');
 	} else {
		ele.style.display = "block";
		text.innerHTML = "hide";
		jQuery('.masthead').addClass('banner-margin');
	}
}
function isEmail(email) {
	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	return reg.test(email);
}
function unloadPopup() {
	jQuery(window).unload(function(){
		alert('Handler for .unload() called.');
		return false;
	});
}
function delayed_popup(ignore_cookie) {
	if(!ignore_cookie) {
		if(jQuery.cookie('litn_list') != null || jQuery.cookie('litn_popup') != null || jQuery.cookie('litn_subscriber') != null)
			return false;
		if(getParam('utm_medium') == 'Newsletter') {
			jQuery.cookie('litn_subscriber', '1', {expires: 14, path: "/"} );
			return false;
		}
		jQuery.cookie('litn_popup', '1', {expires: 7, path: "/"} );
	}
	jQuery.fancybox.open({
		href: 'http://www.stopagingnow.com/shtml/litn_popup_7.html',
		type: 'iframe',
		width: '812',
		height: '492',
		fitToView       : false,
		autoSize        : false,
		closeClick      : false,
		openEffect      : 'none',
		closeEffect     : 'none'
	});
	return false;
}
var scopy = "Search our archive of over 3,014 articles";
jQuery('#searchField3').val(scopy);
jQuery('#searchField3').focus(function(){
	if(jQuery(this).val() == scopy) {
		jQuery(this).val('').css('color','black');
	}
}).blur(function(){
	if(jQuery(this).val() == '') {
		jQuery(this).val(scopy).css('color','#888');
	}
});
function escapeHtml(text) {
	return text.replace(/&/g, "&amp;").replace(/</g, "&lt;").replace(/>/g, "&gt;").replace(/"/g, "&quot;").replace(/'/g, "&#039;");
}
function getParam(sname) {
	var params = location.search.substr(1).split("&");
	var sval   = '';
	// split param and value into individual pieces
	for(var i=0; i<params.length; i++) {
		console.log("idx:" + i + " is " + params[i]);
		temp = params[i].split("=");
		if( [temp[0]] == sname ) { sval = temp[1]; }
	}
	return decodeURIComponent(sval);
}
