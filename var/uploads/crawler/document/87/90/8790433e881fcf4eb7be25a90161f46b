<script type="text/javascript">
try{

	
	
	
	
var pageTracker = _gat._getTracker("UA-7101665-3");
pageTracker._setDomainName(".mybluelight.com");
	

pageTracker._setAllowLinker(true);
pageTracker._setCampNameKey("refcd");
