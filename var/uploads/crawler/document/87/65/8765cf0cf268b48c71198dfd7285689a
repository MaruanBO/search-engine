  	function updateURL(originalurlid, returnUrl)
  	{
  	    var newUrl;
  	    var url = document.getElementById(originalurlid);		
		newUrl = url;
		if (returnUrl.length > 0) {
		    newUrl += "&ReturnUrl=" + returnUrl;
		}
        newUrl += '&LoginRequired=1;'
		window.top.location = newUrl;
		return false;
    }  
</script>

