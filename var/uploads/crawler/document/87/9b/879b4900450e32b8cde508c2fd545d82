<script type="text/javascript">
  jQuery().ready(function(e) {
	   
	jQuery('#preface .block-inner').css('height','auto');
	
	if(!jQuery('.ancient-image-galleries').length && !jQuery('.node-type-photo-gallery').length && !jQuery('.node-book-review').length){
	jQuery('article .content img').each(function(index, element) {
        jQuery(element).colorbox( { href : jQuery(element).attr('src').replace('styles/large/public/',''), title: jQuery(element).attr('alt')  } );
    });
	}
	
	function resizeContent(){
		
		if(jQuery(document).width()>1199){
			jQuery('.main-inner').width(jQuery('#main-content').width()-jQuery('.first-sidebar').width()-jQuery('.second-sidebar').width()-60);
		}else if(jQuery(document).width()>767 && jQuery(document).width()<1200){
			jQuery('.main-inner').width(jQuery('#main-content').width()-jQuery('.second-sidebar').width()-60);
		}else{
			jQuery('.main-inner').css('width','100%');
		}
		
		jQuery('#views_slideshow_cycle_main_slideshow-block > div, .views_slideshow_cycle_slide').css(
	   {
		   'width':jQuery('.main-inner').width()+'px',
		 //  'height':'auto'
	   });
	   
		jQuery('.view-slideshow .views_slideshow_main img').width(jQuery('.main-inner').width()-22);
		
		jQuery('.node-promoted').height('auto');
		
	}
	
	resizeContent();
	
	   if(jQuery(document).width()>767 && ( jQuery('.suggested-books').length || jQuery('.vocabulary-link-category').length) ){ 
	 	
		jQuery('article').height('auto');
		
		jQuery('article.odd').each(function(index, element) {
			if(jQuery(element).hasClass('node-teaser')){
				
				
				if(jQuery(element).height()>jQuery(element).next().height()){
					 jQuery(element).next().height(jQuery(element).height()+imgpad);
					 jQuery(element).height(jQuery(element).height()+imgpad);
				}else{
					 jQuery(element).height(jQuery(element).next().height()+imgpad);
					 jQuery(element).next().height(jQuery(element).next().height()+imgpad);
				}
			}
        });
		
	   }
	
	jQuery(window).resize(function(e) {
		
         resizeContent();
		
		if(jQuery(document).width()>767 && ( jQuery('.suggested-books').length || jQuery('.vocabulary-link-category').length) ){ 
	 
	 	jQuery('article').height('auto');
		
		jQuery('article.odd').each(function(index, element) {
		   if(jQuery(element).hasClass('node-teaser')){	
			  if(jQuery(element).height()>jQuery(element).next().height()){
					 jQuery(element).next().height(jQuery(element).height()+imgpad);
					 jQuery(element).height(jQuery(element).height()+imgpad);
				}else{
					 jQuery(element).height(jQuery(element).next().height()+imgpad);
					 jQuery(element).next().height(jQuery(element).next().height()+imgpad);
				}
		   }
        });
		
	   }
    });
	
	jQuery(window).load(function(e) {
		
       jQuery('#views_slideshow_cycle_main_slideshow-block > div, .views_slideshow_cycle_slide').css(
