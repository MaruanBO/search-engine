        '<a href="{$headlineHref}"><span class="csHeadlineRight">{$headlineText}</span></a>' +
        '<span class="csDate">{$custDate}</span>' +
        '{if $hasVideo}<a href="{$headlineHref}"><img class="csVideoIcon" src="http://ftpcontent.worldnow.com/wncustom/custom/sunbeam/2013/images/video_red.png"></a>{/if}' +
        '<div class="wnClear"></div>' +
    '</li>';
  
  
  var topSide = '' +
  '<li class="wnTSARedBar">'+ 
  '<h3 class="tsaTopText"><span class=" tsaAbridgedHeadline">'+ tsaTopRibbon +'</span>'+
  '<div class="wnClear"></div></h3></li>';
    
    // Load Story Data
    var storyData = [];
    $wn.merge(storyData, $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract());
    $wn.merge(storyData, $wn('#WNCol2 .wnDS-20 .wnDVSummary:lt(10)').wnDVSummaryExtract());
    if (storyData.length < 1) {
      return;
    }

  var ready = false;

  // Add each story

  $wn.each(storyData, function(i) {

    if (i == storyData.length - 1) {
      ready = true;
    }

    function formatDate(dateString){
      
      if(dateString.length < 1){
        return dateString;
      }
      
      if (dateString.indexOf('moment') !== -1 || dateString.indexOf('minute') !== -1 && /\bhour\b/g.test(this.custDate) !== true){
          dateString = 'NEW';
          return dateString;
      } else {
        return 'Updated ' + dateString;
      }
      return;
      
    }
    /*
    if (navigator.userAgent.indexOf('Mac OS X') == -1 && ($.browser.mozilla && parseInt($.browser.version,10) >= 22 && /EST/.test(this.dateText))) {    
      var mDate = new Date(this.dateText);
      mDate = (mDate.setHours(mDate.getHours()-1));
      this.dateText = mDate;   
    }
    */

 //   if (navigator.appVersion.indexOf("Win")!=-1 && $.browser.mozilla && parseInt($.browser.version,10) >= 22) {
        this.custDate = CSHELPERS.timeAgo(this.dateText, '100 years');                   
 /*   } else {
        this.dateText = new Date(this.dateText);
        this.dateText = this.dateText.getTime() + 3600000;
        this.custDate = CSHELPERS.timeAgo(this.dateText, '100 years');
    }    
*/
    this.custDate = formatDate(this.custDate);

    if (!this.graphicSrc) { this.graphicSrc = placeholder; }
    this.hasVideo = (this.isClip || this.isVideo);
    
    // Build left side        
    var $leftSlide = $wn.$csFormatter(leftSide, this);

    // Build right side
    var $rightLinks = $wn.$csFormatter(rightSide, this);
  // Top ribbon addition
  //var $topRibbon = $wn.$csFormatter(topSide, 1);

    // Make the list links do something when interacted with
    $rightLinks.mouseenter(function() {
      $wn('#csTsr .csTsrSlide').not($leftSlide).stop().fadeTo('fast', 0, function() {
        $wn(this).css('z-index', 1);
      });
      $leftSlide.stop().fadeTo('fast', 1, function() {
        $wn(this).css('z-index', 10);
      });
      $wn('#csTsr .csTsrRight li').removeClass('csActive');
      $wn(this).addClass('csActive');
    });

    $wn('#csTsr .csTsrLeft').append($leftSlide);
    $wn('#csTsr .csTsrRight ul').append($rightLinks);
  //heres where you put the conditional to check if not homepage when making this append
  
  
  });
  if (tsaRibbonVisible == true){
  var $topRibbon = $wn.$csFormatter(topSide, 1);
  $wn('#csTsr .csTsrTop').append($topRibbon);
  };

  // Trigger slide 1 (so the stage isn't empty)
  $wn('#csTsr .csTsrRight li:first').mouseover().mouseout();

  // Add classes for right side borders
  $wn('#csTsr .csTsrRight li').first().addClass('csFirst');
  $wn('#csTsr .csTsrRight li').last().addClass('csLast');
  
  if (ready) {
    $wn('#csTsr .csTsrLeft h4.csHeadlineLeft a').dotdotdot();
    $wn('#csTsr .csTsrRight li.csHasImage span').dotdotdot();
    $wn('#csTsr .csTsrLeft .csFeature p').dotdotdot();
  ///$wn('#csTsr .csTsrTop').dotdotdot();
  }

  // Hide original sources
  $wn('#WNTopStoryWrap').hide();
  $wn('#WNCol2 .wnDS-20').hide();
  $wn('#WNDS-20').hide();
  
  // Init Slider
  //$(document).ready(function() {
    $('#mycarousel').jcarousel({
      vertical: true,
      scroll: 1 
    });
  //});  

}, 'WNCol23done');

$wn(document).ready(function(){
  $wn('#csTsr').show();
});
</script>

