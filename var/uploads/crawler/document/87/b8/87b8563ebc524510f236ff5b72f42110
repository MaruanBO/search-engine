<script type="text/javascript">
    function resetNavBarWidth(){
        // exclude the width of holiday menu
        var holidayObj = QuidsiGetElementsByClassName("siteNav", "li", "holidayLi");
        var holidayLength = holidayObj.length;
        var holidayWidth = (holidayLength > 0) ? holidayObj[0].clientWidth : 0;
        var navBarWidth = 940 - holidayWidth;
        var navLink = QuidsiGetElementsByClassName("siteNav", "a", "siteNavLink");
        var navLinkLength = navLink.length - holidayLength;
        var oldItemsWidthTotal = 0;
        var firstItem = navLink[0];
        firstItem.style.paddingLeft = "0";
        for (var i = 0; i < navLinkLength; i++) {
            navLink[i].style.width = "auto";
            oldItemsWidthTotal += navLink[i].children[0].clientWidth;
        };
        var agvDifferWidth = Math.floor((navBarWidth - oldItemsWidthTotal) / navLinkLength);
        var differWidth = agvDifferWidth * navLinkLength + oldItemsWidthTotal - navBarWidth;
        for (var i = 0; i < navLinkLength; i++) {
            var thisItemWidth = navLink[i].children[0].clientWidth;
            navLink[i].style.width = thisItemWidth + agvDifferWidth + "px";
            navLink[i].onclick = function () {
              this.parentNode.children[1].style.visibility = "hidden";
              this.parentNode.children[2].style.visibility = "hidden";
          };
        };
        firstItem.style.paddingLeft = 0 - differWidth + "px";
    }
    (function () {
        resetNavBarWidth();
        var secNavObj = QuidsiGetElementsByClassName("siteNav", "div", "doubleColumnNav");
        for(var i = 0; i < secNavObj.length; i++){
          var categoriesBox = secNavObj[i].children[1];
          var tempContentUL =  categoriesBox.children[1]; 
          var nodeUL = document.createElement("ul");
          var lastNodeUL = document.createElement("ul");
          if(tempContentUL.className == "tempContent"){
              categoriesBox.appendChild(nodeUL);
              categoriesBox.appendChild(lastNodeUL);
              lastNodeUL.className = "secNavLastUl";
              var nodeLi = tempContentUL.children;
              var avgColumnHeight = Math.ceil(tempContentUL.clientHeight / 2);
              var nodeLiCount = nodeLi.length;
              for(var k = 0; k < nodeLiCount; k++){
                  if(nodeUL.clientHeight >= avgColumnHeight){
                      lastNodeUL.appendChild(nodeLi[0]);
                  }else{
                      nodeUL.appendChild(nodeLi[0]);
                  }
              }
              categoriesBox.removeChild(tempContentUL);
          }
        }
    })(window)
</script>
