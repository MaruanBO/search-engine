<script>

$(document).ready(function($){

   var arg =  getCookie("encarg")
          
          if (arg)
          {
	        var sublink = "http://secure.uslegalforms.com/cgi-bin/subscribe//my-subscriptions.pl?" + arg;
				var element = document.createElement("a");
				element.setAttribute("href", sublink);
				element.innerHTML = "My Subscriptions";
				$('#sub_login_id').append(element)   

          }
          else
          {

				//set subscriptionslink
				var sublink = "http://secure.uslegalforms.com/cgi-bin/subscribe/in-subscriptions.pl";
				var element = document.createElement("a");
				element.setAttribute("href", sublink);
				element.innerHTML = "Login";
				$('#sub_login_id').append(element)
          }
          
         




				function getCookie(c_name)
				{
					var c_value = document.cookie;
					var c_start = c_value.indexOf(" " + c_name + "=");
					if (c_start == -1)
					  {
					  c_start = c_value.indexOf(c_name + "=");
					  }
					if (c_start == -1)
					  {
					  c_value = null;
					  }
					else
					  {
					  c_start = c_value.indexOf("=", c_start) + 1;
					  var c_end = c_value.indexOf(";", c_start);
					  if (c_end == -1)
					  {
					c_end = c_value.length;
					}
					c_value = unescape(c_value.substring(c_start,c_end));
					}
					return c_value;
				}


});

</script>



