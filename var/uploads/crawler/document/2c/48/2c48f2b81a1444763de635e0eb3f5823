<script type="text/javascript">
	$(document).ready(function(){		
		$('.tab').hide();
		$('#tab1').show();
		$('.listx1 a').click(function(){
			$('.listx1 a').removeClass('active');
			$(this).addClass('active');
			var currentTab = $(this).attr('href'); 
			$('.tab').hide();
			$(currentTab).show();
			return false;
			
		});  
	});
</script>
