<script>window.jQuery || document.write('<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"><\/script>')</script> 
<script>window.jQuery || document.write('<script src="/wcsstore/MWAuroraStorefrontAssetStore/javascript/vendor/jquery-1.8.0.min.js"><\/script>')</script> 
<script>window.jQuery.ui || document.write('<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js"><\/script>')</script>
<script>window.jQuery.ui || document.write('<script src="/wcsstore/MWAuroraStorefrontAssetStore/javascript/jquery-ui-1.10.0.custom.min.js"><\/script>')</script> 
<script type="text/javascript">
jQuery.fn.carousel = function(zoneRecBool){
return this.each(function(){
var rList = jQuery(this).find('ul');
var n = jQuery(rList).children().length;
//console.log('this is the amount of n: '+ n);
var rPrev = jQuery(this).find('.prev');
var rNext = jQuery(this).find('.next');
var lengthPx = 1020;//added for saved items info for shopping cart
if (zoneRecBool) { 
zoneRecBool = false;
var rPages = n/4;
var perPg = 4;
} else {
var rPages = n/6;
var perPg = 6;
}
var crntPg = 1;
jQuery(rList).children('li:nth-child(6n)').css('borderRight','none');
if (n > perPg) {
jQuery(rPrev).hide();
jQuery(rNext).show();
} else {
jQuery(rPrev).hide();
jQuery(rNext).hide();
}
jQuery(rNext).click(function(e) {
e.preventDefault();
if (crntPg < rPages) {
jQuery(rList).animate({
left: '-=' + lengthPx + 'px'
}, 200, 'swing' );
crntPg ++;
if (crntPg >= rPages) {
jQuery(rNext).fadeOut(150);
}
if(jQuery(rPrev).is(':hidden')) {
jQuery(rPrev).fadeIn(150);
}
}
});
jQuery(rPrev).click(function(e) {
e.preventDefault();
if (crntPg > 1) {
jQuery(rList).animate({
left: '+=' + lengthPx + 'px'
}, 200, 'swing' );
crntPg --;
if (crntPg < 2) {
jQuery(rPrev).fadeOut(150);
}
if(jQuery(rNext).is(':hidden')) {
jQuery(rNext).fadeIn(150);
}
}
});
});
};
</script>
