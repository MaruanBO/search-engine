<script type="text/javascript"> 
function emailSignupFormActionUrlClick(emailId){
var e = jQuery(emailId).val();
jQuery.ajax(
{
url :"https://www.menswearhouse.com/KGUserEmailOptIn?",
dataType :"jsonp",
data:{
emailUpdatesEmail: e,
storeId: '12751',
catalogId: '12004',
},
crossDomain:true,
success:function (data,status){
if (data) {
if (data.result == 'true') {
jQuery(emailId).val('');	
openInfoModal("Thanks! You will begin receiving exclusive sales and promotions emails from The Men's Wearhouse at " + e + ".");
if(typeof cmCreateElementTag != 'undefined') {
cmCreateElementTag('Join Mailing List', 'Email List');
}
} else if (data.result == 'false') {
openInfoModal('Email address is invalid.'); 
} else {
openInfoModal("Service is currently not available.");
}
}
}
});
}	
</script>	
