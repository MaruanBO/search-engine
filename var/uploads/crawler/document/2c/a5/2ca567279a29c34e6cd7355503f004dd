<script type="text/javascript">
dojo.addOnLoad(function () {


jQuery("#esubscription-close").click(function() {
jQuery("#esubscription-container").fadeOut("slow");
});	
jQuery("#esubscription-form").validate({
invalidHandler: function(event, validator) {
var errors = validator.numberOfInvalids();
if (errors) {
jQuery("#form-input-email").css({"border":"1px solid #FF7070","-webkit-box-shadow": "0 0 5px #FF7070","-moz-box-shadow": "0 0 5px #FF7070", "box-shadow": "0 0 5px #FF7070"} );
} 
},
messages: {
emailUpdatesEmail: {
required: "Please enter a valid email address."
}
},
rules: {
emailUpdatesEmail: "required"
}
});
}, false);	
function submitEsubscriptions() {
if(jQuery('#esubscription-form').valid()) {
var e = jQuery("#form-input-email").val();
jQuery.ajax(
{
url: "https://www.menswearhouse.com/KGUserEmailOptIn?",
dataType: "jsonp",
async: true,
data:{
emailUpdatesEmail: e,
storeId: '12751',
referrer: 'subscriptionPopup'
},
success: function (data,status){
if (data) {
if (data.result == 'true') {
jQuery("#esubscription-content").hide();
jQuery("#esubscription-success").fadeIn("slow", function() {
jQuery("#esubscription-container").delay(3000).slideUp("slow");
});	
} else {
showEsubscriptionsError();	
}
} else {
showEsubscriptionsError();
}
},
error: function (xhr, ajaxOptions, thrownError) {
showEsubscriptionsError();
}
});
}
return false;
} 
function showEsubscriptionsError() {
jQuery("#esubscription-content").hide();
jQuery("#esubscription-fail").fadeIn("slow", function() {
jQuery("#esubscription-container").delay(3000).slideUp("slow");
});
}
</script>	

