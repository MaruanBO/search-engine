<script type="text/javascript">	
function getParameterByName(name) {
name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
results = regex.exec(location.search);
return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}
function cmHomePage() {
var cmPageName = 'HomePage';
var cmCategoryId = 'Home';
var endlessAisle = 'EndlessAisle_';
var cm_explore_attributes='';
if( (dojo.cookie("STOREOPSID") && dojo.cookie("STOREOPSID") != 'undefined') || getParameterByName("storeOpsId") != "") {
cmPageName = endlessAisle + 'HomePage';
cmCategoryId = endlessAisle + 'Home';
if(""!= getParameterByName("storeOpsId")){
endlessAisle = endlessAisle+getParameterByName("storeOpsId");
} else {
endlessAisle = endlessAisle+dojo.cookie("STOREOPSID");
}
cm_explore_attributes = cm_explore_attributes + '-_--_--_--_--_--_--_--_--_--_--_-' + endlessAisle;
if (!international.USModeOn) {
var explore_attr15 = "\"International: " + international.getCountryNameFromCookie() + "\"";
cm_explore_attributes = cm_explore_attributes + '-_--_--_-' +explore_attr15;
}
cmCreatePageviewTag(cmPageName, cmCategoryId, null, null,cm_explore_attributes,'');
} else {
if (!international.USModeOn) {
var explore_attr15 = "\"International: " + international.getCountryNameFromCookie() + "\"";	
cm_explore_attributes = cm_explore_attributes + '-_--_--_--_--_--_--_--_--_--_--_--_--_--_-' +explore_attr15;	
}
cmCreatePageviewTag(cmPageName, cmCategoryId, null, null,cm_explore_attributes,'');
}
}
var coreMetricsId = 'Home';
</script>
