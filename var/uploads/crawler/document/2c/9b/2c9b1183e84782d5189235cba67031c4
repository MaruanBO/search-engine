<script type="text/javascript">
    $(document).ready(function () {
        $("#health-center-input-arrow-wrapper-fhc-54e64d5db3b85").click(function(e) {
            e.preventDefault();
            $("#find-health-center-fhc-54e64d5db3b85103667").submit();
        });
        
        $("#find-health-center-fhc-54e64d5db3b85103667").submit(function (e) {
            e.preventDefault();
            if ($("#find-health-center-fhc-54e64d5db3b85103667 #fhc-54e64d5db3b85health-zip").val() != '') {
                window.location.href = '/health-center/all/all/' + $("#find-health-center-fhc-54e64d5db3b85103667 #fhc-54e64d5db3b85health-zip").val();
                return false;
            }
            else if ($("#find-health-center-fhc-54e64d5db3b85103667 #fhc-54e64d5db3b85health-state").val() != "State") {
                window.location.href = '/health-center/' + $("#find-health-center-fhc-54e64d5db3b85103667 #fhc-54e64d5db3b85health-state").val() + '/';
                return false;
            }
        });
        $("#fhc-54e64d5db3b85health-state").change(function (e) {
            if ($("#fhc-54e64d5db3b85health-state").val() != "State") {
                $("#fhc-54e64d5db3b85health-zip").val("");
            }
        });

        $("#fhc-54e64d5db3b85health-zip").click(function () {
            $("#fhc-54e64d5db3b85health-state").val("State");
        });

        $("#fhc-54e64d5db3b85health-zip").keypress(function (e) {
            var char = (typeof e.which == "undefined") ? e.keyCode : e.which;
            if (!((char===08||char===13)||(char >= 48 && char <= 58))) {
                e.preventDefault();
            }
        });
    });
</script><article id="facts" class="visible-desktop">
