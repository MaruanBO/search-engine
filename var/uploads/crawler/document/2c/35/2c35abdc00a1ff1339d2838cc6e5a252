helpers.registerGlobal('PAGESCOPE.helpers.openWin', function (nHeight, nWidth, openURL) {
    var randomnumber=Math.floor(Math.random()*999)
    var winName = "newWin"+randomnumber;
    
    var b = 'left=100,top=100,';
    if (navigator.appName=="Netscape") 
    {   
        b = 'screenX=100,screenY=100,';
    }
    var winOpts = "toolbar=no,scrollbars=yes,width="+nWidth+",height="+nHeight+','+b;
    var nw = window.open(openURL, winName, winOpts);
    nw.focus();
});

