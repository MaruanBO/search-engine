PAGESCOPE.helpers.registerGlobal("PAGESCOPE.layoutAttributes.getCurrentLayout", function() {
    var tag = window.getComputedStyle(document.body,':after').getPropertyValue('content');
    tag = tag.replace( /"/g,'');
    return tag;
});

$(document).ready(function() {
    PAGESCOPE.helpers.registerGlobal("PAGESCOPE.layoutAttributes.layout", PAGESCOPE.layoutAttributes.getCurrentLayout());
});

$(window).on("resize orientationchange", function() {
    var tag = PAGESCOPE.layoutAttributes.getCurrentLayout();
    if (PAGESCOPE.layoutAttributes.layout !== tag) $(document).trigger("layoutChange", {'layout': tag});
    PAGESCOPE.layoutAttributes.layout = tag;
    $("html")
        .removeClass("mobileLayout")
        .removeClass("tabletLayout")
        .removeClass("desktopLayout")
        .addClass(tag);
});

