<script type="text/javascript">

    function swi_asyncLoad(scriptUrl, waitUntilOnLoad) {
        function asyncLoad() {
            var s = document.createElement('script');
            s.type = 'text/javascript';
            s.async = true;
            s.src = scriptUrl;
            var x = document.getElementsByTagName('script')[0];
            x.parentNode.insertBefore(s, x);
        }
        if (!waitUntilOnLoad) {
            asyncLoad();
        } else {
            if (window.attachEvent)
                window.attachEvent('onload', asyncLoad);
            else
                window.addEventListener('load', asyncLoad, false);
        }
    }
    jQuery(function () {
        var searchTextBox = jQuery("#txtSearch");
        // prevent form submittion after click enter in global search box 
        searchTextBox.keypress(function (event) {            
            if (event.keyCode == 13) {
                jQuery('input.searchFieldBtn').trigger('click');
            }            
            return event.keyCode != 13;
        });
        swi_asyncLoad("http://cdn.swcdn.net:80/web/v12.0/js/search/search_2012Q1.min.js", true);


    });

</script>

