<!--[if lte IE 8]> <script type="text/javascript" src="http://cdn.swcdn.net:80/creative/v12.0/images/landing_pages/index6/js/PIE.js"></script> <![endif]-->
<script language="javascript" type="text/javascript">
window.onload = function() {
    $('.videothumbnail, .showVid').click(function(event) {
        event.preventDefault();
        $('#player').attr('src', $(this).attr('href'));
        $('.vWidgetOverlay').fadeIn('800');
        $('.videoWidget').show();
    });
    $('.vWidgetOverlay, .videoWidget .widgetClose').click(function(event) {
        event.preventDefault();
        $('.vWidgetOverlay').hide();
        $('.videoWidget').hide();
        $('#player').attr('src', '');
    });
    $('.thumbnail').click(function(event) {
        event.preventDefault();
        $('.sswLargeImages img').attr('src', $(this).attr('href'));
        $('.ssWidgetOverlay').fadeIn('800');
        $('.ScreenshotWidget').show();
    });
    $('.ssWidgetOverlay, .widgetClose').click(function(event) {
        event.preventDefault();
        $('.ssWidgetOverlay').hide();
        $('.ScreenshotWidget').hide();
        $('.sswLargeImages img').attr('src', '');
    });
    $('.btn.dropdown>a').click(function(event) {
        event.preventDefault();
        $(this).parent().toggleClass("active");
    });
};

function createCookie(value) {
    document.cookie = "homeActiveTabName=" + value;
}

function readCookie() {
    var name = "homeActiveTabName=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1, c.length);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return null;
}
$(document).ready(function() {
    catNav();
    if (window.PIE && Modernizr.borderradius == false) {
        $('.thumbnail img, .overview .videothumbnail, .whatUsersSay .videothumbnail img, .widgetModal, .picture').each(function() {
            PIE.attach(this);
        });
    };
    $(".quadrant a").bind("mouseenter", function() {
        var hrefValue = $(this).attr('href');
        if ((hrefValue != undefined) && (hrefValue != '')) createCookie(hrefValue);
    });
    var activeTabAHref = readCookie();
    if ((activeTabAHref != undefined) && (activeTabAHref != '')) $(".quadrant a[href='" + activeTabAHref + "']").trigger("mouseenter");
});
</script>
