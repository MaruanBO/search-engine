		var hostName = location.host;
		var mcomHost = false;
		var hostNameArray = hostName.split(".");
		var i=0;
		if( (hostName != null)){
			for(i=0;i<4;i++){
				if((hostNameArray[i] == "com") && (hostNameArray[i+1] == null)){
				
					mcomHost = true;
				}
				else if((hostNameArray[i] == "com") && (hostNameArray[i+1] != null)){	
					//console.log("Intl Site, so no proxy calling");
					mcomHost = false;
				}
			}
		}

		if( (mcomHost) &&  (tempReqCookie == "true")){
		
