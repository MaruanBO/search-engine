<script type="text/javascript">
function setCookie(c_name,value,expiredays)
	{
	
		var exdate=new Date();
		exdate.setDate(exdate.getDate()+expiredays);
		if(c_name=="SessionMCID"){
		document.cookie=c_name+ "=" +escape(value)+
		((expiredays==null) ? "" : ";expires="+exdate.toGMTString())+";path=" + "/"+";secure";
		}else{
		document.cookie=c_name+ "=" +escape(value)+
		((expiredays==null) ? "" : ";expires="+exdate.toGMTString())+";path=" + "/";
		}

}		

	function getCookie(c_name)
	{
	if (document.cookie.length>0)
	  {
	  c_start=document.cookie.indexOf(c_name + "=");
	  if (c_start!=-1)
	    {
	    c_start=c_start + c_name.length+1;
	    c_end=document.cookie.indexOf(";",c_start);
	    if (c_end==-1) c_end=document.cookie.length;
	    return unescape(document.cookie.substring(c_start,c_end));
	    }
	  }
	return "";
	}

function checkUKAcceptanceCookie()
{
var accpetanceBanner=document.getElementById("LegalBanner");
var cookieName = "";
var cookieVal=getCookie(cookieName);

	if(cookieVal!=null && cookieVal=="true"){	
		accpetanceBanner.style.display="none";
		document.body.style.backgroundPosition="0 -371px";
	}
	else{
		accpetanceBanner.style.display="block";
		var bannerHt = document.getElementById("LegalBanner").offsetHeight;		
		var newPos=(bannerHt)-371;	
		document.body.style.backgroundPosition="0 "+newPos+"px";

	}
}

function setUKAcceptanceCookie(c_name,value,expiredays)
	{

		var date = new Date();
		date.setTime(date.getTime()+(expiredays*24*60*60*1000));		

		document.cookie=c_name+ "=" +escape(value)+
		((expiredays==null) ? "" : ";expires="+date.toGMTString())+";path=" + "/";
			window.location.reload();
		checkUKAcceptanceCookie();
	}
