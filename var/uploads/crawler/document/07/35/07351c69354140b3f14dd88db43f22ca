			if(getPageUrl.charAt(0) != '/'){
				getPageUrl = "/" + getPageUrl;
			}
			//console.log("First Request and Ml.com Site");
			var remoteUser = getCookie("WT_FPC");
			remoteUser = remoteUser.split(":")[0];
			remoteUser = remoteUser.substring("id=".length);
			
			// Rel 5.0 - 5.1.4 PNP Internal and External Search - gsudheer
