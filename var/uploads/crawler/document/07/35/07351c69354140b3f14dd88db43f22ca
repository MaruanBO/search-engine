	function organicSearchMCID(){
		var mcID = 'null';
		var cookieFromSession = getCookie("SessionMCID");
		if(cookieFromSession!=null && mcID!='null' && cookieFromSession==mcID){
				
				cookieFromSession=mcID;
			}else if(cookieFromSession!=null && mcID!='null' && cookieFromSession!==mcID){
					setCookie("SessionMCID",mcID);
			}else if(mcID!='null'){					
					setCookie("SessionMCID",mcID);
				}else if (mcID =='null'){
					setCookie("SessionMCID","");
				}

	}
		
		//Dojo custom build 1.7 change
		require( 
	 [ 
	 "mldojo/mldojo" 
	 ],function(){ 

		 dojo.addOnLoad(document, function() 
