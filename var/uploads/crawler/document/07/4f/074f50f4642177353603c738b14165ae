<script>
require(["dojo/cookie", "dojo/dom", "dojo/on", "dojo/_base/lang", "dijit/registry", "dojo/ready", "dojo/domReady!"], function(cookie, dom, on, lang, registry, ready) {
        ready(function() {
            var cookieValue = cookie('sceContentAlertDismiss_3c5834fc-9d3d-461e-894e-bf95990869e7');

            if (cookieValue == "true") {
                var contentAlert = dijit.byId('contentAlert_3c5834fc-9d3d-461e-894e-bf95990869e7');
                if (contentAlert != null) {
                    contentAlert.hide();
                }
            } else {
                var alertContainer = document.getElementById('alertContainer_3c5834fc-9d3d-461e-894e-bf95990869e7');
                if (alertContainer != null) {
                    alertContainer.style.display = "block";
                }
            }
            
            var setDismissFlag = function(id) {
                cookie("sceContentAlertDismiss_" + id, "true", {path:"/", expires:1});
            }

            on(dom.byId('contentAlert_3c5834fc-9d3d-461e-894e-bf95990869e7_closeLink'), "click", lang.hitch(null,setDismissFlag,'3c5834fc-9d3d-461e-894e-bf95990869e7'));
        });
    })
