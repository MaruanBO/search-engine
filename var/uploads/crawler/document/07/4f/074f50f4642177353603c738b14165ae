	<script>
	// Add function to format the argument correctly for the mbox (Test & Target)
	function wcmMboxCreate(element) {
    	var title = "Home - SCE";
    	if (title != null) {
        	title = title.replace(/ - SCE/g,"").replace(/ \| /g,"_").replace(/\W/g,"_");
        	if (element != "") {
            title = title + "_" + element;
        	}
        	if (typeof mboxCreate == "function") {
            	mboxCreate(title);
        	}
    	}
    	return true;
	}
	</script>
	
