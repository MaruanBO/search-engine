				            <script>
require(["sce/Carousel"]);

function getLinkText(url ){
    var linkRegExp  = "<a[^>]*>(.*?)<\/a>";
    var linkMatcher = url.match(linkRegExp);
    if (linkMatcher != null && linkMatcher.length > 1)
    { return linkMatcher[1]; }
    else
    { return ''; }
}

function widgetLogin() {
    var redirect = sce.navigation.authenticateUser();
    if (redirect == true) {
         var form = document.getElementById("loginForm");
         if (form != null) {
             window.location = form.action;
         }
    }
}
</script>


