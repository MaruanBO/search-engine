	// Homepage analytics
(function($) {
	$(function() {
		
		// More buttons
		$('.gMore a').each(function() {
			var
			$this = $(this), cat = unescape($this.attr('href')),
			head = $this.parents('.gContainer:first, .gContainerHalf:first').find('.gHead').text();
			cat = cat.match(/([\d]{6,})/ig);
			if (null !== cat && cat.length > 0) {
				$this.analytics('homepage/more-headlines/' + cat[0] + ' - ' + head, false, true);
			}
		});
		
		$('.gTab a').analytics('homepage/tsb/headline-tab', false, true);
		$('.gPhoto a').analytics('homepage/tsb/headline-photo', false, true);
		$('.gBody p a').analytics('homepage/tsb/full-story', false, true);
		$('#gDontMiss a').analytics('homepage/dont-miss', false, true);
		$('.gComment a, .gCommentCount').each(function() {
			var
			$this = $(this), head = $this.parents('.gContainer:first, .gContainerHalf:first').find('.gHead').text(),
			catId = $this.parents('ul:first').attr('data-container');
			catId = catId || 'TSB';
			$this.analytics('homepage/comments/' + catId + ' - ' + head, false, true);
		});
		$('.gNewsBlock li > a').each(function() {
			var $this = $(this);
			if ($this.text() !== 'More Headlines') {
				var
				head = $this.parents('.gContainerHalf:first').find('.gHead').text(),
				catId = $this.parents('ul:first').attr('data-container');
				$this.analytics('homepage/headline/' + catId + ' - ' + head, false, true);
			}
		});

	});
}($wn));
