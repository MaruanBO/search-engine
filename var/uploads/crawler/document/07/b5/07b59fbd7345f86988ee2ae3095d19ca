<script>Worldnow.Helpers.debug('UB CSS/JS for New Category Blocks(296480)', 'UB_296480');</script><div class="wnDVUtilityBlock"><script type="text/javascript">
$(function(){

	$('.singleContainer').filter(':odd').addClass('gContainerOdd');

	var isOdd = function(x) { return x & 1; };
	var prevHeight = 0;
	$('.singleContainer').each(function(index){
		if(isOdd(index)){
			var thisHeight = parseInt($(this).height());
			if(thisHeight > prevHeight){
				$('.singleContainer').eq(index - 1).css('height', thisHeight);
			}
			else {
				$(this).css('height', prevHeight);
			}
		}
		else {
			prevHeight = parseInt($(this).height());
			$(this).css('height', thisHeight);
		}
	});

	//IE FIX
	$('.singleContainer').each(function(index){
		var theHeight = parseInt($(this).height());
		$(this).css('height', theHeight);
	});
});
</script>
