<script type="text/javascript">
$(function(){

	var iframe = '<iframe width="300" height="254" src="http://ftpcontent.worldnow.com/kotv/widgets/stocks/kotv.html" marginwidth="0" marginheight="0" scrolling="no" frameborder="0" style="margin-bottom:15px;"></iframe>';

