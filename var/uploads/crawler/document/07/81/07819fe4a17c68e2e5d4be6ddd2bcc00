<script>
$(document).ready(function() {
(function(app, $) {
	
	app.customModal = {
			
			show : function(contentID, contentWidth, contentHeight) {
				
				var dialogDimensionsMap = {'default': {width: contentWidth, height: contentHeight}};
				if (contentID && contentID.length > 0) {
					var data = {cid: contentID};
					
					// get content id related dimensions or default
					var dim = dialogDimensionsMap[contentID];
					if (!dim) {
						dim = dialogDimensionsMap['default'];
					}
					
					$.ajax({
						type     : "POST",
						url      : app.urls.pageInclude,
						data     : data,
						dataType : "html",
						success  : function(data) {
							
							app.dialog.create({
								target  : $("<div>").attr("id", "dialog-info").html(data),
								options : { width: dim.width, height: dim.height, dialogClass: 'customModal' }
							}).dialog("open");
						},
						failure  : function(data) {
							reportError(app.resources.SERVER_ERROR);
						}
					});
				}
			}
	};
	
}(window.app = window.app || {}, jQuery));
});
</script>	
		
	
