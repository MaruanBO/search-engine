<div class="omnitureasset">

<script type="text/javascript">
	// pass pdict vars to javascript
	var s_account = "rbaproduction";
	var previousPageParameter = "";
</script>

<script language="JavaScript" type="text/javascript" src="http://demandware.edgesuite.net/aahx_prd/on/demandware.static/Sites-brooksbrothers-Site/-/default/v1424360109527/js/omniture.js"></script>


	
		<script language="javascript" type="text/javascript"><!--

		 	//decodes html encoded stringparts. (e.g. transforms &gt; to >) and returns the decoded string
			function decodeHTML(encodedString){
				var decodedString = "";
				if (encodedString != null && encodedString != ""){
					var div = document.createElement('div');
					div.innerHTML = encodedString;
					decodedString = div.firstChild.nodeValue;
				}

				return decodedString;
			}

			
				var omnCustomerID = decodeHTML("unidentified");
			
				var omnLocale = decodeHTML("");
			
				var omnVersion = decodeHTML("desktop");
			
				var omnCountry = decodeHTML("");
			
				var omnCustomerStatus = decodeHTML("guest");
			
				var omnPageTitlePrefix = decodeHTML("brooks brothers| ");
			

			/*  We already have a nice jQuery plugin that handles cookies :(  */
			/*  No need to reinvent the wheel                                 */
			function setCookie(name, value, days) {
				if (days) {
					var date = new Date();
					date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
					var expires = "; expires=" + date.toGMTString();
				} else {
					var expires = "";
				}
				document.cookie = name + "=" + "" + expires + "; path=/";
				document.cookie = name + "=" + encodeURIComponent(value) + expires + "; path=/";
			}

			function readCookie(name) {
				var nameEQ = name + "=";
				var ca = document.cookie.split(';');
				for ( var i = 0; i < ca.length; i++) {
					var c = ca[i];
					while (c.charAt(0) == ' ')
						c = c.substring(1, c.length);
					if (c.indexOf(nameEQ) == 0)
						return decodeURIComponent(c.substring(nameEQ.length, c.length));
				}
				return null;
			}


			function deleteCookie(name) {
				var date = new Date();
				date.setDate(date.getDate() - 1);
				document.cookie = name + "=; expires=" + date.toGMTString() + "; path=/";
			}

			function reducePath(path, depth) {
				var result = new Array();
				var tmp = path.split(" > ");
				var i = 0;
				while (i < depth && i < tmp.length) {
					result.push(tmp[i++]);
				}
				return result.join(" > ");
			}
			
			/**
			 * Returns the marketing channel is set. NULL otherwise.
			 */
			function getMarketingChannel(referrer) {
				if(!referrer) {
					return null;
				}
				
				// Channel set, extract and return it.
				referrer = referrer.toLowerCase();
				if(referrer.indexOf("&cmp=") != -1) {
					var result = referrer.substring(referrer.indexOf("&cmp=")+5, referrer.indexOf("&cmp=")+8);
					return result;
				}
			}

		    function getEvar16(assetID) {
		    	
		    	// Marketing Channel
		    	var marketingChannel = getMarketingChannel(document.URL);
		    	var omnSection = readCookie('omnSection');
		    	if(	(document.referrer && !document.referrer.match(/^https?:\/\/([^\/]+\.)?brooksbrothers\.com(\/|$)/i) && marketingChannel) ||
		    		(!document.referrer && marketingChannel) )
		    	{
		    		return marketingChannel;
		    	}
		    	
		    	// Deeplink
	    	 	if (document.referrer === ""){
					return "deeplink";
				}
		    	
		    	// Previous Page
		    	if (previousPageParameter != "") {
				    return previousPageParameter;
			    }
		    	
		    	// Asset IDs
		    	///////////////////////////////////////////////////////
				if (assetID == "OMNITURE_HOMEPAGE") {
					return "home";
				}
				if (assetID == "OMNITURE_CATEGORYLANDINGPAGE") {
					return (typeof omnSection === 'undefined')? ""  : omnSection;
				}
				if (assetID == "OMNITURE_GRIDPAGE" || assetID == "OMNITURE_GRIDPAGE_REFINEMENTS") {
					//var path = (typeof omnSection === 'undefined')? ""  : omnSection;
					//return reducePath(path, 3);
					return omnSection;
				}
				if (assetID == "OMNITURE_SEARCH_RESULTPAGE") {
					return "search";
				}
				if (assetID == "OMNITURE_PAGE_QUICKVIEW" || assetID == "OMNITURE_PAGE_SHOPTHELOOKQUICKVIEW") {
					return omnSection; 
				}
				if (assetID == "OMNITURE_PAGE_SHOPTHELOOK") {
					return "other look";
				}			
				if (assetID == "OMNITURE_PAGE_PRODUCTDETAIL") {
					return omnSection; 
				}
				if (assetID == "OMNITURE_GRIDPAGE_REMEMBERED") {
					return "remembered items";
				}
			    if (assetID == "OMNITURE_CART_NOTEMPTY") {
					return "bag";
				}
			    if (assetID == "OMNITURE_MYACCOUNT_ORDERHISTORY") {
					return "order history";
				}
			    if (assetID == "OMNITURE_MAGAZINE_VIEW") {
					return "magazine";
				}
				if (assetID == "OMNITURE_ADD2CART_CART_EXIST" || assetID == "OMNITURE_ADD2CART_CART_NEW"){
					return "recommended items";
				}
				
		    	// Referrer different from BB
	    	 	if (document.referrer && !document.referrer.match(/^https?:\/\/([^\/]+\.)?brooksbrothers\.com(\/|$)/i) ){
					return "deeplink";
				}				
				
		    	// No case found
				return "";
		    }

		    function updateOmnPage(omnAssetID) {
			    if (omnAssetID == "OMNITURE_PAGE_QUICKVIEW") {
				    return; // don't update the cookie if we land on certain pages like modal dialogs, etc.
			    }
			    if (omnAssetID == "OMNITURE_PAGE_PRODUCTDETAIL") {
				   return; // don't update the cookie if we land on product detail page
			    }
			    setCookie("omnPP", omnAssetID);
		    }

		    function updateOmnSection(value) {
			    if (omnAssetID == "OMNITURE_GRIDPAGE" || omnAssetID == "OMNITURE_GRIDPAGE_REFINEMENTS") {
			    	setCookie("omnSection", value);
			    }
		    }

		    var omnAssetID  = "OMNITURE_HOMEPAGE";
		    var omnSectionEntry  = (typeof omnPageName === 'undefined')? ""  : decodeHTML(omnPageName);
			// read omnPP cookie and then update it.
		    var previuosAssetID = readCookie("omnPP");
		    updateOmnPage(omnAssetID);
		    updateOmnSection(omnSectionEntry);


		    var omnPreviousPage = decodeHTML(getEvar16(previuosAssetID));
		    if(omnPreviousPage == "mini-bag"){
		    	omnContentPosition = "mini-bag";
		    }

		    if (omnAssetID === "OMNITURE_PAGE_SHOPTHELOOK" || omnAssetID === "OMNITURE_PAGE_SHOPTHELOOKQUICKVIEW"){
			    if (typeof(omnLookReplace) != 'undefined' && omnLookReplace && omnPreviousPage != null && omnPreviousPage.length > 0){
					omnProducts = omnProducts.replace(/evar16=([^|]*)/g, "evar16="+omnPreviousPage);
			    }
			}

			//remove old refine values for ajax requests
		    if (omnAssetID === "OMNITURE_PAGE_QUICKVIEW" || omnAssetID === "OMNITURE_PAGE_SHOPTHELOOKQUICKVIEW" || omnAssetID === "OMNITURE_PAGE_ADDTOCART"){
				if(s){
					for(var prop in s.contextData){
						if(prop.indexOf("ref") == 0 || (prop == "action" && (omnAssetID === "OMNITURE_PAGE_QUICKVIEW" || omnAssetID === "OMNITURE_PAGE_SHOPTHELOOKQUICKVIEW"))){
							delete s.contextData[prop];
						}
					}
				}
			}			

			function extendMyBuyLinks(){
				jQuery("li.mbCitem .mbCimg a").attr("href", function(i, href) {
	   				return app.util.appendParamToURL(href,'omnpp','recommended items');
	   	   	   	});

				//unbind waiting event handler from recommendations as it's work is done.
				jQuery("li.mbCitem .mbCimg a").die("hover", extendMyBuyLinks);
			}

