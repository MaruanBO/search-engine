<script type="text/javascript">
(function ($) {
$(document).ready(function() {
if(typeof document.ontouchend !== 'undefined') {
var lvl1Links = $('ul.level-1 > li');
lvl1Links.each(function(){
var touchBtn = $('<div class="touch-nav-btn" />')
.css({
'position' : 'relative',
'top' : '-30px',
'left' : 0,
'width' : '100%',
'height' : '100%'
});
$(this).append(touchBtn);
var anchor = $(this).children('a');
var href = anchor.attr('href');
anchor.attr({
'data-href': href,
'href': 'javascript:void(0);'
}).addClass('no-hover');
}).find('.touch-nav-btn').bind('touchend', function(e) {
var anchor = $(this).siblings('a'),
sibs = anchor.siblings("div.level-2");
if(sibs.hasClass("active")) {
sibs.removeClass('active');
setTimeout(function(){ location.href = anchor.data('href'); },200);
} else {
$("div.level-2").removeClass('active');
if(sibs.length > 0 && !sibs.hasClass("active")) {
sibs.addClass("active");
} else {
$("div.level-2").removeClass('active');
setTimeout(function(){ location.href = anchor.data('href'); },200);
}
}
});
$("div.level-2 a").bind('touchend', function(e){
e.preventDefault();
var self = $(this);
$(this).parents('div.level-2').removeClass('active');
setTimeout(function(){ location.href = self.attr('href'); },200);
});
} else {
$("ul.level-1 > li").hover(function () {
var $this = this;
$($this).data('timeout', setTimeout( function () {
$($this).children("div.level-2").addClass("active");
}, 100));
}, function () {
var $this = this;
clearTimeout($($this).data('timeout'));
$($this).children("div.level-2").removeClass("active");
});
}
var link = '';
var pageQueryString = '';
$("a.omnitoplink").each(function() {
link = $(this).attr('href');
((link.indexOf("?") != -1) ? pageQueryString = '&lid=topnav-menu' : pageQueryString = '?lid=topnav-menu');
href = $(this).attr('href');
href += pageQueryString;
$(this).attr('href', href);
});
$("a.omnileftlink").each(function() {
link = $(this).attr('href');
((link.indexOf("?") != -1) ? pageQueryString = '&lid=leftnav-menu' : pageQueryString = '?lid=leftnav-menu');
href = $(this).attr('href');
href += pageQueryString;
$(this).attr('href', href);
});
});
})(jQuery);
</script>
