			    	omnPageName = decodeHTML("product > " + readCookie("omnSection") + " > full view");
			    } else if (omnAssetID == "OMNITURE_PAGE_QUICKVIEW") {
			    	omnPageName = decodeHTML("product > " + readCookie("omnSection") + " > quick view");
			    }
			}

