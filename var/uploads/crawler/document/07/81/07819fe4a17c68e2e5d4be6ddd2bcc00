<script>
jQuery(document).ready(function(){
	 // Menu - Tablet global navigation
        $('.gift-cards-mobile').on("click", function(e) {
    		var href = $(this).attr('href');
    		location.href = href;
        });
});


		$.urlParam = function(name){
		    var results = new RegExp('[\\?&]' + name + '=([^&#]*)').exec(window.location.href);
			if(results) {
				return results[1];
			} else {
				return ;
			}
		}

		$(document).ready(function() {



			if($("#pdp-rf-logo").length > 0) {
					var $Divs = $("span a");
					$Divs.each(function(i) {
						  if ($(this).text().indexOf('Red Fleece Logo') >= 0) {
								if($("#pdp-banner").length > 0) {
										var $Divsinner = $("div#pdp-banner");
										$Divsinner.each(function(i) {
											  if ($(this).find("img").length > 0) {
											  	// console.log("content slice");
											  } else {
													$("#pdp-rf-logo").hide();
											 }
										});
								} else {
									$("#pdp-rf-logo").hide();
								}

						  } else {
								//console.log("no RF logo");
						 }
					});
			}


			var pathname = window.location.pathname;
			if (pathname == '/The-Great-Gatsby-Collection/gatsby,default,sc.html') {
				//$('productsearch-content-left').css('display', 'none');
				$("#productsearch-content-left").css("display", "none");
				$('.secondary-bottom div').css("display", "none");
				$('.secondary-bottom').css("border", "0");
				$('#main .productsearch-content').css("border", "0");
				$('#main .productsearch-content').css("background", "none");
				$('#main .productsearch-content').css("margin-left", "-150px");
				
				$('.product-tile .product-pricing').css("display", "none");
			}

			/* 
			if($.urlParam('cgid') == '0210') {
				$('.quantity-wrapper').after('<div class="clearfix" style="padding-left:15px;"><img alt="" src="http://demandware.edgesuite.net/aahx_prd/on/demandware.static/Sites-brooksbrothers-Site/Sites/default/v1424360109527/images/slot/product-details/tiespromo-pdp.jpg" /></div>');
			}


			if (!$.cookie('dw51cache')) {
				$('#navigation').after('<div id="promo-banner" class="clearfix" style="padding-left:13px;"><a href="http://demandware.edgesuite.net/aahx_prd/on/demandware.static/Sites-brooksbrothers-Site/Sites/default/v1424360109527/images/slot/product-details/compshipcaveat.jpg" target="_blank"><img alt="" src="http://demandware.edgesuite.net/aahx_prd/on/demandware.static/Sites-brooksbrothers-Site/Sites/default/v1424360109527/images/slot/product-details/compshipbanner.jpg" /></a></div>');
