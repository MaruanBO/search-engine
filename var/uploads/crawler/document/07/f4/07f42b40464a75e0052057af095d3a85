<script type='text/javascript' src='/uberTemplates2/core/jquery/js/jquery-1.6.1.min.js'></script> 
<script type='text/javascript' src='/uberTemplates2/core/jquery/js/jquery-ui-1.8.13.custom.min.js'></script>
<script type='text/javascript' src='/uberTemplates2/core/js/underscore-min.js'></script>

<script language="JavaScript" type="text/javascript" src="/portal/js/show_hide.js"></script>
<script language="JavaScript" type="text/javascript" src="/portal/js/navigation.js"></script> 

<script>
    $(document).ready(function(){
        $(".no_script").css("display","none");
        $( "#officials" ).accordion({active:"false", collapsible:"true"});

$('#twitter_container').load('/portal/okgov.json.html');
        startFactRefresh();

        var xml_data = new Array();
        var randomnumber;
        var counter = 0;

        $.ajax({
            type: "GET",
            url: "/portal/textData.xml",
            dataType: "xml",
            success: function(xml) {
                $(xml).find('site').each(function(){
                    var title = $(this).find('title').text();
                    var url = $(this).find('url').text();
                    xml_data.push(new Array(title,url));
                });
                //Set initial item
                randomnumber=Math.floor(Math.random()*(0-xml_data.length+1)+xml_data.length);
                counter = randomnumber+1;
                $('#datawrap').html({});
                for(i=0; i<3;i++){
                    var url = xml_data[randomnumber][1];
                    var title = xml_data[randomnumber][0];
                    $('<div></div>').html('<a href="'+url+'">'+title+'</a>').appendTo('#datawrap');
                    randomnumber += 1;
                    if(xml_data.length == randomnumber){
                        randomnumber = 0;
                    }
                }
            }
        });

        if(xml_data.length == counter){
            counter = 0;
        }        

        var factRefresh;
        function startFactRefresh(){
            var delay = 16000;
            factRefresh = function(){
                $('#datawrap').hide("slide", {direction: "down"},"slow", function(){
                    if(xml_data[counter]!=null){
                        $('#datawrap').html({});
                        for(i=0; i<3;i++){
                            var url = xml_data[counter][1];
                            var title = xml_data[counter][0];
                            $('<div></div>').html(title).appendTo('#datawrap');
                            counter += 1;
                            if(xml_data.length == counter){
                                counter = 0;
                            }
                        }
                    }
                });
                $('#datawrap').show("slide", {direction: "up"},"slow",function(){setTimeout(startFactRefresh, 8000)});
            }
            factRefresh();
        }
        function stopFactRefresh(){
            window.clearInterval(factRefresh);
        }
   
       });
</script>

