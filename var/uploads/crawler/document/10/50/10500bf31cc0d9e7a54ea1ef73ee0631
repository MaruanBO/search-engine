    <script>
      var _viewportSettings;
      (function() {
        var forceViewportW = 600,
            pixRatio = window.devicePixelRatio || 1,
            screenWidth = null,
            scale = null;
            
        // Hack alert! Android reports screen CSS pixels for screen.width,
        // while iOS reports device pixels. Android needs to take pixel density
        // into account to get device pixels. Device pixels < 600 are assumed
        // to indicate a handset.
        if(screen.width < forceViewportW || /iP(hone|od|ad)/.test(navigator.userAgent)) {
          screenWidth = screen.width;
        }
        else {
          screenWidth = screen.width / pixRatio;
        }
        
        scale = screenWidth / forceViewportW;
        
        if(screenWidth < forceViewportW)
          _viewportSettings = 'width=' + forceViewportW + ', user-scalable=yes, initial-scale=' + scale;
      })();
      
    </script>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0">
    <meta name="description" content="Book hotel rooms direct with 100+ brands you trust. Always find hotel deals via our hotelsâ€™ lowest rates guarantees, avoid hidden fees or penalties plus earn your hotel rewards each time you book direct.">
