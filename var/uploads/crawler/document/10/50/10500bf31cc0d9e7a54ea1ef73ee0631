    <script>
      (function() {
        if(_viewportSettings && document.querySelector) {
          var  viewportEl = document.querySelector('meta[name="viewport"]');
          viewportEl.setAttribute('content', '');
          viewportEl.setAttribute('content', _viewportSettings);
        }
      })();
      
    </script>
