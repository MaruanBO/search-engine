// Added for GA : AAG Left Nav
$("#leftnavigation").find("a.nav_item").click(function(){
    var ga_code = $(this).attr('data-gacode');
    if(typeof ga_code != "undefined" && ga_code != "NA") {
        if($(this).hasClass('external')) {
            _gaq.push(['_trackEvent', ga_code + '-bumper-open', 'click', 'LeftNav']);
            return;
        }
        _gaq.push(['_trackEvent', ga_code , 'click', 'LeftNav']);
    }
}); 

