<script type="text/javascript">
  
    function desktopRedirect(){
        var redirectPath = "/content/usmobile/en/home";  
        var useragent = navigator.userAgent;
        var domain = "www.mcdonalds.com";
  
        //if ( useragent.indexOf('BlackBerry') != -1 || useragent.indexOf('iPhone OS 4') != -1 || useragent.indexOf('iPhone OS 8') != -1 || useragent.indexOf('iPhone OS 5') != -1 || useragent.indexOf('iPhone OS 6') != -1 ||  useragent.indexOf('iPhone OS 7') != -1 || useragent.indexOf('Android 2') != -1 || useragent.indexOf('Android 4') != -1 || useragent.indexOf('Opera') != -1 || useragent.indexOf('Windows Phone') != -1 )
        if ( useragent.indexOf('BlackBerry') != -1 || useragent.indexOf('iPhone OS') != -1 || useragent.indexOf('Android') != -1 || useragent.indexOf('Opera') != -1 || useragent.indexOf('Windows Phone') != -1 )
        {    
          var alertURL = "";            
          var getAlertPage = "true";
          var fullSite = "no";
          if(getCookie("fullsite") != null){
            fullSite = getCookie("fullsite");
          }
   
      if(fullSite == "yes"){
                //cookie set for full site
                var pagePath = '/us/en/home';
                alertURL = "http://"+domain+pagePath+".html";
      }else{
            if(redirectPath != ""){
                    //redirecting to path specified in page properties
            var pagePath = '/us/en/home';
            var url= "http://"+domain+pagePath+".html";
                    var redPath = "";
                    
                    if((redirectPath.indexOf("/content/")>-1)){
                        redPath = "http://"+domain+redirectPath+".html";
                    }else{
                        redPath = redirectPath;
                    }  
            alertURL = redPath;
         }else{                
            //check if intermediate page is required or not               
            if(getAlertPage == "true"){
                var pagePath = '/us/en/home';
                var url= "http://"+domain+pagePath+".html";
                                          
                        if(("/content/usmobile/en/alert".indexOf("/content/")>-1)){
                             alertURL = "http://"+domain+"/content/usmobile/en/alert.html?desktopurl="+url;
                        }else{
                             alertURL = "/content/usmobile/en/alert?desktopurl="+url;
                        }
                    }else{
                        //do nothing
                    }
                }      
            }          
            
            var mobileAlert = "no"
            if(getCookie("mobilealert") != null){
                mobileAlert = getCookie("mobilealert");
            }
            var desktopVal = "no";
            var fullSiteRedirect = ""; 
            if(getCookie("fullsiteredirect") != null){
                fullSiteRedirect = getCookie("fullsiteredirect");
            } 
            
            if(fullSiteRedirect == "yes" && mobileAlert == "no" && desktopVal == "no"){
                setCookie("fullsiteredirect","no");
                window.location.href = alertURL;
            }
            
            if(mobileAlert == "no" && desktopVal == "no"){
                if(fullSiteRedirect == "yes" || fullSiteRedirect == "no"){
                    //do nothing
                }
                else{
                    if(alertURL != ""){
                        window.location.href = alertURL;
                    }
                }    
            } 
                                      
        }
        else{
            // For desktop do nothing 
        }
    }         
    function getCookie(name){ 
        var re = new RegExp(name + "=([^;]+)"); 
        var value = re.exec(document.cookie); 
        return (value != null) ? unescape(value[1]) : null; 
    }  
    function setCookie(cookieName,cookieValue) {
        var today = new Date();
        var expire = new Date();
        expire.setTime(today.getTime() + 15 * 60 * 1000);
        document.cookie = cookieName+"="+escape(cookieValue)+ ";expires="+expire.toGMTString();
    }
    
   var allowIntRed= "true";
   if("yes" == "yes"){  
   if(allowIntRed == "true"){
       desktopRedirect();  
       } 
   }    
</script> 


    
      



    
   

