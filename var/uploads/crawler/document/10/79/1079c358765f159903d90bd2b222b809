        }           
    } 
             
    $.fn.extend({
            callout1:function(config) {
                
                var config = $.extend({}, $.callout1.defaults, config); 
                
                 return this.each(function() {
                        
                        var imageNames = config.callout1Images;
                        // alert("imagenames"+imageNames ); 
                        if (imageNames !="")
                        {
                            arrayShuffle(imageNames);
                            var imageNamesSize = imageNames.length;
                           
                            //var imageWeightage = Math.floor(Math.random()*imageNamesSize);
                            var imageWeightage = imageNames[0]; 
                            
                            
                            //var firstCalloutStr = imageNames[imageWeightage];
                            var firstCalloutStr = imageWeightage;
    
                              var splitFirstCallout = firstCalloutStr.split("|");
                            
                            var changedImage = splitFirstCallout[0];                           
                            var hoverText = splitFirstCallout[1];
                            var featureLink = splitFirstCallout[2];
                            //var fullPath = config.path + changedImage;
                            var fullPath = changedImage;   
                            
                            var imageName= changedImage.substring((changedImage.lastIndexOf("/"))+1);
                            // alert("imageName"+imageName);
                            /*alert("Changed Image 1 :: " + changedImage);
                            alert("Hover Text 1 :: " + hoverText);
                            alert("Feature Link 1 :: " + featureLink);*/
                            
                            // alt tag will be image filename.
                            $(this).attr( {
                                    src: fullPath
                                    //alt: changedImage
                            });
                            
                            if(!(document.getElementById('calloutLink1') == null))
                                $("#calloutLink1").attr('href', featureLink);
                            
                            if(!(document.getElementById('hoverMsg1') == null)) 
                                document.getElementById('hoverMsg1').innerHTML = hoverText;
                            
                            if(!(document.getElementById('calloutLink1') == null)){
                            $('#calloutLink1').click(function() {                                                           
                                    //Tracker1('DCS.dcsuri','/featuredcallout/1','WT.ti','Featured Callout','DCSext.banner',imageName);
                                })
                           }    
                       }   
                }); 
            }
            
    });
    
    
       
