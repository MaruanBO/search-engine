    /* function to check cookies are enabled or not */
        
       function isCookiesEnabled() 
       {
   
                //alert("========isCookiesEnabled()====calling==="); 
                var cookieEnabled=(navigator.cookieEnabled)? true : false
               //if not IE4+ nor NS6+
               if (typeof navigator.cookieEnabled=="undefined" && !cookieEnabled)
               { 
                   document.cookie="testcookie" ; 
                   cookieEnabled=(document.cookie.indexOf("testcookie")!=-1)? true : false
               }
                
               if(cookieEnabled == false)
               {
                 //alert('Browser cookies must be enabled in order to use the Meal Builder ') ; 
               }
               
               return cookieEnabled;
   
   
        } 
  
        /* function to add items to meal builder */
    
     function addtoMyMealBuilder(id)
        { 

                if(isCookiesEnabled()== true) 
               {                 
                 
                     if(checkLimit(id)==true)
                     {                      
                         //Tracker1('DCS.dcsuri','/food/addItemtoMyMealBuilder','WT.ti','Add to My Meal Builder :: McDonalds.com','DCSext.item',id);
                         addItemstoSession(id,'us_en');   
                     }
                     else
                         return false; 
               }else
                 return false; 
        }
        
        
  /* function to add items to meal builder */
    
     function addtoMyMealBuilder(path,id)
        { 

                if(isCookiesEnabled()== true) 
               {                 
                 
                     if(checkLimit(id)==true)
                     {                      
                         //if( webtrends !="")
                         //Tracker1('DCS.dcsuri','/food/addItemtoMyMealBuilder','WT.ti','Add to My Meal Builder :: McDonalds.com','DCSext.item',id);    
                         addItemstoSession(id,'us_en');   
                     }
                     else
                         return false; 
               }else
                 return false; 
        }
        
         /* function to add item to meal builder */
        
        function addItemtoMyMealBuilder(itemId,compList)
        {

                if(isCookiesEnabled()== true)
               {
                 
                     if(checkLimit(itemId)==true)                         
                     {                      
                        //Tracker1('DCS.dcsuri','/food/addItemtoMyMealBuilder','WT.ti','Add to My Meal Builder :: McDonalds.com','DCSext.item',itemId);
                        addItemtoSession(itemId,compList,'us_en');  
                     }
                     else
                         return false;  
                    
               }
               else
                   return false; 
        }  
        
        function addReverseItemtoMyMealBuilder(itemId,compList,revList,optionalList,masterItemId)
        {
               if(isCookiesEnabled()== true)
               {
                 
                     if(checkLimit(itemId)==true)                         
                     {
                         //Tracker1('DCS.dcsuri','/food/addItemtoMyMealBuilder','WT.ti','Add to My Meal Builder :: McDonalds.com','DCSext.item',itemId);                    
                         addReverseItemtoSession(itemId,compList,revList,optionalList,masterItemId,'us_en');
                     }
                     else
                         return false;           
                    
               }
               else
                   return false; 
        }  
         
        
      function checkLimit(id) 
      {
           //alert("===========checkLimit()======calling========");
           var limit= 20;
           var existLength=getExistMealbuilderLength('us_en');
           var pids=id.split("|");
           var total_length=pids.length+existLength;
           //alert("total_length======>"+total_length);
            
           if(total_length>limit){
            
                //alert('Your Meal Builder is currently full. Please remove items before adding more. ') ; 
                //dcsMultiTrack('DCS.dcsuri','/food/MyMealBuilder/full','WT.ti','My Meal Builder Full :: McDonalds.com');              
                return false;            
            
           }
           else
           return true;              
      
      }
         
         
