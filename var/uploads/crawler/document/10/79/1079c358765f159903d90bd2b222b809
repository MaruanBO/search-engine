    $.callout1 = {
        defaults: {
            path: '',
            //callout1Images: [calloutItem1Str]
            //callout1Images: [ '/content/dam/callouts/AC_CAR_BENEFITS.png','/content/dam/callouts/AC_CAR_SUCCESSSTORIES.png','/content/dam/callouts/AC_FOOD_ANGUSBURGER.png']
