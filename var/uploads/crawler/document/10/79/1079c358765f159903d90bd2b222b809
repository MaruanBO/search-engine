    var table = $("#tfooter");
    table.find('tr').each(function (i) {
        var $tds = $(this).find('td'),
            socialdata= $tds.eq(0).html(),
            copyrightInfo= $tds.eq(1).html();
            $('#socailId').html('<br>'+socialdata);
            $('#copyrightId').html(copyrightInfo);
            $('#tfooter').hide();
         });
