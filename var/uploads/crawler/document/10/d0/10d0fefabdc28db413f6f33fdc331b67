      $('[ng-controller=EnterController]').scope().handleLike(url);
  });

  FB.getLoginStatus(function (data) {
      var entryScope = $('[ng-controller=EnterController]').scope();
      if(entryScope) {
          entryScope.facebookContestantLoginStatus(data);
      }

      var loginScope = $('[ng-controller=LoginController]');
  });

  var app = window.Gleam;

  FB.Canvas.setAutoGrow(91);

  if(app && app.resizeContent) {
    angular.element(document).ready(function () {
      app.resizeContent();
    });
  }
};
