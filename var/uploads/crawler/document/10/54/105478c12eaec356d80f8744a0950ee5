ad_rules:function()
{
if(typeof console.log != 'undefined')
{
console.log('OraTV Referrer: '+ora_meta.referrer);
}
//var html_title = window.parent.document.title.toLowerCase().indexOf('admin');
if(ora_meta.is_embed && ora_meta.referrer != "" && parseInt(ora_meta.referrer.indexOf("ora\.tv")) >= 0 && parseInt(ora_meta.referrer.indexOf("article")) >= 0) //Ora Player Embedded on Ora blog
{
ora_js.video.vertical = "";
ora_js.video.mute = true;
ora_js.video.auto_play = ((ora_meta.is_mobile || window.parent.$('.article-content iframe[src*="ora\.tv\/embed"]').length >=2) && parseInt(ora_meta.referrer.indexOf("admin\.ora\.tv")) <= -1)?false:true;
ora_meta.ora_native_embed(); 
return "";
}
else //Ora Player embedded off-site
{
for(i=0;i<ora_meta.referrals.length;i++)
{
if(parseInt(ora_meta.referrer.indexOf(ora_meta.referrals[i].domain)) >= 0 || parseInt(ora_meta.partner.indexOf(ora_meta.referrals[i].domain)) >= 0)
{
ora_js.video.vertical = (ora_meta.is_mobile)?ora_meta.referrals[i].mobile:ora_meta.referrals[i].vertical;
ora_js.video.mute = ora_meta.referrals[i].mute;
ora_js.video.auto_play = (ora_meta.is_mobile)?false:ora_meta.referrals[i].auto_play;
 
if(ora_meta.referrals[i].player_event)
{
var evt = ora_meta.referrals[i].player_event;
if(evt == "toggle_mute")
{
ora_meta.mute_toggle();
}
}
return true;
}
if(i== (ora_meta.referrals.length-1))
{
ora_js.video.vertical = "";
ora_js.video.mute = false;
ora_js.video.auto_play = (ora_meta.is_embed || ora_meta.is_mobile)?false:true;
return "";
}
}
}
},
