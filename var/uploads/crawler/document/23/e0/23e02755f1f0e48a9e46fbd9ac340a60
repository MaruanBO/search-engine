<script>

geoip_code = '';

try {
    geoip_code = 'gb';
}
catch(ex)
{}

function geoip_country_code() {
    return geoip_code.toUpperCase();
}

$(document).ready(function() {
	$('.bdSoftwareClass').append($('.bdSoftwareToHide>*'));
	$('.bdSoftwareToHide').hide();

	if (DEFAULT_LANGUAGE == 'en') {
 		var head = document.getElementsByTagName('head')[0];
 		var script = document.createElement('script');
 		script.type = 'text/javascript';
		script.src = 'http://pixel.mathtag.com/event/js?mt_id=377540&mt_adid=116042&v1=&v2=&v3=&s1=&s2=&s3=';
 		head.appendChild(script);
	}
	
	/* products init BEGIN */
	if (DEFAULT_LANGUAGE == 'en') { // A/B Test EN
		
		StoreProducts.initSelector({
			product_id:'tsmulti',
			price_class:'price_prod_left',
			buy_class:'buy_prod_left',
			selected_users: '5',
			selected_years: '1'
		});
		
		StoreProducts.initSelector({
			product_id:'ts',
			price_class:'price_ts',
			buy_class:'buy_ts',
			selected_users: '3',
			selected_years: '1'
		});
	} else {
		if (DEFAULT_LANGUAGE == 'es' || DEFAULT_LANGUAGE == 'it' || DEFAULT_LANGUAGE == 'br') {
			StoreProducts.initSelector({
				product_id:'ts',
				price_class:'price_ts',
				buy_class:'buy_ts',
				selected_users: '1',
				selected_years: '1'
			});
		} else if (DEFAULT_LANGUAGE == 'fr') {
			StoreProducts.initSelector({
				product_id:'tsmulti',
				price_class:'price_ts',
				buy_class:'buy_ts',
				selected_users: '3',
				selected_years: '1'
			});
		} else {
			StoreProducts.initSelector({
				product_id:'ts',
				price_class:'price_ts',
				buy_class:'buy_ts',
				selected_users: '3',
				selected_years: '1'
			});
		}
		
		if($('.price_win8sec').length > 0 ) {
			StoreProducts.initSelector({
				product_id:'win8',
				price_class:'price_win8sec',
				buy_class:'buy_ts',
				selected_users: '3',
				selected_years: '1'
			});
		} else {
			if (DEFAULT_LANGUAGE == 'en' || DEFAULT_LANGUAGE == 'uk' || DEFAULT_LANGUAGE == 'au' || DEFAULT_LANGUAGE == 'de') {
				StoreProducts.initSelector({
					product_id:'is',
					price_class:'price_is',
					buy_class:'buy_is',
					selected_users: '3',
					selected_years: '1'
				});
			} else if (DEFAULT_LANGUAGE == 'es' || DEFAULT_LANGUAGE == 'it' || DEFAULT_LANGUAGE == 'br') {
				StoreProducts.initSelector({
					product_id:'is',
					price_class:'price_is',
					buy_class:'buy_is',
					selected_users: '1',
					selected_years: '1'
				});
			} else {
				StoreProducts.initSelector({
					product_id:'is',
					price_class:'price_is',
					buy_class:'buy_is',
					selected_users: '3',
					selected_years: '1'
				});
			}
		}
	}
	/* products init END */
	
	//slider
    $('#strapslide').strapslide({
        animation: "rotate",
        speed: 1000,
        timeout: 5000,
        responsive: "resize",
        autoplay: true,
        preload: true,
        pauseOnHover: true,
        pagination: true,
        mousewheel: false,
        keyboard: true,
        touchscreen: true 
    }, function(){
        //callback when slide enters
        $('#strapslide .slide.active').click(function(){
        	window.location.href = $('#strapslide .slide.active a').attr('href');
        }).css('cursor', 'pointer');
        
		if ($('.DE_slide3, #BlackFriday2014 div div div, .winterSlide, #bd_box').parents('.slide').hasClass('active')) {
        		$('.strapslide-pagination ul').addClass('forWhiteBG') 
        } else{
        		$('.strapslide-pagination ul').removeClass('forWhiteBG')
        };
    }, function(){
        //callback when slide enters
        $("#strapslide .slide.active").unbind('click');
    });
	
	var lang_array = [];
	lang_array.push('en','uk','au','es', 'it', 'ro', 'br', 'de');
	if(jQuery.inArray(DEFAULT_LANGUAGE, lang_array)==-1) {
		var myFunction = function() {
			if ($('.strapslide-pagination ul li:first-child').hasClass('active')) { 
				 $('.strapslide-pagination ul').hide(0); 
			} 
			
			else { $('.strapslide-pagination ul').fadeIn(500); }   
		};	
		var timer = setInterval(myFunction, 1);
	}
});

function initOverlayOutside(frameW, frameH){
	$('#overlay_sec .modal-dialog').css('width',frameW).css('max-width','97%').css('height',frameH).css('margin-left','auto').css('margin-right','auto');
	$('#overlay_sec .modal-content').css('width','100%').css('height','100%').css('background','#fff').css('border-radius',0).css('border',0);
	$('#overlay_sec button.close').css({'position':'absolute','right':'2px','top':'0','font-size':'30px'});
	$('.trialLinktsmulti').click(function(){
		var frameSrc = $(this).attr("href");
		$('#overlay_sec').on('show', function () {
			$('iframe.overlay-modal-sec').attr("src",frameSrc).attr("width",frameW).attr("height",frameH).css('max-width','100%');
		});
		$('#overlay_sec').modal({show:true})
		return false;
	});
}

/*** Overlay BEGIN ***/
function initOverlay(frameW, frameH){
	$('#homeOverlay .modal-dialog').css('width',frameW).css('max-width','97%').css('height',frameH).css('margin-left','auto').css('margin-right','auto');
	$('#homeOverlay .modal-content').css('width','100%').css('height','100%').css('background','#fff').css('border-radius',0).css('border',0);
	$('#homeOverlay button.close').css('position','absolute').css('right','6px').css('font-size','30px');
	$('.trialLinkis, .trialLinkts').click(function(){
		var frameSrc = $(this).attr("href");
		$('#homeOverlay').on('show', function () {
			$('iframe.overlay-modal').attr("src",frameSrc).attr("width",frameW).attr("height",frameH).css('max-width','100%');
		});
		$('#homeOverlay').modal({show:true})
		return false;
	});
}
/*** Overlay END ***/

$(document).ready(function(){
	if(DEFAULT_LANGUAGE == 'en') {
		
		if(geoip_country_code() == 'US' || geoip_country_code() == 'CA') {
			$(".trialLinkav").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/av");
			$(".trialLinkis").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/is");
			$(".trialLinkave").attr("href","http://download.bitdefender.com/npd/ave/fr_FR-nl_NL/Antivirus_Essential.exe");
			
		} else {
			/* no overlay */
			$(".trialLinkav").attr("href","http://download.bitdefender.com/windows/installer/en-us/bitdefender_antivirus.exe");
			$(".trialLinkis").attr("href","http://download.bitdefender.com/windows/installer/en-us/bitdefender_isecurity.exe");
			$(".trialLinkave").attr("href","http://download.bitdefender.com/npd/ave/fr_FR-nl_NL/Antivirus_Essential.exe");
		}

		//functie care schimba linkurile pt OEM
		if(typeof tagit_params !== 'undefined') {
			for (i in tagit_params.obj) {
				if ('pid' in tagit_params.obj[i] && $.inArray(tagit_params.obj[i].pid.param_value,['oem']) != -1) {
					/* no overlay */
					$(".trialLinkav").attr("href","http://download.bitdefender.com/oemkits/en/2014/bitdefender_avplus.exe");
					$(".trialLinkis").attr("href","http://download.bitdefender.com/oemkits/en/2014/bitdefender_isecurity.exe");
					$(".trialLinkts").attr("href","http://download.bitdefender.com/oemkits/en/2014/bitdefender_tsecurity.exe");
				}
			}
		}
	}
	
	$(".trialLinkav").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/av");
	$(".trialLinkis").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/is");
	
	var valid_lang = [ 'en' , 'uk' , 'au' ];
	if($.inArray( DEFAULT_LANGUAGE, valid_lang ) !== -1){
		$(".trialLinkav").attr("href","http://bitdefender.com/media/html/trial-download-overlays-2015/av.html");
		$(".trialLinkis").attr("href","http://bitdefender.com/media/html/trial-download-overlays-2015/is.html");
		$(".trialLinkts").attr("href","http://bitdefender.com/media/html/trial-download-overlays-2015/ts.html");
		
		initOverlay(1100, 750);
	} else {
		$(".trialLinkav").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/av");
		$(".trialLinkis").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/is");
		
		if(DEFAULT_LANGUAGE == 'fr'){
			//$(".trialLinktsmulti").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/tsmulti");
			$('.trialLinktsmulti').attr('href','http://www.bitdefender.fr/media/html/upsell/fr/download-ts/index.html');
			initOverlayOutside(945, 728);
		} else {
			$(".trialLinkts").attr("href","http://www.bitdefender.com/site/Downloads/downloadForm/ts");
		}
		
		initOverlay(800,320);
	}
});
</script>

