	//functie care isi da seama daca ai Windows 8
	function windows8Detect() {
		var UA = navigator.userAgent;
		var detectWV = UA.search("Windows NT 6.2"); //NT 6.2 = Windows 8

		if(detectWV != -1) return true;
	}
</script>

