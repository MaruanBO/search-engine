<script>
  $(function(){
    gyro();
    console.log("Quit poking around the console and head over to powr.io/jobs already");
  });
  
  function gyro() {
    if (window.DeviceOrientationEvent) {
      // Listen for the deviceorientation event and handle the raw data
      window.addEventListener('deviceorientation', function(eventData) {
        // gamma is the left-to-right tilt in degrees, where right is positive
        var w = window.innerWidth;
        var h = window.innerHeight;
        var tiltLR = eventData.gamma;
        // beta is the front-to-back tilt in degrees, where front is positive
        var tiltFB = eventData.beta;
        // alpha is the compass direction the device is facing in degrees
        var dir = eventData.alpha
        var gyro4 = document.getElementById("gyro4");
        gyro4.style.webkitTransform = "rotate("+ tiltLR +"deg) rotate3d(1,0,0, "+ (tiltFB*-1)+"deg)";
        gyro4.style.MozTransform = "rotate("+ tiltLR +"deg)";
        gyro4.style.transform = "rotate("+ tiltLR +"deg) rotate3d(1,0,0, "+ (tiltFB*-1)+"deg)";
        gyro4.style.top =  tiltFB*6+'px';
        gyro4.style.left = tiltLR*12+(w - 142) +'px';
        //console.log('top ' +gyro4.style.top);
        //console.log('left ' + gyro4.style.left);
        if (parseInt(gyro4.style.left) > 100 && parseInt(gyro4.style.left) < 200 && parseInt(gyro4.style.top) < 245 && parseInt(gyro4.style.top) > 160){
          document.getElementById("gyroImg").src="https://s3-us-west-1.amazonaws.com/powr/images/sitewithcontact.png";
          gyro4.style.display="none";
        }
        }, false);
    }
  }
</script>

