										    function isValidZip(val) {
										      var pattern=/^[0-9]{5}$/;
										      return(pattern.test(val));
										    }

										    function go_search() {
										      var zip = document.getElementById('src_zip').value;
										      if (!isValidZip(zip)){
										        alert("Invalid zip code entered");
										        return false;
										      }
										      var trg='http://www.halfoffdeals.com/zip/' + zip + '/10/';
										      var c=document.getElementById('src_cat');
										      if(c) {
										          if(c[c.selectedIndex].text!='' && c[c.selectedIndex].text!='Any') {
										        trg+=escape(c[c.selectedIndex].text)+'/';
										          }
										      }
										      document.location.href=trg;
										      return false;
										    }
										  </script>
