    var player, APIModules, videoPlayer, experienceModule, newPercentage;

    onTemplateLoad = function(experienceID) {
      player     = brightcove.api.getExperience(experienceID);
      APIModules = brightcove.api.modules.APIModules;
    }

    onTemplateReady = function(evt) {
      videoPlayer      = player.getModule(APIModules.VIDEO_PLAYER);
      experienceModule = player.getModule(APIModules.EXPERIENCE);
      if( $('.hero-full .BrightcoveExperience').length > 0) {
        $('.hero-full .hero-img img').remove();
        if(!$('html').hasClass('blog') && !$('html').hasClass('article')) {
          videoPlayer.play();
        }
        $('.hero-full .BrightcoveExperience').css('visibility', 'visible');
      }
    }
  </script>  
