<script type="text/javascript" src="http://www.hotflick.net/js/jquery.masonry.min.js"></script>
<script type="text/javascript" src="http://www.hotflick.net/js/jquery.infinitescroll.min.js"></script>
<script type="text/javascript">
$(function(){
	var $container = $('#container');
	$container.imagesLoaded(function(){
		$container.masonry({
		itemSelector: '.boxmasonry',
		});
	});
	$container.infinitescroll({
		navSelector  : 'div.pagenavi',    // selector for the paged navigation
		nextSelector : "div.pagenavi .alignleft a",
		contentSelector : "#container",
		itemSelector : '.boxmasonry',     // selector for all items you'll retrieve
		bufferPx     : 200,
		debug		 : true,
		pathParse	 : function(path, nextPage){
		  var re = new RegExp('(\/newdesign\/index\_)'+nextPage+'(\.html)');
		 path = path.match(re).slice(1);
         return path;
		},
		loading: {
			finishedMsg: 'No more pages to load.',
			img: 'http://i.imgur.com/6RMhx.gif'
			}
		},
		// trigger Masonry as a callback
		function( newElements, data, url ) {
			// hide new items while they are loading
			var $newElems = $( newElements ).css({ opacity: 0 });
			// ensure that images load before adding to masonry layout
			$newElems.imagesLoaded(function(){
			// show elems now they're ready
			$newElems.animate({ opacity: 1 });
			$container.masonry( 'appended', $newElems, true );
			});
			_gaq.push(['_trackPageview', url]);
		}
	);
});
</script>
