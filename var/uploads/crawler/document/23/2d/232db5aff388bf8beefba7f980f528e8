<script type="text/javascript">
    google.load('feeds', '1');
    
    function loadEMSList() {
      var feeds = [
        {
          title: ' ',
          url: 'http://www.lcwc911.us/lcwc/lcwc/incidents.xml'
        }
      ];
    
      var options = {
        stacked : true,
        horizontal : false,
        numResults : 8,
        title : "Current incidents"
      };
    
      new GFdynamicFeedControl(feeds, 'trafficList', options);
      document.getElementById('trafficList').style.width = "297px";
    }

    var map;
    
    function initializeMap() {
        traffic_map = new google.maps.Map(document.getElementById("trafficMap"), {
			center: new google.maps.LatLng(40.038787,-76.301422),
			zoom: 11,
			mapTypeId: 'roadmap'
		});
        var trafficLayer = new google.maps.TrafficLayer();
		trafficLayer.setMap(traffic_map);
    }
    
    $(document).ready(function(){
       initializeMap();
       google.setOnLoadCallback(loadEMSList);
       $("#refreshList").click(function() {
	   loadEMSList();
           initializeMap();
	})
        var refreshId = setInterval(function(){
	   initializeMap();
	   }, 300000);
        var refreshId2 = setInterval(function(){
	    loadEMSList();
	   }, 900000);
    })
</script>
