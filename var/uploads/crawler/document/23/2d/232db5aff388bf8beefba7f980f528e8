        			<script>
site = 'http://dagobah.lancasteronline.com/closings/php/closings.php';

var yql = 'http://query.yahooapis.com/v1/public/yql?q=' + encodeURIComponent('select * from xml where url="' + site + '"') + '&format=xml&callback=?';

$(document).ready(function(){
	closingsCheck();
	timerId = setInterval(function () {
		closingsCheck();
	}, 60000);
});

function closingsCheck() {
	// Request that YSQL string, and run a callback function.
	// Pass a defined function to prevent cache-busting.
	$.getJSON(yql, function (data) {
		thehtml= "";
		xmlDoc = $.parseXML(data.results[0]);
		$('notopen',xmlDoc).each(function() {
			if ($(this).text() > 0) {
				thehtml +="<a href=\"http://lancasteronline.com/closings\"><img src=\"http://dagobah.lancasteronline.com/closings/closings.jpg\"></a>";
			} else {
				thehtml = "";
			}
		});
			
			$('#lnp-closings').empty();
			$('#lnp-closings').html(thehtml);
	});
}

</script>

