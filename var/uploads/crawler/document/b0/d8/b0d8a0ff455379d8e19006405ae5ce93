  <script type="text/javascript">
    $(function() {

      $('.cg-home-discussion').on('click', function() {
        window.location.href= $(this).attr('data-url');
      })

      $('.cg-home-discussion').on('mouseenter', function() {
        $(this).find('i').addClass('icon-white');      
      });

      $('.cg-home-discussion').on('mouseleave', function() {
        $(this).find('i').removeClass('icon-white');
      });

      $('.cg-home-discussion').each(function() {
        var h = $(this).height();        
        var caretEl = $(this).find('.cg-home-discussionCaret');
        var offset = ((h / 2) + caretEl.height()) + 'px';        
        caretEl.css('top', offset);
      })
    }); 

    
