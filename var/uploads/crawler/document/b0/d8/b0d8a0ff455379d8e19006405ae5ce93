        <script type="text/javascript">
          $(function() {
            var oldBrowser = ($('html').hasClass('ie7') || $('html').hasClass('ie6'));

            if( !oldBrowser ) {
              $('.cg-home-searchForm select#conditionSelector').selectBoxIt({                
                autoWidth:false,
                aggressiveChange: true
              });
              $('.cg-home-searchForm select[cg-name^="ign-m"]').selectBoxIt({                
                showFirstOption: false,
                autoWidth:false,
                width: 120,
                aggressiveChange: true
              });          
            }

            function toggleHomePageSearchForm(){
              var typeOfSearch = $(this).val();
              
              selectHomePageSearchForm(typeOfSearch);

              var selectBox = $('.cg-home-searchForm select[cg-name^="ign-m"]').data('selectBox-selectBoxIt');                
              selectBox.refresh();

            }

            $("#conditionSelector").on('change', toggleHomePageSearchForm);
          })
         
          function getCallbackFunction(slsId) {
              return function() {
                  $('#subscriptionLink' + slsId).remove();
              } 
          }
          
          function selectHomePageSearchForm(typeOfSearch) {
          
            switch (typeOfSearch) {
              case "usedCar":
                $("#usedCarSearch").show();
                $("#newCarSearch").hide();
                $("#CPOCarSearch").hide();
                break;
              case "newCar":
                $("#usedCarSearch").hide();
                $("#newCarSearch").show();
                $("#CPOCarSearch").hide();
                break;
              case "CPOCar":
                $("#usedCarSearch").hide();
                $("#newCarSearch").hide();
                $("#CPOCarSearch").show();
                break;
            }
          }
          
          function convertSavedSearchToSubscription(slsId, entityName) {
                  var callbackFunctionString = "function() {$('#subscriptionLink" + slsId + "').remove();}";
                  CG.MySavedSearches.promptForNewsletterSubscription(
                    slsId, 
                    entityName, 
                    false, 
                    "HOMEPAGE", 
                    callbackFunctionString,
                    "/Cars/inventorylisting/viewPromptConvertSearchToSubscriptionDialog.action"
                  );
          }
        </script>
      </div>
    </div>
