<script type="text/javascript" charset="utf-8">
$(function() {
  //Init popover
  $('.cargurusSearchToggle').popover({
    html : true,
    placement: 'bottom',
    content : '<div id="popoverContent"></div>',
    template: '<div class="popover"><div class="arrow" style="left:200px;"></div><div class="popover-inner"><h3 class="popover-title hidden"></h3><div class="popover-content"><p></p></div></div></div>'
  });
  $('.cargurusSearchToggle').on('click', function() {
    var htmlToInsert = $('.searchCarGurusForm').html();
    $('#popoverContent').html(htmlToInsert);
    $('#popoverContent form').attr('id', 'cse-search-box');
    
    var script = document.createElement('script');
    script.type = 'text/javascript';    
    script.src = 'http://www.google.com/cse/brand?form=cse-search-box&amp;lang=en';
    $('#popoverContent').append(script);
    
    var popoverOffsetNum = $('.popover').css('left').split('px')[0] - 80;
    $('.popover').css('left', popoverOffsetNum+'px');
    //$('#popoverContent input[name="q"]').css('background-color', '#FFFFFF');
    //$('#popoverContent input[name="q"]').focus();
  });

})
</script>




