<script language="JavaScript" type="text/javascript">
        var unloadSet = false, showUnloadMessage = true, dndTarget = null;
        function setUnloadWarning () {
            if (!unloadSet) {
                window.onbeforeunload = function () {
                    if (showUnloadMessage) {
                        return 'You will lose your conversions if you leave this page.';
                    }
                }
            }
        }
        
        function unsetUnloadWarning () {
            window.onbeforeunload = null;
        }
        
        if (typeof (window.FileReader) !== 'undefined') {
            dndTarget = 'body';
        }
        
        var converter = new Zamzar.FileConverter({
            browseButton: 'inputFile',
            convertButton: 'uploadButton',
            //uploadContainer: 'uploader',
            dndTarget: dndTarget,
            generateSessionUrl: '/uploader/convert.php?url=/Convert/GenerateSession',
            uploadUrl: '/uploader/convert.php?url=/Convert/Upload',
            statusUrl: '/uploader/convert.php?url=/Convert/GetStatus',
            progressContainer: 'convert-queue',
            emailTextfield: 'toEmail',
            formatDropdown: 'toExtensionSel',
            statusInterval: 10,
            serverId: tcs,
            maxFiles: 10,
            maxUploadSize: 100,

            /**
             * @callback
             * Called when there is a status update of the progress
             * @param {Number} percent The percentage of the overall upload progress
             * @param {Number} fileNumberUploading The file number in the queue that is being uploaded
             * @param {Number} totalFiles The total number of files in the queue
             * @param {Number} bytesPerSec The number of bytes per second the upload is taken
             */
            onUploadStatus: function (percent, fileNumberUploading, totalFiles, bytesPerSec) {
                setUnloadWarning();
                document.title = Math.floor(percent) + '% Complete - Zamzar';
            },
            
            onUploadComplete: function () {
                showUnloadMessage = false;
                setTimeout(function () {
                    document.location.href = '/uploadComplete.php?convertFile=' + encodeURIComponent(converter.getFromFormat()) + '&to=' + encodeURIComponent(converter.getToFormat());
                }, 0);
            },

            /**
             * @callback
             * Called when there is an error that the user must be notified about
             * @param {String} message The error message
             */
            onError: function (message, additionalInfo) {
                if (typeof (additionalInfo) === 'undefined') {
                    additionalInfo = null;
                }
                Zamzar.Dialog.showError(message, additionalInfo);
            }
        });
        
        $(document).ready(function () {
            if (converter.isDnDSupported()) {
                $('#dnd-help').show();
            }
        });
</script>

