        dnLoadScript("/foundation/scripts/slideshow.js");
      
        Event.observe(document, "dom:loaded", function () {
            var slides = [];
            $$("#TopStories .FeaturedListItem").each(function (a) {
                var src = a.readAttribute("data-slide-src");
                if (!src) return;
                slides.push({
                    src: src,
                    width: new Number(a.readAttribute("data-slide-width")),
                    height: new Number(a.readAttribute("data-slide-height")),
                    href: a.readAttribute("data-slide-href"),
                    caption: a.readAttribute("data-slide-caption")
                });
            });
          
            new dnSlideshow(
                "TopStoriesSlideshow",
                { bezelImage: "/foundation/images/slideshow-bezel.png",
                  transition: "slide",
                  width: 465,
                  height: 200,
                  alwaysAnimate: true,
                  captionId: "caption-TopStories",
                  captionCallback: function (caption) {
                    // Stock caption updater.
                    var captionElem = $("caption-TopStories");
                    if (!captionElem) return;
                    captionElem.innerHTML = caption;

                    // ...And update background of corresponding element.
                    $$("#TopStories .FeaturedListItem").each(function (a) {
                        var slideCaption = a.readAttribute("data-slide-caption");
                        if (caption && (caption == slideCaption)) {
                          a.addClassName('selected');
                        }
                        else {
                          a.removeClassName('selected');
                        }
                    });
                  }
                },
                slides
            ).start();
        });
      //--></script>
    


