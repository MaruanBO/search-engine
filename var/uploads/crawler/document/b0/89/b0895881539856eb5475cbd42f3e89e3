      function play_trailer(){ 

    var src1 = jQuery('#movie').attr('src');

    src2 = src1+'?autoplay=1';

    jQuery('#movie').attr('src', src2);

  }

  

  function pause_trailer(){

      var src1 = jQuery('#movie').attr('src');     

      var src2 = src1.replace("autoplay=1","");

       var src2 = src1.replace("?autoplay=1","");

    jQuery('#movie').attr('src', src2);

  }

  

