<script type="text/javascript">
    var formId = 'newsletter-validate-detail';
    var myForm = new VarienForm(formId);
    var postUrl = "http://www.zagg.com/us/en_us/newsletter/subscriber/newAjax/";
    function doAjax() {
        if (myForm.validator.validate()) {
            $('loading-form').show();
            new Ajax.Request(
                postUrl,
                {
                    method:'post',
                    asynchronous:true,
                    evalScripts:false,
                    onComplete:function(transport) {
                        if (transport && transport.responseText){
                            try{
                                response = eval('(' + transport.responseText + ')');
                            }
                            catch (e) {
                                response = {};
                            }
                        }

                        if (response.success){
                            $('loading-form').hide();
                            $('newsletter-validate-detail').hide();
                            $('formSuccess').show();
                            $$('#formSuccess h3').first().innerHTML = 'Success!';
                            $$('#formSuccess p').first().innerHTML = 'Welcome to the ZAGG family!';

                        } else {
                            $('loading-form').hide();
                            $('newsletter-validate-detail').hide();
							$('formSuccess').show();
                            $$('#formSuccess h3').first().innerHTML = 'Error!';
                            $$('#formSuccess p').first().innerHTML = response.message;
                        }
                        
                    },
                    parameters: $(formId).serialize(true)
                }
            );
        }
    }
    new Event.observe(formId, 'submit', function(e){
        e.stop();
        doAjax();
    });
</script>
        

<script type="text/javascript">
    jQuery(document).ready(function($) {
        $(".newsletter-button").fancybox({
			maxWidth    : 500,
			autoHeight  : true,
			fitToView   : true,
			width       : '100%',
			height      : '100%',
			autoSize    : false,
			closeClick  : false,
			autoResize  : true,
			afterClose : function() {
				$('#newsletter-validate-detail').show();
				$('#formSuccess').hide();
			}
		}).on('click', function(){
			$('#newsletter').val($('#newsletter_1').val());
		});
		
		$('form.mailing-list-subscription').on('submit', function(e){
			e.preventDefault();
			
			$(".newsletter-button").trigger('click');
		});
		
        $('.mailing-list-subscription').on('change', function(e) {
            emailAddress = $('#newsletter_1').val();
            newsletter = $('#newsletter');
            newsletter.val(emailAddress);
        });
    });

</script>
