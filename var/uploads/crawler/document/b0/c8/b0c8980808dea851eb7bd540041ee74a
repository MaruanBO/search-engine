            $(document).ready(function() {
            //Referring Url logic
            var dummyCookie = GetCookie('isFirstRequest');
            if (dummyCookie != null && dummyCookie != ''){
                SetCookie('REFERRING_URL', '', '-1', '/', 'swisscolony.com');
                SetCookie('isFirstRequest', 'false', '24855', '/', 'swisscolony.com');
            } else {
                var referrer = document.referrer;
                if (referrer == null || referrer == '' || referrer.indexOf(site_base_url) > -1){
                    referrer = 'direct';
                }
                SetCookie('isFirstRequest', 'true', '24855', '/', 'swisscolony.com');
                SetCookie('REFERRING_URL', referrer, '', '/', 'swisscolony.com');
            }
