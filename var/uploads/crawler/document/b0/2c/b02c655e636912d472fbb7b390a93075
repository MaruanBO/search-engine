	var menuLeft = document.getElementById( 'menu-mobile' ),
		showLeftPush = document.getElementById( 'show-menu-mobile' ),
		body = document.body;

	showLeftPush.onclick = function() {
		classie.toggle( this, 'active' );
		classie.toggle( body, 'menu-mobile-push-toright' );
		classie.toggle( menuLeft, 'menu-mobile-open' );
	};
        
        var $doc = $(document);
        $doc.ready(function () {
            $('#main, #featured').click(function() {    
              if($('body').hasClass('menu-mobile-push-toright')){
		classie.toggle( this, 'active' );
		classie.toggle( body, 'menu-mobile-push-toright' );
		classie.toggle( menuLeft, 'menu-mobile-open' );                  
                return false;
              }                              
            });
        });
</script>

