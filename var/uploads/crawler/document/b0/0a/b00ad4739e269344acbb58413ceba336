 	function updowntools_ms(udtool) {
 		for (toolcnt=1; toolcnt<=6; toolcnt++) {
 			if(udtool==toolcnt) {
 				if($("#updwnimg_"+toolcnt).attr("src")=='http://img1.moneycontrol.com/images/revamp/plus.gif') {
 					$("#updwnimg_"+toolcnt).attr("src", 'http://img1.moneycontrol.com/images/revamp/minus.gif');
 					$("#updowntooldiv_ms_"+toolcnt).slideUp(1);
 					$("#updowntooldiv_ms_"+toolcnt).slideDown(500);
 				} else {
 					$("#updwnimg_"+toolcnt).attr("src", 'http://img1.moneycontrol.com/images/revamp/plus.gif');
 					$("#updowntooldiv_ms_"+toolcnt).slideUp(500);
 				}
 			} else {
 				$("#updwnimg_"+toolcnt).attr("src", 'http://img1.moneycontrol.com/images/revamp/plus.gif');
 				$("#updowntooldiv_ms_"+toolcnt).slideUp(500);
 			}
 		}
 	}
