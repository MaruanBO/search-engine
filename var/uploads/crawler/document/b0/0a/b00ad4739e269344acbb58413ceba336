 			var str=reply_msg;
 			str=str.toLowerCase();
 
 			for(i=0;i<=char1h.length;++i) {
 				if(str.indexOf(char1h[i],0)!=-1) {
 					alert("Word "+char1h[i]+" not allowed");
 					return false;
 				}
 			}
 		}  
 
 		/*
 		var extr_lnk=$('#exturl_'+dvid).val();
