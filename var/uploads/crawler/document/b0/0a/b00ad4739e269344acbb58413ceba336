 <script type="text/javascript">
 			var arr_follow_elem = new Array("0","1");
 		var arr_follow_elem = new Array("0");
 	var page_refresh_fol = mcfollow_checkCookie(arr_follow_elem.length);
 	page_refresh_fol = parseInt(page_refresh_fol);
 	
 	if(!isNaN(page_refresh_fol)){
 		var fol_i = page_refresh_fol-1;
 	}else{
 		var fol_i = 0;
 	}	
 
 	for(i=0;i<arr_follow_elem.length;i++)
 	{
 		if(i==fol_i){
 			$("#element_"+i).show();
 		}else{
 			$("#element_"+i).hide();
 		}
 	}
 
 	function update_poll_rslt_mod(msg_id,poll_id,option,thread_id,type)
 	{
 		$.ajax({
 			url:host+"india/messageboard/m3_common_functions.php?action=update_poll_rslt&pollid="+poll_id+"&option="+option+"&callback=?",
 			type:"GET",
 			dataType:"jsonp",
 			success:function(jsonp)
 			{
 				$.each(jsonp,function(i,v){
 					if(v.err_txt==0){
 						total=parseInt(v.opt1)+parseInt(v.opt2)+parseInt(v.opt3)+parseInt(v.opt4);
 						opt1_per=Math.round(parseInt(v.opt1)/total*100);
 						opt2_per=Math.round(parseInt(v.opt2)/total*100);
 						document.poll_replyfrm.f_thread_id.value=thread_id;
 						if(type=='common'){
 						$("#mchp_pollres_"+poll_id).html('<div class="FL w138"><div class="pollbar FL"><span class="blubar" style="width:'+opt1_per+'%;"></span></div><div class="FL wd30 ML2 blutxt">'+opt1_per+'%</div></div><div class="FL w138"><div class="pollbar FL"><span class="redbar" style="width:'+opt2_per+'%;"></span></div><div class="FL wd30 ML2 rdtxt">'+opt2_per+'%</div></div><div class="CL"></div>');
 						$("#mchp_graybox_"+poll_id).html('<div class="FL g_11 MT7">Thank you for voting</div><div class="FL ML8 deb_reply"><input type="text" onblur="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}" onfocus="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}; " onclick="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}" id="f_message_1" name="f_message_1" value="Post your reply here" class="FL MT5 PL10 textarbg" /><input name="update_poll_reply" id="update_poll_reply" type="button" onclick="update_pollreply('+msg_id+',1,1);" class="btn_post FR" /><div class="CL"></div></div><div class="CL"></div></div><div class="CL"></div>');
 						$("#mchp_pollres_"+poll_id).show();$("#mchp_graybox_"+poll_id).show();$("#poll_option_"+poll_id).hide()
 						}else if(type=='ibh'){
 						$("#mchp_pollres_"+poll_id).html('<div class="FL pollr"><div class="barbg FL"><span style="width:'+opt1_per+'%;" class="bar"></span></div><span class="FL">'+opt1_per+'%</span> </div><div class="FL pollr"><div class="barbg FL"><span style="width:'+opt2_per+'%;" class="bar no"></span></div><span class="FL">'+opt2_per+'%</span> </div>');
 						$("#mchp_graybox_"+poll_id).html('<p class="b_17">Thank you for voting!</p><div class="PR MB10"><textarea onblur="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}" onfocus="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}; " onclick="javascript:if(this.value==&quot;Post your reply here&quot;){this.value=&quot;&quot;}" id="f_message_1" name="f_message_1" value="Post your reply here" class="pltxa">Post your reply here</textarea><a href="javascript:;" onclick="update_pollreply('+msg_id+',1,1);" class="pst"><img src="http://img-d02.moneycontrol.co.in/images/promo/wots/post.png" width="60" height="25" alt="" /></a> </div>');
 						$("#mchp_pollres_"+poll_id).show();$("#mchp_graybox_"+poll_id).show();$("#poll_option_"+poll_id).hide()
 						}
 					}
 				})
 			}
 		})
 	}
