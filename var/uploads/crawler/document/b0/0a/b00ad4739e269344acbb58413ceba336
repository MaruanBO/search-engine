 <script>
 	function validateMessageM3(dvid,data) {
 		var reply_msg = $("#m3_txtArea_txt_"+dvid).val();
 		if(reply_msg=="" || reply_msg=='Post your stock queries, tips and information on M3 forum') {
 			alert("Please input your message");
 			$("#m3_txtArea_txt_"+dvid).focus();  
 			return;
 		} else if(reply_msg.length < 2) {
 			alert ("Message needs to be more than 1 characters");
 			$("#m3_txtArea_txt_"+dvid).focus();  
 			return false;
 		}
 		/*
 		else if(reply_msg.length > 5000) {
 			alert("Max 5000 characters allowed");
 			$("#m3_txtArea_txt_"+dvid).focus();  
 			return false;
 		}*/
 		else {
