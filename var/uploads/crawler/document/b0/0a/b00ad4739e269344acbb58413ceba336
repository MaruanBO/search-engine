 				<script>
 					function validate_ysu_rhs(form_id){
 					 var usu_query = $("#"+form_id+" #usu_query").val();
 					 var usu_mn = $("#"+form_id+" #usu_mn").val();
 					 var usu_name = $("#"+form_id+" #usu_name").val();
 					 var usu_city = $("#"+form_id+" #usu_city").val();
 					
 
 					 if(usu_query==''){
 						 alert('Please input your query here.'); $("#"+form_id+" #usu_query").focus();return false;
 					 }
 					 if(usu_query=='Your Query Here'){
 						 alert('Please input your query here.'); $("#"+form_id+" #usu_query").focus();return false;
 					 }
 					 else if(usu_query.length<5){
 						 alert('Please input atleast 20 character.'); $("#"+form_id+" #usu_query").focus();return false;
 					 }
 					 else if(usu_mn.length>0 && usu_mn!='Phone Number (optional)' && isNaN(usu_mn)==true) {
 						 alert('Please input valid contact number.'); $("#"+form_id+" #usu_mn").focus();return false;
 					 }
 					 else if(usu_mn.length>0 && usu_mn!='Phone Number (optional)' && isNaN(usu_mn)==false && usu_mn.length!=10) {
 						 alert('Please input valid contact number.'); $("#"+form_id+" #usu_mn").focus();return false;
 					 }
 					 else if(usu_name==''){
 						 alert('Please input your name.'); $("#"+form_id+" #usu_name").focus();return false;
 					 }
 					 else if(usu_name=='Name'){
 						 alert('Please input your name.'); $("#"+form_id+" #usu_name").focus();return false;
 					 }
 					 else if(usu_name.length<3){
 						 alert('Please input valid name.'); $("#"+form_id+" #usu_name").focus();return false;
 					 }
 					 else if(usu_city==''){
 						 alert('Please input your city.'); $("#"+form_id+" #usu_city").focus();return false;
 					 }
 					 else if(usu_city=='City'){
 						 alert('Please input your city.'); $("#"+form_id+" #usu_city").focus();return false;
 					 }
 					 else if(usu_city.length<3){
 						 alert('Please input valid city.'); $("#"+form_id+" #usu_city").focus();return false;
 					 }
 					 else{
 						
 						if(usu_mn=="Phone Number (optional)") 
 							$("#"+form_id+" #usu_mn").val('');
 
 						
 						
 						if(form_id=="usu_form_rhs"){
 
 							$("#usu_form_rhs").attr("action","http://www.moneycontrol.com/india/messageboard/promo/usu_form.php");
 							$("#usu_form_rhs").submit();
 							
 							$("#id_usu_form_rhs").hide();
 							$("#id_usu_form_rhs_msg").html("<p>Thank you for submitting your query.</p>").show();
 							
 						}else if(form_id=="usu_form_rhs1"){
 							$("#usu_form_rhs1").attr("action","http://www.moneycontrol.com/india/messageboard/promo/usu_form.php");
 							$("#usu_form_rhs1").submit();
 							
 							$("#id_usu_form_rhs1").hide();
 							$("#id_usu_form_rhs_msg1").html("<p>Thank you for submitting your query.</p>").show();
 						}
 						return true;
 					 }
 					}
 
 					function closebellPost_rhs(msgid,dvid,data){	
 						var bmsg = $("#cs_bell_m3_rhs #r_message").val();
 						if(bmsg!='' && bmsg!='Your views will be discussed on air.'){
 							if(readCookie('nnmc')){
 							  $("#cs_bell_m3_rhs").attr("action","http://mmb.moneycontrol.com/india/messageboard/home/update_msg2010.php");
 							  $("#cs_bell_m3_rhs").submit();
 							}
 							else{ 
 								$.ajax({
 											url: host+'india/messageboard/msgpost_before.php?action=update_action_json&msg='+escape(bmsg)+'&r_rply_id='+msgid+'&callback=?',
 											type: 'GET',
 											dataType: 'jsonp',     
 											success: function(jsonp) { 	
 											$.each(jsonp,function(i,v){ 
 												document.cs_bell_m3_rhs.rmsgid.value=v.ret_txt;
 												check_login_m3('41');
 												return false;
 											});
 											}
 											});
 							}
 						}
 						else{
 							 alert("Please input your message");
 							 $("#cs_bell_m3_rhs #r_message").focus();
 							 return false;
 						}
 				}
 
 
 				function over__lay(e){
 					var t=$("#opini_bx").height()/2;
 					var n=$("#opini_bx").width()/2;
 					$("#opini_bx").css({"margin-top":-t,"margin-left":-n});
 					$("#pop_txt").html($("#st_"+e).html());
 					$("#f_popmessage").val("Tell us in few words");
 					
 					$("#m3debatebox #f_topicid").val($("#msg_id"+e).attr("topicid"));
 					$("#m3debatebox #f_topic").val($("#msg_id"+e).attr("topic"));
 					$("#m3debatebox #f_topicname").val($("#msg_id"+e).attr("topic"));
 					$("#m3debatebox #f_rply_id").val(e);
 					$("#m3debatebox #f_categoryid").val($("#msg_id"+e).attr("category"));
 					$("#m3debatebox #f_heading").val($("#msg_id"+e).attr("topic"));
 					$("#m3debatebox #f_thread_id").val($("#msg_id"+e).attr("thread_id"));
 					
 					$("#lightbox_cb, #opini_bx").fadeIn(300);
 					$("#lightbox_cb").remove();
 					$("body").append('<div id="lightbox_cb"></div>');
 					$("#lightbox_cb").css({filter:"alpha(opacity=80)"}).fadeIn()
 				}
 				</script>
 								
