 				function go_anchor()
 				{
 					//alert($("#cnbc_anchors_drop").val());
 					var cnbc_anc_link = $("#cnbc_anchors_drop").val();
 					if(cnbc_anc_link==''){
 						alert('Please select anchor!');
 						return false;
 					}
 					if(cnbc_anc_link!=''){
 						var arr_id = cnbc_anc_link.split('-');
 						id = arr_id[(arr_id.length)-1];
 					}
 					if(id=='423'){// sajeet
 						window.open('http://mmb.moneycontrol.com/india/messageboardblog/boarders/73616a6565745f6d616e67686174','_blank');
 					}else if(id=='524'){//sonia
 						window.open('http://mmb.moneycontrol.com/india/messageboardblog/boarders/736f6e69617368656e6f795f31393838','_blank');
 					}else if(id=='2564'){// anuj
 						window.open('http://mmb.moneycontrol.com/india/messageboardblog/boarders/416e756a3734','_blank');
 					}else if(id=='161'){//lathaa
 						window.open('http://mmb.moneycontrol.com/india/messageboardblog/boarders/6c61617461','_blank');
 					}else if(id=='2736'){//varinder
 						window.open('http://mmb.moneycontrol.com/india/messageboardblog/boarders/766172696e6465725f62616e73616c','_blank');
 					}else{
 						window.open('http://www.moneycontrol.com/elite/profile/'+cnbc_anc_link+'.html','_blank');
 					}
 				}
 				function show_validate()
 				{
 					var cnbc_show = $("#search_str_cnbc").val();
 					if(cnbc_show!=''){
 						window.location="http://www.moneycontrol.com"+cnbc_show;
 					}
 				}
