 function validate_ysu()
 {
  var usu_query = $("#usu_form #usu_query").val();
  var usu_name = $("#usu_form #usu_name").val();
  var usu_city = $("#usu_form #usu_city").val();
  var usu_mn = $("#usu_form #usu_mn").val();
 
  if(usu_query==''){
 	 alert('Please input your query here.'); $("#usu_form #usu_query").focus();return false;
  }
  if(usu_query=='Your Query Here'){
 	 alert('Please input your query here.'); $("#usu_form #usu_query").focus();return false;
  }
  else if(usu_query.length<5){
 	 alert('Please input atleast 20 character.'); $("#usu_form #usu_query").focus();return false;
  }
  else if(usu_name==''){
 	 alert('Please input your name.'); $("#usu_form #usu_name").focus();return false;
  }
  else if(usu_name=='Name'){
 	 alert('Please input your name.'); $("#usu_form #usu_name").focus();return false;
  }
  else if(usu_name.length<3){
 	 alert('Please input valid name.'); $("#usu_form #usu_name").focus();return false;
  }
  else if(usu_city==''){
 	 alert('Please input your city.'); $("#usu_form #usu_city").focus();return false;
  }
  else if(usu_city=='City'){
 	 alert('Please input your city.'); $("#usu_form #usu_city").focus();return false;
  }
  else if(usu_city.length<3){
 	 alert('Please input valid city.'); $("#usu_form #usu_city").focus();return false;
  }
 else if(usu_mn.length>0 && usu_mn!='Phone Number (optional)' && isNaN(usu_mn)==true) {
 	 alert('Please input valid contact number.'); $("#usu_form #usu_mn").focus();return false;
  }
  else if(usu_mn.length>0 && usu_mn!='Phone Number (optional)' && isNaN(usu_mn)==false && usu_mn.length!=10) {
 	 alert('Please input valid contact number.'); $("#usu_form #usu_mn").focus();return false;
  }
  else{
 	
 	if(usu_mn=="Phone Number (optional)") 
 		$("#usu_form #usu_mn").val('');
      
 	$("#usu_form").attr("action","http://www.moneycontrol.com/india/messageboard/promo/usu_form.php");
 	 $("#usu_form").submit();
 	 $("#show_tkm").html("");
 	 $("#show_tkm").html("<div class='MT10 w13'>Thank you for submitting your query.</div>");
 	return true;
  }
 }
 </script>
