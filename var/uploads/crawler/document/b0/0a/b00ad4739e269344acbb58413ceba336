 			function show_specials()
 			{
 				var special_html=document.getElementById('spl_ban').innerHTML;
 				if(special_html=='')
 				{
 					$('#spl_ban').load('/mccode/common/show_specials.php', '', function(data) 
 					{					
 						document.getElementById('spl_ban').innerHTML=data;
 						document.getElementById('spl_ban').style.display='block';
 					});
 				}
 				else
 					document.getElementById('spl_ban').style.display='block';	
 			}
 
 			
