					<script type="text/javascript">
				jQuery(function($) {					
					$('#dynamic-list-menu').flexslider({
						asNavFor: '#dynamic-list',
						animation: false,
						animationLoop: false,
						controlNav: false,
						direction: 'vertical',
						itemWidth: 115,
						itemMargin: 8,
						keyboard: false,
						pauseOnAction: true,
						slideshow: false,
						useCSS: false,
						video: true
					});
					$('#dynamic-list').flexslider({
						sync: '#dynamic-list-menu',
						animation: "fade",
						animationLoop: true,
						controlNav: false,
						directionNav: false,
						keyboard: false,
						pauseOnAction: true,
						slideshow: true,
						slideshowSpeed: 5000,
						animationSpeed: 800,
						video: true,
						start: function() {
							$('#dynamic-list.canvas li.flex-active-slide.video-content .action-video')
								.on('click', function(e){
									var $video_content_container = $(this).parent();
									if ($video_content_container.hasClass('status-video-playing')) return;

									$('#dynamic-list').flexslider('pause');

									$video_content_container.find('img.thumbnail').css('display', 'none');
									$video_content_container.find('.action-thumbnail').css('display', 'none');
									$video_content_container.find('.dl-canvas-video').css('display', 'block');

									var video_id = $video_content_container.find('.dl-canvas-video .worldnow-video-container').data('video-id');

									if (video_id && /^[a-z0-9]+$/i.test(video_id)) {
										var function_call = 'cbs_video_wnplayer_' + video_id + '_start';

										if (typeof window[function_call] == 'function') {
											$video_content_container.addClass('status-video-playing'); //prevent multiple loads
											window[function_call]();	//eval(function_call);
										}
									}
								});
							
							var ua = navigator.userAgent.toLowerCase();
							if (ua.indexOf('ipad') > -1) return;
							
							$('#dynamic-list-menu .flex-active-slide .wrapper').on('click', function(e){
								var $active_slide_links = $('#dynamic-list.canvas li.flex-active-slide a.node');
								if ($active_slide_links.length == 1) {
									//trigger and attached events
									$active_slide_links.trigger('click');
									// and if default triger doesn't work (and it won't), redirect manually
									if ($active_slide_links.attr('target') == '_blank') {
										window.open($active_slide_links.attr('href'));
									} else {
										window.location = $active_slide_links.attr('href');
									}
								}
							});
							
						},
						after: function() {
							$('#dynamic-list.canvas li.flex-active-slide.video-content .action-video')
								.on('click', function(e){
									var $video_content_container = $(this).parent();
									if ($video_content_container.hasClass('status-video-playing')) return;

									$('#dynamic-list').flexslider('pause');

									$video_content_container.find('img.thumbnail').css('display', 'none');
									$video_content_container.find('.action-thumbnail').css('display', 'none');
									$video_content_container.find('.dl-canvas-video').css('display', 'block');

									var video_id = $video_content_container.find('.dl-canvas-video .worldnow-video-container').data('video-id');

									if (video_id && /^[a-z0-9]+$/i.test(video_id)) {
										var function_call = 'cbs_video_wnplayer_' + video_id + '_start';

										if (typeof window[function_call] == 'function') {
											$video_content_container.addClass('status-video-playing'); //prevent multiple loads
											window[function_call]();	//eval(function_call);
										}
									}
								});
								
							var ua = navigator.userAgent.toLowerCase();
							if (ua.indexOf('ipad') > -1) return;
							
							$('#dynamic-list-menu .flex-active-slide .wrapper').on('click', function(e){
								var $active_slide_links = $('#dynamic-list.canvas li.flex-active-slide a.node');
								if ($active_slide_links.length == 1) {
									//trigger and attached events
									$active_slide_links.trigger('click');
									// and if default triger doesn't work (and it won't), redirect manually
									if ($active_slide_links.attr('target') == '_blank') {
										window.open($active_slide_links.attr('href'));
									} else {
										window.location = $active_slide_links.attr('href');
									}
								}
							});
						},
						before: function() {
							$('#dynamic-list-menu .flex-active-slide .wrapper').off('click');
							$('#dynamic-list .wn-video-canvas').empty(); // clear ALL video content
							$('#dynamic-list.canvas li.flex-active-slide.video-content').removeClass('status-video-playing');

							$('#dynamic-list.canvas li .dl-canvas-video').css('display', 'none');
							$('#dynamic-list.canvas li img').css('display', 'block');
							$('#dynamic-list.canvas li .action-thumbnail').css('display', 'block');
						}
					});
				});
			</script>
						</section>
	
	
