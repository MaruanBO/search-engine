            function weather_jsonp_cb(data) {
                var html = data['html'];
                if (html) {
                    cmg.query('#cmWeatherWidgetLarge').html(html);
                }
            }
            var widget_path = '/weather/large-widget/';
            
            var request_url = 'http://' + site_domain + widget_path;
            cmg.query.ajax({
                url: request_url,
                method:'GET',
                accepts:'text/javascript',
                data: {format:'jsonp', callback:'weather_jsonp_cb'},
                dataType:'script',
                error: function() { },
                success: function() {
                    
                }
            });
        </script><div id="cmSearchForm" class="cmFloatRight cmClearfix"><form action="/search/" method="get" class="search_form" autocomplete="off"><p><input type="text" id="q" name="q" value=""><button type="submit">Search</button><script type="text/javascript">
            cmg.query(function(){
                var search_fields = document.querySelectorAll("[name=q]");

                for (var i=0; i < search_fields.length; i++) {
                    search_fields[i].setAttribute("value", cmg.utility.loc_query('q'));
                }
            });
        </script></p></form></div></div><!-- header003.html --><div class="cmNavigation cmLeftFloat cmClearfix theme-navigation-background"><ul class="cmSiteNav"><li class="cmPrimaryNavMenu"><a href="/">Home</a></li><li class="cmPrimaryNavMenu"><a href="/s/news/">News</a><ul class="cmSubNav"><li class="cmSubNavMenu"><a href="/s/news/">Local &amp; Regional</a></li><li class="cmSubNavMenu"><a href="/s/news/nation-world/">National News</a></li><li class="cmSubNavMenu"><a href="/s/news/county-by-county/">County By County<span>&raquo;</span></a><ul class="cmSubNav"><li class="cmSubNavMenu"><a href="/s/news/atlanta/">City of Atlanta</a></li><li class="cmSubNavMenu"><a href="/s/news/carroll/">Carroll County</a></li><li class="cmSubNavMenu"><a href="/s/news/cherokee/">Cherokee County</a></li><li class="cmSubNavMenu"><a href="/s/news/clarke/">Clarke County</a></li><li class="cmSubNavMenu"><a href="/s/news/clayton/">Clayton County</a></li><li class="cmSubNavMenu"><a href="/s/news/cobb/">Cobb County</a></li><li class="cmSubNavMenu"><a href="/s/news/coweta/">Coweta County</a></li><li class="cmSubNavMenu"><a href="/s/news/dekalb/">DeKalb County</a></li><li class="cmSubNavMenu"><a href="/s/news/douglas/">Douglas County</a></li><li class="cmSubNavMenu"><a href="/s/news/fayette/">Fayette County</a></li><li class="cmSubNavMenu"><a href="/s/news/forsyth/">Forsyth County</a></li><li class="cmSubNavMenu"><a href="/s/news/fulton/">Fulton County</a></li><li class="cmSubNavMenu"><a href="/s/news/gwinnett/">Gwinnett County</a></li><li class="cmSubNavMenu"><a href="/s/news/hall/">Hall County</a></li><li class="cmSubNavMenu"><a href="/s/news/henry/">Henry County</a></li><li class="cmSubNavMenu"><a href="/s/news/newton/">Newton County</a></li><li class="cmSubNavMenu"><a href="/s/news/paulding/">Paulding County</a></li><li class="cmSubNavMenu"><a href="/s/news/rockdale/">Rockdale County</a></li><li class="cmSubNavMenu"><a href="/s/news/spalding/">Spalding County</a></li></ul></li><li class="cm