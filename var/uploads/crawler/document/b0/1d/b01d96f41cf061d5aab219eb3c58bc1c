	// setup the ticker queue
	
	var last_date = 'Thu Feb 19 21:31:24 +0000 2015';
	var queue = [{"text":"\u003Cdiv class='beat-content'\u003E\r\n    \r\n\t\t\u003Ca href='/obituary/show/Kasimer-Casey-Zabek-102189673' \u003E\r\n\t\t\t\u003Cimg src=\"http://d3trabu2dfbdfb.cloudfront.net/3/9/3958172_75x75.jpeg\" title=\"\" class=\"tickerImage\"\u003E\u003C/img\u003E\r\n\t\t\u003C/a\u003E\r\n\t\r\n\t\u003Cdiv class='beat-text'\u003E\r\n\t\t\u003Ca href='/obituary/show/Kasimer-Casey-Zabek-102189673' class=\"beat-text-personLink\"\u003E\r\n\t\t\tMr. Kasimer \"Casey\" Zabek\r\n\t\t\u003C/a\u003E\r\n\t\t\r\n\t\t\u003Cspan class='beat-memory-text'\u003E\r\n\t\t\t\"I wanted to send my best wishes and apologies that I am not\u003Ca href='/condolences/view_memories/102189673#6774734' \u003E...\u003C/a\u003E\"\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cbr/\u003E\r\n\t\t\u003Cspan class='author'\u003E\r\n\t\t\tMike Zabek - \u003Cem\u003EAnn Arbor\u003C/em\u003E,  \u003Cem\u003EMI\u003C/em\u003E\u0026nbsp;\u0026nbsp;|\u0026nbsp;\u0026nbsp;\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cspan class='content_date' title='February 19 21:29:46 UTC 2015'\u003E\u003C/span\u003E\t\r\n\t\u003C/div\u003E\r\n\u003C/div\u003E","time":"2015/02/19 21:29:46 +0000"},{"text":"\u003Cdiv class='beat-content'\u003E\r\n    \r\n\t\t\r\n\t\t\t\u003Cimg src=\"/images/memory-book/leave_a_message.png\" class=\"tickerImage\"/\u003E\r\n\t\t\r\n\t\t\u003C!-- placeholder image? --\u003E\r\n\t\r\n\t\u003Cdiv class='beat-text'\u003E\r\n\t\t\u003Ca href='/obituary/show/Jerry-L.-James-102201733' class=\"beat-text-personLink\"\u003E\r\n\t\t\tJerry L. James\r\n\t\t\u003C/a\u003E\r\n\t\t\r\n\t\t\u003Cspan class='beat-memory-text'\u003E\r\n\t\t\t\"To the family of jerry ,I meant jerry when I workers at carls \u003Ca href='/condolences/view_memories/102201733#6774735' \u003E...\u003C/a\u003E\"\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cbr/\u003E\r\n\t\t\u003Cspan class='author'\u003E\r\n\t\t\tAndrea colwell  \u0026nbsp;\u0026nbsp;|\u0026nbsp;\u0026nbsp;\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cspan class='content_date' title='February 19 21:30:06 UTC 2015'\u003E\u003C/span\u003E\t\r\n\t\u003C/div\u003E\r\n\u003C/div\u003E","time":"2015/02/19 21:30:06 +0000"},{"text":"\u003Cdiv class='beat-content'\u003E\r\n    \r\n\t\t\u003Ca href='/show/Chadli-Bendjedid-94537359' \u003E\r\n\t\t\t\u003Cimg src=\"http://d3trabu2dfbdfb.cloudfront.net/1/8/1832127_75x75_1.jpeg\" title=\"\" class=\"tickerImage\"\u003E\u003C/img\u003E\r\n\t\t\u003C/a\u003E\r\n\t\r\n\t\u003Cdiv class='beat-text'\u003E\r\n\t\t\u003Ca href='/show/Chadli-Bendjedid-94537359' class=\"beat-text-personLink\"\u003E\r\n\t\t\tChadli  Bendjedid\r\n\t\t\u003C/a\u003E\r\n\t\t\r\n\t\t\u003Cspan class='beat-memory-text'\u003E\r\n\t\t\t\"Hi Justin!I just want to leave some love.   You have improved so\u003Ca href='/condolences/view_memories/94537359#6774736' \u003E...\u003C/a\u003E\"\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cbr/\u003E\r\n\t\t\u003Cspan class='author'\u003E\r\n\t\t\tJean - \u003Cem\u003E6oCcTS9JcG\u003C/em\u003E,  \u003Cem\u003ECA\u003C/em\u003E - Shj1Lvysn\u0026nbsp;\u0026nbsp;|\u0026nbsp;\u0026nbsp;\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cspan class='content_date' title='February 19 21:30:34 UTC 2015'\u003E\u003C/span\u003E\t\r\n\t\u003C/div\u003E\r\n\u003C/div\u003E","time":"2015/02/19 21:30:34 +0000"},{"text":"\u003Cdiv class='beat-content'\u003E\r\n    \r\n\t\t\r\n\t\t\t\u003Cimg src=\"/images/memory-book/leave_a_message.png\" class=\"tickerImage\"/\u003E\r\n\t\t\r\n\t\t\u003C!-- placeholder image? --\u003E\r\n\t\r\n\t\u003Cdiv class='beat-text'\u003E\r\n\t\t\u003Ca href='/show/Andrew-J.-Marmion-92084697' class=\"beat-text-personLink\"\u003E\r\n\t\t\tAndrew J. Marmion\r\n\t\t\u003C/a\u003E\r\n\t\t\r\n\t\t\u003Cspan class='beat-memory-text'\u003E\r\n\t\t\t\"You've been my thought this afternoon.  I feel an enormous loss\u003Ca href='/condolences/view_memories/92084697#6774739' \u003E...\u003C/a\u003E\"\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cbr/\u003E\r\n\t\t\u003Cspan class='author'\u003E\r\n\t\t\tMichael Holcombe - \u003Cem\u003EAtlanta\u003C/em\u003E,  \u003Cem\u003EGA\u003C/em\u003E\u0026nbsp;\u0026nbsp;|\u0026nbsp;\u0026nbsp;\r\n\t\t\u003C/span\u003E\r\n\t\t\u003Cspan class='content_date' title='February 19 21:31:24 UTC 2015'\u003E\u003C/span\u003E\t\r\n\t\u003C/div\u003E\r\n\u003C/div\u003E","time":"2015/02/19 21:31:24 +0000"}];
	var time = 5;
	var first_entity = null;
	var margin_value = 0;
	var ticker_delay = 60;
	
	function update_queue() {
		if ( queue.length > 5 ) {
			trib_debug('Queue Length OK skip lookup');
			// don't bother updating the queue till it is down to 5 or less entries.
			return; 
		}
		trib_debug('Queue Length LOW do lookup');
		
		new Ajax.Request('/ticker/fetch_next/' + last_date, {
	        method: "get",
	        onComplete: function(request){
	            if (request.status == 200) { 
					o = request.responseText.evalJSON();
					if (o != null) {
						trib_debug('Entry added to Queue');
                        queue.push(o);
						last_date = o.time;
					}	
				} 
	       	}
	    });
	}
	
	var beat_index = 1;
    var pause = false;
	
	function move_scroll(){
		if (pause || first_entity == null || queue.length == 0) {return;}
		
		//trib_debug("moveScroll - " + margin_value);
		
		margin_value = margin_value + 1;
		first_entity.morph('margin-left:' + margin_value + 'px;', {duration: 0});
		
		//trib_debug(first_entity.getStyle('margin-left'));
		
		if (margin_value == 0 ) {
			update_ticker();
		}
	}
	
	function update_ticker() {
		if (queue.length == 0 || pause == true) {  
			setTimeout('update_ticker();', ticker_delay)
			return;	
		}
		trib_debug('Queue Entries ' + queue.length);
		entry = queue.shift();
		if (entry) {
			str = '<div class="beat_window" style="width:350px;" id="beat_wi_' + beat_index + '"><div id="beat_no_' + beat_index + '" class="beat">' + entry.text + '</div></div>';
			$('ticker').insert({
				top: str
			});
			beat_in(beat_index);
			beat_index++;
			show_dates_as_local_time();
		}		
	}
	
    function beat_in(beat_no) {
		margin_value = -350;
		first_entity = $('beat_wi_' + beat_no);
	}    
	Event.observe(window, 'load', ticker_date_init);	
		
    var ticker_active_updater;
	function ticker_date_init() {

		show_dates_as_local_time();
		ticker_active_updater = setInterval("show_dates_as_local_time();", 10000);
		ticker_active_updater = setInterval("update_queue();", 5000);
		update_ticker();
		setInterval("move_scroll();", ticker_delay);
	}
</script>

