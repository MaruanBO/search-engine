<script>
(function(window, document, yepnope, $, undefined) {
    yepnope([
        {
            test: $().jcarousel,
            nope: 'http://bloximages.chicago2.vip.townnews.com/theeagle.com/content/tncms/live/components/core_external_jquery_plugins/resources/scripts/jquery.jcarousel.js?_dc=1415916991',
            complete: function() {
                $(function() {
                    $('#blox-center-piece li').show();
                    function centerPieceDisplay(carousel) {
                        
                        // Pause autoscrolling if the user moves with the cursor over the clip.
                        carousel.clip.hover(function() {
                            carousel.stopAuto();
                        }, function() {
                            carousel.startAuto();
                        });
                                    
                        // add center piece navigation
                        if(carousel.size() > 1) {
                            var nStr = '';
                            for(i=1; i<=carousel.size(); i++){
                                nStr += '<a id="center-piece-page_'+i+'">'+i+'</a>';
                            }
                            $('#blox-center-piece ol').before('<div id="center-piece-nav"></div><div class="clear"></div>');
                            $('#center-piece-nav').html(nStr);
                        }
                                    
                        // add paging event
                        $('#center-piece-nav a').bind('click', function() {
                            carousel.scroll($.jcarousel.intval($(this).text()));
                            carousel.startAuto(0);
                            return false;
                        });
                    };
                            
                    // init callback for centerpiece paging
                    function cpPaging(carousel, state) {
                        $('#center-piece-nav a').removeClass('selected');
                        cpSelected = '#center-piece-page_'+carousel.first;
                        $(cpSelected).addClass('selected');
                    };

                    $('#blox-center-piece ol').jcarousel({
                        auto: 10,
                        scroll: 1,
                        animation: 'slow',
                        buttonNextHTML: null,
                        buttonPrevHTML: null,
                        wrap: 'last',
                        itemLoadCallback: cpPaging,
                        initCallback: centerPieceDisplay
                    });
                });
            }
        }
    ]);
})(window, document, yepnope, jQuery);
</script></div><div id="tncms-block-778244" class="tncms-block">






    
