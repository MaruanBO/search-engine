	function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}
	
	var cookieValue  = readCookie("emailSub");
	
	if(jQuery.browser.mobile == false && !(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) && cookieValue != "emailSubAppeared"){
	  var now = new Date();
	  var time = now.getTime();
	  var expireTime = time + 100000*25920;
	  now.setTime(expireTime);
