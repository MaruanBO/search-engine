<script type="text/javascript">
    mboxCreate("cq_hp_area1");
    // hide default mbox content; hiding just the outer div doesn't work, it will be shown again a bit later
    var defaultDivs = document.getElementById("content-lowes-en_us-home-par-onecolumn_4-grid-par-start").getElementsByTagName("div");
    for(var i=0; i<defaultDivs.length; i++) {
        defaultDivs[i].style.visibility="hidden";
    }
    // show default mbox content if still there
    function showDefaultcq_hp_area1() {
        var defaultContent = document.getElementById("content-lowes-en_us-home-par-onecolumn_4-grid-par-start");
        if (defaultContent) {
            CQ_Analytics.TestTarget.ignoreNextUpdate('cq_hp_area1');
            var defaultDivs = defaultContent.getElementsByTagName("div");
            for(var i=0; i<defaultDivs.length; i++) {
                defaultDivs[i].style.visibility="visible";
            }
        }
    }
    // wait 2s for campaign content to arrive before showing default content
    setTimeout("showDefaultcq_hp_area1()", 2000);
</script>



