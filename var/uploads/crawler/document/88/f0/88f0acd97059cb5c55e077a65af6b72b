<script type="text/javascript">
CQ_Analytics.Sitecatalyst.frameworkComponents = ['lowes/components/content/commons/s7bannerimpressionsanalytics','lowes/components/page/commons/global'];
/**
 * Sets SiteCatalyst variables accordingly to mapped components. If <code>options</code>
 * object is provided only variables matching the options.componentPath are set.
 *
 * @param {Object} options Parameter object from CQ_Analytics.record() call. Optional.
 */
CQ_Analytics.Sitecatalyst.updateEvars = function(options) {
    this.frameworkMappings = [];
    this.frameworkMappings.push({scVar:"event53",cqVar:"eventdata.events.internalCampaignImpression",resourceType:"lowes/components/content/commons/s7bannerimpressionsanalytics"});
    this.frameworkMappings.push({scVar:"pageName",cqVar:"pagedata.pageName",resourceType:"lowes/components/page/commons/global"});
    this.frameworkMappings.push({scVar:"prop2",cqVar:"pagedata.subCategory",resourceType:"lowes/components/page/commons/global"});
    this.frameworkMappings.push({scVar:"prop1",cqVar:"pagedata.category",resourceType:"lowes/components/page/commons/global"});
    this.frameworkMappings.push({scVar:"prop3",cqVar:"pagedata.subSubCategory",resourceType:"lowes/components/page/commons/global"});
    this.frameworkMappings.push({scVar:"prop4",cqVar:"pagedata.pageType",resourceType:"lowes/components/page/commons/global"});
    this.frameworkMappings.push({scVar:"channel",cqVar:"pagedata.department",resourceType:"lowes/components/page/commons/global"});
    for (var i=0; i<this.frameworkMappings.length; i++){
        var m = this.frameworkMappings[i];
        if (!options || options.compatibility || (options.componentPath == m.resourceType)) {
            CQ_Analytics.Sitecatalyst.setEvar(m);
        }
    }
}


//Start the timer for analytics
var startTime = new Date().getTime();

CQ_Analytics.CCM.addListener("storesinitialize", function(e) {
    
    //record the time it took for <thead></thead> storesinitialize event to happen
    var analyticsReady = new Date().getTime();
    var analyticsTime = analyticsReady - startTime;
    s.prop75 = analyticsTime;
    

    var collect = true;
    var lte = s.linkTrackEvents;
    s.pageName="content:lowes:en_us:home";
    CQ_Analytics.Sitecatalyst.collect(collect);
    if (collect) {
        CQ_Analytics.Sitecatalyst.updateEvars();
        /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
        var s_code=s.t();if(s_code)document.write(s_code);
        s.linkTrackEvents = lte;
        if(s.linkTrackVars.indexOf('events')==-1){delete s.events};
        $CQ(document).trigger("sitecatalystAfterCollect");
    }
});
