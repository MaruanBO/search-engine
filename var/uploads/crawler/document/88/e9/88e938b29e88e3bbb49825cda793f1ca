        var srRemoveCookieDate = new Date();
        srRemoveCookieDate.setTime(srRemoveCookieDate.getTime() + (-1 * 24 * 60 * 60 * 1000));
        
        document.cookie = "sr_token=; expires=" + srRemoveCookieDate.toGMTString() + "; path=/;";

