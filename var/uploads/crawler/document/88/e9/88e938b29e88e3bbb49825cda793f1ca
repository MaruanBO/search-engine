function emailOmni()
{
/* REQUIRED FOR OMNITURE DO NOT REMOVE */

	if(typeof esclt != "undefined") {
		emailSignupCustomLinkTrack(this);
	}

/* END OMNITURE */
return false;
}

function onSubmitBrand() {
	brandURL = document.brandSearchForm.brands.options[document.brandSearchForm.brands.selectedIndex].value;
	if (document.brandSearchForm.brands.selectedIndex > 0 && 
		brandURL != '#'){
		location.href=brandURL;
	}

}
</script>












 
 
 	
  		

   




	
	
		
	
	





























	
