			.css({"-webkit-box-shadow": "0px 0px 7px rgba(127, 0, 0, 1)", "-moz-box-shadow": "0px 0px 7px rgba(127, 0, 0, 1)", "box-shadow": "0px 0px 7px rgba(127, 0, 0, 1)"})
		}
		// interval because scrolling will re-fetch products and lose the highlight
		window.d = setInterval(function(){
				setHighLight(location.pathname.replace(/\+/g,'\\+'))
		}, 1000);
	}

