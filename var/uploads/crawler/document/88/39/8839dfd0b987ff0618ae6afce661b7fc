jQuery(function () { if (jQuery('.pbtCircleRange, .pbtCircle').size()>0) jQuery('.pbtCircleRange, .pbtCircle').textfill({innerTag: 'div',debug: false,maxFontPixels: 0});})

function cpwm_checkoutStepVisible(nameOfStep) {
	console.log("checkout step: "+nameOfStep)

	if ( nameOfStep=="gift_delivery" && shippingDelayMsg && shippingDelayMsg.enabled ) {
		var smi = setInterval(function(){
			if ( jQuery('#mst_delivery_container').size()>0 ) {
				clearInterval(smi);
				if ( jQuery('div.shipDelayMsg').size()==0 ) jQuery('<div class="shipDelayMsg">'+shippingDelayMsg.msg+'</div>').prependTo('#mst_delivery_container')			
			}
		},500);
	}

	if ( nameOfStep=="payment" ) {
		/* paypal hide in checkout */
		if ( window.cpwm_hidePaypal ) {
			jQuery('#paymentForm table').css("display","none");
			jQuery('.pymtRadio').css("visibility","hidden");
		}
	}

	if ( nameOfStep == "payment" ) {
		/* remove giftcard as tender option - reanabled 2013 10 21 */
		/*
