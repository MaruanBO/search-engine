			<script type="text/javascript">
					jQuery(function () {
					    jQuery("#defaultCountdown").countdown({
					        until: 69318,
					        compact: true,
					        onExpiry: thrillExpired
					    });
					});
					jQuery(function poll() {
					    setTimeout(function () {
					        jQuery.ajax({
					            url: "/thrillstockquery.do?product=5020042",
					            success: function (data) {
					                var remainingstock = data.thrillstock[0].stock;
					                if (remainingstock <= 0) {
					                    thrillExpired();
					                } else {
					                    jQuery("#thrillstock").text(data.thrillstock[0].stock);
					                    poll();
					                }
					            },
					            error: function (data) {},
					            dataType: "json"
					        });
					    }, 300000);
					});
					function thrillExpired() {
					    location.reload();
					}
					function setpricetag() {
					    jQuery('#thrillprice').textfill({
					        debug: false,
					        maxFontPixels: 0
					    });
					}
					jQuery(setpricetag);
				</script>
