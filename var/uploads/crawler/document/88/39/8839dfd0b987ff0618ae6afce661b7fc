<script>

// Keurig tweaks

(function(){
	if (!keurigHack) return;
	var str = "Add to cart to see price";
	var pCl = prodCodes.length;
	var fixHP = function (o) {
		if ( o.size()==0 ) return;
		jQuery('#thrilloftheday #thrillprice').css({ width: 170, margin: "90px 25px 0px 25px", height: 70, padding: 0, background: "#E00420" });
		jQuery('#thrillprice span').css({ fontSize: 30 })
		jQuery('#thrillprice span#productPricing span').text(str)
	}
	var fixPDP = function (o) {
		if ( o.size()==0 ) return;
		jQuery('.pricingContainer span#productPricing, .singlePrice span#productPricing').css({color: "#E00420"});
		jQuery('.pricingContainer span#productPricing span, .singlePrice span#productPricing span').text(str).css({color: "#E00420"})
	}
	var fixDir = function (o) {
		if ( o.size()==0 ) return;
		var p = o.parents('.qveThumbnail');
		jQuery('#productPricing span', p).text(str).css({fontSize: 16})
	}
	jQuery(function(){
		for (var p=0; p<pCl; p++) {
			fixHP(jQuery('img#thrillimage[src*="'+prodCodes[p]+'"]'));
			fixPDP(jQuery('img#mainimage[src*="'+prodCodes[p]+'"]'));
			fixDir(jQuery('.thumbdiv img[src*="'+prodCodes[p]+'"]'));
		}
		clearInterval(window.kI);
		jQuery('#thrilloftheday #thrillprice, .singlePrice span#productPricing, #productPricing span').css("visibility","visible");
	});
})();

// END Keurig tweaks


