<script language="javascript" type="text/javascript">
	
		// Traffic light is always hidden if ISPU is turned off
		setCookie('C_TRAFFIC_LIGHT_VISIBLE','false',1);
	  
 /**
  *This method set the styling of Global cart.
  *
  */
  function stylingExtensionForGlobalCart(){
		if(jQuery(".navQty span").size()==0){//Do not add additional div if already present
			jQuery(".navQty span").detach().prependTo(jQuery(".navQty").contents().wrap("<div class='qtyCount'><span></span></div>").end());
		}
		jQuery('.navQty > div').each(function(){
			var h, w, max;
			w = jQuery(this).width();
			h = jQuery(this).height();
			max = w > h ? w : h;
			jQuery(this).css('margin-left', -parseInt(w/2,10));
			jQuery(this).css('margin-top', -parseInt(h/2,10));
			jQuery(this).parent().width(max);
			jQuery(this).parent().height(max);
		});
  }
  stylingExtensionForGlobalCart();
</script>
