// swap the current dropdown for our hardcoded test dropdown
// Add AB codes to top link (object or URL)
window.addABcode = function(link, s) {
	var u;
	if (typeof(link) === "string") {
		u = link;
	} else {
		u = jQuery(link).attr("href");
	}
	var ab = s + u
		.replace(/http:\/\/[^\/]*\/(product|category|content)/, '')
		.replace(/\.do(\?.*)?/, '')
		.replace(/\/\/?/g, ":");
	var newUrl = u + ((u.indexOf('?') > -1) ? "&" : "?") + "ab=" + ab;
	if (typeof(link) === "string") {
		return newUrl;
	} else {
		jQuery(link).attr("href", newUrl);
	}
};
window.useAlternateDropNav = function() {
	if (jQuery('#sf-menu-text-nav').size() == 0) return;
	var letjQueryCacheAjax = 1;
	var pathToMenus = '/text/navigation/';
	console.log("alternateMenu: init");
	try {
		jQuery("div.flatCats").each(function(i, o) { // these are the divs that will contain the nav dropdown
			// for each toplevel category
			var catcode = o.id.replace(/[^0-9]/g, '');
			var topcat = [];
			if (typeof(cpwmNavJSON) !== "undefined" && cpwmNavJSON[catcode]) {
				topcat = cpwmNavJSON[catcode].subCats;
			} else {
				jQuery("div.wmSubCat", this).each(function(i, o) {
					// for each sub category
					var link = jQuery("a", this),
						catPk = jQuery(this).attr('catPk');
					var subcat = {
						id: catPk,
						name: link.text(),
						url: link.attr("href"),
						children: []
					};
					jQuery("#flatSubCat_" + catPk + " div.wmSubSubCat a").each(function(i, o) {
						subcat.children.push({
							name: jQuery(this).text(),
							url: jQuery(this).attr("href")
						});
					});
					topcat.push(subcat);
				});
			}
			jQuery(this).load(pathToMenus + catcode + ".txt" + ((letjQueryCacheAjax) ? "" : "?" + new Date().getTime()), function() {
					var col3 = jQuery("div:eq(4)", this); // weird; why 4? too tired
					jQuery(topcat).each(function(i, subcat) {
						subcat.id = subcat.id || subcat.code;
						subcat.name = subcat.name || subcat.displayName;
						subcat.children = subcat.children || subcat.subCats;
						var dl = jQuery('dl#' + subcat.id);
						if (dl.size() == 0) { //not in the template!
							dl = jQuery('<dl/>', {
								id: subcat.id
							}).appendTo(col3);
						}
						jQuery('<dt/>').append('<a href="' + subcat.url + '">' + subcat.name + '</a>').appendTo(dl);
						jQuery(subcat.children).each(function(i, child) {
							child.id = child.id || child.code;
							child.name = child.name || child.displayName;
							jQuery('<dd/>').append('<a href="' + child.url + '">' + child.name + '</a>').appendTo(dl);
						});
					});
					// hide any empty dls
					jQuery('#navContainer dl:empty').hide();
					jQuery('.navContainerArticle a', this).each(function() {
						addABcode(this, "content");
					});
					jQuery('dt a', this).each(function() {
						addABcode(this, "second");
					});
					jQuery('dd a', this).each(function() {
						addABcode(this, "third");
					});
				} // end after-load function
			);
		});
		// clobber this function (assigned to onmouseovers) because not needed
		window.loadCategoryCustomData = function(o) {
			return true;
		}
		console.log("alternateNav complete")
	} catch (e) {
		console.log("alternateNav error: " + e.toString())
	}
}
jQuery(function() {
	// trigger alternate Nav
	useAlternateDropNav();
});
