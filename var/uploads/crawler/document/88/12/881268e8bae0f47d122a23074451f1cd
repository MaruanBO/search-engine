            googletag.pubads()
                .setTargeting('sfw',adGlobal.target.sfw)
                .setTargeting('cat',adGlobal.target.cat)
                .setTargeting('postid',adGlobal.target.postid)
                .setTargeting('page',adGlobal.target.page)
                .setTargeting('pageviews',adGlobal.target.pageviews)
                .setTargeting('tags', adGlobal.target.tags);

            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });
    }
