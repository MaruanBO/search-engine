<script type="text/javascript">
	$(function() {
		jQuery("#mostPopAccordian").removeClass('hide');
		jQuery("#mostPopAccordian div").removeClass('temp-height').removeClass('hide');
		jQuery("#mostPopAccordian h3").removeClass('hide');
		jQuery("#mostPopAccordian .blox-list").removeClass('hide');
		jQuery("#mostPopAccordian .blox-loading").removeClass('blox-loading');
		jQuery("#mostPopAccordian").accordion({
			autoHeight:false,
			header: "h3",
			icons: {
				header: "ui-icon-circle-arrow-e",
				headerSelected: "ui-icon-circle-arrow-s"
			}
		});
	});
</script></div></div>
