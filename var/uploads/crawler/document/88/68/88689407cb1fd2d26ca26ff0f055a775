    <script type="text/javascript">
    $(document).ready(function(){
        $(function(){
            $('#tabIncludefront .tabs-container li:last').after('<li class="clear"></li>');

            $('#tabIncludefront .tab:first').addClass('active');
            $('#tabIncludefront .tab:not(:first)').hide();
            $('#tabIncludefront .tabs-container li a span').each(function(index){
                var newText = $('#tabIncludefront .tab').eq(index).find('.blox-title h3').eq(0).text();
                if(newText != ''){
                    $(this).text(newText);
                    $('#tabIncludefront').removeClass('empty');
                } else {
                    $(this).text('no block');
                    
                        $('#tabIncludefront .tab').eq(index).html('<p>No block title found.  This block may not be using the title macro from the core base library component.</p>');
                        $(this).parents('li').hide();
                        $('#tabIncludefront .tab').eq(index).hide();
                    
                }
            });
            $('#tabIncludefront .tabs-container a').click(function(){
                // adjust tab states
                $('#tabIncludefront .ui-tabs-selected').removeClass('ui-tabs-selected');
                $(this).parent().addClass('ui-tabs-selected');

                // activate windows
                $('#tabIncludefront .tab').hide().removeClass('active');
                t = $(this).attr('href');
                t = t.split('#');
                t = '#tabset_front_'+t[1];
                $(t).show().addClass('active');
                return false;
            });
            
                if(!$('#tabIncludefront').hasClass('empty')) $('#tabIncludefront').show();
            
        });
    });
    </script>

