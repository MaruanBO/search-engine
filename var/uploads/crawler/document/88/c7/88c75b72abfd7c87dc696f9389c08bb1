      <script type="text/javascript" src="http://cdn.cpmstar.com/cached/textadv3.js"></script>
      <script type="text/javascript">
          var cpmstar_src = document.getElementById('cpmstar_dynamicEditorial');
          while (cpmStar.adsLeft() > 0) {
              var span = document.createElement('span');
              span.setAttribute('class', 'gameThumbOuter');
              span.innerHTML = "<a href='" + cpmStar.getLink() + "' target='_blank'>" +
                                   "<img src='" + cpmStar.getImageUrl(90, 60) + "'>" +
                                    "<p>" + cpmstar_trim_title(cpmStar.getTitle(), 18) + "</p>" +
                                "</a>";
              cpmstar_src.appendChild(span);
              cpmStar.nextAd();
          }
          function cpmstar_trim_title(title, maxLength) {
              if (title.length > maxLength) {
                  title = title.substr(0, maxLength - 3) + '...';
              }
              return title;
          }
      </script>
