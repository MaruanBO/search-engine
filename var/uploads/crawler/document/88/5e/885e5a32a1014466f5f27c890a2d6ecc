                <script type="text/javascript">
            
            function shippingInit( custom_option) {
                var $ = jQuery;
                var options = {
                    "form_id"           : "opt_in_form",
                    "shipping_val_class": "shipping_value"
                }
                
                if ( "undefined" !== typeof custom_option ) {
                    custom_option = {};                    
                }
                    
                $.extend(options, custom_option);
                
                var shipping_form = $('form#' + options["form_id"]).get(0);
                
                //This is a workaround for IE inconsistencies with submit event
                shipping_form._submit = shipping_form.submit; 
                shipping_form.submit = function() {
                    var country_value = $('#country option:selected').val();
                    if ( cookies_enabled() ) { 
                        setLifeCellCookie("life_cell_ss", country_value, 1);
                    } else {
                        var input = $("<input>").attr("type", "hidden").attr("name", "sbsc_sv").val(country_value);
                        $(shipping_form).append($(input));
                    }
                    shipping_form._submit();
                };
            }
            
            function setLifeCellCookie(c_name,value,exdays) {
                var exdate=new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
                document.cookie=c_name + "=" + c_value;
            }
            
            function cookies_enabled() {
                var cookieEnabled = (navigator.cookieEnabled) ? true : false;

                if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)
                { 
                        document.cookie="testcookie";
                        cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
                }
                return (cookieEnabled);
            }
        </script>
                        <script type='text/javascript'>
                    jQuery(document).ready(function() {
                        
                        // Optional array can be passed in that has the id of the form
                        // and/or the class of the elements that will display the shipping
                        // price if needed. The defaults are shown below.
                        //    var s_options = {
                        //        "form_id"           : "opt_in_form",
                        //        "shipping_val_class": "shipping_value"
                        //    };
                        
                        shippingInit();
                    });
                </script>
    
