  <div class="malware">
    
       <h1>We track malware <strong>so you don't have to.</strong></h1>

    

    
    <div class="container clear">
      <div class="half col">
        

<script type="text/javascript">

    function getJsonLabsData(methodName, callback) {
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: "../../../services/api/labs.asmx/" + methodName,
            data: "{}",
            dataType: "json",
            success: callback
        });
    }

    $(document).ready(function () {
        getJsonLabsData('GetTopPrevalentMalware12H', function (data) {
            FillTable("#panel-home-threat-malware", data['d'], false);
        });
        getJsonLabsData('GetTopWebThreats12H', function (data) {
            FillTable("#panel-home-threat-web", data['d'], true);
        });
        getJsonLabsData('GetTopEmailMalware1D', function (data) {
            FillTable("#panel-home-threat-spam", data['d'], false);
        });

        getJsonLabsData('GetThreatsBlockedByTime1D', function (data) {
            // FillChart("#threats-7days-mini", data['d'], 12);
        });
    });

    function FillTable(tabID, data, isPercent) {
        var i = 0;

        for (i in data['tableData']) {
            if (data['tableData'][i]['trend'] > 0) {
                imgname = 'up';
            }
            else {
                imgname = 'down';
            }

            if (i > -1) {
                row = $("<tr></tr>").appendTo(tabID + " .trend-table .fancy tbody");
                row.append('<td><a href="/search-results.aspx?search=' + data['tableData'][i]['family'] + '">' + data['tableData'][i]['family'] + '</a></td>');
                if (isPercent) {
                    row.append('<td>' + data['tableData'][i]['percent'] + '%</td>');
                } else {
                    row.append('<td>' + data['tableData'][i]['prevalence'] + '%</td>');
                }

                row.append('<td><span class="' + imgname + '"></td>');

                if (i == 7) {
                    break;
                };
            }
        }
    }

    function FillChart(targeID, data, displayLimit) {
        //console.log("Service Data");
        //console.log(data['chartData'][0]['data']);
        var i = 0;
        var newSpineLineData = new Array();

        for (i in data['chartData'][0]['data']) {
            //console.log(data['chartData'][0]['data'][i][1]);

            newSpineLineData.push(data['chartData'][0]['data'][i][1]);

            if (i > displayLimit) {
                break;
            };
        }
        var sparklineOptions1 = {
            type: 'bar',
            barWidth: 5,
            barSpacing: 3,
            height: '28',
            zeroAxis: false,
            barColor: '#dd6000',
            disableTooltips: true,
            disableHighlight: true
        };

        newSpineLineData.reverse();

        $(targeID).sparkline(newSpineLineData, sparklineOptions1);
    }

  </script>

