<script type="text/javascript">
function closeLogin(){
  document.getElementById('nav_login').className = '';
}

function openLogin(){
  document.getElementById('nav_login').className = 'active hd_silver_tmp';
}

window.fbAsyncInit = function() {
  FB.init({
    appId: '134899576650401',
    status: true,
    cookie: true,
    xfbml: true
  });
  
  };

function fb_login_tmp(){
  $("#reg_fb_box").hide();
  $("#reg_fb").show();
  $("#fb_login_loading").show();
  FB.login(function(response) {
    if (response.authResponse) {
      var accessToken = response.authResponse.accessToken;
      FFN.FB.check_reg_from_fb(accessToken);
    }else{
      $("#reg_fb").hide();
    }
    //FFN.FB.check_reg_from_fb(accessToken);
  });
}

// Load the SDK Asynchronously
(function(d, s, id) {
var js, fjs = d.getElementsByTagName(s)[0];
if (d.getElementById(id)) return;
js = d.createElement(s); js.id = id;
js.src = "//connect.facebook.net/en_US/all.js";
fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
</script>

