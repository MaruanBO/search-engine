<script type="text/javascript"> 
  (function($){
   var options,a
   var bufcity = "";
    jQuery(function(){
      // -- Constants --
      var PLACE_HOLDER_COLOR = "rgb(255,255,255)";
      var PLACE_HOLDER_DATA_NAME = "original-font-color";
      
      options = { 
        serviceUrl: '/autocomplete', 
        minChars : 2,
        width: 'auto', 
        params : {mode:'zip'},
        onSelect: function(value, data){
          $("#fb_geoid").val(data);
          bufcity = $("#fb_loc").val();
          $("#fb_loc").focus();
        }
      };
      a = $('#fb_loc').autocomplete(options);
      //this is to stop the city select from disappering
      // when the user clicks away.
      // basically forcing them to make a choice
      a.el.unbind('blur');
      a.el.blur(function(){});
   
      // makes it so the first element returned is automatically
      // activated (highlighted) and that when we end the
      // defaults the selected Index. Which will be the defautl
      // choice if the users clicks enter
      a._oldProcessResponse = a.processResponse;
      a.processResponse = function (text) {
        this._oldProcessResponse(text);
        this.selectedIndex=0;
        this.activate(this.selectedIndex);
      };
    });
  })(jQuery); 
</script>

  
