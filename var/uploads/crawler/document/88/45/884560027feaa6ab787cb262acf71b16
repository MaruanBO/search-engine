    <script type="text/javascript">
function showSfMenu(a) {
  var winksMenu = a.getElementsByTagName('DIV');
  if (winksMenu.length) {
    winksMenu[1].style.display = 'block';
    winksMenu[1].style.zIndex = '9998';
  }
}


function hideSfMenu(a) {
  var winksMenu = a.getElementsByTagName('DIV');
  if (winksMenu.length) {
    winksMenu[1].style.display = 'none';
  }
}
var FlirtManager = {
    flirted: [ "Winked",
               "Kissed",
               "Waved",
               "Smiled",
               "Hugged"
             ],
    flirtSent: [ "Winks Sent",
                 "Kisses Sent",
                 "Waves Sent",
                 "Smiles Sent",
                 "Hugs Sent"
               ],

    level: "",
    hasStandardContact: "0",
    hasFreemium: "0",
    ok2wink: "0",

    doFlirt: function (val, gravity, topwsid, tohandle, tolevel) {
        try {
            var flirtUrl = "/p/wink.cgi?&who=r,ZwWhQNUH3mTSMUh9zYDJzdIM_ol419tjC5GKhSXUuMgOWghHRqNQXc/DLNkXFQHJ1MUaNPt/qYU52gfu0ukrDRBP1UyEbLbYsqw5rbzRJt8_vVh3ZOfQxs2z3uQWr7of&use_ajax=1&flirt_type=" + val;
            if ( gravity == 'superflirt' ) {
                flirtUrl += '&action=power_wink';
                flirtUrl += '&list=';
                flirtUrl += '&search_type=';
                var tmp = document.getElementById('sfButton');
                if ( tmp && tmp.firstChild && tmp.firstChild.tagName == 'SPAN' ) {
                    tmp.firstChild.innerHTML = FlirtManager.flirtSent[val];
                }
                //hideWinksMenu();
            } else if ( gravity == 'flirt' && topwsid != null && tohandle != null ) {
                flirtUrl += '&action=single_flirt&topwsid=' + topwsid + '&tohandle=' + tohandle;
                var tmp = document.getElementById('flirtTxt_'+topwsid);
                              if ( tmp ) { tmp.innerHTML = FlirtManager.flirted[val]; }
                if ( tmp && tmp.parentNode.parentNode.className == 'sf-container' ) {
                    hideSfMenu(tmp.parentNode.parentNode);
                }
                          }

            AjaxRequest.get({
                'url': flirtUrl,
                'onSuccess': function(req) { FlirtManager.redirectToOrderPage(topwsid,tohandle,gravity); },
                'onError': function(req) {}
            });

            FlirtManager.displayStandardUpsell(tolevel);
            return false;
        } catch (e) { //do not remove this comment
                  }
        return true;
    },
    showSingleFlirtMenu: function(elem, pwsid, handle, level) {
        var menu = document.getElementById('hiddenFlirtMenu');
        var divs = elem.getElementsByTagName('DIV');

        if ( menu && divs.length >= 2 ) {
            if ( divs[1].innerHTML.length == 0 ) {
                var hug = document.getElementById('hiddenFlirt4');
                hug.href = "javascript: FlirtManager.doFlirt(4,'flirt','" + pwsid + "','" + handle + "','" + level + "'); void(0);";
                hug.id = '';

                var kiss = document.getElementById('hiddenFlirt1');
                kiss.href = "javascript: FlirtManager.doFlirt(1,'flirt','" + pwsid + "','" + handle + "','" + level + "'); void(0);";
                kiss.id = '';

                var smile = document.getElementById('hiddenFlirt3');
                smile.href = "javascript: FlirtManager.doFlirt(3,'flirt','" + pwsid + "','" + handle + "','" + level + "'); void(0);";
                smile.id = '';

                var wave = document.getElementById('hiddenFlirt2');
                wave.href = "javascript: FlirtManager.doFlirt(2,'flirt','" + pwsid + "','" + handle + "','" + level + "'); void(0);";
                wave.id = '';

                var wink = document.getElementById('hiddenFlirt0');
                wink.href = "javascript: FlirtManager.doFlirt(0,'flirt','" + pwsid + "','" + handle + "','" + level + "'); void(0);";
                wink.id = '';

                divs[1].innerHTML = menu.innerHTML;
                hug.id = 'hiddenFlirt4';
                kiss.id = 'hiddenFlirt1';
                smile.id = 'hiddenFlirt3';
                wave.id = 'hiddenFlirt2';
                wink.id = 'hiddenFlirt0';
            }

            showSfMenu(elem);
        }

        return;
    },
    redirectToOrderPage: function(a,b,c) {
      if (c == 'superflirt') return false;
      var flirtURL = "/p/wink.cgi?&who=r,ZwWhQNUH3mTSMUh9zYDJzdIM_ol419tjC5GKhSXUuMgOWghHRqNQXc/DLNkXFQHJ1MUaNPt/qYU52gfu0ukrDRBP1UyEbLbYsqw5rbzRJt8_vVh3ZOfQxs2z3uQWr7of&from=wink&topwsid="+a+"&tohandle="+b+"&action=single_flirt";
      var orderURL= "/p/order.cgi?who=r,ZwWhQNUH3mTSMUh9zYDJzdIM_ol419tjC5GKhSXUuMgOWghHRqNQXc/DLNkXFQHJ1MUaNPt/qYU52gfu0ukrDRBP1UyEbLbYsqw5rbzRJt8_vVh3ZOfQxs2z3uQWr7of&site=ffsenior&from=wink&p29433=&p_pwsid=" + a;
      var tmp=FlirtManager.level;
      var openInMain = !!window.opener;
      
      if (parseInt(FlirtManager.level) <= 100){
        if (openInMain) { window.opener.open(orderURL,"ffmain") }
        else { window.location.href = orderURL; }
        return false;
      } else if (parseInt(FlirtManager.level) <= 300){
        return false;
      } else {
        if (openInMain) { window.opener.open(flirtURL,"ffmain") }
        else { window.location.href = flirtURL; }
        return true;
      }
    }, 
    displayStandardUpsell: function (a) {
      if ( parseInt(FlirtManager.level) > 100 && FlirtManager.hasStandardContact == '0' && a == '100' ) {
        if (document.getElementById('iframeContainer')) {
          document.getElementById('iframeContainer').style.display = 'block';
          /* reposition iframe container vertically depending on scrolling */
          document.getElementById('iframeContainer').style.top = containerOffsetTop+parseInt(document.body.scrollTop);
          window.onscroll = function () {
            document.getElementById('iframeContainer').style.top = containerOffsetTop+parseInt(document.body.scrollTop);
          }
          document.onclick = function () {document.getElementById('iframeContainer').style.display = 'none'}
          document.getElementById('targetiframe').src = '/go/page/wink_upsale.html?&who=r,ZwWhQNUH3mTSMUh9zYDJzdIM_ol419tjC5GKhSXUuMgOWghHRqNQXc/DLNkXFQHJ1MUaNPt/qYU52gfu0ukrDRBP1UyEbLbYsqw5rbzRJt8_vVh3ZOfQxs2z3uQWr7of'
          document.getElementById('iframeContainer').style.width = 219;
          document.getElementById('iframeContainer').style.height = 197;
          document.getElementById('targetiframe').style.width = 215;
          document.getElementById('targetiframe').style.height = 180;                  
        } else {
          window.open("/go/page/wink_upsale.html?&who=r,ZwWhQNUH3mTSMUh9zYDJzdIM_ol419tjC5GKhSXUuMgOWghHRqNQXc/DLNkXFQHJ1MUaNPt/qYU52gfu0ukrDRBP1UyEbLbYsqw5rbzRJt8_vVh3ZOfQxs2z3uQWr7of", null, 'resizable=yes,scrollbars=yes,height=180,width=215');
        }
      }
    },
    findPos: function (obj) {
        var curleft = curtop = 0;
        if (obj.offsetParent) {
            curleft = obj.offsetLeft;
            curtop = obj.offsetTop;
            while (obj = obj.offsetParent) {
                curleft += obj.offsetLeft;
                curtop  += obj.offsetTop;
            }
        }
        return [curleft,curtop];
    }
}
</script>



