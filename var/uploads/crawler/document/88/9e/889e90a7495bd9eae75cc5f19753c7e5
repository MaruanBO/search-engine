<!-- ////world map//// -->
function changeImage(num) {
	var src = '';

	if(num == '0') {
		src = 'img/worldmap.gif';
	} else {
		src = 'img/worldmap-' + num + '.gif';
	}
	$(".target").attr('src', src);
}
</script>

