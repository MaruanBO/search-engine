    function GetLocation() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(GetCoordinates)
        } else {
            //TODO: do something here, probably simply hide/disable the 'use current location' button
            //alert("Unable to determine your current location");
            alert("Unable to determine your current location.  Please enter an address.");
        }
    }

    function GetCoordinates(position) {
        document.location = "locations and hours.aspx?location=" + position.coords.latitude + "," + position.coords.longitude + "&searchtype=latlon";       
    }

</script>

