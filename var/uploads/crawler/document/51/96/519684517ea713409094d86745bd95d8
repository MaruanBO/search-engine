(function() {
    var getCookie = function(name) {
        var cookies = document.cookie.split('; ');
        for ( var i = 0; i < cookies.length; i++ ) {
            var parts = cookies[i].split('=');
            if ( parts[0] == name )
                return parts[1];
        }
    };
    var setCookie = function(name, value) {
        var expiration = new Date();
        expiration.setTime(expiration.getTime() + 30 * 60 * 1000);
        document.cookie = escape(name) + '=' + escape(value) + '; expires=' + expiration.toGMTString() + '; path=/';
    };
    var noBounce = function() {
        var entry = getCookie('entry_url');
        if ( entry != 'did_not_bounce' ) {
            _gaq.push(['_trackEvent', 'Bounce', 'Antibounce', getCookie('entry_url') || document.location.toString(), 1, true]);
            setCookie('entry_url', 'did_not_bounce');
        }
    };
    if ( !getCookie('entry_url') ) {
        /* first visit */
        _gaq.push(['_trackEvent', 'Bounce', 'Entry', document.location.toString(), 1, true]);
        setCookie('entry_url', document.location.toString());
        setTimeout(noBounce, 30000);
    } else if ( getCookie('entry_url') ) {
        /* second visit, not a bounce */
        noBounce();
    }
})();

