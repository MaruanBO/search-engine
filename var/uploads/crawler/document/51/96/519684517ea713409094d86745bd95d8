        <script type="text/javascript">

            // Polaroid Slider
            $(function(){

                $('#slides').slides({
                    bigTarget: true,
                    crossfade: true,
                    effect: 'fade',
                    fadeSpeed: 750,
                    generateNextPrev: false,
                    hoverPause: true,
                    play: 5000,
                    preload: true
                });
            });

            // Browse Courses
            (function() {
                var category_courses,
                    $courseList = $( '#courseList' );

                $( '#browseCourses button').click(function() {
                    var self = $ ( this),
                        category  = self.attr( 'data-category');

                    self.addClass( 'current' ).siblings( 'button').removeClass( 'current' );

                    if ( category ) {
                        // Specific cateogory
                        category_courses = $courseList.find( '.course.' + category );

                        $courseList.find( '.course' ).not( '.' + category).not( '.viewMore' ).fadeOut(); // Hide courses not in this category

                        // If there are more than 15 courses in this category, hide where idx > 14
                        if ( category_courses.length > 15 ) {
                            category_courses.each(function( idx, el ) {
                                if ( idx < 14 ) {
                                    $( el ).fadeIn();
                                } else {
                                    $( el ).fadeOut();
                                }
                            });
                            $( '#viewMoreCount').text( category_courses.length - 14 );
                            $courseList.find( '.course.viewMore').fadeIn();
                        } else {
                            category_courses.fadeIn(); // Show this category's courses
                            $courseList.find( '.course.viewMore').fadeOut();
                        }
                    } else {
                        // Show all
                        category_courses = null;
                        course_count = 0;
                        $courseList.find( '.course' ).not( '.viewMore' ).each(function( idx, el ) {
                            course_count++;
                            if ( idx < 14 ) {
                                $( el ).fadeIn();
                            } else {
                                $( el ).fadeOut();
                            }
                        });
                        $( '#viewMoreCount').text( course_count - 14 );
                        $courseList.find( '.course.viewMore').fadeIn();
                    }
                });

                $courseList.find( '.course.viewMore' ).click(function() {
                    if ( category_courses ) {
                        category_courses.fadeIn();
                        $courseList.find( '.course.viewMore').fadeOut();
                    } else {
                        $( '#courseList').find( '.course' ).not( '.viewMore' ).fadeIn();
                        $courseList.find( '.course.viewMore').fadeOut();
                    }
                });
            })();
        </script>
    </body>

