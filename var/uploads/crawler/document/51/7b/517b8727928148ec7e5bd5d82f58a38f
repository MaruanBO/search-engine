        $(document).ready(function() {

                // Creating hoverscroll with fixed arrows
                $('#listarea').append($('#my-list-template').clone().attr('id','my-list').show());
                $('#my-list').hoverscroll({
                        width: $('#my-list').parent().width()-100,
                        height: 82,
                        fixedArrows: false
                });
                $(window).resize(function() {
                        $('#listarea').html('');
                        delay(function(){
                                $('#my-list').width($('#my-list').parent().width()-100);
                                $('#listarea').append($('#my-list-template').clone().attr('id','my-list').show());
                                $('#my-list').hoverscroll({
                                        width: $('#my-list').parent().width()-100,
                                        height: 82,
                                        fixedArrows: false
                                        });
                        });

                });
        });
        var delay = (function(){
                var timer = 0;
                return function(callback, ms){
                        clearTimeout (timer);
                        timer = setTimeout(callback, ms);
                };
        })();

</script>
