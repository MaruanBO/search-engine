<script type="text/javascript">
    function getLocation(event, img, entryID) {
        var relX = event.clientX - img.offsetLeft;
        var relY = event.clientY - img.offsetTop;

        if ((relX > 39) && (relX < 260) && (relY > 249) && (relY < 278)) {
            window.location.href = entryID + "/ShopBy/Bedding/Sheet-Set";
        } else if ((relX > 273) && (relX < 490) && (relY > 249) && (relY < 278)) {
            window.location.href = entryID + "/ShopBy/Value-Paks";
        } else if ((relX > 157) && (relX < 376) && (relY > 289) && (relY < 319)) {
            window.location.href = entryID + "/ShopBy/Decorative-Pillows";
        } else {
            window.location.href = entryID + "/ShopBy/Value-Paks";
        }
}
</script>

