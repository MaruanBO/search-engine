<script language="JavaScript">
    var slideTotal = 5;
    function changeSlide(a) {
        for (var i = 1; i < slideTotal + 1; i++) {
            if (i > slideTotal) i = 1;
            document.getElementById("hero_slide" + i).style.display = "none";
            document.getElementById("hero_dot" + i).className = "slidedot";
        }
        document.getElementById("hero_slide" + a).style.display = "block";
        document.getElementById("hero_dot" + a).className = "slidedot active";
    }

    var currentSlide = 1;
    function showSlide(a) {
        if (a == 1) {
            currentSlide++;
            if (currentSlide > slideTotal) currentSlide = 1;
        }
        changeSlide(currentSlide);
    }

    function slideShow() {
        showSlide(1);
        setTimeout(slideShow, 5000);
    }

    window.onload = function () {
        setTimeout(slideShow, 5000);
    }
</script>


</div>


