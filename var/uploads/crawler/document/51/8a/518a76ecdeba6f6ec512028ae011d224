                    });


                    // This will switch the input for the location key, then submit the form
                    $('#vibeSearchForm.combined.hotels').live('submit', function(){

                        $('form', this).append('<input type="hidden" name="locationKey" value="' + $(this).find('input.vibe_auto_complete[name=city]').attr('data-item-key') + '" />');
                        $(this).find('input.vibe_auto_complete[name=city]').remove();
                        $('#vibeSearchForm.combined.hotels .datepicker').each(function(){
                            formatDates($(this));
                        });

						return true; // now submit
                    });


                });

                function buildDropdown_combined_hotel(elm, url){

                    $.getJSON(url, function(result){

                        var dropdown = '<div class="dropdown_container" style="display: none; max-height: 200px; overflow-y: auto !important;">';

                        for( var i = 0; i < result.length; i++ ){
                            var item = result[i];

                            if( item.type == 'parent' ){
                                dropdown += '<div class="historyBack" style="background:#FFF; color: rgb(62, 62, 62); border-left: 1px solid #aaa; border-right: 1px solid #aaa; cursor: pointer; padding: 4px 0px 4px 7px;" data-item-key="' + item.key + '">&laquo; <span class="placeName">' + item.name + '</span></div>';
                            } else if( item.type == 'loc' ){
                                dropdown += '<div data-numchildren="' + item.children + '" data-item-key="' + item.key + '" class="suggestion"><div class="title"><span class="destName">' + item.name + '</span> <span class="hotelcount">(' + (typeof item.hotelcount == 'string' ? item.hotelcount : '1000+') + ' hotels)</span>' + '</div><div class="info">' + (typeof item.parentname != 'undefined' && item.parentname.length > 0 ? item.parentname + ', ' : '') + item.country + '</div></div>';
                            }
                        }
                        dropdown += '</div>';

                        elm.form_item = $(elm).closest('.formItem');

                        // make sure it appends it to just the current search widget
                        $(elm.form_item).find('.dropdown_container').remove();
                        $(dropdown).insertAfter('.vibe_auto_complete', elm.form_item);
                        $(elm.form_item).find('.dropdown_container').slideDown();
                    });
                }
            </script> 
        
        </div>
      </div>

            </article>
    

