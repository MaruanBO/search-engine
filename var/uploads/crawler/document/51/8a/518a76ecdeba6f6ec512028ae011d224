<div id="block-wtg_marketing-7" class="block-wtg-marketing-adsense-avid-replacement-tag block block-wtg_marketing region-odd odd region-count-1 count-5  ">

  
  <div class="content">
    
    <script type="text/javascript">

        $(document).ready(function(){

            var tag = '<!-- 320x100 - Avid Replacement -->' +
                '<ins class="adsbygoogle" style="display:inline-block;width:320px;height:100px" data-ad-client="ca-pub-2710788903622358" data-ad-slot="6582749533"></ins>' +
                '<script> (adsbygoogle = window.adsbygoogle || []).push({}); </scr' + 'ipt>';

            var targetingContainers = [".node.node-type-feature", "#content-area .node  div.content div.group-airport-facilities:before", "#content-area .node  div.content div.field-field-website:after", "#content-area .node  div.content > div", "#content-bottom"];


            // The avid placeholder tag
            if( $('div#avidTag').length == 1 ){
                $('div#avidTag').html(tag);
            } else {


                for (var i = 0; i < targetingContainers.length; i++){
                    //console.log(targetingContainers[i]);
                    //console.log($(targetingContainers[i]).length);

                    if( $(targetingContainers[i]).length > 0 ){


                        // Features
                        if( targetingContainers[i] == '.node.node-type-feature' ){

                            var found = false;
                            var paras = $('.node.node-type-feature .content>p:not(:empty)');
                            $(paras).each(function(i, elm){
                                if( i == 4 ){
                                    $('<div id="avidTag">' + tag + '</div>').insertAfter(elm);
                                    found = true;
                                    return;
                                }
                                if( found ) return;
                            });
                            if( found ) break;
                        }


                        // Any container that has over 4 elements
                        if( $(targetingContainers[i]).length > 4 ){

                            $('<div id="avidTag">' + tag + '</div>').insertAfter( $(targetingContainers[i]).eq(4) );
                            break;
                        }


                        // Content Bottom
                        if( targetingContainers[i] == '#content-bottom' ){
                            $('<div id="avidTag">' + tag + '</div>').appendTo( $(targetingContainers[i]) );
                            break;
                        }

                    }
                }

            }

            $('head').append('<script async src="http://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></scr' + 'ipt>');

        });

    </script>
  </div>

  
</div>
