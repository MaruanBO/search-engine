<script type="text/javascript">
	$(function() {

		$("#vibeSearchForm.combined .datepicker").datepicker({
			minDate: -0,
			maxDate: "+12M",
			numberOfMonths: 2,
			dateFormat: "d MM, yy",
			onSelect: function(){
                //formatDates($(this));
			}
		});

	});


    /**
     * @param dateInput - JQuery object for the date input field
     */
    function formatDates(dateInput){

        var selectedDate   = dateInput.datepicker("getDate");
        var inputContainer = dateInput.closest('.formItem');
        var postName       = dateInput.attr('data-post-name');

        var day = selectedDate.getDate();
        if(day<10) {
            day = '0'+day
        }

        var month = selectedDate.getMonth()+1;
        var year  = selectedDate.getFullYear();
        if(month < 10) {
            month = '0'+month
        }

        $('input[type=hidden]', inputContainer).remove();
        dateInput.after('<input type="hidden" name="' + postName + '" value="' + month + '/' + day + '/' + year + '" />');
    }
</script>
