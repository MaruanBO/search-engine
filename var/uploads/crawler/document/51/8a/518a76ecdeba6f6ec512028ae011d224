            <script type="text/javascript">
                $('#vibeSearchForm.combined.hotels select[name=numRooms]').live('change', function(){

                    var rooms = {
                        currentTotal : $('#vibeSearchForm.combined.hotels .roomHolder .formItem').length,
                        newTotal     : $('#vibeSearchForm.combined.hotels select[name=numRooms]').val()
                    }

                    if( rooms.newTotal < rooms.currentTotal ){

                        // remove items
                        $('#vibeSearchForm.combined.hotels .roomHolder .formItem:gt(' + (rooms.newTotal-1) + ')').remove();

                    } else if( rooms.newTotal > rooms.currentTotal ){

                        // add items

                        for( var i = rooms.currentTotal; i <= rooms.newTotal; i++ ){

                            if( i > rooms.currentTotal ){

                                var template = $('#vibeSearchForm.combined.hotels .roomHolder .formItem:first').clone();

