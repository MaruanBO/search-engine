			<!-- The JavaScript -->
			<script type="text/javascript">
			if( $('div#under_content').length ){
                $(function() {
                    $(window).scroll(function(){
                        var distanceTop = $('div#under_content').offset().top - $(window).height();
                        var right = $('#slidebox').offset().left - $(window).width();

                        if($(window).scrollTop() > distanceTop){
                            if(right > 0)
                                $('#slidebox').stop(true).animate({'right':'0px'},300);
                        }else{
                            $('#slidebox').stop(true).animate({'right':'-430px'},100);
                        }
                    });
                    $('#slidebox .close').bind('click',function(){
                        $('#slidebox').stop(true).animate({'right':'-430px'},100, function(){
                            $('#slidebox').remove();
                        });
                        return false;
                    });
                });
            }
			</script>
