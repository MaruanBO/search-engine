                <script type="text/javascript">
                    $('#vibeSearchForm.combined.flightshotels select[name=numRooms]').live('change', function(){

                        var rooms = {
                            currentTotal : $('#vibeSearchForm.combined.flightshotels .roomHolder .formItem').length,
                            newTotal     : $('#vibeSearchForm.combined.flightshotels select[name=numRooms]').val()
                        }

                        if( rooms.newTotal < rooms.currentTotal ){

                            // remove items
                            $('#vibeSearchForm.combined.flightshotels .roomHolder .formItem:gt(' + (rooms.newTotal-1) + ')').remove();

                        } else if( rooms.newTotal > rooms.currentTotal ){

                            // add items

                            for( var i = rooms.currentTotal; i <= rooms.newTotal; i++ ){

                                if( i > rooms.currentTotal ){

                                    var template = $('#vibeSearchForm.combined.flightshotels .roomHolder .formItem:first').clone();

                                    $('label', template).replaceWith('<label>Room ' + i + ':</label>');
                                    $('select[name=numAdults1]', template).attr('name', 'numAdults' + i);
                                    $('select[name=numChildren1]', template).attr('name', 'numChildren' + i);
                                    $('select[name=numInfants1]', template).attr('name', 'numInfants' + i);

                                    $('#vibeSearchForm.combined.flightshotels .roomHolder').append(template);
                                }
                            }
                        }
                    });
                </script>

                <script>
                    $(function() {

                        $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete:not(.city)').live('keyup', function(){

                            var self = this;

                            if( typeof window.vibeAutoCompleteCheckCombinedHoliday != 'undefined' ){
                                clearTimeout(window.vibeAutoCompleteCheckCombinedHoliday);
                            }

                            window.vibeAutoCompleteCheckCombinedHoliday = setTimeout(function(){

                                var params = {
                                    'a': 'iatalookup',
                                    'input': $(self).val(),
                                    "type": "airport"
                                }

                                if( params.input.length > 0 ){

                                    var url = 'http://' + window.location.hostname + '/wtg_vibe/api';
                                    url += '?' + $.param(params);

                                    $.getJSON(url, function(result){

                                        var dropdown = '<div class="dropdown_container" style="display: none;">';
                                        for( var i = 0; i < result.results.length; i++ ){
                                            var item = result.results[i];

                                            dropdown += '<div data-id="' + item.id + '" class="suggestion">' + item.value + '</div>';
                                        }
                                        dropdown += '</div>';

                                        self.form_item = $(self).closest('.formItem');

                                        // make sure it appends it to just the current search widget
                                        $(self.form_item).find('.dropdown_container').remove();
                                        $(dropdown).insertAfter('.vibe_auto_complete', self.form_item);
                                        $(self.form_item).find('.dropdown_container').slideDown();
                                    });

                                } else {

                                    $(self).closest('#vibeSearchForm.combined.flightshotels').find('.dropdown_container').slideUp(function(){
                                        $(this).remove();
                                    });
                                }

                            }, 650);
                        });


                        $('#vibeSearchForm.combined.flightshotels .dropdown_container .suggestion:not(.city)').live('click', function(){

                            var selected = {
                                "id" : $(this).attr('data-id'),
                                "itemName" : $(this).text()
                            };

                            var form_item = $(this).closest('.formItem');
                            var auto_input = form_item.find('.vibe_auto_complete');

                            auto_input.attr('data-selected', selected.id);
                            auto_input.val(selected.itemName);

                            $(this).closest('#vibeSearchForm.combined.flightshotels').find('.dropdown_container').slideUp(function(){
                                $(this).remove();
                            });
                        });


                        // This will switch the inputs to the airport codes, then submit the form
                        $('#vibeSearchForm.combined.flightshotels').live('submit', function(){
                            $(this).find('input[name=iataFrom]').val( $(this).find('input[name=iataFrom]').attr('data-selected') );

                            // add the hidden input for the location key
                            $('#vibeSearchForm.combined.flightshotels input[type=submit]').after('<input type="hidden" name="locationKey" value="' + $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete[name=city]').attr('data-item-key') + '" />');

                            $('#vibeSearchForm.combined.flightshotels .datepicker').each(function(){
                                formatDates($(this));
                            });
                            return true; // now submit
                        });


                        $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete.city').live('keyup', function(){

                            var self = this;

                            if( typeof window.vibeAutoCompleteCheckCombinedFlightsHotelsCities != 'undefined' ){
                                clearTimeout(window.vibeAutoCompleteCheckCombinedFlightsHotelsCities);
                            }

                            window.vibeAutoCompleteCheckCombinedFlightsHotelsCities = setTimeout(function(){

                                var params = {
                                    'input': $(self).val(),
                                    "type": "autosuggest"
                                }

                                if( params.input.length > 0 ){

                                    var url = 'http://' + window.location.hostname + '/wtg_vibe/api';
                                    url += '?' + $.param(params);

                                    buildDropdown_combined_holiday(self, url);

                                } else {

                                    $(self).closest('#vibeSearchForm.combined.flightshotels').find('.dropdown_container').slideUp(function(){
                                        $(this).remove();
                                    });
                                }

                            }, 650);
                        });


                        $('#vibeSearchForm.combined.flightshotels .dropdown_container .suggestion.city').live('click', function(){

                            var form_item = $(this).closest('.formItem');
                            var auto_input = form_item.find('.vibe_auto_complete');
                            var numchildren = $(this).attr('data-numchildren');


                            if( numchildren > 0 ){

                                var params = {
                                    'typed': $('.title .destName', this).text(),
                                    "type": "autosuggest",
                                    "key" :  $(this).attr('data-item-key')
                                }

                                var url = 'http://' + window.location.hostname + '/wtg_vibe/api';
                                url += '?' + $.param(params);
                                buildDropdown_combined_holiday(auto_input, url);

                            } else {

                                auto_input.attr('data-item-key', $(this).attr('data-item-key'));
                                auto_input.val($('.title .destName', this).text());

                                $('#vibeSearchForm.combined.flightshotels .dropdown_container').slideUp(function(){
                                    $(this).remove();
                                });
                            }
                        });


                        $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete.city').live('focus', function(){

                            if( !$('#vibeSearchForm.combined.flightshotels .dropdown_container').length && $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete').val().length > 0 ){

                                var params = {
                                    'input': $(this).val(),
                                    "type": "autosuggest",
                                    "key" : encodeURIComponent($(this).attr('data-item-key'))
                                }

                                if( params.input.length > 0 ){
                                    var url = 'http://' + window.location.hostname + '/wtg_vibe/api';
                                    url += '?' + $.param(params);
                                    buildDropdown_combined_holiday(this, url);
                                }
                            }
                        });


                        $('#vibeSearchForm.combined.flightshotels .dropdown_container .historyBack').live('click', function(){

                            var autoCompleteInput = $('#vibeSearchForm.combined.flightshotels .vibe_auto_complete.city');
                            var itemKey  = $(this).attr('data-item-key');

                            var itemName = autoCompleteInput.val();

                            var params = {
                                'input': itemName,
                                "type": "autosuggest",
                                "key" : encodeURIComponent(itemKey)
                            }

                            if( params.input.length > 0 ){
                                var url = 'http://' + window.location.hostname + '/wtg_vibe/api';
                                url += '?' + $.param(params);
                                buildDropdown_combined_holiday(autoCompleteInput, url);
                            }
                        });

                    });


                    function buildDropdown_combined_holiday(elm, url){

                        $.getJSON(url, function(result){

                            var dropdown = '<div class="dropdown_container" style="display: none; max-height: 200px; overflow: auto !important;">';

                            for( var i = 0; i < result.length; i++ ){
                                var item = result[i];

                                if( item.type == 'parent' ){
                                    dropdown += '<div class="historyBack" style="background:#FFF; color: rgb(62, 62, 62); border-left: 1px solid #aaa; border-right: 1px solid #aaa; cursor: pointer; padding: 4px 0px 4px 7px;" data-item-key="' + item.key + '">&laquo; <span class="placeName">' + item.name + '</span></div>';
                                } else if( item.type == 'loc' ){
                                    dropdown += '<div data-numchildren="' + item.children + '" data-item-key="' + item.key + '" class="suggestion city"><div class="title"><span class="destName">' + item.name + '</span> <span class="hotelcount">(' + (typeof item.hotelcount == 'string' ? item.hotelcount : '1000+') + ' hotels)</span>' + '</div><div class="info">' + (typeof item.parentname != 'undefined' && item.parentname.length > 0 ? item.parentname + ', ' : '') + item.country + '</div></div>';
                                }
                            }
                            dropdown += '</div>';

                            elm.form_item = $(elm).closest('.formItem');

                            // make sure it appends it to just the current search widget
                            $(elm.form_item).find('.dropdown_container').remove();
                            $(dropdown).insertAfter('.vibe_auto_complete', elm.form_item);
                            $(elm.form_item).find('.dropdown_container').slideDown();
                        });
                    }
                </script>

            </div>
            </div>

        </article>

