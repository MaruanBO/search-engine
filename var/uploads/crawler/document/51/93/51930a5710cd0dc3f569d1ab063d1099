    <div>Например: <a id="exampleSearchLink" href="http://www.pravda.ru/news/world/formerussr/ukraine/children/" target="_blank" onclick="return exampleSearch()" title="Дети Донбаcса">Дети Донбасса</a></div>
    <script type="text/javascript">
        function hidePlaceholder() {
            var inp = $('searchString');
            if (inp.value == 'Поиск')
                inp.value = '';            
        }
        function showPlaceholder() {
            var inp = $('searchString');
            if (inp.value == '')
                inp.value = 'Поиск';            
        }
        function exampleSearch() {
            $('searchString').value = $('exampleSearchLink').innerText;
            window.open ($('exampleSearchLink').href, $('exampleSearchLink').title);
            window.focus();
            return false;
        }
    </script>
