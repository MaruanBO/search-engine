                    <script type="text/javascript">
                        function hideLoginHolder() {
                            var inp = $('login');
                            if (inp.value == 'Логин')
                                inp.value = '';            
                        }
                        function showLoginHolder() {
                            var inp = $('login');
                            if (inp.value == '')
                                inp.value = 'Логин';            
                        }
                        function hidePassHolder() {
                            var ph = $('passHolder');
                            ph.innerText = '';       
                        }
                        function showPassHolder() {
                            var inp = $('password');
                            var ph = $('passHolder');
                            if (inp.value == '')
                                ph.innerText = 'Пароль';            
                        }
                    </script>
