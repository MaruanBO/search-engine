<script type="text/javascript">

// Normalize Carousel Heights - pass in Bootstrap Carousel items.
$.fn.carouselHeights = function() {

    var items = $(this), //grab all slides
        heights = [], //create empty array to store height values
        tallest; //create variable to make note of the tallest slide

    var normalizeHeights = function() {

        items.each(function() { //add heights to array
            heights.push($(this).height()); 
        });
        console.log("heights list = " + heights);
        tallest = Math.max.apply(null, heights); //cache largest value
        console.log("tallest = " + tallest);
        
        items.each(function() {           
            $(this).css('min-height',tallest + 'px');
        });

        // Set the carousel's Prev/Next arrow vertical position
        // Top is half of (carousel-inner-height + control-height)
        var ci_height = $("div .carousel-inner").height();
        var control_height = $(".carousel-control").height();
        var top_height = (ci_height + control_height) / 2;
        $("#carousel_cont_left").css('top', '-' + top_height + 'px');
        $("#carousel_cont_right").css('top', '-' + top_height + 'px');
    };

    normalizeHeights();

    $(window).on('resize orientationchange', function () {
        //reset vars
        tallest = 0;
        heights.length = 0;

        items.each(function() {
            $(this).css('min-height','0'); //reset min-height
        }); 
        normalizeHeights(); //run it again 
    });
};

   $(document).ready(function() {
      // Normalize all slide heights 
      $('#hpCarousel .item').carouselHeights();

      // === carousel auto play === //
      $('#hpCarousel').carousel({ 
          interval: 4500,
          pause: "hover"
      });
      $('#hpCarousel').carousel('cycle');
   
      // Show/Hide prev/next on hover
      $('#hpCarousel').hover(
          function(){  // mouseover
              // Show controls only if viewport width is 1130px or more
              if (window.innerWidth >= 1130) {
                  $('#carousel_cont_left').fadeIn();
                  $('#carousel_cont_right').fadeIn();
              }
          },
          function(){ // mouseout
              $('#carousel_cont_left').fadeOut();
              $('#carousel_cont_right').fadeOut();
          }
      );

      // track clickthrough of banners in GA
      trackBannerLink(); 
      // track clickthrough of spotlight items in GA
      trackSpotlightLink();
      // track clickthrough of footer items in GA
      trackFooterLink();

      // equalize promo tile height
      var maxHeight = 0;
    $("div.promoTile_prodinfo").each(function(){
       if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
    });
    $("div.promoTile_prodinfo").height(maxHeight);

  });

  var bannerDivision, bannerDivisionOnClick, spotLinkURL, spotLinkURLOnClick, exploreLink, exploreLinkOnClick, footerLink, footerLinkOnClick
  function trackBannerLink() {
    $("#hpCarousel a").each(function(index) {
        bannerDivision = $(this).attr('rel');
        bannerDivisionOnClick = "_gaq.push(['_trackEvent', 'Homepage Banner', 'Clickthrough', '" + bannerDivision + "']);";
        $(this).attr("onClick", bannerDivisionOnClick);
     });
  }

  function trackSpotlightLink() {
    $(".promoTile a").each(function(index) {
        spotLinkURL = $(this).attr('rel');
        spotLinkURLOnClick = "_gaq.push(['_trackEvent', 'Homepage Spotlight', 'Clickthrough', '" + spotLinkURL + "']);";
        $(this).attr("onClick", spotLinkURLOnClick);
     });

    $("#exploreLinks a").each(function(index) {
        exploreLink = $(this).text();
        exploreLinkOnClick = "_gaq.push(['_trackEvent', 'Homepage Explore Menu', 'Clickthrough', '" + exploreLink + "']);";
        $(this).attr("onClick", exploreLinkOnClick);
     });
  }

  function trackFooterLink() {
    $("#footer a").each(function(index) {
        footerLink = $(this).text();
        footerLinkOnClick = "_gaq.push(['_trackEvent', 'Homepage Footer', 'Clickthrough', '" + footerLink + "']);";
        $(this).attr("onClick", footerLinkOnClick);
     });
  }

</script>   
