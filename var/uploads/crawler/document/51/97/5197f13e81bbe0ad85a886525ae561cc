    </p></div><!-- end listen live--><div id="cmSearchForm" class="cmClearFix"><div class="cmPopularSearches"><div id="queries_014368166379998286206:3-3_8b5telw"></div><script type="text/javascript">
function renderPopularQueries(queries) {
  var maxQueries = 3;
  var total = 0;

  var seen = new Array();

  var google_url = 'www.google.com\x2Fcse';
  var cxri_url = 'www.clarkhoward.com/query/';

  var myDiv = document.getElementById("queries_014368166379998286206:3-3_8b5telw");
  var heading = document.createElement("p");
  heading.appendChild(document.createTextNode("Popular Searches: "));
  myDiv.appendChild(heading);

  qloop:for (var i = 0; i < queries.popularQueries.length; i++) {
    if (total == maxQueries) {
      break qloop;
    }
    var query = queries.popularQueries[i].query;
    for ( var j = 0; j < seen.length; j++ ) {
      var seenstr = seen[j].replace(/\b(free|from|at)\b/gi,'');
      var cmpstr = query.replace(/\b(free|from|at)\b/gi,'');

      if ((cmpstr.search(seenstr) > -1) || (seenstr.search(cmpstr) > -1)) {
        continue qloop;
      }
    }

    var a = document.createElement("a");
    var href = queries.popularQueries[i].href;
    href = href.replace(google_url, cxri_url);
    href = href + '&cof=FORID%3A11';
    a.setAttribute("href", href);
    a.appendChild(document.createTextNode(query));

    myDiv.appendChild(a);
    myDiv.appendChild(document.createTextNode(" "));

    seen.push(query);

    total++;

    if (total <= maxQueries) {
      myDiv.appendChild(document.createTextNode("| "));
    }
  }
  var a = document.createElement("a");
  a.setAttribute("href","http://www.clarkhoward.com/query/top20searches/");
  a.appendChild(document.createTextNode("more"));
  myDiv.appendChild(a);
