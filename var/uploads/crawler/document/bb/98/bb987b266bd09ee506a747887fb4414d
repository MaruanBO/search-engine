<script type="text/javascript">
$(function() {

var switcher = $('.scoreboard-switcher > .switcher');
 
switcher.on("click", function() {
  $(this).toggleClass('active');
  return false;
});

var scroller = $('.scoreboard-slider .slider').scrollable({ circular: false }).autoscroll(6000).data('scrollable');

var api = scroller;

$('.scoreboard-slider .pp').click(function() {
  if ($(this).hasClass('pause')) {

    $(this).removeClass('pause');
    $(this).addClass('play');
    api.stop();

  } else if ($(this).hasClass("play")) {

    $(this).removeClass('play');
    $(this).addClass('pause');
    api.play();

  };
});

var switcherList = $('a[data-fetch-url]', switcher);
switcherList.on("click", function(e) {
  e.preventDefault();
  var el = $(this);
  if (!el.hasClass("active")) {
    var url = $(this).attr("href");
    if (url.length > 1) {
      scroller.getItems().fadeTo(400, 0.33, function() {
        $.get(url, {"tmpl": "/scoreboard-switcher-template", "ajax": true}, function(data) {
          var html = $('<div />').html(data);
          
          var newItems = html.find('.item').fadeTo(0, 0);
          scroller.seekTo(0, 0);
          scroller.getItems().remove();
          scroller.getItemWrap().append(newItems);
          newItems.fadeTo(600, 1);

          switcherList.removeClass('active');
          el.addClass('active');

          $('.scoreboard-title').html(el.html());
          switcher.children('.selection').html(el.html());

        });
      });
    }
  }
});

});
</script>

 

