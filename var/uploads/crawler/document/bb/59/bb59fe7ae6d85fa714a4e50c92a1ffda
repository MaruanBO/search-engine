	$(document).ready(function () {
		var tollTipString='#/';
		//var tollTipString='';
        var separated = tollTipString.split("#/");
        for (var i = 0; i <= separated.length; i++) {
	   if (separated[i] != '') {
                if (i % 2 == 0) {
                    var htmlStr = $('.ContentPanel').html(); 
                    $('.ContentPanel:contains(' + separated[i] + ')').each(function () {
                        $(this).html($(this).html().replace(separated[i], '<label class="splitAnhcr">' + separated[i] + '</label>'));
                    });
                } else {
                    $('.splitAnhcr').each(function () {
                        if (!$(this).children().hasClass("toolTipDiscri")) {
                            $(this).append('<span class="toolTipDiscri"><span class="toolTipDiscriInner">' + separated[i] + '</span></span>');
                        }
                    })
                }
            }
        }
    })
	
	$(window).load( function() {
				$('.splitAnhcr').hover(function () {
					$(this).children().show('fast');
				}, function () {
					$(this).children().hide();
				})
		})
}
</script>

