        $(function () {
            var bannerSpot = 1;
            $('div[data-track="bottomalert"] a').each(function () {
                var datatrack = $(this).data("track");
                if (datatrack) {
                    $(this).attr("data-track", bannerSpot + "|" + datatrack);
                    bannerSpot++;
                }
            });
        });
