    var footer = (function (window) {

        var init = function () {
            var self = this;

            //set current copyright year
            var copyright = document.getElementById("current-copyright");
            copyright.innerText = new Date().getFullYear();

            //replace mcafee links with ebgames ref.
            if (window.location.host.indexOf("ebgames.com") != -1) {
                var lnkSecure = document.getElementById("lnkSecure");
                lnkSecure.setAttribute("href","https://www.mcafeesecure.com/RatingVerify?ref=www.ebgames.com");

