            var subdomain = function(){
                var url = window.location.href.split('?')[0].split('/');
                var subdomain = url[2].split('.')[0];
                return subdomain.indexOf("login") > -1 ? subdomain.replace("login", "") == "" ? "www" : subdomain.replace("login", "") : url[2].split('.')[0];
            }
            var findStore = function(){
                var baseUrl = "http://" + subdomain() + ".gamestop.com/stores/";
                if('False' != 'True'){ 
                    window.location.href = "http://www.gamestop.com/browse/storesearch.aspx";
                } else {
                    window.location = baseUrl;
                }
            };
        </script>
