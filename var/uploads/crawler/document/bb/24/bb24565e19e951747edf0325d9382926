<script>
    $( document ).ready(function() {
        $('#bannerClose').click(function () {
            var url = "http://www.gamestop.com/searchmerch/Header/CloseBanner";
            SetBannerState(url);
        });


        function SetBannerState(url) {
            $.ajax({
                url: url,
                dataType: "jsonp",
                jsonp: "jsonp",
                data: {close: 'true'},
                success: function(data) {
                    $('#bannerToShow').hide();
                }
            });
        }

    });
</script>

