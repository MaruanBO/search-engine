        $(function () {
            $(".made_simple_carousel li").click(function () {
                var ind = $(".made_simple_carousel li").index(this);
                $(".made_simple_carousel li.act_carousel").removeClass("act_carousel");
                $(".video .contentshow").removeClass("contentshow");
                $(".video .contentshow").removeClass("contenthide");
                $(".video #video-" + ind).addClass("contentshow");
                $(this).addClass("act_carousel");
                if ($('#video-0').hasClass('contentshow')) {
                    $("#slides-0").slides("play");
                }
                else {
                    $("#slides-0").slides("slide", 4);
                    $("#slides-0").slides("stop");
                }
                if ($('#video-1').hasClass('contentshow')) {
                    $("#slides-1").slides("play");
                }
                else {
                    $("#slides-1").slides("slide", 4);
                    $("#slides-1").slides("stop");
                }
                if ($('#video-2').hasClass('contentshow')) {
                    $("#slides-2").slides("play");
                }
                else {
                    $("#slides-2").slides("slide", 4);
                    $("#slides-2").slides("stop");
                }
                if ($('#video-3').hasClass('contentshow')) {
                    $("#slides-3").slides("play");
                }
                else {
                    $("#slides-3").slides("slide", 4);

                    $("#slides-3").slides("stop");
                }
            })
