<script language="javascript">
    function ValidateFooterName(sender, args) {

        args.IsValid = false;
          if (args.Value != "" && (/^[A-Za-z]+[-\']*[A-Za-z]*$/i.test(args.Value))) {
            args.IsValid = true;
        }
    }
    function ValidateFooterEmail(sender, args) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        args.IsValid = false;
        if (re.test(args.Value)) {
            args.IsValid = true;
        }

    }
</script>

