<script src="/static/js/placeholder.js" type="text/javascript"></script>
<script>
$(document).ready(function() {
	  var hashVal = window.location.hash.split("#")[1];
    if (hashVal == 'email-success') {
        $(".email-success").show();
		    $(".email-subscription").hide();
		    setTimeout(function(){ 
          window.location.hash = '';
		    },3000);
    }
    else {
				setTimeout(function(){ 
             $(".email-subscription").show();
             $(".sidebar .obamacare-research").show();
        },500);
    }
		 
		//Set e-mail form
		$('input').placeholder();  
		$("#mystique input.subscribe-button").click(function() {      
		  if ($(".sidebar.right input[name='email']").valid() && $(".sidebar.right input[name='postal_code']").valid() == true) { 
				$("input[name='grp']").val("991475");			
				if ($("#mystique .email-subscription p").html().indexOf("Morning Bell") > 0) {
					 $("input[name='grp']").val("991475,662710");	
				}
				$("input[name='redirect_url']").remove();
				$("input[name='grp[]']").remove();
				$('form').get(0).setAttribute('method', 'get');
				$('form').get(0).setAttribute('action', 'http://www.paramountcommunication.com/NewsLetters/Heritage/optin.aspx');	
				$('input:not(.email-subscription input)').remove();
				_gaq.push(['_trackEvent', 'Homepage', 'Email Signup', '']);
				$("#redirect_url").val("http://www.heritage.org/#email-success");		
			}
		}); 
});
  </script>		
