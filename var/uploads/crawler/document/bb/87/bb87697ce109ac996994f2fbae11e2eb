<script>
  if (new String(document.location).indexOf("shop-and-compare") < 0) {
    document.write('<base href="' + document.location + '"/>');
  }

(function() {
  'use strict';
  var unsupportedUrl = '/unsupported-browser',
      isUnsupportedMatch = /MSIE [678]\.\d+/,
      isUnsupported = isUnsupportedMatch.test(navigator.userAgent),
      redirect = function() {
        window.location = unsupportedUrl;
      };

  if (isUnsupported && window.location.pathname !== unsupportedUrl) {
    redirect();
  }
})();

  // Avoid console errors in browsers that lack a console (IE9 fix)
  var method;
  var noop = function () {};
  var methods = [
      'assert', 'clear', 'count', 'debug', 'dir', 'dirxml', 'error',
      'exception', 'group', 'groupCollapsed', 'groupEnd', 'info', 'log',
      'markTimeline', 'profile', 'profileEnd', 'table', 'time', 'timeEnd',
      'timeStamp', 'trace', 'warn'
  ];
  var length = methods.length;
  var console = (window.console = window.console || {});

  while (length--) {
      method = methods[length];

      // Only stub undefined methods.
      if (!console[method]) {
          console[method] = noop;
      }
  }
</script>


