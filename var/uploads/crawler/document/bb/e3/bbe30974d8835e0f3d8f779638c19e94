<script type="text/javascript"> 
	//Check to see if Print Button is already visible 
	if(!$('#printMemberID').is(':visible')){ 
		var printModal = '<div class="clear"></div><div id="printMemberID" style="padding-top:20px;"><a href="/web_assets/account/print/print.html" onClick="var s=s_gi(\'chchicoscomprod\'); s.tl(this,\'o\',\'PM-printloyaltyCard\');" rel="modal 400 300" title="Print Member ID" alt="Print Member ID" class="modalTrigger"><img src="/web_assets/account/print/080812_PP_Button_tlt.jpg" width="145" height="26" alt="Print"></a></div>'; 
		var loyaltyMember_name; 
		var loyaltyMember_id; 

		//Existing Customer Profile Page// 
		var accountProfile = location.toString().indexOf('edit_profile.jsp'); 
		if(accountProfile >= 0){ 
			loyaltyMember_name = $('#my-account .cpc-form #firstName').val()+ ' '+$('#my-account .cpc-form #lastName').val(); 
			loyaltyMember_id = $('#my-account .passport-num #co-editprofile-loyalty .global-floatLeft').text().replace('Passport Number:','').replace(RegExp(/\s/g),''); 
			// console.log('PROFILE PAGE: '+loyaltyMember_name + ' '+loyaltyMember_id); 
			$('#my-account .passport-num').append(printModal); 
		} 

		//New member welcome screen// 
		var accountWelcome = location.toString().indexOf('login_success.jsp?reg=0'); 
		if(accountWelcome >= 0){ 
			loyaltyMember_name = $('#welcome #wrong-user span').eq(0).text(); 
			loyaltyMember_id = $('#welcome #intro strong').text(); 
			//console.log(console.log('PROFILE PAGE: '+loyaltyMember_name + ' '+loyaltyMember_id); 
			$('#welcome #intro').append(printModal);
		} 
	} 

	//Modal JQM Code 
	$('#loyaltyModal').jqm({ 
		ajax: '@href', 
		title: '@title', 
		trigger:'.loyaltyPrintTrigger' 
	}); 
</script> 

