		<script>

	sliderInt=0;
	sliderNext=1;

	start = 0;

	$('#photo1').fadeIn(0);
	count = $('#head_photo img').size();
	startSliderContent();
	startSlider();

	function startSlider(){

		if(start == 0){
				
			loop = setInterval(function(){

				startSliderContent();
			
			},7000); //repeat cycle
			} 
		start = 1;

	} 

	function startSliderContent(){

		if(sliderNext > count){
			sliderNext = 1;
			sliderInt = 1;
		}
		
		$('#head_photo div').fadeOut(0);
		$('#photo'+sliderNext).fadeIn(300);
		if(sliderNext == 1){
			thumb1_on();
		} else if(sliderNext == 2) {
			thumb2_on();
		} else if(sliderNext == 3) {
			thumb3_on();
		}

		sliderInt = sliderNext;
		sliderNext = sliderNext+1;
		if(sliderNext > count) sliderNext = 1;
	
	}

	function prev(){
		newSlide = sliderInt-1;
		showSlide(newSlide);
	}

	function next(){
		newSlide = sliderInt+1;
		showSlide(newSlide);
	}

	function stopLoop(){
		start =0;
		window.clearInterval(loop);
	}

	function showSlide(Id){
		if(Id > count){
				Id = 1;
				sliderInt = 1;
		}else if(Id < 1){
			Id = count;
		}
			
			$('#head_photo div').fadeOut(0);
			$('#photo'+Id).fadeIn(300);
			if(Id == 1){
				thumb1_on();
			} else if(Id == 2) {
				thumb2_on();
			} else if(Id == 3) {
				thumb3_on();
			}


			sliderInt = Id;
			sliderNext = Id+1;
	}

	$('#head_photo_wrap').hover(
		function(){
			stopLoop();
		},function(){
			startSlider();
		}
	);

	function thumb1_on(){
		$('#thumb1').addClass('on');
		$('#thumb2').removeClass('on');
		$('#thumb3').removeClass('on');

		$('#photo1').show();
		$('#photo2').hide();
		$('#photo3').hide();
	}

	function thumb2_on(){
		$('#thumb2').addClass('on');
		$('#thumb1').removeClass('on');
		$('#thumb3').removeClass('on');

		$('#photo1').hide();
		$('#photo2').show();
		$('#photo3').hide();
	
	}

	function thumb3_on(){
		$('#thumb3').addClass('on');
		$('#thumb1').removeClass('on');
		$('#thumb2').removeClass('on');

		$('#photo1').hide();
		$('#photo2').hide();
		$('#photo3').show();
	}
	$('#thumb1').hover(function(){
		thumb1_on();
		sliderInt = 1;
		sliderNext = 2;
	});
	$('#thumb2').hover(function(){
		thumb2_on();
		sliderInt = 2;
		sliderNext = 3;
	});
	$('#thumb3').hover(function(){
		thumb3_on();
		sliderInt = 3;
		sliderNext = 1;
	});

		</script>	
	<!-- /hdline end -->
							
