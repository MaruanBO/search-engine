<script type="text/javascript">
	jQuery(document).ready(function($){
		/*$('#hero_container .slide_container').before('<div class="thumb_container">').cycle({
			fx: 'scrollHorz',
			//fx: 'fade',
			speed: 1000,
			//timeout: 7500,
			timeout: 5000,
			easing: 'easeInOutExpo',
			pager:  '.thumb_container',

			// callback fn that creates a thumbnail to use as pager anchor 
			pagerAnchorBuilder: function(idx, slide) { 
				return '<div class="thumb"><div class="orb"></div></div>'; 
			} 
		});*/

		if ( $(window).outerWidth() < 602 ) { //mobile only
			var psWindow = $('.product_slider');
			var psProduct = $('.product_slider .product');

			(function resizePsProduct() {
				if ( $(window).outerWidth() > $(window).outerHeight() ) { // landscape
					psProduct.width( psWindow.width() / 3 );
				} else { // portrait
					psProduct.width( psWindow.width() / 2 );
				}
			}())

			//resizePsProduct();

			/*window.addEventListener("resize", function() {
				resizePsProduct();
			}, false);*/
		}

		// set constants
		var imgCount = $('.product_slider').find('.product').length;
		var scrollAmount = $('.product_slider').find('.product').width();
		var scrollSpeed = 500;
		var psContainer = $('#product_slider_container');
		psContainer.width( imgCount * scrollAmount ); 

		// initiate slider in anticipation of a previous click
		$('#product_slider_container a:last-child').prependTo( $( "#product_slider_container" ) );
		$('.product_slider').scrollTo( scrollAmount );

		// previous click function
		function prevHandler() { 
			$('.product_slider').stop().scrollTo(0, scrollSpeed, function() { 
				$('#product_slider_container a:last-child').prependTo( $( "#product_slider_container" ) );
				$('.product_slider').scrollTo(scrollAmount);
			});
		}

		// next click function
		function nextHandler() {
			$('.product_slider').stop().scrollTo(scrollAmount*2, scrollSpeed, function() { 
				$('#product_slider_container a:first-child').appendTo( $( "#product_slider_container" ) );
				$('.product_slider').scrollTo(scrollAmount);
			});
		}

		// fire the actions
		$('.product_nav .prev').on( "click", prevHandler );
		$('.product_nav .next').on( "click", nextHandler );

		// slider actions
		$('#product_slider_container').swipe( {
			swipeLeft:function(event, direction, distance, duration, fingerCount) {
				nextHandler();
			},
			swipeRight:function(event, direction, distance, duration, fingerCount) {
				prevHandler();
			},
			threshold:50,excludedElements: ""
		});


	});
</script>

