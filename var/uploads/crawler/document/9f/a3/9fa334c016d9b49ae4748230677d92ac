<script type="text/javascript">
var FluidSurveys;
FluidSurveys = null != FluidSurveys ? FluidSurveys : [], FluidSurveys.load = function(domain, site) {
	var el, last, v;
	return v = Math.random(), el = document.createElement('script'), el.type = 'text/javascript', el.async = !0,
	el.src = '//' + domain + '/media/static/pulse.js?v=' + v, el.className = 'fs-pulse', el.setAttribute('data-domain', domain),
	el.setAttribute('data-site', site), last = document.getElementsByTagName('script')[0], last.parentNode.insertBefore(el, last)
};

FluidSurveys.load('fw.fs.cm', 3);
_fsq.push(function(){
	var FsCallback = function(){
		_ccq.push(function(){
			var properties = {};
			if (FluidSurveys.getProperty('first_referrer') === undefined) {
				properties['first_referrer'] = document.referrer;
			  	properties['get_variables'] = (window.location.search || "?").slice(1);
			  	var utm_vars = _.chain((window.location.search || "?").slice(1).split('&'))
					.map(function(el){ x=el.split('=');return x[1] && [x[0].toLowerCase(), x[1]]})
					.filter(function(x){return !!x})
					.object()
					.pick('utm_campaign','utm_source','utm_medium','utm_term','utm_content','gclid','keyword')
					.value()
				_.extend(properties, utm_vars);
			}
			if (FluidSurveys.getProperty('country_code') === undefined) {
				properties['country_code'] = COUNTRY_CODE;
			}
			if (!_.isEmpty(properties)) { FluidSurveys.setProperties(properties); }


            if (FluidSurveys.getProperty('get_variables').match(/gclid\=/)) {
                if (COUNTRY_CODE === 'US') {
                    $('.phone-usa-adwords').show();
                    $('.phone-world-adwords').hide();
                    $('.phone-world').hide();
                } else {
                    $('.phone-usa-adwords').hide();
                    $('.phone-world-adwords').show();
                    $('.phone-world').hide();
                }
            } else {
                $('.phone-usa-adwords').hide();
                $('.phone-world-adwords').hide();
                $('.phone-world').show();
            }
		});
	};
	if (USERNAME) {
		FluidSurveys.push(['start', USERNAME, _.extend({}, {"email": EMAIL}, {"plan":PLAN_NAME || undefined, 'subscription_unit': PLAN_TYPE || undefined}), FsCallback]);
	} else {
		FluidSurveys.push(['start', null, {}, FsCallback]);
	}
});
</script>

