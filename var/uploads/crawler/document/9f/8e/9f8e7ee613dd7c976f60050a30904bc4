<script type="text/javascript" language="javascript">

    $('.youthValidationGroup').change(function () {
        var valueSelected = $('select.youthValidationGroup option:selected').val();
        $("#YoungHiddenInput").val(valueSelected);
    });

    $('.adultValidationGroup').change(function () {
        var valueSelected = $('select.adultValidationGroup option:selected').val();
        $("#AdultHiddenInput").val(valueSelected);
    });

    $(document).ready(function () {

//        searchType = "BrandSearch";
//        ShowBookingForm(searchType);


        function getQueryVariable(variable) {
            var query = window.location.search.substring(1).toLowerCase();
            var vars = query.split("&");
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == variable) {
                    return pair[1];
                }
            }

            return "brandsearch";

        }

        var param1var = getQueryVariable("offertype");
        param1var = param1var.replace(/\s+/g, '');
        param1var = param1var.replace("%20", '');
        if (param1var.toLowerCase() != "")
            searchType = param1var.toLowerCase();
        else
            searchType = "BrandSearch";
        //console.log("validator function " + searchType);
        ShowBookingForm(searchType);
    });
</script>



