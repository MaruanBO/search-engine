		<script>

		function change_forum_tab(tab,date_range,page)
		{
			var pars = 'forum_tab='+tab+'&date_range='+date_range+'&page='+page;
			//_gaq.push(['_trackEvent', 'forums_panel', tab, page]);
			ga("send", "event", "streams", "forums", tab, page);
			new Ajax.Updater('forum_posts_panel','/ajax/forum_tab_new2',{parameters:pars,evalScripts:true});
			
			return false;
		}

		</script>
