<script>
	jQuery("#menu .menuButton").hover( function () {
		jQuery("#menu .menuButton.oot").addClass("getToDaChoppa");
		jQuery(this).addClass("oot");
	}, function () {
		var that = jQuery(this);
		jQuery("#menu .menuButton.getToDaChoppa").removeClass("getToDaChoppa");
		setTimeout(function () {
			jQuery(that).removeClass("oot");
		}, 200);
	});
</script>	</div>
