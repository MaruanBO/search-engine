#menu .menuButton.getToDaChoppa {
	 -webkit-transition: opacity 0s, top 0s !important;
    	-moz-transition: opacity 0s, top 0s !important;
    	 -ms-transition: opacity 0s, top 0s !important;
    	  -o-transition: opacity 0s, top 0s !important;
