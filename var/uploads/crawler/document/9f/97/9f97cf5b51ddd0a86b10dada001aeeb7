<script type="text/javascript">
$(document).ready(function() {
        
        var urlStr = window.location.href;
        var urlParams = "";
        var querySepPosition = urlStr.indexOf("?");
        
        if(querySepPosition !== -1){
               urlParams = urlStr.substring(querySepPosition+1, urlStr.length);
        }
        
        var iframeSrc = "https://host1.medcohealth.com/consumer/site/ulpLogin";
        
        if(urlParams.length > 0) {
               
               if(iframeSrc.indexOf("?") == -1) {
                       iframeSrc += "?"+urlParams;
               } else {
                       iframeSrc += "&"+urlParams;
               }
        }
        
        $("#loginwidget").attr("src", iframeSrc);
        
});
</script>
    
