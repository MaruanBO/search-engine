	<script type="text/javascript">
		// *********** LAYOUT SCRIPTS ***********
		
		// Determine layout elements to display
		function selectLayout() {
		
			// Grab info from URL and store it in a hidden field
			var locate = window.location
			document.getElementById('layoutID').value = locate
			
			// Strip out all the text before the "=" sign
			var val = document.getElementById('layoutID').value
			function delineate(str) {
				theleft = str.lastIndexOf("=") + 1;
				return(str.substring(theleft));
			}
			
			// Use the value of the string to determine layout display
			var layoutRef = delineate(val);
			
			// ***** MODAL: WFF Warning *****
			if (layoutRef == 'message01') {
				showModal('message01',550);
			}
			if (layoutRef == 'message02') {
				showModal('message02',550);
			}
			
		}
		
		function closeModalIE6(id) {
			// IE6 - shows select boxes under modal window
			$('#search_tab1 select').show();
			$('#search_tab2 select').show();
			$('#search_tab3 select').show();
			 
			closeModal(); 
			return false;
		}
		
		// Onload scripts
		$(document).ready(function() {
			
			// Load custom layouts
			selectLayout();
			
		});
  // Resize iframe to full height
  function resizeIframe(height)
  {
  //  document.getElementById('loginwidget').height = parseInt(height);
  }
  </script>
