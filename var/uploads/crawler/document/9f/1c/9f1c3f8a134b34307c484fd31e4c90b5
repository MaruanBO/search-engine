    $(document).ready(function () {
        $("#miniCartLink").live("click", function () {
            $("#miniCartPopup").fadeIn('fast');
        });
        $("#divClose").live("click", function () {
            $("#miniCartPopup").fadeOut('fast');
        });
        $("#spnContinueShopping").live("click", function () {
            $("#miniCartPopup").fadeOut('fast');
        });
        $("btnAdd").live("click", function () {
            $("#miniCartPopup").fadeIn('fast');
        });
        $("#miniCart").bind('clickoutside', function () {
            $("#miniCartPopup").fadeOut('fast');
        });

    });
    
    function popMiniCart() {
        $("#miniCartPopup").fadeIn('fast');
    }
</script>

  



