<script language="javascript" type="text/javascript">


    $(document).ready(function () {
        //        $('.megaDDL_tab').live('mouseover', function () {
        //            $(this).find('div.megaDDL').css('display', 'block');
        //            $(this).addClass('hover');
        //            $(this).find('a.megaDDL_tab_link').addClass('hover');
        //        }).live('mouseout', function () {
        //            $(this).find('div.megaDDL').css('display', 'none');6
        //            $(this).removeClass('hover');
        //            $(this).find('a.megaDDL_tab_link').removeClass('hover');
        //        });
        $('.megaDDL_tab').live('mouseover', function () {
            $(this).find('a.megaDDL_tab_link').addClass('hover');
        }).live('mouseout', function () {
            $(this).find('a.megaDDL_tab_link').removeClass('hover');
        });

        function addMega() {
            $(this).find('div.megaDDL').css('display', 'block');
            $(this).addClass('hover');
            //$(this).find('a.megaDDL_tab_link').addClass('hover');

        }

        function removeMega() {
            $(this).find('div.megaDDL').css('display', 'none');
            $(this).removeClass('hover');
            //$(this).find('a.megaDDL_tab_link').removeClass('hover');
        }

        var megaConfig = {
            //interval: 500,
            //sensitivity: 4,
            over: addMega,
            //timeout: 500,
            out: removeMega
        };

        $("li.megaDDL_tab").hoverIntent(megaConfig)


    });
</script>
