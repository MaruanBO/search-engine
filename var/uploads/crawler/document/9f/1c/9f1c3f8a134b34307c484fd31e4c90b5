    $(document).ready(function() {
        $(".tabbedItemCategories > li").click(function(event) {
            event.preventDefault();
            $(".selected").removeClass("selected");
            $(this).addClass("selected");
            $("#tabbedItemContent ul.selected").removeClass("selected");
            $("#tabbedItemContent ul:eq(" + $(this).attr("id") + ")").addClass("selected");
        });
        $(".tabbedItemCategories li:first").click();
    });
</script>

