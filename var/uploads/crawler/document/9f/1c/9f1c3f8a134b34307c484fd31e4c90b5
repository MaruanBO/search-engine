<script type="text/javascript">
    setTimeout("if(false){centerPopup('Welcome');showPopup('Welcome');}", 1500);
    var fpEmail = "/password-recovery.aspx?email=";
    var orderID = 0;
    var taxExemptPopupStatus = 0;
    var duplicateEmailPopupStatus = 0;
    var govSchoolPOPopupStatus = 0;
    var quoteInfoPopupStatus = 0;
    var welcomeStatus = 0;
    var purchaseOrderPopupStatus = 0;
    var wireTransferPopupStatus = 0;
    var emailQuoteStatus = 0;
    var PayPalAlreadyExistsPopupStatus = 0;
    var PayPalPopupStatus = 0;
    var PleaseWaitStatus = 0;
    var ForeignVisitorStatus = 0;
    var OutOfStateVisitorStatus = 0;
    function hidePopup(popupName) {
        //disables popup only if it is enabled 
        var status = 0;
        var blockName = "#" + popupName + "Popup";
        switch (popupName) {
            case "GovSchoolPO":
                status = govSchoolPOPopupStatus;
                break;
            case "QuoteInfo":
                status = quoteInfoPopupStatus;
                break;
            case "Welcome":
                status = welcomeStatus;
                break;
            case "TaxExempt":
                status = taxExemptPopupStatus;
                break;
            case "DuplicateEmail":
                status = duplicateEmailPopupStatus;
                break;
            case "PurchaseOrder":
                status = purchaseOrderPopupStatus;
                break;
            case "WireTransfer":
                status = wireTransferPopupStatus;
                break;
            case "EmailQuote":
                status = emailQuoteStatus;
                break;
            case "PayPalAlreadyExists":
                status = PayPalAlreadyExistsPopupStatus;
                break;
            case "ContactPaypal":
                status = PayPalPopupStatus;
                break;
            case "PleaseWait":
                status = PleaseWaitStatus;
                break;
            case "ForeignVisitor":
                status = ForeignVisitorStatus;
                break;
            case "OutOfStateVisitor":
                status = OutOfStateVisitorStatus;
                break;
        }
        if (status == 1) {
            $("#LightboxPopupBackground").fadeOut("slow");
            $(blockName).fadeOut("slow");
            switch (popupName) {
                case "GovSchoolPO":
                    govSchoolPOPopupStatus = 0;
                    break;
                case "QuoteInfo":
                    quoteInfoPopupStatus = 0;
                    break;
                case "Welcome":
                    welcomeStatus = 0;
                    break;
                case "TaxExempt":
                    taxExemptPopupStatus = 0;
                    break;
                case "DuplicateEmail":
                    duplicateEmailPopupStatus = 0;
                    break;
                case "PurchaseOrder":
                    purchaseOrderPopupStatus = 0;
                    break;
                case "WireTransfer":
                    wireTransferPopupStatus = 0;
                    break;
                case "EmailQuote":
                    emailQuoteStatus = 0;
                    break;
                case "PayPalAlreadyExists":
                    PayPalAlreadyExistsPopupStatus = 0;
                    break;
                case "ContactPaypal":
                    PayPalPopupStatus = 0;
                    break;
                case "PleaseWait":
                    PleaseWaitStatus = 0;
                    break;
                case "ForeignVisitor":
                    ForeignVisitorStatus = 0;
                    break;
                case "OutOfStateVisitor":
                    OutOfStateVisitorStatus = 0;
                    break;
            }
        }
    };

    function showPopup(popupName) {
        //loads popup only if it is disabled 
        var status = 0;
        var blockName = "#" + popupName + "Popup";
        switch (popupName) {
            case "GovSchoolPO":
                status = govSchoolPOPopupStatus;
                break;
            case "QuoteInfo":
                status = quoteInfoPopupStatus;
                break;
            case "Welcome":
                status = welcomeStatus;
                break;
            case "TaxExempt":
                status = taxExemptPopupStatus;
                break;
            case "DuplicateEmail":
                status = duplicateEmailPopupStatus;
                break;
            case "PurchaseOrder":
                status = purchaseOrderPopupStatus;
                break;
            case "WireTransfer":
                status = wireTransferPopupStatus;
                break;
            case "EmailQuote":
                status = emailQuoteStatus;
                break;
            case "PayPalAlreadyExists":
                status = PayPalAlreadyExistsPopupStatus;
                break;
            case "ContactPaypal":
                status = PayPalPopupStatus;
                break;
            case "PleaseWait":
                status = PleaseWaitStatus;
                break;
            case "ForeignVisitor":
                status = ForeignVisitorStatus;
                break;
            case "OutOfStateVisitor":
                status = OutOfStateVisitorStatus;
                break;

        }
        if (status == 0) {
            $("#LightboxPopupBackground").css({ "opacity": "0.7" });
            $("#LightboxPopupBackground").css({ "zIndex": "19000" });
            $("#LightboxPopupBackground").fadeIn("slow");
            $(blockName).fadeIn("slow");
            $(blockName).css({ "zIndex": "20000" });
            switch (popupName) {
                case "GovSchoolPO":
                    govSchoolPOPopupStatus = 1;
                    break;
                case "QuoteInfo":
                    quoteInfoPopupStatus = 1;
                    break;
                case "Welcome":
                    welcomeStatus = 1;
                    break;
                case "TaxExempt":
                    taxExemptPopupStatus = 1;
                    break;
                case "DuplicateEmail":
                    duplicateEmailPopupStatus = 1;
                    break;
                case "PurchaseOrder":
                    purchaseOrderPopupStatus = 1;
                    break;
                case "WireTransfer":
                    wireTransferPopupStatus = 1;
                    break;
                case "EmailQuote":
                    emailQuoteStatus = 1;
                    break;
                case "PayPalAlreadyExists":
                    PayPalAlreadyExistsPopupStatus = 1;
                    break;
                case "ContactPaypal":
                    PayPalPopupStatus = 1;
                    break;
                case "PleaseWait":
                    PleaseWaitStatus = 1;
                    break;
                case "ForeignVisitor":
                    ForeignVisitorStatus = 1;
                    break;
                case "OutOfStateVisitor":
                    OutOfStateVisitorStatus = 1;
                    break;
            }
        }

    };

    function centerPopup(popupName) {
        var blockName = "#" + popupName + "Popup";
        //request data for centering  
        var windowWidth = document.documentElement.clientWidth;
        var windowHeight = document.documentElement.clientHeight;
        var popupHeight = $(blockName).height();
        var popupWidth = $(blockName).width();
        var top = (windowHeight / 2) - (popupHeight / 2);
        var left = (windowWidth / 2) - (popupWidth / 2);

        //centering  
        $(blockName).css({
            "position": "fixed",
            "top": top,
            "left": left
        });

        //only need force for IE6 
        $("#LightboxPopupBackground").css({
            "height": windowHeight
        });
    };
    //Press Escape event!  
    $(document).keypress(function (e) {
        if (e.keyCode == 27 && govSchoolPOPopupStatus == 1) {
            hidePopup("GovSchoolPO");
        }
        if (e.keyCode == 27 && quoteInfoPopupStatus == 1) {
            hidePopup("QuoteInfo");
        }
        if (e.keyCode == 27 && welcomeStatus == 1) {
            hidePopup("Welcome");
        }
        if (e.keyCode == 27 && taxExemptPopupStatus == 1) {
            hidePopup("TaxExempt");
        }
        if (e.keyCode == 27 && duplicateEmailPopupStatus == 1) {
            hidePopup("DuplicateEmail");
        }
        if (e.keyCode == 27 && purchaseOrderPopupStatus == 1) {
            hidePopup("PurchaseOrder");
        }
        if (e.keyCode == 27 && wireTransferPopupStatus == 1) {
            hidePopup("WireTransfer");
        }
        if (e.keyCode == 27 && emailQuoteStatus == 1) {
            hidePopup("EmailQuote");
        }
        if (e.keyCode == 27 && emailQuoteStatus == 1) {
            hidePopup("PayPalAlreadyExists");
        }
        if (e.keyCode == 27 && emailQuoteStatus == 1) {
            hidePopup("ContactPaypal");
        }
        if (e.keyCode == 27 && ForeignVisitorStatus == 1) {
            hidePopup("ForeignVisitor");
        }
        if (e.keyCode == 27 && OutOfStateVisitorStatus == 1) {
            hidePopup("OutOfStateVisitor");
        }

    });
    //Click out event!  
    $("#LightboxPopupBackground").live("click", function () {
        hidePopup("GovSchoolPO");
        hidePopup("QuoteInfo");
        hidePopup("Welcome");
        hidePopup("TaxExempt");
        hidePopup("DuplicateEmail");
        hidePopup("PurchaseOrder");
        hidePopup("WireTransfer");
        hidePopup("EmailQuote");
        hidePopup("PayPalAlreadyExists");
        hidePopup("ForeignVisitor");
        hidePopup("OutOfStateVisitor");
    });
    //Click the x event!  
    $("#GovSchoolPOPopupClose").live("click", function () {
        hidePopup("GovSchoolPO");
    });
    $("#QuoteInfoPopupClose").live("click", function () {
        hidePopup("QuoteInfo");
    });
    $("#WelcomePopupClose").live("click", function () {
        hidePopup("Welcome");
    });
    $("#TaxExemptPopupClose").live("click", function () {
        hidePopup("TaxExempt");
    });
    $("#DuplicateEmailPopupClose").live("click", function () {
        hidePopup("DuplicateEmail");
        window.parent['focusPassword']();
    });
    $("#DuplicateEmail2PopupClose").live("click", function () {
        hidePopup("DuplicateEmail");
        window.parent['focusPassword']();
    });
    $("#PurchaseOrderPopupClose").live("click", function () {
        hidePopup("PurchaseOrder");
    });
    $("#WireTransferPopupClose").live("click", function () {
        hidePopup("WireTransfer");
    });
    $("#EmailQuotePopupClose").live("click", function () {
        hidePopup("EmailQuote");
    });
    $("#PayPalAlreadyExistsPopupClose").live("click", function () {
        hidePopup("PayPalAlreadyExists");
    });
    $("#ForeignVisitorPopupClose").live("click", function () {
        hidePopup("ForeignVisitor");
    });
    $("#OutOfStateVisitorPopupClose").live("click", function () {
        hidePopup("OutOfStateVisitor");
    });
