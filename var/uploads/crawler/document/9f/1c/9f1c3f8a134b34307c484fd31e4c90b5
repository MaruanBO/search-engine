<link href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript">
    $(function () {
        $(".tb").autocomplete({
            
            
            source: function (request, response) {
                $.ajax({
                    url: "/usercontrols/search/SearchTermList.asmx/FetchSearchTermList",
                    data: "{ 'st': '" + request.term + "', 'src': '" + "search" + "'}",
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataFilter: function (data) { return data; },

                    success: function (data) {
                        response($.map(data.d, function (item) {
                            return {
                                value: item.SearchTermPrefix
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                      //alert(textStatus);
                    }
                });
            },
            minLength: 2,

            select: function (event, ui) {
                $('#ctl00_ctl00_ctl00_ctl00_ContentPlaceHolderDefault_SearchSuggest_7_tbAuto').val(ui.item.value);               
                $("[id*=btnSubmit]").click();
                //alert(ui.item.value);
            }

            
        });
    });
	</script>

