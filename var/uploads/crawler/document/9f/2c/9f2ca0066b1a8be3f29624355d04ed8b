        $('.selectairport').defValue();

        $('.selectdate').click(function () {
            if ($(this).val() == 'mm/dd/yyyy') {
                $(this).val('');
            }
        });

        $('#dest').blur(function () {
            $('#destination').val($('#dest').val());
            $('#location').val($('#dest').val());
        });

        $('#destination').blur(function () {
            $('#dest').val($('#destination').val());
            $('#location').val($('#destination').val());
        });

        $('#location').blur(function () {
            $('#destination').val($('#location').val());
            $('#dest').val($('#location').val());
        });





    });

    jQuery.fn.defValue = function () {
        return this.each(function () {
            $(this).click(function () {
                this.defaultValue = $(this).val();
                $(this).val('');
            }).blur(function () {
                var defVal = this.defaultValue;
                if ($(this).val() == '') {
                    $(this).val(defVal);
                }
            });
        });
    }


    function typeextra(seltab, extra) {

        $tabs.tabs("option", "active", seltab);  //.tabs('select', seltab);
        $('#' + extra).click();

    }

    function buildsearch(section) {

        var url = "/results.aspx?"

        var cid = $('#cid').val();
        var type = $('input[name="type"]:checked').val();
        var orig = $('#orig').val();
        var dest = $('#dest').val();
        var departdate = $('#departdate').val();
        var returndate = $('#returndate').val();
        var travelers = $('#travelers').val();
        var destination = $('#destination').val();
        var checkin = $('#checkin').val();
        var checkout = $('#checkout').val();
        var location = $('#location').val();
        var pickup = $('#pickup').val();
        var dropoff = $('#dropoff').val();
        var sectioncode;

        if (section == 0) {
            sectioncode = $('input[name="flightsectioncode"]:checked').val();
        } else if (section == 1) {
            sectioncode = $('input[name="hotelsectioncode"]:checked').val();
        } else if (section == 2) {
            sectioncode = $('input[name="carsectioncode"]:checked').val();
        }

        $.cookie('cid', cid);
        $.cookie('type', type);
        $.cookie('orig', orig);
        $.cookie('dest', dest);
        $.cookie('departdate', departdate);
        $.cookie('returndate', returndate);
        $.cookie('travelers', travelers);
        $.cookie('section', section);
        $.cookie('sectioncode', sectioncode);
        $.cookie('destination', destination);
        $.cookie('checkin', checkin);
        $.cookie('checkout', checkout);
        $.cookie('location', location);
        $.cookie('pickup', pickup);
        $.cookie('dropoff', dropoff);

        url += "cid=" + cid;
        url += "&sectioncode=" + sectioncode;

        if (section == 0) {
            url += "&type=" + type;
            url += "&orig=" + orig;
            url += "&dest=" + dest;
            url += "&departdate=" + departdate;
            if (type == 2) {
                url += "&returndate=" + returndate;
            }
            url += "&travelers=" + travelers;
        } else if (section == 1) {
            url += "&dest=" + destination;
            url += "&departdate=" + checkin;
            url += "&returndate=" + checkout;
        } else if (section == 2) {
            url += "&dest=" + location;
            url += "&departdate=" + pickup;
            url += "&returndate=" + dropoff;
        }

        return url;

    }

    jQuery.validator.addMethod("notCityMask", function (value, element, param) {
        return this.optional(element) || value != param;
    }, "Please specify a city or airport code.");

    jQuery.validator.addMethod("notDateMask", function (value, element, param) {
        return this.optional(element) || value != param;
    }, "Please select a date.");

    jQuery.validator.addMethod("otherDate", function (value, element, param) {
        return Date.parse(param) <= Date.parse(value);
    }, "The second date should be greater than or equal to the first date.");

</script>
</div>
    
</div>



