            $('#cid').val($.cookie('cid'));
        } else {
            $('#cid').val('25eg135');
        }

        if ($.QueryStringExist({ ID: 'type' })) {
            var set = $.getQueryString({ ID: 'type' })
            if (set == "1") {
                $('#oneway').click();
            }
            $.cookie('type', set)
        } else if ($.cookie('type') != undefined) {
            var set = $.cookie('type')
            if (set == "1") {
                $('#oneway').click();
            }
        }

        if ($.QueryStringExist({ ID: 'orig' })) {
