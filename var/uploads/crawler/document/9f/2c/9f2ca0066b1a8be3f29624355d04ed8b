        $("#searchflights").click(function () {

            $("#aspnetForm").validate({
                debug: true,
                rules: {
                    orig: {
                        required: true,
                        minlength: 3,
                        notCityMask: "Enter a city or airport code..."
                    },
                    dest: {
                        required: true,
                        minlength: 3,
                        notCityMask: "Enter a city or airport code..."
                    },
                    departdate: {
                        required: true,
                        minlength: 3,
                        notDateMask: "mm/dd/yyyy"
                    },
                    returndate: {
                        required: true,
                        minlength: 3,
                        notDateMask: "mm/dd/yyyy",
                        otherDate: $('#departdate').val()
                    }
                },
                messages: {
                    orig: {
                        required: "Please enter the city you are traveling from.",
                        minlength: "A three letter city code is the minimum requirement."
                    },
                    dest: {
                        required: "Please enter the city you are traveling to.",
                        minlength: "A three letter city code is the minimum requirement."
                    },
                    departdate: {
                        required: "Please select a depart date.",
                        minlength: "Please select a valid date."
                    },
                    returndate: {
                        required: "Please select a return date.",
                        minlength: "Please select a valid date."
                    }
                },
                submitHandler: function () {
                    window.location = buildsearch(0);
                },
                errorContainer: $('#flightsval'),
                errorLabelContainer: $("ol", $('#flightsval')),
                wrapper: 'li'

            });

        });







        $("#searchhotels").click(function () {

