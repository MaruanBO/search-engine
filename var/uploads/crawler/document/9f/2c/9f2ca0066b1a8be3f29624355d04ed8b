        $('#wizads').height($('#searchtabs').height());
        $('#wizadsinner').height($('#searchtabs').height() - 36);



        //$('.selectdate').datepicker({ showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365 });

        $("#departdate").datepicker({
            showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365,
            onSelect: function (selectedDate) {
                $("#returndate").datepicker("option", "minDate", selectedDate);
                $('#checkin').val(selectedDate);
                $('#pickup').val(selectedDate);
            }
        });
        $("#returndate").datepicker({
            showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365,
            onSelect: function (selectedDate) {
                $("#departdate").datepicker("option", "maxDate", selectedDate);
                $('#checkout').val(selectedDate);
                $('#dropoff').val(selectedDate);
            }
        });


        $("#checkin").datepicker({
            showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365,
            onSelect: function (selectedDate) {
                $("#checkout").datepicker("option", "minDate", selectedDate);
                $('#departdate').val(selectedDate);
                $('#pickup').val(selectedDate);
            }
        });
        $("#checkout").datepicker({
            showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365,
            onSelect: function (selectedDate) {
                $("#checkin").datepicker("option", "maxDate", selectedDate);
                $('#returndate').val(selectedDate);
                $('#dropoff').val(selectedDate);
            }
        });


        $("#pickup").datepicker({
            showOn: 'both', buttonImageOnly: true, buttonImage: '/themes/clouds/images/cal.gif', minDate: 1, maxDate: 365,
            onSelect: function (selectedDate) {
                $("#dropoff").datepicker("option", "minDate", selectedDate);
                $('#departdate').val(selectedDate);
                $('#checkin').val(selectedDate);
            }
        });
        $("#dropoff").datepicker({
