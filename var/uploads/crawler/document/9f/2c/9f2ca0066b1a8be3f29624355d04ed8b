            var set = $.getQueryString({ ID: 'section' })
            if (set == "1") {
                $tabs.tabs("option", "active", 1);  //.tabs('select', 1);
            } else if (set == "2") {
                $tabs.tabs("option", "active", 2);  //.tabs('select', 2);
            }
            $.cookie('section', set)
        } else if ($.cookie('section') != undefined) {
            var set = $.getQueryString({ ID: 'section' })
            if (set == "1") {
                $tabs.tabs("option", "active", 1);  //.tabs('select', 1);
            } else if (set == "2") {
                $tabs.tabs("option", "active", 2);  //.tabs('select', 2);
            }
        }

        if ($.QueryStringExist({ ID: 'sectioncode' })) {
            var set = $.getQueryString({ ID: 'sectioncode' })
            if (set == "1" || set == "14") {
                $tabs.tabs("option", "active", 1);  //.tabs('select', 1);
            } else if (set == "2") {
                $tabs.tabs("option", "active", 1);  //.tabs('select', 2);
