    <script type="text/javascript">

numDots = 6;
itemWidth = 256;
currentPage = 0;
autoPagingInterval = 0;


welcomeHeroContent = {};
welcomeBgHero = {};
welcomeBgSearch = {};
welcomeBgSearchOffsetTop = 0;
welcomeBgDevices = {};
welcomeBgDevicesOffsetTop = 0;
welcomeBgSupport = {};
welcomeBgSupportOffsetTop = 0;
welcomeBgCreators = {};
welcomeBgCreatorsOffsetTop = 0;

isTopNavOpen = true;
isScrollingUp = false;
bodyScrollTop = 0;
isTicking = false;
getOffsetTop = function(elem) {
	var offsetTop = 0;
	do {
		if (!isNaN(elem.offsetTop)) {
			offsetTop += elem.offsetTop;
		}
	} while (elem = elem.offsetParent);
	return offsetTop;
}

updateWelcomeBgOffsets = function() {
		welcomeBgSearchOffsetTop = getOffsetTop(welcomeBgSearch);
		welcomeBgDevicesOffsetTop = getOffsetTop(welcomeBgDevices);
		welcomeBgSupportOffsetTop = getOffsetTop(welcomeBgSupport);
		welcomeBgCreatorsOffsetTop = getOffsetTop(welcomeBgCreators);
}

onWindowScroll = function(e) {

	//console.log('onWindowScroll')

	if(!isTicking) {
		requestAnimationFrame(parallaxUpdate);
	}
	isTicking = true;
	
	//console.log(getOffsetTop(document.getElementById('search')));
	//console.log(getOffsetTop(document.getElementById('devices')));
	//console.log(getOffsetTop(document.getElementById('support')));
	//console.log(getOffsetTop(document.getElementById('creators')));

	isScrollingUp = false;
	if(bodyScrollTop > document.body.scrollTop) {isScrollingUp = true;}
	var bodyScroll = document.documentElement.scrollTop||document.body.scrollTop;
	
	// Scrolling down a bit, already showing: hide it
	if(bodyScroll > 20 && isTopNavOpen && !isScrollingUp) {
		document.getElementById('topnav').classList.add('hide-fixed');
		document.getElementById('topnav').classList.remove('fade-in');
		isTopNavOpen = false;				
	}
	
	// Scrolling up, not showing, below search: show it
	if(!isTopNavOpen && isScrollingUp && bodyScroll > getOffsetTop(document.getElementById('search'))) {
		document.getElementById('topnav').classList.remove('hide-fixed');
		document.getElementById('topnav').classList.add('show-fixed');
		isTopNavOpen = true;				
	}
	
	// Scrolling up, already showing, above search: hide it
	if(isTopNavOpen && isScrollingUp && bodyScroll < getOffsetTop(document.getElementById('search'))) {
		document.getElementById('topnav').classList.add('hide-fixed');
		document.getElementById('topnav').classList.remove('show-fixed');
		isTopNavOpen = false;				
	}
	
	// Near the top, not showing, show it
	if(bodyScroll < 5 && !isTopNavOpen) {
		document.getElementById('topnav').classList.remove('fade-out');		
		document.getElementById('topnav').classList.remove('hide-fixed');
		document.getElementById('topnav').classList.add('fade-in');		
		isTopNavOpen = true;				
	}
	
	// Bg colors
	if(bodyScroll > getOffsetTop(document.getElementById('search'))) {
		document.getElementById('topnav').classList.add('yellow');
	} else {
		document.getElementById('topnav').classList.remove('yellow');
	}
	if(bodyScroll > getOffsetTop(document.getElementById('devices'))) {
		document.getElementById('topnav').classList.add('blue');
	}else {
		document.getElementById('topnav').classList.remove('blue');
	}
	if(bodyScroll > getOffsetTop(document.getElementById('support'))) {
		document.getElementById('topnav').classList.add('red');
	} else {
		document.getElementById('topnav').classList.remove('red');
	}
	if(bodyScroll > getOffsetTop(document.getElementById('creators'))) {
		document.getElementById('topnav').classList.add('purple');
	} else {
		document.getElementById('topnav').classList.remove('purple');
	}
	
	bodyScrollTop = bodyScroll;
	
	//console.log("bodyScrollTop " + bodyScrollTop);
}

parallaxUpdate = function() {
	console.log("parallaxUpdate");
	
	isTicking = false;
	var currentScrollY = bodyScrollTop;
	var rate = .3;
	
	welcomeHeroContent.style.opacity = 1 - (bodyScrollTop/400);
	
	by = -currentScrollY * rate;
	welcomeBgHero.style.webkitTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgHero.style.MozTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgHero.style.msTransform = 'translate3d(0,' + (by) + 'px,0px)';

	by = (currentScrollY-welcomeBgSearchOffsetTop) * rate;
	welcomeBgSearch.style.webkitTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgSearch.style.MozTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgSearch.style.msTransform = 'translate3d(0,' + (by) + 'px,0px)';

	by = (currentScrollY-welcomeBgDevicesOffsetTop) * rate;
	welcomeBgDevices.style.webkitTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgDevices.style.MozTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgDevices.style.msTransform = 'translate3d(0,' + (by) + 'px,0px)';
	
	by = (currentScrollY-welcomeBgSupportOffsetTop) * rate;
	welcomeBgSupport.style.webkitTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgSupport.style.MozTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgDevices.style.msTransform = 'translate3d(0,' + (by) + 'px,0px)';
	
	by = (currentScrollY-welcomeBgCreatorsOffsetTop) * rate;
	welcomeBgCreators.style.webkitTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgCreators.style.MozTransform = 'translate3d(0,' + (by) + 'px,0px)';
	welcomeBgCreators.style.msTransform = 'translate3d(0,' + (by) + 'px,0px)';
}


updateFeaturedScroller = function() {
	
	var rect = document.body.getBoundingClientRect();
	var w = rect.right - rect.left;
	var pagerDots = $('#featured .pager-dots');
	var dotHtml = "<a class='dot'><span></span></a>";
	var dotSelectedHtml = "<a class='dot selected'><span></span></a>";
	
	
	if (w > 300) {
		numDots = 6;
	}

	if (w > 760) {	
		numDots = 4;
	}
	
	pagerDots.empty();
	for (var i = 0; i < numDots; i++) {
		if (i == currentPage) {
			pagerDots.append(dotSelectedHtml);
		} else {
			pagerDots.append(dotHtml);
		}
	}
	
	if (w >= 640) {
		itemWidth = 320;
	} else {
		itemWidth = 256;		
	}
	showFeaturedPage(currentPage);
}

onWelcomeResize = function(e) {
	updateWelcomeBgOffsets();
	//updateFeaturedScroller();
}

autoForwardPaging = function(i) {
	currentPage++
	if (currentPage >= numDots) {
		currentPage = 0;
	}
	showFeaturedPage(currentPage);
}

showFeaturedPage = function(i) {
	by = -1 * (Math.round(i * itemWidth) - (itemWidth/8));
	//$('#featured .side-scroller .list')[0].style.webkitTransform = 'translate3d('+by+'px,0px,0px)';
	$('#featured .side-scroller .list')[0].style.webkitTransform = 'translateX('+by+'px)';
	$(".pager-dots .dot").removeClass("selected")
	$(".pager-dots .dot:nth-child("+(i+1)+")").addClass("selected");
	currentPage = i;
}
	
$(function() {

	window.onresize = onWelcomeResize;
	window.onscroll = onWindowScroll;
	
	topnav = document.getElementById('topnav');
	if (topnav) topnav.classList.add('fade-in');
  	
  welcomeBgHero = document.querySelectorAll('#hero .bg')[0];
  welcomeHeroContent = document.querySelectorAll('#hero .content')[0];
  welcomeBgSearch = document.querySelectorAll('#search .bg')[0];
  welcomeBgDevices = document.querySelectorAll('#devices .bg')[0];
  welcomeBgSupport = document.querySelectorAll('#support .bg')[0];
  welcomeBgCreators = document.querySelectorAll('#creators .bg')[0];
  
  var t = setTimeout(function(){updateWelcomeBgOffsets(); onWindowScroll();}, 400);
  
	//$(".pager-dots").on("click", ".dot", function() {
		//showFeaturedPage($(this).index());
		//clearInterval(autoPagingInterval);
	//});
	//autoPagingInterval = setInterval(autoForwardPaging, 2000);
	
	$("#als-demo").als({
		visible_items: 4,
		scrolling_items: 1,
		orientation: "horizontal",
		circular: "yes",
		autoscroll: "yes",
		interval: 3000
	});
	
})
	
</script>
  </head>

