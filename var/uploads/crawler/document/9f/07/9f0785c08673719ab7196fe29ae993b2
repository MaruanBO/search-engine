<script type="text/javascript">

	jQuery(document).ready(function() {		
		var body = jQuery('#navigation').width();
		jQuery("div.level-2").each(function() {			
			var location = jQuery(this).siblings('a.level-1').position();						
			var right = location.left + jQuery(this).outerWidth();
			if (right > body) {
				jQuery(this).css('right',0);
			} else {
				jQuery(this).css('left',location.left);
			}		
		});		
	});
</script>






