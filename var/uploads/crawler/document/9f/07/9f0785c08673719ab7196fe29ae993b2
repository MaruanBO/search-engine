	<script type="text/javascript">
		jQuery(document).ready( function() {
			jQuery("#emailsignupform").dialog({
				 autoOpen: false,
				 height: 350,
				 width: 600,
				 modal: true
			});	
			
			jQuery("#emailsignupformIE").dialog({
				 autoOpen: false,
				 height: 350,
				 width: 600,
				 modal: true
			});	
			
			jQuery("#emailsignupthankyou").dialog({
				 autoOpen: false,
				 width: 400,
				 height: 300,
				 modal: true
			});
			
			jQuery('#email_continue-btn').click(function() {
				var sEmail = jQuery('#popup_email').val();
				if (jQuery.trim(sEmail).length == 0) {
					alert('The email address is invalid.');
				}
				if (validateEmail(sEmail) && jQuery('#popup_email').val() == jQuery('#confirm_popup_email').val()) {
					var url = "/on/demandware.store/Sites-SA-Site/default/LoyaltyServer-LSAddEmail?email=" + jQuery('#popup_email').val() + "&lastname=" + jQuery('#last_name').val() + "&firstname=" + jQuery('#first_name').val();
					jQuery.ajax({
						type: "GET",
						url: url
					});
					jQuery("#emailsignupform").dialog("close");
					jQuery("#emailsignupformIE").dialog("close");
					jQuery("#emailPopUpThankYouName").html(jQuery('#first_name').val() + ' ' + jQuery('#last_name').val());
					jQuery("#emailPopUpThankYouEmail").html(jQuery('#popup_email').val());
					jQuery("#emailsignupthankyou").dialog("open");
					jQuery("#go-shop-btn").click(function() {
						jQuery("#emailsignupthankyou").dialog("close");
					});
				} else {
					alert('The email address is invalid.');
				}
			});
			
			jQuery('#email_cancel-btn').click(function() {
				jQuery("#emailsignupform").dialog("close");
				jQuery("#emailsignupformIE").dialog("close");
			});
			
			if (jQuery.cookie('dw_homepage_visited') == undefined) {
				if (navigator.userAgent.indexOf('MSIE') > 0) {
					jQuery("#emailsignupformIE").dialog("open");
				} else {
					jQuery("#emailsignupform").dialog("open");
				}
				
				jQuery(".ui-icon-closethick").html("&nbsp;");
				jQuery(".ui-dialog-titlebar").css("border","0px");
				jQuery(".ui-dialog-titlebar").css("background-color","#FFFFFF");
				jQuery(".ui-icon-closethick").css("height","32px");
				jQuery(".ui-icon-closethick").css("width","32px");
				jQuery(".ui-icon-closethick").css("marginRight","5px");
				jQuery(".ui-dialog").css("border","4px solid #e60d55");
				jQuery(".ui-dialog").css("border-radius","0");
				
				jQuery.cookie('dw_homepage_visited', 'true', { expires: 365 });
			}
		});
		
		function validateEmail(sEmail) {
		    var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
		    
		    if (filter.test(sEmail)) {
		        return true;
		    }
		    else {
		        return false;
		    }
		}
	</script>

			


<div id="browser-check">
