jQuery(document).ready( function() {
	app.product.tile.init();
	Invodo.init({
		pageName: "Home Page",
		pageType: "home",
		onload: function() {
			for (var i=0;i<products.length;i++) {
				Invodo.Widget.add({ type:"inplayer", 
									mode:"overlay", 
									widgetId:"ovpWidget"+products[i],
									mpd:products[i],
									listensTo:"cta"+products[i] });
			}
		},
		onpodload: function() {
			for (var i=0;i<products.length;i++) {
				Invodo.Widget.add({ type:"cta",
									widgetId:"cta"+products[i],
									parentDomId:"pdOvpCTA"+products[i],
									mpd:products[i],
									data:"http://e.invodo.com/3.0/i/mer/playbutton_lime_64px.png" });
			}								
		}
	});
});
