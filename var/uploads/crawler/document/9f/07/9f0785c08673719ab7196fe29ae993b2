	jQuery(document).ready(function() {	
		
		var imagecount = 0;
		
		jQuery('#tophomepageslider .headerimage').each(function() {
			jQuery(this).attr("id", "selector" + imagecount);
			if (imagecount != 0){jQuery(this).hide();}
			imagecount += 1;			
		});		
		
		jQuery('#tophomepageslider .headerimage').first().addClass('selected');
			
		var slidertimer = 5000.0;
		var imageWidth = jQuery('.mainslider').width();
		var imageCount = jQuery('#tophomepageslider .headerimage').length;		
			
		var slider = new HomePageSlider(imageCount, imageWidth, slidertimer);
		var homepageslider = setInterval(function() {slider.startAnimation(imageCount, imageWidth, slidertimer)}, slidertimer);
		
		jQuery("#pagecount span").click(function(){
			clearInterval(homepageslider);	
		});
		
	});		
</script>
	



