<!--  Mobile Redirect -->
<script type="text/javascript">
	if (navigator.userAgent.toLowerCase().indexOf('iphone') > -1 ||
		((navigator.userAgent.toLowerCase().indexOf('android') > -1) && (navigator.userAgent.toLowerCase().indexOf('mobile') > -1))||
		navigator.userAgent.toLowerCase().indexOf('blackberry') > -1 ||
		navigator.userAgent.toLowerCase().indexOf('ipod') > -1) {
		
		if ('' == 'true') {
			document.cookie = 'forceMainSite=true;';
		} else if ('' == 'false') {
			document.cookie = 'forceMainSite=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
		}
		
		var forceMainSite = getCookie('forceMainSite') == 'true';
		
		if (!forceMainSite) {
		
			var url = window.location.href;
			var urlChanged = false;

			if (url.indexOf('Home-Show') > -1 || window.location.pathname == '/') {
				url = "/on/demandware.store/Sites-SA-Site/default/mHome-Show";
				urlChanged = true;
			} else if (url.indexOf('Product-Show') > -1) {
				url = url.replace('Product-Show', 'mProduct-Show');
				urlChanged = true;
			} else if (url.indexOf('Search-Show') > -1) {
				url = url.replace('Search-Show', 'mSearch-Show');
				urlChanged = true;			
			}
			if (urlChanged) {
				window.location = url;
			}
		}
	}
	
	function getCookie(name) {
		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + name + "=");
		if (c_start == -1) {
		  	c_start = c_value.indexOf(name + "=");
		}
		if (c_start == -1) {
		  	c_value = null;
		} else {
		  	c_start = c_value.indexOf("=", c_start) + 1;
		 	var c_end = c_value.indexOf(";", c_start);
		 	if (c_end == -1) {
				c_end = c_value.length;
			}
			c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;		
	}
</script>
