            				<div id="{{ game.Id }}" class="game-status post-game" ng-if="game.State == 'Final'">
            					<div class="status">{{ game.StateDetails }}</div>
                        		<div class="game-station">{{ ::game.Channel }}</div>
            				</div>
        					<div class="game-teams">
            					<div class="team-1" ng-class="{'winner': game.Away.IsWinner}">
									<div class="team-name"><div ng-if="currentSport != 'wjhc'" class="logo" team-class team-name="{{ ::game.Away.Team.SeoId }}" logo-size="24" sport="{{currentSport}}"></div><div ng-if="currentSport == 'wjhc'" class="logo flags-logo" team-class team-name="{{ ::game.Away.Team.SeoId }}" logo-size="24" sport="flags"></div><span>{{ ::game.Away.Team.Acronym }}</span></div>
									<div class="team-score" ng-if="currentSport == 'mlb'"><span>{{ ::game.Away.Runs }}</span></div>
									<div class="team-score" ng-if="currentSport != 'mlb'"><span>{{ ::game.Away.Linescore.Score }}</span></div>
                                </div>
            					<div class="team-2" ng-class="{'winner': game.Home.IsWinner}">
            						<div class="team-name"><div ng-if="currentSport != 'wjhc'" class="logo" team-class team-name="{{ ::game.Home.Team.SeoId }}" logo-size="24" sport="{{currentSport}}"></div><div ng-if="currentSport == 'wjhc'" class="logo flags-logo" team-class team-name="{{ ::game.Home.Team.SeoId }}" logo-size="24" sport="flags"></div><span>{{ ::game.Home.Team.Acronym }}</span></div>
        						    <div class="team-score" ng-if="currentSport == 'mlb'"><span>{{ ::game.Home.Runs }}</span></div>
									<div class="team-score" ng-if="currentSport != 'mlb'"><span>{{ ::game.Home.Linescore.Score }}</span></div>
            					</div> 
                                <div class="team-record" ng-if="currentSport != 'wjhc'">
        							<span class="record">{{::game.Away.Record}} <span streak-initial streak="{{::game.Away.Streak}}" ng-if="game.Away.Streak"></span></span>
        							<span class="record">{{::game.Home.Record}} <span streak-initial streak="{{::game.Home.Streak}}" ng-if="game.Home.Streak"></span></span>
    							</div>
            				</div>
        				</div>
    				</a>
            	</li>
			</ul>
			</div>
        </div>
    </div>

</section>




      
  
              </div>
