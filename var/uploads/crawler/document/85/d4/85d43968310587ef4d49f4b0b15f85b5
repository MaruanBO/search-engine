		function max_no_days(){
			var o='',m=$('.p_bday select[name="mon"]').val(),d=$('.p_bday select[name="day"]').val();
			var lim=[0,31,29,31,30,31,30,31,31,30,31,30,31];
			if(lim[m]<d) d=1;
			for(i=1;i<=lim[m];i++){
				o+="<option value="+i+(d==i?" selected":'')+">"+i+"</option>";
			}
			$('.p_bday select[name="day"]').html(o);
		}
