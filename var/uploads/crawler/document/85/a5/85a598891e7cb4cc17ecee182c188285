<script>
function getDirectoryName(whichDir) { //returns the name of directory you need: call  - getDirectoryName(1) or getDirectoryName(2) etc
  var subsection = document.location.href;
  var subsectionArray = new Array();
  subsectionArray = subsection.split("\/");
  var directory = whichDir + 2;
  var DirName = subsectionArray[directory];
  if (DirName == undefined){
  return '';
   } else { 
   return DirName.toLowerCase();
   }
  
}

var var_FirstDirName = getDirectoryName(1);
if (var_FirstDirName.match('asp')){
	//var_FirstDirName = '';
	document.write('<style type="text/css"><!--');// start CSS
	//29 Apr 2013 - Rebranding
	//document.write('#homepageli a.top {background-color: #ff9900; color: #FFFFFF;}');// highlight home tab
	document.write('#homepageli a.top {background-color: #e51937; color: #FFFFFF;}');// highlight home tab
	document.write('--></style>');// End CSS
}else{

	if ( (var_FirstDirName=='dodge')||(var_FirstDirName=='sweets-and-snap')||(var_FirstDirName=='enr')||(var_FirstDirName=='greensource')||(var_FirstDirName=='architectural-record') )
	{
		var_FirstDirName='products';
	}

	document.write('<style type="text/css"><!--');// start CSS
	//29 Apr 2013 - Rebranding
	//document.write('#'+var_FirstDirName+'li a.top {background-color: #ff9900; color: #FFFFFF;}');// highlight other tabs
	document.write('#'+var_FirstDirName+'li a.top {background-color: #e51937; color: #FFFFFF;}');
	document.write('--></style>');// End CSS
}
</script>



