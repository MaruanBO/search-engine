<script type="text/javascript" > 
    
    var ALL_FUNNELS = [
      "organic","affiliate1","affiliate1_home","account_signup","account_home","home","c2c","market1","market2","market3","wireless3gpromo1","wireless3gpromo2","wireless3gaccount1","account","accounttest","account_free_trial","homeaccount1","homeaccountfree","activation","activatephone","mobileactivate","mobile","mmarket","m1weekfree","product","phones","markethome1","markethomeaccount1","wifi1","qual3g","marketwifi1","q4g4b","nq4g4b","betaphone","phone","phone1","tablets","marketbundle1","marketbundle2","marketbundle3","marketbundle4","marketbundle5","marketbundle_sem","freeinternet","odppremiere","internalphone","discountaccount1","discountnwaccount1","byod","offlinebyod","discountbyod","idprotection1","ppsecurity","phhpdctst","photonholidaylease","phoneaccountlife","retail_activation","instoreactivate","theprivacyphone","iphone5","connectedcar","mobileOTT","tabletott","infomercialpromo","infomercialpromoretry","nationwidewifi","highspeedinternet","zteforcepromo1","zteforcepromo2","nationwidereconcile","byodwireless","highspeednationwideinternet","highspeedinternet30trial"
    ];
    /**
     * Waits 750ms for T&T to set the experience cookie (if running campaign
     * is targeted to this page).
     * 
     * Removes all funnel experience cookies, except for the one named in the 
     * parameter (in case T&T has set it).  
     * 
     * Checks to see if the experience cookie named in the parameter is already
     * set (i.e. by call to T&T).  If it hasn't been set, use the funnel's 
     * default fallback value.  If it was set, leave it alone.
     * 
     * @param {string} funnelName Name of the funnel (set by controller when landing on page, or defaulted to 'organic').
     */
    function experienceFallback(funnelName){
      
      
        console.log("Landed on "+funnelName+" page, clearing other experience cookies.");

        console.log("Deleting unmatching experience cookies...");

        // Delete other experience cookies.
        for( aFunnel in ALL_FUNNELS){

            if(ALL_FUNNELS[aFunnel] !== funnelName || CookieUtil.readCookie("experience_"+funnelName) !== null && CookieUtil.readCookie("experience_"+funnelName).contains("default")){
                
                CookieUtil.eraseCookie("experience_"+ALL_FUNNELS[aFunnel]);
            }
        }
        
        console.log("Waiting for T&T to set experience...");

        setTimeout(function(){

            var expCookie = CookieUtil.readCookie("experience_"+funnelName);

            if( expCookie === null){
                console.log("experience_"+funnelName+" was NOT found, falling back to default: experience_"+funnelName+"="+funnelName+".default");
                CookieUtil.createCookie("experience_"+funnelName, funnelName+".default");
            }
            else{
                console.log("Experience was already set to a non-default: experience_"+funnelName+"="+expCookie);
            }
        },750);
    }
</script>

<div class="mboxDefault" ></div>    
