               });

               // Log the event in T&T
               tntClickTrack(funnelName,"qualify");

                $('.qualIntro').fadeOut('slow');
                $('#qualifyForm').fadeIn('slow');
               return false;
            });

            $('.showQualForm').click(function(){
               $('.qualIntro').hide();
               $('#qualifyForm').show();
            });
        });

