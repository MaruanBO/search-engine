    <script type="text/javascript">
        $(document).ready(function(){

            $("#wirelessChoice").on("click",function(){

                clearExperiences();

                setGA({
                    "category"  : "wireless_clicked",
                    "action"    : "homepage"
                });

                var  href = $(this).attr("href");
                setTimeout(function(){
                    window.location.href = href;
                },750);
            });

            $("#homeChoice").on("click",function(){

                clearExperiences();

                CookieUtil.createCookie("experience_home", "home.default");

                setGA({
                    "category"  : "home_clicked",
                    "action"    : "homepage"
                });
                var  href = $(this).attr("href");
                setTimeout(function(){
                    window.location.href = href;
                },750);
                return false;
            });

            setTimeout( function(){

                
                var html = '';

                //-------------------- Check for experience cookie.
                var key;
                var cookies = document.cookie.split(";");
                for ( i=0 ; i < cookies.length ; i++){

                    key = cookies[i].split("=")[0];
                    key = key.replace( /^\s+|\s+$/g, "" );

                    if ( key.contains("experience") ){

                        var cookieVal = (unescape(cookies[i].split("=")[1])).split(";")[0];
                        html += (typeof cookieVal !== null) ? cookieVal : "";
                    }
                }
                //--------------------

                $('#exp').html( html );
            }, 250);

        });
    </script>
    <div id="show" style="width:100%;height:20px;float:left;margin-top:-20px;" onmouseover="$('#exp').css('visibility','visible');" onmouseout="$('#exp').css('visibility','hidden');">
        <div id="exp" style="visibility:hidden;color:lightskyblue;text-align:left;font-size:11px"></div>
    </div>
