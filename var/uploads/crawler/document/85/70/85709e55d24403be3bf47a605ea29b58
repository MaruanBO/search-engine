    <script type="text/javascript">
        function measureSection() {
            var windowHeight = Math.max($(window).height(), 460);
            var contentMargin = (windowHeight * .2);
            var sectionHeight = (windowHeight - $('header').outerHeight(true)) - $('#phone').outerHeight(true);
            //var sectionHeight = windowHeight - $('header').outerHeight(true);


           // if (windowHeight > 460){
                $('.topQual').css('height', windowHeight);
                $('.qualIntro').css('top', contentMargin);
                $('#qualifyForm').css('top', contentMargin);
                $('.phone').css('height', sectionHeight);
                $('.wireless').css('height', sectionHeight);
                $('.home').css('height', sectionHeight);
            //}
            //return false;
            if (windowHeight < 640){
                $('.topQual').css('height', '640px');
            }
        }

        $(document).ready(function() {
            

            measureSection();

            if($(window).scrollTop() > 0){
                $('header').addClass('scrolled');
            }

            $(window).resize(function(){
                measureSection();
            });

            $(window).scroll(function(){
                if ($(window).scrollTop() > 0){
                    $('header').addClass('scrolled');
                }else {
                    $('header').removeClass('scrolled');
                }
            });
            $('.qualIntro a').click(function(){

               console.log("Clicked qualIntro");
