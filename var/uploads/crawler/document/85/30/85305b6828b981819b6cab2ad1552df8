    <script type="text/javascript">
        $(document).ready(function(){
            $('.navbar-search').placeholder();

            // this needs to be in page_js block due to context and needs to be read AFTER old store item counter
            TTAM.LANGUAGE_CODE = 'en-us';
            var locale = TTAM.LANGUAGE_CODE || 'en-us';
            var cart_count = $.cookie('cart_count.' + locale );
            var cart_number = $('.js-cart-count');
            var store_link = $('.js-cart-locale');

            // add cart count to page
            if (cart_count === undefined && locale !== 'en-us') {
                cart_number.text('0');
            } else {
                cart_number.text(cart_count);
            }

            // set localized store link if cookie exists
            if (cart_count !== undefined || locale !== 'en-us') {
                store_link.attr({ href: '//store.23andme.com/' + locale });
            }
        });
    </script>

    
