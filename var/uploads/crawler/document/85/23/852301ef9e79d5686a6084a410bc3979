        <script>
            $(function () {
                if ($('div.alertRoadblockContainer #road_block_alert').length > 0) {
                    $("div.campaignRoadblockContainer").hide();
                    $.blockUI({ baseZ: 10000, message: $('#campaignRoadblock'), overlayCSS: { backgroundColor: '#fff' }, css: { border: '', width: ''} });
                }
                else if ($('div.campaignRoadblockContainer div.campaignAsset').length > 0) {
                    //Added this If to check for guided nav typed roadblocks - remove when cms driven
                    if (!$('div.campaignAsset .noblock').length) {
                        $(window).load(function () {
                            var roadblock = $('#campaignRoadblock');
                            roadblock.find('script').remove();
                            $.blockUI({ baseZ: 10000, message: roadblock, overlayCSS: { backgroundColor: '#fff' }, css: { border: '', width: '', top: '50%', left: '50%', marginLeft: -(roadblock.width() / 2), marginTop: -(roadblock.height() / 2)} });
                        });
                    }
                }
            });
        </script>


        <script type="text/javascript">

                
        $('#fbToken').on('change', function(){ 
            $.ajax({ 
                url : "/myaccount/facebookloginajax",    
                data : { token : $(this).attr('value') } ,
                beforeSend : function(xhr) { xhr.setRequestHeader("X-LB-SECURE", "1"); }
            })
            .done(function(res){
                if(res.success) window.location.reload(true);
            });
        });
                


            //Facebook automatic Login
            window.fbAsyncInit = function () {

                // initialize facebook api
                FB.init({ appId : '185374358234765'
                    ,     status : true
                    ,     cookie : false
                    ,     xfbml : true
                });

                // get login status
                FB.getLoginStatus(function(res) {
                    $('#fbStatus').val(res.status).trigger('change');
                    if(res.authResponse && res.authResponse.accessToken)
                    {
                        $('#fbToken').val(res.authResponse.accessToken).trigger('change');
                    }
                });
            };

        </script>

