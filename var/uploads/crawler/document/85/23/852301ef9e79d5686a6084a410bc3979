<script type="text/javascript">
    $(function () {
        var validateEmailOnKeyPress = false;
        var emailError = $('span#emailError');
        var emailTextBox = $('#enterEmail');
        var emailForm = emailTextBox.parents('form');
        function isEmailValid() {
            return Pet360.validateEmail(emailTextBox.val());
        }
        function validateEmail() {
            var emailIsValid = isEmailValid();
            if (emailIsValid)
                emailError.text('');
            else{
                emailError.text('Invalid email address');
            emailTextBox.addClass('error');}
            return emailIsValid;
        }
        emailTextBox.keypress(function () {
            if (!validateEmailOnKeyPress)
                return;

            validateEmail();
        });
        $(".continueOn").click(function (event) {
            event.preventDefault();
            $.unblockUI();
        });
        $("#emailCaptureForm").submit(function (e) {
            e.preventDefault();
            if (!validateEmail()) {
                validateEmailOnKeyPress = true;
                e.preventDefault();
                return false;
            }
            dataString = $("#emailCaptureForm").serialize();
            console.log(dataString);
            $.ajax({
                type: "POST",
                url: "/captureemail",
                data: dataString,
                dataType: "json",
                success: function (data) {
                    _gaq.push(['_trackEvent', 'Email', 'Roadblock_Love_Tails_giveaway']);
                    $('#theForm').hide();
                    $("#theGoods").show(); 
                }

            });

        });

    });
</script>
