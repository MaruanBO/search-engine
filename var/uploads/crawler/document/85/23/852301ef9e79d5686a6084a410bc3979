    <script type="text/javascript" src="/Scripts/responsiveslides.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#slides").responsiveSlides({ auto: true, pager: false, nav: true, pause: true, maxwidth: "none", controls: "", namespace: "rslides", prevText: "", nextText: "", speed: 500, timeout: 10000 });
            $(".videoStart").click(function (event) {
                event.preventDefault();
                var width = $('#largeSlideshow').width() * '.7275390625';
                var height = ((1080 / 1920) * width + 51);
                $('object').attr('height', height);

                if ($(window).width() <= 500) {
                    var height = ((1080 / 1920) * width);
                    $('.limelight-player-footprint').height(height);
                    $(".rslides1_tabs").hide();
                    $("#videoOverlay").width('100%').height($(".rslides").height() + 30).fadeIn("slow");
                    $("#slides").height($('object').height() + 20);
                }
                else {
                    $("#videoOverlay").width('100%').height($("#largeSlideshow").height()).fadeIn("slow");
                    $(".rslides1_tabs").hide();
                }
            });
            $(".goPlay").click(function (event) {
                event.preventDefault();
                $("#videoOverlay").hide();
                $(".goPlay").hide();
                $("#slides").height("auto");
                $(".rslides1_tabs").show();
            });
            $(".videoPlayer .closeButton").click(function (event) {
                event.preventDefault();
                $("#videoOverlay").hide();
                $(".goPlay").hide();
                $("#slides").height("auto");
                $(".rslides1_tabs").show();
            });
            if ($(window).width() >= 500) {
                $(window).resize(function () {
                    var height = ((1080 / 1920) * $(".videoPlayer").width()) + 49;
                    $('object').attr('height', height);
                    if ($(window).width() <= 500) {
                        $("#videoOverlay").width('100%').height($(".rslides img").height())
                    }
                    else { $("#videoOverlay").width('100%').height($("#largeSlideshow").height()); }
                });
            }
        });
        function delvePlayerCallback(playerId, eventName, data) {
            var id = 'limelight_player_337199';
            if (eventName == 'onPlayerLoad' && (DelvePlayer.getPlayers() == null || DelvePlayer.getPlayers().length == 0)) {
                DelvePlayer.registerPlayer(id);
            }

            switch (eventName) {
                case 'onMediaComplete':
                    $(".goPlay").show();
                    break;
                case 'onPlayStateChanged':
                    if (data.isPlaying) $(".goPlay").hide();
                    break;
            }
        }
    </script>


