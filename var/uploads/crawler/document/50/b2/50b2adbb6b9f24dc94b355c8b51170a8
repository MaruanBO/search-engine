          SBN.Ads.showAd = function(id) {
            if (console && console.log) {
              console.log("Ads did not load so we cannot render ad " + id + ".");
            }
          };
        }
      
//]]>
</script><script type="text&#x2F;javascript">
//<![CDATA[
sbi_morpheus.setKeys();
//]]>
</script>






