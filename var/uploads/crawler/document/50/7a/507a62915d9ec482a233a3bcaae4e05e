	function TrackTiming(category, variable, opt_label, opt_sampleRate) {
		this.category = category;
		this.variable = variable;
		this.label = opt_label ? opt_label : undefined;
		this.startTime;
		this.endTime;
		this.sampleRate = opt_sampleRate ? opt_sampleRate : 100;
		return this;
	}

	TrackTiming.prototype.startTime = function() {
		this.startTime = new Date().getTime();
		return this;
