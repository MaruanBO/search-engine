	// Abstracts GA track timing feature for creating timing tests //
	var kink = kink || {};
	kink.analytics = kink.analytics || {};
	// save instances of TrackTiming to this object //
	kink.analytics.timings = kink.analytics.timings || {};

	window._gaq = window._gaq || [];

	// use new to create timing object //
	kink.analytics.TrackTiming = function (category, variable, opt_label, opt_sampleRate) {
		this.category = category;
		this.variable = variable;
		this.label = opt_label ? opt_label : undefined;
		this.startTime;
		this.endTime;
		this.sampleRate = opt_sampleRate ? opt_sampleRate : 10;
		return this;
	}

	// create and store the start time //
	kink.analytics.TrackTiming.prototype.setStartTime = function() {
		if ( !this.startTime ) {
		this.startTime = new Date().getTime();
		}

		return this;
	}
	
	// create and store the end time //
	kink.analytics.TrackTiming.prototype.setEndTime = function() {
		var that = this;
		if ( !this.endTime ) {
			this.endTime = new Date().getTime();
			return this;
		} else {
			this.send = function () {
				return that;
			}
			return this;
		}
	}
	
	// calculate timing and send to ga. set category, variable, label, and sample rate before executing this func //
	kink.analytics.TrackTiming.prototype.send = function() {
		if (this.endTime && this.startTime && this.startTime < this.endTime) {
			var timeSpent = this.endTime - this.startTime;
			window._gaq.push(['_trackTiming', this.category, this.variable, timeSpent, this.label, this.sampleRate]);
		}

		return this;
	}

	kink.analytics.TrackTiming.prototype.onClipStart = function (clip) {
		var label = 'Highwinds,' + 
			clip.extension + 
			',' + 
			clip.metaData.width + 
			'x' + 
			clip.metaData.height + 
			',' + 
			Math.round(clip.metaData.videoframerate) + 
			'fps';
		kink.analytics.timings.videoBuffer.label = label;
		kink.analytics.timings.videoBuffer.setEndTime();
		kink.analytics.timings.videoBuffer.send();
	}
	
</script>

