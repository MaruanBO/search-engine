<script type="x-jquery-tmpl" id="selectedItemTemplate">
    <div class="clearfix leaderboard-banner-item">
        <div class="dropdown">
            {{if items && items.length > 0}}
                <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">{{= data.tourName}} <i class="icon-chevron-down"></i></a>
            {{else}}
                <span class="dropdown-toggle">{{= data.tourName}}</span>
            {{/if}}

            <ul class="dropdown-menu">
                {{each(i, item) items}}
                    {{if item.tourName && item.title}}
                        <li>
                            <a href="javascript:;" class="tour-selector" lb-banner-index="{{= item.id}}">
                                {{= item.tourName}} - {{= formatString(item.title, 20, 16)}}
                            </a>
                        </li>
                    {{/if}}
                {{/each}}
            </ul>
        </div>

        {{if data.type == 'weather' || data.type == 'lbMessage'}}
            <span class="tournament-state delayed">
                {{if data.labelOverride && data.labelOverride.length}}
                    {{= data.labelOverride}}
                {{else}}
                    DELAYED
                {{/if}}
            </span>
        {{else data.type == 'standings'}}
            <a href="{{= safeLink(data.standingsPage)}}" class="tournament-state standings">
                {{if data.labelOverride && data.labelOverride.length}}
                    {{= data.labelOverride}}
                {{else}}
                    STANDINGS
                {{/if}}
            </a>
        {{else data.itemData.roundState == 'Groupings Official'}}
            <a href="{{= safeLink(data.teeTimesPage)}}" class="tournament-state tee-times">
                {{if data.labelOverride && data.labelOverride.length}}
                    {{= data.labelOverride}}
                {{else}}
                    TEE TIMES
                {{/if}}
            </a>
        {{else data.itemData.roundState == 'In Progress' || data.itemData.roundState == 'In-Progress'}}
            <a href="{{= safeLink(data.leaderboardPage)}}" class="tournament-state live-now">
                {{if data.labelOverride && data.labelOverride.length}}
                    {{= data.labelOverride}}
                {{else}}
                    LIVE NOW
                {{/if}}
            </a>
        {{else}}
            <a href="{{= safeLink(data.leaderboardPage)}}" class="tournament-state leaderboard">
                {{if data.labelOverride && data.labelOverride.length}}
                    {{= data.labelOverride}}
                {{else}}
                    LEADERBOARD
                {{/if}}
            </a>
        {{/if}}
    </div>

    <p class="title">
        {{if data && data.title}}
            <a href="{{if data.type == 'standings'}}{{= safeLink(data.standingsPage)}}{{else}}{{= safeLink(data.tournamentPage)}}{{/if}}">
                {{= formatString(data.title, 40, 36)}}
            </a>
        {{/if}}
    </p>
</script>

<script type="x-jquery-tmpl" id="leaderboardTemplate">
    <div class="item-content players-table r-tour {{if data.itemData.roundState == 'In Progress'}}live-tournament{{/if}} ">
        {{each(p,player) data.itemData.players}}
            {{if p < 5}}
                <div class="player-cell">
                    {{if data.itemData.roundState == 'Groupings Official'}}
                        <a href="{{= safeLink(data.teeTimesPage)}}">
                    {{else}}
                        <a href="{{= safeLink(data.leaderboardPage)}}">
                    {{/if}}

                    {{if player.teedOf || player.currentRound > 1}}
                        <span class="player-position">
                            {{= player.position}}
                        </span>
                    {{/if}}

                    <span class="player-name">
                        {{= player.firstName.substring(0, 1)}}. {{= player.lastName}}
                    </span>
                    <span class="player-score">
                        {{if player.teedOf || player.currentRound > 1}}
                            {{= player.score}}
                        {{/if}}

                        {{if player.teedOf}}
                            <span class="player-thru">
                                {{if player.thru == '18'}}
                                    F
                                {{else}}
                                    {{= player.thru}}
                                {{/if}}
                            </span>
                        {{else}}
                            <span class="player-tee-time">
                                {{= moment(player.teeTime).format('h:mm a')}}
                            </span>
                        {{/if}}
                    </span>
                    </a>
                </div>
            {{/if}}
        {{/each}}
    </div>
</script>

<script type="x-jquery-tmpl" id="standingsTemplate">
    <div class="item-content players-table r-tour">
        {{each(p,player) data.itemData.players}}
            {{if p < 5}}
                <div class="player-cell">
                    <a href="{{= safeLink(data.standingsPage)}}">
                    <span class="player-position">
                        {{= player.position}}
                    </span>
                    <span class="player-name">
                        {{= player.firstName.substring(0, 1)}}. {{= player.lastName}}
                    </span>
                    <span class="player-score">
                        {{= data.extraSymbol}}{{= player.points}}
                    </span>
                    </a>
                </div>
            {{/if}}
        {{/each}}
    </div>
</script>

