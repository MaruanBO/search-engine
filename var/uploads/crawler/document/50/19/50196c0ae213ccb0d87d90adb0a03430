<script>
    $(document).ready(function () {

        $("#subsButton").click(function () {
            var email = document.getElementById('sdsa');

            var filter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i

            if (filter.test(email.value)) {
                $("#errsubsc").hide();
                $("#subscmn").fadeOut(function () {
                    $("#oksubsc").show();
                });
                
                
                $.post("/pp/?dt=" + $("#sdsa").val());
            }
            else
            {
                $("#errsubsc").show();                
            }


        });
    });
</script>

