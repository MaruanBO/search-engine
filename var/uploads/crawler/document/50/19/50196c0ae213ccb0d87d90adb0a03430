<script>
    index_document_ready();
    function onHover(ssc) {
        var st = ($(ssc).attr('src'));
        $(ssc).attr('src', st.substring(0, st.length - 4) + "_RO.png");
    }

    function offHover(ssc) {
        var st = ($(ssc).attr('src'));
        $(ssc).attr('src', st.substring(0, st.length - 7) + ".png");
    }

    function runInterval() {
        setInterval(function () {
            changeImage("#backgimg", "http://static.stuccu.com/images/designs/categories/main_image_");
        }, 4000);
    }

    var counter1 = 2;

    function changeImage(a,imgurl) {
        try {
            //var image_src = $(a).css('background-image');
            $(a).fadeOut("slow", function () {
                // Animation complete.
                $(backgimg).css({
                    'background-image': 'url("' + imgurl + counter1 + ".jpg" + '")',
                    'display': 'none'
                });
            });

            $(a).fadeIn();
            counter1++;
            if (counter1 == 6) {
                counter1 = 1;
            }
        }
        catch (err)
        { }
    }
    //$("#backgimg").css('background-image',)
    setTimeout(function () {
        changeImage("#backgimg", "http://static.stuccu.com/images/designs/categories/main_image_");
        runInterval();
    }, 500);
</script>";i:2;s:3:"282";}