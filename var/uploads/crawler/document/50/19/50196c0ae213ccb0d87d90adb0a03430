    <script>
        function reportLog(msg, type) {
            url = "http://stuccu.com/lg?msg=" + escape(msg);
            if (type != null) {
                url = url + "&type=" + type.toString();
            }
            $.ajax(
                  {
                      type: 'GET',
                      url: url
                  });
        }

        window.onerror = function (msg, url, line) {
            reportLog("Error in script " + url.toString() + " with message " + msg.toString() + " on line " + line.toString(), 2);
        }

    </script>
