    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.js"></script>
    <script>
        function GetQueryStringVariable(varToFind) {
            return function () {
                try {
                    var sourceToRunOn = document.location.search;
                    if ((sourceToRunOn !== undefined) && (sourceToRunOn !== null) && (sourceToRunOn.length > 1)) {
                        sourceToRunOn = sourceToRunOn.substring(1);
                        var collToRunOn = sourceToRunOn.split("&");
                        for (var i = 0; i < collToRunOn.length; ++i) {
                            var firstIndex = collToRunOn[i].indexOf("=");
                            var varName = collToRunOn[i].substring(0, firstIndex);
                            if (varName === varToFind) {
                                return collToRunOn[i].substring(firstIndex + 1).replace(/\+/g, " ");
                            }
                        }
                    }
                }
                catch (err) {
                    if ((console !== null) && (console !== undefined)) {
                        console.log(err);
                    }
                }
                return null;

            }();
        }

        $(document).ready(function () {
            var stcIdValue = GetQueryStringVariable("stcId");
            var addressValue = window.location.href;
            if (stcIdValue !== null && stcIdValue !== undefined){
                var st = $("[stcid=" + stcIdValue + "]");
                if (st.text().length > 0) {
                    $("#category_filter").html(st.text());
                }
                else $("#category_filter").html($("#alldep").html());
            }
            else $("#category_filter").html($("#alldep").find('span').html());
            $(".hover_bottom").mouseenter(function () {

                $(this).siblings().css("display", "block");
                //Lazy image load
                var tn_array = $(this).siblings().find(".dropdown_internal").first().find("img").map(function () {
                    return $(this);
                });
                load_imgs(tn_array);


                var before_side = $(this).siblings();
                var side = before_side.find(".side_bottoms");
                var side_button = side.find("li").first();
                side_button.find(".categories").addClass("categories_hover");

                var name = side_button.attr("name");
                var dsd = before_side.find(".dropdown_internal[name='" + name + "']").css("display", "block");
            });

            $(".hover_bottom").mouseleave(function () {

                $(this).siblings().css("display", "none");
            });

            $(".top_buttons li[name='true']").mouseenter(function () {

                $(this).css("background", "#FFFFFF");
                $(this).find("a").css("color", "#252525");
            });

            $(".top_buttons li[name='true']").mouseleave(function () {

                $(this).css("background", "#6a7575");
                $(this).find("a").css("color", "white");
            });

            //Div container

            $(".drospdown_div_con").mouseenter(function () {

                $(this).css("display", "block");

            });

            $(".drospdown_div_con").mouseleave(function () {

                $(this).css("display", "none");

            });

            $(".catname").each(function (a) {
                $(this).click(function () {
                    $(".catname").css("background", "white");
                    $(this).css('background-color','rgb(239, 239, 239)');
                    $(".tdcat").css("display", "none");
                    
                    //$(".tdcat[data-cat='" + ($(this).attr("data-cat")) + "']").css("display", "block");
                    //$(".tdcat" + ($(this).attr("data-cat"))).fadeToggle();
                });
                /*
                $(this).mouseleave(function () {
                    $(".tdcat[data-cat='" + ($(this).attr("data-cat")) + "']").css("display", "none");
                    //$(".tdcat" + ($(this).attr("data-cat"))).fadeToggle();
                });
                */
            });


            //Side buttons
            $(".side_bottoms li").mouseenter(function () {
                //Lazy image load
                //var st = $(this).attr('name');

                var sf = $(this).closest(".drospdown_div_con");
                var tn_array2 = $(sf).find('.dropdown_internal[name="1"]').siblings().find("img").map(function () {
                //var tn_array2 = $(".dropdown_internal[name='" + st + "']").siblings().find(".trfd").map(function () {
                    return $(this);
                });
                load_imgs(tn_array2);

                //(".categories").removeClass("categories_hover");
                var data = $(this).attr("name");

                $(".dropdown_internal[name='" + data + "']").css("display", "block");


            });


            $(".side_bottoms li").mouseleave(function () {
                var data = $(this).attr("name");

                $(".dropdown_internal[name='" + data + "']").css("display", "none");

            });


            $(".categories").mouseenter(function () {

                $(".categories").removeClass("categories_hover");

                $(this).addClass("categories_hover");
            });


            //
            $(".dropdown_internal").mouseenter(function () {
                                

                $(this).css("display", "block");

            });

            $(".dropdown_internal").mouseleave(function () {

                $(this).css("display", "none");

            });


            $("#category_filter_con").click(function () {
                if ($("#category_filter_con").css('border-bottom-left-radius') == '18px') 
                    $("#category_filter_con").css('border-bottom-left-radius', '0px');
                else $("#category_filter_con").css('border-bottom-left-radius', '18px');
                $("#category_list_con").toggle();

            });

            $("#category_list_con table td").click(function () {
                
                var cat = $(this).find('span').html();
                $("#category_filter").html(cat);
                $("#category_filter_con").css('border-bottom-left-radius', '7px');
                $("#category_list_con").toggle();
                $("#category_filter").attr("stcId", $(this).attr("stcId"));
                
            });


            
            $("#catli").mouseenter(function () {
                unmarkobj($("#category_filter_con2").find("img"));
                if ($("#category_filter_con2").css('border-bottom-left-radius') == '7px') {
                    $("#category_filter_con2").css('border-bottom-left-radius', '0px');
                    $("#catli").css('background-color', 'white');
                    $("#category_filter2").css('color', '#6a7575');
                    
                }           
                //alert(7);
                $("#category_list_con2").css('display', 'block');
                //$("#category_list_con2").slideToggle();
            });
            
            
            
            $("#catli").mouseleave(function () {
                //$("#category_list_con2").slideToggle();
                markobj($("#category_filter_con2").find("img"));
                $("#category_filter_con2").css('border-bottom-left-radius', '7px');
                $("#catli").css('background-color', '   #6a7575');
                $("#category_filter2").css('color', 'white');
                $("#category_list_con2").css("display", "none");
            });
            /*
            $("#category_list_con2 table td").click(function () {

                var cat = $(this).find('span').html();
                $("#category_filter2").html(cat);
                $("#category_filter_con2").css('border-bottom-left-radius', '7px');
                $("#category_list_con2").slideToggle();
                $("#category_filter2").attr("stcId", $(this).attr("stcId"));

            });

            */
        });
        function load_imgs(imgList) {
            for (var i = 0; i < imgList.length; i++) {
                if (imgList[i].attr("src").length < 2)
                    imgList[i].attr("src", imgList[i].attr("klk"));
            }
        }

    </script>


