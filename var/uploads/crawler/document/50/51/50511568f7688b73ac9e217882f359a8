  function convertToEntities(tstr)
    {
      var bstr = '';
      for(i = 0; i < tstr.length; ++i)
        {
          if(tstr.charCodeAt(i) > 127)
        {
          bstr += '&#' + tstr.charCodeAt(i) + ';';
        }
      else
        {
          bstr += tstr.charAt(i);
        }
        }
      return bstr;
    }

  function checkReplacementChr(tstr) {
    if (encodeURI(tstr).indexOf("%EF%BF%BD") >= 0) {
      return false;
    } else {
      return true;
    }
  }

