<script>
var onLeft = true;
$(".bargain_arrow_r").click(function(){
	if (onLeft) {
		onLeft = false;
		$("#bargain_innerContainer ul").css("margin-left","-515px");
	} else {
		onLeft = true;
		$("#bargain_innerContainer ul").css("margin-left","0px");
	}
});
$(".bargain_arrow_l").click(function(){
	if (onLeft) {
		onLeft = false;
		$("#bargain_innerContainer ul").css("margin-left","-515px");
	} else {
		onLeft = true;
		$("#bargain_innerContainer ul").css("margin-left","0px");
	}
});
</script>


