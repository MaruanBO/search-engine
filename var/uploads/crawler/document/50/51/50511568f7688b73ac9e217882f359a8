<script>
function shift_right(){
	div = $('#bargain');
	last = div.find('li:last');
	first = div.find('li:first');
	fifth = last;
	if(div.find('li').length >= 5){
		fifth=div.find('li:eq(4)');
	}
	hidingEffect(fifth,"left",
		function(){
		last.insertBefore(first);//first = div.find('li:first');
		showingEffect(last,"left");
	});
}
function shift_left(){
    div = $('#bargain');
	last = div.find('li:last');
	first = div.find('li:first');
	fifth = first;
	//inserted so min:6 eq:5
	if(div.find('li').length > 5){
		fifth=div.find('li:eq(5)');
	}
	hidingEffect(first,"right",
		function(){
			first.insertAfter(last);//fifth=div.find('li:eq(4)');
			showingEffect(fifth,"right");
		});
}
function hidingEffect(el,dir,callback){
	div = $('#bargain');
	if (dir == "left"){
		el.animate({opacity: 0.0,left:'44px'},400,callback);
		el.animate({left:'0px'},0);
	}
	else{
		el.animate({opacity: 0.0,right:'44px'},400,callback);
		el.animate({right:'0px'},0);
	}
}
function showingEffect(el,dir){
	div = $('#bargain');
	el_width = el.css("width")/2;
	if (dir == "left"){
		el.animate({left:'-44px'},0);
		el.animate({opacity: 1.0,left:'0px'},400);
	}
	else{
		el.animate({right:'-44px'},0);
		el.animate({opacity: 1.0,right:'0px'},400);
	}
}
</script>
<link href='//fonts.googleapis.com/css?family=Coda:400,800' rel='stylesheet' type='text/css'>
