        <script>
          var switch_bg = "";

          jQuery(document).ready(function() {

            jQuery("#megamenu").mouseleave(function() {
              showmm("clear");
            });


            jQuery('#header').mouseleave(function(){
              showmm("clear");
            });


            jQuery("ul#categories li").each(function(index){
              jQuery(this).hover(function(){
                megaMenuTimer = setTimeout(function(){
                  showmm('mm'+(index+1));
                  ajxmegamenu('http://hobbyking.com/hobbyking/store/customerScoring.asp?type='+(index+1))
                },300);
              },function(){
                clearTimeout(megaMenuTimer);
              })
            });


          });


          function showmm(mmvar) {
            jQuery("#megamenu .links").hide();
            if (mmvar != "clear") {
              jQuery("#megamenu").show();
              jQuery('#'+mmvar).show();
            } else {
              jQuery("#megamenu").hide();
            }
          }

          function ajxmegamenu(pagevar,destinationvar) {
            jQuery.ajax({
              url:pagevar
            });
          }
        </script>    <!-- Search Bar
    ================================================== -->


<script>
	$(function()
	{
		//alert("['Avios Sea Fury Warbird', 'Assault Reaper 3D Quad', 'Brushless Gimbal', 'TrackStar ROAR Approved', 'EFX Racer', 'RTF Quad', 'Quanum Venture FPV', 'DJI products', 'Italeri Plastic Model Kit', 'KK2']");
		var default_value = "Avios Sea Fury Warbird";
		var placeholder = "Avios Sea Fury Warbird";
		
		var keyword_source = ['Avios Sea Fury Warbird', 'Assault Reaper 3D Quad', 'Brushless Gimbal', 'TrackStar ROAR Approved', 'EFX Racer', 'RTF Quad', 'Quanum Venture FPV', 'DJI products', 'Italeri Plastic Model Kit', 'KK2'];
		var url_source = ['//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=Avios+Sea+Fury+Warbird&nearby=yes', '//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=Assault%20Reaper%203D%20Quad', '//www.hobbyking.com/hobbyking/store/__963__960__Multi_Rotors_Parts-Brushless_Gimbals.html', '//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=TrackStar%20Roar%20Approved', '//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=EFX%20Racer%20Model', '//www.hobbyking.com/hobbyking/store/__592__501__Multi_Rotors_Parts-RTF_PNF_ARF.html', '//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=Quanum%20Venture%20FPV', '//www.hobbyking.com/hobbyking/store/__1260__501__Multi_Rotors_Parts-DJI_Products.html', '//www.hobbyking.com/hobbyking/store/__1500__1504__Plastic_Model_Kits-1_48_Scale_Aircraft.html', '//www.hobbyking.com/hobbyking/store/RC_PRODUCT_SEARCH.asp?strSearch=kk2.1.5'];
		
		var keyword_url_mapping = {};
		
		for(i=0;i<keyword_source.length;i++)
		{
			keyword_url_mapping[keyword_source[i]] = url_source[i];
		}
		//alert($("input[name='strSearch']").attr('name'));
		var new_suggestion = $("input[name='strSearch']").autocomplete({
			source: keyword_source,
			minLength: 0,
			select: function(event, ui)
			{
				var go_to = keyword_url_mapping[ui.item.value];
				//alert(go_to);
				location.href = go_to;
			},
			focus: function(event, ui)
			{
				var selectedObj = ui.item;              
				$(ui.item).css({"font-size":"100px!important"});
			}
		});
		
		$( "#ui-id-1" ).css({background:"none",backgroundColor:"rgba(0, 0, 0, 0.6)"});

		$("input[name='strSearch']").click(function()
		{
			var c_value = $.trim($(this).val());
			//var c_value = $.trim(this.value);
			
			//var type = $(".dropdown-menu").find("[aria-labelledby='search-product-list']").attr("class");
			if(($.inArray(c_value, keyword_source)!=-1) || (c_value==""))
			{
				if($("#search_result_menu").css("display") == "block")
				{
					$(this).autocomplete("disable");
				}
				else
				{
					$(this).val("");
					$(this).autocomplete("enable");
				}
			}
			else if(c_value != "")
			{
				$(this).autocomplete("disable");
			}

			$(this).attr("placeholder","");
			$(this).autocomplete("search", "");
			
			$(".ui-corner-all").css({'color':"white",
									 'font-size':"14px"
									});
			$(".ui-state-focus").css({'backgroundColor':"rgba(0, 0, 0, 0.8)"});
		});
		
		$("input[name='strSearch']").focusout(function()
		{
			var c_value = $.trim($(this).val());
			if(c_value=="")
			{	
				$(this).autocomplete("enable");
				//this.value = default_value;
				$(this).val(default_value);
			}
			$(this).attr("placeholder", placeholder);
		});

		$("input[name='strSearch']").keypress(function()
		{
			$(this).autocomplete("disable");
		});
	});

</script>
