            <input id="calWidgetSearchDate" type="hidden" name="d" value="" />
            
            <input type="hidden" name="s" value="start_time" />             
            <input type="hidden" name="sd" value="asc" />                         
            <input type="hidden" name="unrolled" value="1" />   
            
            <div id="calendar-widget"></div>
        </form><div id="calendar-widget-buttons">
            
            <a href="/calendar/search/?advancedFormState=off&amp;unrolled=1&amp;&amp;s=start_time&amp;sd=asc&amp;l=10&amp;d=02/19/2015" title="view today's events" class="today" rel="nofollow">
                today's events
            </a>
            <a href="/calendar" title="browse events" class="browse">
                browse
            </a>
            <a href="https://roanoke-dot-com.bloxcms-ny1.com/users/admin/calendar/event/?referer_url=/" title="submit your event" class="event-submit" rel="nofollow">
    submit
    </a>
