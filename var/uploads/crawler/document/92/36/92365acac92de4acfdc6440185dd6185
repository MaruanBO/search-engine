<script>
  function doFacebookLogin(userId, accessToken)
  {
    document.getElementById('fbUserId').value = userId;
    document.getElementById('fbLoginForm').submit();
  }

  window.fbAsyncInit = function() {
    // init the FB JS SDK
    FB.init({
      appId      : '113172505507830', // App ID from the App Dashboard
      channelUrl : '//fsguestsfirst.com/channel.html', // Channel File for x-domain communication
      status     : false, // check the login status upon init?
      cookie     : true, // set sessions cookies to allow your server to access the session?
      xfbml      : true  // parse XFBML tags on this page?
    });

    // Additional initialization code such as adding Event Listeners goes here
    FB.Event.subscribe('auth.statusChange',
        function(response)
        {
            if (response.status === 'connected')
            {
                doFacebookLogin(response.authResponse.userID, response.authResponse.accessToken);
            }
        }
    );

  };

  // Load the SDK's source Asynchronously
  (function(d){
     var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
     if (d.getElementById(id)) {
        return;
     }
     js = d.createElement('script'); js.id = id; js.async = true;
     js.src = "//connect.facebook.net/en_US/all.js";
     ref.parentNode.insertBefore(js, ref);
   }(document));
   
</script>

