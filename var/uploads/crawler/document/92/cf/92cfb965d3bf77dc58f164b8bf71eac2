            if (!totalSavingsCounted) {
                $(function () {
                    var delays = [400, 1000, 1800, 2200, 2600];
                    $('ul.points li').addClass('pointless').each(function (i) {
                        setTimeout(function (li) {
                            li.addClass('show');
                        }, delays[i], $(this));
                    });
                    $({price: 15}).animate({price: 49}, {delay: 1000, duration: 4000, easing: 'linear', step: function () {
                        $('#counter').text("$" + Math.ceil(this.price) + ".00").addClass("show");
                    }});
                    totalSavingsCounted = true;
                });
            }
