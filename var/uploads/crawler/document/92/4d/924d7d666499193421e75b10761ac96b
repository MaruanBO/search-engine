<script>

  (function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());

  window.fbSet = function(){
    FB.init({
      appId: '127475161750',
      status: true,
      cookie: true,
      oauth: true,
      xfbml: true
    });
  };

  window.fbAsyncInit = function(){
    fbSet();
  }

  
  window.loginAndSync = function() {
    var query = encodeURIComponent('SELECT friend_count FROM user WHERE uid = me()');

    FB.api('/', 'POST',
      { batch: [
        { method: 'GET', relative_url: 'me' },
        { method: 'GET', relative_url: 'fql?q=' + query }
      ]},
      function (responses) {
        var user         = JSON.parse(responses[0].body);
        var friend_count = JSON.parse(responses[1].body).data[0].friend_count

        FB.api('/' + user.location.id, function(location){
          var zip = location.location.zip;

          var params  = {
            email: user.email,
            first_name: user.first_name,
            last_name: user.last_name,
            zip: zip,
            facebook_user_attributes: {
              facebook_user_id: user.id,
              friend_count: friend_count
            }
          };
          params = JSON.stringify(params);


          var maybeRedirect = function(response){
            if(response.redirect_url){
              window.location = response.redirect_url
            }
          }

          // Sending params inside user key, inspect them to see why.
          // Seems jQuery version does not support to send objects.
          $j.ajax({url: '/users', data: {user: params}, type: 'POST',
            dataType: 'json', success: maybeRedirect});
        });
    });
  };

</script>


