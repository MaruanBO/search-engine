<script type="text/javascript">
		$(function() {
			if (getCookie('STOREUSERNAME') !== '') {
				$('#vscUserName').val(getCookie('STOREUSERNAME'));
			}

			$('#vscLoginFormSubmit').click(function() {
				if (($('#vscUserName').val() === '') && ($('#vscPassword').val() === '')) {
					$('#vscError').html('<b>Login Failed </b> <br> Please enter a User Name and Password.');
					$('#vscError').show();
					$('#vscUserName').focus();
					return false;
				} 
				
				if ($('#vscUserName').val() === '') {
					$('#vscError').html('<b>Login Failed </b> <br> Please enter a User Name.');
					$('#vscError').show();
					$('#vscUserName').focus();
					return false;
				}  		
				
				if ($('#vscPassword').val() === '') {
					$('#vscError').html('<b>Login Failed </b> <br> Please enter a Password.');
					$('#vscError').show();
					$('#vscPassword').focus();
					return false;
				}  
					
				if (nyl.cda.vsc.login.IsNumeric($('#vscUserName').val()) && nyl.cda.vsc.login.IsNumeric($('#vscPassword').val())) {
					$('#vscForm').attr('action', 'https://www.vsc3.newyorklife.com/VSCRegWebApp/login');
				  	$("#vscForm").submit();
				} else {
					if ($("#rememberUserName").attr('checked')) {
						setCookie('STOREUSERNAME', escape($('#vscUserName').val()));
					}
					
					$('#USER').val($('#vscUserName').val());
					$('#PASSWORD').val($('#vscPassword').val());
					$('#vscForm').attr('action', 'https://www.vsc3.newyorklife.com/siteminderagent/forms/VSCLogin.fcc');
					$("#vscForm").submit();
				}
			});

			$('#vscNewUser').click(function() {
				location.href = 'https://www.vsc3.newyorklife.com/VSCRegWebApp/register?Step=1';
			});
		});
	</script>

