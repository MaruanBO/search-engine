	<script type="text/javascript" src="/menswear/JABAssetStore/static/scripts/jquery.cycle.js"></script>
	<script type="text/javascript">
		$().ready(function(){
			$('#switch-it').after('<div id="switch" class="switch">').cycle({fx:'fade', speed:'1000', timeout:6000, height:'319px', pager:'#switch', pause:1});
			$('#switch').prepend('<div class="play-control">');
			$('#switch .play-control').click(function(){
				if($(this).hasClass('paused')){
					$('#switch-it').cycle('resume');
					$(this).removeClass('paused');
				}
				else{
					$('#switch-it').cycle('pause');
					$(this).addClass('paused');
				}
			});
		});
	</script>
	
