function showDOD() {
	if(navigator.appName != "Microsoft Internet Explorer"){
		document.getElementById('DODImg').style.display = 'block';
	}
}

function hideDOD() {
	if(navigator.appName != "Microsoft Internet Explorer"){
		document.getElementById('DODImg').style.display = 'none';
	}
}

</script>

