<script type="text/javascript">


function handleEnterKey (field, event) {
		var keyCode = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;
		if (keyCode == 13) {
			/*var i;
			for (i = 0; i < field.form.elements.length; i++)
				if (field == field.form.elements[i])
					break;
			i = (i + 1) % field.form.elements.length;
			field.form.elements[i].focus();*/
			var val = field.value ;
		//	alert(val);
			modifySearchTerm(val, field.form);
			search(field.form);
			return true;
		} 
		else
		return true;
	}  
	//Added for removing first 2 letters from letter-letter-numbers combination from search term.
	function modifySearchTerm(value, form)
	{
	var key=value;
	if(key !=null && key != '')
			{
				 if(key.match("^[A-Za-z][A-Za-z][0-9]+"))
				 {
				 	var end=key.length;	
				 	var numKeyword= key.substring(2,end);
				 	form.searchKeywords.value=numKeyword;
				 	return true;
				  }	 
				  else
				  return false;				  
			}
	}
function search(form){


	createSearchTermCookie(form.searchKeywords.value);
	form.submit();
// }
}



</script>


	
	
