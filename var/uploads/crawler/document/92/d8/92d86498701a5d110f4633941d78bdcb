<script type="text/javascript">
 
  var topCatId=null;
  var catId=null;
  catId= "1000";
	
	$(function()
	 {	
	  topCatId="";
		
		 //only for public site
		if(!dc_user)
		{		
			var catId="1000";			
			//handle all the pages except search & topNav Categories
			if(document.location.href.indexOf("BSAOmnifindQueryCmd") == -1)	
			{
				//pass categoryId 					
				 if((topCatId !='') && (document.location.href.indexOf("Product")== -1))	
					cmCreatePageviewTag(document.title,topCatId);			
								
				//pass ProductId	
				else if (document.location.href.indexOf("Product")!= -1)						
					cmCreatePageviewTag("Product : "+document.title+"()","");					
							
				//pass catId from X_CMCATEGORY table				
				else if(catId !='')	
				{
					if(document.location.href.indexOf("JoinEmailEntryView")!= -1)
						cmCreatePageviewTag("Email Signup","1000");
					else if(document.location.href.indexOf("SweepstakesEntryView")!= -1)
						cmCreatePageviewTag("Sweepstakes Signup","1000");
					else								
						cmCreatePageviewTag(document.title,"1000");
					}
				 }
			}
		}
	);	
</script>

