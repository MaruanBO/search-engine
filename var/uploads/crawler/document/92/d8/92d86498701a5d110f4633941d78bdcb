<script type="text/javascript">
function createSearchTermCookie(value){
		createCookie('searchTerm',value,1);
}



function setText(theText) 
{

var min=2;

	if(theText.value == ""){
         theText.value = "Search";
         theText.style.color="#666666";
    }
    
 }

function doClear(theText) 
{
	 if (theText.value == theText.defaultValue)
     {
	 	 theText.style.color="#000000";
         theText.value = ""
     }
    
 }
 function submitSearchBox(form){
 	var box2 = form.searchKeywords.value;
	 if(modifySearchTerm(box2))
	 {
		 form.submit();
	 }
	 else if (box2 != null && box2 != '')
	 {	
		form.submit();
	 }
	}
