	<script type="text/javascript">
			
		var intlShipEnabled = true;
			
		function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}				
		 var isRememberMeCookiePresent = readCookie("WCX_RMBRME");
		 var intCookie = readCookie("isIntl_cookie");
		 var countryCodeCookie = readCookie("country_cookie");
		 var countryNameCookie = readCookie("country_name_cookie");
    		if(intlShipEnabled && ( intCookie == null || intCookie === 'undefined' 
    				|| countryCodeCookie == null || countryCodeCookie === 'undefined'
    				|| countryNameCookie == null || countryNameCookie === 'undefined')){
				$.ajax({
					url: "SetIntlSession?storeId=11001&catalogId=10050",			
				  	cache: false,
				  	async: false,
				  	success: function(result){
					  	intCookie = readCookie("isIntl_cookie");
				  		countryCodeCookie = readCookie("country_cookie");
				  		countryNameCookie = readCookie("country_name_cookie");  					
  					}					
			  	});	   			
