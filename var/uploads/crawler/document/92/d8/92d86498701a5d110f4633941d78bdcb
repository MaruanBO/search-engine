				function SweepFormSubmit()
				{
				if(checkAllErrors(13))
				{			
					if(!dc_user)
					{				
						//coremetrics Registration tag cookie for Sweepstakes 							
						createCookie("coremetrics_Sweepstakes_cookie",  document.getElementById('email11').value ,null);		
					}
					document.JoinSweepstakesForm.submit();
				}
			
				}
				function clearDefaultValue(element)
					{
						if (element.value == 'Email Address')
							element.value = '';
					}
				
