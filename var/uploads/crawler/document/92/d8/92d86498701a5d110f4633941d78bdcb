    		if(countryNameCookie == null || countryNameCookie === 'undefined' ){
    			countryCodeCookie = 'US';    		
    			countryNameCookie = 'USA';
    		}
    		if (intCookie==null || intCookie == "" || intCookie === 'undefined'){
    			intCookie = 'FALSE' ;
    		}else{
    			intCookie=intCookie.toUpperCase();
    		}
    		if (!intlShipEnabled) {
    			intCookie = 'FALSE' ;
    		}

				if (guest_user) {
