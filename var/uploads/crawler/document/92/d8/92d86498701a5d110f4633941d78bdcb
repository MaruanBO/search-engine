<script type="text/javascript">
/*
  Close active window automatically if the user have the window open but do nothing within 25 minutes. 
*/
function getCookie(NameOfCookie){
    if (document.cookie.length > 0) {              
    begin = document.cookie.indexOf(NameOfCookie+"=");       
    if (begin != -1) {           
      begin += NameOfCookie.length+1;       
      end = document.cookie.indexOf(";", begin);
      if (end == -1) end = document.cookie.length;
        return unescape(document.cookie.substring(begin, end));
    } 
  }
  return null;
}
var getSecond = 0;
function CheckWindowRunningTime(){  
var directConnectFlag = getCookie('directConnect');

var isValidStoreFlag = getCookie('isValidStore');
	if (directConnectFlag == 'true'){ 
		if (directConnectFlag == 'true'){ 
		    getSecond++;   
		    window.status = getSecond;
		    if (getSecond == 1800) {  
			var winMsging ="A session timeout has occurred.  Please click OK to close this window.";     
			alert(winMsging); window.open('','_self','');window.opener='x'; window.close();
			return false;
		    }  
		    window.setTimeout("CheckWindowRunningTime();", 1000);
		}
 	} else {

 	}
}
CheckWindowRunningTime();

function JAB_handle_Error_OnEachWindow(){return true;}
window.onerror=JAB_handle_Error_OnEachWindow;
</script>

