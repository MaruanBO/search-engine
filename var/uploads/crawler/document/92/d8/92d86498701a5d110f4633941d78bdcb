<script>

var intlShipEnabled = true;

String.prototype.trim = function () {
	return this.replace(/^\s*/, "").replace(/\s*$/, "");
}
function DisplayMiniCart(){
	var miniCartCookie = readCookie("WCX_MINICART");
	var totalAmt = 0.00;
	var totalItems = 0;
	var totalForFreeShipping = 75.00;
	var intlCookie = readCookie("isIntl_cookie") || '';
	var intlOrderTotalCookie = readCookie("intl_orderTotal_cookie") || '';
	var cartTotalAmt = '';
	
	if (intlCookie==null || intlCookie == "" || intlCookie === 'undefined'){
		intlCookie = 'FALSE' ;
	}else{
		intlCookie=intlCookie.toUpperCase();
	}
	if (!intlShipEnabled) {
		intCookie = 'FALSE' ;
	}	
		
	if (miniCartCookie != null && miniCartCookie.trim().length > 0)
	{
		var miniCartTotalsArray = miniCartCookie.split('|');
		totalAmt  = miniCartTotalsArray[0];
		totalItems = miniCartTotalsArray[1];
		totalForFreeShipping = miniCartTotalsArray[3];
	}
	if(intlOrderTotalCookie != "" && intlOrderTotalCookie!="null" && intlOrderTotalCookie.trim().length > 0 && intlCookie == 'TRUE' ){
		cartTotalAmt = unescape(intlOrderTotalCookie);
		
	}else{
		var userCurrencySymbol = "$";
		if(intlCookie == 'TRUE'){
			var userCurrency = getCookie('currency_cookie') || 'USD';
			var fxrate = getCookie('fx_rate_cookie');
			var price = parseFloat(totalAmt.replace(/[^0-9/.]+/g,''));						
		    var lcpCoeff =getCookie('lcp_coeff_cookie'); 
		    if (isNaN(lcpCoeff)) { 
		       lcpCoeff = 1; 
		    } 
		    var roundCoeff =getCookie('round_cookie');
			if (isNaN(roundCoeff)) {
				roundCoeff = 2;
			}
	    	if (isNaN(fxrate) || userCurrency == 'USD') {
		    	totalAmt = price.toFixed(2);
		    }else {
		    	totalAmt = price * fxrate * lcpCoeff;
		    	userCurrencySymbol = userCurrency;
		    	if (roundCoeff >= 0) {
					 totalAmt = parseFloat(totalAmt.toFixed(roundCoeff)).toFixed(2);
				} else if (roundCoeff== -1) {
					 totalAmt /= 10;
					 totalAmt = Math.round(totalAmt);
					 totalAmt *= 10;
					 totalAmt = totalAmt.toFixed(2);				
				} else if (roundCoeff== -2) {
					 totalAmt /= 100;
					 totalAmt = Math.round(totalAmt);
					 totalAmt *= 100;
					 totalAmt = totalAmt.toFixed(2);			
				}        
		    }
		    if(window.location.href.indexOf("source=PLA")>0)
		  
		       userCurrencySymbol = "$";
		      
		}
	 	cartTotalAmt =userCurrencySymbol + " "+totalAmt ;
	}
	var mcHtml = '';
	mcHtml += "<span id='my_cart' class='last'><span class='uline'>My Cart</span>: ";
	mcHtml += "<span id='cookieCartTotal'>"+ cartTotalAmt + "</span>";
		
	mcHtml += "<span id='cookieCartQuantity'>";
	if (totalItems > 1)
		mcHtml += " ("+totalItems+ " items)";
	else
		mcHtml += " ("+totalItems+ " item)";
	mcHtml += "</span>";
	if (intlCookie != null && intlCookie.trim() == 'FALSE') {
	mcHtml += "<span id='free_shipping' class='freeShip'><span id='cookieFreeShipping'>";

	if (75.00 == 0)
		mcHtml += "FREE SHIPPING, NO MINIMUM ORDER!";
	else if (parseInt(totalForFreeShipping.valueOf()) <= 0)
		mcHtml += "You have qualified for Free Shipping";
	else
		mcHtml += "$"+totalForFreeShipping+" until Free Shipping";
	mcHtml += "</span>";
	}
	mcHtml += "</span></span>";
	$('#branding_cart').html(mcHtml);
}

$(window).load(function() {DisplayMiniCart();});
</script>
	
													
