		<script language="javascript">
			$(document).ready(function() {
				$(".help-block").hide();
				$(":input").on('focus', function(){
					if ($(this).parent().find('.help-block')){
						$(this).parent().find('.help-block').slideDown("fast");
					}
				})
				.on('blur', function(){
					if ($(this).parent().find('.help-block')){
						$(this).parent().find('.help-block').slideUp("fast");
					}
				});
				var bar = $('#headerSlideContainer');
				$('#headerSlideContent').html($('#header').html());
				var top = bar.css('top');
				$(window).scroll(function() {
					if($(this).scrollTop() > 50) {
						bar.stop().animate({'top' : '0px'}, 500);
					} else {
						bar.stop().animate({'top' : top}, 0);
					}
				});
			});
		</script>       
        
