            <script type="text/javascript">
                //<![CDATA[
                function toggleForm() {
                    $("#optInSignUpPanel").toggle(500);
                    $("#optInResult").toggle(500);
                }

                function verifyForm(emailAddress, fullName) {
                    $("#newsletter-frmValidations").empty();
                    var bSubmit = true;

                    if (!(fullName)) {
                        $("#optInSignUpPanel #newsletter-frmValidations").append("<p>Name is required</p>");
                        bSubmit = false;
                    }                                                
                    if (!(emailAddress)) {
                        $("#optInSignUpPanel #newsletter-frmValidations").append("<p>Email Address is required</p>");
                        bSubmit = false;
                    }

                    if (bSubmit) {
                       submitSignUp(emailAddress, fullName);
                    }
                }

                function submitSignUp(email, name) {
                    $("#optInResult").html('Signing up...');

                    toggleForm(); // show loading

                    var proxyURL = '';
                    var thisUrl = document.URL;
                    $.ajax({
                        url: '/account/callnewslettersubscription?subscribe=true&email=' + email + "&fullName=" + name + '&proxyAddress=' + proxyURL,
                        type: "GET",
                        dataType: "text",
                        data: {
                            isSubscribe: true,
                            email: email,
                            fullName: name,
                            proxyAddress: proxyURL
                        },
                        success: function (data) {
                            //alert("data: " + data);
                            if (data != 'undefined' && data.toLowerCase() == "success") {
                                $("#optInResult").html(' <br /><b>Thanks!</b>');
                            } else {
                                $("#optInResult").html('The request failed. <br /><a href="javascript:toggleForm()"><u>Try again.</u></a>');
                            }                          
                        },
                        error: function (xhr, status) {
                            $("#optInResult").html('The request failed. <br /><a href="javascript:toggleForm()"><u>Try again.</u></a>');                            
                        }
                    });
                }

                //]]>
            </script>

