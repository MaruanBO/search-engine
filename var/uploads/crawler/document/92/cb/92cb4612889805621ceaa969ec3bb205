                source : function(request, response) {
					$.ajax({
						url : "http://auto.liveauctioneers.com/ajax_scripts/suggest.php",
                        dataType: "jsonp",
                        data : {
                            featureClass: "P",
                            style: "full",
                            maxRows: 12,
                            term : request.term
                        },
						error: function(xhr,err ) {
                            //console.log( xhr );
                            //console.log(err);
                        },
                        success : function(data) {//TODO: after release of aliases fix auction links in response
                            raw = data;
                            //alert(data);
                            //console.log(data);
                            response(data.suggestions);
                            makePullLinks($("ul.ui-autocomplete a"));
                        }
					});
				},
				minLength : 2,
				select : function(event, ui) {
					//console.log(ui.item ? "Selected: " + ui.item.label : "Nothing selected, input was " + this.value);
				},
				open : function() {
					$(this).removeClass("ui-corner-all").addClass("ui-corner-top");
				},
				close : function() {
					$(this).removeClass("ui-corner-top").addClass("ui-corner-all");
				}
				//,       appendTo:"#frm_search"
			});
			

                        })
