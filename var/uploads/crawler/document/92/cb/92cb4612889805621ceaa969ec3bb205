<script type="text/javascript">
if (document.location.pathname.indexOf('/all-') > -1) {
    var infinite = true;
    reached = false;
    curr_offset = 2;
    live = 1; // just in case
    switch (document.location.pathname) {
        case '/all-auctions':
            live = 1;
            break;

        case '/all-upcoming-auctions':
            live = 0;
            break;
    }

    get_data = getQueryData(document.location.href);
    if (get_data['view'] == 'list') {
        viewtype = 'list';
        lastauct = 'auct_view_items_wrapper';
    }

    else {
        viewtype = 'grid';
        lastauct = 'auct_box_block';
    }

    $(window).scroll(function() {
        $footer_height = $('.acn-news-holder').height() + $('.bid-items-container').height() + $('.footer-container').height();
        if ($(window).scrollTop() + $(window).height() >= $(document).height() - $footer_height) {
            if (!reached  && infinite == true) {
                reached = true;
                // do ajaxy things here
                $.ajax({
                    async: true,
                    cache: false,
                    type: 'GET',
                    url: '/ajax_scripts/infinite_scroll.ajax.php?pagenumoffset=' + curr_offset + '&live=' + live + '&view=' + viewtype,
                    dataType: 'json',
                    success: function(data) {
                        if (data.status == 'success') {
                            $('.' + lastauct).last().after(data.more_stuff);
                            ++curr_offset;
                        }

                        else {
							if (typeof console !== "undefined" && typeof console.log !== "undefined")
								console.log(data.message);
                        }
                    }
                });
            }
        }

        else {
            if (reached) {
                reached = false;
            }
        }
    });
}else{
    var infinite = false;
}
</script>



<script type="text/javascript">
// <![CDATA[

function checkEmail(emaddr) {
    if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(emaddr.value)){
        return (true)
    }
    return (false)
}
function verifyandsend(){     
    var myemail   = document.getElementById("newsletteremail");
    if(checkEmail(myemail)){
        var data2= 'email='+myemail.value+'&amp;CustomFields[1]='+myemail.value+'&amp;CustomFields[9]=signup';

        $.ajax( {async: false, type: "POST", url: "/ajax_scripts/profile_functions.php?function=add_email_to_nl" , data: data2 });
        document.getElementById("newsletterform").innerHTML="<div class='verifyandsend'>Thank You<br />for Subscribing</div>";
        document.getElementById("submitbutton").innerHTML="";
    }else{
        alert("Invalid E-mail Address! Please re-enter.")
        return false;
    }
}

// ]]>
</script>
             
    <script>
        $(function() {
            $('div.jcarousel-skin-tango').click(function() {
                _gaq.push(['_trackEvent', 'Navigation', 'Sliding calendar', 'Home page']);
            });
            $( "#highlight_sel_dragger" ).draggable({ axis: "x",
                containment: "#highlight_boundary",
                grid: [ 66, 66 ],
                scroll: false,
                start: function() {
                    jQuery.data(document.body, "date drag start offset", getHighlightPosition());
                    jQuery.data(document.body, "date drag revert position", document.getElementById('highlight_sel').style.left);
                },
                drag: function() {
                    document.getElementById('highlight_sel').style.left = document.getElementById('highlight_sel_dragger').style.left;
                },
                stop: function() {
                    triggerDateLink();
                }
            });

            // return the current left-position of the date highlighter
            function getHighlightPosition() {
                return parseInt($("#highlight_sel").offset().left) + 10;
            }

            /* pick out which carousel item we're hovering over,
             and then trigger its "click" method. */
            function triggerDateLink() {
                var oldHighlightPosition= jQuery.data(document.body, "date drag start offset");
                var highlightPosition= getHighlightPosition();

                if (highlightPosition == oldHighlightPosition) // if we didn't drag anywhere, do nothing.
                    return;

                $("#mycarousel").children("li").each(
                        function() {
                            var thisChild = $(this);
                            if (Math.abs(parseInt(thisChild.offset().left) - highlightPosition) < 10) {
                                if (thisChild.children("div").attr("onclick") === undefined) {
                                    // if the link wouldn't do anything, bounce the highlight back to its original position
                                    var oldLeftPosition = jQuery.data(document.body, "date drag revert position");
                                    $("#highlight_sel").animate({left:oldLeftPosition},"fast", "swing");
                                    document.getElementById('highlight_sel_dragger').style.left= oldLeftPosition;
                                }
                                else {
                                    // otherwise, trigger it.
                                    thisChild.children("div").trigger("click");
                                }
                            }

                        }
                )
            }
        });
        $(function() {
            var orig_grid_btn = $( "#grid_btn" ).attr( 'src' );
            var orig_list_btn = $( "#list_btn" ).attr( 'src' );
            
            $( "#grid_btn" ).mouseover(function() {
                $( this ).attr( 'src', '/static/images/gridview_active.png' );
                $( "#list_btn" ).attr( 'src', '/static/images/listview_other.png' );
            })
            .mouseout(function() {
                $( this ).attr( 'src', orig_grid_btn );
                $( "#list_btn" ).attr( 'src', orig_list_btn );
            });
            $( "#list_btn" ).mouseover(function() {
                $( this ).attr( 'src', '/static/images/listview_active.png' );
                $( "#grid_btn" ).attr( 'src', '/static/images/gridview_other.png' ); 
            })
            .mouseout(function() {
                $( this ).attr( 'src', orig_list_btn );
                $( "#grid_btn" ).attr( 'src', orig_grid_btn );
            });

            $('.auction_box .box_truncate').dotdotdot();
        });
    </script>

