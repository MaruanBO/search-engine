<script type="text/javascript">
$(function() {
    add_onload = function(){
        if( $('.item_box .box_truncate').length ){
            $('.item_box .box_truncate').each(function(){
                $(this).dotdotdot( {height: 32} );
            });
        }
        if( $('img.delayed_load').length ){
            $('img.delayed_load').each(function(){
                $(this).attr('src', $(this).attr('data-delay-src'))
        })};    
    }
    if ( typeof window.addEventListener != "undefined" )
        window.addEventListener( "load", add_onload, false );
    else
        window.onload = add_onload;
    
    
});


</script>

