				   function openWindow(pageUrl) {
						var winName = Math.round(9999*Math.random()) + new Date().getTime();
						var winNew = window.open(pageUrl, winName);	
						if(!winNew){ // opening window failed (Safari 3)
							if(document.getElementById("FlashID2")){
								document.getElementById("FlashID2").openWindowFromSwf(pageUrl);
							} else {
								return false;
							}
				   		 }else{
							winNew.focus();
							return true;
					  	}
					}

					function openCatalog(pageUrl, pageConfig) {
