<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		// Process P tag issue
		$(".wnDVSummary").each(function(index) {
			var oddPText = '';
			$(this).find("p").each(function(index) {
				if (index > 0) {
					oddPText = oddPText + $(this).text() + "<br>";
				}
			});
			if ($(this).find(".iceSummary").text() == '') {
				$(this).find(".iceSummary").html(oddPText);
			}
		});

		// Load story data
		var leftData = $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		var rightData = $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
		if (leftData.length < 1 || rightData.length < 1) {
			return;
		}

		// Build left side
		var story = leftData[0];
		if (!story.graphicSrc) {
			story.graphicSrc = 'http://ftpcontent.worldnow.com/kbmt/custom/2012/default_image.jpg';
		}
		story.custDate = (new Date(story.dateText)).customFormat('%MMMM% %D%, %YYYY%');
		story.hasVideo = (story.isClip || story.isVideo);
		$wn('#csTsa .csTsaLeft').csFormatterHtml('{if $graphicSrc}' + '<a href="{$headlineHref}"><img class="csFeatureImage" src="{$graphicSrc}"></a>' + '{/if}' + '<h4>' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}<img class="csHasVideo" src="http://kbmt.images.worldnow.com/images/static/container-e/icon_video_included.gif">{/if}' + '</h4>' + '{if $custDate}<div class="csDate">{$custDate}</div>{/if}' + '<p>{$summaryHtml}</p>', story);

		// Build right side
		$wn.each(rightData, function(i) {
			if (i >= 8) {
				return false;
				//break
			}

			// Slideshow Trigget Label
			if (this.sectionText.match(/slideshow/i)) {
				this.slideshow = true;
			}

			this.custDate = (new Date(this.dateText)).customFormat('%MMMM% %D%, %YYYY%');
			this.hasVideo = (this.isClip || this.isVideo);
			$wn('#csTsa .csTsaRight ul').csFormatterAppend('<li>' + '<h4>' + '{if $slideshow}<img class="csSlideshow" src="http://ftpcontent.worldnow.com/wncustom/custom/kbmt/2012/images/slideshow.png">{/if}' + '<a href="{$headlineHref}">{$headlineText}</a>' + '{if $hasVideo}<img class="csHasVideo" src="http://kbmt.images.worldnow.com/images/static/container-e/icon_video_included.gif">{/if}' + '</h4>' + '{if $custDate}<div class="csDate">{$custDate}</div>{/if}' + '</li>', this);
			this.$self.remove();
		});

		$wn('#csTsa .csTsaRight li').last().addClass('csLast');

		// Move the TSA into place, hide original sources
		$wn('#csTsa').parent().hide();
		$wn('#WNTSWContent').before($wn('#csTsa'));
		$wn('#WNTSWContent').hide();

		// Is Col2 empty now?
		if ($wn('#WNCol2 .wnDS-20 .wnDVSummary').length < 1) {
			$wn('#WNCol2 .wnDS-20').hide();
		} else {
			$wn('#WNCol2 .wnDS-20 .wnDSHeader h3 span').text('More Top Stories');
		}
	}, 'WNCol23done'); 
</script>

