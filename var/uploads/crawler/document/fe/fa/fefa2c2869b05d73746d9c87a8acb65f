			appendSnippet += amp + "searchTerm=" + encodeURIComponent($("#searchbox__searchbox_WAR_sitesearchportlet_ #searchTerm").val());
			appendSnippet += amp + "scopeType="  + $("#searchbox__searchbox_WAR_sitesearchportlet_ #scopeType").val();
			appendSnippet += amp + "sortType="   + $("#searchbox__searchbox_WAR_sitesearchportlet_ #sortType").val();
		}		
		window.location = url+appendSnippet;
	}
	$('#searchbox__searchbox_WAR_sitesearchportlet_ #searchbox_form').submit(function(e) {
		e.preventDefault();
		submit_searchbox_WAR_sitesearchportlet_Form();
	});
/*]]>*/</script> </div> </div> </div> </div> </div> <div class="container"></div> <div id="main" class="container" data-role="content"> <div id="p_p_id_103_" class="portlet-boundary portlet-boundary_103_ portlet-static portlet-static-end portlet-borderless " > <span id="p_103"></span> <div class="portlet-body"> </div> </div> <div class="page page__home" id="main-content" role="main"> <div class="layout__container area__full-col--top area__break--first"> <div class="layout__content portlet-area portlet-area--1" id="column-1"> <div class="portlet-dropzone empty portlet-column-content" id="layout-column_column-1"></div> </div> <div class="layout__side portlet-area portlet-area--2" id="column-2"> <div class="portlet-dropzone empty portlet-column-content" id="layout-column_column-2"></div> </div> </div> <div class="layout__container area__full-col--middle portlet-area portlet-area--3 area__break--second" id="column-3"> <div class="portlet-dropzone portlet-column-content" id="layout-column_column-3"> <div id="p_p_id_56_INSTANCE_u677qF0ZBHJ4_" class="portlet-boundary portlet-boundary_56_ portlet-static portlet-static-end portlet-borderless portlet-journal-content " > <span id="p_56_INSTANCE_u677qF0ZBHJ4"></span> <div class="portlet-body"> <div class="portlet-borderless-container" style=""> <div class="portlet-body"> <div class="journal-content-article" id="article_10155_130949115_617169751_1.1"> <div class="layout__container area__full-col--top area__break--first"> <div class="layout__content"> <!-- DEBUG --> <!-- groupId : 130949115 --> <!-- request.theme-display.language-id : en_US --> <!-- objThemeDisplay : com.liferay.portal.theme.ThemeDisplay@11a6e67c --> <!-- httpServletRequest : com.liferay.portal.kernel.servlet.ProtectedServletRequest@3cb11c1c --> <!-- serviceContext : com.liferay.portal.service.ServiceContext@fac9165 --> <!-- moduleID : 237690004 --> <!-- ServerTime: 16:32 --> <!-- Day of Week: Thu --> <!-- Day Options: [Mon, Tue, Wed, Thu, Fri] --> <!-- Day Optio