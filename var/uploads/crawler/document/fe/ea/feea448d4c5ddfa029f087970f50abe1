  jQuery(document).ready(function($) {
         // newsletter subscription
        var repword=$('#repword');
        $('#nlsubmit').click(function(){
          $.ajax({
            url:'/home/news/newsletter/main_insert.php',type:'post',data:$('#nletter').serialize(),
            success:function(r){
              repword.css('display','block');
              if(r==3)repword.html('<span class="arr">Your email couldn\'t be accepted. Please contact us.</span>');
              if(r==4)repword.html('<span class="arr">You have already subscribed. Thank you.</span>');
              if(r==2)repword.html('<span class="arr">Please provide a valid email address.</span>');
              if(r==1)repword.html('<span class="arr">Thank you for your subscribing.</span>');
            }
          });
  });
    });
