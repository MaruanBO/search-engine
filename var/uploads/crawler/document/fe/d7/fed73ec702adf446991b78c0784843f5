<script>
  $('.x_focus_overlay')
  .bind('onLoad', function() {
    var overlay = $(this).data('overlay').getOverlay();
    overlay.find(':input:visible:enabled:first').focus();
  })
  .bind('onClose', function() {
    var overlay = $(this).data('overlay').getOverlay();
    overlay.find('.error-explanation').remove();
  });
  $('.chatlink').click(function() {
      window.open('/index.cfm?action=gmschat.main','_blank','width=560, height=750, status, resizable',''); 
  });
</script> 






