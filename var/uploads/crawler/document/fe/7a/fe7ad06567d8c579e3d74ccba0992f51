		(function($) {
			Drupal.behaviors.mymodule = {
				attach : function(context, settings) {
					$("#edit-sa--2").click(function() {
						tip = "Enter Keywords or Ticker Symbol";
						if ($("#edit-query--2").val() != "" && $("#edit-query--2").val() != tip) {
							$("#google-cse-results-searchbox-form").submit();
						} else {
							return false;
						}
					});
					
					$("#edit-sa").click(function() {
						tip = "Enter Keywords or Ticker Symbol";
						if ($("#edit-query").val() != "" && $("#edit-query").val() != tip) {
							$("#google-cse-results-searchbox-form").submit();
						} else {
							return false;
						}
					});

					/*			  $("#edit-query--2").focus( function() {
					 $("#edit-query--2").val("");
					 $("#edit-query--2").keypress(function(event) {
					 if ( event.which == 13 ) {
					 $("#google-cse-results-searchbox-form").submit();
					 }
					 });
					 })	*/

$("#edit-sa--3").click( function() {
	if( $("#edit-query--2").val() == '' || $("#edit-query--2").val() == 'Enter Keywords or Ticker Symbol' ){
		return false;
	}
 });


					jQuery("#edit-query--2").bind("focus", function() {
						if (jQuery(this).val() == 'Enter Keywords or Ticker Symbol')
							jQuery(this).val('');

						$("#edit-query--2").keypress(function(event) {
							if (event.which == 13) {
								$("#google-cse-results-searchbox-form").submit();
							}
						});

					}).bind("mouseleave blur", function() {
						if (jQuery(this).val() == '' && !jQuery(this).is(":focus")) {
							jQuery(this).val("Enter Keywords or Ticker Symbol");
						}
					});
					
					jQuery("#edit-query").bind("focus", function() {
						if (jQuery(this).val() == 'Enter Keywords or Ticker Symbol')
							jQuery(this).val('');

						$("#edit-query").keypress(function(event) {
							if (event.which == 13) {
								$("#google-cse-results-searchbox-form").submit();
							}
						});

					}).bind("mouseleave blur", function() {
						if (jQuery(this).val() == '' && !jQuery(this).is(":focus")) {
							jQuery(this).val("Enter Keywords or Ticker Symbol");
						}
					});

				}
			};

		})(jQuery);

	
	
function user_login_submit() 
	{
		uname = document.getElementById('edit-name').value;
		upass = document.getElementById('edit-pass').value;

	    if (uname == "" || uname == null || uname.charAt(0) == ' ')
		   {
			    alert("Username/Password required !");
				return false;
		    }

	   if (upass == "" || upass == null || upass.charAt(0) == ' ')
		  {
		    alert("Username/Password required !");
		    return false;
          }

	    return true;
	}

