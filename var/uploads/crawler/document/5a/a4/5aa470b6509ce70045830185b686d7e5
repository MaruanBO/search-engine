<script>
var controlDiv = new Element('div', {'class':'control_buttons'}).inject(document.getElement('#sliderPoints'));
    for (var i = 1; i <= 9; i++)
    {
        new Element('span', {'class':'ce_noobSlide_button ce_noobSlide_controls', html:'<img src="/tl_files/theme_images/lila/slider_point_normal.png" border="0" />'}).inject(document.getElement('#sliderPoints .control_buttons'));
    }
var noobslide9;
window.addEvent('domready', function() {
	noobslide9 = new noobSlide({
		box: document.getElement('#test-slider .ce_noobSlide_container'),
		items: [0,1,2,3,4,5,6,7,8],
		mode:'horizontal',
		size: 957,
    	interval: 10000, 
    	fadeDuration: 1200,
    	autoPlay: 1    	
		,
		onWalk: function(currentItem,currentHandle){
    		this.handles.removeClass('active');
	  		currentHandle.addClass('active');

            this.handles.getElement('img').set('src','/tl_files/theme_images/lila/slider_point_normal.png')
            currentHandle.getElement('img').set('src','/tl_files/theme_images/lila/slider_point_active.png');
    	}
		,
		handles: $$('#sliderPoints .ce_noobSlide_controls')
 	});
//walk to item witouth fx
noobslide9.walk(0,false,true);

new MooSwipe(document.getElement('#test-slider .ce_noobSlide_container'), {
		onSwipeLeft: function() {
			noobslide9.next(this);
	  	},
		onSwipeRight: function() {
			noobslide9.previous(this);
		}
	});
});
</script>
		 	
		
		
