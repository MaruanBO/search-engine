<script type="text/javascript">
	(function () {
		var headerSearch = findId('headerSearch');
		var searchText = QuidsiGetElementsByClassName("headerSearch", "label", "search-text");
	    var searchInput = findId('searchInput');
	    searchInput.onfocus = function(){
	    	headerSearch.className = "header-search header-search-active";
	    	searchText[0].style.display = "none";
	    }
	    searchInput.onblur = function(){
	    	headerSearch.className = "header-search";
	    	if(searchInput.value != ""){
	            searchText[0].style.display = "none";
	        }else{
	            searchText[0].removeAttribute("style");
	        }
	    }
	})();
</script>
