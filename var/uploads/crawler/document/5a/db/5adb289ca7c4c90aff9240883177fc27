<script type="text/javascript">
        function resetNavBarWidth(){
        // exclude the width of holiday menu
        var holidayObj = QuidsiGetElementsByClassName("siteNav", "li", "holidayLi");
        var holidayLength = holidayObj.length;
        var holidayWidth = (holidayLength > 0) ? holidayObj[0].clientWidth : 0;
        var navBarWidth = 990 - holidayWidth;
            var navLink = QuidsiGetElementsByClassName("siteNav", "a", "siteNavLink");
        var navLinkLength = navLink.length - holidayLength;
            var oldItemsWidthTotal = 0;
            var firstItem = navLink[0];
            firstItem.style.paddingLeft = "0";
            for (var i = 0; i < navLinkLength; i++) {
                navLink[i].style.width =  navLink[i].children[0].style.width = "auto";
                oldItemsWidthTotal += navLink[i].children[0].clientWidth;
            };
            var agvDifferWidth = Math.floor((navBarWidth - oldItemsWidthTotal) / navLinkLength);
            var differWidth = agvDifferWidth * navLinkLength + oldItemsWidthTotal - navBarWidth;
            for (var i = 0; i < navLinkLength; i++) {
                var thisItemWidth = navLink[i].children[0].clientWidth;
                navLink[i].style.width = navLink[i].children[0].style.width = thisItemWidth + agvDifferWidth + "px";
                navLink[i].onclick = function () {
                    this.parentNode.children[1].style.visibility = "hidden";
                    this.parentNode.children[2].style.visibility = "hidden";
                };
            };
            firstItem.style.paddingLeft = 0 - differWidth + "px";
        }
        (function () { 
          resetNavBarWidth();
          //For SecNev Height
          var secNavContent = QuidsiGetElementsByClassName("siteNav", "div", "secNavContent");
            for(var i = 0; i < secNavContent.length; i++){
              var navigationList = secNavContent[i].children;
              var navigationListNum = navigationList.length;
              if(navigationListNum > 1){
                  navgationListLeftItemHeight = navigationList[0].offsetHeight;
                  navgationListRightItemHeight = navigationList[1].offsetHeight;
                  if(navgationListLeftItemHeight < navgationListRightItemHeight){
                    navigationList[0].style.height = navgationListRightItemHeight - 34 + "px";
                  }
              }
            } 
        })(window)
</script>
