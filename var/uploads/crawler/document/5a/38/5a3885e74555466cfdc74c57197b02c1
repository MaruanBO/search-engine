  <script type="text/javascript">
  // Create namespce for function if it doesn't already exist
  var app = app || {};
  app.analytics = app.analyltics || {};
  app.analytics.newsletterSignup = app.analytics.newsletterSignup || {};
  // Save any existing analytics
  var oldFire = app.analytics.newsletterSignup.fire;
  // Create new analytics `fire` function
  app.analytics.newsletterSignup.fire = function(data){
    // Fire any existing analytics
    if(oldFire){
      oldFire(data);
    }
    // Fire Convertro analytics
    $CVO = window.$CVO || [];
    $CVO.push([ 'trackEvent', {
        type: 'newsletter',
        id: null,
        amount: '1'
    }]);
  };
  </script>
  
  
  






