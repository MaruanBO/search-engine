<script type="text/javascript">
// Analytics code
/* I'm not a fan of placing the following code here,
but it's neccesary to keep the old analytics code working.
*/
app.URL = {
// Local variable
_localQS: function(){
var query_string = {},
query = window.location.search.substring(1),
vars = query.split("&"),
i = 0,
pair = [],
arr = [];
for (i = 0; i < vars.length; i++) {
pair = vars[i].split("=");
// If first entry with this name
if (typeof query_string[pair[0]] === "undefined") {
query_string[pair[0]] = pair[1];
// If second entry with this name
}
else if (typeof query_string[pair[0]] === "string") {
arr = [ query_string[pair[0]], pair[1] ];
query_string[pair[0]] = arr;
// If third or later entry with this name
}
else {
query_string[pair[0]].push(pair[1]);
}
}
return query_string;
},
// Method to access _localQS
get_param: function(query){
return (this._localQS[query] || '');
},
// Returns the relative page, ie: Account-StartRegister, animals-pets-magazines, Search-Show?q=perfect
// Used as the search parameter we send to Google when the page is not actually a search page (ie: a category or landing page)
get_page: function(){
var loc = window.location.href,
locArr = loc.split('/');
return locArr[locArr.length-1].split(',')[0];
}
};
</script>




