<script type="text/javascript">
jQuery(document).ready(function() {
	jQuery('#footer-signup-input-email').focus(function() {
		if(jQuery(this).val() == "Enter Your Email Address") {
			jQuery(this).val('');
		}
	});
	
	jQuery('input#emailOptIn').click(function(eventData) {
		jQuery('#footercontents form[name=emailSignUp] .message, #footercontents form[name=emailSignUp] .error').remove();
		jQuery.ajax({
			type:'post',
			cache:false,
			dataType:'json',
			data:jQuery('form[name="emailSignUp"]').serialize(),
			url:'/on/demandware.store/Sites-oreck-Site/default/Knotice-OptIn',

			success: function(data,textStatus,httpRequest) {
				if(data.retcode == "1") {
					csOrderNum = '' // order id
					csOrderType = 'Email Signup';

					// Footer sign-up
					csCustKey =  jQuery('form[name="emailSignUp"]').val();

					// Header sign-up
					//csCustKey = $('form[name="emailSignUpHeader"]').val();

					csSalesStageCode = 'Closed/Won';
					csAjaxTracking = true;
					setTimeout(function() { csExecuteShoppingCart(); csExecuteTracker(); }, 100);
					jQuery('#footercontents form[name=emailSignUp]').append('<span class="message">Thank you for signing up!</span>');
				} else {
					jQuery('#footercontents form[name=emailSignUp]').append('<span class="error">Invalid Email Address</span>');
				}
			},
			error: function(XMLHttpRequest, textStatus, errorThrown) {
				var result = { XMLHttpRequest:XMLHttpRequest, textStatus:textStatus, errorThrown:errorThrown };
				alert("An error occurred registering this email.  Please try again!!");
			}
		});
		return false;
	});
});
</script>






