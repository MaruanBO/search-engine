    <script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery('#header .emailsignup').hover(
			function(e) {
				jQuery('#header-subbar-email-updates-dropdown').show();
			}, 
			function(e) {
				jQuery('#header-subbar-email-updates-dropdown').hide();
			}
		);

		jQuery('#header-subbar-signup-input-email').bind('focus', function() {
			if(jQuery('#header-subbar-signup-input-email').val() == "Enter Your Email Address") {
				jQuery('#header-subbar-signup-input-email').val('');
			}
		});

		jQuery('input#header-subbar-emailOptIn').click(function(eventData) {
			jQuery('#header-subbar-email-updates-dropdown form[name=emailSignUpHeader] .message, #header-subbar-email-updates-dropdown form[name=emailSignUpHeader] .error').remove();
			jQuery.ajax({
				type:'post',
				cache:false,
				dataType:'json',
				data:jQuery('form[name="emailSignUpHeader"]').serialize(),
				url:'/on/demandware.store/Sites-oreck-Site/default/Knotice-OptIn',

				success: function(data,textStatus,httpRequest) {
					if(data.retcode == "1") {
						csOrderNum = '' // order id
						csOrderType = 'Email Signup';

						// Footer sign-up
						csCustKey =  jQuery('form[name="emailSignUpHeader"]').val();

						// Header sign-up
						//csCustKey = $('form[name="emailSignUpHeader"]').val();

						csSalesStageCode = 'Closed/Won';
						csAjaxTracking = true;
						setTimeout(function() { csExecuteShoppingCart(); csExecuteTracker(); }, 100);
						jQuery('#header-subbar-email-updates-dropdown form[name=emailSignUpHeader]').append('<span class="message">Thank you for signing up!</span>');
					} else {
						jQuery('#header-subbar-email-updates-dropdown form[name=emailSignUpHeader]').append('<span class="error">Invalid Email Address</span>');
					}
				},
				error: function(XMLHttpRequest, textStatus, errorThrown) {
					var result = { XMLHttpRequest:XMLHttpRequest, textStatus:textStatus, errorThrown:errorThrown };
					alert("An error occurred registering this email.  Please try again!!");
				}
			});
			return false;
		});
	});
</script>

<script type="text/javascript">
$(document).ready(function(){
   // Change the image of hoverable images
   $(".imgHoverable").hover( function() {
       var hoverImg = HoverImgOf($(this).attr("src"));
       $(this).attr("src", hoverImg);
     }, function() {
       var normalImg = NormalImgOf($(this).attr("src"));
       $(this).attr("src", normalImg);
     }
   );
});

function HoverImgOf(filename)
{
   var re = new RegExp("(.+)\\.(gif|png|jpg)", "g");
   return filename.replace(re, "$1-hover.$2");
}
function NormalImgOf(filename)
{
   var re = new RegExp("(.+)-hover\\.(gif|png|jpg)", "g");
   return filename.replace(re, "$1.$2");
}
</script>


