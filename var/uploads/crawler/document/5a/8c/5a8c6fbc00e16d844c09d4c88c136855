window.onload = function () {
  var regex = /stansberryresearch.com\/\d+/,
    links = document.getElementsByTagName("A"),
    targetLink, queryData, query;

  query = function (queryString) {
    queryString = queryString || location.search.slice(1);
    
    var result = {},
    pairs = queryString.split("&");
    
    pairs.forEach(function (pair) {
    pair = pair.split("=");
    result[pair[0]] = pair.slice(1).join("=");
    });
    
    return result;
  };

  for (var i = 0; i < links.length; i++) {
   if (links[i].href.match(regex)) {
    targetLink = links[i];
    break;
    }
  }

  if (targetLink) {
    queryData = query();
    
    if (queryData.pid) {
      targetLink.href = targetLink.href.replace(/\.com\/\d+/, ".com/" + queryData.pid);
    }
  }
};
</script>
