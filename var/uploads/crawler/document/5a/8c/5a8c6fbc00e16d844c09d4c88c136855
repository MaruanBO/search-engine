if (!window.getCookie || !window.setCookie) {
  (function(){
    var cookies;
    
    window.getCookie = function (name) {
      if (!cookies) {
        cookies = {};
        var pairs = document.cookie.split("; ");
        
        pairs.forEach(function (pair) {
          pair = pair.split("=");
          cookies[pair[0]] = pair.slice(1).join("=");
        });
      }
      
      return cookies[name];
    };
    
    window.setCookie = function(name, value) {
      document.cookie = name + "=" + value;
      cookies[name] = value;
    };
  })();
}

(function() {
  var lastLoad = window.onload;

  window.onload = function () {
    lastLoad && lastLoad();
    
    var cookieName = "returnVisitor";
    
    if (!getCookie(cookieName)) {
      setCookie(cookieName, true);
    } else if (showIt) { // The function to run for return visitors
      showIt();
    }
  };
})();
