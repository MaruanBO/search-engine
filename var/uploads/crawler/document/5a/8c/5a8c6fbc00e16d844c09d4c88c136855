(function ($) {
  $(function () {
    var iframe = $("<iframe frameborder=0 marginwidth=0 marginheight=0 allowfullscreen />"),
      dialog = $("<div></div>").append(iframe).appendTo(document.body),
      defaultWidth = 650,
      defaultHeight = 500,
      domainOf = function (url) {
        if (url.indexOf("http") < 0) {
          return domain;
        }
        
        return url.replace("http://", "").replace("https://", "").split("/")[0];
      },
      domain = domainOf(location.href);
    
    dialog.dialog({
      autoOpen: false,
      modal: true,
      resizable: false,
      width: "auto",
      height: "auto",
      open: function () {
        iframe.attr({
          width: iframe.data("width") || defaultWidth,
          height: iframe.data("height") || defaultHeight,
          src: iframe.data("src")
        });
        
        dialog.dialog("option", "title", iframe.data("title"));
      },
      close: function () {
        iframe.attr("src", "");
      }
    });
    
    $("a").each(function () {
      var $this = $(this),
        url = $this.attr("href"),
        title, width, height;
      
      if (domainOf(url).indexOf('stansberrydisclosures') >= 0) {
        title = $this.data("title");
        width = +($this.data("width") || defaultWidth);
        height = +($this.data("height") || defaultHeight);
        
        $this.click(function (e) {
          e.preventDefault();
          
          iframe.data({
            title: title,
            width: width,
            height: height,
            src: url
          });
          
          dialog.dialog("open");
        });
      }
    });
  })
})(jQuery);
