<script type="text/javascript">
  function setLang(lang) {
    if ( !lang ) {
      return false;
    }
    var newloc = (self.location.href.replace(/[\?&](m|show_lang|lang)=(\w+|\d+)/gi, '')+'&lang='+lang+'&show_lang='+lang).replace(/[\?&]/,'?');
    self.location.href = newloc;
  }
</script>
