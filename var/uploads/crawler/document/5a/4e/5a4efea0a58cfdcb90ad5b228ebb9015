  self.findpos = function (a) {
    var curleft = curtop = 0;
    if (a.offsetParent) {
      curleft = a.offsetLeft;
      curtop = a.offsetTop;
      while (a = a.offsetParent) {
        curleft += a.offsetLeft;
        curtop  += a.offsetTop;
      }
    }
    return [curleft,curtop];
  }
  self.hide_info = function () {
