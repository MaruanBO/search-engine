    b = b ? b : 'object';
    for (var k in a) {
        s += '<' + k + '><![CD' + 'ATA[' + a[k] + ']' + ']></' + k + '>'
    };
    return string_to_xml('<' + b + '>' + s + '</' + b + '>')
