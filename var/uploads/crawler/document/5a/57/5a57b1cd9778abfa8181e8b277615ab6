<script type="text/javascript">
/** cookie function **/
var btCookie = (function () {
    function set(name, value, days) {
        var Days = typeof(days) === "undefined" ? 1 : days; 
        var exp  = new Date();    
        exp.setTime(exp.getTime() + Days*24*60*60*1000);
        document.cookie = name + "=" + escape (value) + ";expires=" + exp.toGMTString()+";domain="+document.domain+";path=/";
    }
    
    function get(name) {
        var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
        if (arr !== null) {
            return unescape(arr[2]);
        }
        return null;
    }
    
    function del(name) {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        var cval=this.getCookie(name);
        if (cval !== null) {
            document.cookie = name + "="+cval+";expires=" + exp.toGMTString() + ";domain="+document.domain+";path=/";
        }
    }
    
    return {
        set : set,
        get : get,
        del : del
    };
})();

/** store pageviews **/
if (isNaN(parseInt(btCookie.get("_bt_pageviews"), 10))) {
    btCookie.set("_bt_pageviews", 1, 30);
} else {
    btCookie.set("_bt_pageviews", parseInt(btCookie.get("_bt_pageviews"), 10) + 1, 30);
}

siteGlobals.category = 'home';

//console.log(siteGlobals);

</script>

<script type="text/javascript">
    var _brkref = "";
    function getQueryVariable(variable) {
       var query = window.location.search.substring(1);
       var vars = query.split("&");
       for (var i=0;i<vars.length;i++) {
               var pair = vars[i].split("=");
               if(pair[0] == variable){return pair[1];}
       }
       return(false);
    }

    if(getQueryVariable('brkref')) {
        _brkref = getQueryVariable("brkref"); 
    }

    var adGlobal = {
            unit: {
                leaderboard : siteGlobals.category + '-top',
                rectangle   : siteGlobals.category + '-right-atf',
                footer      : siteGlobals.category + '-bottom',
                skyscraper  : siteGlobals.category + '-right-btf',
                oop         : siteGlobals.category + '-interstitial'
            },

            target: {
                sfw         : 'yes', 
                cat         : siteGlobals.category, 
                postid      : siteGlobals.post_id, 
                page        : 'thegloss_' + siteGlobals.category, 
                pageviews   : btCookie.get("_bt_pageviews"),
                brkref      : _brkref,
                tags        : siteGlobals.tags
            }

    };
    
    //console.log(adGlobal);

    // Replace all ad unit names (except for footer) with "swapAdZone" value if it exists.
    if(getQueryVariable('swapAdZone')) {
        adGlobal.unit.leaderboard = adGlobal.unit.rectangle = adGlobal.unit.skyscraper = adGlobal.unit.oop = getQueryVariable('swapAdZone');
    }

    if(typeof googletag != "undefined") {
        
        googletag.cmd.push(function() {

            // Define ad slots. For gurl.com, remove [160,600] from the skyscraper unit
            googletag.defineSlot('/4678/thegloss.dmp/'+adGlobal.unit.leaderboard, [[728, 90],[970, 66],[970,90],[970,250],[970,40],[1000,40],[1000,66],[1000,250]], 'div-gpt-ad-leaderboard')
                .setCollapseEmptyDiv(true, true).addService(googletag.pubads());
            googletag.defineSlot('/4678/thegloss.dmp/'+adGlobal.unit.rectangle, [[300, 250],[300,600],[300,1050]],'div-gpt-ad-rectangle')
                .setCollapseEmptyDiv(true, true).addService(googletag.pubads());
            googletag.defineSlot('/4678/thegloss.dmp/'+adGlobal.unit.footer, [728, 90],'div-gpt-ad-footer')
                .setCollapseEmptyDiv(true, true).addService(googletag.pubads());
            googletag.defineSlot('/4678/thegloss.dmp/'+adGlobal.unit.skyscraper, [[160, 600],[300,250]],'div-gpt-ad-skyscraper')
                .setCollapseEmptyDiv(true, true).addService(googletag.pubads());
            googletag.defineOutOfPageSlot('/4678/thegloss.dmp/'+adGlobal.unit.oop, 'div-gpt-ad-oop')
                .addService(googletag.pubads());

            // Sets targeting values for all ad tags
            googletag.pubads()
                .setTargeting('sfw',adGlobal.target.sfw)
                .setTargeting('cat',adGlobal.target.cat)
                .setTargeting('postid',adGlobal.target.postid)
                .setTargeting('page',adGlobal.target.page)
                .setTargeting('pageviews',adGlobal.target.pageviews)
                .setTargeting('tags', adGlobal.target.tags);

            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });
    }
</script>
