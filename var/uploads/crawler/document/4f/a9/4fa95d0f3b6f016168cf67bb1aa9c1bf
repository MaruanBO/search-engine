    jQuery(function($) {
    $('.main-nav-wrapper').on({
        mouseover: function() {
            var subnavid= '#'+$(this).data('sub-nav');
                $('.current-nav').hide();
                $(subnavid).show();
        },
        mouseout: function() {
            var subnavid= '#'+$(this).data('sub-nav');
                $(subnavid).hide();
                $('.current-nav').show();
        }
    })
});

