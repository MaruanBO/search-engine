<div class="module draft-view" >
            <div class="mod-wrp-1"><div class="hd">
                        <div class="title">
                                <h2><span>Draft Picks</span></h2>
                            </div>
                        </div>
                    <div class="bd">
                        
    <span data-otcurl='http://feeds.nfl.com/feeds-rs/draft/onTheClock/2015.json'
          data-year='2015'
          data-displaytype='Vertical'
          data-showbio='yes'
          data-numberofpicks='1'
          class='drafttracker-config'></span>
    
    
    
    
    
    

    <script type="text/html" id="post-template">
        <div class="post card">
            <div class="tooltip" style="display:none;" data-bind='text:pickNote'></div>
            <div class="hd">
            <h3>Round <span data-bind="text:round"></span>
                Pick <span data-bind="text:pickInRound"></span> (<span class="overall" data-bind="text:overall"></span>)
                <span class='tradeInfo'>
                    <div class="glyphicon glyphicon-info-sign" data-trigger="tooltip" data-bind='visible: $data.pickNote, click: $parent.clickToolTip, event:{mouseover: $parent.overToolTip, mouseout: $parent.outToolTip}' ></div>
                </span>
            </h3>
            </div>

            <div class="bd">
            <div class="main">
                <img data-bind='attr: {src: draftPlayer.headShotURL}' class="player">
                <h4>
                    <!-- ko if: draftPlayer.draftPlayerPage() !== null -->
                    <a data-bind="attr: {href: draftPlayer.draftPlayerPage}">
                        <div data-bind="text: draftPlayer.firstName"></div>
                        <div data-bind="text: draftPlayer.lastName"></div>
                    </a>
                    <!-- /ko -->
                    <!-- ko if: draftPlayer.draftPlayerPage() === null -->
                        <div data-bind="text: draftPlayer.firstName"></div>
                        <div data-bind="text: draftPlayer.lastName"></div>
                    <!-- /ko -->
                </h4>
                <div class="playerbio">
                    <p><span data-bind="text:draftPlayer.position"></span></p>
                    <p><span data-bind="convertToFeet:draftPlayer.height"></span> - <span data-bind="text:draftPlayer.weight"></span> lbs.</p>
                    <p data-bind="text:draftPlayer.college"></p>
                </div>
            </div>
            <div class="submain">
                <span data-bind="if: draftPlayer.relatedContent().length <= 0 && $parent.config.showBio === 'yes'">
                    <p data-bind="html:draftPlayer.overView"></p>
                </span>

                <span data-bind="if: draftPlayer.relatedContent().length > 0">
                    <ul>
                        <!-- ko foreach: draftPlayer.relatedContent -->
                        <li>
                            <a data-bind="attr: {href: $data.link}">
                                <div class="glyphicon" data-bind="css : $data.type"></div>
                                <div class="relatedContent" data-bind="text: $data.title"></div>
                            </a>
                        </li>
                        <!-- /ko -->
                    </ul>
                </span>
            </div>
            <div class="ft">
            </div>
        </div>
        </div>
    </script>

