<script type="text/javascript">

var msgArrKey = 0;
msgArrJson = ["Hey teens! Wanna join a community of other teens on Answers, where you can strengthen your leadership and community skills, and build up your resume or college app? <a href=\"\/Q\/WikiFAQs:Teens_program\">Here's how<\/a>.","Late for something? You can find excuses starting with each letter of the alphabet <a href=\"\/Q\/Special:Search&notextsearch=0&search=What+are+some+reasons+for+being+late+that+begin+with\" name=\"&amp;lid=hpb_late\">here<\/a>!","Promote your Answers.com profile and your answers! <br> Add them to your email signature... <a href=\"http:\/\/www.wisestamp.com\/partners\/answers\" name=\"&amp;lid=hpb_sigbio\">Here's how<\/a>.","Find an answer that could use some work? Learn how to <a href=\"http:\/\/wiki.answers.com\/help\/flag_for_improvement\" name=\"&amp;lid=hpb_flag\">Flag for Improvement<\/a>.","<i>You know you\u2019re a Wikiholic if\u2026<\/i> Find out if you should be signing up for <a href=\"http:\/\/wiki.answers.com\/static\/wikiholics_anonymous.html\" name=\"&amp;lid=hpb_wikiholic2\">Wikiholics Anonymous<\/a>.","Do you blog?  Give your readers instant information without leaving your site with <a href=\"http:\/\/www.answers.com\/main\/answertips.jsp\" name=\"&amp;lid=hpb_ansTips\">AnswerTips<\/a>!","Can't decide?  Go to a <a href=\"\/Q\/Special:Randompage\" name=\"&amp;lid=hpb_random2\">random question<\/a>!","Add your Answers.com contributions to your email signature and share your very own dynamic content with the folks you email. <a href=\"http:\/\/www.wisestamp.com\/partners\/answers\" name=\"&amp;lid=hpb_sig\">Here's how<\/a>.","In the mood to help? Try <a href=\"\/Q\/Special:NeedsImprovement&action=Search\" name=\"&amp;lid=hpb_NI\">fixing up<\/a> some answers that need a little extra somethin'. ","It\u2019s easy to share any Q&amp;A with friends from Facebook, Twitter, and all over. Use the <a href=\"http:\/\/wiki.answers.com\/help\/share\" name=\"&amp;lid=hpb_share\">Share button<\/a>. ","Meet your neighbors! Learn about <a href=\"http:\/\/wiki.answers.com\/static\/community_members.html\" name=\"&amp;lid=hpb_commMemb\">community roles<\/a> and how you can participate even more. ","With over 7,000 categories, sometimes reorganizing Q&amp;A\u2019s becomes necessary. Learn how to <a href=\"http:\/\/wiki.answers.com\/help\/recategorize\" name=\"&amp;lid=hpb_recat\">Recategorize<\/a>.","Feeling spontaneous? <a href=\"http:\/\/wiki.answers.com\/Q\/Special:Randompage\" name=\"&amp;lid=hpb_random1\">Jump to a random question<\/a>! ","Learn how to use <a href=\"\/Q\/How_do_you_get_RSS_feeds_of_WikiAnswers_content_on_your_blog\" name=\"&amp;lid=hpb_rss\">RSS feeds of WikiAnswers content on your blog<\/a>.  "];
function nextMessage(){
	msgArrKey++;
	if(msgArrKey > msgArrJson.length-1)
		msgArrKey = 0;
	
	jQuery('#message').html(msgArrJson[msgArrKey]);
	_hbLink('next_MB','{$wssStatus}_messageBar');
}
function previousMessage(){
	if(msgArrKey == 0)
		msgArrKey = msgArrJson.length-1;
	else
		msgArrKey--;
	if(msgArrKey > msgArrJson.length-1)
		msgArrKey = 0;

	jQuery('#message').html(msgArrJson[msgArrKey]);
	_hbLink('previous_MB','{$wssStatus}_messageBar');
}

