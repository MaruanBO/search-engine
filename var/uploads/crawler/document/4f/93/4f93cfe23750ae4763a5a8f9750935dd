	<!-- Javasript Toggle Script -->
    <script>

			$(".menu-toggle").click(function(e) {
				e.preventDefault();
				$("#wrapper").toggleClass("toggled", 2000);
				$(".logo").toggle();
			});


			$(document).load(function(){
				$('.tag-line').hide('fast');
			});


			$(document).ready(function(){
				$('.tag-line').fadeOut('fast');
				tranTocolor();
			});


			function tranTocolor(){
				
						setTimeout( function(){
							$('.header').addClass('grayscaletocolor', {duration:100});
					  }
					 , 1000 );
				
				
		//				tag-line visibile
						setTimeout( function(){
//						$('.header').removeClass('grayscale');
						
//						$('.header').removeClass('grayscale');
//						$('.header').addClass('grayscaletocolor');
//							$('.header').addClass('grayscaletocolor', {duration:200});
							
//						$('.header').css('');
							
							
							
						$('.tag-line').css('visibility','visible');
							   $('.tag-line').fadeTo(3000,1);
					  }
					 , 5000 );
				}

			$('.officeLocation').hover(function(){
				var location = $(this).attr('id');
				var address="";
				if(location=='Seattle')
				{
					address='2414 1st Ave <br /> Seattle, WA, 98121';
				}
				else if(location=='NewYork')
				{
					address='1412 Broadway, 22nd floor, Room # 4 <br /> New York, NY, USA';
				}
				else if(location=='Toronto')
				{
					address='111 Richmond St. W, Suite 5 <br /> Toronto, ON M5H 2G4';
				}
				else if(location=='London')
				{
					address='69-89 Mile End Road <br /> London, UK, E1 4UJ';
				}

				$('#address').html(address);
			});

				$(function () {
				  $('[data-toggle="tooltip"]').tooltip()
				});

		
		$('.request-demo-btn-clicked').click(function(){

			
		});
		
		
    </script>

