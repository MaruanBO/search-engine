								function filterSearch(event) {
									var searchString = $("#sInput").val();
									if (/^\d{5}(-\d{4})?$/.test(searchString)) {
										event.preventDefault();
										$("#headerZip").val(searchString);
										$("#headerLocator").submit();
										return false;
									}
									else {
										return true;
									}
									return false;
