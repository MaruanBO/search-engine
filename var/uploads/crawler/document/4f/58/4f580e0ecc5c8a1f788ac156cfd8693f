<script type="text/javascript">

jQuery(document).ready(function($) {
	
	$('#foot-email-submit').click(function() {
		
		
		//$(this).prop('disabled', true);
		if($('#foot-email-cb').prop('checked')) {
			//do nothing
		} else {
		

        var hasError = false;
        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
 
        var emailaddressVal = $('input[name="foot_email"]').val();
        var emailaddressVal2 = $('input[name="foot_email_confirm"]').val();
        if(emailaddressVal == '') {
            alert('Oops! You forgot to enter your email address.');
            hasError = true;
        }

        if(emailaddressVal2 == '') {
            alert('Oops! You forgot to verify your email address.');
            hasError = true;
        }

        else if(!emailReg.test(emailaddressVal)) {
            alert('Oops! Your email address does not appear to be valid.');
            hasError = true;
        }

        else if(emailaddressVal != emailaddressVal2) {
            alert('Oops! Your emails don\'t match.');
            hasError = true;
        }
 
        if(hasError == true) { return false; }
        
        else {
      		//$('#foot-email-loader').addClass('on');        	
			$('#email-footer-loader').fadeIn('fast');

        	//Begin Ajax
        		var email = emailaddressVal;
        		//alert(email);
				var data = 'email=' + email;
        
        		$.ajax(
				{
					   type: "GET",
					   url: "/ajax/foot-newsletter-signup.php",
					   data: data,
					   dataType: 'json',
					   success: function(data) {

		    				var ctct_status = data.the_status;
		    				
		    				if(ctct_status == 'ACTIVE') { 
		    				
		    					//alert person they already have a newsletter subscrip
		    					$('#email-footer-loader').fadeOut('fast');
		    					$('#cc-foot-table').remove();
		    					$('#nl-signup-2014').append('<p style="font-weight: 800;">Good news - it looks like your email address is already signed up for our newsletter!</p>');
		    					
		    				} else if (ctct_status == 'FA_ADDED') {
		    				
		    					//alert people they've been added
		    					$('#email-footer-loader').fadeOut('fast');
		    					$('#cc-foot-table').remove();
		    					$('#nl-signup-2014').append('<p style="font-weight: 800;">Thank you for signing up! You\'ve been added to our weekly newsletter list!</p>');
		    					
		    					faTrackEventNoClk('Footer Clicks', 'Newsletter', 'Footer Signup');
		    					
		    				} else {
		    					
		    					
		    					
		    				}
					   		
      						//jQuery('form#email-form.header-bar').fadeOut('slow', function() {
      						//signup succeded, tell them thanks
      						//jQuery('#email-thx.header-bar').fadeIn('slow');
      						//jQuery('#email-loader.header-bar').removeClass('on');
							//});
    					}
					
				 }); 	
        	//End Ajax
        	return false;
        	
        }
        
		} //if else for if the box is checked
		
	});
	
});

</script>


<link rel="stylesheet" type="text/css" href="http://cdn.farmersalmanac.com/wp-content/themes/almanac/js/shadowbox/shadowbox.css">
<script type="text/javascript" src="http://cdn.farmersalmanac.com/wp-content/themes/almanac/js/shadowbox/shadowbox.js"></script>
<script type="text/javascript">
Shadowbox.init();
</script>

<script type="text/javascript">

	var isiPad = navigator.userAgent.match(/iPad/i) != null;
	
	if(typeof(Storage) !== "undefined" && !isiPad) {
    	// Code for localStorage/sessionStorage.
    	//alert('supports storage!');
    	//localStorage.setItem("fapopup", 180);
    	//alert(localStorage.getItem("fapopup"));
    	
    	//localStorage.removeItem("fapopup");
    	
    	var get_last_fa_popup = Number(localStorage.getItem("fapopup"));
    	
    	if(get_last_fa_popup == 0 || get_last_fa_popup == 20) {
    		window.onload = function() { fa_show_popup(); }
    		localStorage.setItem("fapopup", 1);
    	} else {
    		var new_num = get_last_fa_popup + 1;
    		localStorage.setItem("fapopup", new_num);
    	}
    	
    	//localStorage.removeItem("fapopup");
	} else {
	    // Sorry! No Web Storage support..
	}


function fa_show_popup() {

    // open a welcome message as soon as the window loads
    Shadowbox.open({
    	

        content:    '<a href="https://farmersalmanac.com/digital/" onclick="faTrackEvent(this, \'Pop Up Clicks\', \'2015 Digital FA\');return false;"><div id="welcome-msg" style="position: relative; height: 333px; width: 500px; background: #fff url(http://cdn.farmersalmanac.com/wp-content/themes/almanac/images/home/popups/2015-digital-popup.jpg) 0 0 no-repeat;"></div></a>',


        player:     "html",
        title:      "Farmers' Almanac for Kindle, iPad & NOOK!",
    	        height:     333,
        width:      500
    });
}; //end function fa_show_popup()

//track the newsletter popup
function faTrackPopup() {
_gaq.push(['_trackEvent', 'Pop Up Event', 'FA Newsletter']);
}

</script>
