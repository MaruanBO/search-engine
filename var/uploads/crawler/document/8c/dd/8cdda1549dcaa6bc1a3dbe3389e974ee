<script type='text/javascript'>
    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 58456;
    _sf_async_config.domain = 'news-record.com';
    _sf_async_config.useCanonical = true;
    _sf_async_config.sections = 'front';  //CHANGE THIS
