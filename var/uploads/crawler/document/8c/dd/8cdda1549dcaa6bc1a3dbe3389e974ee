<script type="text/javascript">
    $(window).load(function() {
        $(".article_lightbox").magnificPopup({
            type: 'image',
            closeOnContentClick: true,
            mainClass: 'mfp-img-mobile',
            image: {
                verticalFit: true,
                titleSrc: function(item) {
                    var t = $(item).prev().prop('tagName');
                    return item.el.parents(t).find('img').attr('alt');
                }
            }
	});
        
        $(".popup-youtube, .popup-vimeo, .popup-gmaps").magnificPopup({
            disableOn: 700,
            type: 'iframe',
            mainClass: 'mfp-fade',
            removalDelay: 160,
            preloader: false,
            fixedContentPos: false
        });
        
        $("a.pop-iframe").click(function(e) {
            e.preventDefault();
            var i = $(this).attr("href");
            var w = $(this).data("width");
            var h = $(this).data("height");
            if ($("#nr_lightbox").length > 0) {
                $("#nr_lightbox #content").html("<iframe src=\"" + i + "\" style=\"width: " + w + "px; height: " + h + "px;\"></iframe><div class=\"nr-close\"></div>").width(w).height(h);
                $("#nr_lightbox").fadeIn(200);
            }
            else {
                var lightbox = '<div id="nr_lightbox">' + 
                    '<div id="content" style="width: ' + w + 'px; height: ' + h + 'px;">' +
                    '<iframe src="' + i + '" style="width: ' + w + 'px; height: ' + h + 'px;"></iframe>' +
                    '<div class="nr-close"></div>';
                    '</div>' + 
                    '</div>';
                $('body').append(lightbox);
            }
            $("#nr_lightbox #content .nr-close").click(function() {
                $("#nr_lightbox").fadeOut(200);
            });
	});
        
    });
</script>



