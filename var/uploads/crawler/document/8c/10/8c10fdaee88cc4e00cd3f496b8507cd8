  <script language="javascript">

      head.ready(function()
      {
          head.js('/js/jquery-ui-1.8.18.custom.min.js', function()
          {
              var dts = new Date($("#filter_date_start").val());
              var dte = new Date($("#filter_date_end").val());


              if (!global_is_mobile_safari && !global_is_chrome) {

                  var cal_months = 1;
                  if (screen.width > 481 && !global_is_android) {
                      cal_months = 2;
                  } //if

                  if (head.browser.ie && parseFloat(head.browser.version) < 9) {
                  }
                  else {
                      dte.setDate(dte.getDate() + 1);
                  }

                  $("#filter_date_start").datepicker({
                      showOn: "focus",
                      numberOfMonths: cal_months,
                      minDate: $.datepicker.formatDate('mm/dd/yy', new Date()),
                      dayNamesMin: ['S', 'M', 'T', 'W', 'T', 'F', 'S'],
                      dateFormat: "mm/dd/yy"
                  }).datepicker('setDate', dts)

                  $("#filter_date_end").datepicker({
                      showOn: "focus",
                      numberOfMonths: cal_months,
                      minDate: $.datepicker.formatDate('mm/dd/yy', new Date()),
                      dayNamesMin: ['S', 'M', 'T', 'W', 'T', 'F', 'S'],
                      dateFormat: "mm/dd/yy"
                  }).datepicker('setDate', dte)

              } //if





          });

      });
    
      
  </script>



