	<script>		
		var scrollPage = 1;
		var scrolling = false;	
		var scrollOffset = 12;
		
		require(["jquery"], function() {
			<!-- Endless Scroll -->
			$(window).scroll(function () { 
				if (!scrolling && $(window).scrollTop() >= $(document).height() - $(window).height() - 100) {
					getContent();
				}
			});
		});
		
		function getContent(){
			if (!scrolling) {
			   	scrolling = true;
				
			  	//Loading GIF
			   	$("#endlessContainer > .thumbnails").append('<div id="loading" class="item box"><div class="thumbnail"><img src="http://staticcontent.chacha.com/f7eb0a87e0bdbfe6527bcc37eb52ef64/images/loading.gif" alt="Loading more Galleries" style="display: block; margin-left: auto; margin-right: auto;"/></div></div>');
			   
				$.ajax({
					type: "GET",
					url: "/ajax/galleryGrid",
					data: {offset : scrollOffset},
					dataType: "html"
				}).done(function( html ) {
					$("#loading").remove();
					$("#endlessContainer > .thumbnails").append(html);
					scrollOffset += 9;
					scrollPage++;
					scrolling = false;
				});
		   	}
		}
	</script>
	<!-- Endless Script END -->

