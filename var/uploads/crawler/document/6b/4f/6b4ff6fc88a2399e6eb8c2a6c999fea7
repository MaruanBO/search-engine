<script language="javascript">var g_currentDisplayItem="";function rotateBanner(){if(g_currentDisplayItem=='feature_1'){g_currentDisplayItem='feature_2';}
else if(g_currentDisplayItem=='feature_2'){g_currentDisplayItem='feature_3';}
else if(g_currentDisplayItem=='feature_3'){g_currentDisplayItem='feature_4';}
else{g_currentDisplayItem='feature_1';}
showSelected(g_currentDisplayItem)
setTimeout("rotateBanner()",10000);}
function showSelected(p_strSelectedItem){document.getElementById('feature_1').className="FEATUREHIDE";document.getElementById('feature_2').className="FEATUREHIDE";document.getElementById('feature_3').className="FEATUREHIDE";document.getElementById('feature_4').className="FEATUREHIDE";document.getElementById(p_strSelectedItem).className="FEATURESHOW";}</script>
