<script type="text/javascript">
  	
	function validate(form){	

		var zip = form["zipCode"].value;
		var city = form["city"].value;

		if(city == "City"){
			form["city"].value = "";
		}

		if(zip == "Zip"){
			form["zipCode"].value = "";
		}
	}
</script></div>
	
