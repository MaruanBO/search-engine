splashText.push("LabCorp is the world’s leading health care diagnostics company, providing comprehensive clinical laboratory services through LabCorp, and end-to end drug development support through Covance.  ");
splashLinks.push("http://www.covance.com");splashImages.push("/wps/wcm/connect/6790b7004f47b487acbeac37fb856a47/Covance_Logo.jpg?MOD=AJPERES");
splashTitles.push("LabCorp Acquires Covance");
splashText.push("LabCorp is the world’s leading health care diagnostics company, providing comprehensive clinical laboratory services through LabCorp, and end-to end drug development support through Covance.");
splashLinks.push("http://www.covance.com");splashImages.push("/wps/wcm/connect/7f1c6f804f47b488acceac37fb856a47/Covance_Logo.jpg?MOD=AJPERES");
splashTitles.push("LabCorp Acquires Covance");
splashText.push("LabCorp is the world’s leading health care diagnostics company, providing comprehensive clinical laboratory services through LabCorp, and end-to end drug development support through Covance.");
splashLinks.push("http://www.covance.com");splashImages.push("/wps/wcm/connect/108be1804f47b489acd6ac37fb856a47/Covance_Logo.jpg?MOD=AJPERES");
