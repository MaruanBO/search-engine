<script type="text/javascript">
var splashImages = Array();
var splashTitles = Array();
var splashText = Array();
var splashLinks = Array();
//var splashLinkText = Array();

//function to get random number upto m
function randomXToY(minVal,maxVal)
{
  var randVal = minVal+(Math.random()*(maxVal-minVal));
//alert("The random value is:" + randVal);
  return randVal;
}

function getRotation() {
populateArrays();
var minVal = 0;
var maxVal = splashImages.length -1;
//alert("the minimum val is: " + minVal);
//alert("the maximum val is: " + maxVal);
var numIndex = Math.round(randomXToY(minVal,maxVal));
var pImage = splashImages[numIndex];
//alert("index number is:" + numIndex);
var bTitle = splashTitles[numIndex];
var pText = splashText[numIndex];
var pLink = splashLinks[numIndex];
//var pLinkText = splashLinkText[numIndex];
document.getElementById("splashImage").src = pImage;
document.getElementById("splashTitle").innerHTML = bTitle;
document.getElementById("splashText").innerHTML = pText;
document.getElementById("splashLink").href = pLink;
//document.getElementById("splashLink").innerHTML = pLinkText; 
}

function populateArrays() {splashImages.push("/wps/wcm/connect/f73665804f47b487acc6ac37fb856a47/Covance_Logo.jpg?MOD=AJPERES");
