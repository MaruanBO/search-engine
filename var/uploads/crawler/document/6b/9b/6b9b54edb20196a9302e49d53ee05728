		<script type="text/javascript">
			//setup some variables we need php to set
			fav_dashboard_url = "http://www.oncewed.com/dashboard/favorites";
			site_url = "http://www.oncewed.com/";
			function checkFavorites() {
    			favsArray = [];
			    jQuery.post(
			        fav_dashboard_url, 
			        {
			            'action': 'get_favs'
			        },
			        function(data) {
			        var response = jQuery.parseJSON(data);
			        if (response.favs) {
			            var response = jQuery.parseJSON(data);
			            favsArray = response.favs;
			            jQuery('.favorite-link').each(function(){
			                post_id = jQuery(this).data("post-id");
			                fav_dashboard_url = jQuery(this).data("fav-url");
			                if ( inArray( post_id, favsArray ) ) {
			                    jQuery(this).find('.heart').addClass('pink');

			                }
			            });
			        }
			    });
			}
			jQuery(document).ready(function(){
				//check for favorites
				checkFavorites();
			});
		</script>
		
				
