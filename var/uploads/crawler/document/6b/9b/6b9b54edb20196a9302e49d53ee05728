					<script  type='text/javascript'>
						jQuery(document).ready(function(){
							jQuery(window).scroll(function(){
								var scrollPos = jQuery(window).scrollTop();
								if(jQuery('#main').width() < 500){
									scrollPos = scrollPos - 355;
								}else{
									scrollPos = scrollPos - 210;
								}
								if (scrollPos > jQuery('#floatingAdsIdentifier').position({ scroll: false }).top){
									jQuery('#floatingAds').css('position', 'fixed');
									jQuery('#floatingAds').css('top', '10px');
									jQuery('#floatingAds').css('margin', '0');
									var posLeft = jQuery('#sidebar1').position({ scroll: false }).left;
									if(jQuery('#main').width() < 500){
										posLeft = posLeft + 10;
									}else{
										posLeft = posLeft + 38;
									}
									jQuery('#floatingAds').css('left', posLeft);			
								}
								if (scrollPos <= jQuery('#floatingAdsIdentifier').position({ scroll: false }).top){
									jQuery('#floatingAds').css('position', 'relative');
									jQuery('#floatingAds').css('top', jQuery('#floatingAdsIdentifier').position);
									jQuery('#floatingAds').css('left', 0);
								}				
							});
						});
					</script>	  
