<!-- BTM SIGNUP TEXT SCRIPT -->
<script type="text/javascript">
    $=jQuery;
    $(document).ready(function(){
        $("select.plan").change(function(){
            $( "select.plan option:selected").each(function(){
                if($(this).attr("value")=="platinum"){
                    $(".gold-signup-text").hide();
                    $(".platinum-signup-text").show();
                }
                if($(this).attr("value")=="gold"){
                    $(".platinum-signup-text").hide();
                    $(".gold-signup-text").show();
                }
            });
        }).change();
    });
</script>
