<!-- BTM SIGNUP TEXT SCRIPT -->
  <script type="text/javascript">
    $=jQuery;
    $(document).ready(function(){
        $("select.plan2").change(function(){
            $( "select.plan2 option:selected").each(function(){
                if($(this).attr("value")=="platinum"){
                    $(".gold-signup-text2").hide();
                    $(".platinum-signup-text2").show();
                }
                if($(this).attr("value")=="gold"){
                    $(".platinum-signup-text2").hide();
                    $(".gold-signup-text2").show();
                }
            });
        }).change();
    });
</script>
<!-- END BTM SIGNUP -->

