  <script>
  // 
  (function(){
    var searchFormSelector = '#SearchLoginSocial form.search-form';
    var searchInputSelector = '#SearchLoginSocial input#search-input';
    var searchDropdownSelector = '#SearchLoginSocial ul#search-dropdown';
    var searchOptionLinkSelector = '#SearchLoginSocial li.search-option a';

    focusInput = function(e){
      console.log(searchInputSelector);
      jQuery(searchInputSelector).off('focus', focusInput);
      jQuery(document).on('click', blurInput);
      jQuery(searchInputSelector).removeClass("searchUpDown");
      jQuery(searchDropdownSelector).slideDown('fast');
    }

    blurInput = function(e){
      // 
      if( jQuery(e.target).closest(searchFormSelector+', '+searchOptionLinkSelector).length === 0 ) {
        jQuery(document).off('click', blurInput);
        jQuery(searchInputSelector).on('focus', focusInput);
        jQuery(searchInputSelector).addClass("searchUpDown");
        jQuery(searchDropdownSelector).slideUp('fast');
      }
    }

    canProceed = function(){
      return jQuery(searchInputSelector).val() != "";
    }

    enterKeywordMessage = function (){
      jQuery(searchInputSelector).attr('placeholder',"Enter a keyword.");
      window.setTimeout(function(){
        jQuery(searchInputSelector).attr('placeholder',"");
      },1000);
    }

    jQuery(searchInputSelector).on('focus',focusInput);

    // 
    jQuery(searchFormSelector).submit(function(e){
      if(canProceed()){
        console.log('form submitted proceed');
      }else{
        e.preventDefault();
        console.log('form submitted can\'t proceed');
        enterKeywordMessage();
      }
    });

    // 
    jQuery(searchOptionLinkSelector).on('click', function(e){
      e.preventDefault();
      if(canProceed()){
        console.log('link clicked proceed');
        if(jQuery(e.target).hasClass('search-all')){
          jQuery(searchFormSelector).submit();
        } else {
          var newLocation = jQuery(e.target).attr('href') || "";
          var catVal = jQuery(e.target).attr('data-category');
          var catParam = catVal != undefined ? "&category="+catVal : "";
          window.location = newLocation 
                          + "?keywords=" + jQuery(searchInputSelector).val() 
                          + "&submit=Search" 
                          + catParam;
        }
      }else{
        console.log('link clicked can"t proceed');
        enterKeywordMessage();
      }
    });

  })();
  </script>

</div>
