        <h2><a href="http://www.nuvo.net/ArtsBlog/archives/2015/02/19/slideshow-steampunk-through-the-looking-glass">Slideshow: Steampunk through the Looking Glass</a></h2>
  
      <div class="subhead">
        <a href="http://www.nuvo.net/ArtsBlog/archives/2015/02/19/slideshow-steampunk-through-the-looking-glass">The Circle City Aerodrome returned to Columbia Club over Valentine's Day weekend to host an immersive steampunk experience. 
         <i>&mdash; by Michelle Craig</i>
        </a>
      </div>
      </div>
    </div><!-- #FeaturedRotatorWithThumbsBig .MainColumn Home  -->

    <script type="text/javascript"><!--
      dnLoadScript("/foundation/scripts/slideshow.js");

      Event.observe(document, "dom:loaded", function () {
          var slides = [];
          $$("#FeaturedRotatorWithThumbsBig div.follow>a").each(function (a) {
              var src = a.readAttribute("data-slide-src");
              if (!src) return;
              slides.push({
                  src: src,
                  width: new Number(a.readAttribute("data-slide-width")),
                  height: new Number(a.readAttribute("data-slide-height")),
                  href: a.readAttribute("href"),
                  caption: a.readAttribute("data-slide-caption")
              });
          });

          var slideshowFeaturedRotatorWithThumbsBig = new dnSlideshow(
              "FeaturedRotatorWithThumbsBigSlideshow",
              { bezel: false,
                transition: "dissolve",
                width: 595,
                height: 370,
                alwaysAnimate: true,
                captionId: "caption-FeaturedRotatorWithThumbsBig",
                delay: 10 },
              slides
          );

          
          $$("#FeaturedRotatorWithThumbsBig div.follow>a").each(function (a) {
              var pos = a.readAttribute("data-slide-pos");
              if (!pos) return;
              a.observe("mouseover", function() {
                var oldstate = slideshowFeaturedRotatorWithThumbsBig.state;
                slideshowFeaturedRotatorWithThumbsBig.state = "paused";
                if (slideshowFeaturedRotatorWithThumbsBig.curTimer) clearTimeout(slideshowFeaturedRotatorWithThumbsBig.curTimer);

                var numSlides = slideshowFeaturedRotatorWithThumbsBig.slides.length;
                if (!numSlides) return;

                var nextSlide = pos % numSlides;
                if (!slideshowFeaturedRotatorWithThumbsBig.slides[nextSlide].slide) return;
                if (nextSlide == slideshowFeaturedRotatorWithThumbsBig.curSlide) {
                  slideshowFeaturedRotatorWithThumbsBig.state = oldstate;
                  return;
                }

                var direction = nextSlide - slideshowFeaturedRotatorWithThumbsBig.curSlide;

                if (slideshowFeaturedRotatorWithThumbsBig.transitionDirection) {
                  if (slideshowFeaturedRotatorWithThumbsBig.curAnimator) {
                    slideshowFeaturedRotatorWithThumbsBig.curAnimator.pause();
                    slideshowFeaturedRotatorWithThumbsBig.curAnimator.callback(slideshowFeaturedRotatorWithThumbsBig.curAnimator,
                        slideshowFeaturedRotatorWithThumbsBig.curAnimator.lastStep, slideshowFeaturedRotatorWithThumbsBig.curAnimator.lastStep);
                    slideshowFeaturedRotatorWithThumbsBig.curAnimator = null;
                  }
                  return;
                }

                if (slideshowFeaturedRotatorWithThumbsBig.config.alwaysAnimate) {
                  slideshowFeaturedRotatorWithThumbsBig.doTransition(direction);
                }
                else {
                  for (var i = slideshowFeaturedRotatorWithThumbsBig.slides.length - 1; i >= 0; i--) {
                      var slide = slideshowFeaturedRotatorWithThumbsBig.slides[i].slide;
                      if (!slide) continue;

                      if (i == nextSlide)
                          slide.style.display = "block";
                      else
                          slide.style.display = "none";
                  }

                  slideshowFeaturedRotatorWithThumbsBig.curSlide = nextSlide;
                  slideshowFeaturedRotatorWithThumbsBig.updatePreloadWindow();
                  slideshowFeaturedRotatorWithThumbsBig.updateCaption();
                }
              });
          });

          slideshowFeaturedRotatorWithThumbsBig.start();
      });
    //--></script>
  

      <!-- gridCenterColumn --></div>
    
