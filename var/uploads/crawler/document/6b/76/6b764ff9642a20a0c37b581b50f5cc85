    	var ie = 0;
	   	var NTPT_NOINITIALTAG = true;
  	  if (!Array.prototype.indexOf) {
  	        Array.prototype.indexOf = function (searchElement /*, fromIndex */ ) {
  	            "use strict";
  	            if (this == null) {
  	                throw new TypeError();
  	            }
  	            var t = Object(this);
  	            var len = t.length >>> 0;
  	            if (len === 0) {
  	                return -1;
  	            }
  	            var n = 0;
  	            if (arguments.length > 1) {
  	                n = Number(arguments[1]);
  	                if (n != n) { // shortcut for verifying if it's NaN
  	                    n = 0;
  	                } else if (n != 0 && n != Infinity && n != -Infinity) {
  	                    n = (n > 0 || -1) * Math.floor(Math.abs(n));
  	                }
  	            }
  	            if (n >= len) {
  	                return -1;
  	            }
  	            var k = n >= 0 ? n : Math.max(len - Math.abs(n), 0);
  	            for (; k < len; k++) {
  	                if (k in t && t[k] === searchElement) {
  	                    return k;
  	                }
  	            }
  	            return -1;
  	        }
  	    }
  	  
  	      function getQueryVariable(variable) {
  	        var query = window.location.search.substring(1);
  	        var vars = query.split("&");

  	        for (var i=0;i<vars.length;i++) {
  	          var pair = vars[i].split("=");
  	          if(pair[0] == variable){return encodeURIComponent(pair[1]);}
  	        }

  	        return(false);
  	      }

  	      function goToDefaultURL() {
  	        var defaultURL = './index.jsp?country=US&locale=en';
  	        window.location = defaultURL;
  	      }

  	      function goToURL( country,locale ) {
  	        window.location = './index.jsp?country='+country+'&locale='+locale;
  	      }

  	      var country = getQueryVariable('country');
  	      var locale = getQueryVariable('locale');
  	      var countryArray = ['US','CA','AU'];
  	      var localeArray = ['en','en-AU','en-CA','fr-CA'];
  	      //Validate locale query string
  	      if (locale != false) {
  	        if (localeArray.indexOf(locale) == -1) {
  	          goToDefaultURL();
  	        }
  	      }

  	      // change language of html document on select
  	      document.getElementsByTagName('html')[0].setAttribute('lang', locale);

  	      //check for query string
  	      if (country != false) {
  	        //check that country = US,AU,CA'
  	        if (countryArray.indexOf(country) > -1) {
  	        } else {
  	          goToDefaultURL();
  	        }
  	      } else {
  	        //no query string params
  	        //if no query string, check if cookies exists
  	        if (Cookies('country_preference') && Cookies('locale')) {
  	          //if cookie exists, JS (200) redirect to values in cookie
  	          country = Cookies('country_preference');
  	          locale = Cookies('locale');
  	          goToURL(country,locale);
  	        } else {
  	          //if no cookie exists, JS (200) redirect to US/EN
  	          goToDefaultURL();
  	        }
  	      }

