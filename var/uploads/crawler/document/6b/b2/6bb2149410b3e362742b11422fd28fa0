<script type="text/javascript">
//<![CDATA[
jQuery( document ).ready(function( $ ) {

    if( !$(".header-container.hide-for-small").is(":visible")) {
        $('#footer-links ul li').each(function( index ) {
            if($(this).hasClass('toggler') == true){
                $(this).parent().find('li').hide();
            }
        });
        $('#footer-links ul li:first-child').show();



        //temp hack to add pipes
        var count = 0;
        jQuery('#footer-links ul').each(function() {
            if(count > 0){
                var ul = this;
                jQuery( ul ).find( 'li' ).each(function() {
                    var li = this;
                    var orig_content = jQuery( li ).find( 'a' ).html();
                    if(jQuery( li ).is( ":last-child" ) == false){
                         jQuery( li ).find( 'a' ).html(orig_content+'&nbsp |');
                    }
                });
            }
            count++;
        });

    }

    $('#simple-menu').sidr({
        onClose:function(){
            $('#sticky-header').attr('style', 'position:fixed; top:0; left:0;');
        },
        onOpen:function(){
            $('#sticky-header').attr('style', 'position:fixed; top:0;');
        }
    }
    );

    $(window).resize(function(){
        if ( $(window).width() > 768 ) {
            $.sidr('close', 'sidr');
            $('body').attr('style', '');
        }
    });

    $('#footer-links ul li.toggler').click(function(){

        if( !$(".header-container.hide-for-small").is(":visible")) {
            var ul = $(this).parent();
            var count = 0;
            $(ul).find('li').each(function( index ) {
                if(count > 0){
                    $(this).fadeToggle();
                }
                else{
                    var toggle = $(this).find('.footer-toggle');
                    if($(toggle).hasClass('tilted')){
                        $(toggle).removeClass('tilted');
                    }
                    else{
                        $(toggle).addClass('tilted');
                    }
                }
                count++;
            });
        }
    });

});
//]]>
</script>

