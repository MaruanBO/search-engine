<script>
jQuery(document).ready(function(){

	/*--> international popup start <--*/
	var canSee = 'false';

	//forcing popup off on next line
	canSee = 'false';
	if(canSee == 'true'){
		Jquery.colorbox({
			html:'<p> <a href="/international"><img src="//cdn.alexandani.com/media/popup.jpg"/> </a> </p>',
			innerWidth: 550,
			innerHeight: 200,
			scrolling:false
		});
	}

	/*--> international popup end <--*/



	/*--> coupon popup start <--*/
	var coupon = {"promo_code":"","label":"","hide_codes":false,"show_popup":false};

	if(coupon.show_popup == true){

		var label = coupon.label;
		//var current_code = coupon.promo_code;

		//free shipping coupon start
		if(coupon.promo_code == 'free_shipping_abandoned_cart_2'){

			jQuery.colorbox({
				inline:true,
				href:"#free-shipping-coupon",
				innerWidth: 420,
				scrolling:false
			});
		}
		//free shipping coupon end

		//EWAY START
		if(coupon.label == 'Free Shipping --'){

			var image = '<img class="color-close" src="//cdn.alexandani.com/media/coupon_images/coupon-complementary-shipping.jpg"/>';
			jQuery('#image-coupon').find('.image-coupon-popup-container').html(image);
			jQuery.colorbox({
				inline:true,
				href:"#image-coupon",
				innerWidth: 500,
				innerHeight:175,
				scrolling:false
			});
		}
		if(coupon.label == 'Free Shipping Comp --'){

			var image = '<img class="color-close" src="//cdn.alexandani.com/media/coupon_images/coupon-our-appreciation.jpg"/>';
			jQuery('#image-coupon').find('.image-coupon-popup-container').html(image);
			jQuery.colorbox({
				inline:true,
				href:"#image-coupon",
				innerWidth: 500,
				innerHeight:175,
				scrolling:false
			});
		}
		if(coupon.label == 'Free shipping and 15 percent off --'){

			var image = '<img class="color-close" src="//cdn.alexandani.com/media/coupon_images/coupon-15-pct-off.jpg"/>';
			jQuery('#image-coupon').find('.image-coupon-popup-container').html(image);
			jQuery.colorbox({
				inline:true,
				href:"#image-coupon",
				innerWidth: 500,
				innerHeight:175,
				scrolling:false
			});
		}
		if(coupon.label == '5 dollars off --'){

			var image = '<img class="color-close" src="//cdn.alexandani.com/media/coupon_images/coupon-first-time-5-dollars-off.jpg"/>';
			jQuery('#image-coupon').find('.image-coupon-popup-container').html(image);
			jQuery.colorbox({
				inline:true,
				href:"#image-coupon",
				innerWidth: 500,
				innerHeight:175,
				scrolling:false
			});
		}
		if(coupon.label == '10% off item --'){

			var image = '<img class="color-close" src="//cdn.alexandani.com/media/coupon_images/coupon-10-pct-off.jpg"/>';
			jQuery('#image-coupon').find('.image-coupon-popup-container').html(image);
			jQuery.colorbox({
				inline:true,
				href:"#image-coupon",
				innerWidth: 500,
				innerHeight:175,
				scrolling:false
			});
		}
		//EWAY END

		//--> other coupon specific popups
		//-->
		//-->
		//-->

	}
	//This is coupon agnostic and will hide various shipping information if free shipping is
	//applied by the current promotion
	if(coupon.hide_codes == true) {
		var message = "Congrats! You've reached Free Shipping!";

		jQuery('.free-shipping-note').html(message);
		jQuery('.free-shipping-msg').html(message);

		jQuery('#coupon_code').val(coupon.label);
		jQuery('#discount-coupon-form .button.cta').hide();

	}
	/*--> coupon popup end <--*/





	//used to close popups
	jQuery('.color-close').click(function(){
		var parent = jQuery(this).attr('popup');
		jQuery('#'+parent).colorbox.close()

	});


});
</script>
