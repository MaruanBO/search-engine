function hpfeatureheightadjust(){
  $(".secondaryFeatures, .leadFeature").height("auto");
  var leadheight = $(".leadFeature #feature-slider").height();
  var secondaryheight = $(".secondaryFeatures").height();
  if(leadheight>secondaryheight){
    $.each(
      $(".secondaryFeatures li"),
      function(){
        $(".secondaryFeatures").height(leadheight);
      }
    )
  }else{
    $("#featured-content").height(secondaryheight);
  }
}
