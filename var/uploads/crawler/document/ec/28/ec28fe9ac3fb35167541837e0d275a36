        //CD
    
        //Functions used by the flash to set the cookie for sound
        function setCookie(playsoundbool)
        {
            CookieControl.createCookie("olliesplaysound", playsoundbool, 365); 
        }
        
        //Function to see if cookie exists to control sound functionality
        function checkCookie()
        {
            var exists = CookieControl.readCookie("olliesplaysound");

            return exists;
        }
    
    
