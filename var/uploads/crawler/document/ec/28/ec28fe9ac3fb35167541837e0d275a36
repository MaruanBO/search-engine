<script type="text/javascript">
    jQuery(document).ready(function () {




        var settngs = $.data($('form')[0], 'validator').settings;
        var oldErrorFunction = settngs.errorPlacement;
        var oldSucessFunction = settngs.success;
        settngs.errorPlacement = function (error, inputElement) {

            if (jQuery(inputElement).data('hasTooltip') != true) {
                jQuery(inputElement).data('hasTooltip', true);
                jQuery(inputElement).tooltip({
                    title: function () { return jQuery(error).html(); },
                    template: '<div class="tooltip tooltip-validation-error" data-tooltip-for="' + jQuery(inputElement).attr('id') + '"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>',
                    placement: 'right',
                    trigger: 'focus',
                    html: true,
                    container: 'body'
                }); //.tooltip('show');

            }

            error.data('field', jQuery(inputElement));

            oldErrorFunction(error, inputElement);
        }
        window.$data = [];
        settngs.success = function (error) {
            //console.log('success');
            //$(error).closest('.fieldset').removeClass('fieldset-validation-error')
            
            jQuery(error.data('field'))
                .data('hasTooltip',false)
                .tooltip('destroy');
            jQuery('[data-tooltip-for="' + jQuery(error.data('field')).attr('id') + '"]').remove();
            oldSucessFunction(error);
        }

    });
</script>
