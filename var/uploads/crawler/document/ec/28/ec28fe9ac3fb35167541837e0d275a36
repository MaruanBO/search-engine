<SCRIPT>
		
		function controlEnterCommand(event, clickObjName){
			var isEnter = false;
			var keyCode = "";
			if(window.event)
             			keyCode = window.event.keyCode;
         		else
              			keyCode = event.which; 
			if(keyCode == 13 || keyCode == 3)
				isEnter = true

				
			if(isEnter){
				var btn = document.getElementById(clickObjName);
				if(btn)
					btn.click();
			}
				
			return !isEnter;
		}
		

	</SCRIPT>

