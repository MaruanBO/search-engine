                        function storeFinderEnterCommand(event, clickObjName){
            var isEnter = false;
            var keyCode = "";
            if(window.event)
                        keyCode = window.event.keyCode;
                else
                        keyCode = event.which; 
            if(keyCode == 13)
                isEnter = true

            if(isEnter){
                storeFinder();
            }
                
            return !isEnter;
        }
                        
                    function storeFinder(){									
                      var radius = 0;
                      var selRadius = "";
                      var selPostalCode = "";
                      
                      selPostalCode = document.getElementById("tfZip").value;
                      selRadius = document.getElementById("ddMiles").value;
                      if (selPostalCode=="Zip Code"){
                        selPostalCode = "Zip+Code";
                      }
                      switch(selRadius){
                        case "5 miles":
                          radius = 5;
                          break;
                          
                        case "10 miles":
                          radius = 10;
                          break;
                          
                        case "20 miles":
                          radius = 20;
                          break;
                          
                        case "50 miles":
                          radius = 50;
                          break;
                          
                        case "All":
                          radius = "ALL";
                          break;
                      }
                      
                        var url = "/storeLocator/storeLocator.aspx?radius=" + radius + "&postalcode=" + selPostalCode;
                        self.location.href = url;
                        return false;
                      
                    }	
                  </script>
