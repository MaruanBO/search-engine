                <script>
                    jQuery(document).ready(function() {

//sort sidebar nav groups

                    	var theID = '2';

                    	jQuery('ul#menu-tremor-menu>li').tsort({attr:'class'});

                    	//jQuery( ".2-g:first,.3-g:first,.4-g:first,.5-g:first" ).addClass('nav-sep')
                    	jQuery( ".1-g:first,.2-g:first,.3-g:first,.4-g:first,.5-g:first" ).prepend('<div class="nav-sep"></div>');


//set active section
                    	jQuery('ul#menu-tremor-menu>li,ul.tremor-submenu>li').each(function( index ) {

                    		myRel = jQuery(this).attr('rel');

                    		if(myRel == theID){

                    			jQuery(this).addClass('active-item');

                    			myParent = jQuery(this).closest('ul').closest('li');
                    			myRel = myParent.attr('rel');

                    			if(!myRel) {

                    				jQuery(this).addClass('active-section')

                    			} else {

									jQuery(this).closest('ul').closest('li').addClass('active-section')

                    			}
                    			
                    			//console.log('closest:'+myParent.attr('rel'));
                    			//console.log( jQuery(this).attr('rel'));

                    		}
						  
						});

//show only active submenu, hide the others

                    	jQuery('.tremor-submenu').each(function( index ) {

							findActiveSection = jQuery(this).closest('ul').closest('li');

							findActiveItem = jQuery(this).find('.active-item');
							
							if(findActiveItem.length == 0 && findActiveSection.hasClass('active-section')==false){
								jQuery(this).hide();
							}
							
							//console.log("activeItem:"+findActiveItem.length);
							//console.log("activeSection:"+findActiveSection.hasClass('active-section'));
							

                    	});

//hide nav-sep if inside an active-section

                    	jQuery('.active-section').each(function( index ) {

                    		jQuery(this).children('div').css('display','none');

                    	});

//hide nav-sep if previous section has active-section

                    	jQuery('.nav-sep').each(function( index ) {

                    		if(jQuery(this).parent().prev('li').hasClass('active-section')==true){

								jQuery(this).hide();

                    		}
                    		
                    		//console.log("parent li:"+jQuery(this).parent().prev('li').attr('rel'))

                    	});						

                    });
                </script>	
    
    
