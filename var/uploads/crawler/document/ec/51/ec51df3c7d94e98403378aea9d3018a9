<script type="text/javascript">
$(document).ready(loadSignupOverlay);
function loadSignupOverlay(){
    $("input#fb-button").click(function(){
        fbPromoValue = $('input[name=ebates.signup.promo]:checked', '#signForm').val();
        linkWithFacebook('214330088590858', 'http://www.ebates.com/','FBUserSignupOrLogin.do');
    });
    $("form#signForm").ebatesValidate({
        giftscards: true,
        overlay: true,
        ieZindexIds:"pge-lt,topnav", // zindex ids for ie
        popupPositionDiv:"signup-form" // popup position
    });
    $("a#terms").click(function(){
        loadwindow('/help/terms_and_conditions_popup.htm',425,250);
    });
    $("a#privacy").click(function(){
        loadwindow('/help/privacy_popup.htm',425,250)
    });
	        $("a.referrer").click(function(){
            $("li.refer").remove();
            $("li.refer-input").show();
            $("#referral_email_address").focus().removeClass('inputblur');
        });
	    // check giftcard
    $("ul#gcs li").click(function(){
        $(this).children(':first-child').attr('checked', true).focus();
    });
    $("#backgroundPopup").click(function(evt){
        $("#backgroundPopup").fadeOut("slow",function(){
            if(jQuery.browser.msie && jQuery.browser.version == 7.0){
                var zIndexNumber = 9999;
                $('div#pge-lt').each(function(){
                    $(this).css('zIndex', zIndexNumber);
                    $('ul').css('zIndex', zIndexNumber);
                    zIndexNumber -= 10;
                });
            }
            $(this).remove();
            $(document).trigger("overlay-closed");
        });
        clearOvr(evt);
    });
    $(".remove-ovr").click(function(evt){
        //clearOvr(evt);
    });
    function clearOvr(e){
        signupOverlayCached = $("#signupOverlay");
        if(!e.ctrlKey && !e.metaKey && !e.shiftKey && !e.altKey){
            if($("div.coupon-landing").length>0){/*do nothing*/}else{$("#signupOverlay").remove();}
        }
    }
    // trademark popup
    $("#trademark").click(function(){
        $("#trademark_popup").show();
    });
    var suri = location.pathname;
}
</script>
		

