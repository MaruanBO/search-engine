			}else{
			window.addEventListener('load', async_load, false);
			}
			
			function linkWithFacebook(fbAppId, siteRoot, urlAction){
                var browserUrlInfo = location.pathname + location.search;
				var finalUrlAction;
				if(typeof fbPromoValue !== "undefined" && fbPromoValue !== ""){
					if(typeof fbsplitId !== "undefined" && fbsplitId !== ""){
						finalUrlAction = urlAction+"?split_entry_id="+fbsplitId+"&ebates.signup.promo="+fbPromoValue+"&urlIdentifier="+encodeURIComponent(browserUrlInfo); 
					} else {
						finalUrlAction = urlAction+"?ebates.signup.promo="+fbPromoValue+"&urlIdentifier="+encodeURIComponent(browserUrlInfo);
					}
				} else {
				    finalUrlAction = urlAction+"?urlIdentifier="+encodeURIComponent(browserUrlInfo);
				}
				var url_info = 'https://www.facebook.com/dialog/oauth?client_id='+fbAppId+'&redirect_uri='+siteRoot+encodeURIComponent(finalUrlAction)+'&display=popup'; 
				if (unlock == "true"){
				    getLoginWithFacebook(url_info);
				}
			   fbPromoValue = "";
			}
			function getLoginWithFacebook(url_info) {
				unlock = "false";
				FB.login(function(response) {
					unlock = "true";
					  if (response.authResponse) {
						if (response.status == "connected") {
							window.location = url_info;
						}				
					  } 
				}, {scope:'email'});
			}	
