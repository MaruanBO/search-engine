      $(document).ready(function (){
      
      	/* Fade on screen hover */      	
      	$('.monitor-overlay', this).fadeTo(0, function(){ $(this).show(); });    	       
        $('.monitor').bind({
        	mouseenter: function() {
        		$('.monitor-overlay', this).stop(true, true).fadeTo(200, 0.3);
        	},
        	mouseleave: function() {
        		$('.monitor-overlay', this).stop(true, true).fadeTo(200, 0);
        	}
        });
      
      	/* Set illustration classes */
      	setTimeout(function(){
		    $('.survey-illustration').addClass('visible');  	
      	}, 500)      	
      
      	/* Bind tab events */      	
        $('#front-alt-tabs li').live('click', function(){        	
        	
        	if ($(this).hasClass('current')) {        	
	        }
        	else {        	
	        	$('#front-alt-tabs li.current').removeClass('current');
	        	$(this).addClass('current');
	        	rel = '#'+$(this).attr('rel');        	
	        	$('.tab-content.current').removeClass('current').stop(true, true).fadeOut(100);
	        	$(rel).addClass('current').stop(true, true).fadeIn(500);
	        }
        });
        
	  //PrettyPhoto script
	  $("a[rel^='prettyPhoto']").prettyPhoto();
        
      });
      
    </script>

