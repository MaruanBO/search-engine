<script type="text/javascript">
var anonymousDivId='login-info';
var loggedDivId='logged-info';
$(function() {
	var beAuth = "";
	if(anonymousDivId) {
		var anonymousDiv = $('#' + anonymousDivId);
		if(anonymousDiv){
			if(beAuth) {
				anonymousDiv.hide();
			} else {
				anonymousDiv.show();
			}
		}
	}	
	if(loggedDivId) {
		var loggedDiv= $('#' + loggedDivId);
		if(loggedDiv){
			if(beAuth) {
				loggedDiv.show();
			} else {
				loggedDiv.hide();
			}
		}
	}	
});

</script>

