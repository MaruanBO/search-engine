sortingRuleID
template


<script type="text/javascript">
// scripting to handle mikasa email popup
jQuery(function($) {
  var createCookie = function (name, value, days) {
      var expires = '';
      if (days) {
          var date = new Date();
          date.setTime(date.getTime() + (days*24*60*60*1000));
          var expires = '; expires=' + date.toGMTString();
      }
      document.cookie = name + '=' + value + expires + '; path=/';
  };
  
  var readCookie = function (name) {
      var cookieCrumbs = document.cookie.split(';');
      var nameToFind = name + '=';
      for (var i = 0; i < cookieCrumbs.length; i++) {
          var crumb = cookieCrumbs[i];
          while (crumb.charAt(0) == ' ') {
              crumb = crumb.substring(1, crumb.length); /* delete spaces */
          }
          if (crumb.indexOf(nameToFind) == 0) {
              return crumb.substring(nameToFind.length, crumb.length);
          }
      }
      return null;
  };
  
  /*
  * check for 'sr' param and 'ecid' param to denote Eway
  * if exists, set mkaEmail cookie to 'subscribed'
  */
   var isEway = false;
  
  $.params = function(param_name){
    var value = new RegExp('[\\?&]' + param_name + '=([^&#]*)').exec(window.location.href);
    if(value != null && value != undefined){
      return value[1];
    }
  }

  if($.params('sr') != null && $.params('sr') != ""){
    isEway = true;
  }

  if($.params('ecid') != null && $.params('ecid') != ""){
    isEway = true;
  }

  if(isEway){
    createCookie('mkaEmail', 'subscribed', 3650);
  }

  // Check the pagetype; skip email popup for "BrandExperience" pages
  // (homepage and magazine pages)
  var mkaPageType = jQuery('#pageContext').text();
  if (mkaPageType != null)
  {
    if(mkaPageType.indexOf('BrandExperience') == -1)
    {
      // Only trigger dialog if mkaEmail cookie is not found
      // AND the page context is NOT "BrandExperience"
      if (readCookie('mkaEmail') == null)
      {
        // skip the popup for urls containing certain keywords
        if (window.location.href.indexOf("affinity-solutions") == -1)
        {
             jQuery('#mikasa-email').click();
        }
        
      }     
    }
  }
  
  $('.mkaEmail a').live('click', function(event) {
    
    if (this.id == 'submit-form')
    {
      createCookie('mkaEmail', 'subscribed', 3650);
      
      // submit the form
      mkaEmailForm.submit();
    }
    else if (this.id == 'no-thanks')
    {
      // No Thanks -- Don't Ask Again
      createCookie('mkaEmail', 'no-thanks', 3650);
    }
    else if (this.id == 'remind-me')
    {
      // Not Now -- Remind Me Later
      createCookie('mkaEmail', 'remind-me', 7);
    }
    else if (this.id == 'subscribed')
    {
      // Already Subscribed to Emails
      createCookie('mkaEmail', 'subscribed', 3650);
    }
    
    // Close the layer if open
    if($('#QuickViewDialog').dialog('isOpen'))
    {
      $('#QuickViewDialog').dialog('close');
    }
    
    return false;
  });
  
  $(".mkaEmail a.ui-dialog-titlebar-close")
    .live("dialogclose", function(event, ui) {
      // register dialog close as a "remind me later" action
      createCookie('mkaEmail', 'remind-me', 7);
    });
      
});
</script>

