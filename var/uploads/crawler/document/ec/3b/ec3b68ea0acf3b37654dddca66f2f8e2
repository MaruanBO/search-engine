    	<script>
    	jQuery('.loginpopup').hide();
			function hideLogin() {
				jQuery('.loginpopup').stop(true,true).delay(500).hide(100);
			}

			function showLogin() {
				var loginInput = jQuery('.loginpopup').find('.textinput');
				var pwInput = jQuery('.loginpopup').find('.textinputpw');
				var dummyInput = jQuery('.loginpopup').find('.dummypassword');
				jQuery('.loginpopup').find('.bluebutton').show();
				pwInput.hide().removeClass('errorclient').blur();
				dummyInput.show().blur();
				loginInput.val('Enter email address/username').css('color', '#ccc').removeClass('errorclient');
				jQuery('.loginpopup').show();
				loginInput.blur();
			}
    	
    		jQuery('#headerLogin').on("hover", function() {
	   			if(!jQuery('.loginpopup').is(":visible")) {
					showLogin();
	   			}    				
    		});
    		
    		jQuery('.loginpopup').on("mouseleave", function() {
    			hideLogin();
    		});

    		jQuery(document).ready(function() {
    			var loginInput = jQuery('.loginpopup').find('.textinput');
    			var pwInput = jQuery('.loginpopup').find('.textinputpw');
				var dummyInput = jQuery('.loginpopup').find('.dummypassword');
    			dummyInput.click(function(){
					dummyInput.hide();
					pwInput.show().val('').focus();
        		});
    			dummyInput.focus(function(){
					dummyInput.blur().hide();
					pwInput.show().val('').focus();
        		});
    			loginInput.click(function(){
					loginInput.val('').css('color', 'black');
        		});
    		});
    	</script>
    
		
