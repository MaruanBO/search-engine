	<script type="text/javascript">
		function CheckForm_old(f) {
			var rx = new RegExp("\\w+([-+.\']\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*");
			var matches = rx.exec(f.email.value);
			//RegExp for email
			if(matches == null || f.email.value != matches[0]){
				alert("Please check your email address.");
				f.email.focus();
				return false;
			}
			return true;
		}
	</script>
