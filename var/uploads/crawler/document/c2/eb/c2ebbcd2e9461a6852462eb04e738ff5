<script type="text/javascript">
	function callCheetahPreferencesFromFooter(){
		var userEmailFooter = $("#userEmailFooter").val();		
		if (trim(userEmailFooter) != '')
			$("#userEmail").val(userEmailFooter);		
		else
			$("#userEmail").val('your email address');		
		var subscribeForm = document.subscribeForm;	    
		subscribeForm.submit();		
	    
	}
</script>

	<script type="text/javascript">
	
   	$(document).ready(function() {
			$(".email-signup-contianer #userEmailFooter").val('your email address');
			var userEmail;
	
			$(".email-signup-contianer #userEmailFooter").focus(function(){
				userEmail = $(this).val();
				if (userEmail == 'your email address' || trim(userEmail) == ''){
					$(this).val('');
				}
			});
			
			$(".email-signup-contianer #userEmailFooter").blur(function(){
				userEmail = $(this).val();
				if (trim(userEmail) == '' || userEmail == 'your email address'){
					$(this).val('your email address');
				}
			});
	});
	
	</script>

