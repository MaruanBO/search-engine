	<script>
	$(document).ready(function() {
		
		
		$('#form_RtTEI5QG').form({
			submit: function(e) {},
			beforeSubmit: function(e) {return createForumNextStep(this, e);},
			validations: [{"opts":{},"name":"exists","message":"Forum Name Required","field":"username"},{"opts":{"less_than_equal_to":20},"name":"length","message":"Name can only be up to 20 characters long","field":"username"},{"opts":{},"name":"exists","message":"Select a category.","field":"category"},{"opts":{},"name":"exists","message":"Select your birth month.","field":"birthday_month"},{"opts":{},"name":"exists","message":"Select your birth day.","field":"birthday_day"},{"opts":{},"name":"exists","message":"Select your birth year.","field":"birthday_year"},{"opts":{},"name":"exists","message":"Select your gender.","field":"gender"},{"opts":{},"name":"exists","message":"Email Required","field":"email"},{"opts":{},"name":"email","message":"Invalid Email","field":"email"},{"opts":{},"name":"exists","message":"Password Required","field":"password"},{"opts":{"greater_than_equal_to":6},"name":"length","message":"Your password must be between 6 and 100 characters long","field":"password"},{"opts":{"less_than_equal_to":100},"name":"length","message":"Your password must be between 6 and 100 characters long","field":"password"}],
			errors: {},
			positionError: 
	function(message) {
		var $input     = $(this);
		var errorHtml  = message;
		var $errorElem = $('.ui-form-error');
		var isMobile   = $.isMobile && $.isMobile();
		
		// If the input is not visible, try to position it by the submit button instead
		if (!$input.is(':visible')) {
			$input = $('input[type="submit"]', $input.closest('form'));
		}
		
		$errorElem
			.removeClass('mobile')
			.removeAttr('style')
			.html(errorHtml)
			.show();
		
		// Display the error next to the input field if it's not mobile.
		if (!isMobile) {
			$errorElem
				.position({
					my:         'left center',
					at:         'right center',
					of:         $input,
					collision:  'flipfit',
					offset:     '5 0'
				})
				.attr('role', 'alert');
		}
		// If mobile, we simply display the error at the top of the screen.
		else {
			$errorElem.addClass('mobile');
		}
		
		var windowTop    = $(window).scrollTop();
		var windowBottom = windowTop + $(window).height();
		
		// If the input is the submit button, scroll to the top of the form... always
		if ($input.is('input[type="submit"]')) {
			$.scrollTo($input.closest('form'));
		}
		
		// Otherwise, scroll to the input if it's not already on screen
		else {
			var inputTop    = $input.offset().top;
			var inputBottom = inputTop + $input.outerHeight();
			
			// If mobile, scroll a little further up to allow space for the error msg
			if (isMobile) inputTop -= $errorElem.outerHeight() + 5;
			
			// Top and bottom should be within viewport bounds
			if (windowTop > inputTop || windowBottom < inputBottom) {
				$.scrollTo(inputTop);
			}
		}
	}

		});
	});
	</script>
	
