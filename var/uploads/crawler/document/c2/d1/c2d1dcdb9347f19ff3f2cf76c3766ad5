									<p class="summary-sidebar-order-item-discount" ng-show="isDiscounted($parent.$parent.$parent.$index)">with discount applied<span class="summary-sidebar-order-item-price">{{cartItem.accessoryArray[0].effectivePrice |  tivoCurrency}}</span></p>
								</div>								
								<div ng-switch-default>
									<p class="summary-sidebar-order-item-discount" ng-show="isDiscounted($parent.$parent.$parent.$index)">with discount applied<span class="summary-sidebar-order-item-price">{{cartItem.boxBundleArray[0].boxEffectivePrice |  tivoCurrency}}</span></p>
								</div>				
							</div>						
						</div>		
						<div ng-switch-when="ACCESSORY">
							<p class="summary-sidebar-order-item-discount" ng-show="isDiscounted($parent.$parent.$index)">with discount applied<span class="summary-sidebar-order-item-price">{{cartItem.effectivePrice |  tivoCurrency}}</span></p>
						</div>    
						<div ng-switch-when="SOFTWARE">
							<p class="summary-sidebar-order-item-discount" ng-show="isDiscounted($parent.$parent.$index)">with discount applied<span class="summary-sidebar-order-item-price">{{cartItem.effectivePrice |  tivoCurrency}}</span></p>
						</div> 
					</div>							
                </li>                
                <li>				
					<div ng-show="isNoService($parent.$index) && cartItem.boxBundleArray[0].boxSku !== 'RA9300'">
