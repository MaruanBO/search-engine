         <div id="cart-container"><img class="show-mini-cart" ng-click="showMiniCart()" src="/sites/all/themes/tivo3/images/navigation/desktop-nav-shopping-cart.png" /> <!-- <a id="cartCount" href="/shop/cart">{{tivoCookie.cartCount}} </a> -->
			<a id="cartCount" ng-click="showMiniCart()" href="javascript:void(0)"  ng-class="tivoCookie.cartCount  < 1 ? 'mini-cart-disable' : ''">{{tivoCookie.cartCount}}</a>			
			
			<!--  Start mini cart drop down div -->			
				<div ng-show="miniCartFlag" class="mini-cart-dropdown-outer" id="miniCartDiv" ng-click="detectMiniCartClick()">	
					<div ng-controller="CartCtrl" ng-cloak class="ng-cloak" ng-click="triggerEvent()"> 		
							<div class="mini-cart-dropdown">
								<div class="mini-cart-dropdown-arrow"></div>			
								<div class="mini-cart-dropdown-head">Your cart</div>
								<div class="mini-cart-dropdown-con" ng-show="!isEmptyCartItems()" >	
									<div class="mini-cart-con-inner">	
										<tivo-mini-cart-items cart-items="getCartItems()" ></tivo-mini-cart-items>	
									</div>
									<ul class="subtotal">
										<li class="mini-cart-text-14">
											<!-- add total section -->
											<div class="order-summary clearfix" ng-controller="OrderSummaryCtrl">
												<div ng-hide="isEmptyCartItems()">										
														<span>SUBTOTAL:</span>
														<span class="mini-cart-dropdown-right" ng-if="!isDiscountApplied()">
															<span class="summary-sidebar-price-number subtotal-display">{{getCartSubTotal()|currency:'$'}}</span>
														</span>
														<span class="mini-cart-dropdown-right" ng-if="isDiscountApplied()">
															<span class="summary-sidebar-price-number subtotal-display strike">{{getCartSubTotal()|currency:'$'}}</span><span class="summary-sidebar-price-number subtotal-display discount">&nbsp;{{getSubTotalAfterPromo()|currency:'$'}}</span>
														</span> 
												</div>
											</div>
											<!-- end total section -->                   
										</li>         
									</ul>			
									<div class="mini-cart-txt-center"><br />
									<a href="/shop/cart" class="mini-cart-orange-btn"><span class="mini-cart-text-20">View cart</span>&nbsp; ({{tivoCookie.cartCount}}) items</a>
								  </div>		
							</div>
