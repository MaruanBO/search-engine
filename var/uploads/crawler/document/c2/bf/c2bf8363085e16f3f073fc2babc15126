    var SpanishLink = (function () {


        var init = (function (enableSpanishLink, url, showMessage) {

            if (enableSpanishLink) {
                $("#spanHeadSpanishPortal").show();
                $("#lnkHeadSpanishPortal").attr("href", url);
                $("#lnkHeadSpanishPortal").attr("data-showmessage", showMessage);

                var showModal = $("#divSpanishCustomMessages div[data-showmessage='" + showMessage + "']");

                if (showModal.length > 0) {
                    $(showModal).dialog({
                        autoOpen: false,
                        modal: true,
                        draggable: false,
                        stack: true,
                        resizable: false,
                        closeOnEscape: false,
                        dialogClass: 'spanishlnk-dialog',
                        open: function (event, ui) {
                            $(this).parents(".ui-dialog:first").find('.ui-dialog-titlebar-close').focus();
                            
                            $(this).parents(".ui-dialog:first").find("*:focusable:last").bind("keydown", function (e) {
                                if (!e.shiftKey && e.keyCode == 9) {
                                    e.preventDefault();
                                    $(this).closest(".ui-dialog").find(".ui-dialog-titlebar-close").focus();
                                }

                            });
                        },
                        buttons: {
                            Continue: function () {
                                window.location.href = url;
                            },
                            Cancel: function () {
                                $(this).dialog('close');
                            }
                        },
                        width: '450px',
                        height: '200px'
                    });
                }


                $("#lnkHeadSpanishPortal").click(function () {
                    if (showModal.length > 0) {
                        $(showModal).dialog('open');
                        return false;
                    }
                    else {
                        return true;
                    }
                });

            }
            else {
                $("#spanHeadSpanishPortal").hide();
            }

            return false;
        });

        return {
            Init: init
        };

    })();

    
    SpanishLink.Init(true, "https://es.anthem.com/cp/web/abcbs/inicio", "AVAILABLE");
    
