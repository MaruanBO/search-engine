<script type="text/javascript">
    /* menu slide animation duration (in milliseconds) */
    var duration = 100;

    function closeNow() {
        $('.placeholderDropdown').hide();
    }

    $(document).ready(function () {

        /*functions for shopping abg dropdown */

        $(".bag").mouseover(function () {
            $('.placeholderDropdown').show();
        });

        $(".placeholderDropdown").hover(
          function () {
              //console.log("mouseoVER");
              $('.placeholderDropdown').show();
          }, function () {
              //console.log("mouseout");
              closeNow()
          }
        );

        newDeptIdGet();
        //oldDeptIdGet();
    });

    function oldDeptIdGet() {
        if (!($.browser.msie && (parseFloat($.browser.version) < 8)) || !location.href.match(/AffiliateBrowsing|bags.womanwithin|art.womanwithin|pbj.womanwithin|stg.bags/)) {
            $(".m-subCat-dropdown[deptid]").each(function (index) {
                var cutoff = $(this).attr('cutoff');
                var currentColumn = $(this);

                $.ajax({
                    type: 'GET',
                    url: '../MainMenu.ashx',
                    data: {
                        deptid: $(this).attr('deptid')
                    },
                    dataType: "json",
                    success: function (data) {
                        AjaxGetCategorySuccess(data);
                    },
                    error: AjaxGetCategoryError
                });

                function AjaxGetCategorySuccess(data) {

                    if (data == null) {
                        var currentATag = $("<a />").html("OH God something is wrong").attr("href", "google.com").appendTo(currentColumn).addClass("viewExtras").wrap('<li></li>');
                    }
                    else {
                        $.each(data, function (i, item) {

                            if (typeof cutoff !== 'undefined' && cutoff !== false) {
                                if (i >= cutoff - 1) {
                                    var currentATag = $("<a />").html(item.displayName).attr("href", item.catUrl).appendTo(currentColumn).addClass("viewExtras").wrap('<li></li>');
                                }
                                else {
                                    var currentATag = $("<a />").html(item.displayName).attr("href", item.catUrl).appendTo(currentColumn).wrap('<li></li>');
                                }
                            }

                        });
                    }
                    $('.viewAll').on('click', function () {
                        'use strict';
                        //console.log('click view all');
                        $('.viewExtras').css('display', 'block');
                        $(this).hide();
                    });
                    var lastElement = $(".m-subCat-dropdown[deptid]").length - 1;
                    if (lastElement >= index) {
                        setAfterAjax();
                    }
                }

                function AjaxGetCategoryError(XMLHttpRequest, textStatus, errorThrown) {
                }

            });
        }
    }
    function newDeptIdGet() {
        var DeptIDArray = "";
        /* dynamic adding of divs */
        $(".m-subCat-dropdown[deptid]").each(function (index) {
            var cutoff = $(this).attr('cutoff');
            if (index == 0) {
                DeptIDArray = $(this).attr('deptid');
            }
            else {
                DeptIDArray += "|" + $(this).attr('deptid');
            }
        });

        $.ajax({
            type: 'GET',
            url: '../MainMenu.ashx',
            data: {
                deptid: DeptIDArray
            },
            dataType: "json",
            success: function (data) {
                AjaxGetCategorySuccess(data);
            },
            error: AjaxGetCategoryError,
        });

        function AjaxGetCategorySuccess(data) {
            if (data == null) {
                var currentATag = $("<a />").html("Oh god something is wrong").attr("href", "Google.com").appendTo(currentColumn).wrap('<li></li>');
            }
            if (!($.browser.msie && (parseFloat($.browser.version) < 8)) || !location.href.match(/AffiliateBrowsing|bags.womanwithin|art.womanwithin|pbj.womanwithin|stg.bags/)) {
                $(".m-subCat-dropdown[deptid]").each(function (index) {
                    var cutoff = $(this).attr('cutoff');
                    var currentColumn = $(this);
                    $.each(data, function (i, item) {
                        if (item.id == currentColumn.attr('deptid')) {
                            $.each(item.childCategories, function (i, subItem) {
                                //console.log(subItem);
                                if (typeof cutoff !== 'undefined' && cutoff !== false) {
                                    if (i >= cutoff - 1) {
                                        var currentATag = $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(currentColumn).addClass("viewExtras").wrap('<li></li>');
                                    }
                                    else {
                                        var currentATag = $("<a />").html(subItem.displayName).attr("href", subItem.catUrl).appendTo(currentColumn).wrap('<li></li>');
                                    }
                                }
                            });
                        }
                    });
                });
            }
            $('.viewAll').on('click', function () {
                'use strict';
                //console.log('click view all');
                $('.viewExtras').css('display', 'block');
                $(this).hide();
            });

            setAfterAjax();
        }

        function AjaxGetCategoryError(XMLHttpRequest, textStatus, errorThrown) {

            console.log("XMLHttpRequest: " + XMLHttpRequest);
            console.log("textStatus: " + textStatus);
            console.log("errorThrown: " + errorThrown);
        }
    }


    function setAfterAjax() {
        $(".m-subCat-dropdown").attr("style", "display: block !important");
        var isTouchDevice;
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent))
        { var isTouchDevice = true; }
        else
        { var isTouchDevice = false; }

        //fix for Ipad  so you dont have to double click an a tag
        if (isTouchDevice) {
            //When we click white space we want to close the dropdowns

            $('body').on('touchmove', function (event) {
                dragging = true;
            });
            $('body').on('touchstart', function (event) {
                dragging = false;
            });

            $('body').on('click touchend', function (event) {
                if (dragging) {
                    return;
                }
                var target = $(event.target);
                if (typeof OverrideRefinementLinkBehavior != 'function') {
                    if (target.parents('.nav-level-2').length > 0 || target.parents('#department-nav').length) {
                        if (target.closest('a').length > 0) {
                            event.preventDefault();
                            var link = target.closest('a').attr('href');
                            window.location = link;
                        }
                    }
                }
                //Do we want to click on a dropdown item? Then dont close the dropdown and click through on 1 click.
                if (target.hasClass('e-rootCat-link')) {
                    if (target.hasClass("isActive")) {
                        //Do Nothing
                    }
                    else {
                        event.preventDefault();
                        $('.e-rootCat-link').removeClass('isActive');
                        target.addClass("isActive");
                    }
                }
                else {
                    if (target.parents('.m-globalNav').length > 0) {
                        if (target.is("a")) {
                            $('.m-subCat-dropdown').css('left', '-9999px');
                            var link = target.attr('href');
                            window.location = link;

                        }
                        else {
                            // We didnt click on the dropdown but we did click on what pops up the dropdown so dont open then close
                            if (!target.hasClass('e-rootCat-link')) {
                                $('.m-subCat-dropdown').css('left', '-9999px');
                            }
                        }
                    }
                }
            });
        }

    }


</script>
