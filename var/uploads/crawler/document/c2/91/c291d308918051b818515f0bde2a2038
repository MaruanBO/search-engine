    <script type="text/javascript">
        var CreditCardAndEmail = {
            setup: function () {
                // CREDIT CARD OFFERS
                // checkForOffers() from HeaderInit.js uses hard-coded images. We'll use populateCreditCardOffer() instead.
                function checkForOffers() { /* Doesn't work. checkForOffers() needs to be removed from the solution. */ }

                function populateCreditCardOffer() {
                    // Original code showed a blank image on page load, then replaced it with default or pre-approved message.
                    // We'll show a default message on page load, then replace it if necessary. No blank space and less code.
                    if (userinfo != null && userinfo != '') {
                        var offerBrandCreditCard = GetSubCookieValue('User', 'Indy.BrandCreditCard'),
                        offerPreApproval = GetSubCookieValue('User', 'Indy.PreApproval'),
                        offerAvailableCredit = GetSubCookieValue('User', 'Indy.AvailableCredit'),
                        blankImage = '<img class="blank" src="//secureimages.plussizetech.com/images/site_images/womanwithin/blank.png" height="20" alt="credit card" />';

                        if (offerPreApproval == 'A' || offerPreApproval == 'True') {
                            $('#header-credit-cards').html('<a href="/Account/Acct_CreditCards.aspx">Great news! Your available credit: <strong>$' + offerAvailableCredit + '</strong>' + blankImage + '</a>');
                            showofferBrandCreditCard(offerBrandCreditCard);
                        } else if (offerPreApproval == 'P') {
                            $('#header-credit-cards').html('<a href="/Account/Acct_PreQualifiedOffer_plcc.aspx">Pre-approved? <strong>Learn more!</strong>' + blankImage + '</a>');
                            showofferBrandCreditCard(offerBrandCreditCard);
                        }
                    }
                }

                populateCreditCardOffer();

                // checkBrand() was tightly coupled with checkForOffers() and was pretty sloppy. We'll use
                // showofferBrandCreditCard() instead. checkBrand() needs to be removed from the solution.
                function showofferBrandCreditCard(card) {
                    // Woman Within cards have a 'W' flag. Combinations of FullBeauty Brands credit cards return a number
                    // of different two-digit flags. We'll default to the WW card like the account home page does.
                    var imageSource = imageSource = 'ww_header_10-2013_ww_credit_card.png';

                    if (card == 'O') { imageSource = 'ww_header_10-2013_osp_credit_card.png'; }
                    else if (card == 'P') { imageSource = 'ww_header_10-2013_osp_credit_card.png'; }
                    else if (card == 'A') { imageSource = 'ww_header_10-2013_ave_credit_card.png'; }
                    else if (card == 'J') { imageSource = 'ww_header_10-2013_jl_credit_card.png'; }
                    else if (card == 'R') { imageSource = 'ww_header_10-2013_rm_credit_card.png'; }
                    else if (card == 'K' || card == 'N') { imageSource = 'ww_header_10-2013_ksd_credit_card.png'; }
                    else if (card == 'G' || card == 'H') { imageSource = 'ww_header_10-2013_bh_credit_card.png'; }

                    $('#header-credit-cards img').attr('src', '//secureimages.plussizetech.com/images/site_images/womanwithin/' + imageSource);
                }

                // EMAIL SIGNUP
                var emailInputDefault = 'First time registrants only',
                emailLabel = $('#email-label'),
                emailLabelText = 'Sign up for emails today and get a <strong>40% OFF coupon</strong>',
                emailAddress = $('#email-address'),
                emailSubmit = $('#email-submit'),
                emailConfirmation = $('#email-confirm'),
                emailRegex = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

                // Set default text and focus/blur/enter key behaviors for the email input.
                emailAddress.val(emailInputDefault)
                .on('focus', function () { if (emailAddress.val() == emailInputDefault) { emailAddress.val(''); } })
                .on('blur', function () { if (emailAddress.val() == '') { emailAddress.val(emailInputDefault); } })
                .on('keypress', function (e) { if (e.which == 13) { emailSubmit.click(); } });

                emailLabel.html(emailLabelText);

                // Submit email signup request and display server response.
                emailSubmit.on('click', function () {
                    if (emailAddress.val() && emailAddress.val() != emailInputDefault) {
                        if (emailRegex.test(emailAddress.val())) {
                            $.ajax({
                                type: 'GET', url: '/Checkout/ChameleonEmail.aspx?eml=' + emailAddress.val(), dataType: "text",
                                success: function (data) {
                                    if (data.split('^')[0] != 'succ') {
                                        emailLabel.addClass('error').html('Error: ' + data.split('^')[1]);
                                    } else {
                                        // Restore the sign up and load the promotion overlay.
                                        loadConfirmationPopup(emailAddress.val())
                                        emailLabel.removeClass('error').html(emailLabelText);
                                        emailAddress.val(emailInputDefault);
                                    }
                                },
                                error: function () { emailLabel.addClass('error').html('We\'re sorry. There\'s been an error signing you up.'); }
                            });
                        } else {
                            emailLabel.addClass('error').html('That email address does not appear to be valid.');
                        }
                    } else {
                        emailLabel.addClass('error').html('Please enter an email address.');
                    }
                });
            }
        }

        CreditCardAndEmail.setup()

        // Code from /scripts/email_With_Confirm.js for loading confirmation overlay. Crazy z-indexes are to battle ads.
        function loadConfirmationPopup(emailAddress) {
            $emailConfirmPopup = $('<div/>', { 'class': 'email-confirm-popup' });
            var $overlay = $('<div/>', { 'class': 'overlay' }),
            $body = $('body');
            $overlay.css({ position: 'fixed', width: '100%', height: '100%', backgroundColor: '#fff', opacity: '0.666', zIndex: '9999999999' });
            $body.prepend($emailConfirmPopup)
            .prepend($overlay)
            .find($emailConfirmPopup).css({ position: 'fixed', top: '5%', left: '20%', 'z-index': '9999999999', background: '#fff', border: '1px solid #ccc' })
            .load('/Catalog/email_confirm_popup.aspx', function () {
                $('.email-confirm-popup span.display-email').html(emailAddress);
                $('.email-confirm-popup a.close').on('click', function () {
                    $('.email-confirm-popup').remove();
                    $('.overlay').remove();
                });
            });
        }
    </script>
