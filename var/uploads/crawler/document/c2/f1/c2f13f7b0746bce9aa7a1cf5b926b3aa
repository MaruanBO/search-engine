<h2><span>
<span id="counter_1" class="counter"><span class="zahl">3</span><span class="zahl">0</span><span class="zahl">9</span><span class="zahl">,</span><span class="zahl">3</span><span class="zahl">9</span><span class="zahl">8</span></span><script type="text/javascript">
if (!window.counters) window.counters = {};
var refreshSeconds = 900;
var schedSeconds = 120;

function addCommas(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
        x1 = x1.replace(/(\d|,)/g, function(m) { return '<span class="zahl">' + m + '</span>' });
	return x1 + x2;
}


function createCounter(id, count, rate, updateURL) {
    var counter = document.getElementById(id);

    var rt = rate;
    var incr = rate > 0 ? 1 : -1;
    
    // high rates shouldn't gobble CPU, so in that case do fewer increments
    // but larger steps
    
    while ( Math.abs(rt) / incr > 15 ) {   
        incr += rt > 0 ? 2 : -1;
    }

    window.counters[id] = {};
    window.counters[id].rate = rt;
    window.counters[id].count = count;
    window.counters[id].incr = incr;
    

    var increment = window.counters[id].increment = function() {
        var count = window.counters[id].count += window.counters[id].incr;
        document.getElementById(id).innerHTML = addCommas(count);
    };
    
    var beginUpdate = function () {
        var updateScriptTag = document.createElement('script');
        updateScriptTag.src = updateURL + '&rand=' + Math.random();
        updateScriptTag.type = 'text/javascript';
        document.body.appendChild(updateScriptTag);
    };

    var scheduleIncrements = function() {

        var numIncrements 
            = Math.floor( Math.abs(window.counters[id].rate) 
              * schedSeconds 
              / Math.abs(window.counters[id].incr) );
        
        for (var i = 0; i < numIncrements; ++i) {
            setTimeout(increment, Math.random() * schedSeconds * 1000);
        }

        setTimeout( beginUpdate, ( refreshSeconds - 3 ) * 1000 );
        setTimeout( scheduleIncrements, schedSeconds * 1000 );

    };
    
    scheduleIncrements();
}

createCounter(
    'counter_1', 
    309398, 
    0.00111111111111111, 
    '/counter/countjs.html?petition_id=1482&id=counter_1&type=petition&offset=47006'
);
</script>
people have joined the effort asking Elizabeth Warren to run for president.</span> <span>Donate to the effort here:</span></h2>
