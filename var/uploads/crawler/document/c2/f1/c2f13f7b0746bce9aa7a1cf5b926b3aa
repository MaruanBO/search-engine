<script>
 	/*Geolocation script*/
var redirect = (function () {
    var redirectBrowser = function (state) {
        var uri = "http://runwarrenrun.org/" + state
        window.location = uri;
    };
    var states = {
        "ia": true,
        "nh": true
    };
    var defaultSite = "";
 
    var onSuccess = function (geoipResponse) {
	        if (!geoipResponse.subdivisions[0].iso_code) {
        	console.log("no code return default");
            //redirectBrowser("");
            return;
        }
 
        /* ISO country codes are in upper case. */
        var code = geoipResponse.subdivisions[0].iso_code.toLowerCase();
 		console.log(code);
        if ( states[code] ) {
        	console.log(states[code]);
            redirectBrowser(code);
        }
        else {
        	console.log("no code so redirect to default");
            //redirectBrowser("");
        }
    };
 
    var onError = function (error) {
    	console.log("error");
        //redirectBrowser("");
    };
 
    return function () {
        geoip2.city( onSuccess, onError );
    };
}());
 
redirect();
</script>
