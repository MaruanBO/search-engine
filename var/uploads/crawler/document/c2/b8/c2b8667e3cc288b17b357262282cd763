        $(document).ready(function () {
            var pageId = 12884907127
            // Get the content for carousel
            // Warning: Please do not remove this function
            $.ajax({
                type: "POST",
                url: '/Templates/WebServices/WebService.asmx/GetContentList',
                data: '{"pageId": "' + pageId + '"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.d.length > 0) {
                        $("#multiple_slides_visible").agile_carousel({
                            carousel_data: data.d,
                            carousel_outer_height: 230,
                            carousel_height: 200,
                            slide_height: 197,
                            carousel_outer_width: 480,
                            slide_width: 198,
                            number_slides_visible: 4,
                            transition_time: 330,
                            control_set_1: "previous_button,next_button",
                            control_set_2: "group_numbered_buttons",
                            persistent_content: "<div class='persistent_content'><h2 class='carousel-title'><a href='/news'>" + data.d[0].title + "</a></h2></div>"
                        });
                        $('.slideshow').after('<div id="slideshowMobile"></div>');
                        $('#slideshowMobile').append('<h2>Recent News</h2>');
                        if ($('.mainMenu').width() > 768) {
                            $('#slideshowMobile').hide();
                            $('.caption').each(function () {
                                $('#slideshowMobile').append('<div class="slideshowMobileLink" id="tmpSlideshowID">');
                                $(this).clone().appendTo('#tmpSlideshowID');
                                $('#tmpSlideshowID').attr('id', '');
                            });
                        }
                        $('.slideshowMobileLink a').each(function () {
                            //$(this).removeClass("caption");
                        });
                    }
                },
                error: function (data, jqXHR, exception) {

                }
            });
        });
    </script>
