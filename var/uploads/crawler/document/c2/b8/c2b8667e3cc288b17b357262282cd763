    <script type="text/javascript">
        $(function () {
            $('.topLoginAreaMyBowl').off('click.mainfunction').on('click.mainfunction', function () {
                var lb = $(this).find('.login-box');
                if (!lb.is(':visible')) {
                    $('body').off('click.mainfunction').on('click.mainfunction', function (evt) {
                        var ancestor = $(evt.target).parents('.topLoginAreaMyBowl');
                        if (ancestor.length == 0) {
                            $('.login-box').slideToggle("slow");
                            $('body').off('click.mainfunction');
                        }
                    });
                    lb.slideToggle("slow");
                    return false;
                }
            });

            $('#btn-login-submit').click(function (e) {
                e.preventDefault();
                $('.topLoginveil').show();
                var $this = $(this);
                if ($this.attr('disable') != true) {
                    var usbc_u = $('#login-username').val(),
                        usbc_p = $('#login-password').val();

                    $this.attr('disable', true).text($this.data('loading-text'));
                    $.ajax({
                        url: '/templates/microsites/findsubbowler/login.aspx?type=signin',
                        type: 'post',
                        data: { 'usbcu': usbc_u, 'usbcp': usbc_p }
                    }).done(function (data) {
                        console.log(data);
                        //alert(data);
                        //alert(data.status);

                        if (data.status === 'valid') {
                            //document.location.reload(true);
                            var new_path = location.href.replace(location.search, '');
                            new_path = '?ti=2|' + data.crmId;
                            location.href = new_path;
                        }
                       
                        else {
                            if (data.status === 'changepassword') {
                                document.location = '/templates/microsites/findsubbowler/changepassword.aspx';
                            } else {
                                $('.topLoginveil').hide();
                                $this.text('Sign In').attr('disable', false);
                                alert("Your username or password is incorrect. Please try again.");
                            }
                        }
                       

                        }).fail(function () {
                        $('.topLoginveil').hide();
                        $this.text('Sign In').attr('disable', false);
                        alert("An Exception occurs. Please try again.");
                    });
                }

            });
        });
    </script>

