									<script type="text/javascript">

										function topStoriesClick() {
											document.getElementById( 'todayHead' ).className = 'off';
											document.getElementById( 'yesterdayHead' ).className = 'off';
											document.getElementById( 'commentTab' ).className = 'off';

											document.getElementById( 'Today' ).className = 'hide';
											document.getElementById( 'Yesterday' ).className = 'hide';
											document.getElementById( 'MostCommented' ).className = 'hide';

											this.className = 'on';
											document.getElementById( this.getAttribute('title') ).className = 'show';

										}

										if ( document.getElementById( 'regTab' ) ) {
											document.getElementById( 'todayHead' ).onclick = topStoriesClick;
											document.getElementById( 'yesterdayHead' ).onclick = topStoriesClick;
											document.getElementById( 'commentTab' ).onclick = topStoriesClick;
										}
										
									</script>

