					( function( $ ) {
						var calendarPromo = {
							init:function( obj ) {
								var daysOfWeek = [ 'Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday' ];
								var months = [ 'Jan.', 'Feb.', 'Mar.', 'Apr.', 'May', 'Jun.', 'Jul.', 'Aug.', 'Sep.', 'Oct.', 'Nov.', 'Dec.' ];
								var venues = {};
								$( obj.venues ).each( function( index ) {
									var venue = this;
									venues[ venue.id ] = { 'name':venue.name, 'city':venue.city };
								} );
								$( obj.events ).each( function( index ) {
									var event = this;
									var dateArray = event.date.split( '-' );
									var timeArray = event.time.split( ':' );
									var eventDate = new Date( new Date( dateArray[0], dateArray[1] - 1, dateArray[2], timeArray[0], timeArray[1] ) );
									var eventTime = '' + ( eventDate.getHours() > 12 ? ( eventDate.getHours() - 12 ) : eventDate.getHours() ) + ( eventDate.getMinutes() > 0 ? ':' + eventDate.getMinutes() : '' ) + ( eventDate.getHours() > 12 ? ' PM' : ' AM' );
									var currentDate = new Date();
									currentDate.setHours( 0, 0, 0, 0 );
									$( '.calWidgetHouse > .container' ).append(
										$( '<a/>', { 'href':'http://www.dailyherald.com/calendar/#event/' + event.id + '', 'title':event.title + ' ' + event.subtitle, 'class':'calEventLink' } ).append( 
											$( '<div/>', { 'class':'calEventDayDateTime' } ).append( 
												$( '<span/>', { 'class':'calEventDay' } ).text( daysOfWeek[ eventDate.getDay() ] )
											).append(
												document.createTextNode( ', ' )
											).append(
												$( '<span/>', { 'class':'calEventDate' } ).text( months[ eventDate.getMonth() ] + ' ' + eventDate.getDate() )
											).append(
												document.createTextNode( ', ' )
											).append(
												$( '<span/>', { 'class':'calEventTime' } ).text( eventTime )
											)
										).append(
											$( '<img/>', { 'src':( event.image ? 'http://www.dailyherald.com/apps/pbcsi.dll/urlget?url=' + event.image.url.replace( new RegExp( '^//' ), 'http://' ) + '&exactH=178&exactW=178&exactfit=crop' : '/gfx/site/spingo-back-image.jpg' ), 'class':'calEventImage' } )
										).append(
											$( '<div/>', { 'class':'calEventDescription' } ).append( $( '<div/>', { 'class':'calEventLocation' } ).text( venues[ event.venue_id ].city ) ).append( $( '<div/>', { 'class':'calEventTitle' } ).text( event.title + ' ' + event.subtitle ) ).append( $( '<div/>', { 'class':'calEventVenue' } ).text( venues[ event.venue_id ].name ) )
										)
									);
								} );
							}
