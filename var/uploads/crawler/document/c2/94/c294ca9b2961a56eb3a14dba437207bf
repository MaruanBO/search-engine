function trackJavaScriptError(e) {
  var errMsg = e.message;
  var errSrc = e.filename + ': ' + e.lineno;
  ganalytics('send', 'event', 'JavaScript Error', errMsg, errSrc, { 'nonInteraction': 1 });
}
