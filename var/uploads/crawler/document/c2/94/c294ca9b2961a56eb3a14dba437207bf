<script type="text/javascript">

  
  function checkForEnter(e) {
    var isEnter = false;
    if (!e) {
      e = window.event;
    }
    if (e.keyCode == 13) {
      isEnter = true;
      //alert("enter pressed");
    }
    return isEnter;
  }
  
  
function submitSubscription() {
  var emailAddress = document.getElementById("subscription_email").value;

  if (emailAddress.length > 0) {
    $.ajax({
      url: "ajaxNewsletter.php",
      type: "POST",
      data: "email_address=" + emailAddress,
      success: function(data) {
        document.getElementById("subscription_email").value = "";
        document.getElementById("newsletter_subscribe").style.display = "none";
        document.getElementById("newsletter_subscribed").style.display = "table";
      }
    });
  }
}
function sendBannerData(){
  alert('yippee');
}
</script>
