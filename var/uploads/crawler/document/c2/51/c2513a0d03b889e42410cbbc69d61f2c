<script type="text/javascript">
function displaySubnav(id) {
    var el = document.getElementById('subnav_bg');
    el.style.display = "block";
    var subnav_menus = new Array("about_us_subnav","protecting_our_parks_subnav","exploring_our_parks_subnav","news_subnav","get_involved_subnav","ways_to_give_subnav");
    for (x in subnav_menus) {
        var el = document.getElementById(subnav_menus[x]);
        el.style.display = "none";
    }
    var nav_options = new Array("about_us_nav_option","protecting_our_parks_nav_option","exploring_our_parks_nav_option","news_nav_option","get_involved_nav_option","ways_to_give_nav_option");
    for (x in nav_options) {
        var el = document.getElementById(nav_options[x]);
        el.style.background = "none";
    }
    var el = document.getElementById(id + '_subnav');
    el.style.display = "block";
    var el = document.getElementById(id + '_nav_option');
    el.style.background = "#5e9732";
}
</script>

