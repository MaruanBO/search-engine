<script type="text/javascript">
function validate_required(field, alerttxt) {
    with (field) {
        if (value == null || value == "") {
            alert(alerttxt);
            return false;
        }
        else {
            return true;
        }
    }
}

function validate_email(field, alerttxt) {
    with (field) {
        apos = value.indexOf("@");
        dotpos = value.lastIndexOf(".");
        if (apos<1 || dotpos-apos < 2) {
            alert(alerttxt);
            return false;
        } else {
            return true;
        }
    }
}

function validate_form(thisform) {
    with (thisform) {
        if (validate_required(cons_email,"Email must be filled out.")==false) {
            cons_email.focus();return false;
        } else {
            if (validate_email(cons_email,"Not a valid e-mail address.")==false) {
                cons_email.focus();return false;
            }
        }
    }
}
</script>
<div id="login_form">
    <div id="login_top">
        <img src="http://www.npca.org/assets/wrapper-images/npca-lightbox-logo-top.png" alt="National Parks Conservation Association" id="lb_logo_top" />
