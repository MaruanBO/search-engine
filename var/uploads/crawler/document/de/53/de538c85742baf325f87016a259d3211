<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		var topHeadlines = $wn('#WNCol4 #DisplaySizeId25 li.feature').wnItemExtract(); 
		if (topHeadlines.length < 1) {
      return;
    }

    $wn.each(topHeadlines, function(i) {
      if(i >= 5) {
        return;
      }

      this.hasVideo = (this.isClip || this.isVideo);
      this.custDate = CSHELPERS.timeAgo(this.dateText, '100 years');
      this.custDate = this.custDate.replace('minutes ago','m');
      this.custDate = this.custDate.replace('hour ago','h');
      this.custDate = this.custDate.replace('hours ago','h');

      $wn('#csTopHeadlines li.feature ul').csFormatterAppend(
        '<li class="csHeadline">' +
        	'<div class="csText">' +
	          '<a href="{$headlineHref}">{$headlineText}</a>' +
	          '{if $hasVideo}' +
	            '<img src="http://ftpcontent.worldnow.com/wncustom/custom/ktul/2013/images/videp.png">' +
	          '{/if}' +
	        '</div>' +
          '<span class="csTime">{$custDate}</span>' +
          '<div class="wnClear"></div>' +
        '</li>', this
      );
    });

    $wn('#csTopHeadlines li.feature .csHeadline').last().addClass('last');

    $wn('#DisplaySizeId25').hide();

	}, 'WNCol4done');
</script>
