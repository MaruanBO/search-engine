<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		$wn('#csJenForecast').parent().css('margin-bottom', 0);
		csJenForecast();
		//$wn('#csJenForecast').show();
	}, 'WNCol23done');

	var iconPath = 'http://ftpcontent.worldnow.com/wncustom/custom/ktul/2013/images/ktul_wx_icons/40_white/wx_';

	function csJenForecast() {
		WNHttpRequestManager.makeRequest( 'http%3A%2F%2Fservices.intellicast.com%2F200904-01%2F215635328%2FWeather%2FReport%2FUSOK0537', { 
			onSuccess: function() {
				var $data = $wn(this.response.responseXML).find('HourlyForecast');
				var $hour = $data.find('Hour');

        $hour.each(function(i) {
        	if (i >= 5) {
        		return false;
        	}
        	var feedData = {
        		hour: $wn(this).attr('ValidDateLocal'),
        		temp: $wn(this).attr('TempF'),
        		icon: $wn(this).attr('IconCode')
        	};  

        	var date = new Date(feedData.hour);
        	var hour = date.getHours() % 12;
        	if (hour == 0) {
        		hour = 12;
        	}
        	var ampm = date.getHours() >= 12 ? 'pm' : 'am';
			
        	var template = '<li><span class="wxTime">' + hour + ' ' + ampm + '</span><span class="wxTemp">{$temp}&deg;</span><img class="wxIcon" src="' + iconPath + '{$icon}.png"></li>';
    			$wn('#csJenForecast').find('ul').csFormatterAppend(template,feedData);
        });
      }        
		});
	}
</script>
