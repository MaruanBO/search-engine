<script type="text/javascript" src="http://ftpcontent.worldnow.com/wncustom/js/jquery.jcarousel.min.js"></script>
<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		var topVideos = $wn('#WNCol4 #DisplaySizeId24 li.feature').wnItemExtract(); 
    $wn.each(topVideos, function(i) {
      $wn('#csTopVideos li.feature #mycarousel').csFormatterAppend('' +
        '<li class="csVid">' +
        	'<a href="{$headlineHref}"><img class="csOverlay" src="http://ftpcontent.worldnow.com/wncustom/custom/ktul/2013/images/video_overlay.png"></a>' +
          '<a href="{$headlineHref}"><img class="csGrpahic" src="{$graphicSrc}"></a>' +
          '<a href="{$headlineHref}">{$headlineText}</a>' +
        '</li>', this
      );
    });

    var total      = topVideos.length,
        current    = 1,
        currentMax = 4;

    $wn('#csTopVideos').find('.header').append('<div class="counter">' + 
                                                  '<span id="csCurrent">' + current + '</span> - ' + 
                                                  '<span id="csCurrentMax">' + currentMax + '</span> of ' +
                                                  '<span id="csCount">' + total + '</span>' + 
                                                '</div>');

    $wn('#csTopVideo li.feature .csVid').last().addClass('last');

    $wn('#DisplaySizeId24').hide();

 		$wn('#mycarousel').jcarousel({
 			scroll: 1
 		});

 		// Move buttons to top
 		var $prev  = $wn('#csTopVideos .jcarousel-prev'),
 				$next  = $wn('#csTopVideos .jcarousel-next'),
 				$cur   = $wn('#csTopVideos #csCurrent'),
 				$max   = $wn('#csTopVideos #csCurrentMax'),
 				$count = $wn('#csTopVideos #csCount');

 		$prev.appendTo('#csTopVideos .header');
		$next.appendTo('#csTopVideos .header');

		$next.click(function() {
			if (current == (total - 3)) {
				return false;
			}
			current++;
			currentMax++;
			$cur.text(current);
			$max.text(currentMax);
		});
		$prev.click(function() {
			if (current == 1) {
				return false;
			}
			current--;
			currentMax--;
			$cur.text(current);
			$max.text(currentMax);
		});

	}, 'WNCol4done');
</script>
