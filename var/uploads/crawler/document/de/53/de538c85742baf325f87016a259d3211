<script type="text/javascript">
$wn(document).bind('csWxCol4Ready', function() {

	var wxStations = [
		{station: '74101', name: 'Tulsa'},
		{station: '74401', name: 'Muskogee'},  
		{station: '74003', name: 'Bartlesville'},
		{station: '74464', name: 'Tahlequah'},
		{station: '74502', name: 'McAlester'}
	];
	
    CSWxManager.setOption('wsi');
	CSWxManager.setOption('iconSize', 45);
	CSWxManager.loadConfig(wxStations);
	
	CSWxManager.ready(function(wx) {

		if (!wx.hasData()) { return; }

		function rotate() {

			// Specific to the MyWeather XML format
			var hi = wx.fetch('high');
			var lo = wx.fetch('low');
			var wd = wx.fetch('winddirection');
			var ws = wx.fetch('windspeed');
			
			// Values to fetch
			$wn('#csWxCol4 #csWxTemperature').html(wx.fetch('temperature') + '&deg;');
			$wn('#csWxCol4 #csWxIcon').css('background-image', wx.fetch('iconcss'));	
			$wn('#csWxCol4 #csWxHi').html(hi + '&deg;');
			$wn('#csWxCol4 #csWxLo').html(lo + '&deg;');
			$wn('#csWxCol4 #csWxWind').html(wd + ' @ ' + ws + ' mph');
			$wn('#csWxCol4 #csWxPressure').html(wx.fetch('pressure'));
			$wn('#csWxCol4 #csWxHumidity').html(wx.fetch('humidity'));
			$wn('#csWxCol4 #csWxSky').html(wx.fetch('sky'));
			$wn('#csWxCol4 #wxCity').text(wx.fetch('city'));
			wx.next();
		}
		rotate();
    	setInterval(rotate, 5000);
	});
});
</script>
<div style="position:relative;">
<div id='csWxCol4' class='wnBlock displaySize'>
	<ul class='wnGroup contentGroup'>
		<li class="wnItem header">
			<h3><span class="text siteDefault">Current Conditions</span><div class="wnClear"></div></h3>
			<span id="wxCity"></span>
		</li>	
		<li class='wnItem feature'>
			<div id='csWxTemperature'></div>
			<div id='csWxIcon'></div>
			<ul class="first">
				<li>Hi <span id='csWxHi'></span></li>
				<li>Lo <span id='csWxLo'></span></li>
			</ul>
			<ul class="last">
				<li id="csWxWind-container">Wind: <span id="csWxWind"></span></li>
				<li id="csWxPressure-container">Barometer: <span id="csWxPressure"></span></li>
				<li id="csWxHumidity-container">Humidity: <span id="csWxHumidity"></span></li>			
			</ul>
			<div id='csWxSky'></div>
		</li>
	</ul>
</div>
