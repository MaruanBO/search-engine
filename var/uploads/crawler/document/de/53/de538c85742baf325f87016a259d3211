<script type="text/javascript">

PLATFORM.EventMan.registerToEvent(function() {

  var topTemplate = '<div class="csFeatureStory">' + 
                      '{if $graphicSrc}' +
                        '<a href="{$headlineHref}"><img class="csFeatureImage" src="{$graphicSrc}"></a>' +
                      '{/if}' +
                      '</div>' +
                      '<div class="csTopRight">' +
                        '<h4>' +
                          '<a href="{$headlineHref}">{$headlineText}</a>' +
                          '{if $hasVideo}' +
                            '<a href="{$headlineHref}"><img src="http://ftpcontent.worldnow.com/wncustom/custom/ktul/2013/images/videp.png"></a>' +
                          '{/if}' +
                        '</h4>' +
                        '<p>{$summaryHtml}</p>' +
                        '<span data-storyid="{$storyId}" class="csMobdubTarget">...</span>' +
                      '</div>';

  var botTemplate = '<div class="csStory">' +
                      '{if $graphicSrc}' +
                        '<a href="{$headlineHref}"><img class="csImage" src="{$graphicSrc}"></a>' +
                      '{/if}' +
                      '<h4>' +
                        '<a href="{$headlineHref}">{$headlineText}</a>' +
                        '{if $hasVideo}' +
                          '<a href="{$headlineHref}"><img src="http://ftpcontent.worldnow.com/wncustom/custom/ktul/2013/images/videp.png"></a>' +
                        '{/if}' +
                      '</h4>' + 

                    '</div>';             

  // Load story data
  var storyTop    = $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').wnDVSummaryExtract();
  var featureStory = storyTop[0];
  var storyBottom = $wn('#WNCol2 .wnDS-20 .wnDVSummary').wnDVSummaryExtract();

  featureStory.hasVideo = (this.isClip || this.isVideo);

  // Build top
  featureStory.storyId = (function(href) {
    var matches = href.match(/^.*\/story\/(\d+)\/.*$/i) || [];
    return matches[1] || '';
  })(featureStory.headlineHref); 
  if (!featureStory.graphicSrc) featureStory.graphicSrc = 'http://ktul.images.worldnow.com/images/452232_G.jpg';
  $wn('#csTsr #csTop').csFormatterAppend(topTemplate, featureStory);

  // Build bottom
  $wn.each(storyBottom, function(i) {
    if (i >= 4) {
      return;
    }
    if (!this.graphicSrc) this.graphicSrc = 'http://KTUL.images.worldnow.com/images/2321473_G.jpg';
    this.hasVideo = (this.isClip || this.isVideo);
    var $bottom = $wn.$csFormatter(botTemplate, this);
    $wn('#csTsr #csBottom').append($bottom);
  });
  
  $wn('#csBottom').find('.csStory').first().addClass('first');
  $wn('#csBottom').find('.csStory').last().addClass('last');

  $wn('#csTsr #csTop .csTopRight > p').dotdotdot();
  $wn('#csTsr #csBottom .csStory h4 a').dotdotdot();

  // Hide any overflow
  $wn('#WNTopStoryWrap .wnDS-20 .wnDVSummary').hide();
  $wn('#WNCol2 .wnDS-20').hide();
  $wn('#WNTopStoryWrap').hide();

}, 'WNCol23done');
</script>
