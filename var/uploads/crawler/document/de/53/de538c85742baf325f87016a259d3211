<script type="text/javascript">
	PLATFORM.EventMan.registerToEvent(function() {
		var features = $wn('#WNCol4 #DisplaySizeId68 li.feature').wnItemExtract(); // NEST CATEGORY COL 4
    $wn.each(features, function(i) {
      $wn('#csSiteFeatures li.feature #mycarousel2').csFormatterAppend('' +
        '<li class="csFeature">' +
          '<a href="{$headlineHref}"><img class="csGrpahic" src="{$graphicSrc}"></a>' +
          '<a href="{$headlineHref}">{$headlineText}</a>' +
          '<p>{$summaryHtml}</p>' +
        '</li>', this
      );
    });

    var total      = features.length,
        current    = 1,
        currentMax = 4;

    $wn('#csSiteFeatures').find('.header').append('<div class="counter"><span id="csCurrent">' + current + '</span> - <span id="csCurrentMax">' + currentMax + '</span> of <span id="csCount">' + total + '</span></div>');

    $wn('#csSiteFeatures li.feature .csFeature').last().addClass('last');

    $wn('#DisplaySizeId68').hide();

 		$wn('#mycarousel2').jcarousel({
 			scroll: 1
 		});

    $wn('#csSiteFeatures .feature .csFeature p').dotdotdot();

 		// Move buttons to top
    var $prev  = $wn('#csSiteFeatures .jcarousel-prev'),
        $next  = $wn('#csSiteFeatures .jcarousel-next'),
        $cur   = $wn('#csSiteFeatures #csCurrent'),
        $max   = $wn('#csSiteFeatures #csCurrentMax'),
        $count = $wn('#csSiteFeatures #csCount');

    $prev.appendTo('#csSiteFeatures .header');
    $next.appendTo('#csSiteFeatures .header');

    $next.click(function() {
      if (current == (total - 3)) {
        return false;
      }
      current++;
      currentMax++;
      $cur.text(current);
      $max.text(currentMax);
    });
    $prev.click(function() {
      if (current == 1) {
        return false;
      }
      current--;
      currentMax--;
      $cur.text(current);
      $max.text(currentMax);
    });

	}, 'WNCol4done');
</script>
