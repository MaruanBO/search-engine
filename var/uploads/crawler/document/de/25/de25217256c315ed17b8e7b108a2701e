interstitialData['URL'][0]['top'] ='0px';
interstitialData['URL'][0]['left'] ='0px';
interstitialData['URL'][0]['width'] ='700px';
interstitialData['URL'][0]['height'] ='500px';


if(getCookie(interstitialData['uniqueID'])){ 
	return; //If Unique ID is loaded to cookie die
}

//Add elements to page
	$('body').append('<div id="interstatial-ad-container" style="width:700px; height:500px; background-color: #ffffff; box-shadow: 0px 5px 30px rgba(0,0,0,.5); position:absolute; left: 50%; margin-left: -350px; top:10%; background-image:url('+interstitialData['backgroundFile']+'); z-index: 5001"><a id="tracking-pixel"><img src="'+interstitialData['TrackingPixel'] +'"/></a><a id="close-button-interstatial"><img src="'+interstitialData['closeButtonImage'] +'"/></a><a id="skip-ad-link" style="color:#FFCC00; padding:5px 10px; bottom:5px; right: 5px; position:absolute;  line-height:30px; text-decoration:none; display:block; cursor:pointer; z-index:5003; ">[ skip this ad ]</a></div>')
		
