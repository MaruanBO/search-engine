<script>

jQuery(document).bind('theiaPostSlider.changeSlide', function(event, slideIndex) {
        hotness();
});

function hotness()
{
    jQuery('#q1-unit').remove();
    jQuery.getScript('http://Q1MediaHydraPlatform.com/ads/video/unit_desktop_impact_right.php?eid=68159');
    jQuery.getScript('');

    jQuery('iframe').each(function(e)
    {
        var $this = jQuery(this);
        var src   = $this.attr('src');


        if(src)
        {
            if(src.match(/adserver.thebuzztrend.com/))
            {
                    var cb          = src.match(/cb\=(\d*)/);
                    var bust        = parseInt(cb[1]);
                    var newsrc      = src.replace(bust, bust+1);
                    $this.attr('src', newsrc);
            }
        }
    });
}

setInterval(function()
{
	document.getElementById('bnmla_v_unit').src = document.getElementById('bnmla_v_unit').src
}, 15000);

</script>


