		<script>
		var CountryName;	
		var RegionName;
		var BacktoRegion;
		var LinkName ;
$(document).ready(function(){
	
	$(".primary").find('a').addClass('footerlink');
	$(".type").find('a').addClass('bk2rgn');
			$(".type").bind("click",function(){
  			RegionName = $(this).find('h2').text();

	});
	
	$(".prompt").click(function(){
  			CountryName = $(this).html();
  			s.linkTrackVars='prop3,prop4,eVar3,eVar4,eVar18'; 
				s.prop3=s.eVar3='Visitor Action'; 
				s.prop4=s.eVar4=RegionName +"_" +CountryName; 
				s.tl(true,'o','RegionName_CountryName');
  			

	});
	$(".bk2rgn").click(function(){
  			BacktoRegion = $(this).html();
  			s.linkTrackVars='prop3,prop4,eVar3,eVar4'; 
				s.prop3=s.eVar3='Visitor Action'; 
				s.prop4=s.eVar4='BacktoRegion'; 
				s.tl(true,'o','BacktoRegion');
  			
  			});
  			
  		$(".footerlink").click(function(){
  			LinkName = $(this).html();
  			s.linkTrackVars='prop3,prop4,eVar3,eVar4'; 
				s.prop3=s.eVar3='Visitor Action'; 
				s.prop4=s.eVar4='LinkName'; 
				s.tl(true,'o','LinkName');
  			
  			});	
 
	});
</script>


