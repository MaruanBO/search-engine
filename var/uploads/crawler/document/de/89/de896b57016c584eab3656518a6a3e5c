  var linker = null;  
  var attemptsWaitingGA = 0;
  
  function waitGAobject (callback) {
    if (typeof ga === 'undefined' && attemptsWaitingGA < 10) {
      setTimeout (function () {
         waitGAobject (callback);
      }, 100);
      attemptsWaitingGA++; 
    } else { callback (); }
  }
       
  function gaLinker() {
     if (typeof ga !== 'undefined') {
       ga(function() {
                      var trackers = ga.getAll();
                      if(trackers.length) {
                                     linker = new window.gaplugins.Linker(trackers[0]);
                      }
       });     
     }
  }  
  
  function linkBuilder(linker, targetURL) {
    if (linker) {
      return linker.decorate(targetURL, true);
    } else {
      return targetURL
    }
  }
         
