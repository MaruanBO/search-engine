<script type="text/javascript">

    (function (w, $) {

        $(function () {

            $("a.switch-to-mobile").click(function () {
                //console.log("clicked.");
                
                var date = new Date();
                var minutes = 120;
                date.setTime(date.getTime() + (minutes * 60 * 1000));

                $.removeCookie("no-mobile", { path: "/"});
                $.cookie("X-CVX-Device-Type", "smartphone", { path: "/", expires: date });
                var reload = true;

               
                if (w.location.href.indexOf('/stories') !== -1) {
                    w.location.href = '/mobile/pages/stories/';
                    reload = false;
                }

                if (w.location.href.indexOf('/about/advertising') !== -1) {
                    w.location.href = '/mobile/pages/about/advertising/';
                    reload = false;
                }


                if (reload) {
                    w.location.reload(true);
                }


            });

        });

    })(this, this.jQuery);


</script>



            
