function InsertAdsLazy(){
	if(!HasAds && $(window).width() > 940){
		HasAds = true;
		
		
		var s = document.createElement("script");
	    s.type = "text/javascript";
	    s.src = "http://ads.blogherads.com/10K/10/100/10054/160a.js";
	    // Use any selector
	    $(".FirstBlogHerAd").append(s);
		
