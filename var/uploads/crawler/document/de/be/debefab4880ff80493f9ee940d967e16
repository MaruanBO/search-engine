	<script type='text/javascript'>
var fbLoginType;
var fbRedirectURL;

// Additional JS functions here
window.fbAsyncInit = function() {
	FB.init({
	  appId      : '111408825697563', // App ID
	  channelUrl : 'http://www.theyummylife.com/channel.php', // Channel File
	  status     : true, // check login status
	  cookie     : true, // enable cookies to allow the server to access the session
	  xfbml      : true  // parse XFBML
	});
	
	// Additional init code here
	/*
	$('.FacebookLoginButton').bind('click', function(){
		fbLoginType = $(this).attr('LoginType');
		fbRedirectURL = $(this).attr('RedirectURL');
		
		FB.getLoginStatus(function(response) {
			if (response.status === 'connected') {
				// connected
				SubmitFBLoginInfo();
			} else if (response.status === 'not_authorized') {
				// not_authorized
				fbLogin();
			} else {
				// not_logged_in
				fbLogin();
			}
		});
	});
	*/
};

function RedirectFacebookDialog(RedirectURL){
	fbLoginType = 'Redirect';
	fbRedirectURL = RedirectURL;
	
	fbLogin();
}

// Load the SDK Asynchronously
(function(d){
	var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
	if (d.getElementById(id)) {return;}
	js = d.createElement('script'); js.id = id; js.async = true;
	js.src = "//connect.facebook.net/en_US/all.js";
	ref.parentNode.insertBefore(js, ref);
}(document));

function fbLogin() {
    FB.login(function(response) {
        if (response.authResponse) {
            // connected
			SubmitFBLoginInfo();
        } else {
            // cancelled
			Dialog("<div style='font-size:16px; font-weight:bold; color:#666;'>Uh oh, something went wrong when you tried logging in with Facebook. Please try again.</div><br /><br /><div align='center'><a href='javascript://' onclick='CloseDialogs();' class='GreenButton'>Ok</div>");
        }
    }, {scope:'email'});
}

function SubmitFBLoginInfo() {
	$('.FacebookLoginButton').html("<img src='/i/loading.gif' />");
	
    FB.api('/me', function(response) {
		$.post('/ajax/account/login.php', response, function(Data){
			if(Data == 'Error'){
				Dialog("<div style='font-size:16px; font-weight:bold; color:#666;'>Uh oh, something went wrong when you tried logging in with Facebook. Please try again.</div><br /><br /><div align='center'><a href='javascript://' onclick='CloseDialogs();' class='GreenButton'>Ok</div>");
				('.FacebookLoginButton').html("Log in with Facebook");
			}
			else{
				Data = jQuery.parseJSON(Data);
				if(fbLoginType == 'Redirect'){
					if(fbRedirectURL == 'current'){
						document.location.reload(true);
					}
					else{
						window.location = 	fbRedirectURL;
					}
					return;
				}
				else if(fbLoginType == 'Comment'){
					PopulateUserLoginData(Data)
					ShowLoggedInCommentForms();
				}
				
				//replace the sidebar with the logged in version
				$('#NotLoggedInSidebarWelcome').hide();
				$('#SidebarPlaceholderAvatar').html(GetUserAvatarImage(Data['UserId'], Data['HasImage']));
				$('#SidebarPlaceholderFirstName').html(Data['FirstName']);
				$('#NotLoggedInSidebarPlaceholder').show();
				
				
			}
		});
    });
}

function PopulateUserLoginData(Data){
	$('#Account_UserId').html(Data['UserId']);
	$('#Account_DisplayName').html(Data['DisplayName']);
	$('#Account_HasImage').html(Data['HasImage']);
}

</script>
	
		

