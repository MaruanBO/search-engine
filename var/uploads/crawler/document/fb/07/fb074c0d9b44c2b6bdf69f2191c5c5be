									<script type="text/javascript">
										function mobileexclude247search(searchterm){
											prot = window.location.protocol;
											hst = window.location.host;
											// window.location = 'http://reason.com/search?f[pagetype][]=all-magazine-articles&f[pagetype][]=all-reasontv-videos&f[pagetype][]=brickbats&f[pagetype][]=customer-support-and-general-pages&f[pagetype][]=hit-and-run-posts&f[pagetype][]=reason-rupe-poll-posts&f[pagetype][]=web-articles&q=' + searchterm.replace(" ", "+") ;
											window.location = '//' + hst + '/search?f[pagetype][]=all-magazine-articles&f[pagetype][]=all-reasontv-videos&f[pagetype][]=brickbats&f[pagetype][]=customer-support-and-general-pages&f[pagetype][]=hit-and-run-posts&f[pagetype][]=reason-rupe-poll-posts&f[pagetype][]=web-articles&q=' + searchterm.replace(" ", "+") ;
											return false;
										}
									</script>
