<script>
$(document).ready(function() {
      var objectTags = $('object');
      objectTags.each( function(){
        var vidwidth = $(this).attr('width');
        var vidheight = $(this).attr('height');
        var vidid = $(this).children('param[name="src"]').attr('value');
        if (!(vidid && vidid.length)) {
          vidid = $(this).children('embed').attr('src');
        }
        if (vidid && vidid.length) {
          //we need to make sure this is a youtube link - don't transform anything else
          var isYoutube = vidid.match('youtube');
          console.log(isYoutube);
          if (isYoutube) {
            idStr = vidid.split('/')[4].split('?')[0];
            linkurl = 'http://www.youtube.com/watch?v='+idStr;
            imgurl = 'http://img.youtube.com/vi/'+idStr+'/0.jpg';
            embedurl = '//www.youtube.com/embed/'+idStr;
            var iframe = document.createElement('iframe');
            iframe.setAttribute('src', embedurl);
            if (vidwidth && vidwidth.length) {
              iframe.setAttribute('width', vidwidth);
            }
            if (vidheight && vidheight.length) {
              iframe.setAttribute('height', vidheight);
            }
            iframe.setAttribute('frameborder', '0');
            iframe.setAttribute('allowfullscreen', '1');
            $(this).replaceWith(iframe);
          }
        }
      }); //end objectTags.each
});
</script>	<div class="navtools"></div>
