<script>
$(function(){
	$("#topstoriessidebar .selector a").click(function(e){
		var $this=$(this), catref = $this.attr("data-category"),
			$sidebar = $this.parent().parent().parent();
		e.preventDefault();
		$sidebar.find('> .selector > li').removeClass('active');
		$this.parent().addClass('active');
		$sidebar.find('> .inner > .stories').hide().filter('[data-category='+catref+']').show();
	});
});
</script>
	</div>
</aside>

<!-- Overrides the default active tab -->
<script type="text/javascript" charset="utf-8">
$(function() {
			$("#topstoriessidebar ul.selector li:first-child, #topstoriessidebar ul.selector li:nth-child(2)").toggleClass('active');
		$("#topstoriessidebar > .inner > .stories").hide().filter('[data-category=viewed]').show();
	});
</script>
