<script type="text/javascript">

    var RoviHeader = {

        initialize: function(){
            $("#searchForm").on("click", "a[href]", this.submitSearchForm);
        },

        submitSearchForm: function(e){

            if (e){
                e.preventDefault();
            }
            
            var q = $('#searchForm #query').val();
            if (q && q.length >= 3){
               $('#searchForm').submit();
            }
        }
    };

    $(document).ready(function(){
      RoviHeader.initialize();
    });

</script>



