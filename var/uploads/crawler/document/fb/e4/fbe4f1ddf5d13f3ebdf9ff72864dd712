      eventCategory: 'Viewport',
      eventAction: 'Width',
      eventLabel:myWidth,
      eventValue: myWidth
    });
    ga("send", "event", {
      eventCategory: 'Viewport',
      eventAction: 'Height',
      eventLabel:myHeight,
      eventValue:myHeight
    });
