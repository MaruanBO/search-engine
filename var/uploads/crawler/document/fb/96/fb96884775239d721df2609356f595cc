<script type="text/javascript">
    var isHighlighted;
    var keyUp = 38;
    var keyDown = 40;

    jQuery(function () {
        jQuery("#btnSearchGo").click(function () {
            var searchTerm = jQuery("#txtSearchTerm").val();
            SearchBoxDoSearch(searchTerm, false);
        });

        jQuery("#txtSearchTerm").keyup(function (event) {
            if (event.keyCode == 13 && !isHighlighted) {
                jQuery("#btnSearchGo").click();
            }

            isHighlighted = false;

            if (event.keyCode == keyUp || event.keyCode == keyDown) {
                if (jQuery(".autocomplete_highlightedListItem").length > 0) {
                    isHighlighted = true;
                }
            }
        });

        jQuery("#ddlScopes").keydown(function (event) {
            if (event.keyCode == 13) {
                event.preventDefault();
                jQuery("#btnSearchGo").click();
            }
        });
    });

	function SearchBoxOnAutoCompleteSelected(source, eventArgs) {
	    var searchTerm = eventArgs.get_value();
	    SearchBoxDoSearch(searchTerm, true);
	}

	function SearchBoxDoSearch(searchTerm, exactMatch) {
		    var searchScope = jQuery("#ddlScopes").val();

			if (searchTerm == "Search For:") return;

	    var searchUrl = jQuery(".SearchButton").data("url") || "/search/results";

			var queryString = "?";
			var hasSearchTerm;
			if (searchTerm != null && searchTerm.trim() != "") {
				hasSearchTerm = true;
	        queryString += "q=" + encodeURIComponent(searchTerm) + "&exactMatch=" + exactMatch;
			}
        if (searchScope != null) {
				queryString += "&scope=" + searchScope;
			}

			if (hasSearchTerm)
				window.location.href = searchUrl + queryString;
			}
</script>

