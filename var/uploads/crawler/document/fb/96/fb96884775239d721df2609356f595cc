    /*Going off the assumption that 495px or less in portrait mode and 800px or less in landscape mode is a phone, based on this list: http://i-skool.co.uk/mobile-development/web-design-for-mobiles-and-tablets-viewport-sizes  */

    function hasTouch() {
        return Modernizr.touch;
    }

    function isSmall() {
        if (Modernizr.mq('screen and (orientation:portrait)') == true) {
            return Modernizr.mq('only screen and (max-device-width:495px)');
        } else {
            return Modernizr.mq('only screen and (max-device-width:767px)');
        }
    }

    function detectDevice() {
        var device = "desktop";
        if (runDetect.toLowerCase() == "true") {
            if (hasTouch()) {
                if (isSmall()) {
                    device = 'phone';
                }
                else {
                    device = 'tablet';
                }
            }
        }
        return device;
    }

    if (document.cookie.indexOf("DetectMobile") < 0) {
        if ("false" == "true") {
            document.cookie = "DetectMobile=false; path=/";
        }
    } 
    else {
        runDetect = "false";
    }

    deviceType = detectDevice();
    if (deviceType == "phone" && runDetect.toLowerCase() == "true") {
        if (isHomePage.toLowerCase() == "true") {
            location.href = "http://m.clevelandclinic.org";
        }
    }

</script>


