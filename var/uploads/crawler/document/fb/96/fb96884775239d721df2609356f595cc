			<input name="layoutcontent_0$contentmain_1$txtAutoFill" value="Enter any search term here..." id="txtAutoFill" accesskey="k" name="txtAutoFill" type="text" class="" onFocus="javascript: this.value = &#39;&#39;;" onBlur="javascript: if (this.value == &#39;&#39;) {this.value = &#39;Enter any search term here...&#39;};" />

		</div>
