		$(function() {
			var zipField = $("#sStoreLocFtrPostalCode");
			var defaultVal = zipField.val();
			zipField.focus( function() { 
				if (zipField.val() == defaultVal) { zipField.val(""); }
			});
			zipField.blur ( function() {
				if (zipField.val() == "") { zipField.val(defaultVal); }
			});
			zipField.focus( function() { $(this).select(); });
		});
