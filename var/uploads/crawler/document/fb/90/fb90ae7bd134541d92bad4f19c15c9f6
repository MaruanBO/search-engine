<!-- home_social.tpl.php -->
<div class="wrapper kontext-home_social kontext">
<div id="block-local-responsive-home-social" class="block block-local-responsive">
        
  <div class="content">
    
<script type="text/javascript">

    ThrowBlock ('local_blocks:home_social_front', ['wide', 'tablet']);
    ThrowBlock ('local_blocks:home_soc_fr_mob', ['mobile']);
    //set some hooks needed for social_feeds_block

    preThrow = function () {
       if ((typeof Drupal.settings.local_responsive.storageFacebook === 'undefined')  && (jQuery("#facebook-feed").html() !== null)) {
        //The html of facebook has not been saved yet, so save it for use on future throws.
        Drupal.settings.local_responsive.storageFacebook = jQuery("#facebook-feed").html();
       }
    };

    postThrow = function () {
        //Re-initialize twitter
        twttr.widgets.load();
        //Put back the saved facebook feed if I have it
        if (Drupal.settings.local_responsive.storageFacebook){
         jQuery("#facebook-feed").html(Drupal.settings.local_responsive.storageFacebook);
        }
    };
    hookJS.hook.screenStateChangePre =  (typeof hookJS.hook.screenStateChangePre === 'object' ) ? hookJS.hook.screenStateChangePre : {};
    hookJS.hook.screenStateChange =  (typeof hookJS.hook.screenStateChange === 'object' ) ? hookJS.hook.screenStateChange : {};


    hookJS.hook.screenStateChangePre.socialTabs = preThrow;
    hookJS.hook.screenStateChange.socialTabs = postThrow;
</script>
  </div>
</div>

</div>
<!-- /home_social.tpl.php -->

