<script type="text/javascript">

function changeFontSize(fontReq) {
	fontSizeVal = fontReq;
	toggleFont();
}

/**
 * flush_cookie default is true
 * set flush_cookie=false will NOT flush cookie
 */
function toggleFont(flush_cookie) {
	switch(fontSizeVal)
	{
	case 0:
		$('#body_container').css('font-size', '80%');
		$('#avx0').css({'color':'#0d5cab','text-decoration':'none','font-weight':'bold'});
		$('#avx1').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		$('#avx2').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		break;
	case 1:
		$('#body_container').css('font-size', '90%');
		$('#avx0').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		$('#avx1').css({'color':'#0d5cab','text-decoration':'none','font-weight':'bold'});
		$('#avx2').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		break;
	case 2:
		$('#body_container').css('font-size', '100%');
		$('#avx0').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		$('#avx1').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		$('#avx2').css({'color':'#0d5cab','text-decoration':'none','font-weight':'bold'});
		break;
	default:
		$('#body_container').css('font-size', '80%');
		$('#avx0').css({'color':'#0d5cab','text-decoration':'none','font-weight':'bold'});
		$('#avx1').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
		$('#avx2').css({'color':'#d60d8c','text-decoration':'underline','font-weight':'normal'});
	}
	if(flush_cookie == undefined || flush_cookie != false) {
		$.cookie('fontSizeSetting', fontSizeVal, { path: '/', expires: 1000 });
	}
}

function recipeWin(val){
	
	xloc = "\/recipe-details\/" + val;
	location.href = xloc;	
}
</script>

