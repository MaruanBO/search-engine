<!-- search javascript -->
<script language="JavaScript">
var userprofile = null;
var cnum = "";
$(document).ready(function() {
	if (userprofile != null) {
		$('#header-utitliy-username-part-1').text(userprofile.firstName);
		cnum = userprofile.username;
	}
	
	$('#queryText').keyup(function(e) {
		//alert(e.keyCode);
		
		if(e.keyCode == 13) {
			var queryTextVal = $('#queryText').val();
			if (queryTextVal != "") {
				sendSearchMSAS();
			}
			else {
				$('#queryText').css('border-color', '#fff000;');
				alert("Please Enter the Search Term");
				$('#queryText').focus();
				return false;
			}
		}
	});
	
	$('#header-utitliy-username-part-1, #header-utitliy-username-part-2').hover(
		function() {
			$(this).css('cursor', 'pointer');
		},
		function() {
			$(this).css('cursor', 'default');
		}
	);
	
	$('#header-utitliy-username-part-1, #header-utitliy-username-part-2').click(function() {
		var username_part_1 = $('#header-utitliy-username-part-1');
		var dropdown_div = $('#header-utility-dropdown');
		if(dropdown_div.is(":visible")) {
			$('#header-utitliy-username-part-1').addClass('header-utitliy-username-part-1-normal');
			$('#header-utitliy-username-part-1').removeClass('header-utitliy-username-part-1-clicked');
			$('#header-utitliy-username-part-2').addClass('header-utitliy-username-part-2-normal');
			$('#header-utitliy-username-part-2').removeClass('header-utitliy-username-part-2-clicked');
		} else {
			$('#header-utitliy-username-part-1').addClass('header-utitliy-username-part-1-clicked');
			$('#header-utitliy-username-part-1').removeClass('header-utitliy-username-part-1-normal');
			$('#header-utitliy-username-part-2').addClass('header-utitliy-username-part-2-clicked');
			$('#header-utitliy-username-part-2').removeClass('header-utitliy-username-part-2-normal');
			
			dropdown_div.css('top', username_part_1.position().top + username_part_1.outerHeight());
			dropdown_div.css('left', username_part_1.position().left);
		}
		dropdown_div.toggle();
	});
	
	$(document.body).click(function(e){
		var dropdown = $('#header-utility-dropdown');
		if(dropdown.length==0) {
			return;
		}
		if(!dropdown.is(":visible")) {
			return;
		}
		
		if(e.target.id == 'header-utility-dropdown' || $.contains(dropdown[0], e.target)) {
			return;
		}
		
		var part_1 = $('#header-utitliy-username-part-1');
		if(e.target.id == 'header-utitliy-username-part-1' || $.contains(part_1[0], e.target)) {
			return;
		}
		
		var part_2 = $('#header-utitliy-username-part-2');
		if(e.target.id == 'header-utitliy-username-part-2' || $.contains(part_2[0], e.target)) {
			return;
		}
		
		part_1.addClass('header-utitliy-username-part-1-normal');
		part_1.removeClass('header-utitliy-username-part-1-clicked');
		part_2.addClass('header-utitliy-username-part-2-normal');
		part_2.removeClass('header-utitliy-username-part-2-clicked');
		
		dropdown.hide();
	});
});

function sendSearchMSAS() {
	var queryTextVal = $('#queryText').val();
	var searchUrl = '/search/';
	if (queryTextVal != "") {
		searchUrl = searchUrl + queryTextVal;
		self.location = searchUrl;
		}
	else {
		alert("Please Enter the Search Term");
		$('#queryText').focus();
		return false;
	}
}
</script>

