				var o = document.createElement('div').style, r=/transform/i, good=false;
				if ('transform' in o || 'WebkitTransform' in o) good = true;
				if (good) {
					$('#featured_curve').css('background-image', 'url(/images/home/featured_curve.png)');
					$('#featured_articles').on('select-article', 'a.block', function(){
						var $a = $(this);
						var y = 135 - $a.position().top - ($a.outerHeight(false)/2);
						var angle = Math.asin(y / 1010) * (180/Math.PI);
						$('#featured_curve').jqrotate(angle);
						return false;
					});
