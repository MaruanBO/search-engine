<script>
(function(){ 
var rotating = false;
function rotate_go() {
	if (!rotating) return;
	$current = $('#featured_articles menu a.chosen');
	$next = $current.next();
	if (!$next.length) $next = $('#featured_articles menu a:first');
	$('#featured_slide').attr('href', $next.attr('href'));
	// prepare animation
	$('#featured_image2 img').attr('src', $('#featured_image img').attr('src')).parent().css('opacity',1);
	$('#featured_image img').attr('src', $next.find('img.more').attr('src'));
	// animate image
	$('#featured_image2').animate({opacity:0}, {duration:500});
	// animate copy
	var h2=$next.children('h2').html(), p=$next.children('p.more').html();
	var original_headline = decodeURIComponent($next.children('h2').data('original'));
	if (!original_headline.length) original_headline = h2;
	$('#featured_text')
		.animate({top:'100%'}, {duration:500, complete:function(){ $('#featured_text_copy h2').html(original_headline); $('#featured_text_copy p').html(p);  }})
		.delay(1500)
		.animate({top:'0%'}, {duration:500, complete:function(){  }})
		;
	$current.removeClass('chosen'); $next.addClass('chosen').trigger('select-article');
	rotating = setTimeout(rotate_go, 5000);
}
$('#featured_articles').on('mouseover', 'menu > a', function(){
	var original_headline = decodeURIComponent($(this).children('h2').data('original'));
	if (!original_headline.length) original_headline = $(this).children('h2').html();
	$(this).addClass('chosen').trigger('select-article').siblings('.chosen').removeClass('chosen');
	$('#featured_slide').attr('href', $(this).attr('href'));
	$('#featured_image img').attr('src', $(this).find('img.more').attr('src'));
	$('#featured_text_copy h2').html(original_headline);
	$('#featured_text_copy p').html($(this).find('p.more').html());
	
}).find('menu a').first().trigger('mouseover');
$('#featured_articles').mouseenter(function(){
	rotating = clearTimeout(rotating);
	$('#featured_slide > *').stop(true,true);
	$('#featured_text').css('top','0%');
}).mouseleave(function(){
	rotating = setTimeout(rotate_go, 3000);
});
rotating = setTimeout(rotate_go, 5000);
})();
</script>

