<script type="text/javascript" src="https://nh166.infusionsoft.com/app/webTracking/getTrackingCode?trackingId=05e450f72f08781e506c1a42b388795e"></script><script src="/js/eScreen.js"></script>
<script>
	$(window).load(function(){
		// Page markers
		var pageID = "index";
		var section = "";
		$('nav a[href]').each(function(){
			var href = $(this).attr('href');
			var baseURL = '';
			if(pageID=='index' && (href=='/' || href=='/index.php')) { // home page
				 $(this).addClass('current');
			} else if(section.length) { // section/ or section/page, section only links have to be an index
				baseURL += '/'+section+'/';
				pageID = pageID.replace(section+'_','');
				if(href.search(baseURL)!=-1) { 
					if(href.search(baseURL+pageID)!=-1) $(this).addClass('current');
					else if(pageID=='index' && href==baseURL) $(this).addClass('current');
				}
			} else { // root level page
				baseURL += pageID;
				if(href.search(baseURL)!=-1) $(this).addClass('current');
			}
			
		});
	});
</script>
