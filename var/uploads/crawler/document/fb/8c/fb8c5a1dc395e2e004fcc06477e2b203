			<script>
			function obr($current, $next) {
				$current.find('.tab').stop(true,true).each(function(){ $(this).animate({right:-1*$(this).outerWidth(false)}, {duration:500}); });
				$next.find('.tab').stop(true,true).each(function(){ $(this).css('right', -1*$(this).outerWidth(false)).delay(2000).animate({right:0}, {duration:500}); });
				var id = $next.attr('id').split('_').pop();
				$('#medianav [data-name='+id+']').addClass('open').siblings('.open').removeClass('open');
			}
			$('#media').eRotate({ elementType:'.media', autoRotate:7000, onBeforeRotate:obr });
			$('#medianav').on('click', '.thumb', function() {
				$('#media').eRotate('show', $('#medianav .thumb').index(this));
			}).find('.thumb:first').addClass('open');
			$('#media_prev').click(function(){ $('#media').eRotate('prev'); });
			$('#media_next').click(function(){ $('#media').eRotate('next'); });
			$('#media_youtube a.play').click(function(){ MediaPopup.open('video', $(this).data('id')); return false; });
			$('#media_radio a.play').click(function(){ MediaPopup.open('podcast', $(this).data('id')); return false; });
			</script>
