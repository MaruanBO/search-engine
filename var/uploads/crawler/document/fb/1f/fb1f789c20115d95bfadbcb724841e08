		function submitEmailSignUpFormNonInt() 
		{
			var email = document.getElementById("recipientEmail").value;			
			if (email == '')
			{
				document.getElementById("emailSignUpErrorNonInt").innerHTML = '<p style="color:#c00;">* Please enter your email address.</p>' ;
				return false;
			}
			else if(!isValidEmail(email)) 
			{ 
				document.getElementById("emailSignUpErrorNonInt").innerHTML = '<p style="color:#c00;">* Please verify that you entered a valid email.</p>' ;
				return false;
			}
			else
			{
				document.emailSignUpFormSubmitNonInt.recipient.value=email.toLowerCase();
				document.emailSignUpFormSubmitNonInt.submit();
			}
		}
