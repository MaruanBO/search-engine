<script type="text/javascript">
	// On document ready, show My Dillard's bar
	$(displayMyDillardsBar);
	
	var pageNameForOnClick = "Default Page Name";
	var z,n,isCart;
	var conf = {over: show,timeout: 200,out: hide};
	var cartConf = {over: showCart,timeout:200, out:hide};
	window.$topNav={w : $('#attributes')};
	
	function remove() {$("#top-nav ul li a").removeClass("on");}
	function show() {remove();$(this).children('.sub').fadeIn(); z=$(this).children('a'); setTimeout("$(z).addClass('on');", 145 );} //$(this).find('input:visible:first').focus();
	function hide() {$(this).children('.sub').fadeOut(); n=$(this).children('a'); setTimeout("$(n).removeClass('on');", 165 );}
	function showCart() {remove(); if(isCart != "y"){getMiniCart();}$(this).children('.sub').fadeIn(); z=$(this).children('a'); setTimeout("$(z).addClass('on');", 145 );}
	
	$(document).ready(function(){
		$(".print").click(function() {
			cmCreateConversionEventTag("Print Clicked", "2", "Print");
		});
		$("#find-store").hoverIntent(conf);
		$("#account-guest").hoverIntent(conf);
		$("#account").hoverIntent(conf);
		$("#connect").hoverIntent(conf);
	}); 
	
	
	function filterSearchTerm(input) {
		s = $.trim(input.value);
		s = Validate.stripProhibitedCharacters(s);
		input.value = encodeSpecialCharacters(s);
			
		if (s != ""){	
			cmCreateConversionEventTag(input.value,"2","Search Term");
			cmCreateElementTag(input.value,"Search Term");
			return true;
		}
		else{
			return false;
		}
	}
	
	function filterTypeAheadSearchTerm(input) 
	{
		s = input;
		s = s.replace("/","-");
		filteredValues = "}#\'`~$\&^+=<>\"|\\%}";     // Characters stripped out
		var i;
		var returnString = "";
		
		for (i = 0; i < s.length; i++) 
		{  // Search through string and append to unfiltered values to returnString.
			var c = s.charAt(i);
			c = c.replace("/","-");
			if (filteredValues.indexOf(c) == -1) returnString += c;
		}
			
		document.CatalogSearchForm.Ntt.value = returnString;
		
		return encodeSpecialCharacters(returnString);
	}

		
	
	function makeLinksNonSecured() {
		
		var hostName = window.location.hostname;
	
		var pathName = window.location.pathname;
		var portNumber = window.location.port;
		pathName = pathName.substring(0,pathName.lastIndexOf("/"));
		var pathFullQualifier = "http://"+ hostName + pathName + "/" ;
		var hostQualifier =  "http://"+ hostName ;
		var tempvar = "";
		
	   $("a").each(function(i) {

		  	if($(this).attr("href") === undefined || $(this).attr("href") == "" || (portNumber != null && (portNumber == 8000 || portNumber == 8002 || portNumber == 8006 || portNumber == 8007)))
		  	{
		  		// it does not have any link associated...Don't do any thing  
		  	}
			
	      	else if ( (  /^(http|ftp):\/\/.*$/i.test( $(this).attr("href") ) ) 
	      				|| ( /^(javascript).*$/i.test( $(this).attr("href") ) || (($(this).attr("href") !== undefined) && $(this).attr("href").replace(/^\s+|\s+$/, '').indexOf('#' ) == 0 )) )
	      	{
	      		// Don't do any thing 
	      	}
	      	
	      	else if ( ($(this).attr("href") !== undefined) && ($(this).attr("href").indexOf("/") == 0))
	      	{
				$(this).attr("href" ,  hostQualifier +   $(this).attr("href"));
	      	}
	      			      
	      	else
	      	{
	       		if(($(this).attr("href").split("https://")[1]).split("/")[0] == hostName) 
	       		{
		       		tempvar = tempvar + " " + $(this).attr("href") + "\n" ;
		      		$(this).attr("href" ,     pathFullQualifier +   $(this).attr("href"));
	      		}
			}
	   });
	};


	$(document).ready(function(){
		var searchFilter=$('#search-filter');
		var sFList=searchFilter.find('ul');
		var searchFilterVal=$("#searchFilterValue");
		searchFilter.hoverIntent({over: showDD,timeout: 200,out: hideDD});

		sFList.bind('click', function(r){var n=$(r.target); if(n.is('li a')){searchFilter.children('div').html(n.html()); searchFilterVal.val(n.parent().attr('title'));($(n).html()); sFList.hide();}});

	    
		
	    var isIE = (navigator.appName=="Microsoft Internet Explorer");
	    
	});

	function setCategoryDropdownValue(form, clickedDropdownValue)
	{
		form.N.value = clickedDropdownValue;
	}

	var viewTaskNameValue =  '';
	var callingPageValue =  '';

	function getMiniCart(){	
    	var ajaxOptions = {
				type: "GET",
				dataType: "html",
				url: "/webapp/wcs/stores/servlet/MiniShopBagAJAXView",
				data: "catalogId=" + 301 + "&storeId=" + 301 + "&langId=" + -1,
				success: function(html){
					$("#cart-wrapper").removeClass("f"); //remove the loading image 
					$("#mini-bag-popUp").html(html); //populate with the shopping bag data
				   	isCart="y"; //This prevents the ajax call from being called twice
				   	$("#mini-bag-popUp").fadeIn("fast");
			  		}
			   };
		  
		  if((viewTaskNameValue != null && viewTaskNameValue == 'ChangePassword') || (callingPageValue != null && callingPageValue == 'ResetPassword')) {
			//don't do Ajax call in such case
		  }else {
			$.ajax(ajaxOptions); //This makes the AJAX call
		  }
		return false;
	}

	$(document).ready(function() {
	
		var searchForm=$('#search-form');
		var searchInput=$('#search-input');

		searchInput.focus(function() {
			if ($(this).val() == ''){
				$(this).val('');
			}
		});
		
		searchForm.submit(function() {
			if (searchInput.val() == "" || searchInput.val() == 'Search keyword or item #') {
				return false;
			}else{
				return true;
			}
		});
	});

	function encodeSpecialCharacters(str){
		str = str.replace(/ô/ig, 'o');
		str = str.replace(/î/ig, 'i');
		str = str.replace(/é/ig, 'e');
		return str;
	}

	var config = {over: shows,timeout: 300,out: hides};
	var b,left;
	function shows() {$(this).addClass('on');$(this).find('.sub').fadeIn(); loadDDImage(this);}
	function hides() { $(this).removeClass("on").find('.sub').fadeOut(165); }
	function loadDDImage(cat) {
		if (!$(cat).hasClass("loaded")) {
			$(cat).find('img').attr("src",function() {return $(this).data('src');});
			$(cat).addClass('loaded');
		}	
	}
	
	$(document).ready(function() {
		var cookieValue = getCookie("WL_HEADER");
		if (cookieValue != null){
			var start = cookieValue.indexOf("NUM:") + 4;
			var stop = cookieValue.indexOf("OPTION:");
	
			if ((stop-start) > 0){
				$('#backToRegistry').show();
			}
		}
		
		var frameLogonCookie = getCookie("fromMyFrame");
		//if logged on through My Dillards Bar, fromMyFrame cookie is set
		if (frameLogonCookie != null){
			var domainName = 'www.dillards.com';
	    	if ( domainName.indexOf( "dillards.com" ) == -1 ){
	    		domainName = domainName;
	    	} else if( domainName == 'www.dillards.com' ) {
	    		domainName = ".dillards.com";
	    	} else {
	    		domainName = "."+domainName;
	    	}
			document.cookie = 'fromMyFrame=; max-age=0; expires=Fri, 31 Dec 1999 23:59:59 GMT; path=/; domain=' + domainName + ';';
			if( document.URL.indexOf("ResetPasswordRecommendView") == -1 )
			{
				openMyDillardsFlyOut();
			}
		}
		
	});
</script>
 
	<script type="text/javascript">
		$(document).ready(function(){
			$.extend({
			  getUrlVars: function(){
			    var variables = [], vars;
			    var parameters = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
			    for(var i = 0; i < parameters.length; i++)
			    {
			      vars = parameters[i].split('=');
			      variables.push(vars[0]);
			      variables[vars[0]] = vars[1];
			    }
			    return variables;
			  },
			  getUrlVar: function(name){
			    return $.getUrlVars()[name];
			  }
			});
			var value = $.getUrlVar('cptc');
			if(value != "" && value != null){
 				var cookieName = "cptc";
				var cookieValue = value;
				var domainName = ".dillards.com";
 	 			$.cookie(cookieName, cookieValue,{ path: '/',domain: domainName});
 	 		}
	 	});
	</script> 


