<script type="text/javascript">
	function callQuickView(catentryId, categoryId){		
		$.ajax({
			url: "/webapp/wcs/stores/servlet/BuildSelectBox",
			cache: false,
			type: "POST",
			data: "catalogId=301&langId=-1&storeId=301&qv=Y&catentryId=" + catentryId,
			dataType: "json",
			beforeSend: function(){
				$("#added-to-bag").html("<div class='dd' style='text-align:center;font-weight:bold;width:540px;'>Loading<span><br><img src='/images/common/loader-big.gif'></span></div>").show();
				$("#qvContent").hide();
				$("#added-to-bag-modal").removeClass("qvModalSizer");
				$("#added-to-bag-modal").find(".modalTitle").html('<div class="DillardsModalTitle">Quick View</div>')
				$("#added-to-bag-modal").DillardsModalOpen();
			},
			success: function (json) {
				$("#added-to-bag").hide();
				if(json.JSON.notBuyable == null){
					jsonJSON[json.JSON.catentryId] = json;
					buildDDs(json);
					if(typeof categoryId == 'undefined'){
						categoryId = ( DillardsURLEvaluator.getValue("categoryId") != undefined && DillardsURLEvaluator.getValue("categoryId") != "") ? DillardsURLEvaluator.getValue("categoryId") : "-10035"
					}
					$("#qvContent div[id='add-box']").html(ddJSON[catentryId]);
					$("#qvContent div[id='add-box']").append("<div id='pdpQuick'></div>");
					$(qvForm.quantity).val("1")
					bindColorDD('.qvRight #add-box','#qvProductImage');
					$(".qvRight #add-box").append(swatchContentString);
					$(".qvRight #add-box .swatches").hide();
					$("#action-area").show();
					matt();
					cmCreateProductviewTag("Product:" + json.JSON.productId + ":" + json.JSON.qvName,json.JSON.productId,json.JSON.qvName, categoryId, "301", "Y", null, null, json.JSON.qvBrand + "-_-PP-QV-_--_-Y");
				}else{
					$("#action-area").hide();
					$("#qvContent div[id='add-box']").html('<div class="noLongerAvailable">This item is no longer available for purchase on Dillards.com</div>');
				}
				populateProductInfo(json,catentryId,categoryId);
				DillardsModal.sizeModal($("#added-to-bag-modal"))
				if("undefined" != typeof runMonetateTestQV1) runMonetateTestQV1();
	        	if("undefined" != typeof runMonetateTestQV2) runMonetateTestQV2();
	        	if("undefined" != typeof runMonetateTestQV3) runMonetateTestQV3();
	        	if(isInternational){
					$(".internationalRemove").hide();
				}
			}	
		});
	 	 return false;				
	}
	
	function populateProductInfo(json,catentryId, categoryId){
		$(".qvDescription").val(json.JSON.productId + "|" + catentryId + "|" + categoryId + "|" + "Y");
		$(".qvCatentry").val(catentryId);
		$("#qvProductName").html(json.JSON.qvName);
		$("#qvProductId").html("Item #" + json.JSON.qvProductId);
		$("#qvPrice").html(json.JSON.qvPrice);
		$("#avAltImageDiv").html(json.JSON.avAltImageDiv);
		$("#qvDescription").html(json.JSON.qvDescription);
		$("#qvProductUrl").attr("href","/product/_301_-1_301_" + json.JSON.qvCatentryId + "?categoryId=-10041");
		$("#qvProductImage").attr("src",json.JSON.qvImage + "?$c7product$");
		if(json.JSON.qvPreOrder){
			$(".qvRight #add-wishlist").hide()
		}
		sliderColorDDBind.runIt();
		$("#qvContent").fadeIn();
	}
	
	function altImageSwap (color){
		if(typeof(color) !== 'undefined' && color != null && color != 'sku'){
			imgPlace=protocol + "//dimg.dillards.com/is/image/DillardsZoom/" + color; 
			 $("#qvProductImage").prop('src',imgPlace);	
		}
		cache: false;	
	}
	$(document).ready(function(){	
		$("#added-to-bag").parent(".modalContent").append($("#qvContent"));
		var displayQV = "true";
		if(displayQV != 'true'){
			$(".quick-view-to-hide").hide();
		}
	})
	function matt(){
		$("#qvContent").find("select").change(function(){
	    $(window).scrollTop($(window).scrollTop()-1);
		});
	}

</script>
