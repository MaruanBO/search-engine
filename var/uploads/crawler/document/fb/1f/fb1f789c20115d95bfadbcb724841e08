<script type="text/javascript">
$(document).ready(function() {
	var cookieValue = getCookie("WL_HEADER");
	if (cookieValue != null){
		var start = cookieValue.indexOf("NUM:") + 4;
		var stop = cookieValue.indexOf("OPTION:");
		
		var optionValue = cookieValue.substring(cookieValue.indexOf("OPTION:") + 7, cookieValue.indexOf("OPTION:") + 8);
		
		if ((stop-start) > 0 && optionValue == 'M'){
			$('#registry').show();
		}
	}
});
</script>
