	<script type="text/javascript">
			//Mini Cart popout code
		 	$(document).ready(function(){
			
				 var miniCartOpen = false;
			 
		 		$('#headerCartInfo2, .checkoutButton').hoverIntent(function(){
		 		var cartQuanity = $.cookie('301_CVMINICART'); 
				 	if(miniCartOpen == false && parseInt(cartQuanity.split("~~~")[1].split('@')[0], 10) > 0 > 0){
				 		miniCartOpen = true; 
				 		$('#myDillardsContent').fadeOut();
				 		$('#myDillardsInvisibleWall').hide(); 
				 		retrieveMiniCartPopout(); 
					}
				},function(){});
				
				 $('#header-wrapper, #wrapper').on('mouseenter', function(){ miniCartOpen =
				 false; $('#mini-cart-popup').fadeOut(); });
				 
				 $(document).mouseup(function(e){ var container = $('#mini-cart-popup'); if
				 (!container.is(e.target) && container.has(e.target).length === 0) {
				 miniCartOpen = false; container.fadeOut(); } });
				 
				 $('.openClose').on('mouseenter', function(){ miniCartOpen = false;
				 $('#mini-cart-popup').fadeOut(); }); 
			 });
	</script>



<script type="text/javascript">
	var currentCookieValue = "";
	var shoppingBagValuesSet = false;

	//Check for change in cookie value to update display of item count and amount
	function watchForCookieChange() {
		var newCookieValue = $.cookie('301_CVMINICART');
		
		//Change CHECKOUT button
		
		if (newCookieValue && newCookieValue !== null) {
			if (parseInt(newCookieValue.split("~~~")[1].split('@')[0], 10) > 0) {
				$(".checkoutButton").removeClass('secondary').addClass('primary');
			} else {
				$(".checkoutButton").removeClass('primary').addClass('secondary');
			}
		}
		
		if (currentCookieValue == null || currentCookieValue == "") {
			currentCookieValue = newCookieValue;
		} else {
			if ((currentCookieValue != newCookieValue) || ($("#myDillardsBarText .fiftyOne-cartTotalItems").html() == "(0)" && shoppingBagValuesSet == false)) {
				shoppingBagValuesSet = true;
				currentCookieValue = newCookieValue;
				var totalItems = currentCookieValue.split("~~~")[1].split("@")[0];
				var totalAmount = currentCookieValue.split("~~~")[2].split("@")[0];
		 		$("#myDillardsBarText .fiftyOne-cartTotalItems").html("("+$.trim(totalItems)+")");
		 		$("#myDillardsBarText .fiftyOne-cartTotal").html($.trim(totalAmount));
			}
		}
		
		var cookieChangeTimoutHandle = setTimeout(watchForCookieChange, 2000);
	}
	
	 function retrieveMyDillardsWelcome() 
	 {
	 	var ajaxUrl = "/webapp/wcs/stores/servlet/MyDillardsWelcomeView?";
	 	ajaxUrl += "storeId=301&langId=-1&catalogId=301";

	  	$.ajax({			  		
			type: "GET",
			url: ajaxUrl,
			dataType: "html",
			success: function(html){
				$("#welcome-message").html(html);
			},
			error: function (XMLHttpRequest, textStatus, errorThrown) {
				$("#welcome-message").html("");
			}
	  	});
	  	
	 };
	 
	 function retrieveMiniCartPopoutOops() 
	 {
	 	var ajaxUrl = "/webapp/wcs/stores/servlet/MiniCartPopoutView?";
	 	ajaxUrl += "storeId=301&langId=-1&catalogId=301";

	  	$.ajax({			  		
			type: "GET",
			url: ajaxUrl,
			dataType: "html",
			success: function(html){
				$("#mini-cart-popup").html(html);
 			},
			error: function (XMLHttpRequest, textStatus, errorThrown) {
				$("#mini-cart-popup").html("");
			}
	  	});
	  	
	 };
	 
	$(document).ready(function(){
		if( 'true' == 'true' )
		{
			retrieveMyDillardsWelcome();
		}
		watchForCookieChange();
	});


	function retrieveMiniCartPopout() {
		$.ajax({
			url 		: "/webapp/wcs/stores/servlet/MiniCartPopoutCmd",
			cache 		: false,
			type 		: "POST",
			data 		: "storeId=301&langId=-1&catalogId=301",
			dataType	: "json",
			beforeSend 	: function(){
				$("#mini-cart-popup").hide();
				$("#mini-cart-popup").html("");
			},
			success		: function(json) {
 				if(json){
 					$("#mini-cart-popup").html("");
	 				var successResp = json.JSON;
		 			var errorResp = json.JSONError;
		 			if(successResp != null){		 
 		 				var emptyCartContents = successResp.emptyCartContents==null?"":successResp.emptyCartContents;
	 					if(emptyCartContents != null && emptyCartContents != ""){
	 		 				$("#mini-cart-popup").append("<div id='cart'><div class='emptyCart'>" + emptyCartContents+ "</div></div>");
			 				//$("#mini-cart-popup").show();
			 			}
						else{
 	 	 	   			 	var nonRegistryOrderItemsMap = successResp.nonRegistryOrderItemsMap==null?"":successResp.nonRegistryOrderItemsMap;
		   			   	 	var registryOrderItemsMap = successResp.registryOrderItemsMap==null?"":successResp.registryOrderItemsMap;
							if(nonRegistryOrderItemsMap != "" || registryOrderItemsMap != "" ){
 								$("#mini-cart-popup").append("<div id=\"miniClose\"><p>Close<\/p><img src=\"\/images\/buttons\/xButton.png\" \/><\/div><div class=\"stickyHeaders hideWhenEmpty\"><div class=\"itemDescription\">Item Description</div><div class=\"itemQTY\">QTY</div><div class=\"subTotal\">Subtotal</div></div>");
 			 				}
  
		 			 		$.each(nonRegistryOrderItemsMap, function(key, value) { 
		 			 			 var attrs;
		 			 			 if(value.attrs != ""){
			 			 			 if(value.attrs.indexOf('|') != -1){
			 			 			    attrs= value.attrs.split('|');
			 			 			 }
			 			 			 else{
			 			 			    attrs = [value.attrs,""];
			 			 			 }
		 			 			 }
		 			 			 else{
			 			 		    attrs = ["",""];
			 			 		 }
		 			 			 $("#mini-cart-popup").append("<div class=\"itemRow\">"
		 			 										+"<div class=\"imgBlock\">"
		 			 										+"<a href=" + decodeURIComponent(value.url.replace(/\+/g, ' ')) + "><img alt="+ decodeURIComponent(value.name.replace(/\+/g, ' ')) +" src=\"//dimg.dillards.com/is/image/DillardsZoom/"+ decodeURIComponent( value.image) +"\" border=\"0\"></a>"
		 			 										+"</div>"
		 			 										+"<div class=\"descriptionBlock\">"
		 			 										+"<a href=" + decodeURIComponent(value.url.replace(/\+/g, ' ')) + ">"+ decodeURIComponent(value.name.replace(/\+/g, ' ')) +"</a><br>"
 		 			 										+"<span>"+ decodeURIComponent(attrs[0].replace(/\+/g, ' ')) +"</span>"
															+"<br>"
		 			 										+"<span>"+ decodeURIComponent(attrs[1].replace(/\+/g, ' ')) +"</span>"
															+"<br>"
 															+"</div>"
 															+"<div class=\"QTYblock\">"
 															+ value.qty 
															+"</div>"					
															+"<div class=\"subTotalBlock\">"
															+ value.price
															+"</div>"												
 													        +"</div>");
 							});
 		 			 		$.each(registryOrderItemsMap, function(key, value) { 
		 			 			 var attrs = value.attrs.split('|');
		 			 			$("#mini-cart-popup").append("<div class=\"itemRow\">"
		 			 										+"<div class=\"imgBlock\">"
		 			 										+"<a href=" + decodeURIComponent(value.url.replace(/\+/g, ' ')) + "><img alt="+ decodeURIComponent(value.name.replace(/\+/g, ' ')) +" src=\"//dimg.dillards.com/is/image/DillardsZoom/"+ decodeURIComponent( value.image) +"\" border=\"0\"></a>"
		 			 										+"</div>"
		 			 										+"<div class=\"descriptionBlock\">"
		 			 										+"<a href=" + decodeURIComponent(value.url.replace(/\+/g, ' ')) + ">"+ decodeURIComponent(value.name.replace(/\+/g, ' ')) +"</a><br>"
 		 			 										+"<span>"+ decodeURIComponent(attrs[0].replace(/\+/g, ' ')) +"</span>"
															+"<br>"
		 			 										+"<span>"+ decodeURIComponent(attrs[1].replace(/\+/g, ' ')) +"</span>"
															+"<br>"
 															+"</div>"
 															+"<div class=\"QTYblock\">"
 															+ value.qty 
															+"</div>"					
															+"<div class=\"subTotalBlock\">"
															+ value.price
															+"</div>"												
 													        +"</div>");
	 						});
							if(nonRegistryOrderItemsMap != "" || registryOrderItemsMap != "" ){
 			 					$("#mini-cart-popup").slideDown("slow");
 			 					$('#miniClose').on('click', function(){
 			 						miniCartOpen = false;
									$('#mini-cart-popup').fadeOut();
									
								});
  							} 							
						}
  	 	   			}
	 	   	 		else if(errorResp != null) {
		 				$("#mini-cart-popup").append("<div id='errorWithBreaks'><br><br><div class='error-msg'>" + errorResp+ "</div></div>");
		 				$("#mini-cart-popup").slideDown("slow");
		 				$('#miniClose').on('click', function(){
		 					miniCartOpen = false;
							$('#mini-cart-popup').fadeOut();
						});
		     		}
	     		}
 			}
		});
	}
	
	 function retrieveMyDillardsLinksLogin() 
	 {
	 	var ajaxUrl = "/webapp/wcs/stores/servlet/MyDillardsLinkiFrameView?";
	 	ajaxUrl += "storeId=301&langId=-1&catalogId=301";

	  	$.ajax({			  		
			type: "GET",
			url: ajaxUrl, 
			dataType: "html",
			success: function(html){
				$("#myDillardsContentFloat .leftSide").html(html);
			},
			error: function (XMLHttpRequest, textStatus, errorThrown) {
				$("#myDillardsContentFloat .leftSide").html("There was a problem submitting your request");
			}
	  	});
	  	
	 };
	 
	function replaceModalContent(html){
		$("#continueToWishListHeader #modalContent").html(html);	
		$(window).trigger("resize");
	}
	 
//Checks if user is logged in and links user to to login or wishlist
var wishListLoggedIn = (function ($) {

	"use strict";

	$('span#myDillardsWishlist a').on('click', function (event) {
		
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		
		if ($('#welcome').length > 0) {
		 
			replaceModalContent('Continuing to Your Wish List');
 			document.location.href = '/webapp/wcs/stores/servlet/InterestItemDisplay?storeId=301&langId=-1&catalogId=301';
		}
		else {
			
			replaceModalContent('Continuing to Login');
 			document.location.href = '/webapp/wcs/stores/servlet/Logon?storeId=301&langId=-1&catalogId=301&rURL=InterestItemDisplay';
		}
	})

}(jQuery)); 
