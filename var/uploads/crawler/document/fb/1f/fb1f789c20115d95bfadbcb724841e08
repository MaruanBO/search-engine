			<script type="text/javascript">

			var typeAheadContents;
				
			function getCoOrdsForDiv()
		    {
		        // Get top-left co-ordinate of div
		        var textbox = $("#search-input");
		        var offset = textbox.offset();
		        
		        // Add height of textbox to the top co-ordinate to get the botton left co-ordinate
		        offset.top += textbox.height();
		        
		        // Add some pixels to fix div position, don't know why offset wasnt accurate
		        offset.top += 6;
		        offset.left += 3;
		        
		        return offset;
		    }

			function getContent(){
				
				var ajaxUrl = "/webapp/wcs/stores/servlet/Typeahead?";
				
				// D=vitamin%20d&Dx=mode+matchall&Dn=0&N=0&Ntt=vitamin%20d&Ntk=product.name";
				ajaxUrl += "storeId=301&catalogId=301&langId=-1";
				ajaxUrl += "&D=" + $.trim(filterTypeAheadSearchTerm(document.getElementById('search-input').value)) + "*";
				ajaxUrl += "&Ntt=" + $.trim(filterTypeAheadSearchTerm(document.getElementById('search-input').value)) + "*";
				ajaxUrl += "&N=" + document.getElementById('N').value;
				ajaxUrl += "&Dn=" + document.getElementById('N').value;
				ajaxUrl += "&Dk=1";
				//Can use Di when upgrade to MDEX 6.2.0 (allows you to specify a list of dimensions to search against)
				// 6.2.0 also allows you to show the counts of products in each category returned.
				// ajaxUrl += "&Di=300000+10000000+100000";
				ajaxUrl += "&Ns=p_best_sellers_rank";
				ajaxUrl += "&Dx=rel+first,static(nbins,descending)";
				ajaxUrl += "&Ntk=p_short_description"; 
				// set up the nav state
			
				$.manageAjax.add('unique_identifier',{
					url: ajaxUrl,
					cache: false,
					type: 'POST',
					success: function(html){
					var offset = getCoOrdsForDiv();
					$("#SuggestionListDiv").html(html);
					typeAheadContents=$("#typeAheadContents");
					if ($.trim($("#typeAheadContainer").text()) != ""){
						if ($("#search-input").val().length >= 3) {
							if( $("#typeAheadContainer").is(":hidden")){
								typeAheadContents.show();
													  	typeAheadContents.hover(
							function () {
								$(this).show();
							}, 
							function () {
								$(this).hide();
							}
						);
							}
						}

					}

					// handle wrapping on typeahead results
					$(".typeAheadGray").each(function () {

						var lineHeight = $(this).height(),
						    liOffset = $(this).parents("li").eq(0).offset().top,
						    grayOffset = $(this).offset().top;

						if (grayOffset >= liOffset + lineHeight) {

							$(this).addClass("wrapped");
						}
					});
					//Allows for UP DOWN and ENTER navigation on search sugestions
					searchBoxKeyNav.runIt();
				}
			});
			
			//jQuery.manageAjax.abort('unique_identifier');
			//jQuery.manageAjax.clear('unique_identifier');	
			//jQuery.manageAjax.add('unique_identifier',{success: function(data) {}});
			
			}
			
			$(document).ready(function(){
				$.manageAjax.create('unique_identifier',{
					queue:'clear',cacheResponse:false,maxRequests:1,abortOld:true
				});
				
				if( 'true' == 'true' )
				{
					$("#search-input").keyup(function(e) {
						var keyPressed = e.which;
								
						if ($("#search-input").val().length >= 3 && keyPressed !== 40 && keyPressed !== 38){
							getContent();
					  	}
					});
					
					$("#search-input").focus(function() {
						if ($("#search-input").val().length >= 3){
							getContent();
				  		}
					});
	
				};
			});
			
			</script>
			
			</div>
		</form>
	</div>
