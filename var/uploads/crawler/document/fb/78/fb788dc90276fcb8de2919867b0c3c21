<script>
	var current;
	$(document).ready(function() {
		$('.dornobCategoriesParent').click(function(e){
			if(current){ try{$(current).children().slideUp('fast');}catch(e){} }
			
			try{$(".dornobCategoriesParent").children().slideUp('fast');}catch(e){}
			
			current = e.target;
			try{$(current).children().slideDown('fast');}catch(e){}
		});
	});
	
	function splayOpenSidebar(p, c){
		current = p;
		try{$("#" + current).children().slideDown('400');}catch(e){}
	}
</script>

