					function isSearchEngineReferred(){
				var pattern = new RegExp("\/\/.*\.(google|bing|yahoo|msn|ask|aol|blekko|volunia)\.com");
				var referrer = document.referrer;
	
				if(pattern.test(referrer)){
					return true;
				}
				return false;
			}
	
