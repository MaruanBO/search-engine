            $(document).ready(function() {
              var url = '/vanilla/discussions';
              $.ajax({
               url: url, dataType: 'json',
               success: function(data) {
                   // Initial list
                   var list = data;
                   if (list.length > 0) {
                     $('.hot-discussions').html('');
                     for(var i = 0; i < list.length; i++){
                       var d = list[i];
                       var content_str = '<li><strong class="discussion-title"><a href="'+d.url+'">'+d.name+'</a></strong></li><li><span class="discussion-time-stamp">'+d.last_comment+'</span><span class="discussion-views">'+d.views+' Views</span> | <strong class="discussion-comment-count"><a href="'+d.url+'">'+d.comments+' Replies</a></strong></li>';
                      $('.hot-discussions').append('<ul class="discussion">'+content_str+'</ul>');
                     }
                   }
                }
              });
            });
