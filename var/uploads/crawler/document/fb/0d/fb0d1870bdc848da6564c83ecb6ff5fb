<script language="javascript">
$(document).ready(function() {

    
    function sharethis_callback_click (event,service)
    {
        var shared_nid = '';        
        if(shared_nid != null && shared_nid.length > 0){
           $.ajax({ type: "POST", url: "/ajax.php", data: "type=saveshare&nid=" + shared_nid });
        }
    }
    try{
    stLight.subscribe("click",sharethis_callback_click);
    }catch(e){}


    setMakes(0, '#make_rail');

    var d = new Date();
    var curr_year = d.getFullYear();
    for(var year = curr_year; year >= 1900; year--){
        $('#year_rail').append('<option value="'+year+'">'+year+'</option>');
    }
});
function RailSearchSurvey(){
    var url = '/content/car-talk-car-survey?make='+$('#make_rail').val()+'&model='+$('#model_rail').val()+'&y='+$('#year_rail').val();
    window.location.href = url;
}

function ToggleArchive(y){    
    for(var x = 2015; x >= 1980; x--){
        try{ $('#archive_link_' + x).hide();  } catch(e){}
    }
    $('#archive_link_' + y).show();
}
</script>




