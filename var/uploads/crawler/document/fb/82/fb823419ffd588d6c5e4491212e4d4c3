    <!-- [COOKIE SLIDER] -->
		
    <script type="text/javascript">

        var welcome_image_error = "";
        var welcome_image_duplicate = "";
        var welcome_image_ok = "";
        var welcome_image = "";
        var welcome_image_arrow = "";
        var welcome_image_submit = "";
        var submiturl = "";



        welcome_image_error = "http://www.forever21.com/images/en/20150127/welcome_slider_invalid.jpg";
        welcome_image_duplicate = "http://www.forever21.com/images/en/20150127/welcome_slider_duplicate.jpg";
        welcome_image_ok = "http://www.forever21.com/images/en/20150127/welcome_slider_confirmed.jpg";
        welcome_image = "http://www.forever21.com/images/en/20150127/welcome_slider_main.jpg";
        welcome_image_arrow = "http://www.forever21.com/images/en/20150127/welcome_slider_open.jpg";
        welcome_image_submit = "http://www.forever21.com/images/en/20150127/welcome_slider_button.jpg";

        submiturl = AppPath + "/Ajax/Subscribe.aspx?email={email_address}";
        

                 

        function ShowSlide() {
            if ($j("#popUp").length) {
                $j("#popUp").css("display", "block");
            }

            SetAnotherCookie(); // Set Second Cookie            
        }

        function ShowMarketingSlide() {

            if ($j("#popUp_marketing").length) {
                $j("#popUp_marketing").css("display", "block");
            }

            SetAnotherCookie(); // Set Second Cookie            
        }

        function setSecondCookie(cname, cvalue) {
            //var d = new Date();
            //d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));


            var expires = "";  // No expiration - Deletes on Exit

            if ($j("#popUp").length) {
                var dd = new Date();
                var ee = new Date();
                ee.setTime(dd.getTime() + 3600000 * 24 * 365);

                expires = ee.toGMTString();


            }

            document.cookie = cname + "=" + cvalue + ";expires=" + expires;
        }

        function getSecondCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
            }
            return "";
        }

        function checkCookie() {
            var sliderval = getSecondCookie("slider_r1");
            if ($j("#popUp").length) {
                sliderval = getSecondCookie("slider_w");
            }
            if ($j("#popUp_marketing").length) {
                sliderval = getSecondCookie("slider_m");
            }
            if (sliderval == "S1") {
                $j(".PromoCode").css("display", "none");

                // popup=1 will always have slider out first
                if (document.referrer.indexOf('popup%3D1') > 0 || window.location.search.indexOf("popup=1") > -1) {
                    if ($j("#popUp").length) {
                        //if (getSecondCookie("welcome_count") >= 1) {
                        //    $j("#popUp").css("display", "block");
                        //} else {
                            $j("#popUp").css("right", "152px");

                            // Show initially, stop, and slowly fade right
                            $j("#popUp").stop(true, true).delay(2000).animate({
                                right: "-397px"
                            }, { duration: 2000 });
                        //}
                    }

                    if ($j("#popUp_marketing").length) {
                        $j("#popUp_marketing").css("right", "152px");

                        // Show initially, stop, and slowly fade right
                        $j("#popUp_marketing").stop(true, true).delay(2000).animate({
                            right: "-397px"
                        }, { duration: 2000 });
                    }

                }
                else {
                    // S1 Value then display
                    if ($j("#popUp").length) {
                        $j("#popUp").css("display", "none");
                    }

                    if ($j("#popUp_marketing").length) {
                        $j("#popUp_marketing").css("display", "block");
                    }
                }
            }
            else {

                // Show initially, stop, and slowly fade right


                if ($j("#popUp").length) {
                    if (getSecondCookie("welcome_count") >= 1) {
                        $j("#popUp").css("display", "block");
                    } else {
                        $j("#popUp").css("display", "block");
                        $j("#popUp").css("right", "152px");

                        $j("#popUp").stop(true, true).delay(2000).animate({
                            right: "-397px"
                        }, { duration: 2000 });
                    }
                }

                if ($j("#popUp_marketing").length) {
                    $j("#popUp_marketing").css("display", "block");
                    $j("#popUp_marketing").css("right", "152px");

                    $j("#popUp_marketing").stop(true, true).delay(2000).animate({
                        right: "-397px"
                    }, { duration: 2000 });
                }

            }

        }

        function SetAnotherCookie() {
            if ($j("#popUp").length) {
                setSecondCookie("slider_w", "S1"); // Set Value S1            
            }

            if ($j("#popUp_marketing").length) {
                setSecondCookie("slider_m", "S1"); // Set Value S1            
            }

        }

        $j(document).ready(function () {
            checkCookie(); // Must load after   
            
            // Remove Hover After Initial Close        
            var hoverVal = getSecondCookie("hover");
            if (hoverVal == "Y1")
            {
                $j("#imgArrow_marketing").removeAttr("onmouseover");
            }

        });

        $j(window).load(function () {
            //welcome
            if ($j("#popUp").length) {
                checkCount();
            }

            //marketing
            if ($j("#popUp_marketing").length) {
                checkMarketingCount();
            }


            if ($j("#10slide_img").length) {
                $j('#10slide_img').attr('src', welcome_image);
            }

            if ($j("#imgArrow").length) {
                $j('#imgArrow').attr('src', welcome_image_arrow);
            }

            if ($j("#imgBtn").length) {
                $j('#imgBtn').attr('src', welcome_image_submit);
            }

        });				
    </script>

    <!-- CHECK FORM -->
    <script language="javascript" type="text/javascript">
        var $j = jQuery.noConflict();

        function EmailCheckForm(type) {
            var objRegExpEmail = /^[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+)*@[0-9a-zA-Z-]+(\.)+([0-9a-zA-Z-]+)([\.0-9a-zA-Z-])*$/;
            var objRegExpZipCode = /[~!@\#$%^&*\()\=+_'\"<>]/gi;
            var form_pass = true;
            var err_msg = "";



            var emailaddress = "";

            if ($j("#popErrMsg").length) {
                $j('#popErrMsg').text(err_msg);
            }

            if ($j("#popErrMsg_marketing").length) {
                $j('#popErrMsg_marketing').text(err_msg);
            }

            if ($j("#inputEmailAddress").length) {
                if (!$j('#inputEmailAddress').val()) {
                    err_msg = "";
                    form_pass = false;
                } else {
                    if (!objRegExpEmail.test($j('#inputEmailAddress').val())) {
                        err_msg = "";
                        form_pass = false;
                    }
                }

                emailaddress = $j('#inputEmailAddress').val();
            }


            if ($j("#inputEmailAddress_marketing").length) {
                if (!$j('#inputEmailAddress_marketing').val()) {
                    err_msg = "";
                    form_pass = false;
                } else {
                    if (!objRegExpEmail.test($j('#inputEmailAddress_marketing').val())) {
                        err_msg = "";
                        form_pass = false;
                    }
                }

                emailaddress = $j('#inputEmailAddress_marketing').val();
            }


            slideMsg = '';



            if (type == 'marketing') {
                //marketing
                submiturl = AppPath + "/Ajax/MarketingSubscribe.aspx?email=" + emailaddress + "&eventid=1805&eventname=Marketing_Slider_Entry";
            } else {
                //welcome
                submiturl = submiturl.replace("{email_address}", emailaddress);
            }

            if (!form_pass) {

                //welcome
                if ($j("#popErrMsg").length) {
                    $j('#popErrMsg').text(err_msg);
                    $j('#10slide_img').attr('src', welcome_image_error);
                }

                //marketing
                if ($j("#popErrMsg_marketing").length) {
                    $j('#popErrMsg_marketing').text(err_msg);
                    $j('#10slide_img_marketing').attr('src', 'http://www.forever21.com/images/en/20141129/cbd-error.jpg');
                }
            }
            else {

                $j.ajax({
                    async: false,
                    type: "GET",
                    url: submiturl,
                    success: function (result) {

                        if (result == 'OK') {
                            //welcome
                            if ($j("#10slide_img").length) {
                                $j('#10slide_img').attr('src', welcome_image_ok);
                                $j('#imgBtn').hide();
                                $j('#inputEmailAddress').hide();
                            }

                            //marketing
                            if ($j("#10slide_img_marketing").length) {
                                $j('#10slide_img_marketing').attr('src', 'http://www.forever21.com/images/en/20141129/cbd-confirm.jpg');
                                $j('#imgBtn_marketing').hide();
                                $j('#inputEmailAddress_marketing').hide();
                            }

                            slideMsg = 'ok';
                            //SetAnotherCookie(); // Set Second Cookie            
                        } else if (result == 'DUPLICATED') {
                            //welcome
                            if ($j("#10slide_img").length) {
                                $j('#10slide_img').attr('src', welcome_image_duplicate);
                                $j('#imgBtn').hide();
                                $j('#inputEmailAddress').hide();
                            }

                            //marketing
                            if ($j("#10slide_img_marketing").length) {
                                $j('#10slide_img_marketing').attr('src', 'http://www.forever21.com/images/en/20141129/cbd-confirm.jpg');
                                $j('#imgBtn_marketing').hide();
                                $j('#inputEmailAddress_marketing').hide();
                            }


                            slideMsg = 'duplicated';
                            //SetAnotherCookie(); // Set Second Cookie            
                        } else if (result == 'ERROR') {
                            //err_msg = "ERROR";

                            //welcome
                            if ($j("#10slide_img").length) {
                                $j('#10slide_img').attr('src', welcome_image_error);
                                $j('#popErrMsg').text(err_msg);
                            }

                            //marketing
                            if ($j("#10slide_img_marketing").length) {
                                $j('#10slide_img_marketing').attr('src', 'http://www.forever21.com/images/en/20141129/cbd-error.jpg');
                                $j('#popErrMsg_marketing').text(err_msg);
                            }

                            slideMsg = '';
                        }

                    }
                });
            }
        }

    </script>

    <!-- BLINK TEXT -->
    <script type="text/javascript">
        $j(document).ready(function () {
            // COMMENT ALL TO DISABLE //
            //$j('div.t_freeship span').attr('id', 'blink');
            //$j('div.t_freeship span').css('color', 'red');
            //Blink();			
        });

        function Blink() {
            obj = document.getElementById("blink");
            if (obj.style.visibility == "hidden") obj.style.visibility = "visible";
            else obj.style.visibility = "hidden";
            window.setTimeout("Blink();", 500);
        }
    </script>


    <script type="text/javascript">
        $j(document).ready(function () {            		
            if ($j("#popUp").length) {
                $j('#popUp').live('click', function (e) {
                    showPopup();
                });

                $j('#popUpBody').click(function (e) {
                    e.stopPropagation();
                    return false;
                });
            }

            if ($j("#popUp_marketing").length) {
                $j('#popUp_marketing').live('click', function (e) {
                    showPopup();
                });

                $j('#popUpBody_marketing').click(function (e) {
                    e.stopPropagation();
                    return false;
                });
            }
        });

        $j('html').click(function () {
            var rightValue = '';

            if ($j("#popUp").length) {
                rightValue = $j("#popUp").css("right");
            }

            if ($j("#popUp_marketing").length) {
                rightValue = $j("#popUp_marketing").css("right");
            }

            if (rightValue == '152px') {
                closePopup();

                if ($j("#popUp").length) {
                    if (getSecondCookie("slider_w") == "S1") {
                        $j("#popUp").css("display", "none");
                    }
                }
            }
        });
			
           
            	
    </script>

	<script type="text/javascript">
	    function showPopup() {
	        //$j("#popUp").css("right", "152px");

	        // Show initially, stop, and slowly fade right
	        if ($j("#popUp").length) {
	            $j("#popUp").stop(true, true).delay(0).animate({
	                right: "152px"
	            }, { duration: 500 });
	        }

	        if ($j("#popUp_marketing").length) {
	            $j("#popUp_marketing").stop(true, true).delay(0).animate({
	                right: "152px"
	            }, { duration: 500 });
	        }
	    }

	    function closePopup() {
	        //$j("#popUp").css("right", "-397px");

	        if ($j("#popUp").length) {
	            $j("#popUp").stop(true, true).delay(0).animate({
	                right: "-397px"
	            }, { duration: 500 });
	        }

	        if ($j("#popUp_marketing").length) {
	            $j("#popUp_marketing").stop(true, true).delay(0).animate({
	                right: "-397px"
	            }, { duration: 500 });
	        }

	        $j("#imgArrow_marketing").removeAttr("onmouseover");
	        $j("#imgArrow").removeAttr("onmouseover");
	        setSecondCookie("hover", "Y1");
	    }

    </script>

    <!-- [SEARCH FADE] -->
    <script type="text/javascript">
        $j(document).ready(function () {
            // CLICK EXPAND
	        $j("#ihKeyword").click(function () {
	            $j("#divSearchBox").animate({ width: '320px' });
	            $j("#divSearchBox .m_search_inputbox").animate({ width: '320px' });
	            $j("#ihKeyword").animate({ width: '280px' });
	            $j("#divSearchBox").fadeIn();
	        });
            // FOCUS OUT
	        $j("#ihKeyword").blur(function () {
	            $j("#divSearchBox").animate({ width: '200px' });
	            $j("#divSearchBox .m_search_inputbox").animate({ width: '200px' });
	            $j("#ihKeyword").animate({ width: '160px' });
	            $j("#search_suggestion").fadeOut();
	        });	    
	    });	                   
	</script>
    
	
    
