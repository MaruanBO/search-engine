        function fnGetBasketSubTotal() {
            var url = "/Ajax/AjaxBasketUpdate.aspx?action=status";
            $j.ajax({
                type: "POST",
                url: url,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $j("#" + haveTotalID).html(response[0].SubTotal);
                    $j("#" + haveBasketID).html(response[1].Qty);
                },
                error: function (msg) {

                }
            });
        }

        $j(document).ready(function () {
            //fnGetBasketSubTotal();
        });

    </script>

