<div class="contentasset" contentid="OPTIN_OVERLAY">



<script type="text/javascript">

	jQuery(document).ready(function() {
	
		// Overlay Close Button Event
		jQuery('a#optInOverlayCloseButton').click(function(e) {
			jQuery('div#optInOverlay').hide();
			e.preventDefault();
						
			// And set the cookie so it will not show again
			var expiry = new Date();
			expiry.setDate(expiry.getDate() + 30);
			SetCookie('OptInOverlayDismissed', 'true', expiry, '/');				
		});
		
		// If the overlay has not been dismissed, show it
		if (GetCookie('OptInOverlayDismissed')!='true' && GetCookie('OptedIn')!='true') {
			jQuery('div#optInOverlay').show();
		}
	});
</script>
