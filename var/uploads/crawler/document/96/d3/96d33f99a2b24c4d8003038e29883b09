<script type="text/javascript">
function detailsOpen(objName) {
	if (objName == "coupon" || objName == "bodyCoupon"){
	 if (objName == "coupon") {var url = "/on/demandware.static/Sites-easyspirit-Site/Sites-easyspirit-Library/default/images/homepage/2014.02.19/coupon.gif";
		widthX = "616";

		heightY = "353";
		}
		if (objName == "bodyCoupon") {var url = "/on/demandware.static/Sites-easyspirit-Site/Sites-easyspirit-Library/default/images/homepage/2013.07.21/bodyCoupon.gif";
		widthX = "615";
		heightY = "415";
		}



		var newwindow = ''

		if (newwindow.location && !newwindow.closed) {
			newwindow.location.href = url;
			newwindow.focus();
			Event.observe(newwindow.window, 'load', function() { newwindow.window.print(); });
			//newwindow.window.print();
		} else {
					var myString = "width=" + widthX + ", height=" + heightY + ", resizable=1";

			newwindow=window.open(url,'newwindow', myString);
			newwindow.focus();
			Event.observe(newwindow.window, 'load', function() { newwindow.window.print(); });
		}
	} else {
		var obj = $(objName);
		if(obj != null) //obj.style.display = 'block';
		jQuery(obj).fadeIn(350);
					//if (objName !="shippingPopup") obj.style.cursor="pointer";

	}
}
function detailsClose(objName) {
	var obj = $(objName);
	if(obj != null) obj.style.display = 'none';
}

function theCoords() {jQuery('#theheaderpopup').css('padding-top','400px')}
</script>



