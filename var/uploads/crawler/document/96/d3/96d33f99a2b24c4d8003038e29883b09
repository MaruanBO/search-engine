//used to update the page_name and page_type in tealium and push to omniture
function updateOmniturePageName(_pageName,_pageType) {
//set up page type data
//ensure that regardless of how we get here, at least the checkout start will be called.
utag_data.page_name= "easy spirit" + ":checkout:checkout Info";
utag_data.page_type= "easy spirit" + ":checkout:checkout Info";
utag_data.site_section="easy spirit" + ":checkout:checkout Info";
//utag_data.page_type=_pageType;
utag.view(utag_data)
switch( _pageType) {
case "billing":
utag_data.page_name= "easy spirit" + ":checkout:billing info";
utag_data.page_type= "easy spirit" + ":checkout:billing info";
utag_data.site_section="easy spirit" + ":checkout:billing info";
break;
case "shipping":
utag_data.page_name= "easy spirit" + ":checkout:shipping info";
utag_data.page_type= "easy spirit" + ":checkout:shipping info";
utag_data.site_section= "easy spirit" + ":checkout:shipping info";
break;
case "summary":
utag_data.page_name= "easy spirit" + ":checkout:summary";
utag_data.page_type= "easy spirit" + ":checkout:summary";
utag_data.site_section= "easy spirit" + ":checkout:summary";
break;
default :
break;
}
utag_data.page_type=_pageType;
utag.view(utag_data)
}
function openQuickViewSuccess(transport) {
document.body.style.cursor='default';
var response = transport.responseText;
if(response != null && response != '') {
try{addGrayBackGround();}catch(e){jQuery('#jagGrayBG').remove();}
jQuery('#pdqv').html(response);
jQuery('#pdqv').css('display','block');
try{var picker = new skuPicker(eval($('1lookupTable').value)[0],'1','quickView',eval($('1imagesLookupTable').value)[0]);
picker.init();
qvPicker = picker;}catch(err){}
var calculatedTop = 0;
if(windowscroll) {
calculatedTop = (jQuery(window).height() - jQuery('#pdqv').outerHeight()) / 2;
} else {
calculatedTop = (jQuery(window).height() - jQuery('#pdqv').outerHeight()) / 2 + jQuery(window).scrollTop();
}
if(calculatedTop < 0) {
calculatedTop = 0;
}
if(qvCalculateTop) {
jQuery('#pdqv').css("top", calculatedTop + "px");
}
if(windowscroll) {
window.scrollTo(0,0);
}
jQuery('#evShopBag').css('display','none');
jQuery('#soldOutImgHolder').hide();
backtotopbackupstateqvopen = true;
if($('vabacktotop')) {
$('vabacktotop').style.visibility = 'hidden';
BTNShown = false;
}
}

sr_refreshMessages();

}
function closeQV(){
backtotopbackupstateqvopen = false;
if($('vabacktotop') && jQuery(window).scrollTop() > 0) {
$('vabacktotop').style.visibility = 'visible';
BTNShown = true;
}
try{removeGrayBackGround();}catch(e){jQuery('#jagGrayBG').remove();}
jQuery('#pdqv').css('display','none');
jQuery('#pdqv').html('');
if($(qvPicker.idPrefix+'isProductSoldOut') && $(qvPicker.idPrefix+'isProductSoldOut').value == 'yes') {
jQuery('#soldOutImgHolder').show();
}
}
function openINTL() {
jQuery('#intl_Container_master').css('display','block');
try{addGrayBackGroundINTL();}catch(e){jQuery('#jagGrayBG').remove();}
var calculatedTop = (jQuery(window).height() - jQuery('#intl_Container_master').outerHeight()) / 2;
if(calculatedTop < 0) {
calculatedTop = 0;
}
jQuery('#intl_Container_center').css("top", calculatedTop + "px");
jQuery('#jagGrayBG').click(function(){closeINTL();});
}
function closeINTL(){
try{removeGrayBackGroundINTL();}catch(e){jQuery('#jagGrayBG').remove();}
jQuery('#intl_Container_master').css('display','none');
jQuery('#intl_Container_master').html('');
}
