function sr_sign_out() {
jQuery.ajax({
url: "/on/demandware.store/Sites-easyspirit-Site/default/FluidCheckout-SRSignOut",
context: document.body
}).done(function() {
window.location.reload();
});
}
function sr_sign_in() {
jQuery.ajax({
url: "/on/demandware.store/Sites-easyspirit-Site/default/FluidCheckout-AssignShippingMethodToBasket",
data: { shippingMethodID: "01", token: jQuery('#ajaxToken').val() },
context: document.body
}).done(function() {
window.location.reload();
});
}
