function show_cart_link() {
   if ( $.cookie('nitems') && $.cookie('nitems') >= 1 ) {
       if ( $(window).width() < 656 ) {
           $('#brown_bar #cart_link').css('display','inline');
       }
       else {
           $('#brown_bar #cart_link').css('display','none');
       }
   }
}
$(show_cart_link);
$(window).resize(show_cart_link);
</script>




