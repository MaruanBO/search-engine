			omnitureData.productsInBasket.raw = array;
			omnitureData.productsInBasket.formatted = stringifyProductIds(array);
			data.omniture = omnitureData;
		}
	}
	function stringifyProductIds (array) {
		if( Object.prototype.toString.call( array ) === '[object Array]' && array.length > 0 ) {
			var string = '',
				formattedArray = [];

			for ( var i = 0; i < array.length; i++ ) {
				formattedArray.push(";" + array[i]);
			}

			string = formattedArray.join(",");

			return string;
		}
	}
	function updateProductsInOrder (array) {
		if( Object.prototype.toString.call( array ) === '[object Array]' && array.length > 0 ) {
			omnitureData.order.raw = array;
			omnitureData.order.formatted = stringifyProductInfos(array);
			data.omniture = omnitureData;
		}
	}
	function stringifyProductInfos (array) {
		if( Object.prototype.toString.call( array ) === '[object Array]' && array.length > 0 ) {
			var string = '',
				formattedArray = [],
				item,
				itemArray = [];

			for ( var i = 0; i < array.length; i++ ) {
				item = array[i];
				itemArray = [];
				for ( var key in item ) {
					if ( item.hasOwnProperty(key) ) {
						itemArray.push(";" + item[key]);
					}
				}
				formattedArray.push(itemArray.join(""));
			}

			string = formattedArray.join(",");

			return string;
		}
	}
	// Prepare basket data placeholder for omniture
	if ( basket.length > 0 ) {
		var productsInBasket = basket.find("tr.product-line-item"),
			totalProductsInBasket = productsInBasket.length,
			productInBasket,
			productInBasketId,
			productsInBasketArr = [],
			orderObjs = [];

		if ( totalProductsInBasket > 0 ) {
			omnitureData.order = {};
			omnitureData.productsInBasket = {};

			for ( var i = 0; i < totalProductsInBasket; i++ ) {
				productInBasket = $(productsInBasket[i]);
				productInBasketId = productInBasket.attr("data-external-id");
				productInBasketQuantity = productInBasket.attr("data-quantity");
				productInBasketTotal = productInBasket.attr("data-total").replace(/\$/g, '');
				productsInBasketArr.push(productInBasketId);

				if ( document.getElementById("summary-basket") ) {
					orderObj = {
						id : productInBasketId,
						quantity : productInBasketQuantity,
						subtotal : productInBasketTotal
					};

					orderObjs.push(orderObj);
				}
			}

			updateProductsInBasket(productsInBasketArr);
			if ( document.getElementById("summary-basket") ) {
				updateProductsInOrder(orderObjs);
			}

		}
	}

	// Output page content data
		 pageName = "Home";

	if ( hierArr.length > 0 ) {
		for ( var i = 0; i < hierArr.length; i++ ) {
			if ( i < 10 ) {
				hierTrailArr.push(hierArr[i]);
			}
			if ( i < 5 ) {
				if ( hierTrailArr.length === 0 ) {
					s["prop" + (i + 1)] =  hierArr[i];
				} else {
					s["prop" + (i + 1)] =  hierTrailArr.join(":");
				}
			}
		}
		// If product detail, use the product name as the page name instead of the default page name "Detail"
		if ( body.is(".product-detail") ) {
			hierTrailArr.push('');
		} else {
			hierTrailArr.push(pageName);
		}
	} else {
		s.prop1 = pageName;
	}

	s.hier1 = ( hierArr.length > 0 ) ? hierArr.join(":") : pageName;

	s.pageName = ( hierTrailArr.length > 0 ) ? hierTrailArr.join(":") /*+ ":" + pageName*/ : pageName;

	if ( hierArr.length > 0 ) {
		s.prop2 = s.prop2 ? s.prop2 : s.prop1;
		s.prop3 = s.prop3 ? s.prop3 : s.prop2;
		s.prop4 = s.prop4 ? s.prop4 : s.prop3;
	} else {
		s.prop2 = s.prop2 ? s.prop2 : s.pageName;
		s.prop3 = s.prop3 ? s.prop3 : s.pageName;
		s.prop4 = s.prop4 ? s.prop4 : s.pageName;
	}


	// Outout Products viewed
	if ( body.is(".product-detail") || body.is(".product-list") || body.is(".configurable-meal") ) {
		var productsArr = [],
			products,
			totalProducts,
			product;

		/*
		NOTE: Somehow Omniture returns s.events = "prodViewundefinedevent1undefinedevent"
		*/
		s.events = "prodView";

		if ( body.is(".configurable-meal") ) {
			var bundleArticle = $("#configurable-meal"),
				activeProduct;

			if ( bundleArticle.is(".bundle-detail") ) {
				activeProduct = $("#configurable-meal").find("input[name='bundleOptions']").filter(":checked").val();
				s.products = ";" + activeProduct;
			} else {
				products = $("#configurable-meal").find("input[name='bundleOptions']");
				totalProducts = products.length;

				for ( var i = 0; i < totalProducts; i++ ) {
					product = $(products[i]);
					productsArr.push(";" + product.val());
				}
				s.products = productsArr.join(",");
			}
		} else if ( body.is(".product-detail") ) {
			s.products = ";" + $("#uxProductid").val();
		} else if ( body.is(".product-list") ) {
			products = $("#configurable-meal").find("a.bundle-item-image, a.bucket-item-image");
			totalProducts = products.length;

			for ( var i = 0; i < totalProducts; i++ ) {
				product = $(products[i]);
				productsArr.push(";" + product.attr("data-product-external-id"));
			}
			s.products = productsArr.join(",");
		}
	}

	// Events
	var eventObject = {};

	body.on("omniture.quickview", function (e, productExternalId) {

	});

	body.on("omniture.addtocart", function (e) {
		//s.events = s.apl(s.events,'scAdd', ',', 2);
		s.events = "scAdd";
		s.products = s.products;
		s.linkTrackVars = "events,products";
		s.linkTrackEvents = "scAdd";
		s.tl(true, 'o', 'Cart addition');
		//s.t(); // triggers image request
	});

	body.on("omniture.cartremove", function (e, sequenceNo) {
		var productsInBasket = body.data("omniture").productsInBasket.raw,
			updatedProductsInBasket = productsInBasket.splice( sequenceNo, 1);

		updateProductsInBasket(productsInBasket);
		s.events = "scRemove";
		s.products = updatedProductsInBasket.toString();
		s.linkTrackVars = "events,products";
		s.linkTrackEvents = "scRemove";
		s.tl(true, 'o', 'Cart edit');
		//s.t(); // triggers image request
	});

	body.on('omniture.findmystore', function (e, keyword) {
		s.linkTrackVars = 'eVar9,events';
		s.linkTrackEvents = 'event4';
		s.events = 'event4';
		s.eVar9 = keyword;
		s.tl(true, 'o', 'Find my store');
	});

	// Checkout
	if ( body.is(".checkout") ) {
		if ( body.is(".fullbasket") ) {
			s.events = "scView";
			if ( body.data("omniture").productsInBasket ) {
				s.products = body.data("omniture").productsInBasket.formatted;
			}
		}
		if ( body.is(".pickup") ){
			s.events = "scCheckout";
			if ( body.data("omniture").productsInBasket ) {
				s.products = body.data("omniture").productsInBasket.formatted;
			}
		}
		if ( body.is(".receipt") ) {
			s.events = "purchase,event10";
			if ( body.data("omniture").productsInBasket ) {
				s.products = body.data("omniture").order.formatted;
			}
			s.purchaseID = '';
			s.zip = '';
			s.state = '';
		}

		s.prop2 = s.prop2 ? s.pageName : s.pageName;
		s.prop3 = s.prop3 ? s.prop2 : s.pageName;
		s.prop4 = s.prop4 ? s.prop3 : s.pageName;
		s.hier1 = s.hier1 ? s.prop4 : pageName;
	}

	// Search
	if ( body.is(".search-result") ) {
		var searchMode = '';

		s.eVar3 = '';
		s.eVar4 = '';
		s.eVar5 = '';
		s.events = ( searchMode === "content" ) ? "event7" : "";
	}

	s.server = 'Server2';

	s.channel="";
	s.pageType="";
	/* Conversion Variables */
	/*
	s.campaign="";
	s.state="";
	s.zip="";
	s.purchaseID="";
	s.eVar1="";
	s.eVar2="";
	*/
