    <script>
    if ($(window).width() > 768) {
        $('.perfect-fold').css('height', $(window).height() - 90 + 'px');

        var message = $('#call-to-action .message');
        message.css({
            'margin-left': '-' + message.width() / 2 + 'px',
            'margin-top': '-' + message.height() / 2 + 'px',
        });
    } else {
        $('.perfect-fold').css('height', 'auto');

        var message = $('#call-to-action .message');
        message.css({
            'margin-left': 0,
            'margin-top': 0
        });
    }
    </script>

