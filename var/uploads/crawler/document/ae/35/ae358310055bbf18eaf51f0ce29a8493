<script>
$(document).ready(function(){
	$('#main_menu li a').click(function(){
		var log = '/log.txt';
		var contents = '';
		var current = $(this).text().replace(">","");
		var current_page = "Home";
		if( current_page == "" ){ current_page = "Newsletter"; }
		var parent = $(this).parents('ul').parents('li');
		var the_parent = $(parent[0]).find('> a').text().replace(">","");
		var grandparent = $(this).parents('ul').parents('li').parents('li');
		var the_grandparent = $(grandparent[0]).find('> a').text().replace(">","");
		var greatgrandparent = $(this).parents('ul').parents('li').parents('li').parents('li');
		var the_greatgrandparent = $(greatgrandparent[0]).find('> a').text().replace(">","");
		var cloc = "Top Nav";
		if( the_greatgrandparent.length > 1 ){ contents = contents+" / "+the_greatgrandparent; }
		if( the_grandparent.length > 1 ){ contents = contents+" / "+the_grandparent; }
		if( the_parent.length > 1 ){ contents = contents+" / "+the_parent; }
		contents = contents+" / "+current;
		_gaq.push(['_trackEvent', 'Navigation', cloc, contents+' (from '+current_page+')']);
	});
});
</script>
