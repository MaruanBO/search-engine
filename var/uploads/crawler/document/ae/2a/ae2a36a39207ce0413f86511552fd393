					<div class="dropdown-brand-list col-xs-10 col-sm-9 col-med-9">
						<select id="model-search-dropdown" class="front-page-selector dropdown-hidden" style="width: 100%"></select>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript">
	jQuery(document).ready(function () {
		// The main, original menu adds the brand menu
		$jQueryModern('#brand-selector').on("change", function(e) {
			$jQueryModern('.dropdown-hidden').fadeOut('fast'); 
			
			if (isNaN($jQueryModern(this).val())) {
				$jQueryModern("#" + $jQueryModern(this).val()).css('display','block');
			} else { 	// Brands without schematics go directly to the page
				VPWfunctions.goVPW(this.options[this.selectedIndex].value, '688943');
			}
		});
		
		// Any of the second level menus with schematics add dropdowns with subcategories
		$jQueryModern('.second-level-selector').not('.brand-no-schematic').not('#Kirby').not('#Rainbow').not('#FilterQueen').on("change", function(e) {
			VPWfunctions.addDropdown(this.options[this.selectedIndex].value);
		});
		
		// The third, model search menu goes directly to the schematic page
		$jQueryModern('#model-search-dropdown, #Kirby, #Rainbow, #FilterQueen').on("change", function(e) {
			VPWfunctions.goVPW(this.options[this.selectedIndex].value, '688943');
		});
		
		$jQueryModern('.category-buttons-first a').each(function() { 
			$jQueryModern(this).on("click", function() {
				ga('send', 'event', $jQueryModern(this).attr('title'), 'Homepage Buttons', location.href);
			})
		});
		
	});
	</script>
</div>
