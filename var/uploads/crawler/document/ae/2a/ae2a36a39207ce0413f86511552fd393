            if (select.type == "TOP_SEARCH_QUERIES"){
                window.location = "//find.vacpartswarehouse.com?q=" + encodeURIComponent(select.value)
            }
            else if(select.type == "KEYWORD_SUGGESTION"){
                var modelId = select.value.substr(0,select.value.indexOf(' '));
                var modelName = select.value.substr(select.value.indexOf(' ')+1);
                var modelUrl = '/'+ modelName.replace(" ","-") +"-s/"+ modelId + ".htm";
                console.log('Model Url ', modelUrl);
                window.location = modelUrl;
            }
            else if (select.type == "POPULAR_PRODUCTS"){
            console.log('path ',obj.productUrl);
            window.location =  obj.productUrl;
            }
        }
