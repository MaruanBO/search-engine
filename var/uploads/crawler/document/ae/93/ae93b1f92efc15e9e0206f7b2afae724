    if (!Array.prototype.indexOf) {
        Array.prototype.indexOf = function (a, b) {
            var c = this.length >>> 0;
            b = +b || 0;
            if (Math.abs(b) === Infinity) {
                b = 0
            }
            if (b < 0) {
                b += c;
                if (b < 0) {
                    b = 0
                }
            }
            for (; b < c; b++) {
                if (this[b] === a) {
                    return b
                }
            }
            return -1
        }
    }
