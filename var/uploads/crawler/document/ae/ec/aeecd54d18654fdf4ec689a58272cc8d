	if(typeof (utag_data) != 'undefined' && typeof (utag_data.product_skuz) == 'undefined'){
		var itemInCartCookieVal = getCookie("WC_XITEMINCART");
		if(null != itemInCartCookieVal && 'null' != itemInCartCookieVal && itemInCartCookieVal != ""){
			utag_data.product_skuz=itemInCartCookieVal.split("|");
		}
		
		var itemInCartCookieQtyVal = getCookie("WC_XITEMQTYINCART");
		if(null != itemInCartCookieQtyVal && 'null' != itemInCartCookieQtyVal && itemInCartCookieQtyVal != ""){
			utag_data.product_quantityz=itemInCartCookieQtyVal.split("|");
		}
	}


	var saved_data = $.extend(true, {}, utag_data);

	var DMTMMapCookie = getCookie("DMTMMapCookieValue");
	if (typeof (DMTMMapCookie) != 'undefined' && DMTMMapCookie != '') {
		deleteCookie("DMTMMapCookieData");
		deleteCookie("DMTMMapCookieValue");
		DMTMMapCookie = $.parseJSON(DMTMMapCookie);
		for (var value in DMTMMapCookie) {
			utag_data[value] = DMTMMapCookie[value];
		}
	}
