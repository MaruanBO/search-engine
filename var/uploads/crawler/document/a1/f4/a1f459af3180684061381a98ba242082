<script type="text/javascript" charset="utf-8">

	function executeGetBestSellers(cache) {
		
		var sortId = $F('bestSellersSort');
		
		if(cache[sortId]) {
			updateBestSellersContainer(cache[sortId]);
		}
		else {
			new Ajax.Request('/index.cfm?event=products.best.sellers', {
			  method: 'get',
			  parameters: {	bestSellersSort: sortId, pCode: '/WHM' },
				onSuccess: function(transport) {
					updateBestSellersContainer(transport.responseText);
					cache[sortId] = transport.responseText;
				},
				onFailure: function(transport) {
				  $('bestSellersGridContent').update(new Element('div', {'style': 'width: 380px; height: 270px; padding: 10px;'}).update(new Element('p', { 'class': 'error'}).update('ERROR LOADING...')));
      		$('loadingBestSellersContainer').hide();
      		$('bestSellersGridContent').setOpacity(1.0);
				}
			});
		}
	}
	
	function updateBestSellersContainer(html) {
		$('bestSellersGridContent').update(html);
		$('loadingBestSellersContainer').hide();
		$('bestSellersGridContent').setOpacity(1.0);
		//$('bestSellersGrid').show();
	}

	document.observe('dom:loaded', function(){
		//trying to implement a very simple cache
		var bestSellersCache = new Object;
		
		$('bestSellersSort').observe('change', function (e) {
			//$('bestSellersGrid').hide();
			$('bestSellersGridContent').setOpacity(0.25);
			$('loadingBestSellersContainer').show();
			executeGetBestSellers(bestSellersCache);
		});
	});
</script> 
