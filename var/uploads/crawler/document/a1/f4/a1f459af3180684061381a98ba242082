<script type="text/javascript" charset="utf-8">

	function executeGetJustAdded(cache) {
		
		var sortId = $F('justAddedSort');
		
		if(cache[sortId]) {
			updatejustAddedGrid(cache[sortId]);
		}
		else {
			new Ajax.Request('/index.cfm?event=products.just.added', {
			  method: 'get',
			  parameters: {	justAddedSort: sortId, pCode: '/WHM' },
				onSuccess: function(transport) {
					updatejustAddedGrid(transport.responseText);
					cache[sortId] = transport.responseText;
				},
				onFailure: function(transport) {
					updatejustAddedGrid(new Element('div', {'style': 'padding: 18px;'}).update(new Element('p', { 'class': 'error'}).update('ERROR LOADING...')));
				}
			});
		}
	}
	
	function updatejustAddedGrid(html) {
		$('justAddedGridContent').update(html);
		$('justAddedGrid').undoPositioned();
		$('loadingProductsContainer').hide();
		$('justAddedGridContent').setOpacity(1.0);
	}

	document.observe('dom:loaded', function(){
		//trying to implement a very simple cache
		var justAddedCache = new Object;
		
		$('justAddedSort').observe('change', function (e) {
			$('justAddedGridContent').setOpacity(0.25);
			$('justAddedGrid').makePositioned();
			$('loadingProductsContainer').show();
			executeGetJustAdded(justAddedCache);
		});
	});
</script> 
