<script type="text/javascript" src="js/validation.js"></script>
<script type="text/javascript">
$(document).on("change","#program_subject",function(){
    $("#concentration").css("display","block");
	this_id = $(this).attr('id');
	 $.ajax({
             //alert('test');
			type: 'post',
			url: "controller.php",
			data: this_id + '=' + $(this).val() + '&getoptions=true',
			success: function (msg) {			
				$("#college_concentrations").html("<option value=''>Select One</option>" + msg) ;
			}
	
	});
});
</script>


