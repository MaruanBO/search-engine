<script type="text/javascript">
$(document).on("change","#ps",function(){
    
	this_id = $(this).attr('id');
	 $.ajax({
			type: 'post',
			url: "controller.php",
			data: 'program_subject_id=' + $(this).val() + '&getoptions=true',
			success: function (msg) {			
				$('#college_concentrations_id1').removeAttr('disabled');
				$("#college_concentrations_id1").html("<option value=''>Select Concentration</option>" + msg) ;
				document.getElementById("college_concentrations_id1").style.color= '#7A7A7A';
			}
	
	});
});

function removeValues()
{
	if($('#zip').val()=='Country or State') {
		$('#zip').val('');
	}
	if($('#zip1').val()=='Zip code') {
		$('#zip1').val('');
	}
	return true;
}
</script>
