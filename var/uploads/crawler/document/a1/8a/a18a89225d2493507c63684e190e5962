<script type="text/javascript">
// Script to position the beauty shot in center of screen
alignSlides = window.onresize = function(){
  var image = $j('.hp-beauty-shot'), page = $j('#HP_TopLevel_Div'), dots = $j('.slideControlSmallOuter');
  if (image.width() > page.width()) {
    image.css('margin-left','-' + (image.width()-page.width())/2 + 'px');
  }
  else {
    image.css('margin-left','0px');
  }
}
$j(document).ready(function(){alignSlides();});
</script>

