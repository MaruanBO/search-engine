<!-- scripts for selector window mock-up -->

    <script type="text/javascript">
    <!--

	    // to make window navs change classes

	    var lastFeature = 0;
	    function selectFeatured(id){
		if (lastFeature > 0) {
		    document.getElementById('selector' + lastFeature).style.visibility="hidden";
		    document.getElementById('selectorbtn' + lastFeature).className = "hmslctbtn";
		    document.getElementById('hmslctarw' + lastFeature).className = "hmslctarw";
		}

		document.getElementById('selector' + id).style.visibility="visible";
		document.getElementById('selectorbtn' + id).className = "hmslctbtnon";
		document.getElementById('hmslctarw' + id).className = "hmslctarwon";
		lastFeature = id;
	    }
		function closeFeatured(id){
		document.getElementById('selector' + id).style.visibility="hidden";
		document.getElementById('selectorbtn' + id).className = "hmslctbtn";
		document.getElementById('hmslctarw' + id).className = "hmslctarw";
		}

		var lastGroupA = 0;
		function changeGroupA(id){
		if (lastGroupA > 0) {
		    if (document.getElementById('groupa' + lastGroupA)) document.getElementById('groupa' + lastGroupA).className = "hmwinbtn hmwinbtnoff";
		}
		if (document.getElementById('groupa' + id)) document.getElementById('groupa' + id).className = "hmwinbtn hmwinbtnon";
		lastGroupA = id;
		}

		var lastGroupB = 0;
		function changeGroupB(id){
		if (lastGroupB > 0) {
		    if (document.getElementById('groupb' + lastGroupB)) document.getElementById('groupb' + lastGroupB).className = "hmwinbtn hmwinbtnoff";
		}
		if (document.getElementById('groupb' + id)) document.getElementById('groupb' + id).className = "hmwinbtn hmwinbtnon";
		lastGroupB = id;
		}

		var lastGroupC = 0;
		function changeGroupC(id){
		if (lastGroupC > 0) {
		    if (document.getElementById('groupc' + lastGroupC)) document.getElementById('groupc' + lastGroupC).className = "hmwinbtn hmwinbtnoff";
		}
		if (document.getElementById('groupc' + id)) document.getElementById('groupc' + id).className = "hmwinbtn hmwinbtnon";
		lastGroupC = id;
		}


		// ajax content to simulate scroll effect in the top selling tab

		var rootdomain="http://"+window.location.hostname

		function display(url, containerid){
			var page_request = false
			if (window.XMLHttpRequest) // if Mozilla, Safari etc
				page_request = new XMLHttpRequest()
			else if (window.ActiveXObject){ // if IE
			try {
				page_request = new ActiveXObject("Msxml2.XMLHTTP")
				}
			catch (e){
				try{
					page_request = new ActiveXObject("Microsoft.XMLHTTP")
				}
				catch (e){}
				}
			}
			else
			return false

			page_request.onreadystatechange=function(){
				if(page_request.readyState==4 && page_request.responseText) {
					if (page_request.responseText.length > 0) {
						loadpage(page_request, containerid)
					}
				}
			}

			page_request.open('GET', url, true)
			page_request.send(null)

		}

		function loadpage(page_request, containerid){
			if (page_request.readyState == 4 && (page_request.status==200 || window.location.href.indexOf("http")==-1))
			    if (document.getElementById(containerid)) document.getElementById(containerid).innerHTML = page_request.responseText;

		}


		// onload stuff

		window.onload=function(){
		changeGroupA(2);
		changeGroupB(3);
		changeGroupC(4);

		}

//-->
</script>

<!-- end scripts for mock-up -->

