<script src="https://mardel.idevsearch.net/includes/idevAutoSuggest.min.js" type="text/javascript"></script>
<script type="text/javascript" language="javascript">

    if (window.addEventListener) {
        window.addEventListener('load', idevInitAutoSuggest, false);
    } else if (window.attachEvent) {
        window.attachEvent('onload', idevInitAutoSuggest);
    }
    function idevInitAutoSuggest() {
        divWidth = '280px';
        idevInitQueryCode('ctl02_txtSearch', 'https://mardel.idevsearch.net/Autosuggest.aspx?f=GetSuggestions&df=Search', '');
    }

    function hideRecentlyViewed() {
        if (document.getElementById('divScreen'))
            document.getElementById('divScreen').style.display = 'none';

        if (typeof document.body.style.maxHeight == "undefined" && document.getElementById('ctrlRecipientsDropDown_drpRecipient'))
            document.getElementById('ctrlRecipientsDropDown_drpRecipient').style.visibility = 'visible';

        ypSlideOutCart.hideAll();
    }

    function processRecentlyViewed(s) {
        document.getElementById('divRecentlyViewed').innerHTML = s;
    }

    function loadingRecentlyViewed() {
        if (document.getElementById('divRecentlyViewed').offsetHeight && document.getElementById('divRecentlyViewed').offsetHeight > 1)
            document.getElementById('divRecentlyViewed').innerHTML = '<div style="height:' + (document.getElementById('divRecentlyViewed').offsetHeight + 4) + 'px;"><div style="margin-top:' + ((document.getElementById('divRecentlyViewed').offsetHeight / 2) - 25) + 'px;text-align:center;"><p style="margin-top:13px;color:#666666;">Loading...</p><p><img src="/images/loading.gif" /><br /></p></div></div>';
        else
            document.getElementById('divRecentlyViewed').innerHTML = '<div style="text-align:center"><p style="margin-top:13px;color:#666666;">Loading...</p><p><img src="/images/loading.gif" /><br /></p></div>';
    }

    function showFullCart() {
        document.getElementById('divMerchSubtotal').style.display = 'none';
        if (document.getElementById('divScreen') && document.getElementById('divSiteWrapper') && document.getElementById('divSiteWrapper').offsetHeight) {
            document.getElementById('divScreen').style.height = document.getElementById('divSiteWrapper').offsetHeight + 'px';
            document.getElementById('divScreen').style.display = 'block';
        }

        if (typeof document.body.style.maxHeight == "undefined" && document.getElementById('ctrlRecipientsDropDown_drpRecipient'))
            document.getElementById('ctrlRecipientsDropDown_drpRecipient').style.visibility = 'hidden';

        enqueue('/includes/ajax.aspx?F=GetShoppingCart', processFullCart, 'loadingFullCart()');
        ypSlideOutCart.showCart('livefullcart');
    }

    function loadingFullCart() {
        document.getElementById('divFullCart').innerHTML = '<table border="0" style="width:100%;height:300px;"><tr><td><div style="text-align:center"><p style="margin-top:13px;color:#666666;">Loading Shopping Cart...</p><p><img src="/images/loading.gif" /><br /></p></div></td></tr></table>';
    }

    function processFullCart(s) {
        var a = s.split('|');
        if (parseInt(a[2]) && parseInt(a[2]) > 4) {
            window.location = "/store/cart.aspx";
            return;
        }
        document.getElementById('divFullCart').innerHTML = a[0];
        document.getElementById('MerchSubtotal').innerHTML = a[1];
        document.getElementById('divMerchSubtotal').style.display = 'block';
    }
    //-->
</script>

