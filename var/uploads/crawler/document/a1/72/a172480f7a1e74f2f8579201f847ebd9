$(document).ready(function() {
	$("form.validated_form").validate();
	
if($('#shop_by_category_extern').length) {
  $("#shop_by_category_extern").css("margin-top", "20px").css("z-index", "3000");
  $("#shop_by_category_extern").load('/nav/renderNav/', function() {
		$("#nav_item_list .top_item").hoverIntent({
			over: function() {
				if($(this).children('.sub_menu').size() > 0) {
					var column_buffer = 242;

					var $list = $("#nav_item_list");
					var $nav_item = $(this);
					var $sub_menu = $(this).children('.sub_menu:first');

					var nav_top = $list.position().top - 1;
					var nav_right = $nav_item.width() + $nav_item.position().left;

					var column_count = $nav_item.children('.sub_menu').find('.column').size();

					var width = column_count * column_buffer;

					$sub_menu.show()
						.css('z-index', 5000)
						.css('top', nav_top)
						.css('left', 0)
						.css('position', 'absolute')
						.css('width', width);

					var sub_top = $sub_menu.position().top;
					var sub_height = $sub_menu.height();
					var sub_bottom = $sub_menu.position().top + $sub_menu.height();

					if(sub_height < $list.height()) {
						if(sub_bottom < ($nav_item.position().top + $nav_item.height())) {
							if(($list.height() - $nav_item.position().top) > sub_height) {
								$sub_menu.css('top', $nav_item.position().top - 1);
							} else {
								$sub_menu.css('top', $list.height() - sub_height - 2);
							}
						}
					}
				}
			},
			timeout: 60,
			out: function() {
				$(this).children('.sub_menu').hide();
			}
		});

		$('a.sub_menu_cat').each(function() {
			if("" == $(this).text()) {
				$(this).remove();
			} else if("" == $(this).attr('href')) {
				var $span = $(document.createElement($span));
				$span.attr('class', $(this).attr('class')).text($(this).text());
				$(this).replaceWith($span);
			}
		});

		$(".moreClick > a").each(function() {
			if("" == $(this).attr('href')) {
				$(this).parent().parent().remove();
			}
		});

			});
}

$(function() {
  $.ui.autocomplete.prototype._renderItem = function(ul, item) {
    var re, t;
    re = new RegExp("^" + this.term);
    t = item.label.replace(re, "<span class='search_term'>" + this.term + "</span>");
    return $("<li></li>").data("item.autocomplete", item).append("<a>" + t + "</a>").appendTo(ul);
  };
  return $("#search_field").autocomplete({
    source: function(request, response) {
      return $.ajax({
        url: "/suggestions/",
          dataType: "jsonp",
          data: {
            term: request.term
          },
          success: function(data) {
            return response($.map(data, function(item) {
              return {
                suffix: item.text.substr(request.term.length),
                  label: item.text,
                  value: item.text
              };
            }));
          }
      });
    },
      minLength: 1,
      select: function(event, ui) {
        $("input#search_field").val(ui.item.value);
        return $("#search_button").click();
      }
  });
});
});
</script>
