<script type="text/javascript">
$(document).ready(function() {
	$("ul.product_carousel").show().jcarousel({
		scroll: 6
	});

	$(".full_accessories").hide();

	$("a.show_all_acc").toggle(
		function() {
			var $container = $(this).parent().parent().parent();
			$container.find('.carousel_wrap').hide();
			$container.find('.full_accessories').show();
			$(this).text('See Less');
			return false;
		},
		function() {
			var $container = $(this).parent().parent().parent();
			$container.find('.carousel_wrap').show();
			$container.find('.full_accessories').hide();
			$(this).text('See More');
			return false;
		}
	);

	$('#carousel_8a3a852c8d3837fd5658274a21c607b5bc8260dd a, #full_list_8a3a852c8d3837fd5658274a21c607b5bc8260dd a').click(function() {
		var product_id = $(this).parents('.accessory').find('input[name="product_id[]"]').val();
		var preview_url = '/product/preview/' + product_id + '?hash=8a3a852c8d3837fd5658274a21c607b5bc8260dd';
		$.popup(preview_url, {
			height: '280',
			width: '440',
			top: '20'
		});
		return false;
	});
});
</script>
