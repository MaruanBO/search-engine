                        <div class="ad-block ad-block-300x600 has-bg">
        <div class="ad-block-inner">
                                                  <div id="div-gpt-ad300x600-pos2"></div>
                    



      

    

  <script>
/* 
 * Site: outside.home
 * adSiteSection: 
 * Page: landing
 * env: 
 * AdZone: outside.home
 * Ad Sec: 
 * Ad size map: false
 * Size: [300, 600]
*/

// Set adPos & tile
  var sizeStr = [300, 600].join("x").replace(/,/g,"x"); // joins single or multiple array values //
  window["pos"+sizeStr] = window["pos"+sizeStr] || 1;
  window["adPosdivgptad300x600pos2"] = window["pos"+sizeStr]++;
  window["adTile"] = window["adTile"] || 1;
  window["tiledivgptad300x600pos2"] = window["adTile"]++;

  googletag.cmd.push(function() {
    window["div-gpt-ad300x600-pos2"] = googletag.defineSlot('/299403/outside.home', [300, 600], "div-gpt-ad300x600-pos2").addService(googletag.pubads()); 
                      // Set targeting
          window["div-gpt-ad300x600-pos2"].setTargeting("page", ["landing"]);
        // Set pos & tile
        window["div-gpt-ad300x600-pos2"].setTargeting("pos", [window["adPosdivgptad300x600pos2"]]);
        window["div-gpt-ad300x600-pos2"].setTargeting("tile", [window["tiledivgptad300x600pos2"]]);
    document.getElementById("div-gpt-ad300x600-pos2").setAttribute("data-pos",window["adPosdivgptad300x600pos2"]);
    document.getElementById("div-gpt-ad300x600-pos2").setAttribute("data-tile",window["tiledivgptad300x600pos2"]);
    // Display Ad
    googletag.enableServices();
    googletag.display("div-gpt-ad300x600-pos2");
      });
</script>


        </div> <!-- /.ad-block-inner -->
      </div> <!-- /.ad-block -->
      </div> <!-- /.group-ad-bundle -->
                                                            <script>
    // Ad Tile Count
    var AdTileCount = 5,
        AdPos = 0;

    // Randomized Rendering of Bundles
    function renderBundle(obj) {

      var items = obj.items,
          trackEvent = obj.title == "Outside Exclusive" ? "exclusives" : (obj.title == "Sponsored Content" ? "nativebundle" : "partnersbundle" );
      
      for (var i = 0; i < obj.maxItems; i++) {

        // Randomize item selection
        var randomIndex = ~~(Math.random() * items.length),
            item = items[randomIndex],
            bundleClass = obj.maxItems == 1 ? " full-width" : "";

        // Print Item
        document.write('<div class="bundle pos'+(i+1)+bundleClass+'">');
        document.write('  <div class="bundle-item bundle-title-block">');
        document.write('    <img src="http://media.outsideonline.com/designimages/spacer.gif" class="bundle-img">');
        document.write('    <div class="bundle-content">');
        document.write('      <h3 class="bundle-section"><span>'+obj.title+'</span></h3>');
        document.write('      <h2 class="bundle-title">'+item.title+'</h2>');
                if (item.adSponsorshipID) {
          AdTileCount++;
          AdPos++;
          // Render Ad 
          document.write('      <div class="sponsored-by centered"><p>Sponsored By</p>');
          if (typeof ord=='undefined') {ord=Math.random()*10000000000000000;}
          document.write('        <div><script language="JavaScript" src="http://ad.doubleclick.net/adj/outside.home;page=landing;tile='+AdTileCount+';pos='+AdPos+';sz=76x19;sec='+item.adSec+';ord='+ord+'?" type="text/javascript"><\/script></div>');
          document.write('      </div>');
        }
                document.write('    </div> <!-- /.bundle-content -->');
        document.write('  </div> <!-- /.bundle-item -->');
        document.write('  <div class="bundle-item">');
        document.write('    <a href="'+item.link+'" onclick=\'javascript:subNavTrack("homepage", "'+trackEvent+'", this)\' class="bundle-link">');
        // Image
        if (item.imgUrl) {
          document.write('      <img src="http://media.outsideonline.com/designimages/spacer.gif" data-original="'+item.imgUrl+'" data-horiz="'+item.imgHorzUrl+'" class="bundle-img lazy">');
        }
        document.write('      <div class="bundle-content">');
        document.write('        <h3 class="bundle-title"><span>'+item.dek+'</span></h3>');
        document.write('      </div> <!-- /.bundle-content -->');
        document.write('    </a> <!-- /.bundle-link -->');
        document.write('  </div> <!-- /.bundle-item -->');
        document.write('</div> <!-- /.bundle -->');
        
        // remove item
        items.splice(randomIndex, 1);
      };
    }
    </script>
                                <section class="home-section section-bundle section-bundle-sp section-bundle-sp-exclusive  sp-1up ">
                <script>
        var exclusives = {
          "title" : "Outside Exclusive",
          "maxItems" :  1 ,
                    
          "items" : [
                                  {
              "title" : "Fitness",
                                                                                                                                          "adSec" : "Zero-to-Hero-2015",
              "adSponsorshipID" : "1234567",
                                                                                                                          // img found                                                 
              "imgUrl" : "http://cf-resrc.outsideonline.com/C=W100P,H100.0P/S=W1200,U/O=90,P/http://media.outsideonline.com/images/zero-to-hero-comic-lead-slp.jpg",
                                // img horz found                                                 
              "imgHorzUrl" : "http://cf-resrc.outsideonline.com/C=W77.99999999999999P,H100P/S=H250,U/O=90,P/http://media.outsideonline.com/images/zero-to-hero-comic-lead-slp.jpg",
                            "link" : "http://www.outsideonline.com/fitness/agility-and-balance/Zero-to-Hero-2015.html",
              "dek" : "The 2015 Zero to Hero Project"            }
                    ]
        };
        renderBundle(exclusives);
