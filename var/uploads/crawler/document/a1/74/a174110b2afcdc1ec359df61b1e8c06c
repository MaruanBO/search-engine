        <div class="ad-block-inner">
                                                  <div id="div-gpt-ad300x600-pos1"></div>
                    



      

    

  <script>
/* 
 * Site: outside.home
 * adSiteSection: 
 * Page: landing
 * env: 
 * AdZone: outside.home
 * Ad Sec: 
 * Ad size map: false
 * Size: [300, 600]
*/

// Set adPos & tile
  var sizeStr = [300, 600].join("x").replace(/,/g,"x"); // joins single or multiple array values //
  window["pos"+sizeStr] = window["pos"+sizeStr] || 1;
  window["adPosdivgptad300x600pos1"] = window["pos"+sizeStr]++;
  window["adTile"] = window["adTile"] || 1;
  window["tiledivgptad300x600pos1"] = window["adTile"]++;

  googletag.cmd.push(function() {
    window["div-gpt-ad300x600-pos1"] = googletag.defineSlot('/299403/outside.home', [300, 600], "div-gpt-ad300x600-pos1").addService(googletag.pubads()); 
                      // Set targeting
          window["div-gpt-ad300x600-pos1"].setTargeting("page", ["landing"]);
        // Set pos & tile
        window["div-gpt-ad300x600-pos1"].setTargeting("pos", [window["adPosdivgptad300x600pos1"]]);
        window["div-gpt-ad300x600-pos1"].setTargeting("tile", [window["tiledivgptad300x600pos1"]]);
    document.getElementById("div-gpt-ad300x600-pos1").setAttribute("data-pos",window["adPosdivgptad300x600pos1"]);
    document.getElementById("div-gpt-ad300x600-pos1").setAttribute("data-tile",window["tiledivgptad300x600pos1"]);
    // Display Ad
    googletag.enableServices();
    googletag.display("div-gpt-ad300x600-pos1");
      });
</script>


