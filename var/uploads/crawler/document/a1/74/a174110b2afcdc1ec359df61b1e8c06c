  function subNavTrack(cat, top, aObj) {
      var link = aObj.href.indexOf('outsideonline.com') != -1 ? aObj.href.split('outsideonline.com')[1] : aObj.href ;
      if (window.ga) {
        ga('send', 'event', cat, top, link);
      } else if (window._gaq) {
        _gaq.push(['_trackEvent', cat, top, link]);
      }
  }
