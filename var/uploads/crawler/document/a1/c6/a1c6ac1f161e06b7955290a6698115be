<script>
	

	var loaderData = {};
	function initLoader(){
		var tmpEl = document.getElementById('LoaderDiv');
		// Attaching events
		if (typeof tmpEl.getElementsByClassName!='function') {
			tmpEl.getElementsByClassName = function() {
				var elms = tmpEl.getElementsByTagName('*');
				var ei = new Array();
				for (i=0;i<elms.length;i++) {
					if (elms[i].getAttribute('class')) {
							ecl = elms[i].getAttribute('class').split(' ');
							for (j=0;j<ecl.length;j++) {
								if (ecl[j].toLowerCase() == arguments[0].toLowerCase()) {
									ei.push(elms[i]);
								}
							}
					} else if (elms[i].className) {
							ecl = elms[i].className.split(' ');
							for (j=0;j<ecl.length;j++) {
								if (ecl[j].toLowerCase() == arguments[0].toLowerCase()) {
									ei.push(elms[i]);
								}
							}
					}
				}
				return ei;
			}
		}
	
		
		var loader = tmpEl.getElementsByClassName('buttonClick');
		for(var x =0; x<loader.length; x++){
			eval("loader["+x+"].onmouseover = function(){loaderChangeSlide("+x+")}");
			eval("loader["+x+"].onclick = function(){loaderClickSlide("+x+")};");
		} 
		
		// Getting & Parsing XML data
		if (window.XMLHttpRequest)
			xmlhttp=new XMLHttpRequest();
		else
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  
		xmlhttp.open("GET","/xml/feeds/specialgamefeed.xml",false);
		xmlhttp.send();
		var xmlDoc=xmlhttp.responseXML;

		 var games = xmlDoc.getElementsByTagName('game');
	
		 for(var x=0; x<7; x++){
			
			var gameName  = games[x].getElementsByTagName('title')[0].childNodes[0].nodeValue;
			var gameLink  = games[x].getElementsByTagName('link')[0].childNodes[0].nodeValue;
			var gameImage = games[x].getElementsByTagName('image')[0].childNodes[0].nodeValue.replace('http://arcadecandy.com/', 'http://aa.gv.facdn.com/ac/');
			var gameDesc  = games[x].getElementsByTagName('description')[0].childNodes[0].nodeValue;
			
			loaderData[x] = {'gameName':gameName,'gameLink':gameLink,'gameImage':gameImage,'gameDesc':gameDesc};
			loaderChangeSlide(x);
			
			document.getElementById("locaderS"+(x+1)).innerHTML = gameName.length >= 18 ?gameName.substr(0,18)+'...':gameName;
			document.getElementById("locaderS"+(x+1)).title = gameName;
			
			// precaching images
			document.getElementById("loaderPrecache").innerHTML = document.getElementById("loaderPrecache").innerHTML + '<img src="'+gameImage+'">';
			
			 if(x==0){
				 document.getElementById("loaderDescName").innerHTML = gameName;
				 document.getElementById("loaderDescText").innerHTML = gameDesc;
				 
			}
		 }
		 // displaying fist slide
		 loaderChangeSlide(0);
	}

	function loaderChangeSlide(id){
		document.getElementById('loaderMainStage').style.backgroundImage = "url('"+loaderData[id].gameImage+"')";
		document.getElementById("loaderDescName").innerHTML = loaderData[id].gameName;
		document.getElementById("loaderDescText").innerHTML = loaderData[id].gameDesc;
	}
	function loaderClickSlide(id){
		window.location = '/'+loaderData[id].gameLink;
	}

initLoader();
</script>
