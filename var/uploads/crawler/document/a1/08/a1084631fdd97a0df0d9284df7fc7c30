function callTumblerService(){
url ='http://adq.nextag.com/buyer/dyad/getRecoProducts.jsp?s=33149&callback=?';
if (nxt.lastId != null) url += "?after=" + nxt.lastId;

/* Ajax velocity check.  dont allow this to call out more then once per 5 seconds */
ts1 = new Date().getTime();
if ((ts1 - nxt.refreshed) < 2000) {
/* Too Soon! - take a nap. */
clearTimeout(nxt.timeout);
nxt.timeout = setTimeout(callTumblerService, 2000);
} else {
j.getJSON(url, function(data, txtStatus){
nxt.refreshed = new Date().getTime();
displayData(data);
});
}
}
