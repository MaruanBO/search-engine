function displayData(data){
nxt.records = [];
j.each(data, function(i,item){
    var nonZeroPrice = (typeof(item.productTitle.lowestPrice) != "undefined") && (typeof(item.productTitle.lowestPriceRaw) != "undefined") && (item.productTitle.lowestPriceRaw != "0.00")
    && (item.productTitle.lowestPriceRaw != "0.0") && (item.productTitle.lowestPriceRaw != "0");
if(nonZeroPrice && (item.productTitle.numSellers > 0)){
/*valid ad. If filterAdult true then for each ad , 
 * now do string check for adult keywords via string api,  crappy but still do it*/
if(true){
/*Get the values from the list of adult keywords*/
var isAdult = false;
for(var idx = 0; idx < adultKeywords.length; idx ++){
              if((item.productTitle.name != "undefined") && (item.productTitle.name.toLowerCase().indexOf((adultKeywords[idx]).toLowerCase()) != -1)){
              isAdult = true;
              break;
              }
        }
if(!isAdult){
//alert('its NOT ADULT so push it');
nxt.records.push(item);
}
} else{
/*no filter so simply push*/
nxt.records.push(item);
}
}
});
rebuildLists(function(){
showTumblerLists(); 
}); 
