        var isFirstTime = 'true';
        var id = 'cookiePolicy';
        var timer;
        if (isFirstTime == 'true') {
               $(id).show();
               timer=setTimeout("$('" + id + "').fade({ duration: 0.8})",15000);
               $(id).observe('mouseover',
