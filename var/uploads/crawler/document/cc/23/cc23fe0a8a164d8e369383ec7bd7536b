	<script>
		$(document).ready(function() {
			$('.mobile-menu-toggle').on('click', function(event) {
				$(this).next('.menu-items').slideToggle("slow", "linear");
				if ($(this).next('.menu-items').hasClass('isOpen')) {
					$('.isOpen').removeClass('isOpen');
					$('.mainOpen').removeClass('mainOpen');
				}
				else {
					$('.isOpen').slideToggle("slow", "linear");
					$('.isOpen').removeClass('isOpen');
					$('.mainOpen').removeClass('mainOpen');
					$(this).next('.menu-items').addClass('isOpen');
					$(this).parent().addClass('mainOpen');
				}
				event.preventDefault();
			});

			$('.NavClose').on('click', function(event) {
				$('.isOpen').slideToggle("slow", "linear");
				$('.isOpen').removeClass('isOpen');
				$('.mainOpen').removeClass('mainOpen');
				event.preventDefault();
			});

			$('.TabNot').on('click', function(event) {
				$('.TabSelected').removeClass('TabSelected');
				$(this).addClass('TabSelected');
				event.preventDefault();
			});
		});
		
		var xStart, yStart = 0;
 
		document.addEventListener('touchstart',function(e) {
			xStart = e.touches[0].screenX;
			yStart = e.touches[0].screenY;
		});
		 
		document.addEventListener('touchmove',function(e) {
			var xMovement = Math.abs(e.touches[0].screenX - xStart);
			var yMovement = Math.abs(e.touches[0].screenY - yStart);
			if((xMovement * 3) > yMovement) {e.preventDefault();}
		});

		$(document).ready(function() {
			$('.clickBanner').on('click', function(event) {
				if ($(this).hasClass('clickCloseBanner')) {
					$('.bottomBanner').animate({width: 'toggle'});
					$('.clickBanner').removeClass('clickCloseBanner');
				}
				else {
					$('.bottomBanner').animate({width: 'toggle'});
					$('.bottomBanner').addClass('DTHide');
					$('.clickBanner').addClass('clickCloseBanner');
				}
				event.preventDefault();
			});
			$('.clickBanner').on('mouseover', function(event) {
				if (!$(this).hasClass('clickCloseBanner')) {
					$('.bottomBanner').animate({width: 'toggle'});
					$('.bottomBanner').addClass('DTHide');
					$('.clickBanner').addClass('clickCloseBanner');
				}
				event.preventDefault();
			});
		});
	</script>
	
