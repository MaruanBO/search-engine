<script>

function getUrlVar(){
        var urlVar = window.location.search;
        var arrayVar = [];
        var valueAndKey = [];
        var resultArray = [];
        arrayVar = (urlVar.substr(1)).split('&');
        if(arrayVar[0]=="") return false;
        for (i = 0; i < arrayVar.length; i ++) {
            valueAndKey = arrayVar[i].split('='); //
            resultArray[valueAndKey[0]] = valueAndKey[1]; //
        }
        return resultArray;
}


    jQuery(document).ready(function(){

        var parseUrl = getUrlVar();

        //console.log(parseUrl);

        if(parseUrl['shop'])
        {
            console.log(parseUrl['shop']);
            var date = new Date( new Date().getTime() + 7*24*3600*1000 );
            document.cookie = "shop="+parseUrl['shop']+"; path=/; expires="+date.toUTCString();
        }

        if(parseUrl['aff'])
        {

            //console.log(parseUrl['aff']);
            //console.log(window.location.search);


            var date = new Date( new Date().getTime() + 7*24*3600*1000 );
            document.cookie = "affiliate="+parseUrl['aff']+"; path=/; expires="+date.toUTCString();

            var url =  window.location.href;

            url = url.slice( 0, url.indexOf('?') );
            document.location.href = url;
        }
      
    });
</script>
