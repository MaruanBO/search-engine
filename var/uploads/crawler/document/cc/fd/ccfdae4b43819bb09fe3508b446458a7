							<script type="text/javascript">

	var consoleExists = false;
	// debugging...
	if (typeof console == "object") { 
		consoleExists = true; 
	}


	$(document).ready(function(){	
		/* redirect from /rl and /sc2 to full catalog */
		
		var currLoc = new String(window.location);
		
		var newLoc = "";
		
		
		/* 1st, trap any outside RL and sc2 links */
		if (currLoc.match("/sc2/") || currLoc.match("/rl/")) {
			
			
			if(consoleExists){console.log("Not in full catalog. btcatalog cookie = "+$.cookie("btcatalog"));}
			
			//delete cookie
			$.cookie("btcatalog","", {expires: 1, path: '/'});
			//$.cookie("btcatalog", null);
			//document.cookie = "btcatalog=; expires=Thu, 01 Jan 1970 00:00:00 GMT"; 
		
			if(consoleExists){console.log("cookie removed");}
		
			newLoc = currLoc.replace('/sc2','').replace('/rl','');
			
			
			if(consoleExists){console.log("Relocate to: "+newLoc);}
			
			/* redirect */
			location.assign(newLoc);
		
		}  
	});
	
</script>



