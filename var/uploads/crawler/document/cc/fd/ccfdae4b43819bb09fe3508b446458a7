<script type="text/javascript">
function getQueryParam(param) {
	var vars = [], hash;
	var q = document.URL.split('?')[1];
	if(q != undefined){
		q = q.split('&');
		for(var i = 0; i < q.length; i++){
			hash = q[i].split('=');
			vars.push(hash[1]);
			vars[hash[0]] = hash[1];
		}
	}
	return vars[param];
}

(function($) {

	// be sure we're on a PDP...
/*
	if ((location.pathname.indexOf("/sc1") != -1 || location.pathname.indexOf("/product") != -1) && location.pathname.indexOf(".html") != -1 && location.pathname.indexOf("/gift-card") == -1) { 
		var ORIGINALIMAGEID = $("meta[name='application-name']").attr("data-imageid");
		var defaultImg = "http://s7d4.scene7.com/is/image/BonTon/" + ORIGINALIMAGEID + "?$2010_large$";
		var bigImg = "http://s7d4.scene7.com/is/image/BonTon/" + ORIGINALIMAGEID + "?wid=2000";
		$("#flyoutviewer").attr("src", defaultImg).attr("data-zoom-image", bigImg);
	}
*/

	// be sure we're on a PDP...
	var currURL = location.pathname;

	if ((currURL.indexOf("/sc1") != -1 || location.pathname.indexOf("/product") != -1) && currURL.indexOf(".html") != -1 && currURL.indexOf("/gift-card") == -1 && currURL.indexOf("coach") == -1  ) { 
//		var TNTFLYOUT = true;
		var ORIGINALIMAGEID = $("meta[name='application-name']").attr("data-imageid");

		if (location.search.indexOf("attrValId") != -1) {
			var indexPageFacetColor = getQueryParam("attrValId");
			if (indexPageFacetColor.length != 0) {
				ORIGINALIMAGEID = ORIGINALIMAGEID + "_" + indexPageFacetColor;
			}
		}


	if (typeof console == "object") { 
		console.log("****************************************************************************"); 
		console.log("MC ORIGINALIMAGEID: " + ORIGINALIMAGEID); 
	}

		var defaultImg = "http://s7d4.scene7.com/is/image/BonTon/" + ORIGINALIMAGEID + "?$2010_large$";
		var bigImg = "http://s7d4.scene7.com/is/image/BonTon/" + ORIGINALIMAGEID + "?wid=2000";
		$("#flyoutviewer").attr("src", defaultImg).attr("data-zoom-image", bigImg);

	if (typeof console == "object") { 
		console.log("****************************************************************************"); 
		console.log("MC default image updated."); 
	}
	} else {
		$("#productPhoto").css('visibility', 'visible');  // show swatches
		$(".productMoreImagesContainer").css('visibility', 'visible');  // show swatches
		$("#defineAttributes").css('visibility', 'visible');
}


})(jQuery);
</script>

