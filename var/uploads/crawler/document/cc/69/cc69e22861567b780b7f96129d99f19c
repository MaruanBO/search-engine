                        %><div class="cm-ad-slide-container"><div class="cm-img-gallery-ad-slide" data-loaded="false" data-ad-loaded="false"><div class="ad-content"></div></div></div><% } %><% }); %></div><div class="cm-gallery-stage"><% var imgArry = [] %><% data.forEach(function (image, i) { %><%
                    var position_class;
                    if(i < 2 || i == data.length - 1) {
                        switch(i) {
                            case 0:
                                position_class = "cm-gallery-primary";
                            break;
                            case 1:
                                position_class = "cm-gallery-next";
                            break;
                            default:
                                position_class = "cm-gallery-previous";
