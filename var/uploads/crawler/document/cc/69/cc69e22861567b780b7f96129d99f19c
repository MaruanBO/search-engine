function AddCollectionToArray(el){config.collection_ids.push(el.id.toString());config.collections_content.push([{articleId:el.articleId,siteId:el.siteId,title:el.title,url:el.url}]);}
function TruncateContent(html){var $existing_content=$widget.find('.cm-lc-body').html(),text_words;if(typeof $existing_content!=='undefined'&&$existing_content!==null&&$existing_content.indexOf(html)>-1){return false;}
