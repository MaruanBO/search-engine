        (function ($) {
            var cmg_rotator_config_obj = cmg_rotator_config_obj || {};
            cmg_rotator_config_obj.rotator_speed = 12000;
            cmg.cmgRotator = $('#cm-media-rotator').cmgMediaRotator();
            cmg.cmgRotator.init(cmg_rotator_config_obj);

            
            cmg.cmgRotator.pushMetrics = function (obj) {
                var link = obj.attr('href'),
                    title = obj.data('title').toLowerCase(),
                    page_name = obj.data('pagename').toLowerCase(),
                    site_type = 'news-site',
                    index = obj.index + 1,
                    channel = (site_type == 'city-site') ? 'entertainment' : cmg.s_coxnews.channel;
                payload = ['47', this];
                payload.push(['pageName', page_name]);
                payload.push(['eVar40', 'interactive_rotator_slide' + index + '_np_' + channel]);
                payload.push(['eVar13', 'prop13', title]);
                payload.push(['eVar42', 'prop42', cmg.s_coxnews.siteName]);
                payload.push(['eVar22', 'prop22', cmg.s_coxnews.prop22]);
                payload.push('np rotator');
                cmg.s_coxnews.utilities.track_event.apply(null, payload);
            };

            
            $('.adgeletti-ad-div.NPR03').appendTo('.cm-media-rotator-frame:last');

            $(window).on('ads/ready', function () {
                $('#cm-npr02-block').renderWithAd('NPR02');
                cmg.harmony.slot('NPR01').on('slotRenderEnded', function (event) {
                    if (event.isEmpty) {
                        cmg.cmgRotator.doRotate();
                    } else {
                        cmg.cmgRotator.pauseRotate();
                    }
                });
                cmg.harmony.slot('NPR03').on('slotRenderEnded', function (event) {
                    if (!event.isEmpty) {
                        $('.cm-media-rotator-frame:last').
                            children().
                                not('.adgeletti-ad-div').
                                    remove();
                        $('#cm-ad-block-npr04').
                            siblings().
                                remove().
                                end().
                            show();
                    }
                });
            });
        }(cmg.query));
    </script><div class="cm-column-640 cm-tablet-show"><div class="row cm-subrotator-section"><div class="cm-storycard cm-storycard-image_medium cm-section-news cm-img-box cm-img-box-med cmStoryImage_medium  " data-published="2015-02-19T10:03:16-06:00"


