<div id="fb-root"></div>

<script type="text/javascript">
	var socialScriptsLoaded = false;
	var facebookScriptTimer;
	$(function() {
		$( "#SocialTabs-RB" ).hover(function() {
			if (!socialScriptsLoaded) {
				loadScript("http://s7.addthis.com/js/250/addthis_widget.js#pubid=xa-4ffdbef47e5ccef9");
				socialScriptsLoaded = true;
			}
			
		})
		
		$('#twitterhref').bind('show', function(e) {
			$.get('/spa/content/social/twitter.htm', function(data) {
				$('#twitter').html(data)
			});
		});
		
		$('#fbhref').bind('show', function(e) {
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=145494725524096";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));

            facebookScriptTimer = setTimeout(showLikeButton, 200);
		});
		
	});
	
</script>


