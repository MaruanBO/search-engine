				'readyCallback': function (playerId) {
					console.log('kWidget player ready: ' + playerId);
					var kdp = jQuery('#' + playerId).get(0);
					kdp.addJsListener('doPlay', 'doPlayCallback');
				}
			});
		</script>
		

