    <script type="text/javascript">
        function adjustMarginsForAd() {
            theDiv = $('#adSpotHome');
            if (theDiv !== undefined &&
                theDiv.height() > 10) {
                theDiv.css({ 'marginBottom': '10px' });
            }
        }
        
        $(function() {
            setTimeout(adjustMarginsForAd, 30);
        });
    </script>



<!--End Ad Control-->
    
    
</div>
            </div>
        
