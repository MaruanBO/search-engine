<script type="text/javascript">
    (function($) {
        $.widget('ux.drilllist', {
            options: {
        },
        _create: function() {
            o = this.options;
            var li = this.element.find('li'),
					 a = this.element.find('a'),
					 w = this.element.width,
					 rows = 8,
					 cols = 6;
            if (li.length == 2) {
                rows = 1;
                cols = 2;
            }
            var line = li.height() * rows;
            for (i = 0; i < li.length; i++) {
                var newnum = Math.floor(i / rows);
                li.eq(i).css({ 'margin-left': (newnum == 1 ? '110' : (newnum == 2 ? '220' : (newnum == 3 ? '330' : (newnum == 4 ? '440' : (newnum == 5 ? '550' : '0'))))) + 'px', 'width': (w / cols) + 'px' });
                if (newnum > 0) {
                    li.eq(i).css({ 'border-left': 'solid 1px #ccc' });
                    a.eq(i).css({ 'margin': '0 5px' });
                }

                if ((i + 1) % rows == 0) {
                    var marg = line * -1;
                    li.eq(i + 1).css({ 'margin-top': marg + 'px' });
                }
                //this.element.height(line);

                if (i == li.length - 1) {
                    for (j = 0; j < (rows * cols - li.length); j++) {
                        $('.cnt-drill ul').append('<li style="margin-left: 550px; width: 120px; border-left-style: solid; border-left-width: 1px; border-left-color: #ccc;height:19px;"></li>');
                    }
                }
            }
        }
    });
})(jQuery);

$(function() {
    $('.cnt-drill').drilllist();
});
</script>
