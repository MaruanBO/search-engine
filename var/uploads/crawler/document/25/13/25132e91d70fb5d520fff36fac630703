        <script src="http://cdn.newsok.biz/scripts.php?ver=1.26"></script>

        <script>
            
            offsetPos = document.getElementById('subNav').offsetTop;
            $('#subNav').affix({offset: offsetPos});
            
        </script>

<script type="text/javascript">
// Array of page_sids
var pages = ['home', 'sports', 'news', 'business', 'editorials', 'ae', 'blogs', 'life', 'weather_2010', 'multimedia_photos', 'multimedia_video'];

// Array of corresponding urls
var pageUrls = ['/', '/sports', '/news', '/business', '/opinion', '/ae', '/blogs', '/life', '/weather', '/multimedia/photos', '/multimedia/video'];

// Total pages
var total = pages.length - 1;

// Current page
var curPage = $.inArray('home', pages);


// Index of previous page
if (curPage == 0) {
	var prevPage = total;
} else {
	var prevPage = curPage-1;
}

// Index of next page
if (curPage == total) {
	var nextPage = 0;
} else {
	var nextPage = curPage+1;
}


// Set the prev/next link urls
$('#prev-section').attr('href', pageUrls[prevPage]+'?prevPage='+pages[prevPage]);
$('#next-section').attr('href', pageUrls[nextPage]+'?nextPage='+pages[nextPage]);
</script>

            <script>
                var hideSub;
                var showSub;
                var thisSub = "home_main";
                var mouseover = false;
                
                $(document).ready(function() {
                    $('.openSubNav').mouseenter(function(){
                        mouseover = true;
                    }).mouseleave(function(){
                        mouseover = false;
                    });

                    $('.openSubNav').mouseover(function() {
                        if ($('.subSubnavWrapper').css('height') == '65px') {
                            $('.subSubnav').hide();
                            $('#subSubnav_'+this.id).show();
                        } else {
                            var navId = this.id;
                            setTimeout(function () {
                                if (mouseover == true) {
                                    $('.subSubnav').hide();

                                    $('.subSubnavWrapper').css('height', '65px');
                                    $('#subSubnav_'+navId).show();
                                }
                            }, 500);
                            setTimeout("$('.subSubnavWrapper').css('overflow', 'visible');", 1500);
                        }
                    });

                    $('#subNav').bind("mouseleave",function() {
                        hideSub=setTimeout("hideSubNav()", 1000);
                        showSub=setTimeout("$('#subSubnav_'+thisSub).show()", 1000);
                    });
                    $('#subNav').bind("mouseenter",function() {
                        clearTimeout(hideSub);
                        clearTimeout(showSub);
                    });

                    // search box
                    var searchImg = $('.search-icon img').attr('src');
                    $('.search-icon').click(function(e) {
                        e.preventDefault();
                        if ($(this).find('img').attr('src') == searchImg) {
                            $(this).toggleClass('active').find('img').attr('src', 'http://s3.amazonaws.com/content.newsok.com/newsok/refactor/icon_search_close.png').parent().next().toggleClass('hide');
                        } else {
                            $(this).toggleClass('active').find('img').attr('src', searchImg).parent().next().toggleClass('hide');
                        }
                    });
                });
                function hideSubNav() {
                    if(document.getElementById('subSubnav_'+thisSub) == null) {
                        $('.subSubnavWrapper').css('overflow', 'hidden');
                        $('#mainBody').removeClass('subnav');
                        $('.subSubnavWrapper').css('height', '0px');
                    }
                }
                if (document.getElementById('subSubnav_'+thisSub) != null) {
                    $('.subSubnav').css('height', '0px');
                    $('.subSubnavWrapper').css('height', '65px');
                    $('#subSubnav_'+thisSub).css('height', '65px');
                    $('#mainBody').addClass('subnav');
                }
                
            </script>
            
<div class="pagetab">
