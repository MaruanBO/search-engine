<script type="text/javascript">
function urlToLink(text) {
	var link = / (\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;
	var atReply = /\B[@]\w+/ig;
	var hashTag = /\B[#]\w+/ig;
	var hashTagLink = /search[?]q=[#]+/ig;
	text = text.replace(link," <a href='$1' target='_blank'>$1</a>");
	text = text.replace(atReply,"<a href='http://twitter.com/$&' target='_blank' class='at-reply'>$&</a>");
	text = text.replace(hashTag,"<a href='http://twitter.com/search?q=$&' target='_blank' class='hashtag'>$&</a>");
	text = text.replace(hashTagLink,"search?q=%23");
	return text;
}

$('.tweet-text').each(function() {
	var text = $(this).html();
	var returned = urlToLink(text);
	$(this).html(returned);
});
</script>


