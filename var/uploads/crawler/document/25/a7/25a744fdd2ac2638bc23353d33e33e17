    <script type="text/javascript">
      $(function() {
        String.prototype.replaceAll = function(strReplace, strWith) {
            var reg = new RegExp(strReplace, 'ig');
            return this.replace(reg, strWith);
        };
        var searchIsUp = false;
        $('#searchForItems').on('submit', function(e){
                e.preventDefault();
                var action = '/search/items/';
                $.get(action, $(this).serialize(), function(data){
                    var srchRlts = $('<div></div>');
                    var template = $('#searchResults').html();
                    var rsltHtml = '';
                    var searchQ = data.q;
                    $('#searchResultsContent').html('');
      
      /* CHANNELS */
                    if (data.channels.length !== 0) {
      $('#searchResultsContent').append('<p class="srchHeader">CHANNELS</p>');
                    }
                    for(var i = 0; i < data.channels.length; i +=1) {
      var title = data.channels[i].title.replaceAll(searchQ, '<strong>' + searchQ + '</strong>');
      var itmRslt = "<p class='srchResult'><a href='" + data.channels[i].link + "'>" + title + "</a></p>";
                      $('#searchResultsContent').append(itmRslt);
                    }
      
                  if (data.items.length !== 0) {
                      $('#searchResultsContent').append('<p class="srchHeader">VIDEOS</p>');
                  }
      /* ITEMS */
                    for(var i = 0; i < data.items.length; i += 1) {
      var title = data.items[i].title.replaceAll(searchQ, '<strong>' + searchQ + '</strong>');
      var itmRslt = "<p class='srchVideo'><a href='" + data.items[i].link + "'><img src=\"" + data.items[i].thumbnail + "\" />" + title + "</a></p><div class='clear'></div>";
                        $('#searchResultsContent').append(itmRslt);
                    }
                    if (data.items.length === 0) {
                      $('#searchResultsContent').append('<p class="srchResult"><strong>No Videos Found</strong></p>');
                    }
                searchIsUp = true;
                $('#searchResults').fadeIn(250);
                }, 'json');
            });

            $(document).bind('click', function(e){
                if (searchIsUp === true) {
                  $('#searchResults').fadeOut(250);
                  searchIsUp = false;
                }
            });
  $('a').each(function() {
     var _href = $(this).attr("href"); 
     $(this).attr("href", _href + window.location.search);
  });
      });
    </script>

    <!-- LAZY LOAD -->
    <script type="text/javascript">
	$(function(){
      jQuery.fn.outerHTML = function() {
  return jQuery('<div></div>').append(this.eq(0).clone()).html();
};

    var $container = $('.channel-lazyload-div');

    $container.imagesLoaded(function(){
      $container.masonry({
        itemSelector: '.thumb-container',
		isAnimated: true
      });
    });
      
      $container.infinitescroll({
      	navSelector: '.channel-navigation',
        nextSelector: '.channel-lazyload-next',
        itemSelector: '.thumb-container',
        pathParse: function(path, nextPage){                                   
             var re = new RegExp('^(.*?page=)'+nextPage+'(\/.*|$)');    
             path = path.match(re).slice(1);                                       
             return path;
        }
      },
      // trigger Masonry as a callback
      function( newElements ) {
        // hide new items while they are loading
        var $newElems = $( newElements ).css({ opacity: 0 });
        // ensure that images load before adding to masonry layout
      //        $newElems.imagesLoaded(function(){
          // show elems now they're ready
      setTimeout(function(){
          $newElems.animate({ opacity: 1 });
          $container.masonry( 'appended', $newElems, true ); 
     	}, 1500);
      //        });

      });
      
     });
    </script>
