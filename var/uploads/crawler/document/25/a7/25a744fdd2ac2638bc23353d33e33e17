    <script type="text/javascript">
      $(function() {
        $(window).scroll(function(){
      		var windowTop = 0;
      		var windowTopSrch = 0;
      		if ($(window).scrollTop() > ($('#body').offset().top + $('#body h3').height())) {
      			windowTop = $(window).scrollTop() - ($('#body').offset().top + $('#body h3').height());
      			windowTopSrch = windowTop + 30;
            }
          $(".ads").stop().animate({"marginTop": (windowTop) + "px"}, "slow" );
          $("#searchResults").stop().animate({"marginTop": (windowTopSrch) + "px"}, "slow" );
        });
      });
    </script>
