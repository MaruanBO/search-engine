     var getMsnProperty = function (propName, defVal) {
        if (window.msnGeoOverrides) {
            if (window.msnGeoOverrides.channels[PageCriteria.channel]) {
                return window.msnGeoOverrides.channels[PageCriteria.channel][PageCriteria.pageType][propName];
            } else {
                return "";
            }
        }
        return defVal;
    }

