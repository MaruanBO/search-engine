  <script type="text/javascript">
    $(document).ready(function () {
      $("#home-apply-tab").tabs();

      var slider = $('.homeslider').knslider();

      //Rotator
      $("#home-rotate-headers").tabs({ show: { effect: "fade", duration: 600 } });
      $("#home-rotate").tabs({ hide: { effect: "slide", duration: 300 }, show: { effect: "slide", duration: 300, direction: "right" } });

      //This automatically loops through the second set of tabs
      var intid = setInterval("loop_tabs()", 5000);
      $("#circle-selectors li").click(function () {
        window.clearInterval(intid); 	//Stop the loop if user clicks the indicators
        //Make sure the H3 changes too
        var active = $("#home-rotate").tabs("option", "active");
        $("#home-rotate-headers").tabs("option", "active", active);
      });
    });

    function loop_tabs() {
      var active = $("#home-rotate").tabs("option", "active");
      active++;
      if (active == $("#home-rotate").find("ul > li").length)
        active = 0;
      $("#home-rotate").tabs("option", "active", active);
      $("#home-rotate-headers").tabs("option", "active", active);	//Change the H3 with it
    }

  </script>

