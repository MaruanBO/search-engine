    <script>
        SpilGames(['SWPUtils', 'DOMSelect', 'DOMEvent'], function(SWPUtils, DOMSelect, DOMEvent) {
            var isOpen,
                container = DOMSelect.get('.inner.container'),
                rootElement = DOMSelect.get('.wdg_menu'),
                body = document.body,
                html = document.documentElement,
                menu = DOMSelect.get('.left.offcanvas'),
                timer;

            function delayResizeCallback () {
                clearTimeout(timer);
                timer = setTimeout(setMenuHeight, 250);
            }

            function setMenuHeight () {
                var height = Math.max( body.scrollHeight, body.offsetHeight, html.clientHeight, html.scrollHeight, html.offsetHeight );

                if (menu) {
                    // reset inline menu styling for proper
                    // testing the height of the menu
                    menu.style.height = '';
                    menu.style.overflowY = '';

                    if (menu.offsetHeight > height) {
                        menu.style.height = height + 'px';
                        menu.style.overflowY = 'scroll';
                    }
                }
            }

            function open() {
                isOpen = true;
                container.setAttribute('data-offcanvas', 'left');
                SWPUtils.addClass(rootElement, 'is-open');
                setMenuHeight();
                DOMEvent.add(window, 'resize', delayResizeCallback);
            }

            function close() {
                isOpen = false;
                container.removeAttribute('data-offcanvas');
                SWPUtils.removeClass(rootElement, 'is-open');
                DOMEvent.remove(window, 'resize', delayResizeCallback);
            }

            DOMEvent.add(document, 'click', function(e) {
                if (!isOpen) return;

                e = e || window.event;
                var target = e.target || e.srcElement;

                while (target) {
                    if (SWPUtils.hasClass(target, 'wdg_menu')) return;

                    target = target.parentNode;
                }

                close();
            });

            DOMEvent.add(window, "orientationchange", setMenuHeight);

            DOMEvent.add('.browse-toggle', 'click', function() {
                isOpen ? close() : open();
            });
        });
    </script>
