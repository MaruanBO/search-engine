    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    var track_interval = 300000, _gaTrackers=[];

    w.spilTracker = function(name,ua,domain) {
        var prefix = name ? name + "." : "";
        ga('create', ua, 'auto', {'name': name, 'legacyCookieDomain': domain});
        ga(prefix+'require', 'linkid', 'linkid.js');
        ga(prefix+'require', 'displayfeatures');
        ga(prefix+'send', 'pageview');
        _gaTrackers.push(prefix);
    }

    w.spilEvent = function(category,action,label,value,opt_noninteraction) {
        for(var i=_gaTrackers.length;i--;) {
            ga(_gaTrackers[i]+'send', 'event', category, action,
                label, value, {nonInteraction:!!opt_noninteraction});
        }
    }

    w.sendPageviewToAllTrackers = function() {
        var domain = '.gamesgames.com';
        spilTracker('', 'UA-2000540-1', domain);
        spilTracker('aggregated', 'UA-8223336-1', domain);
    }

    w.spilTrackInterval = function() {
        spilEvent('time_on_page','5_minutes_interval');
    }

    w.startSpilIntervalTracker = function() {
        window.setInterval(spilTrackInterval, track_interval);
    }
})(window);

sendPageviewToAllTrackers();
startSpilIntervalTracker();
</script>



        
            
