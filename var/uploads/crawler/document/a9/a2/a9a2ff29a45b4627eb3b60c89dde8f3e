		var img4 = 'http://workingperson.com/skin/frontend/blank/wps3/images/home_logo_cat.png';
		var img5 = 'http://workingperson.com/skin/frontend/blank/wps3/images/home_logo_redkap.png';
		
		jQuery("#wpse_button").live("click", function() {
			if(!validateEmail(jQuery('#wpse_email_input').val())) {
				jQuery('#wpse_email_input').addClass('wpse_invalid');
			} else {
				var wpse_email = jQuery('#wpse_email_input').val();
				jQuery.cookie("newsletter", 1, { expires: 10000, path: "/" });
				jQuery('#wpse_email_input').removeClass('wpse_invalid');
				jQuery(".wpse_title").html("Your Coupon Code Has Been Sent!");
				jQuery("#wpse_result_text").html('<div class="wpse_row">Thank you for subscribing to our newsletter. Your coupon code has been sent to your email address.  <strong>Please check your email to claim your coupon</'+'strong>.</'+'div><div class="wpse_title2" style="padding-bottom: 0px;">Save BIG On Our Popular Brands Today!</'+'div><div class="wpse_row"><a href="/brands/carhartt-clothing-1.html"><img style="padding-left: 10px;" src="'+img1+'" width="175" height="54" alt="Carhartt Clothing" title="Carhartt Clothing" /><'+'/a><a href="/brands/wolverine-boots-shoes.html"><img style="padding-left: 40px;" src="'+img2+'" width="179" height="54" alt="Wolverine" title="Wolverine" /><'+'/a><a href="/brands/dickies-clothes.html"><img style="padding-left: 40px;" src="'+img3+'" width="87" height="54" alt="Dickies" title="Dickies" /><'+'/a><a href="/brands/caterpillar-boots-shoes.html"><img style="padding-left: 45px;" src="'+img4+'" width="62" height="54" alt="Caterpillar" title="Caterpillar" /><'+'/a><a href="/brands/red-kap-uniforms.html"><img style="padding-left: 50px;" src="'+img5+'" width="64" height="54" alt="Red Kap" title="Red Kap" /></'+'a></'+'div>');		
				jQuery("#wpse_result").html('<img src="http://email.workingperson.com/public/?q=direct_add&fn=Public_DirectAddForm&id=bszjhojvwptqiqlezmgdxipsbvuobnk&email='+wpse_email+'&list1=0bc403ec0000000000000000000000056843&field1=signup_source,set,shadowbox" width="0" height="0" border="0" alt="" /><input id="wpse_continue_button" type="image" src="'+continue_img+'" />');
			}
		});
		
		jQuery("#wpse_continue_button").live("click", function() {
			jQuery('#wps_email').bPopup().close();
		});
		
		jQuery(".wpse_nt").live("click", function() {
			jQuery('#wps_email').bPopup().close();
		}); 
		
		jQuery(".newsletter_top_link").live("click", function() {
			jQuery("#wps_email").bPopup({
				closeClass: 'wpse_close',
				zIndex: '9999999',
				positionStyle: 'fixed',
				modalClose: false,
				onClose: function() {
					if(jQuery.cookie("newsletter") != '1') {
						jQuery.cookie("newsletter", 1, { expires : 30, path: "/" });
					}
				}
			});
		}); 
		
		
		function validateEmail($email) {
			var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
			if( !emailReg.test( $email ) ) {
				return false;
			} else {
				return true;
			}
		}
		
		jQuery("#wpse_email_input").attr("autocomplete", "off");
		
		jQuery().ready(function(){
		
			// Select all textboxes and assign them to an array
			var textboxes = jQuery('#wpse_email_input');
			
			// Iterate through all textboxes in the form
			textboxes.each(function(index, input) {
			
				var label = jQuery(input).prev();
				label.children("span").animate({opacity: 0.6}, 0);
				
				// check for autocomplete by browser
				if(index == 0) {
					setInterval(function() {
						textboxes.each(function(index, inputX) {
							if(inputX.value != "") {
								jQuery(inputX).prev().children("span").animate({opacity: 0}, 0);
							}
						});
					}, 100);
				}
				
				// Fade the label back when a field gains focus     
				input.onfocus = function() {
					if(input.value == "") {
						label.children("span").animate({opacity: 0.4}, 200);
					}
				}
				
				// Check if a field is empty when the user switches out
				input.onblur = function() {
					if(input.value == "") {
						label.children("span").animate({opacity: 0.6}, 200);  
					}
				}
				
				// Fade the label back if a field has text      
				if(input.value != "") {
					label.addClass('hastext');
				}
				
				// Fade the label back when the user starts to type     
				input.onkeyup = function() {
					if(input.value == "") {
						label.children("span").animate({opacity: 0.6}, 200);  
					} else {
						label.children("span").animate({opacity: 0}, 200);
					}
				};
				
			});
		});
	</script>


						
