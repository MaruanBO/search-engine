<script type="text/javascript">
	
	var is_promo = true;
	
	jQuery("#header_promo_submit").live('click', function(event) {

		var regex = /^([A-Za-z0-9_\+\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
		if(!regex.test(jQuery('#wps_email').val())) {
			alert("Your email address format appears to be invalid.  Please enter a valid email address.");
			return(false);
		}
		
		if(!regex.test(jQuery('#wps_email_friend').val())) {
			alert("Your friend's email address format appears to be invalid.  Please enter a valid email address.");
			return(false);
		}
		
		if(jQuery('#wps_email').val() == jQuery('#wps_email_friend').val()) {
			alert("You appear to have entered the same email address in both fields.  Please enter two different email addresses.");	
		}

		
		jQuery.ajax({
			url: '/custompage/sendfriend/submit',
			data: {
				sender_email: jQuery("#wps_email").val(),
				friend_email: jQuery("#wps_email_friend").val()
			},
			type: "POST",
			dataType: "text",
			timeout: 30000,
			beforeSend: function() {
				
			},
			success: function(data) {
								jQuery("#header_promo_form").hide();
				
								jQuery("#header_promo_success").html(data);
				jQuery("#header_promo_success").show();
				
			},
			error: function(xhr, textStatus, errorThrown) {
							}
		});

	});
</script>





