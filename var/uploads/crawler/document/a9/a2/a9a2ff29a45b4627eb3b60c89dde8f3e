	<script type="text/javascript">
		jQuery(window).scroll(function () {
			var opacity = ((100-((jQuery(window).scrollTop() / jQuery(document).height())*100))/100);
			
			if((jQuery(document).height()-jQuery(window).scrollTop()) <= jQuery(window).height()+200) {
				opacity = 0;
			}
			jQuery('#header_scroll').fadeTo(50, opacity);
		});
		jQuery('#header_scroll').click(function () {
			jQuery('html,body').animate({
				scrollTop: jQuery(window).scrollTop() + 400 + 'px'
			}, 300);
		}).hover(function () {
			jQuery(this).fadeTo(200, 1);
		});
		
		
				jQuery(window).scroll(function () {
			if((jQuery(window).scrollTop()+jQuery(window).height()) >= (jQuery(document).height()-612)) {
				var new_pos = (jQuery(window).height() - ((jQuery(window).scrollTop()+jQuery(window).height()) - (jQuery(document).height()-612))) - 365;
				var bg_css = '0px '+new_pos+'px';
				jQuery('#scroll_bg').css({backgroundPosition: bg_css});
			} else {
				jQuery('#scroll_bg').css({backgroundPosition: '0px bottom'});
			}
		});
		
		jQuery(document).ready(function() {
			if(jQuery(window).height() >= jQuery(document).height()-612) {
				var new_pos = (jQuery(window).height() - ((jQuery(window).scrollTop()+jQuery(window).height()) - (jQuery(document).height()-612))) - 365;
				var bg_css = '0px '+new_pos+'px';
				jQuery('#scroll_bg').css({backgroundPosition: bg_css});
			}
		});
	</script>

