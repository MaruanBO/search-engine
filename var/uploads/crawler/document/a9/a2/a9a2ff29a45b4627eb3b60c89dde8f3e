				<script type="text/javascript" language="JavaScript">
			// <![CDATA[
								jQuery('#header_nav_search_input').focus(function() {
					jQuery("#header_search_help").hide();
					if(jQuery('#header_nav_search_input').val() == 'Ex: Work Boots, Carhartt, Dickies, Etc') {
						jQuery('#header_nav_search_input').val('');
						jQuery('#header_nav_search_input').addClass('header_nav_search_focus');	
					}
				});
				
								jQuery(".header_search_button").live('click', function(event) {
			  		if(jQuery("#header_nav_search_input").val() == 'Ex: Work Boots, Carhartt, Dickies, Etc' || jQuery("#header_nav_search_input").val() == '') {
			  			jQuery("#header_search_help").show();
			  		} else {
			  			jQuery("#header_search_help").hide();
			  			jQuery('#searchForm').submit();
			  		}
		  		});
		  		
		  				  		jQuery("#header_search_help").live('click', function(event) {
		  			jQuery("#header_search_help").hide();
		  		});
				
								var top_mouse = false;
				var sub_mouse = false;
				var button_mouse = false;
				
				var is_home = 1;				
				jQuery(".header_nav_menu").live('mouseenter', function(event) {
					jQuery("#header_menu_top_categories").show();
					button_mouse = true;
				});
				
				jQuery(".header_nav_menu").live('mouseleave', function(event) {
					button_mouse = false;
					setTimeout(function() {
						if(top_mouse == false && is_home == 0 && button_mouse == false && sub_mouse == false) {
							jQuery("#header_menu_top_categories").hide();
						}
					}, 1000);
				});
				
				jQuery("#header_menu_top_categories").live('mouseenter', function(event) {
					jQuery("#header_menu_top_categories").show();
					top_mouse = true;
				});
				
				jQuery("#header_menu_top_categories").live('mouseleave', function(event) {
						top_mouse = false;
						setTimeout(function() {
							if(sub_mouse == false && top_mouse == false) {
								jQuery(".header_menu_top_category_container").hide();
								if(is_home == 0 && button_mouse == false) {
									jQuery("#header_menu_top_categories").hide();
								}
							}
						}, 1000);
				});
				
				jQuery(".header_menu_top_li").live('mouseenter', function(event) {
					
					if(is_home == 1 && top_mouse == false) {
						jQuery(".header_menu_top_category_container").hide();
						var id_num = this.id.split('_');
						setTimeout(function() {
							if(jQuery('#header_menu_'+id_num[3]).length > 0 && top_mouse == true && !jQuery(".header_menu_top_category_container").is(":visible")) {
								top_mouse = true;
								jQuery('#header_menu_'+id_num[3]).show();
							}
						}, 500);
						
					} else {
						top_mouse = true;
						jQuery(".header_menu_top_category_container").hide();
						var id_num = this.id.split('_');
						if(jQuery('#header_menu_'+id_num[3]).length > 0) {
							jQuery('#header_menu_'+id_num[3]).show();
						}
					}
				});
				
				jQuery(".header_menu_top_category_container").live('mouseenter', function(event) {
					sub_mouse = true;
				});
				
				jQuery(".header_menu_top_category_container").live('mouseleave', function(event) {
					sub_mouse = false;
					var close_id = this.id;
					setTimeout(function() {
						if(top_mouse == false && sub_mouse == false) {
							jQuery("#"+close_id).hide();
						}
						if(top_mouse == false && is_home == 0 && sub_mouse == false && button_mouse == false) {
							jQuery("#header_menu_top_categories").hide();
						}
					}, 1000);
				});
				
								jQuery(".header_menu_close").live('click', function(event) {
					jQuery(".header_menu_top_category_container").hide();
					if(is_home == 0) {
						jQuery("#header_menu_top_categories").hide();
					}
				});
			// ]]>
		</script>
	</div>


		
		
		
		
