						<script type="text/javascript">
							 jQuery('#footer_email').keypress(function(e) {
								if(e.which == 13) {
									subscribe_email('footer_email_signup', 'footer_signup_text', 'footer_email_loading', 'footer_email');
								}
							});
							
							jQuery("#email_submit").click(function() {
									subscribe_email('footer_email_signup', 'footer_signup_text', 'footer_email_loading', 'footer_email');
							});
							
							
							function subscribe_email(signupId, textId, loadingId, emailId) {
								jQuery("#"+textId).hide();
								jQuery("#"+loadingId).show();

								jQuery.ajax({
									type: "POST",
									url: "/bronto.php",
									async: true,
									data: {
										email: jQuery("#"+emailId).val(),
										action: "add",
										rewards: rewards_member
									},
									success: function(data) {
																				if(data != '') {
											var result = data.split("||");
											if((result[0] == '1' || result[0] == '305' || result[0] == '314') && data != '0') {
												jQuery("#"+loadingId).hide();
												if(signupId == 'footer_email_signup') {
													jQuery("#"+signupId).hide();
												}
												jQuery("#"+textId).html('<p class="footer_email_response"><strong>Success!</'+'strong> You are now subscribed to our deals list.  We\'ll keep you informed about special sales and send you coupons from time to time.</'+'p>');
												jQuery("#"+textId).show();
												
												if(rewards_member == 1 && result[1] == 1) {
																										clearTimeout(rtimer);
													jQuery("#r_notify").slideUp("normal");
													
													jQuery('#r_notify_title').html(result[2]);
													jQuery('#r_notify_message').html(result[3]);
													if(result[4] != undefined) {
														jQuery('#r_notify_points').html(result[4]);		
														jQuery('#r_bar_points').html(result[4]);
													}
													
													jQuery("#r_notify").slideDown("normal");
													rtimer = setTimeout(function(){jQuery("#r_notify").slideUp("normal")},15000);
												}
												
											} else {
												jQuery("#"+loadingId).hide();
												jQuery("#"+textId).html('<p class="footer_email_response">There was an error subscribing you to the list.  Please check the email address you entered and try again.</'+'p>');
												jQuery("#"+textId).show();
											}
										}
									} 
								});	
							}
						</script>
					</div>	
					
