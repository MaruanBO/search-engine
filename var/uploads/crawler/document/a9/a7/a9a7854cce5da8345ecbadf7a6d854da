        </div>

        <% if (data.secondary_title) { %>
            <p class="topic-title-mobile global-category-small"><%= data.secondary_title %></p>
        <% } %>

        <% if (data.title) { %>
            <h4 class="content-title-mobile"><%= data.title %></h4>
        <% } %>

        <div class="mega-video-description clearfix">
            <div class="mega-items-description">
                <p class="global-description">
                    <% if(data.description) { %>
                        <%= data.description %>
                    <% } %>
                </p>
            </div>
            <div class="mega-items-amount">
                <% if(data.overal_type) { %>
                        <div class="overal_type"><%= data.overal_type %></div>
                    <% } %>
                    <div class="navigation">
                        <div class="left-button" style="height: 30px;">
                            <div class="left"></div>
                        </div>
                        <div class="devider"></div>
                        <div class="right-button" style="height: 30px;">
                            <div class="right"></div>
                        </div>
                    </div>
                <% if(data.call_to_action_url) { %>
                    <div class="mega-featured-button">
                        <a href="<%= data.call_to_action_url %>" class="page-homepage-cta-button">Watch now</a>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
