var author = 'PEOPLEENESPANOL.com';
byline = jQuery('.node-type-gallery .byline, .node-type-article .post-author'); //collection of elements which may contain author names

if(byline.length > 0){
  author = byline.html(); //store author name from first element found
  if(byline.length > 1){ //if more than one author name is found, concatenate additional author names
    byline.slice(1).each(function(){
      author += ", " + jQuery(this).html();
    });
  }
}

