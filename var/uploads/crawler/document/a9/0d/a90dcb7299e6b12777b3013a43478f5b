			<script language="javascript" type="text/javascript">
        
          function get_page_url(){
            var url = window.location.protocol + "//" + window.location.host + window.location.pathname;
            return url;
          }
          
          function interstitial_restricted(url){
            //if & else check for 'mary-kay' and 'gallery' in url
            if((url.indexOf("mary-kay") > -1)  && (url.indexOf("gallery") > -1)){
              return true;
            }
            else{
              return false;
            }
          }
          
          var url = get_page_url();
          //console.log(url);
          
          var bool = interstitial_restricted(url);
          //console.log(bool);

        if(!interstitial_restricted(url)){
          var adCm = adFactory.getCmAd(240, 90, "globalheader", "tout");
          ad.setParam("dcopt", "ist");
          adCm.write();
        }
        else{
          //console.log("coming to else");
        }
			</script>
