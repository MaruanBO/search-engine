            <div class="section">
              <a id="main-content"></a>
                                            <div class="title-container" id="page-title-container">
      	          <h1 class="title" id="page-title">Inicio</h1>
    	          </div>
    	          <div id="title-divider"></div>
                
      	        	        
    	          
      	        
      	            	        <div class="tabs">
      	                	        </div>
      	        
      	        
      	        <div class="region region-content">
    <div id="block-pesp-homepage-homepage-center-dl" class="block block-pesp-homepage">

    
  <div class="content">
    <script type="text/javascript">
var delay = 3000;
var currPic = 1;
var totalPic = 3;
var timer1;


function changeNumber(num) {
    if ($('#ppBtn').hasClass('pause')) {
        $('#ppBtn').removeClass('pause').addClass('play');
        clearTimeout(timer1);
    }
    changeSlide(num);
    clearTimeout(timer1);
}
function changeButton() {
    if ($('#ppBtn').hasClass('pause')) {
        clearTimeout(timer1);
        $('#ppBtn').removeClass('pause').addClass('play');
    }
    else {
        $('#ppBtn').removeClass('play').addClass('pause');
        clearTimeout(timer1);
        slideShow();
    }
}
function changeSlide(num) {
  
        $('.slideshow,.slideNavButton').removeClass('on');
        $('.slideshow').eq(num-1).addClass('on');
        $('.slideNavButton').eq(num-1).addClass('on');
        return false;
    
}
function slideShow () {
    if (currPic > totalPic){currPic = 1;}
    changeSlide(currPic);
    timer1 = setTimeout('slideShow()', delay);
    currPic++;

}
$(document).ready(function() {
            changeSlide(1);
            $('.slideNavButton a').each(function() {
                 $(this).bind("click", function() {
                       $('.slideNavButton.on').removeClass('on');
                       $(this).addClass('on');
                       changeNumber($(this).text());
                       return false;
                 });
            });

            $('#ppBtn a').bind("click", function() {
                 changeButton();
                 return false;
            });
            timer1 = setTimeout('slideShow()', delay);
 });

</script>


