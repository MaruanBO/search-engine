    jQuery(document).ready(function($){
        $('#datepicker-trigger').DatePicker({
            starts: 0,
            date: '15-02-19',
            onChange: function(formated, dates){
                var urlday = dates.getDate();
                var urlmonth = dates.getMonth() + 1;
                var urlyear = dates.getFullYear();
                var urldate = urlmonth + '%2F' + urlday + '%2F' + urlyear;
