            jQuery(document).ready(function($){
                
                
                $('#centerpiece-477792').tinycarousel({
                    pager: true,
                    
                    interval: true,
                    intervaltime: 5000,
                    
                    axis: 'x',
                    duration: 500,
                    
                    callback: function(element, index){
                        
                        $('#centerpiece-477792 .pager li').removeClass('active');
                        $('#centerpiece-477792 .pager li').eq(index).addClass('active');
                        
                    }
                });
                
                  
                    setTimeout(function(){
                        $('#centerpiece-477792 .viewport').css('height', $('#centerpiece-477792 .overview').height());
                    },500);
                
                
                //$('#centerpiece-477792 .image img').css('width', $('#centerpiece-477792').width());
                
                
                    $('#centerpiece-477792 .slider-display').each(function(){
                        $sImg = $(this).parent().find('.image');
                        if($sImg.length > 0 ) {
                            $(this).css('bottom',0).css('background-color','rgba(0, 0, 0, 0.5)').css('width', ($(this).parent().width()-30));
                            $(this).parent().parent().show().css('height',$sImg.height());
                            $aHeight = $(this).parent().height();
                            $hHeight = ($(this).find('h1').height()+8);
                            $(this).attr('target',$(this).height());
                            $(this).attr('rel',$hHeight);
                            $(this).height($hHeight).css('overflow','hidden');
                        }
                    });
                                    
                    $('#centerpiece-477792 .slider-display').hover(
                        function (){
                            $(this).animate({height: $(this).attr('target')}, {queue:false,duration:300}, 'linear');
                        }, 
                        function () {
                            $(this).animate({height: $(this).attr('rel')}, {queue:false,duration:300}, 'linear');
                        }
                    );
                
            });
