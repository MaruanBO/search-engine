            function OpenAFHSPromoBottomPage(relativeURL) {
                var baseURL = 'http://www.ashleyfurniturehomestore.com';
                    if (!baseURL.endsWith("/")) {
                        baseURL = baseURL + "/";
                    }
                    if (relativeURL.toString().startsWith("/")) {
                        relativeURL = relativeURL.toString().substring(1, relativeURL.toString().length);
                    }
                    var completeAFHSURL = baseURL + relativeURL;
                    window.location.href = completeAFHSURL;
                }
