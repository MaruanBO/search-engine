        function OpenApplyForCredit(countryName) {
            if (countryName == 'US') {
                var viewerURL = 'https://app.citifinancial-sf.com/index.html?dealer=77007&store=36878&amount=500';
            } else {
                var viewerURL = 'https://app.citifinancial-sf.com/can/eng/index.php?visionid=910009864&amount=50';
            };
            var winLeft = (screen.width - 800) / 2;
            var winTop = (screen.height - 600) / 2;

            var viewerArgs = 'scrollbars=1';
            viewerArgs = viewerArgs + ',width=' + 800
            viewerArgs = viewerArgs + ',height=' + 600
            viewerArgs = viewerArgs + ',left=' + winLeft;
            viewerArgs = viewerArgs + ',top=' + winTop;
            viewerArgs = viewerArgs + ',resizable=1,status=1,toolbar=1,menubar=1,modal=no';

            window.open(viewerURL, '', viewerArgs);
        }

