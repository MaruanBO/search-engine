        function roll_over(img_name, img_src) {
            var objectImg;
            objectImg = document.getElementById(img_name);
            if (objectImg != null) {
                objectImg.src = img_src;
                objectImg.style.width = "100px";
                objectImg.style.height = "70px";
            }
        }

