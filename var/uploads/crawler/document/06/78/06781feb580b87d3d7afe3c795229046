                Sys.WebForms.PageRequestManager.getInstance().add_endRequest(globalEndRequest);
                function globalEndRequest(sender, e) {
                    if (e.get_error()) {
                        showError(e.get_error().description);
                        e.set_errorHandled(true);
                    }
                    LoadHeaderYourHomeStore();
                    $(".changeSerchBoxclass").keypress(function (event) {
                        var key = event.keyCode || event.charCode;
                        //if(event.keyCode == 13) {
                        if (key == 13) {
                            fnChangeSearchLocator();
                            event.preventDefault();
                        }
                    });
                }
                function showError(errorText) {
                    $get('ajaxErrorMessage').innerHTML = (errorText) ? errorText : "An error occurred while processing your request. Please try again.";
                    $get('modalPage').style['display'] = 'block';
                    $get('ajaxError').style['display'] = 'block';
                }
                function hideError() {
                    $get('modalPage').style['display'] = 'none';
                    $get('ajaxError').style['display'] = 'none';
                }
                function TrackSiteCatalyst(event, link) {
                    var s = s_gi('ashleyhomestoreprod');
                    s.server = 'ashleyfurniturehomestore.com';
                    s.linkTrackVars = 'events';
                    s.linkTrackEvents = event;
                    s.events = event;
                    s.tl(this, 'o', link);
                }
