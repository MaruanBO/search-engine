            if (e.keyCode == 27) {
                if (emailOverLay == true) {
                    var nothanksdelay = $get('ctl00_ctl00_ctl00_hdnnothanks').value;
                    setCookie("afhs:emailoverlay:nothanks", "true", nothanksdelay);
                    var behavior = $find('ModalEmailOverlay')
                    behavior.hide();
                }
            }

        });


        if ($get('ctl00_ctl00_ctl00_EmailSignUp').value == "true") {

            var delay = $get('ctl00_ctl00_ctl00_EmailSignUpDelay').value
            setTimeout(function () {
                var behavior = $find('ModalEmailOverlay')
                behavior.show();
                emailOverLay = true;
            }, delay);


        }

        function setOverlayCookies(cookiename) {

            if (cookiename == "xout") {
                var xoutdelay = $get('ctl00_ctl00_ctl00_hdnxout').value;
                setCookie("afhs:emailoverlay:" + cookiename, "true", xoutdelay);
                $find('ModalEmailOverlay').hide();
                emailOverLay = false;
            }
            else if (cookiename == "onlist") {
                var onlistdelay = $get('ctl00_ctl00_ctl00_hdnonlist').value;
                setCookie("afhs:emailoverlay:" + cookiename, "true", onlistdelay);
                $find('ModalEmailOverlay').hide();
                emailOverLay = false;
            }
            else if (cookiename == "nothanks") {
                var nothanksdelay = $get('ctl00_ctl00_ctl00_hdnnothanks').value;
                    setCookie("afhs:emailoverlay:" + cookiename, "true", nothanksdelay);
                    $find('ModalEmailOverlay').hide();
                    emailOverLay = false;
                }
        return false;

    }

    Date.prototype.addHours = function (h) {
        this.setTime(this.getTime() + (h * 60 * 60 * 1000));
        return this;
    }

    function setCookie(c_name, value, exhrs) {
        var exdate = new Date().addHours(exhrs);
        var c_value = escape(value) + ((exhrs == null) ? "" : "; expires=" + exdate.toUTCString());
        document.cookie = c_name + "=" + c_value + "; path=/";
    }

    function getCookie(c_name) {
        var c_value = document.cookie;
        var c_start = c_value.indexOf(" " + c_name + "=");
        if (c_start == -1) {
            c_start = c_value.indexOf(c_name + "=");
        }
        if (c_start == -1) {
            c_value = null;
        }
        else {
            c_start = c_value.indexOf("=", c_start) + 1;
            var c_end = c_value.indexOf(";", c_start);
            if (c_end == -1) {
                c_end = c_value.length;
            }
            c_value = unescape(c_value.substring(c_start, c_end));
        }
        return c_value;
    }
    function showEmailSignUp() {

        var behavior = $find('ModalEmailOverlay')
        var emailAddress = $get('ctl00_ctl00_ctl00_ucModalEmailOverlay_txtEmailAddress').value;

        var reg = /^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/
        if (reg.test(emailAddress)) {
            $get('ctl00_ctl00_ctl00_ucModalEmailOverlay_lblErrorMessage').style.display = "none";
            behavior.hide();
            emailOverLay = false;
            showEmailExclusivesPopupPanelWithEmail(emailAddress);
        }
        else {
            $get('ctl00_ctl00_ctl00_ucModalEmailOverlay_lblErrorMessage').style.display = "inline";

        }


        return false;
    }

    function hideEmailSignUp(cookiename) {
        if (cookiename == "xout") {
            var xoutdelay = $get('ctl00_ctl00_ctl00_hdnxout').value;
            setCookie("afhs:emailoverlay:" + cookiename, "true", xoutdelay);
            $find('ModalEmailExclusives').hide();
        }
        else if (cookiename == "nothanks") {
            var nothanksdelay = $get('ctl00_ctl00_ctl00_hdnnothanks').value;
                setCookie("afhs:emailoverlay:" + cookiename, "true", nothanksdelay);
                $find('ModalEmailExclusives').hide();
            }
        return false;

    }

    function showEmailExclusives() {
        var emailAddress = '';
        var activeTab = $get('ctl00_ctl00_ctl00_hidActiveTabIndex').value;
        switch (activeTab) {
            case "0":
                emailAddress = $get('ctl00_ctl00_ctl00_inpInfoEmail').value;
                break;
            default:
        }
        showEmailExclusivesPopupPanelWithEmail(emailAddress);
    }
    function showEmailExclusivesPopupPanel() {
        var behavior = $find('ModalEmailExclusives')
        behavior.show();
    }
    function showEmailExclusivesPopupPanelWithEmail(emailAddress) {
        var behavior = $find('ModalEmailExclusives')
        var emailAddressTextBox = $get('ctl00_ctl00_ctl00_ucModalEmailExclusives_txtEmailAddress');
        if (emailAddress == 'Your Email Address') {
            emailAddressTextBox.value = '';
        }
        else {
            emailAddressTextBox.value = emailAddress;
        }
        behavior.show();
    }
    function hideEmailExclusivesPopupPanel() {
        var behavior = $find('ModalEmailExclusives')
        behavior.hide();
    }


    </script>
