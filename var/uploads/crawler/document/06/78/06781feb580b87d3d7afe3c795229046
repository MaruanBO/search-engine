<script type="text/javascript">
    var scrollCount = 0;
    var curImage = 0;
    var isProcessing = 0;
    var waitMiliSec = 1100;
    var objInterval;
    var imageBoxInside = $('[id$=imageBoxInside]').attr("id");
    var imageCount = document.getElementById("ctl00_ctl00_ctl00_content_content_content_ShopByCollectionSlider_imageCount").value
    // debugger;
    function moveToPrevious() {
        if (isProcessing == 0) {
            setIsProcessing(1, 0);
            if (scrollCount > 0) {
                new Effect.Move(imageBoxInside, {
                    x: 328,
                    y: 0,
                    transition: Effect.Transitions.sinoidal,
                    mode: "relative"
                });
                new Effect.Move("imageBox", {
                    x: 0,
                    y: 0,
                    transition: Effect.Transitions.sinoidal,
                    mode: "absolute"
                });
                $("#imgNext").attr("src", "../images/collbanner_right.png");
                $('#imgNext').bind('click');
                scrollCount -= 1;
                if (scrollCount <= 0) {

                    $("#imgPrev").attr("src", "../images/imageScrollerPrevDisabled.png");
                    $('#imgPrev').unbind('click');
                }
            } else {
                $("#imgPrev").attr("src", "../images/imageScrollerPrevDisabled.png")
                $('#imgPrev').unbind('click');
            }
            objInterval = setInterval(function() {
                setIsProcessing(0, 1)
            }, waitMiliSec)
        }
    }
    function moveToNext() {
        if (isProcessing == 0) {
            setIsProcessing(1, 0);
            if (scrollCount < parseInt(imageCount, 10) - 3) {
                new Effect.Move(imageBoxInside, {
                    x: -328,
                    y: 0,
                    transition: Effect.Transitions.sinoidal,
                    mode: "relative"
                });
                new Effect.Move("imageBox", {
                    x: 0,
                    y: 0,
                    transition: Effect.Transitions.sinoidal,
                    mode: "absolute"
                });
                $("#imgPrev").attr("src", "../images/collbanner_left.png");
                $('#imgPrev').bind('click');
                scrollCount += 1;
                var breakUpCount = parseInt(imageCount, 10) - 3;
                // alert("scrollCount " + scrollCount + "break COunt " + breakUpCount);
                if (scrollCount >= breakUpCount) {
                    $("#imgNext").attr("src", "../images/imageScrollerNextDisabled.png");
                    $('#imgNext').unbind('click');
                }
            } else {

                $("#imgNext").attr("src", "../images/imageScrollerNextDisabled.png");
                $('#imgNext').unbind('click');
            }
            objInterval = setInterval(function() {
                setIsProcessing(0, 1)
            }, waitMiliSec)
        }
    }
    function setIsProcessing(a, b) {
        isProcessing = a;
        if (b == 1) {
            clearInterval(objInterval)
        }
    }

</script>



