<script language="javascript" type="text/javascript">

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }


    $(document).ready(function () {
        LoadHeaderYourHomeStore();
    });

    function LoadHeaderYourHomeStore() {

        $("#changeLidiv").show();
        $("#divStorelocator").show();
        $('#divFieldSet').height('0px');
        $(".dataYourHomeStoreSummary").click(function (e) {
            e.stopPropagation();
            $("#yourHomeStoreSummary").toggle();
            $(".insides-hidden").hide();
            $(".change").removeClass("active");
            $('.locator').toggleClass("active");
        });



        $("#yourHomeStoreSummary").click(function (e) {
            e.stopPropagation();
        });

        $(document).click(function () {
            $('#yourHomeStoreSummary').hide();
            $('.locator').removeClass("active");
        });

        $(".change").click(function (e) {
            e.stopPropagation();
            $(".insides-hidden").toggle();
            $("#yourHomeStoreSummary").hide();
            $('.locator').removeClass("active");
            $(".change").toggleClass("active");

            $(".denystorelocator").toggleClass("active");
        });

        $(".denystorelocator").click(function (e) {
            e.stopPropagation();
            $(".insides-hidden").toggle();
            $("#yourHomeStoreSummary").hide();
            $('.locator').removeClass("active");
            $(".change").toggleClass("active");

            $(".denystorelocator").toggleClass("active");
        });

        $(".insides-hidden").click(function (e) {
            e.stopPropagation();
        });
        $(document).click(function () {
            $('.insides-hidden').hide();
            $(".change").removeClass("active");
            $(".denystorelocator").removeClass("active");
        });

        $('#footer_contactusid').hide();
        var SessionID = "aak22szr0dlir555wrgiwlvc";
        var cookVal = readCookie("afhs:geolocation:inheader");
        if (cookVal != null) {
            var oob = JSON.parse(cookVal);
            if (oob.ContactEmail != "" || oob.contPh != "") {
                bindStoreAddFromResp(oob, 1);
            }
            else {
                bindStoreAddFromResp(oob, 0);
            }

            $('.printNot').removeClass('denystorelocator');
            $('.printNot').addClass('change');
            $('.drop').css('top', '45px');
            $('.storeLocator').removeAttr('style');
            $('.locator').removeAttr('style');
            $('.printNot').removeAttr('style');

        }
        else {
            var cookSetLoc = readCookie("setGeoLoc");
            if (cookSetLoc == null) {
                AskTracingLocation();
            }

        }

    }

    function AskTracingLocation() {

        var options = {
            timeout: 7000
        };
        if (navigator.geolocation) {


            var uaForFindSafa = navigator.userAgent.toLowerCase();
            if (uaForFindSafa.indexOf("safari/") !== -1 && uaForFindSafa.indexOf("windows") !== -1 && navigator.userAgent.search("Chrome") < 0) {
                //alert("You are using Safari!");
                navigator.geolocation.getCurrentPosition(function (position) { }, function (erro) {
                    if (erro.code == 3 || erro.code == 2) {
                        handleNoGeolocation();
                    }

                }, options);
            }
            else {

                navigator.geolocation.getCurrentPosition(function (position) {
                    $('.printNot').removeClass('denystorelocator');
                    $('.printNot').addClass('change');
                    $('.drop').css('top', '45px');
                    $('.storeLocator').removeAttr('style');
                    $('.locator').removeAttr('style');
                    $('.printNot').removeAttr('style');

                    $('#headfiveid').show();



                    var clintUr = 'Common/Common.ashx/GetGeolocateDetails'
                    SessionID = "aak22szr0dlir555wrgiwlvc";

                    clintUr = clintUr + "?lat=" + position.coords.latitude + "&lang=" + position.coords.longitude + "&sess=" + SessionID;


                    var cookVal = readCookie("afhs:geolocation:inheader");
                    if (cookVal != null) {
                        var oob = JSON.parse(cookVal);

                        if (cookVal != null) {

                            if (oob.ContactEmail != "" || oob.contPh != "") {
                                $('#footer_contactusid').show();

                                if (oob.ContactEmail != "") {
                                    $('#aGeoContactEmail').show();
                                }
                                else {
                                    $('#aGeoContactEmail').hide();
                                }

                                if (oob.contPh != "") {
                                    $('#ctl00_ctl00_ctl00_lblGeoContactUs').show();
                                }
                                else {
                                    $('#ctl00_ctl00_ctl00_lblGeoContactUs').hide();
                                }
                            }
                            else {
                                $('#footer_contactusid').hide();

                            }
                        }
                        else {
                            $.ajax({
                                type: "POST",
                                url: clintUr,
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                async: false,
                                cache: false,
                                success: function (msg) {
                                    // alert('succes');
                                    var sRes = JSON.parse(msg);
                                    if (sRes.ContactEmail != "" || sRes.contPh != "") {
                                        bindStoreAddFromResp(sRes, 1);
                                    }
                                    else {
                                        bindStoreAddFromResp(sRes, 0);

                                    }

                                },
                                error: function (msg) {
                                    handleNoGeolocation();
                                    displayStorelocatorOnly();
                                }

                            });
                        }
                    }
                    else {
                        $.ajax({
                            type: "POST",
                            url: clintUr,
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            async: false,
                            cache: false,
                            success: function (msg) {
                                var sRes = JSON.parse(msg);

                                if (sRes.ContactEmail != "" || sRes.contPh != "") {
                                    bindStoreAddFromResp(sRes, 1);
                                }
                                else {
                                    bindStoreAddFromResp(sRes, 0);

                                }
                            },
                            error: function (msg) {
                                handleNoGeolocation();
                                displayStorelocatorOnly();
                            }

                        });
                    }




                }
                , function () {
                    if (cookVal != null) {
                    } else {
                        $('#changeLidiv').html("Store Locator");
                        $('#headfiveid').hide();
                        $('.storeLocator').css('top', '17px');
                        $('.storeLocator ul li').css('top', '-3px');
                        $('.storeLocator ul li').css('left', '-7px');
                        $('.storeLocator ul li').css('position', 'relative');
                        $('.storeLocator').css('background-position', '0px');
                        $('.storeLocator').css('margin-left', '140px');
                        $('.printNot').removeClass('change');
                        $('.printNot').addClass('denystorelocator');
                        $('.drop').css('top', '33px');

                        handleNoGeolocation();
                    }
                });
            }
        } else {
            // Browser doesn't support Geolocation
            // //alert('2- deny');
            $('#changeLidiv').html("Store Locator");
            $('#headfiveid').hide();
            $('.drop').css('top', '33px');
            $('.storeLocator').css('top', '17px');
            $('.storeLocator .change.active a:after').css('bottom', '-12px');
            handleNoGeolocation();
        }
    }



    function displayStorelocatorOnly() {
        $('#changeLidiv').html("Store Locator");
        $('#headfiveid').hide();
        $('.storeLocator').css('top', '17px');
        $('.storeLocator ul li').css('top', '-3px');
        $('.storeLocator ul li').css('left', '-7px');
        $('.storeLocator ul li').css('position', 'relative');
        $('.storeLocator').css('background-position', '0px');
        $('.storeLocator').css('margin-left', '140px');
        $('.printNot').removeClass('change');
        $('.printNot').addClass('denystorelocator');
        $('.drop').css('top', '33px');
    }
    function handleNoGeolocation() {
        //alert('no geo support');
        $('#lblGeolocatTraceErr').show();
        setTimeout(function () { $('#lblGeolocatTraceErr').hide(); }, 3000);
        document.cookie = "setGeoLoc=false";
    }

    function bindAfterChange() {
        var cookVal = readCookie("afhs:geolocation:inheader");

        if (cookVal != null) {
            var oob = JSON.parse(cookVal);
            //bindStoreAddFromResp(oob);
        }

    }

    function bindStoreAddFromResp(obj, contactFlag) {

        $('#ctl00_ctl00_ctl00_content_topNav_lblStateCode').html(obj.stCode);
        $('#ctl00_ctl00_ctl00_content_topNav_lblCityHdrAndStateHdr').html(obj.city + ", " + obj.stCode)

        $('#ctl00_ctl00_ctl00_content_topNav_lblCityPopupAndStatePopup').html(obj.city + ", " + obj.stCode);
        if (obj.strAlias.length > 0) {
            $('#ctl00_ctl00_ctl00_content_topNav_lblCityPopupAndStatePopup').attr('href', "/storelocator/StoreDetail.aspx?store=" + obj.strAlias);
        }
        else {
            $('#ctl00_ctl00_ctl00_content_topNav_lblCityPopupAndStatePopup').attr('href', "#");
        }

        var formAddr = '';
        if (obj.counCod.toLowerCase() == 'usa' || obj.counCod.toLowerCase() == 'can') {
            if ((obj.add1 != "" || obj.add1 != null) && isNaN(obj.add1.substring(0, 1)) == false) {

                formAddr = obj.add1;
            }
            else if ((obj.add2 != "" || obj.add2 != null) && isNaN(obj.add2.substring(0, 1)) == false) {
                formAddr = obj.add2;
            }

        }
        else {
            formAddr = obj.add1 + '<br />' + obj.add2 + '<br />';
        }
        $('#ctl00_ctl00_ctl00_content_topNav_lblStorAddres').html(formAddr);

        $('#ctl00_ctl00_ctl00_content_topNav_lblCity').html(obj.city);

        $('#ctl00_ctl00_ctl00_content_topNav_lblStateCode').html(obj.stCode);


        $('#ctl00_ctl00_ctl00_content_topNav_lblPhoneNos').html("");

        if ((typeof obj.contPh != "undefined") && (obj.contPh != null && obj.contPh != "")) {
            $('#ctl00_ctl00_ctl00_content_topNav_lblPhoneNos').html("<br />" + "Store Phone: " + obj.contPh );
        }
        if ((typeof obj.custServPh != "undefined") && (obj.custServPh != null && obj.custServPh != "")) {
            $('#ctl00_ctl00_ctl00_content_topNav_lblPhoneNos').html($('#ctl00_ctl00_ctl00_content_topNav_lblPhoneNos').html() + "<br />" + "Customer Service: " + obj.custServPh);
        }
        

        $('#ctl00_ctl00_ctl00_content_topNav_lblZipCode').html(obj.zip);


        if (contactFlag == 1) {
            $('#footer_contactusid').show();
            var objContactPhone = null
            if (obj.contPh != null)
                objContactPhone = obj.contPh.split("-", 1);
            var objOrginalValue = null;
            if (objContactPhone != null) {
                objOrginalValue = obj.contPh.replace(objContactPhone + "-", "(" + objContactPhone + ") ");
                document.getElementById("ctl00_ctl00_ctl00_lblGeoContactUs").innerHTML = objOrginalValue;
                
                document.getElementById("aGeoContactEmail").href = "mailto:" + obj.ContactEmail + "";
                if (obj.ContactEmail != "") {
                    $('#aGeoContactEmail').show();
                }
                else {
                    $('#aGeoContactEmail').hide();
                }

                if (obj.contPh != "") {
                    $('#ctl00_ctl00_ctl00_lblGeoContactUs').show();
                }
                else {
                    $('#ctl00_ctl00_ctl00_lblGeoContactUs').hide();
                }
            }
        }
        else {
            $('#footer_contactusid').hide();
        }


        ////alert(document.getElementById("ctl00_ctl00_ctl00_lblGeoContactUs").innerHTML)

        var content = '';

        //if (obj.monOp != null) {
        //    content = "<dt>Mon-Fri</dt><dd class='textRight'>" + obj.monOp + "-" + obj.friCl + "</dd>"
        //}

        //if (obj.satOp != null) {
        //    content += "<dt>Sat</dt><dd class='textRight'>" + obj.satOp + "-" + obj.satCl + "</dd>"
        //}
        //if (obj.sunOp != null) {
        //    content += "<dt>Sun</dt><dd class='textRight'>" + obj.sunOp + "-" + obj.sunCl + "</dd>"
        //}
        ////alert(content);

        // $('#StoreTimings').html(content);
        $('#StoreTimings').html(frameStoreTimings(obj));

        $("#storeWeeklyAdd").click(function () {
            //StoreListTracking(obj.strAlias, 'event31', 'StoreList', 'WeeklyAd', GetSiteCatalystServer(), GetSiteCatalystReportSuitId());
        });
      
        if (obj.weekUrl != "") {
            $("#storeWeeklyAdd").show();
            $("#storeWeeklyAdd").addClass("mu");
            $("#storeWeeklyAdd").click(function () {
                OpenWeeklyAd(obj.StrFrntId);

            });
        }
        else {
            $("#storeWeeklyAdd").hide();
        }

        if (obj.coupUrl != "") {
            $("#storeCoupon").show();
            $("#storeCoupon").click(function () {
                window.open("/storelocator/CouponDisplay.aspx?custno=" + obj.custNo + "&shipto=" + obj.shipTo);
            });
        } else {
            $("#storeCoupon").hide();
        }
        if (obj.strAlias != "") {
            $("#sotreGetDirections").show();
            $("#sotreGetDirections").addClass("mu");
            $('#sotreGetDirections').click(function () {
                window.location = ("/storelocator/StoreDetail.aspx?store=" + obj.strAlias + "&state=" + "&direction=true");
            });
        }
        else {
            $("#sotreGetDirections").hide();
        }

        $('#headfiveid').show();
        $('#changeLidiv').html("Change");

        document.getElementById('ctl00_ctl00_ctl00_content_topNav_hdfFlagToLoadStore').value = "1";
    }

    $(".changeSerchBoxclass").keypress(function (event) {
        var key = event.keyCode || event.charCode;
        //if (event.keyCode == 13) {
        if (key == 13) {
            $('#divStoreList').height('100px');
            $('#miniSearchStoreResults').hide();
            $('.yourHomeStore').removeClass('sd_tick_bg');
            $('#loading').show();

            fnChangeSearchLocator();

            setTimeout("$('#loading').hide(); $('#miniSearchStoreResults').show();$('.nano').nanoScroller();", 500);
            event.preventDefault();
        }
    });

    function frameStoreTimings(objTime) {

        if (typeof objTime.monOp === "undefined") {
            objTime.monOp = '-';

        }
        else {
            objTime.monOp = objTime.monOp.toString().toLowerCase();
        }
        if (typeof objTime.tueOp === "undefined") {
            objTime.tueOp = '-';

        }
        else {
            objTime.tueOp = objTime.tueOp.toString().toLowerCase();
        }
        if (typeof objTime.wedOp === "undefined") {
            objTime.wedOp = '-';

        }
        else {
            objTime.wedOp = objTime.wedOp.toString().toLowerCase();
        }
        if (typeof objTime.thuOp === "undefined") {
            objTime.thuOp = '-';

        }
        else {
            objTime.thuOp = objTime.thuOp.toString().toLowerCase();
        }
        if (typeof objTime.friOp === "undefined") {
            objTime.friOp = '-';

        }
        else {
            objTime.friOp = objTime.friOp.toString().toLowerCase();
        }



        if (typeof objTime.monCl === "undefined") {
            objTime.monCl = '-';

        }
        else {
            objTime.monCl = objTime.monCl.toString().toLowerCase();
        }
        if (typeof objTime.tueCl === "undefined") {
            objTime.tueCl = '-';
            

        }
        else {
            objTime.tueCl = objTime.tueCl.toString().toLowerCase();
        }

        if (typeof objTime.wedCl === "undefined") {
            objTime.wedCl = '-';

        }
        else {
            objTime.wedCl = objTime.wedCl.toString().toLowerCase();
        }
        if (typeof objTime.thuCl === "undefined") {
            objTime.thuCl = '-';

        }
        else {
            objTime.thuCl = objTime.thuCl.toString().toLowerCase();
        }
        if (typeof objTime.friCl === "undefined") {
            objTime.friCl = '-';


        }
        else {
            objTime.friCl = objTime.friCl.toString().toLowerCase();

        }

        if (typeof objTime.satOp === "undefined") {
            objTime.satOp = '-';


        }
        else {
            objTime.satOp = objTime.satOp.toString().toLowerCase();

        }

        if (typeof objTime.sunOp === "undefined") {
            objTime.sunOp = '-';


        }
        else {
            objTime.sunOp = objTime.sunOp.toString().toLowerCase();

        }

        if (typeof objTime.satCl === "undefined") {
            objTime.satCl = '-';


        }
        else {
            objTime.satCl = objTime.satCl.toString().toLowerCase();

        }

        if (typeof objTime.sunCl === "undefined") {
            objTime.sunCl = '-';


        }
        else {
            objTime.sunCl = objTime.sunCl.toString().toLowerCase();

        }





        var FinalTimes = " ";

        if ((objTime.monOp == objTime.tueOp == objTime.wedOp == objTime.thuOp == objTime.friOp) && (objTime.monCl == objTime.tueCl == objTime.wedCl == objTime.thuCl == objTime.friCl)) {
            FinalTimes += '<dt>Mon-Fri</dt> <dd class="textRight">' + objTime.monOp + ' - ' + objTime.monCl + '</dd>';
        }
        else {
            if (objTime.monOp == objTime.tueOp && objTime.monCl == objTime.tueCl) {
                if (objTime.tueOp == objTime.wedOp && objTime.tueCl == objTime.wedCl) {

                    if (objTime.wedOp == objTime.thuOp && objTime.wedCl == objTime.thuCl) {

                        //FinalTimes += '<dt>Mon-Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Mon-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            // FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Mon-Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                        }
                    }
                    else {
                        FinalTimes += '<dt>Mon-Wed</dt> <dd class="textRight">' + objTime.wedOp + ' - ' + objTime.wedCl + '</dd>';

                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                        }

                    }
                }
                else {
                    FinalTimes += '<dt>Mon-Tue</dt> <dd class="textRight">' + objTime.tueOp + ' - ' + objTime.tueCl + '</dd>';

                    if (objTime.tueOp == objTime.wedOp && objTime.tueCl == objTime.wedCl) {

                        if (objTime.wedOp == objTime.thuOp && objTime.wedCl == objTime.thuCl) {

                            FinalTimes += '<dt>Wed-Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';

                            if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                                FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                            }
                            else {
                                FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                                FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                            }
                        }
                        else {
                            FinalTimes += '<dt>Wed</dt> <dd class="textRight">' + objTime.wedOp + ' - ' + objTime.wedCl + '</dd>';
                            if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                                FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                            }
                            else {
                                FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                                FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                            }
                        }
                    }
                    else {
                        FinalTimes += '<dt>Wed</dt> <dd class="textRight">' + objTime.wedOp + ' - ' + objTime.wedCl + '</dd>';



                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                        }
                    }

                }
            }
            else {
                FinalTimes += '<dt>Mon</dt> <dd class="textRight">' + objTime.monOp + ' - ' + objTime.monCl + '</dd>';


                if (objTime.tueOp == objTime.wedOp && objTime.tueCl == objTime.wedCl) {

                    if (objTime.wedOp == objTime.thuOp && objTime.wedCl == objTime.thuCl) {

                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Tue-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Wed-Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }



                    }
                    else {
                        FinalTimes += '<dt>Tue-Wed</dt> <dd class="textRight">' + objTime.wedOp + ' - ' + objTime.wedCl + '</dd>';
                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                    }
                }
                else {
                    FinalTimes += '<dt>Tue</dt> <dd class="textRight">' + objTime.tueOp + ' - ' + objTime.tueCl + '</dd>';

                    if (objTime.wedOp == objTime.thuOp && objTime.wedCl == objTime.thuCl) {

                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Wed-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Wed-Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                    }
                    else {
                        FinalTimes += '<dt>Wed</dt> <dd class="textRight">' + objTime.wedOp + ' - ' + objTime.wedCl + '</dd>';

                        if (objTime.thuOp == objTime.friOp && objTime.thuCl == objTime.friCl) {
                            FinalTimes += '<dt>Thu-Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';

                        }
                        else {
                            FinalTimes += '<dt>Thu</dt> <dd class="textRight">' + objTime.thuOp + ' - ' + objTime.thuCl + '</dd>';
                            FinalTimes += '<dt>Fri</dt> <dd class="textRight">' + objTime.friOp + ' - ' + objTime.friCl + '</dd>';
                        }
                    }


                }



            }

        }

        //FinalTimes += '<dt>Sat</dt> <dd class="textRight">' + objTime.satOp.toString().toLowerCase() + ' - ' + objTime.satCl.toString().toLowerCase() + '</dd>';
        //FinalTimes += '<dt>Sun</dt> <dd class="textRight">' + objTime.sunOp.toString().toLowerCase() + ' - ' + objTime.sunCl.toString().toLowerCase() + '</dd>';
        if (objTime.satCl.toString().toLowerCase() != '') {
            FinalTimes += '<dt>Sat</dt> <dd class="textRight">' + objTime.satOp.toString().toLowerCase() + ' - ' + objTime.satCl.toString().toLowerCase() + '</dd>';
        }
        else {
            FinalTimes += '<dt>Sat</dt> <dd class="textRight">' + objTime.satOp.toString().toLowerCase() + '</dd>';
        }


        if (objTime.sunCl.toString().toLowerCase() != '') {
            FinalTimes += '<dt>Sun</dt> <dd class="textRight">' + objTime.sunOp.toString().toLowerCase() + ' - ' + objTime.sunCl.toString().toLowerCase() + '</dd>';
        }
        else {
            FinalTimes += '<dt>Sun</dt> <dd class="textRight">' + objTime.sunOp.toString().toLowerCase() + '</dd>';
        }
        return FinalTimes;

    }

    function fnChangeSearchLocator() {
        //if (event.keyCode == 13) {

        var clintUrChange = 'Common/Common.ashx/GetGeolocateDetails_Change'
        clintUrChange = clintUrChange + "?searchStr=" + $('#siteFindHomeStoreId').val();
        $.ajax({
            type: "POST",
            url: clintUrChange,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: false,
            cache: false,
            success: function (msg) {

                var sRes = JSON.parse(msg);
                $(".changeSerchBoxclass").show();

                //Loop Vi
                var contInc = 0;
                for (var i = 0; i < sRes.length; i++) {
                    contInc = i + 1;
                    $('#store' + contInc).show();
                    $('#divStoreList').css('height', '300px');
                    $('#city' + contInc).html(sRes[i].city + ', ' + sRes[i].stCode);
                    if (sRes[i].strAlias != null && sRes[i].strAlias.length > 0) {
                        $('#city' + contInc).attr('href', "/storelocator/StoreDetail.aspx?store=" + sRes[i].strAlias);
                }
                else {
                    $('#city' + contInc).attr('href', "#");
                }
                if (sRes[i].counCod.toLowerCase() != 'can') {
                    $('#miles' + contInc).html(parseInt(sRes[i].proxDist) + " Miles away");
                }
                else {
                    $('#miles' + contInc).html(parseInt(sRes[i].proxDist) + " Kilometers away");
                }
                var contactPhone = sRes[i].contPh;
                var monOpen = sRes[i].monOp;
                var monClose = sRes[i].friCl;
                var satOpen = sRes[i].satOp;
                var satClose = sRes[i].satCl;
                var sunOpen = sRes[i].sunOp;
                var sunClose = sRes[i].sunCl;
                if (typeof contactPhone === "undefined") {
                    contactPhone = '';
                }
                if (typeof monOpen === "undefined") {
                    monOpen = '-';
                }
                if (typeof monClose === "undefined") {
                    monClose = '-';
                }
                if (typeof satOpen === "undefined") {
                    satOpen = '-';
                }
                if (typeof satClose === "undefined") {
                    satClose = '-';
                }
                if (typeof sunOpen === "undefined") {
                    sunOpen = '-';
                }
                if (typeof sunClose === "undefined") {
                    sunClose = '-';
                }
                $('#strTimings' + contInc).html(frameStoreTimings(sRes[i]));

                $('#pMoreStore').removeClass('hide');
                $('#divFieldSet').height('auto');
                var formAddr = '';
                if (sRes[i].counCod.toLowerCase() == 'usa' || sRes[i].counCod.toLowerCase() == 'can') {
                    if ((sRes[i].add1 != "" || sRes[i].add1 != null) && isNaN(sRes[i].add1.substring(0, 1)) == false) {

                        formAddr = sRes[i].add1 + '<br />';
                    }
                    else if ((sRes[i].add2 != "" || sRes[i].add2 != null) && isNaN(sRes[i].add2.substring(0, 1)) == false) {
                        formAddr = sRes[i].add2 + '<br />';
                    }

                }
                else {
                    formAddr = sRes[i].add1 + '<br />' + sRes[i].add2 + '<br />';
                }
                $('#address' + contInc).html(formAddr + sRes[i].city + ', ' + sRes[i].stCode + ' ' + sRes[i].zip + '<br />' + contactPhone);
                //$('#dtMon' + contInc).html('Mon-Fri');

                //$('#ddMonVal' + contInc).html(monOpen + '-' + monClose);
                //$('#dtsat' + contInc).html('Sat');
                //$('#ddsatVal' + contInc).html(satOpen + '-' + satClose);
                //$('#dtsun' + contInc).html('Sun');
                //$('#ddsunVal' + contInc).html(sunOpen + '-' + sunClose);
                $('#aGet' + contInc).html('Get Directions');
                $('#aMake' + contInc).html('Make this your HomeStore');

                if (sRes[i].strAlias != "") {
                    $('#aGet' + contInc).attr('href', "/storelocator/StoreDetail.aspx?store=" + sRes[i].strAlias + "&state=" + "&direction=true");
                    $('#aGet' + contInc).show();
                    $('#spanGet' + contInc).show();
                }
                else {
                    $('#aGet' + contInc).hide();
                    $('#spanGet' + contInc).hide();
                }
                var sessionIdVal = "aak22szr0dlir555wrgiwlvc";



                    $('#aMake' + contInc).click(function () {

                        var cntrInc = this.id.slice(-1)
                        var URLIndex = (cntrInc - 1);
                        var clintUr1 = 'Common/Common.ashx/GetGeolocateDetails'
                         clintUr1 = clintUr1 + "?lat=" + sRes[URLIndex].lat + "&lang=" + sRes[URLIndex].long + "&sfrontid=" + sRes[URLIndex].StrFrntId + "&sess=" + sessionIdVal;
                        //clintUr1 = clintUr1 + "?lat=" + sRes[contInc - 1].lat + "&lang=" + sRes[contInc - 1].long + "&sess=" + sessionIdVal;

                        $.ajax({
                            type: "POST",
                            url: clintUr1,
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            async: false,
                            cache: false,
                            success: function (msg) {
                                if (msg != "") {
                                    var sRes = JSON.parse(msg);

                                    if (sRes.ContactEmail != "" || sRes.contPh != "") {
                                        bindStoreAddFromResp(sRes, 1);
                                    }
                                    else {
                                        bindStoreAddFromResp(sRes, 0);

                                    }

                                    $('.yourHomeStore').html('Make this your HomeStore');
                                    $('.yourHomeStore').removeClass('sd_tick_bg');
                                    $('#aMake' + cntrInc).addClass('sd_tick_bg');
                                    $('#aMake' + cntrInc).html('Your HomeStore');

                                    $('.printNot').removeClass('denystorelocator');
                                    $('.printNot').addClass('change');
                                    $('.drop').css('top', '45px');
                                    $('.storeLocator').removeAttr('style');
                                    $('.locator').removeAttr('style');
                                    $('.printNot').removeAttr('style');
                                  
                                    $('.insides-hidden').hide();
                                    $(".change").removeClass("active");
                                    $(".denystorelocator").removeClass("active");
                                }
                            },
                            error: function (msg) {
                                //alert('m1 f');
                            }
                        });
                    });

                    // for consistant behaviour of 'this is your homestore'
                    if ($('#ctl00_ctl00_ctl00_content_topNav_lblCityHdrAndStateHdr').html() == $('#city' + contInc).html()) {
                        $('.yourHomeStore').html('Make this your HomeStore');
                        $('.yourHomeStore').removeClass('sd_tick_bg');
                        $('#aMake' + contInc).addClass('sd_tick_bg');
                        $('#aMake' + contInc).html('Your HomeStore');

                    }
                }
                //Loop Vi     
                

                if (sRes.length > 0) {
                    $('#lnkFindMoreStores_change').attr('href', "/storelocator/StoreLocator.aspx?userinput=" + $('#siteFindHomeStoreId').val());
                }
                else {
                    $('#lnkFindMoreStores_change').attr('href', "/storelocator/StoreLocator.aspx");
                }

            },
            error: function (msg) {
            }

        });

    }



    function switchLanguage(lang) {
        MP.SrcUrl = unescape('mp_js_orgin_url'); MP.UrlLang = 'mp_js_current_lang'; MP.init();
        MP.switchLanguage(MP.UrlLang == lang ? 'en' : lang);
        return false;
    }
    function siteCatalystTopNavTracking(obj, page, event, link) {
        var reportsuitid = 'ashleyhomestoreprod';
        var server = 'ashleyfurniturehomestore.com';
        linkCode(obj, page, server, reportsuitid);

        if (event != '') {
            var s = s_gi(reportsuitid);
            s.linkTrackVars = 'events';
            s.linkTrackEvents = event;
            s.server = server;
            s.events = event;
            s.tl(this, 'o', link);
        }
    }

    function fireSearchButtonEvent() {
        document.getElementById('ctl00_ctl00_ctl00_content_topNav_btnSearch').click();
    }
    function redirectToSearch() {
        linkCode('topNavSearch', '', 'ashleyfurniturehomestore.com', 'ashleyhomestoreprod');
        var searchTerms = $get("ctl00_ctl00_ctl00_content_topNav_txtSearch").value;
        searchTerms = encodeURI(searchTerms.replace(/[^a-zA-Z0-9\s]/g, ''));
        if (!searchTerms) return;
        searchTerms = searchTerms.trim();
        if (searchTerms.length > 0 && searchTerms != "Product")
            window.location = "/Catalog/SearchResults.aspx?searchTerms=" + searchTerms;
    }

</script>


        
