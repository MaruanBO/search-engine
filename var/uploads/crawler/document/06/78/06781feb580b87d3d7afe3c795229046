    <script type="text/javascript">
        function TrackSiteCatalystandRedirect(eventid, path, name) {

            var s = s_gi('ashleyhomestoreprod');
            s.server = 'ashleyfurniturehomestore.com';

            s.linkTrackVars = 'events,eVar41';
            s.linkTrackEvents = eventid;
            s.eVar41 = path.substring(path.lastIndexOf("&id=") + 4, path.lastIndexOf("&spi="));
            s.events = eventid;
            s.tl(this, 'o', name);
            window.location = path;
        }
