    <script>
        jQuery(function () {
            jQuery.support.placeholder = false;
            webkit_type = document.createElement('input');
            if ('placeholder' in webkit_type) jQuery.support.placeholder = true;
        });
        $(function () {

            if (!$.support.placeholder) {

                var active = document.activeElement;

                $(':text, textarea, :password').focus(function () {

                    if (($(this).attr('placeholder')) && ($(this).attr('placeholder').length > 0) && ($(this).attr('placeholder') != '') && $(this).val() == $(this).attr('placeholder')) {
                        $(this).val('').removeClass('hasPlaceholder');
                    }
                }).blur(function () {
                    if (($(this).attr('placeholder')) && ($(this).attr('placeholder').length > 0) && ($(this).attr('placeholder') != '') && ($(this).val() == '' || $(this).val() == $(this).attr('placeholder'))) {
                        $(this).val($(this).attr('placeholder')).addClass('hasPlaceholder');
                    }
                });

                $(':text, textarea, :password').blur();
                $(active).focus();
                $('form').submit(function () {
                    $(this).find('.hasPlaceholder').each(function () { $(this).val(''); });
                });
            }
        });
    </script>

        <script type="text/javascript">
            function OpenCorporateSiteInCurrentWindow(pageUrl) {
                var newURL = 'http://www.ashleyfurniturehomestore.com/' + pageUrl;
                window.location = newURL;
            }

            function OpenCorporateSite(pageUrl) {
                var viewerURL = 'http://www.ashleyfurniturehomestore.com/' + pageUrl;
                var winLeft = (screen.width - 800) / 2;
                var winTop = (screen.height - 600) / 2;
                if ('' == 'IE6') {
                    var viewerArgs = 'menubar=no,toolbar=yes,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                }
                if ('' == 'IE7') {
                    var viewerArgs = 'menubar=no,toolbar=no,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                }
                if ('' == 'DESKTOP') {
                    var viewerArgs = 'menubar=yes,toolbar=yes,location=yes,personalbar=yes,status=no,scrollbars=yes,resizable=yes,modal=yes';
                }
                else {
                    var viewerArgs = 'menubar=no,toolbar=yes,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                }

                viewerArgs = viewerArgs + ',width=' + 810;
                viewerArgs = viewerArgs + ',height=' + 600;
                viewerArgs = viewerArgs + ',left=' + winLeft;
                viewerArgs = viewerArgs + ',top=' + winTop;

                window.open(viewerURL, '', viewerArgs);
            }

            function OpenAdSite(pageUrl) {
                var viewerURL = 'http://local.ashleyfurniturehomestore.com/' + pageUrl;
                var winLeft = (screen.width - 990) / 2;
                var winTop = (screen.height - 600) / 2;
                if ('' == 'IE6') {
                        var viewerArgs = 'menubar=no,toolbar=yes,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                    }
                    if ('' == 'IE7') {
                    var viewerArgs = 'menubar=no,toolbar=no,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                }
                if ('' == 'DESKTOP') {
                    var viewerArgs = 'menubar=yes,toolbar=yes,location=yes,personalbar=yes,status=no,scrollbars=yes,resizable=yes,modal=yes';
                }
                else {
                    var viewerArgs = 'menubar=no,toolbar=yes,location=yes,personalbar=no,status=no,scrollbars=yes,resizable=yes,modal=no';
                }

                viewerArgs = viewerArgs + ',width=' + 990;
                viewerArgs = viewerArgs + ',height=' + 600;
                viewerArgs = viewerArgs + ',left=' + winLeft;
                viewerArgs = viewerArgs + ',top=' + winTop;

                window.open(viewerURL, '', viewerArgs);
            }
