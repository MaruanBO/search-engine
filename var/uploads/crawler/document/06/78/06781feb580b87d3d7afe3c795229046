    <input name="ctl00$ctl00$ctl00$content$content$content$ShopByCollectionSlider$imageCount" type="hidden" id="ctl00_ctl00_ctl00_content_content_content_ShopByCollectionSlider_imageCount" class="displayPhotoText" value="14" />
