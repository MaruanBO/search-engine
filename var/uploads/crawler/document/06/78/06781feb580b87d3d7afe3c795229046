<script type="text/javascript">

    function siteCatalystTopNavFinanceTracking(link) {
        var s = s_gi('ashleyhomestoreprod');
        s.server = 'ashleyfurniturehomestore.com';
        s.linkTrackVars = 'events';
        s.linkTrackEvents = 'event22';
        s.events = 'event22';
        s.tl(this, 'o', link);
    }
</script>
<script type="text/javascript">

    $(document).ready(function () {
        $("#ctl00_ctl00_ctl00_content_topNav_loginView_inpInfoEmailHeader").keypress(function (e) {
            if (e.which == 13) {
                showEmailExclusivesHeader();
            }
        });
    });

    function siteCatalyst() {
        linkCode('topNavLogInCreateAccount', '', 'ashleyfurniturehomestore.com', 'ashleyhomestoreprod');
  }

  function showEmailExclusivesHeader() {
      var emailAddress = '';
      var activeTab = $get('ctl00_ctl00_ctl00_content_topNav_loginView_hidActiveTabIndex').value;
        switch (activeTab) {
            case "0":
                emailAddress = $get('ctl00_ctl00_ctl00_content_topNav_loginView_inpInfoEmailHeader').value;
                break;
            default:
        }
        showEmailExclusivesPopupPanelWithEmail(emailAddress);
    }
</script>

