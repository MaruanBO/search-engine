<script>
    var showLocalDealsMenu = false;
    var timerLocalDeals = false;
    var skipTimeout = false;
    var timeout = 500;

    $(function() {

        $('#linkLocalDeals').mouseover(function()
        {
            var timeout = skipTimeout ? 0 : 500;

            if (timerLocalDeals)
                clearTimeout(timerLocalDeals);

            var p = $(this).offset();
            var l = document.getElementById('menuLocalDeals');

            timerLocalDeals = setTimeout(function(){
                l.style.left = (p.left - 1) + 'px';
                l.style.top = (p.top + 35) + 'px';

                document.getElementById('linkLocalDeals').style.color = '#234E08';
                l.style.color = '#666666';

                document.getElementById('siteSectionlocaldeals').style.backgroundColor = '#DEEDCB';
                document.getElementById('siteSectionlocaldeals').style.borderBottomColor = '#DEEDCB';
                $(l).fadeIn(100);
            }, timeout);
        });

        $('#linkLocalDeals').mouseout(function(){
            hideLocalDealsMenu();
        });

        $('#menuLocalDeals').mouseover(function(){
            if (timerLocalDeals)
                clearTimeout(timerLocalDeals);
        });

        $('#menuLocalDeals').mouseout(function(){
            hideLocalDealsMenu();
        });

    });

    function hideLocalDealsMenu()
    {
        skipTimeout = true;

        setTimeout(function(){
            skipTimeout = false;
        }, 1000);

        if(timerLocalDeals)
            clearTimeout(timerLocalDeals);

        timerLocalDeals = setTimeout(function() {

            var l = document.getElementById('menuLocalDeals');

            l.style.left = '-1000px';
            l.style.top = '-1000px';
            l.style.display = 'none';
            document.getElementById('linkLocalDeals').style.color = '';
            document.getElementById('siteSectionlocaldeals').style.backgroundColor = '';

        }, 300);
    }
</script>

