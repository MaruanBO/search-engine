<!--[if lte IE 8]>
<script>
    function jsGridSizer()
    {
                    var wWidth = window.innerWidth || document.documentElement.clientWidth;
            var addColClass;

                        
                                    if(wWidth < 1197)
                                {addColClass = 'cols3'}
                            
                else                    if(wWidth > 1197 && wWidth < 1357)
                                {addColClass = 'cols4'}
                            
                else                    if(wWidth > 1357)
                                {addColClass = 'cols5'}
                                        var gridColClasses = ['cols3', 'cols4', 'cols5'];
            $('.offer-list-grid').addClass(addColClass);
            for(var i = 0; i < gridColClasses.length; i++)
            {
                if (addColClass != gridColClasses[i])
                {
                    $('.offer-list-grid').removeClass(gridColClasses[i]);
                }
            }
            }
</script>
