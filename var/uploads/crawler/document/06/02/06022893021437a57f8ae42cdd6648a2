		<script>
			var bwidth = document.body.scrollWidth;
				if (bwidth > 1800) {
					$("#stoplist").css("right", (52 + (bwidth - 1800) / 2) + "px");
					$("#backtop").css("right", (35 + (bwidth - 1800) / 2) + "px");
				}
	
				
	
				var NavArry = {
					htmlbody : null,
					$slide : null,
					headerheight : 97,
					nowindex : 0,
					scrolltop : null
				};
			/*  this code is for nav scroll down */
			$(document).ready(function() {
				
				$('#stoplist a').smoothScroll({
					offset : -97
				});
				/*$('#stoplist a').click(function(event) {
				 event.preventDefault();
				 //alert($(this).index());
				 var index = $(this).index();
				 $('#stoplist a').each(function() {
				 $(this).removeClass("stop_lilive");
				 });
				 $('#stoplist a').eq(index).addClass("stop_lilive");
				 });*/

				$('#backtop').click(function(event) {
					event.preventDefault();
					var link = this;
					$.smoothScroll({
						//scrollTarget : link.hash
						offset : -97
					});
					//change_nav($("body").scrollTop());
					$('#stoplist a').each(function() {
						$(this).removeClass("stop_lilive");
					});
					$('#stoplist a').eq(0).addClass("stop_lilive");
				});

				//$("#stoplist").hide();
				$("#backtop").hide();
				initSideNav();

			});

			function initSideNav() {
				var homeScreens = $('.homeScreen');
				var $navcircles = $("#stoplist .stop_li");
				//alert(homeScreens.length);
				// Highlight current section while scrolling DOWN
				homeScreens.waypoint(function(direction) {
					if (direction === 'down') {
						//var index = $(this).index(homeScreens);
						var index = homeScreens.index($(this));
						//alert(index);

						NavArry.nowindex = index;
						//Show of the second screen
						if (index > 0) {
							$("#stoplist,#backtop").fadeIn('slow');
						}
						$navcircles.removeClass("stop_lilive");
						$navcircles.eq(index).addClass("stop_lilive");
					}
				}, {
					offset : 99
				});

				// Highlight current section while scrolling UP
				homeScreens.waypoint(function(direction) {
					if (direction === 'up') {
						var index = homeScreens.index($(this));
						NavArry.nowindex = index;
						//alert(index);
						if (index == 0) {
							$("#backtop").fadeOut("slow");
						}
						$navcircles.removeClass("stop_lilive");
						$navcircles.eq(index).addClass("stop_lilive");
					}
				}, {
					offset : function() {
						// This is the calculation that would give you
						// "bottom of element hits middle of window"
						return $.waypoints('viewportHeight') / 2 - $(this).outerHeight();
					}
				});

			}

			/*$('#pagecontent').on('mousewheel', function(event, delta) {
			 //alert($("body").scrollTop()+" px");
			 //alert('pageX: ' + event.pageX + ' pageY: ' + event.pageY );
			 change_nav($("body").scrollTop());

			 });*/

