              function showAds(cpmStar) {                 
                 var adContainer= document.getElementById('other_games');
                 counter = 0;
                 while (cpmStar.adsLeft()) {
                    var spacer = document.createElement('div');
                    spacerWidth = 53;
                    
                    if(counter == 0 || counter == 4){
                        spacerWidth = 46;                        
                     }
                    counter++;
                    
                    spacer.setAttribute("class", "left horizontal-space-normal-2");
                    spacer.className = "left horizontal-space-normal-2";
                    spacer.setAttribute("style", "width:"+spacerWidth+"x;height:10px;");
                    spacer.style.width=spacerWidth+"px";
                    spacer.style.height="10px";
                    
                    adContainer.appendChild(spacer);
                    adContainer.appendChild(spacer);
                    adContainer.appendChild(spacer);
                 
                    var ad = document.createElement('div');
                    ad.setAttribute("class", "left banner-game-bg");
                    ad.className = "left banner-game-bg";
                                        
                    ad.innerHTML = "" +
                       "<a href='" + cpmStar.getLink() + "' target='_blank'>" +
                       "<img width=162 height=89 src='" + cpmStar.getImageUrl(162, 89) + "'>" +
                       "<div class='editorial-title'>" + cpmStar.getTitle() + "</div>"
                       "</a>";                    
                    adContainer.appendChild(ad);
                    cpmStar.nextAd();
                 }
              }
        
        
