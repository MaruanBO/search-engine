	<script type="text/javascript">
	var selected      = 0;
	var autoInstance  = {};
	var delayInstance = {};
	var autoRun       = {};
	var loaderData    = {};
	var loaderDataCat = {};
	var elsCount      = 5;
	var fromCat       = false;
 	
	function initLoader(){
		var tmpEl = document.getElementById('LoaderDiv');
		// Attaching events
		if (typeof tmpEl.getElementsByClassName!='function') {
			tmpEl.getElementsByClassName = function() {
				var elms = tmpEl.getElementsByTagName('*');
				var ei = new Array();
				for (i=0;i<elms.length;i++) {
					if (elms[i].getAttribute('class')) {
							ecl = elms[i].getAttribute('class').split(' ');
							for (j=0;j<ecl.length;j++) {
								if (ecl[j].toLowerCase() == arguments[0].toLowerCase()) {
									ei.push(elms[i]);
								}
							}
					} else if (elms[i].className) {
							ecl = elms[i].className.split(' ');
							for (j=0;j<ecl.length;j++) {
								if (ecl[j].toLowerCase() == arguments[0].toLowerCase()) {
									ei.push(elms[i]);
								}
							}
					}
				}
				return ei;
			}
		}		
		// Getting & Parsing XML data
		if (window.XMLHttpRequest)
			xmlhttp=new XMLHttpRequest();
		else
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");  
		xmlhttp.open("GET","/xml/feeds/specialgamefeed.xml",false);
		xmlhttp.send();
		var xmlDoc = xmlhttp.responseXML;		
		var games  = xmlDoc.getElementsByTagName('game');	
		var catCount = 0;
		if(typeof games.length != 'undefined'){
			elsCount     = games.length;
		}
		for(var x=0; x<elsCount; x++){	
			var gameName     = games[x].getElementsByTagName('title')[0].childNodes[0].nodeValue;
			var gameLink     = games[x].getElementsByTagName('link')[0].childNodes[0].nodeValue;
			var gameImage    = games[x].getElementsByTagName('image')[0].childNodes[0].nodeValue;
			var gameDesc     = games[x].getElementsByTagName('description')[0].childNodes[0].nodeValue;
			var gameCat      = games[x].getElementsByTagName('category')[0].childNodes[0].nodeValue;
			loaderData[x]    = {'gameName':gameName,'gameLink':gameLink,'gameImage':gameImage,'gameDesc':gameDesc,'gameCat':gameCat};
			var check = false;			
			for(var key in loaderDataCat){	
				if(loaderDataCat[key].gameCat == gameCat){
					check = true;
				}
			}
			if(!check){
				loaderDataCat[catCount] = {'gameCatId':x,'gameCat':gameCat};	
				catCount++;
			}
			// precaching images
			//document.getElementById("loaderPrecache").innerHTML = document.getElementById("loaderPrecache").innerHTML + '<img width="425" height="235" src="'+gameImage+'">';
			// set init data 
			if(x==0){
				document.getElementById('loaderMainStage').innerHTML = '<a href="'+gameLink+'">'+'<img width="425" height="235" src="'+gameImage+'">'+'</a>';
				document.getElementById("loaderDescName").innerHTML  = '<a href="'+gameLink+'">'+gameName+'</a>';
				document.getElementById("loaderDescText").innerHTML  = gameDesc;
				document.getElementById("loaderDescPlay").innerHTML  = '<a href="'+gameLink+'">'+'<img width="168" height="46" src="http://static.as.facdn.com//images/slider-play-now.png">'+'</a>';				 
			}
		}
		var loaderImgItems = document.getElementById('mainSlider'); 
		for(var key in loaderData){	
			if(key == 0){
				loaderImgItems.innerHTML = '<div id="loaderS'+key+'" class="buttonClickHover" title="'+loaderData[key].gameName+'" onmouseover="changeSliderFromEvent('+key+')" onclick="loaderClickSlide('+key+')"><img width="78" height="78" src="'+loaderData[key].gameImage.replace('screenshot', 'image').replace('_s1', '_i1')+'"></div>';
			}else{
				loaderImgItems.innerHTML = document.getElementById('mainSlider').innerHTML + '<div id="loaderS'+key+'" class="buttonClick" title="'+loaderData[key].gameName+'" onmouseover="changeSliderFromEvent('+key+')" onclick="loaderClickSlide('+key+')"><img width="78" height="78" src="'+loaderData[key].gameImage.replace('screenshot', 'image').replace('_s1', '_i1')+'"></div>';
			}
		}
		document.getElementById('mainSliderWrapper').style.width = elsCount*89+'px'; 
		var loaderCatItems = document.getElementById('loaderCatItems'); 
		for(var key in loaderDataCat){	
			if(key == 0){
				loaderCatItems.innerHTML = '<div id="loaderC'+loaderDataCat[key].gameCatId+'" class="buttonClickCatHover" onclick="changeCatFromEvent('+loaderDataCat[key].gameCatId+')">'+loaderDataCat[key].gameCat+'</div>';
			}else{
				loaderCatItems.innerHTML = document.getElementById('loaderCatItems').innerHTML + '<div id="loaderC'+loaderDataCat[key].gameCatId+'" class="buttonClickCat" onclick="changeCatFromEvent('+loaderDataCat[key].gameCatId+')">'+loaderDataCat[key].gameCat+'</div>';
			}
		}		 
		// play auto slide show
		slideShow();
		// precaching images
		setTimeout(function(){loaderPrecache()}, 5000);
	}
	function loaderPrecache(){
		for(var x=0; x<elsCount; x++){
			document.getElementById("loaderPrecache").innerHTML = document.getElementById("loaderPrecache").innerHTML + '<img width="425" height="235" src="'+loaderData[x].gameImage+'">';
		}
	}
	function loaderChangeSlide(id){
		document.getElementById('loaderMainStage').innerHTML = '<a href="'+loaderData[id].gameLink+'">'+'<img width="425" height="235" src="'+loaderData[id].gameImage+'">'+'</a>';
		document.getElementById("loaderDescName").innerHTML = '<a href="'+loaderData[id].gameLink+'">'+loaderData[id].gameName+'</a>';
		document.getElementById("loaderDescText").innerHTML = loaderData[id].gameDesc;
		document.getElementById("loaderDescPlay").innerHTML = '<a href="'+loaderData[id].gameLink+'">'+'<img width="168" height="46" src="http://static.as.facdn.com//images/slider-play-now.png">'+'</a>';		
	}
	function loaderClickSlide(id){
		window.location = loaderData[id].gameLink;
	}
	function changeCatFromEvent(tmpId){
		fromCat = true;
		delayInstance = clearTimeout(delayInstance);
		delayInstance = setTimeout(function(){changeSlide(tmpId, true);}, 50);
	}
	function changeSliderFromEvent(tmpId){
		delayInstance = clearTimeout(delayInstance);
		delayInstance = setTimeout(function(){changeSlide(tmpId, true);}, 50);
	}
	function changeSlide(id, bdisableAuto){
		if(bdisableAuto){
			autoInstance = clearInterval(autoInstance);
			autoRun = clearTimeout(autoRun);
			autoRun = setTimeout(function(){autoInstance = setInterval(function(){slideShow(true)}, 3000)}, 2000);
		}	
		if(selected == id) return;
		//if(loaderData[selected].gameCat != loaderData[id].gameCat){
			if(fromCat){
				if(elsCount > 5){
					document.getElementById('mainSliderWrapper').style.left = -1*(id*89)+'px';
				}
				fromCat = false;
			}else if((id%5) == 0){
				document.getElementById('mainSliderWrapper').style.left = -1*(id*89)+'px';					
			}
			for(var key in loaderDataCat){
				if(loaderDataCat[key].gameCat == loaderData[id].gameCat){
					document.getElementById("loaderC"+loaderDataCat[key].gameCatId).className       = 'buttonClickCatHover';
				}else{
					document.getElementById("loaderC"+loaderDataCat[key].gameCatId).className       = 'buttonClickCat';
				}
			}	
		//}
		loaderChangeSlide(id);
		document.getElementById("loaderS"+selected).className    = 'buttonClick';
		document.getElementById("loaderS"+id).className          = 'buttonClickHover';				
		selected    = id;
		return;
	}
	function  slideShow(ext){
		if(ext == true){
			changeSlide((selected + 1)%elsCount, false);
		}else{
			autoRun = setTimeout(function(){autoInstance = setInterval(function(){slideShow(true)}, 3000)}, 5000);
		}
	}	
	// INIT
	initLoader();
</script>
</div>
	</div>
	
