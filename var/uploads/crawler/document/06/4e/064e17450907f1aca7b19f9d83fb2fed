<!-- JS to delete cookie -->
	
<script type="text/javascript">
 
        //used to get (any) cookie by name (required for solution).
        function getCookieValue(name) {
            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length == 2) return parts.pop().split(";").shift();
        }
 
        //will expire the bad cookie, if found.
        function findAndExpireBadCookie() {
            var cookie = getCookieValue("com.silverpop.iMAWebCookie");
            if (cookie != undefined && cookie == "c3735103-ab5a-a5cc-8e7b-c4891e9e8c4c") {
                document.cookie = "com.silverpop.iMAWebCookie=c3735103-ab5a-a5cc-8e7b-c4891e9e8c4c; expires=Fri, 31 Jan 2014 23:59:59 GMT";
            }
        }
 
        (function () {
            findAndExpireBadCookie();
        })();
</script>

<!-- End JS to delete cookie -->

