<script>
jQuery('div[id^="pid-article"]').find('#block-mostpopular-2').find('.item_img2').css('display', 'none'); 

function sticky_relocate() {
  var window_top = jQuery(window).scrollTop()+1;
  var div_top = (jQuery('#sticky-anchor').offset().top + 20);
  var div_bottom=(jQuery('#boxes-box-mental_floss_bottom_circ_banner').offset().top -10);
  var conditional_height = div_bottom - div_top;
  var isFirefox = typeof InstallTrigger !== 'undefined';
  var mp_height = jQuery('#block-mostpopular-2').find('.sticker').height();  
  var height_container =  ( (jQuery('#content-group').height()) -(jQuery('#sticky-anchor').offset().top) + 260);
  if(conditional_height > 1000) {
  	if ((navigator.userAgent.match('iPad')) || (location.search.match('ipad=true'))) {
	  		jQuery('#block-mostpopular-2 .sticker').css( "display", "block" )
			jQuery('#block-mostpopular-2 .sticker').css( "position", "absolute" );
			}
   	else if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){ //test for MSIE x.x;
		    jQuery('#block-mostpopular-2 .sticker').css( "display", "block" );
		    jQuery('#block-mostpopular-2 .sticker').css( "position", "relative" );
		    }
    else
	        {
	    
			if ((window_top > div_top) && ((window_top+mp_height) < div_bottom)) {
		           if(window.location.href.indexOf("quizzes") != -1) {
		              	jQuery('#block-mostpopular-2 .sticker').removeClass('end_quizzes');
		            }else{
		             	jQuery('#block-mostpopular-2 .sticker').removeClass('end');
		            }
		    		jQuery('#block-mostpopular-2 .sticker').addClass('stick');
		    		jQuery('#block-mostpopular-2 .sticker').css( "display", "block" );
		  	}else if ((window_top+mp_height) >= div_bottom) {
                   	if(window.location.href.indexOf("quizzes") != -1) {
                   	    jQuery('#block-mostpopular-2 .sticker').removeClass('stick'); 
                        jQuery('#block-mostpopular-2 .sticker').addClass('end_quizzes');
		    			jQuery('#block-mostpopular-2 .sticker').css( "display", "block" );
                        jQuery('#block-mostpopular-2 .sticker').css("position", "inherit");
                   
                	}else{
		    			jQuery('#block-mostpopular-2 .sticker').removeClass('stick');
                    	jQuery('#block-mostpopular-2 .sticker').addClass('end');
		    			jQuery('#block-mostpopular-2 .sticker').css( "display", "block" );
                        jQuery('#block-mostpopular-2 .stickem-container').height(height_container);
                 }
		  	}
         	else{
		  			jQuery('#block-mostpopular-2 .sticker').removeClass('stick');
		  			if(window.location.href.indexOf("quizzes") != -1){ jQuery('#block-mostpopular-2 .sticker').removeClass('end_quizzes');}
		  			else{
                    jQuery('#block-mostpopular-2 .sticker').removeClass('end'); }
				   }
			}
    }else { jQuery('#block-mostpopular-2 .sticker').css( "display", "none" );}
}
 
jQuery(window).load(function() {
  jQuery(window).scroll(sticky_relocate);
  sticky_relocate();
});

function trackMostPopular(){
	if(window.location.href.indexOf("article") > -1) {
      _gaq.push(['_trackEvent', 'Promos', 'Article right rail', 'most-popular' ]);
      }
      else{
      	 _gaq.push(['_trackEvent', 'Promos', 'Homepage right rail', 'most-popular' ]);
      }
}

</script>

