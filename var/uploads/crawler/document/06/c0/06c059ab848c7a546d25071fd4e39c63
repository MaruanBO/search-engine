<script type="text/javascript">
        // --- page-specific settings ------------------------------------------
        janrain.settings.capture.flowName = 'standard';
        janrain.settings.capture.stylesheets = ['http://static.cnhionline.com/ksherman/janrain/styles/janrain.css'];
        //janrain.settings.capture.mobileFlowName = 'mobile';
        janrain.settings.capture.mobileStylesheets = ['http://static.cnhionline.com/ksherman/janrain/styles/janrain-mobile.css'];

        // --- define event handlers and start the janrain ui ------------------
        function janrainCaptureWidgetOnLoad() {
            janrain.events.onCaptureLoginSuccess.addHandler(function(result) {
                janrain.capture.ui.modal.close();
                window.Backplane.userinfo = result;
                if (window.console && window.console.log) console.log(result) ;
                document.getElementById('captureSignInLink').style.display = 'none';
                document.getElementById('captureSignOutLink').style.display = '';
                document.getElementById('captureProfileLink').style.display = '';
            });

            janrain.events.onCaptureSessionFound.addHandler(function(result) {
                janrain.capture.ui.modal.close();
                if (window.console && window.console.log) console.log(result);
                document.getElementById('captureSignInLink').style.display = 'none';
                document.getElementById('captureSignOutLink').style.display = '';
                document.getElementById('captureProfileLink').style.display = '';
            });

            janrain.events.onCaptureRegistrationSuccess.addHandler(function(result) {
                janrain.capture.ui.modal.close();
                if (window.console && window.console.log) console.log(result);
                document.getElementById('captureSignInLink').style.display = 'none';
                document.getElementById('captureSignOutLink').style.display = '';
                document.getElementById('captureProfileLink').style.display = '';
            });

            janrain.events.onCaptureSessionEnded.addHandler(function(result) {
                document.getElementById('captureSignInLink').style.display = '';
                document.getElementById('captureSignOutLink').style.display = 'none';
                document.getElementById('captureProfileLink').style.display = 'none';
            });

            janrain.events.onCaptureScreenShow.addHandler(function(result) {
                if (result.screen == 'returnTraditional') {
                    janrainReturnExperience();
                }
            });
            janrain.events.onCaptureBackplaneReady.addHandler(function(result) {  
  

 function ssoLogin() { 
 var match = document.cookie.match(/tncms-livefyre-user-token=([^;]+)/); 
 
 if ( match ) { 
 fyre.conv.login(match[1]); 
 return true; 
 } 
 else { 
 return false; 
 } 
 } 
 
var auth = new fyre.conv.BackplaneAuthDelegate(window.Backplane); 
 
auth.login = function(delegate) {
   var successCallback = function(delegate) {
      // These need to be replaced with the actions that correspond to successful login 
      // and when the Janrain modal is closed.
      janrain.events.onCaptureLoginSuccess.removeHandler(successCallback);
      janrain.events.onModalClose.removeHandler(failureCallback);      
      $.ajax('http://mankatofreepress.com/tncms/livefyre/janrain-profile-update/', {
         type:"POST",
         data: {
            'uuid': window.Backplane.userinfo.userData.uuid
         },
         success: function(data) {
            console.log('success data ' + data);
         },
         error: function(err){
            console.log({error: err})
         }
      })
      delegate.success();
   };
   
   var failureCallback = function() {
      delegate.failure();
      janrain.events.onModalClose.removeHandler(failureCallback);
      janrain.events.onCaptureLoginSuccess.removeHandler(successCallback);
   };

   // Open the modal to log a user in
   janrain.capture.ui.modal.open();
   // Send a backplane message
   window.Backplane.expectMessages('identity/login');
   // Add handlers to specific janrain events
   janrain.events.onCaptureLoginSuccess.addHandler(successCallback);
   janrain.events.onModalClose.addHandler(failureCallback);
};

 auth.logout = function(delegate) {
 window.Backplane.resetCookieChannel();
 fyre.conv.BackplaneAuthDelegate.prototype.logout.call(this, delegate);
 janrain.capture.ui.endCaptureSession();
};

auth.editProfile = function(delegate, author) {
	window.location.replace('/edit-profile');
};
 
 fyre.conv.load( 
 { 
 network: 'cnhi.fyre.co', 
 authDelegate: auth 
 },
 [{ 
 siteId: '348237', 
 articleId: '', 
 el: 'livefyre', 
 collectionMeta: '', 
 checksum: '' 
 }], 
 function() { 
 ssoLogin(); 
 } 
 ); 
 } );
            // should be the last line in janrainCaptureWidgetOnLoad()
            janrain.capture.ui.start();
        }
</script>





