                     if(authorMode=='true'){
                    	 userlocation = "";
                     }else{
                    	 userlocation = pbl.cookie.getClientCookie('userlocation');
                     }
                     
                     if(userlocation!==null && userlocation!==""){
                    	 userlocation=userlocation.toUpperCase();
                     }else{
                    	 userlocation="US";
                     }
                     
