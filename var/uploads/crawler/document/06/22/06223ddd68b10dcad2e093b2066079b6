<script>
function focusIfNotVIP(formField)
{
var locale = "en_US_USGCB";
var vipReg = /VIP/i;
if (locale.search(vipReg) == -1  && formField !=  null) {
formField.focus();
}
}
</script>

<SCRIPT language=javascript src="/JSO/js/signon.js" type=text/javascript></SCRIPT>




<script type="text/javascript">
var validate = false;
clearFormOnError=false;
mtSupported=true;
var gpPlsMyCitiUsrId="Please enter a User ID";
var gpPlsMyCitiPass="Please enter a Password.";
var gpDashOnCookiedScreen="---------";
var gpErrorOnUserIDSelect="Please select a User ID";

validate = true;
unameMinLength=2;
pwdMinLength=6;
var gpMyCitiCond="Your User ID must be greater than 2 characters";
var gpMyCitiPassCond="Your Password must be at least 6 characters";

function doSubmit(e)
{
if(e){
var f = e;
while(f && f.nodeName != 'FORM') f = f.parentNode;
} else f = document.SignonForm;
if(document.getElementById('showerrordynamic'))
jQuery('#showerrordynamic').html("");
if(f && signOnUnamePwd(false, validate, f)){
if(f.ioBlackBox !=null && f.ioBlackBox != 'undefined')
{
setIOBlackBox(f);
}
if(f.devicePrint !=null && f.devicePrint != 'undefined')
{
populateClientData(f);
}

f.submit();
}
}
function enterkeySubmit()
{
populateClientData(document.SignonForm);
if(document.SignonForm.ioBlackBox !=null && document.SignonForm.ioBlackBox != 'undefined')
{
setIOBlackBox(document.SignonForm);
}
return signOnUnamePwd(false, validate);
}

function unblock(url)
{
document.SignonForm.action = url;
document.SignonForm.submit();
}


function onDelete() {
var len = document.DeleteNameForm.elements.length;
for (var i = 0; i < len; i++) {
if (document.DeleteNameForm.elements[i].checked == true) {
document.DeleteNameForm.submit();
return true;
}
}
alert("Please select user id(s) to delete.");
return false;
}

</script>




