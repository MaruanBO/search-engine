function inc(filename)
{
var body = document.getElementsByTagName('head').item(0);
script = document.createElement('script');
script.src = filename;
script.type = 'text/javascript';
body.appendChild(script);
}
var locale = "en_US_USGCB";
if(!(locale.search('en_US_VIP') == -1)){
inc('/JRS/cm/js2/bauble.js');
jQuery("span.rememberSpan span").addClass('rememberSpan');
}
function processBubble(){
setTimeout(Delay,1);
function Delay()	{
if(document.getElementById('rememberuserid').checked==false){
processInfoBubble('');
jQuery('#rmid').jfpwidget('toggle',true);
document.getElementById('rememberuserid').checked = true;
document.getElementById("rememberuserid").focus();
}
else{
jQuery('#rmid').jfpwidget('toggle',false);
document.getElementById('rememberuserid').checked = false;
document.getElementById("rememberuserid").focus();
}
clearTimeout();
}
}
