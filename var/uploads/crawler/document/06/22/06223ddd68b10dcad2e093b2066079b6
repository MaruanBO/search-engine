<script>
function noError(){return true;}
window.onerror = noError;
var signonLock = true;
var callbackFunction;
function doSubmit(e){
if (!signonLock){
var now = new Date()
var offset = now.getTimezoneOffset();
var gmtHours = (new Date().getTimezoneOffset() / 60) * (-1);
document.getElementById('efdBTZ').value=gmtHours;
document.getElementById('efdCSR').value=screen.width+'x'+screen.height;
toggleInfoBubble('true');
if(e){
var f = e;
while(f && f.nodeName != 'FORM') f = f.parentNode;
} else f = document.SignonForm;
populateClientData(f);
if(f && signOnUnamePwdError(false, validate)){
if(f.ioBlackBox !=null && f.ioBlackBox != 'undefined'){
setIOBlackBox(f);
}
s.tl(this,'o',SCFormElementReporting.report('SignonForm', ['rememberuserid']));
jQuery(f).submit();
}
}
else{
callbackFunction = function(e) {doSubmit(e);  };
return false;
}
}
function  dosubmit()
{
if (!signonLock){
signOnUnamePwdError(false, validate, this);
s.tl(this,'o',SCFormElementReporting.report('SignonForm', ['username','rememberuserid']));
populateClientData(document.SignonForm);
if(document.SignonForm.ioBlackBox !=null &&document.SignonForm.ioBlackBox != 'undefined'){
setIOBlackBox(document.SignonForm);
}
}
else{
callbackFunction = function(e) {doSubmit();  };
return false;
}
}
</script>
<script type="text/javascript" language="javascript">
jQuery(document).ready(function($)
{
$('#usernameMasked').attr('aria-describedby','ui-tooltip-EnterUserIDTooltip');
$('#username').attr('aria-describedby','ui-tooltip-EnterUserIDTooltip');
$("#pwd").keypress(function(){
t=setTimeout(clearInputBox, 600000);
});
jQuery('.ui-dialog-titlebar-close').click(function() {
hideTooltipWidget();
});
}
);
function hideTooltipWidget()
{
jQuery('#EnterUserIDTooltip').jfpwidget('hide');
jQuery('#EnterPwdTooltip').jfpwidget('hide');
jQuery('#MinUserIDTooltip').jfpwidget('hide');
jQuery('#MinPwdTooltip').jfpwidget('hide');
jQuery('#rmid').jfpwidget('toggle',false);
}
function clearInputBox()
{
$("#username").val("");
$("#pwd").val("");
}
</script>
<script>
var enterUserIDTooltip = false;
var enterPwdTooltip = false;
var minUIdTooltip = false;
var minPwdTooltip = false;
var flag= false;
function toggleInfoBubble(flag)
{
if (flag == "true")
{
jQuery('#EnterUserIDTooltip-info').css('display', 'inline-block');
jQuery('#EnterPwdTooltip-info').css('display', 'inline-block');
jQuery('#MinUserIDTooltip-info').css('display', 'inline-block');
jQuery('#MinPwdTooltip-info').css('display', 'inline-block');
}
else{
jQuery('#EnterUserIDTooltip-info').css('display', 'none');
jQuery('#EnterPwdTooltip-info').css('display', 'none');
jQuery('#MinUserIDTooltip-info').css('display', 'none');
jQuery('#MinPwdTooltip-info').css('display', 'none');
}
}
toggleInfoBubble('false');
function userIDErrorBubble(){
if(enterUserIDTooltip == true){
processInfoBubble("#EnterUserIDTooltip");
enterUserIDTooltip = false;
}
return;
}
function pwdErrorBubble(){
if(enterPwdTooltip == true){
processInfoBubble("#EnterPwdTooltip");
enterPwdTooltip = false;
}
return;
}
function minUserIDErrorBubble(){
if(minUIdTooltip == true){
processInfoBubble("#MinUserIDTooltip");
minUIdTooltip = false;
}
return;
}
function minPwdErrorBubble(){
if(minPwdTooltip == true){
processInfoBubble("#MinPwdTooltip");
minPwdTooltip = false;
}
return;
}
function processInfoBubble(id)
{
switch(id){
case'#EnterUserIDTooltip':
jQuery('#EnterUserIDTooltip').jfpwidget('show');
jQuery('#usernameMasked').focus();
jQuery('#EnterPwdTooltip').jfpwidget('hide');
jQuery('#MinUserIDTooltip').jfpwidget('hide');
jQuery('#MinPwdTooltip').jfpwidget('hide');
break;
case'#EnterPwdTooltip':
jQuery('#EnterUserIDTooltip').jfpwidget('hide');
jQuery('#EnterPwdTooltip').jfpwidget('show');
jQuery('#MinUserIDTooltip').jfpwidget('hide');
jQuery('#MinPwdTooltip').jfpwidget('hide');
break;
case'#MinUserIDTooltip':
jQuery('#EnterUserIDTooltip').jfpwidget('hide');
jQuery('#EnterPwdTooltip').jfpwidget('hide');
jQuery('#MinUserIDTooltip').jfpwidget('show');
jQuery('#username').attr('aria-describedby','ui-tooltip-MinUserIDTooltip');
jQuery('#usernameMasked').focus();
jQuery('#MinPwdTooltip').jfpwidget('hide');
break;
case'#MinPwdTooltip':
jQuery('#EnterUserIDTooltip').jfpwidget('hide');
jQuery('#EnterPwdTooltip').jfpwidget('hide');
jQuery('#MinUserIDTooltip').jfpwidget('hide');
jQuery('#MinPwdTooltip').jfpwidget('show');
jQuery('#pwd').attr('aria-describedby','ui-tooltip-MinPwdTooltip');
break;
default:
jQuery('#EnterUserIDTooltip').jfpwidget('hide');
jQuery('#EnterPwdTooltip').jfpwidget('hide');
jQuery('#MinUserIDTooltip').jfpwidget('hide');
jQuery('#MinPwdTooltip').jfpwidget('hide');
}
return;
}
</script>
