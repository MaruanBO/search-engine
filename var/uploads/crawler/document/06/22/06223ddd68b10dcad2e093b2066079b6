<script>
function checkStatusCsq(event, ui) {
	var closeWidget = closeIconClickCsq();
	if(jQuery("#answerLabelToolTip0")){
		jQuery('#answerLabelToolTip0').jfpwidget('hide'); }
	if(jQuery("#answerLabelToolTip1")){
		jQuery('#answerLabelToolTip1').jfpwidget('hide'); }
	return closeWidget;
		
}
function closeIconClickCsq(){

	//SiteCatalyst Change
	if(typeof(s)!='undefined' && null != s &&  s!=''){ 
	var tmp2 = s.pageName ; 
	s.pageName = s.pageName + " CloseOption";
	s.t();
	s.pageName = tmp2;
	}
	jQuery('#InterdictionOverlayContent').html('');
	return true;
}

function checkKBAInterdictionOverlay(url)
{
	jQuery('body').append('<div id="machineTagSpin" class="ui-widget-overlay" style="width: 1346px; height: 1465px; z-index: 1001;"><div id = "spinMe" style= "position:absolute;top:300px;left:650px;"><img border="0" width="30" src="/CBOL/common/images/loading.gif"></div></div>');
	//jQuery(url).parent().append('<span style="color:red;font-weight:bold;">Authenticating...</span>');
	if(callCyotaFlag){
	if(machTagfirstHit){
		machTagfirstHit = false;
	jQuery.ajax({
		 url:url,
		 data : {isAjaxCall:"true"},
		 type:"POST",
		 success: function(showData,status, jqXHR){
				if(jqXHR.status == '200'){
					machTagfirstHit = true;
                                  if(showData.returnURL.indexOf("true")== 0){
				 	    	showCSQOverlay(url);
				   		}
                                  else if(showData.returnURL.indexOf("globalOTP")==0)
					 {
						 	
							showOTPOverlay(url);
					 }
                                  else if(showData.returnURL.indexOf("error")== 0){
						 	document.forms["ajaxform"].action = "/JRS/portal/DefaultError.do";
							document.forms["ajaxform"].submit();
					 }
					 else{
                                         checkKBA(url,showData.returnURL);
					 }
				}else {
				/* 
				Error handled in the action, reponse status would always be 200
				*/
					document.forms["ajaxform"].action = "/JRS/portal/DefaultError.do";
					document.forms["ajaxform"].submit();
		}
	}});
	}
	}else{
		lnk(url);
	}
}
	
function showCSQOverlay(url){
	jQuery('#InterdictionOverlayContent').jfpwidget('open');
	jQuery('#machineTagSpin').remove(); // remove the machine tag spin for csq
	var actualContent=executeOnLoad();
	jQuery('#InterdictionOverlayContent').html(actualContent);
  
	jQuery.ajax({ url:url, success: function(showData){
	if(showData.indexOf("NoCSQQuestions")==0){
		 document.forms["ajaxform"].action = url;
		 document.forms["ajaxform"].method = "POST";
		 document.forms["ajaxform"].submit();
	}
	else{
	jQuery('#InterdictionOverlayContent').html(showData);
	}
}});
}
function showOTPOverlay(url){
	// do not remove the machine tag spin for otp, since form submit
	 jQuery.ajax({
		 url:url,
		 cache:false,
		 type:"POST",
		 success: function(showData,status, jqXHR){
				
			 }	  
		});
}

function checkKBAInterdictionResetPasswordOverlay(url)
{
	if(machTagfirstHit){
		machTagfirstHit = false;
	jQuery.ajax({
		 url:url,
		 data : {isAjaxCall:"true"},
		 type:"POST",
		 success: function(showData,status, jqXHR){
				if(jqXHR.status == '200'){
					machTagfirstHit=true;
					 if(showData.indexOf("true")== 0){
				 	    	showCSQOverlay(url);
				   		}
					 else if(showData.indexOf("error")== 0){
						 	document.forms["ajaxform"].action = "/JRS/portal/DefaultError.do";
							document.forms["ajaxform"].submit();
					 }
					 else{
						 document.forms["ajaxform"].action = _jcontext+ showData;
						 document.forms["ajaxform"].method = "POST";
						 document.forms["ajaxform"].submit();
					 }
				}else {
				/* 
				Error handled in the action, reponse status would always be 200
				*/
					document.forms["ajaxform"].action = "/JRS/portal/DefaultError.do";
					document.forms["ajaxform"].submit();
		}
	}});
	}
}


function executeOnLoad(){
	var display;
	var titleText="Security CheckPoint";
	var waitMessage="Loading";
		display='<div class="titleLabel fontSize20px overlayColorBackground ">'+titleText+'</div>';


		display+='<div class="StructContentDiv" id="mainContentWrapper">';
		display+='<div style="margin-left: 200px; margin-top: 140px;"><div><img border="0" width="30" src="/CBOL/common/images/loading.gif"></div><div style="margin-left: -5px;">'+waitMessage+'</div></div>';
		display+='</div>';
		return display;
}
</script>

				






