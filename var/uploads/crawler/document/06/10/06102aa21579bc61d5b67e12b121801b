var customerAccountUrl  = 'https://www.pickyourplum.com/customer/account/';

var customerCookieData  = Mage.Cookies.get('CUSTOMER_DATA');
if (customerCookieData) {
    customerCookieData = customerCookieData.split(';', 3);
}
var itemsInCart         = customerCookieData ? customerCookieData[0] : 0;
var isLoggedIn          = customerCookieData ? customerCookieData[1] : 0;
var customerName        = customerCookieData ? customerCookieData[2].replace('+', ' ') : null;
var headerPanel         = $$('.header-container .header-panel')[0];

if (headerPanel) {
    var accountLinks;
    if (isLoggedIn == '1') {
        accountLinks = '<span class="welcome-msg user-logged-in">Hi, '
            + '<a class="link-customer-name" href="'+customerAccountUrl+'">'+customerName+'</a>!</span>'
            + '<ul class="links"><li class="first last">'
            + '<a class="link-log-out" href="'+customerLogoutUrl+'">Log out</a></li></ul>';
