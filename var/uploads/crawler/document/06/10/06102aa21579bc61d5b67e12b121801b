            + '<ul class="links"><li class="first">'
            + '<a class="link-log-in" href="'+customerLoginUrl+'">Log in</a></li>'
            + '<li class="last"> <span>or</span>'
            + '<a class="link-register" href="'+customerRegisterUrl+'">'
            + 'register</a></li></ul>';
    }
    headerPanel.insert({top: accountLinks});
    if ($('persistent-login')) {
        if (customerName) {
            $('persistent-login').setAttribute('title', '(Not ' + customerName + '?)');
            $('persistent-login').update('(Not ' + customerName + '?)');
            $$('a.link-log-out').each(function (element) {element.remove()});
        } else {
            $('persistent-login').remove();
        }
    }
}
var topCartLink = $$('.top-cart .cart-link li')[0];
if (topCartLink) {
    topCartLink.update('<a href="'+checkoutCartUrl+'">My Cart ('+itemsInCart+')</a>');
}
