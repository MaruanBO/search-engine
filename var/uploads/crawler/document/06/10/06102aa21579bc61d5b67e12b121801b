                    jQuery(document).ready(function(){
                        var customerData = Mage.Cookies.get('CUSTOMER_DATA');
                        var countryIds = [];
                        if (customerData) {
                            customerData = customerData.split(';');
                            countryIds = customerData[6];
                            if (countryIds) {
                                try {
                                    countryIds = JSON.parse(countryIds);
                                    countryIds.each(function(country){
                                        var container = $('shipping-price-cost-container-' + country);
                                        if (container) {
                                            container.setStyle({
                                                display: 'inline-block'
                                            });
                                        }
                                    });
                                } catch (e) {

                                }
                            }
                        }
                        if (!countryIds || countryIds.length == 0) {
                            var containers = $$('.shipping-price-cost-container');
                            if (containers) {
