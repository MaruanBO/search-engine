<script type="text/javascript">
//<![CDATA[
var formId = 'product_addtocart_form';
var myForm = new VarienForm(formId);
var postUrl = $(formId).action;
function submitAddToCartForm() {
    if (myForm.validator.validate()) {
        var parameters = $(formId).serialize(true);
        parameters.ajax = 1;
        new Ajax.Updater(
            { success:'formSuccess' }, postUrl, {
                method:'post',
                asynchronous:true,
                evalScripts:false,
                onCreate: function(){
                    if($('page_loader')){
                        $('page_loader').setStyle({
                            display: 'inherit'
                        })
                    }
                },
                onComplete:function(request, json) {
                    $('cart-popup-container').update(request.responseText);
                    $('cart-popup-container').scrollTo();
                    Event.observe($$('.close-popup')[0], 'click', function(event){
                        event.stop();
                        $('cart-popup-container').update();
                    });
                    $$('.validate-greater-than-zero').each(function(qtyElement){
                        $(qtyElement).value = 0;
                        $(qtyElement).isUpdated = false;
                    });
                    if($('page_loader')){
                        $('page_loader').setStyle({
                            display: 'none'
                        })
                    }
                },
                onLoading:function(request, json){

                },
                parameters: parameters
            }
        );
    }
    return false;
}
var addToCartButtons = $$('button.action-add-to-cart');
for (var i=0; i<addToCartButtons.length; i++) {
    Event.observe(addToCartButtons[i], 'click', function(event){
        event.stop();
        submitAddToCartForm();
    });
}

Validation.add('required-entry-qty-related', 'This is a required field', function(v, el){
    var elValue = new String($(el).value);
    if(elValue.trim().length == 0 && parseInt($(el).up('div.tr').down('.validate-greater-than-zero').value) > 0) {
        return false;
    }
    return true;
});
Validation.add('validate-one-required-by-name-qty-related', 'This is required field', function(v, elm){
    if(parseInt($(elm).up('div.tr').down('.validate-greater-than-zero').value) <= 0) {
        return true;
    }

    var inputs = $$('input[name="' + elm.name.replace(/([\\"])/g, '\\$1') + '"]');

    var error = 1;
    for(var i=0;i<inputs.length;i++) {
        if((inputs[i].type == 'checkbox' || inputs[i].type == 'radio') && inputs[i].checked == true) {
            error = 0;
        }

        if(Validation.isOnChange && (inputs[i].type == 'checkbox' || inputs[i].type == 'radio')) {
            Validation.reset(inputs[i]);
        }
    }

    if( error == 0 ) {
        return true;
    } else {
        return false;
    }
});
Validation.add('validate-greater-than-zero', 'Tell us how many you want first!', function(v, el){
    var nonZeroQtyFound = false;
    $$('.validate-greater-than-zero').each(function(qtyElement){
        if (parseInt($(qtyElement).value) > 0) {
            nonZeroQtyFound = true;
        }
    });

    if(!nonZeroQtyFound) {
        return false;
    }

    return true;
});

var buyNowGuestText = 'Login & Buy Now';
var formSubmitter = Class.create();
formSubmitter.prototype = {
    initialize : function()
    {
        this.bindButtonSubmit();
    },
    bindButtonSubmit : function()
    {
        var buyNowButtons = $$('.action-buy-now');
        // if persistent_shopping_cart is enabled and is persistent session
        var isPersistent = 0;
        for (var i=0; i<buyNowButtons.length; i++) {
            if (!isLoggedIn()) {
                Event.observe(buyNowButtons[i], 'click', this.showLoginModal.bind(this));
            } else {
                Event.observe(buyNowButtons[i], 'click', this.submitForm.bind(this));
            }
        }
    },
    showLoginModal : function()
    {
        jQuery('#register-modal-wrapper').addClass('active');
    },
    submitForm : function(event)
    {
        var submitButton = Event.element(event);
        if (submitButton.nodeName.toLowerCase() != "button") {
            submitButton = submitButton.up('button');
        }
        var url = null;
        if (submitButton.id.search('submit_button_') != -1) {
            var productId = submitButton.id.replace('submit_button_', '');
        } else {
            var productId = submitButton.id.replace('buy_now_button_', '');
            url = 'buynow/1/';
        }

        var productAddToCartForm = new VarienForm('product_addtocart_form');
        productAddToCartForm.submit = function(button, url) {
            if (this.validator.validate()) {
                var form = button.form;
                if (url) {
                    form.action = form.action + url;
                }
                form.submit();
                if (button && button != 'undefined') {
                    button.disabled = true;
                }
            }
        };
        productAddToCartForm.submit(submitButton, url);
    }
}
productFormSubmit = new formSubmitter();

jQuery(document).ready(function(){
    if(!isLoggedIn() && Mage.Cookies.get('curr_pid')){
        Mage.Cookies.set('curr_pid', '', null, '/');
        Mage.Cookies.clear('curr_pid');
    }

    var cookieName = 'pyp_visitor';
    if (!Mage.Cookies.get(cookieName)) {
        Mage.Cookies.set(cookieName, new Date().getTime());
    }
    cookieName = 'pyp_messages';
    var messages = Mage.Cookies.get(cookieName);
    if (messages) {
        messages = decodeURIComponent(messages);
        messages = messages.split('+').join(' ');
        jQuery('#messages_product_view').html(messages);
        Mage.Cookies.set(cookieName, '');
        Mage.Cookies.clear(cookieName);
    }

    var customerData = Mage.Cookies.get('CUSTOMER_DATA');
    if (customerData) {
        customerData = customerData.split(';');
        if (customerData[4]) {
            var cpids = jQuery.parseJSON(unescape(customerData[4]));
            var length = cpids.length;
            for (var i = 0; i < length; i++) {
                if (cpids[i]) {
                    var cpid = cpids[i];
                    jQuery('#subscribe-to-product-' + cpid).remove();
                }
            }
        }
    }
    // if persistent_shopping_cart is enabled and is persistent session
    var isPersistent = 0;
    if (!isLoggedIn() && !noThanksChoosen() && !isPersistent) {
        jQuery('.login-modal').addClass('active');
    }

    jQuery('body').on('click.setActiveDeal', '.product-list > li', function() {
        var that = jQuery(this),
            productId = that[0].id.replace('product-carousel-element-', '');

        Mage.Cookies.set('curr_pid', productId, null, '/');

        if (jQuery.inArray('first', that[0].classList)) {
            jQuery('.login-modal').removeClass('active');
            if (!isLoggedIn() && !isPersistent) {
                jQuery('#register-modal-wrapper').addClass('active');
                jQuery('.product-tile-actions').each(function (key, element) {
                    element.hide();
                });
            }
        } else {
            if (!isLoggedIn() && !noThanksChoosen() && !isPersistent) {
                jQuery('.login-modal').addClass('active');
            }
            jQuery('#register-modal-wrapper').removeClass('active');
            jQuery('.product-tile-actions').each(function (key, element) {
                element.show();
            });
        }
        that.addClass('active')
            .siblings()
            .removeClass('active');


        jQuery('#product-view-'+productId)
            .addClass('active')
            .siblings()
            .removeClass('active first');
    });


    jQuery('body').on('click.modalSignUp', '#modal-sign-up', function() {
        jQuery('#register-modal')
            .addClass('modal-step-register');
    }).on('click.modalLogin', '#back-to-login', function() {
        jQuery('#register-modal')
            .removeClass('modal-step-register');
    }).on('click.modalClose', '.modal-back-home', function() {
        jQuery('#register-modal-wrapper')
            .removeClass('active');
    });

    if (jQuery('.product-carousel .product-list').length) {
        var timeout = setTimeout(function(){
            jQuery('.product-carousel').hcSticky();
        }, 500);

        var heights = jQuery(".product-list > li").map(function ()
            {
                return jQuery(this).height();
            }).get(),

            maxHeight = Math.max.apply(null, heights) - 20;

        jQuery('.product-list .product').css({
            minHeight: maxHeight
        })
    }




    if(Mage.Cookies.get('curr_pid')){
        jQuery('#product-carousel-element-' + Mage.Cookies.get('curr_pid')).trigger('click');
    }

});
function isLoggedIn()
{
    var customerData = Mage.Cookies.get('CUSTOMER_DATA');
    var returnValue = false;
    if (customerData) {
        customerData = customerData.split(';');
        if (customerData[1] == 1) {
            returnValue = true;
        }
    }
    return returnValue;
}

function noThanksChoosen()
{
    var noThanks = Mage.Cookies.get('no_thanks');
    if (noThanks) {
        return true;
    }
    return false;
}

function clickSubscribeProduct(url)
{
    if (!isLoggedIn()) {
        jQuery('#register-modal-wrapper').addClass('active');
        jQuery('.product-tile-actions').each(function (key, element) {
            element.hide();
        });
    }else{
        setLocation(url);
    }
}
