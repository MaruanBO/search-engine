<script type="text/javascript">
document.getElementById("ad1").style.display = "none";
document.getElementById("ad2").style.display = "none";

var maxAd = 2;
var prevAd = 1;
var nextAd = 1;

var ran_unrounded = Math.random() * maxAd;
var ran_number = Math.ceil(ran_unrounded);
var loopAd = false;

function displayAd(whichAd) {

    if (loopAd) {
        clearTimeout(loopAd);
    }

    for (var i = 1; i <= maxAd; i++) {
        if (document.getElementById("ad" + i).style.display != "none") {
            document.getElementById("ad" + i).style.display = "none";
        }
    }

    document.getElementById("ad" + whichAd).style.display = "block";
    if (whichAd < maxAd) {
        nextAd = whichAd + 1;
    } else {
        nextAd = 1;
    }
    if (whichAd > 1) {
        prevAd = whichAd - 1;
    } else {
        prevAd = maxAd;
    }
}
displayAd(ran_number);
</script>
