<script type="text/javascript"> 
$(function () {
    var slideshow = $('.slider').cycle({
        timeout: 3500,
        speed: 2000,
        startingSlide: 0,
        fit: 1,
        height: 350,
        pager: '#slideNav',
        pagerAnchorBuilder: function (idx, slide) {
            return '#slideNav li:eq(' + (idx) + ') a';
        }
    });
});
$(document).ready(function () {
    $('#pause').click(function () {
        $('.slider').cycle('toggle');
        $(this).addClass('hide');
        $('#play').removeClass('hide');
    });
    $('#play').click(function () {
        $('.slider').cycle('toggle');
        $(this).addClass('hide');
        $('#pause').removeClass('hide');
    });
    $('#slideNav a').click(function () {
        $('.slider').cycle('pause');
        $('#play').removeClass('hide');
        $('#pause').addClass('hide');
    });
});
</script>
