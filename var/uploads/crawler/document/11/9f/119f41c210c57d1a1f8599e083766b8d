		var element_pos = 0;
		$('li.depth-0').each(function(){
			var submenu_1 = $(this).find('ul.menu-depth-1');
			var submenu_1_width = $(submenu_1).width();
			
			if((submenu_1_width + element_pos) > mainNav_width){
				$(submenu_1).css('left',(element_pos + ($(this).width() - submenu_1_width)) + 'px');
