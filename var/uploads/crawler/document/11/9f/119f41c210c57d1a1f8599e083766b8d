					<script src="http://www.trustyou.com/wp-content/plugins/contact-form-7/includes/js/jquery.form.min.js"></script>
					<script src="http://www.trustyou.com/wp-content/plugins/contact-form-7/includes/js/scripts.js"></script>
					<script type="text/javascript" src="http://www.trustyou.com/wp-content/themes/trustyou-theme-4/js/knockout-2.0.0.js"></script>
					<script type="text/javascript" src="http://www.trustyou.com/wp-content/themes/trustyou-theme-4/js/trustyou-com.js"></script>

					<script>
						var error_message = 'Please select a hotel';
						var showWidget = function(clusterId){
							$('.hotel-uuid').attr('value', clusterId);
							submitform_ts();
						};
						function showErrorBelow(element, message) {
							$('<p></p>')
							.text(message)
							.addClass('error')
							.hide()
							.insertAfter(element)
							.slideDown();
						}
						function submitform_ts() {
							if ($('.hotel-uuid').attr('value' ) ) {
								document.getElementById('Feedback').submit() ;
							}
							else
							{
								$('.error').remove();
								showErrorBelow('#Feedback', error_message);
							}
						}
						var typeahead = $('#select-hotel');
						typeahead.typeahead({
							sorter:function (items) {
								return items;
							},
							items:20,
							matcher:function (item) {
								var ret = [];
					            var parts = this.query.toLowerCase().split(/[\s,]+/); // split by whitespace

					            // check if each part matches the item name
					            var matched = true;
					            $(parts).each(function (p) {
					            	var m = item.toLowerCase().indexOf(this.toLowerCase()) >= 0;
					            	if (!m) matched = false;
					            });
					            return matched;
					        }
					    });

						typeahead.data('typeahead').name2Id = {};
						typeahead.data('typeahead').select = function() {
							var val = this.$menu.find('.active').attr('data-value');
							var clusterId = typeahead.data('typeahead').name2Id[val];
							showWidget(clusterId);
							this.$element.val(val);
							return this.hide();
						}

						$('#select-hotel').keyup(function (e) {
							e.stopPropagation()
							e.preventDefault()
							var dropdown = $(e.target);

							switch (e.keyCode) {
					            case 40: // down arrow
					            case 38: // up arrow
					            return

					            case 9: // tab
					            case 13: // enter
					            if (!dropdown.shown) return
					            	dropdown.select()
					            return

					            case 27: // escape
					            dropdown.data('typeahead').hide()
					            return
					        }

					        var val = $(dropdown).val();

					        if (val.length > 2) {
					        	$.ajax({
					        		url: 'https://api.trustyou.com/hotels/search?prefix=' + val + '&page=0&page_size=15',
					        		dataType: 'jsonp',
					        		success: function(data, textStatus, jqXHR){
					        			var names = [];
					        			$.each(data.response.cluster_list, function (key, val) {
					        				var stringValue = val.name + ", " + val.city;
					        				names.push(stringValue);
					        				typeahead.data('typeahead').name2Id[stringValue] = val.cluster_id;
					        			});
					                        // TODO delegate call to Typeahead object
					                        $(dropdown).data('typeahead').source = names;
					                        $(dropdown).data('typeahead').lookup();
					                        //$(dropdown).typeahead('show');
					                    }
					                });
					        } else {
					        	$(dropdown).data('typeahead').source = [];
					        }
					    });
</script>
