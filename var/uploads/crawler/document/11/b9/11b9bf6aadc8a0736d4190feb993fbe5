  var startDelay = 1000;
  var switchDelay = 500;

    
  function sparkleTimer(customStartDelay) {
    if(!customStartDelay) customStartDelay = startDelay;
  
    setTimeout(function(){
    
      /* !!!SPARKLE TIMER!!! */
      
      if( $("div.presents").length > 0 ) {
      
        setTimeout(function(){
          $("#demo_circle").show();
        }, switchDelay);
        
        setTimeout(function(){
          $("#demo_circle").addClass("pos_1");
        }, switchDelay*2);
        
        setTimeout(function(){
          $("#demo_circle").removeClass("pos_1").addClass("pos_2");
        }, switchDelay*3);
        
        setTimeout(function(){
          $("#demo_circle").removeClass("pos_2").addClass("pos_3");
        }, switchDelay*4);
        
        setTimeout(function(){
          $("#demo_circle").removeClass("pos_3").addClass("pos_4");
        }, switchDelay*5);
        
        setTimeout(function(){
          $("#demo_circle").removeClass("pos_4").hide();
        }, switchDelay*6);
      
      }
    
    },customStartDelay);
  }

  </script>


