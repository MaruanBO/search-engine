    <script>
      var navClassOn  = new Array('navOnTop', 'navOnFirst', 'navOnSecond', 'navOnThird', 'navOnBottom');
      var navClassOff = new Array('navOffTop', 'navOffFirst', 'navOffSecond', 'navOffThird', 'navOffBottom');
      var navCnt = 5;
      
      for (var i = 0; i < navCnt; i++)
      {
        var navImg = new Image();
        navImg.src = 'http://static1.lowerprices.us/images/' + navClassOn[i] + '.png';
        navImg.src = 'http://static1.lowerprices.us/images/' + navClassOff[i] + '.png';
      }
      
      function changeNavClassName(num)
      {
        for (var i = 0; i < navCnt; i++)
        {
          if (i == num)
          {
            document.getElementById('frontPageCaroNav' + i).className = navClassOn[i];
            document.getElementById('frontPageCaro' + i).style.display = '';
          }
          else
          {
            document.getElementById('frontPageCaroNav' + i).className = navClassOff[i];
            document.getElementById('frontPageCaro' + i).style.display = 'none';
          }
        }
        var arrowMargin = num * 63;
        document.getElementById('farrow').style.marginTop = arrowMargin + 'px';
      }
    </script>
