    <script type="text/javascript">
        (function ($) {
            $(window).load(function () {
                $(".cscroll").mCustomScrollbar();

                if ($.cookie('hide_help') != "1") {
                    ShowHelp();
                }

                $("#search-btn").mouseenter(function (e) {
                    ShowSearch();
                });

                $("#search-btn").click(function (e) {
                    ExecuteSearch();
                });
                var searchShowHideSearchTimer = "t_pctShowHideSearch";
                $('#search-term').blur(function (e) {
                    searchShowHideSearchTimer = setTimeout(function () { HideSearch(); }, 500);
                });

                function HideSearch() {
                    $('#search-term').animate({ "right": "0px", "width": "0px" }, 400, function () { $('#search-term').val('').hide(); });
                }

                function ShowSearch() {
                    clearTimeout(searchShowHideSearchTimer);
                    $('#search-term').val('').show().animate({ "right": "115px", "width": "150px" }, 500, function () { $('#search-term').focus(); });
                }

                $('#search-term').keypress(function (e) {
                    if (e.which == 13) {
                        jQuery(this).blur();
                        ExecuteSearch();
                    }
                });

                function ExecuteSearch() {
                    window.location.href = "/NFL/Search/" + $('#search-term').val();
                }

                function ShowHelp() {
                    $('#screenBlock').fadeIn("fast");
                    $('#helpOverlay').fadeIn("fast");
                }

                function HideHelp() {
                    $('#screenBlock').fadeOut("fast");
                    $('#helpOverlay').fadeOut("fast");
                    $.cookie('hide_help', '1', { expires: 365, path: '/' });
                }

                $("#screenBlock").click(function (e) {
                    HideHelp();
                });

                $("#helpOverlay").click(function (e) {
                    HideHelp();
                });

            });
        })(jQuery);

