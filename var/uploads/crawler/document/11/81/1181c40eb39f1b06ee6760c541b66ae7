    $(".fieldset").each(function (i) {
        var filterName = $(this).attr("data-filter-name");
        var timerName = "timer_" + filterName;
            $(this).mouseenter(function(e) {
                var pos = $(this).offset();
                $(this).addClass("on");
            });
            
            $(this).mouseleave(function(e) {
                var $this = jQuery(this);
                $this.removeClass('on');
             }
            );

            $("#" + filterName + "-container").mouseleave(function(e) {
            },
             function(){
                    var $this = jQuery(this);
                    timerName = setTimeout( function(){ $this.removeClass('on'); }, 50);
                    }   
            );
    });    
   
    
    function FetchStats(isTimeout) {
           
            var conferenceId = $("#Query_ConferenceId_Value").val();          
           if ("FALSE" == isTimeout) {
               $("#Query_GameResult_ValueCollection").val(GetCheckBoxValueArray('input:checkbox:checked.game-result'));
               $("#Query_GameLocation_ValueCollection").val(GetCheckBoxValueArray('input:checkbox:checked.game-location'));
               $("#Query_GamePostSeason_ValueCollection").val(GetCheckBoxValueArray('input:checkbox:checked.game-post-season'));
           }

        var form = $('form');

            $.ajax({
                type: 'POST',
                url: "/Stats/GetConferenceStats",
                data: form.serialize(),
                beforeSend: function () {$("#stats-container").fadeTo(200, .2);HideDatasetFilters();},
                success: function(result) {
                    if (result.indexOf("[000:DNR]") >= 0) {
                        setTimeout(function() {
                            FetchStats("TRUE");
                        }, 300);
                    }
                    else {
                        sLog(result);
                        SetConferenceName(conferenceId);
                        $("#stats-container").fadeTo(100, 1);
                        $("#stats-container").html(result);
                        Initialize();
                        UpdateFilterDisplay($('#filter-ul'));
                        document.title = GetDatasetDescription();
                        UpdateSocialButtons;
                         _gaq.push(['_setAccount', 'UA-27365001-1']);
                        _gaq.push(['_trackPageview']);
                    }
                }
            });
        }
    
            function SetConferenceName(confId) {
            var name = conferenceNameHashTable[confId];
                if (0 == confId) {
                    name = "NFL";
                }
            $('#subject-name').html(name);
            return name;
        }
            
