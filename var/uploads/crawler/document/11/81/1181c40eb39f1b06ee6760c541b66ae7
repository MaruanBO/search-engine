<script type="text/javascript">
    
        function RefreshStats() {
            sLog("-------------> RefreshStats");
            var filterCriteria = GetFilterCriteria();
            confId = $("#Query_ConferenceId_Value").val();
            History.pushState({ confId: confId, q: filterCriteria }, "@StatMilk - Fetching Stats..." , "/../NFL/" + confId + "/?" + filterCriteria );
        }


        function ChangeTeam (teamId) {
            if (teamId > 0)
            {
                window.location = "/../NFL/Team/" + teamId;
            }
        }

        function ChangeConference (confId) {
            if (confId >= 0)
            {
                var filterCriteria = GetFilterCriteria();
                History.pushState({ confId: confId, q: filterCriteria }, "@StatMilk - Fetching Stats..."  , "/../NFL/" + confId + "/?" + filterCriteria );
                $("#pop-change-team").hide();
                $("#subject-team").attr('class', 'subject-team');
                return false;
            }
        }
        
        
        function FilterTeamSelect(criteria) {
               $('.team-select-list li').hide();
               $('.ul-conf li').attr('class', '');
              $(".ul-conf li[data-conference='" + criteria + "']").attr('class', 'selected');
              if (criteria == "ALL") {
                     $('.team-select-list li').show();
                    } else {
                  $(".team-select-list li[data-conference='" + criteria + "']").show();
              }
               $(".cscroll").mCustomScrollbar("update");
          }
    

            function GetFilterCriteria()
        {
            var filterArray = [];

            if ($("#Query_OpponentTeam_Value").val() != "0"){filterArray.push("oid=" + $("#Query_OpponentTeam_Value").val())};
if ($("#Query_OpponentConference_Value").val() != "0"){filterArray.push("ocid=" + $("#Query_OpponentConference_Value").val())};
if ($("#Query_OpponentDivision_Value").val() != "0"){filterArray.push("od=" + $("#Query_OpponentDivision_Value").val())};
if (GetCheckBoxValueArray('input:checkbox:checked.game-location') != "1,0"){filterArray.push("gl=" + GetCheckBoxValueArray('input:checkbox:checked.game-location'))};
if (GetCheckBoxValueArray('input:checkbox:checked.game-result') != "1,0"){filterArray.push("gr=" + GetCheckBoxValueArray('input:checkbox:checked.game-result'))};
if (GetCheckBoxValueArray('input:checkbox:checked.game-post-season') != "1,0"){filterArray.push("ps=" + GetCheckBoxValueArray('input:checkbox:checked.game-post-season'))};
if ($("#Query_SeasonRange_RangeStart").val() != $("#Query_SeasonRange_RangeStart").attr("dataValueDefault")|| $("#Query_SeasonRange_RangeEnd").val() != $("#Query_SeasonRange_RangeEnd").attr("dataValueDefault")){filterArray.push("sr=" + $("#Query_SeasonRange_RangeStart").val() + "-" + $("#Query_SeasonRange_RangeEnd").val())};
if ($("#Query_DayOfWeekRange_RangeStart").val() != $("#Query_DayOfWeekRange_RangeStart").attr("dataValueDefault")|| $("#Query_DayOfWeekRange_RangeEnd").val() != $("#Query_DayOfWeekRange_RangeEnd").attr("dataValueDefault")){filterArray.push("dr=" + $("#Query_DayOfWeekRange_RangeStart").val() + "-" + $("#Query_DayOfWeekRange_RangeEnd").val())};
if ($("#Query_WeekRange_RangeStart").val() != $("#Query_WeekRange_RangeStart").attr("dataValueDefault")|| $("#Query_WeekRange_RangeEnd").val() != $("#Query_WeekRange_RangeEnd").attr("dataValueDefault")){filterArray.push("w=" + $("#Query_WeekRange_RangeStart").val() + "-" + $("#Query_WeekRange_RangeEnd").val())};
if ($("#Query_TimeOfPossessionRange_RangeStart").val() != $("#Query_TimeOfPossessionRange_RangeStart").attr("dataValueDefault")|| $("#Query_TimeOfPossessionRange_RangeEnd").val() != $("#Query_TimeOfPossessionRange_RangeEnd").attr("dataValueDefault")){filterArray.push("topr=" + $("#Query_TimeOfPossessionRange_RangeStart").val() + "-" + $("#Query_TimeOfPossessionRange_RangeEnd").val())};
if ($("#Query_PassingYardsRange_RangeStart").val() != $("#Query_PassingYardsRange_RangeStart").attr("dataValueDefault")|| $("#Query_PassingYardsRange_RangeEnd").val() != $("#Query_PassingYardsRange_RangeEnd").attr("dataValueDefault")){filterArray.push("pyr=" + $("#Query_PassingYardsRange_RangeStart").val() + "-" + $("#Query_PassingYardsRange_RangeEnd").val())};
if ($("#Query_RushingYardsRange_RangeStart").val() != $("#Query_RushingYardsRange_RangeStart").attr("dataValueDefault")|| $("#Query_RushingYardsRange_RangeEnd").val() != $("#Query_RushingYardsRange_RangeEnd").attr("dataValueDefault")){filterArray.push("ryr=" + $("#Query_RushingYardsRange_RangeStart").val() + "-" + $("#Query_RushingYardsRange_RangeEnd").val())};
if ($("#Query_TotalOffenseRange_RangeStart").val() != $("#Query_TotalOffenseRange_RangeStart").attr("dataValueDefault")|| $("#Query_TotalOffenseRange_RangeEnd").val() != $("#Query_TotalOffenseRange_RangeEnd").attr("dataValueDefault")){filterArray.push("tor=" + $("#Query_TotalOffenseRange_RangeStart").val() + "-" + $("#Query_TotalOffenseRange_RangeEnd").val())};
if ($("#Query_PassingAttemptsRange_RangeStart").val() != $("#Query_PassingAttemptsRange_RangeStart").attr("dataValueDefault")|| $("#Query_PassingAttemptsRange_RangeEnd").val() != $("#Query_PassingAttemptsRange_RangeEnd").attr("dataValueDefault")){filterArray.push("par=" + $("#Query_PassingAttemptsRange_RangeStart").val() + "-" + $("#Query_PassingAttemptsRange_RangeEnd").val())};
if ($("#Query_RushingAttemptsRange_RangeStart").val() != $("#Query_RushingAttemptsRange_RangeStart").attr("dataValueDefault")|| $("#Query_RushingAttemptsRange_RangeEnd").val() != $("#Query_RushingAttemptsRange_RangeEnd").attr("dataValueDefault")){filterArray.push("rar=" + $("#Query_RushingAttemptsRange_RangeStart").val() + "-" + $("#Query_RushingAttemptsRange_RangeEnd").val())};
