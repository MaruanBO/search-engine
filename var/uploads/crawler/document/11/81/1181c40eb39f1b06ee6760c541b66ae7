    $(document).ready(function () {
     
    $("#filter-open").click(function (e) {
        ShowDatasetFilters();
        $(this).hide();
    });

    $("#btn-edit-filters").click(function (e) {
        ShowDatasetFilters();
    });

    $("#btn-filter-cancel").click(function (e) {
        HideDatasetFilters();
    });
      
                
    $("#btn-update-stats").click(function (e) {
        RefreshStats();
    });     

   
    
