        var History = window.History; // Note: We are using a capital H instead of a lower h
        sLog("[History Object Initialized] = " + History);
        var State = History.getState();

        // Bind to StateChange Event
        History.Adapter.bind(window,'statechange',function(){ // Note: We are using statechange instead of popstate
            var State = History.getState(); // Note: We are using History.getState() instead of event.state
            sLog("**History State Changed**")
            //History.log(State.data, State.title, State.url);
        
            var now = new Date();
            var confId = State.data["confId"];
            var query = State.data["q"];
            sLog("[History].Adapter] " + query);
            sLog("[History] Hash Changed = "  + " (" + now.getMinutes() + ":" + + now.getSeconds() + ") Conference " + confId);
                
            if (confId >= 0)
            {
                $("#Query_ConferenceId_Value").val(confId);
                sLog("[History] Query = " + query);
                    
                if (query.length > 0)
                {
                    var qObj = $.deparam.querystring(query.replace("/","&"));
                    sLog("qObj=" + qObj);
                    sLog(qObj);
                    
//                    if (qObj.hasOwnProperty("orr")) {
//                        SetRangeFilterValue('#slider-opponentRank-range', startVal, endVal, 'FALSE');
//                    } else {
//                        SetRangeFilterValue('#slider-opponentRank-range', 0, 26, 'FALSE'); 
//                    }
                    
                    $.each(
                        qObj,
                        function(key, values){
                            sLog("Processing key [" + key + "] values = [" + values + "]");
                            var valueArray = values.split("-"); 

                            if (2 == valueArray.length) {
                                var startVal = valueArray[0];
                                var endVal = valueArray[1];
                                
                                if (IsNumber(startVal) && IsNumber(endVal)) {
                                     if (key == "orr") {
                                           SetRangeFilterValue('#slider-opponentRank-range', startVal, endVal, 'FALSE');
                                        } else if (false == qObj.hasOwnProperty("orr")) {
                                            SetRangeFilterValue('#slider-opponentRank-range', 0, 26, 'FALSE');
                                     }
                                    if (key == "psr") {
                                           SetRangeFilterValue('#slider-pointsScored-range', startVal, endVal, 'FALSE');
                                        } else {
                                        SetRangeFilterValue('#slider-pointsScored-range', 0, 90, 'FALSE');
                                     }
                                }
                                else {
                                    "Alert: startVal or endVal isNumber = false";
                                }
                            }
                            else {
                                sLog("**valueArray.length == " + valueArray.length);
                            }
                        }
                        );

                }
                FetchStats("FALSE");
            } else {
                sLog("[History] confId < 0 (History.Adapter.bind)");
            }

        });
                        

        var filterCriteria = GetFilterCriteria();
        History.replaceState({ confId: $("#Query_ConferenceId_Value").val(), q: filterCriteria }, SetConferenceName($("#Query_ConferenceId_Value").val()) , "/../NFL/" + $("#Query_ConferenceId_Value").val() + "/?" + filterCriteria);       
        //RefreshStats();
            }); //Ready        function Initialize() {
        var popChangeTeamHideTimer = "t_pctHide";
        var popChangeTeamShowTimer = "t_pctShow";                 $("#subject-team").click(function() {
             ShowSubjectSelect();
        });        
        $("#pop-change-team").mouseenter(function(e) {
            clearTimeout(popChangeTeamHideTimer);
             //popChangeTeamShowTimer = setTimeout(function() { ShowSubjectSelect();}, 250);
        });        $("#subject-team").hover(function() {
            clearTimeout(popChangeTeamHideTimer);
            // popChangeTeamShowTimer = setTimeout(function() { ShowSubjectSelect();}, 250);
        });
        
        $("#subject-team").mouseleave(function(e) {
             clearTimeout(popChangeTeamShowTimer);
            popChangeTeamHideTimer = setTimeout(function() { HideSubjectSelect();}, 500);
        });
        
        $("#change-team-cancel").click(function() {
           HideSubjectSelect();
        });
            
        $("#pop-change-team").mouseleave(function(e) {
            clearTimeout(popChangeTeamShowTimer);
            popChangeTeamHideTimer = setTimeout(function() { HideSubjectSelect(); }, 500);
        });

        
        function ShowSubjectSelect() {
            var pos = $("#subject-team").offset();
            $("#pop-change-team").css( { "left": (pos.left - 2) + "px"} );
            $("#pop-change-team").css( { "top": ($("#subject-team").scrollTop() + 128) + "px"} );
            $("#pop-change-team").show();
            $(".cscroll").mCustomScrollbar("update");
            $("#subject-team").attr('class', 'subject-team-sel');
        }

        function HideSubjectSelect() {
            $("#pop-change-team").hide();
            $("#subject-team").attr('class', 'subject-team');
        }

    }

</script>
