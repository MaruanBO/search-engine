								var mbn_share_delay = $(this).attr('data-share-delay'); 
				    		var href_load = '/sites/all/modules/spe_share/likes/likes.php?b=' + $(this).attr('data-share');
				    		if (jQuery.inArray(mbn_share_id, mbn_share_loaded) == -1) { 
				    			$('#' + mbn_share_id).fadeOut();
				    			$('#' + mbn_share_id).load(href_load);
				    			$('#' + mbn_share_id).fadeIn();
