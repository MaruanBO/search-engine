<script>
function rand_num(min,max) {
	var number = min + Math.floor(Math.random() * max);
	return number;
}
function numberCom(val) {
	while (/(\d+)(\d{3})/.test(val.toString())) val = val.toString().replace(/(\d+)(\d{3})/, "$1"+","+"$2");
	return val;
}
function mkChanges() {
	//var user = $("#CurrentMembers").html().replace(/\,/g,'');
	//var quote = $("#quote").html().replace(/\,/g,'');
	var current_money = $("#TotalProfit").html().replace(/\,/g,'');
	var member = $("#MemberAvailable").html().replace(/\,/g,'');
	
	//var plus = rand_num(0,2);
	//var quote_plus = rand_num(0,8);
	var money_plus = rand_num(200,800);
	var member_minus = rand_num(0,2);
	
	//var newOnline = parseInt(user) + plus;
	//var newQuote = parseInt(quote) + quote_plus;
	var newMoney = parseInt(current_money) + money_plus;
	var newMember = parseInt(member) - member_minus;

	//$("#CurrentMembers").html(numberCom(newOnline));
	//$("#quote").html(numberCom(newQuote));
	$("#TotalProfit").html(numberCom(newMoney));
	if(member>1)
		$("#MemberAvailable").html(numberCom(newMember));
}
setInterval(mkChanges,6000);
</script>

