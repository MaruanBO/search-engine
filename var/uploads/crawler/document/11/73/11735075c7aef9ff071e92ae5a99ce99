                 };*/
              }
           var staffSpot = document.getElementsByClassName("staff-spotlight-rotator")[0];
           var staffSpotLinkDivs = staffSpot.getElementsByClassName("in_ohio");
           var staffSpotLink;
           for(var i=0;i<staffSpotLinkDivs.length;i++)
