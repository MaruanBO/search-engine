            <!-- START NEW SCRIPT FOR SHOW DROP DOWN DESKTOP VERSIONS -->
			<script type="text/javascript">
			// Wait for the page and all the DOM to be fully loaded
			jQuery('body').ready(function() {
				// Add the 'hover' event listener to our drop down class
				jQuery('.menu-drop-item-show').hover(function() {
					// When the event is triggered, grab the current element 'this' and
					// find it's children '.sub_navigation' and display/hide them
					jQuery(this).find('#section-div-drop-menu').stop(true, true).fadeToggle(300);
					jQuery(this).find('.menu-drop-link-item').toggleClass('active').stop(true, true);
				});
			});
			</script>
			<!-- END NEW SCRIPT FOR SHOW DROP DOWN DESKTOP VERSIONS -->

            <!-- ADVERTISING MARKETPLACE MENU SCRIPT -->
              <script type="text/javascript">
					jQuery(document).ready(function(){
					jQuery('#page-menu-select').bind('mouseenter', function(){
						//$(this).parent().find('#page-menu-closed').toggleClass('closed');
						jQuery('#section-drop-menu').toggleClass('menu-open').stop(true,true).fadeIn(300);
					});
					jQuery('#page-menu-select').bind('mouseleave', function(){
						//$(this).parent().find('#page-menu-open').toggleClass('open');
						jQuery('#section-drop-menu').toggleClass('menu-open').stop(true, true).fadeOut(300);
					});
                    jQuery('#section-drop-menu').hover(function () {
                        jQuery('#page-menu-select').toggleClass('active').stop(true, true);
                    });
                    });
				</script>
            <!-- END ADVERTISING MARKETPLACE MENU SCRIPT -->

            <!-- HOMES DROP DOWN MENU SCRIPT -->
              <script type="text/javascript">
					jQuery(document).ready(function(){
					jQuery('#page-menu-select-two').bind('mouseenter', function(){
						//$(this).parent().find('#page-menu-closed').toggleClass('closed');
						jQuery('#section-drop-menu-two').toggleClass('menu-open').stop(true,true).fadeIn(300);
					});
					jQuery('#page-menu-select-two').bind('mouseleave', function(){
						//$(this).parent().find('#page-menu-open').toggleClass('open');
						jQuery('#section-drop-menu-two').toggleClass('menu-open').stop(true, true).fadeOut(300);
					});
                    jQuery('#section-drop-menu-two').hover(function () {
                        jQuery('#page-menu-select-two').toggleClass('active').stop(true, true);
                     });
