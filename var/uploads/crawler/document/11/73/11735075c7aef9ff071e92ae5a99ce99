                   if(isOverflowing)
                      {
                      this.parentNode.parentNode.style.height = "auto";
                      }
                      else
                      {
                      this.parentNode.parentNode.style.height = "204px";
                      }
                 };
               blogLink.onmouseout = function()
                 {
                   this.parentNode.parentNode.style.height = "204px";
                   checkOverflow(this.parentNode.parentNode);
                 };*/
              }
           var staffSpot = document.getElementsByClassName("staff-spotlight-rotator")[0];
           var staffSpotLinkDivs = staffSpot.getElementsByClassName("in_ohio");
           var staffSpotLink;
           for(var i=0;i<staffSpotLinkDivs.length;i++)
           {
              staffSpotLinkDivs[i].style.height = "204px";
              staffSpotLink = staffSpotLinkDivs[i].getElementsByTagName("a")[0];
              title = staffSpotLink.textContent;
              staffSpotLink.title = title.trim();
              checkOverflow(staffSpotLinkDivs[i]);
             //"Staff SpotLight" Hover Functionality
             /*staffSpotLink.onmouseover = function()
					{
