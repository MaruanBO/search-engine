<script type="text/javascript">
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
	jQuery("#homepage-link").css("display", "none");
}

//function msieversion() {
        //var ua = window.navigator.userAgent;
        //var msie = ua.indexOf("MSIE ");
//}
 
/* Add as home page script - Dave Goodman goodmangraphics.co.uk */
function addashomepage(){
	//Bring back the browser object
	jQuery.browser = {};
	//Rest all is self explanatory.
	jQuery.browser.mozilla = /mozilla/.test(navigator.userAgent.toLowerCase())
						&& !/webkit/.test(navigator.userAgent.toLowerCase());
	jQuery.browser.webkit = /webkit/.test(navigator.userAgent.toLowerCase());
	jQuery.browser.opera = /opera/.test(navigator.userAgent.toLowerCase());
	jQuery.browser.msie = /msie/.test(navigator.userAgent.toLowerCase());
	//jQuery.browser.maxthon = /(lunascape|maxthon|netfront|jasmine|blazer)[\/\s]?((\d+)?[\w\.]+)*/i.test(navigator.userAgent.toLowerCase());
	//jQuery.browser.maxthon = navigator.userAgent.indexOf("Maxthon") > -1 && jQuery.browser.webkit && !window.chrome;
	//jQuery.browser.maxthon = (navigator.userAgent.indexOf('Maxthon') != -1 && navigator.userAgent.indexOf('Maxthon') == -1);
	//jQuery.browser.maxthon = !!navigator.userAgent.match(/Version\/[\d\.]+.*Maxthon/)
	//jQuery.browser.maxthon = /maxthon/.test(navigator.userAgent.toLowerCase());
 
    // makes jQuery differentiate between Chrome and Safari - thanks to Vikrant Chaudhary
    jQuery.browser.chrome = jQuery.browser.webkit && !!window.chrome;
    jQuery.browser.safari = jQuery.browser.webkit && !window.chrome;
	//$.browser.opera  = /opera/.test(navigator.userAgent.toLowerCase());
	//var Opera = (navigator.userAgent.match(/Opera|OPR\//) ? true : false);
	//var isOpera = Object.prototype.toString.call(window.opera) == '[object Opera]';
	
	var ua = window.navigator.userAgent;
	var msie = ua.indexOf("MSIE ");
	
    if (jQuery.browser.msie) { // IE 
        if (jQuery.browser.msie  && parseInt(jQuery.browser.version, 10) < 8) { // IE is v7 or lower - use ye olde homepage code
            document.body.style.behavior='url(#default#homepage)';
            document.body.setHomePage(window.location.href);
        } else { // IE v8 or higher
            var homesettext = "<p><strong>Internet Explorer</strong><br /><br />To set your homepage in Internet Explorer 9 or later:</p><ol ><li>Open the main internet tools menu in your browser. This will be called something like <strong>TOOLS, EDIT, FIREFOX or SAFARI</strong>.</li><li>Select the tab marked <strong>GENERAL, PREFERENCES, OPTIONS or INTERNET OPTIONS</strong> (depending on your browser).</li><li>Look for a box labeled <strong>HOMEPAGE, HOMEPAGE LOCATION</strong> or something similar. Type <em>http://www.ohio.com/ </em> into this box and click OK. Your new homepage will be set.</li><li style='list-style-type: none;'><br>If you need further help setting your homepage, check your browser's Help menu.</li></ol>";
        };
		//if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) { // IE11
			//var homesettext = "<p><strong>Internet Explorer</strong><br /><br />To set your homepage in Internet Explorer 9 or later:</p><ol ><li>Open the main internet tools menu in your browser. This will be called something like <strong>TOOLS, EDIT, FIREFOX or SAFARI</strong>.</li><li>Select the tab marked <strong>GENERAL, PREFERENCES, OPTIONS or INTERNET OPTIONS</strong> (depending on your browser).</li><li>Look for a box labeled <strong>HOMEPAGE, HOMEPAGE LOCATION</strong> or something similar. Type <em>http://www.ohio.com/ </em> into this box and click OK. Your new homepage will be set.</li><li style='list-style-type: none;'><br>If you need further help setting your homepage, check your browser's Help menu.</li></ol>";       
		//}
    } else if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) { // IE11
		var homesettext = "<p><strong>Internet Explorer</strong><br /><br />To set your homepage in Internet Explorer 9 or later:</p><ol ><li>Open the main internet tools menu in your browser. This will be called something like <strong>TOOLS, EDIT, FIREFOX or SAFARI</strong>.</li><li>Select the tab marked <strong>GENERAL, PREFERENCES, OPTIONS or INTERNET OPTIONS</strong> (depending on your browser).</li><li>Look for a box labeled <strong>HOMEPAGE, HOMEPAGE LOCATION</strong> or something similar. Type <em>http://www.ohio.com/ </em> into this box and click OK. Your new homepage will be set.</li><li style='list-style-type: none;'><br>If you need further help setting your homepage, check your browser's Help menu.</li></ol>";       
	
	} else if (navigator.userAgent.match(/maxthon/i) || navigator.userAgent.match(/maxthon/i) ) { // maxthon
        var homesettext = "<p><strong>Maxthon</strong><br /><br />To set your homepage in Maxthon:</p><ol><li>In the top-right corner of your window, click the Maxthon menu. From the drop-down menu that appears, select <strong>SETTINGS</strong>.</li><li>Under 'Open On Startup', select the <strong>Open a specific page or set of pages. Set pages</strong> radial box.</li><li>Click the <strong>SET PAGES</strong> text, type <em>http://www.ohio.com/ </em>, in the text box, then click <strong>OK</strong>.</li></ol>";      
    
	} else if (jQuery.browser.safari) { // safari
        var homesettext = "<p><strong>Safari</strong><br /><br />To set your homepage in Safari for Mac OS X or Microsoft Windows:</p><ol><li>Select the Safari gear icon at the top of your browser window. From the drop-down menu that appears, select <strong>PREFERENCES</strong>.</li><li>At the top of the dialog box, select the <strong>GENERAL</strong> tab.</li><li>In the <strong>HOME PAGE</strong> section, type <em>http://www.ohio.com/ </em>, then click the red 'close' button.</li></ol>";       
 
    } else if (jQuery.browser.webkit) { // chrome
        var homesettext = "<p><strong>Chrome | Opera</strong><br /><br />To set your homepage in Chrome or Opera:</p><ol><li><span style='color:#9e0a0a;'>(Chrome)</span> In the top-right corner of your window, click the Chrome menu.<br /><span style='color:#9e0a0a;'>(Opera)</span> In the top-left browser window click the Opera button.<br /> From the drop-down menu that appears, select <strong>SETTINGS</strong>.</li><li>Under 'On startup', select the <strong>Open a specific page or set of pages. Set pages</strong> radial box.</li><li>Click the <strong>SET PAGES</strong> text, type <em>http://www.ohio.com/ </em>, in the text box, then click <strong>OK</strong>.</li></ol>";     
 
    } else if (jQuery.browser.mozilla) { // firefox
        var homesettext = "<p><strong>Mozilla Firefox</strong><br /><br />To set your homepage in Firefox</p><ol><li>In the <strong>TOOLS</strong> menu, select <strong>OPTIONS</strong>.</li><li>At the top of the dialog box, select the <strong>GENERAL</strong> tab.</li><li>In the <strong>HOME PAGE</strong> text box, type <em>http://www.ohio.com/ </em>, then click OK. </li></ol>";     
	
	} else if (jQuery.browser.opera) { // opera
        var homesettext = "<p><strong>Opera</strong><br /><br />To set your homepage in Opera on Microsoft Windows:</p><ol><li>Navigate to and click on the 'Preferences' link from the drop-down menu at the top of the Opera browser. The Preferences box will open. Select the 'General' tab.</li><li>Click on 'Set current page as home page'.</li><li>Click <strong>OK</strong>.</li></ol>To set your homepage in Opera on Mac OS X:</p><ol><li>Click on the top menu option 'Opera,' and then click on the 'Preferences' option. The Preferences box will open with the General tab already selected.</li><li>click 'Use Current.' Then utilize the drop-down menu to choose the desired navigation option.</li><li>Click <strong>OK</strong>.</li></ol>";      
    
	} else { // unknown browser
        var homesettext = "<p><strong>Unknown browser type</strong><br /><br />To set your homepage please refer to your browser's help</a>";       
    };
 
    if (homesettext){
        $("#homepagePopup .set-text").html(homesettext);
		$("#homepagePopup-overlay").fadeIn();
        $("#homepagePopup").fadeIn();
		$("#homepagePopup").draggable({containment: "self"});
    };
 
};
 
$(document).ready(function() {
    $("#homepagePopup .close").click(function() {
		$("#homepagePopup").fadeOut();
		$("#homepagePopup-overlay").fadeOut();
    });
 
    $("#bookmarkPopup .close").click(function() {
		$("#bookmarkPopup").fadeOut();
		$("#homepagePopup-overlay").fadeOut();
    });
});
</script>

      
    
    
