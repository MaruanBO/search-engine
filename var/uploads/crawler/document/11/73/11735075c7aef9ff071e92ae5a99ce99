		function checkOverflow(el)
		 {
			curOverflow = el.style.overflow;
			if (!curOverflow || curOverflow === "visible")
				el.style.overflow = "hidden";
			isOverflowing = el.clientWidth < el.scrollWidth || el.clientHeight < el.scrollHeight;
			while(isOverflowing)
			{
				fChild = el.getElementsByTagName("a")[0];
				text = fChild.innerHTML.split(" ");
				text.pop();
				fChild.innerHTML = text.join(" ") + "...";
				isOverflowing = el.clientWidth < el.scrollWidth || el.clientHeight < el.scrollHeight;

			}
			el.style.overflow = curOverflow;
		 }
		 
		 window.onresize = function()
		 {
		 for(var j=0;j<classNames.length;j++)
		 {
				categoryDiv = document.getElementsByClassName(classNames[j])[0];
				fourStoryDiv = categoryDiv.getElementsByClassName("four_stories")[0];
				storyLink = fourStoryDiv.getElementsByTagName("a");
				storyDiv = fourStoryDiv.getElementsByClassName("section-lead-headline");
				
				if(divWidth > storyDiv[0].clientWidth)
		{
			 for(var i=0;i<storyDiv.length;i++)
			 {
					checkOverflow(storyDiv[i]);
			 }
		 }
		
		if (divWidth < storyDiv[0].clientWidth)
			{
				for(var i=0;i<storyDiv.length;i++)
				{
					text = storyDiv[i].getElementsByTagName("a")[0].title;
					storyDiv[i].getElementsByTagName("a")[0].innerHTML = text;
					checkOverflow(storyDiv[i]);
				}
			}
		 }
		 
			
		divWidth = storyDiv[storyDiv.length - 1].clientWidth;
		 }
		 
		 window.onload = function()
		 {
           if(document.URL == "http:\\ohio.com")
           { console.log(document.URL);}
			 for(var j=0;j<classNames.length;j++)
			 {
				categoryDiv = document.getElementsByClassName(classNames[j])[0];
				fourStoryDiv = categoryDiv.getElementsByClassName("four_stories")[0];
				storyLink = fourStoryDiv.getElementsByTagName("a");
				storyDiv = fourStoryDiv.getElementsByClassName("section-lead-headline");
               for(var i=0;i<storyDiv.length;i++)
               {
                 storyDiv[i].style.height = "40px";
               }
				
				for(var i=0;i<storyLink.length;i++)
				{
					storyLink[i].title = storyLink[i].textContent.trim(); 
					storyLink[i].onmouseover = function()
					{
                      this.textContent = this.title;
                     
                      isOverflowing = this.parentNode.clientWidth < this.parentNode.scrollWidth || this.parentNode.clientHeight < this.parentNode.scrollHeight;
                      
                      if(isOverflowing)
                      {
                        this.parentNode.style.height = "auto";
                      }
                      else
                      {
                        this.parentNode.style.height = "40px";
                      }
					};
					storyLink[i].onmouseout = function()
					{
                      this.parentNode.style.height = "40px";
                      checkOverflow(this.parentNode);
					};
				}
				
				for(var i=0;i<storyDiv.length;i++)
				{
					checkOverflow(storyDiv[i]);
				}
			 }
           
             var blogDiv = document.getElementsByClassName("blog-spotlight-rotator")[0];
             var blogLinkContainers = blogDiv.getElementsByClassName("in_ohio");
             var blogLink;
             var title;
			 for (var i=0;i<blogLinkContainers.length;i++)
			 {
               blogLinkContainers[i].style.height = "204px";
               blogLink = blogLinkContainers[i].getElementsByTagName("a")[0];
               title = blogLink.textContent.split("")[1];
               blogLink.title = title.trim();
               checkOverflow(blogLinkContainers[i]);
               //"Our Blogs" Hover Functionality
               /*blogLink.onmouseover = function()
                 {
