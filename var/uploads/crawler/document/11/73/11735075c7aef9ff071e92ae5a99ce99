        <script type="text/javascript">
//Web service calls for weather data
    jQuery(document).ready(function() {


        var surl = "http://www4.thebeaconjournal.com/NOAAWebService/AkronWeatherService.asmx/GetCurrentWeather";
        jQuery.ajax({ url: surl, data: "{}", dataType: "jsonp",
            success: WidgetWeather,
            error: OnError
        });
        surl = "http://www4.thebeaconjournal.com/NOAAWebService/AkronWeatherService.asmx/GetDailyHiLo";
        jQuery.ajax({ url: surl, data: "{}", dataType: "jsonp",
            success: HiLo,
            error: OnError
        });
        surl = "http://www4.thebeaconjournal.com/NOAAWebService/AkronWeatherService.asmx/GetWeatherAlerts"
        jQuery.ajax({ url: surl, data: "{}", dataType: "jsonp",
            success: GetAlerts,
            error: OnError
        });
     });
        //These calls use the json datatype, POST, and do not work cross-domain
//        jQuery.ajax({ type: "POST", url: surl, data: "{}", dataType: "json",
//            contentType: "application/json; charset=utf-8",
//            success: WidgetWeather,
//            error: OnError
//        });

//        var surl = "http://localhost/NOAAWebService/AkronWeatherService.asmx/GetDailyHiLo";
//        jQuery.ajax({ type: "POST", url: surl, data: "{}", dataType: "json",
//            contentType: "application/json; charset=utf-8",
//            success: HiLo,
//            error: OnError
//        });
        function HiLo(data, status) {
            var wdata = data.d.split("|");
            //Hi Temp
            jQuery('#currentHigh').text("High: " + Math.round(wdata[0]) + "\u00B0");
            //Low Temp
            jQuery('#currentLow').text(" Low: " + Math.round(wdata[1]) + "\u00B0");
        }

       
        function WidgetWeather(data, status) {
            var adata = new Array;
            var wdata = "";
            var conditions = "";
            //alert(status);
            //alert(data.d);
            wdata = data.d;
            adata = wdata.split("|");

            //Current Temp F
            jQuery('#currentTemp').text(Math.round(adata[3]) + "\u00B0");
            //Current Icon and conditions
            
            //Current Temp F
            jQuery('#currentTemp-mobile').text(Math.round(adata[3]) + "\u00B0");
            //Current Icon and conditions
            
            //Current Temp F
            jQuery('#currentTemp-mobile').text(Math.round(adata[3]) + "\u00B0");
            //Current Icon and conditions

            conditions = "Current conditions: " + adata[2] + " Winds: From " + adata[6] + " @ " + Math.round(adata[8]) + " mph, Gusting: " + Math.round(adata[9]) + " mph, Humidity: " + adata[5] + "%" + ", Dew point: " + Math.round(adata[10]) + "\u00B0" + "F";
            jQuery('#conditionsIcon').append("<img src='" + "/img/akron/weather/fcicons/" + adata[15].replace("jpg", "png") + "' title='" + conditions + "'/>");
            //Wind chill
            //if (adata[12] == '') { adata[12] = Math.round(adata[3]) }; //No wind chill use current temp.
            //jQuery('div.current_windchill').text('Wind chill: ' + adata[12] + "\u00B0" + "F" + " Gusting: " + Math.round(adata[9]) + " mph");
            
            conditions = "Current conditions: " + adata[2] + " Winds: From " + adata[6] + " @ " + Math.round(adata[8]) + " mph, Gusting: " + Math.round(adata[9]) + " mph, Humidity: " + adata[5] + "%" + ", Dew point: " + Math.round(adata[10]) + "\u00B0" + "F";
            jQuery('#conditionsIcon-mobile').append("<img src='" + "/img/akron/weather/fcicons/" + adata[15].replace("jpg", "png") + "' title='" + conditions + "'/>");
            //Wind chill
            //if (adata[12] == '') { adata[12] = Math.round(adata[3]) }; //No wind chill use current temp.
            //jQuery('div.current_windchill').text('Wind chill: ' + adata[12] + "\u00B0" + "F" + " Gusting: " + Math.round(adata[9]) + " mph");


        }

        function GetAlerts(data, status) {
            //alert(data.d);
            var adata = data.d.split("||");
            var acount = adata.length - 1;
            var afields = new Array;
            var severitycolor = "";
           
            if (acount != 0) { $("#currentAlert").append("<a href='http://www4.thebeaconjournal.com/NOAAweatherdata/weather_sample5.html#page=page-3'><b>Weather Warning</b></a>") };
            
        }
        
        function OnError(request, status, error) {
            //alert("Request: " + request + " Status: " + status + " Error: " + error);
        }

    

</script>


