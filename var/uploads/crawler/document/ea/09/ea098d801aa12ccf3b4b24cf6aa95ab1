                        if (typeof(va['FIS_'+key]) == "undefined") {
                            va['FIS_'+key] = value;
                        }
                        va['LST_'+key] = value;
                    });
                    jQuery.each(va, function(key, val) {
                        if (key.indexOf('FIS_') == 0 || key.indexOf('LST_') == 0) {
                            var v = key + '=' + val;
                            cookieStr += cookieStr == '' ? v : '&' + v;
                        }
                    });
                    this.writeMktCookie(cookieStr);
