<script>
	function submitDealerForm()
	{
		var theForm = document.dealerLocatorHeaderForm;
		var theLocation = theForm.loc.value;
		var result;
		result = true;
		if(theLocation.length > 0 && theLocation != 'Zip Code')
		{
			if(theLocation.indexOf(",") > 0 || isNaN(theLocation))
			{
				theForm.CityState.value=1;
			}
			// handle city or state
			if(theLocation.indexOf(",") < 0 && isNaN(theLocation) && theLocation.length > 2)
			{
				theForm.action="DealerLocatorForm2?langId=-1&storeId=750001";
			}
			//theForm.submit();
		}
		else
		{
			alert("Please enter a valid (city, state) or zipcode in the location field.");
			result = false;
		}
		return result;
	}

</script>

	
		
	
	

 
	
	
	
	
	

