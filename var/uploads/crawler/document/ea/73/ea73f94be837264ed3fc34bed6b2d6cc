	<!-- scripts needed on page -->
	<script type="text/javascript">
		$(document)
				.ready(
						function() {

							jQuery
									.preLoadImages("/tts/card_solutions/prepaid_cards/resources/images/banners/ccc_banner_1.jpg");
							jQuery
									.preLoadImages("/tts/card_solutions/prepaid_cards/resources/images/banners/ccc_banner_2.jpg");
							//jQuery
							//		.preLoadImages("/tts/card_solutions/prepaid_cards/resources/images/banners/ccc_banner_3.jpg");

							ccc.page.onReady();
							setTimeout(ccc.page.rotate, ccc.page.delay);
							$('select.nav_select').change(
									ccc.nav.onSelectChange);

							if (ccc.QueryString().p == "contactus") {
								ccc.contactus.open();
							}
							
							

							var newest_assets = ccc.data.getNewestAssets();

							var news = newest_assets['news'];
							if (typeof news != 'undefined') {
								var asset = $('#left_float .asset.news span.link_title');
								asset.html(news.title);							

								
								$('#left_float .asset.news').show();
								$('#left_float .asset.news + div').show();
								
								asset.ellipsisText();
								
								$('.asset.news + div a.asset_link').attr('href', news.url);
							} 

							var case_study = newest_assets['case_study'];
							if (typeof case_study != 'undefined') {
								var asset = $('#left_float .asset.case_study span.link_title');
								asset.html(case_study.title);								

								if (typeof case_study.video != 'undefined' && case_study.video) {
									var with_video = $('#left_float .with_video');
									with_video.show();
									$('a', with_video).attr('href',
											'#' + case_study.video);
								}
								
								$('#left_float .asset.case_study').show();
								$('#left_float .asset.case_study + div').show();
								
								asset.ellipsisText();
								
								$('.asset.case_study + div a.asset_link').attr('href', case_study.url);
								
							} 

							var white_paper = newest_assets['white_paper'];
							if (typeof white_paper != 'undefined') {
								var asset = $('#left_float .asset.white_paper span.link_title');
								asset.html(white_paper.title);
								
								$('#left_float .asset.white_paper').show();
								$('#left_float .asset.white_paper + div').show();								
								
								asset.ellipsisText();
								
								$('.asset.white_paper + div a.asset_link').attr('href', white_paper.url);
							}
							
							/*
							$('select').selectbox();
							$('select.nav_select').selectbox();
							*/
							$('select.nav_select', '#left_float').selectbox();
							

						});
	</script>




