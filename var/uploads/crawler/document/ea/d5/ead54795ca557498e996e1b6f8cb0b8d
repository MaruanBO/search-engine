        function authType(status) {
            if (!status) {
                return "non-subscriber";
            }

            var type = cmg.query.cookie("janus.authorization") || "";

            // Remove quotes, anything including and after the first ":", and the string "backend"
            type = type
            .replace(/^\s*"|"\s*$|backend|:[\s\S]*/gi, "")
            .toLowerCase();

