<script>
jQuery(function($) {
    $('.loc-global-header-menu-link a.menu-link').click(function () {
        $('.loc-global-header-submenu, .loc-global-header-menu, .loc-global-header-menu-link').toggleClass('on-off');
    });
    var delay = 800, 
        global_nav_header_timeout;
    $('.loc-global-header-menu li').hover( function(event) {
        
        if( $(event.target).parents('.hover').length ){
            return;
        }
        
        $('.loc-global-header-menu li.hover').removeClass('hover');
        $(event.target).closest('li').addClass('hover');
        global_nav_header_timeout = setTimeout(function(){
            $('.loc-global-header-dropdown').hide();
            $('.loc-global-header-dropdown', $(event.target).closest('li')).show();
        }, delay);
    }, function(event){
        clearTimeout( global_nav_header_timeout );
        $(event.target).closest('li').removeClass('hover');
        $('.loc-global-header-dropdown', $(event.target).closest('li')).hide();
    });
    
    $('.loc-global-header-menu').hover( function(event) {/*
        do nothing on hover over, only on hover out
    */}, function(event){
        $('.loc-global-header-menu li.hover').removeClass('hover');
        $(event.target).parents('.loc-global-header-dropdown').hide();
    });
    
    //Metrics tracking how users are using the new header
    $('.loc-global-header-menu a, .intersite-nav a, .locfoot a').click(function(event){
        s.linkTrackVars = s.linkTrackVar + ",prop57";
        var label = $(this).attr('name');
        if ( !label ){
            label = $(this).text().trim();
        }
        s.prop57 = label;
        s.tl(this,'o',label);
    });
});
</script>
