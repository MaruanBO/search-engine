<script type="text/javascript">

    $(function () {
        //ea: #14594 - 20140704 - The form included by Ektron will be replaced with its content.
        var $content = $(".content > .sidebar");
        var $form1 = $("#aspnetForm");
        if (($form1.length > 0) && ($content.length > 0)) {
            var leftContent = $("#left-column .email-signup").html();
            $("#left-column .email-signup").html('');
            var $form = $("#aspnetForm");
            var cnt = $form.html();
            //ea: we need the current form attributes
            var formMethod = $form.attr("method");
            var formAction = $form.attr("action");
            var formClass = $form.attr("class");
            var formOnSubmit = $form.attr("onsubmit");
            //We can convert the current form to build the new form with an earlier closing form ta
            var formTag = '<form name="aspnetForm" id="aspnetForm" ';
            if (formMethod)
                formTag += ' method="' + formMethod + '" ';
            if (formAction)
                formTag += ' action="' + formAction + '" ';
            if (formClass)
                formTag += ' class="' + formClass + '" ';
            if (formOnSubmit)
                formTag += ' onsubmit="' + formOnSubmit + '" ';
            formTag += ' > ';
            cnt = cnt.replace('javascript"><!--', 'javascript"> ');
            cnt = cnt.replace('javascript><!--', 'javascript> ');
            cnt = cnt.replace('--></script', ' </script');
            var mainCnt = formTag.concat(cnt);
            $form.innerHtml = mainCnt;
            for (var x = 1; x < $content.length; x++) {
                $($content[x]).children().each(function () {
                    $(this).appendTo($($content[0]));
                });
                $($content[x]).remove();
            }
            var leftContent2 = $("#left-column .email-signup");
            var newHTMLForLeftContent = '</form>' + leftContent;
            leftContent2.html(newHTMLForLeftContent);
        }
    });

    function getInternetExplorerVersion() {
        var rv = -1;
        if (navigator.appName == 'Microsoft Internet Explorer') {
            var ua = navigator.userAgent;
            var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
            if (re.exec(ua) != null)
                rv = parseFloat(RegExp.$1);
        }
        else if (navigator.appName == 'Netscape') {
            var ua = navigator.userAgent;
            var re = new RegExp("Trident/.*rv:([0-9]{1,}[\.0-9]{0,})");
            if (re.exec(ua) != null)
                rv = parseFloat(RegExp.$1);
        }
        return rv;
    }
    </script>


