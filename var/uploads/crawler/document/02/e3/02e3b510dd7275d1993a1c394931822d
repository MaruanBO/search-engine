    <script>
        $(function () {
            // Adresses z-index bug in IE7 - so the autocomplete menu is back on top
            var zIndexNumber = 100;
            $('#navbar').find('div').each(function () {
                $(this).css('zIndex', zIndexNumber);
                zIndexNumber -= 1;
            });

            // Global navigation
            $('#primary .collapsed a').click(function () {
                var element = '#' + $(this).attr('id') + '-sub'; // id of the div to show
                nice_menu(element, this);
                return false;
            });

            
                    //15921 - 20141222 - The first top navigation collapsed menu should be always visible, when the others are not.
                    var firstElement = $('#primary .collapsed a:first');
                    var element = '#' + $(firstElement).attr('id') + '-sub'; // id of the div to show
                    nice_menu(element, firstElement);
            
            
        });

        /**
        * Menu animation function
        **/
        function nice_menu(element, link) {
            var $theDiv = $(element);
            var $visibleSiblings = $theDiv.siblings(':visible');
            if ($visibleSiblings.length) {
                // underline the clicked link
                $('#primary a').removeClass("active");
                $(link).addClass("active");
                // hide element
                // hide the siblings and wait till ready
                $visibleSiblings.animate({
                }, 200, 'swing', function () {
                    if ($visibleSiblings.height() == $theDiv.height()) {
                        // if no height change needed quickly change the height to avoid line jitter
                        // take siblings out
                        $visibleSiblings.animate({
                            "height": "hide"
                        }, 1, 'swing', function () {
                        });
                        // show element
                        $theDiv.animate({
                            "height": "show"
                        }, 1, 'swing', function () {
                        });
                    }
                    else {
                        // if height change IS needed transition the height
                        // take siblings out
                        $visibleSiblings.animate({
                            "height": "hide"
                        }, 400, 'swing', function () {
                        });
                        // show element
                        $theDiv.animate({
                            "height": "show"
                        }, 400, 'swing', function () {
                        });
                    }
                });
            }
            else {
                // toggle the element
                $theDiv.animate({
                    "height": "toggle"
                }, 400);
                // underline handling
                $(link).toggleClass("active");
            }
        } 
    </script>
    



