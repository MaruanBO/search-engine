<script type="text/javascript"> // maps intitalize
    var defaultLatLng = null;
    var defaultZoom = 9;
    var map = null;
    function initialize() {
        if (map == null) {
            var mapTypes = new Array();
            mapTypes.push("roadmap");
            mapTypes.push("satellite");
            if(defaultLatLng == null)
                defaultLatLng = new google.maps.LatLng(39.1, -96.8);
        
            var mapOptions = {
                zoom:  defaultZoom,
                center: defaultLatLng,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                mapTypeControl: true,
                mapTypeControlOptions: {
                    style: google.maps.MapTypeControlStyle.HORIZONTAL_BAR,
                    mapTypeIds: mapTypes
                }
            };
            map = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
            var d = new Date();
            var url = "http://www.texasroadhouse.com/interactive-map/storelocationskml.ashx?t=" + d.getTime();
            var kml = new google.maps.KmlLayer(url,{ preserveViewport: true });
            kml.setMap(map);
        
            var handleBounds = true;
            if(handleBounds == true){
                google.maps.event.addListener(map, 'idle', function() {
                    var bounds = map.getBounds();
                    var southWest = bounds.getSouthWest();
                    var northEast = bounds.getNorthEast();
                    if(map.getZoom() >= 9 && typeof(mapBoundsUpdated) != "undefined" && mapBoundsUpdated != null)
                    {
                        mapBoundsUpdated(northEast.lat(), northEast.lng(), southWest.lat(), southWest.lng());
                    } 
                });
            }
        } else {
            map.setZoom(9);
            map.setCenter(defaultLatLng);
        }
    }
</script>

