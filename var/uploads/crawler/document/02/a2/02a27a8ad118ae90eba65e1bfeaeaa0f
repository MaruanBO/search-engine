<script type="text/javascript">
    var locatorLoadCallback = null;
    $(document).ready(function () {
        $('.ibFindLocation').click(function (e) {
            e.preventDefault();
            var address = $('.tbAddress').val();
            if (address == '' || address == 'Zip or City & State')
                return;
            _gaq.push(['_trackEvent', 'Header', 'Find Your Roadhouse', address]);
            if (googleMapsScriptIsLoaded)
                codeAddress(address, successAddress, errorAddress);
            else {
                locatorLoadCallback = function () {
                    codeAddress(address, successAddress, errorAddress);
                };
                loadGoogleMapsScript("storeLocatorScriptLoaded");
            }
        });

        $('.tbAddress, .txtAddress').each(function () {
            var default_value = this.value;
            $(this).focus(function () {
                if (this.value == default_value) {
                    this.value = '';
                }
            });
            $(this).blur(function () {
                if (this.value == '') {
                    this.value = default_value;
                }
            });
        });
        //showCurrentLocation();
    });

    function storeLocatorScriptLoaded() {
        if (locatorLoadCallback != null) {
            locatorLoadCallback();
            locatorLoadCallback = null;
        }
    }

    // Store Locator Functionality
    var geocoder = null;
    function codeAddress(address, successCallback, errorCallback) {
        if (geocoder == null)
            geocoder = new google.maps.Geocoder();
        geocoder.geocode({ 'address': address }, function (results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                if (successCallback != null && typeof (successCallback) == "function") {
                    successCallback(results[0].geometry.location);
                } else {
                    alert(results[0].geometry.location);
                }
            } else {
                if (errorCallback != null && typeof (errorCallback) == "function") {
                    errorCallback(status);
                } else {
                    alert("Geocode was not successful for the following reason: " + status);
                }
            }
        });
    }

    function successAddress(address) {
        window.location.href = "/locations/find-us?lat=" + address.lat() + "&lng=" + address.lng();
    }

    function errorAddress(error) {
        alert('search failed: ' + error);
    }

    function showCurrentLocation() {
        $.get('/sitefinity/services/LeapFrog/TRH_Proxy.asmx/GetCurrentLocation', function (data) {
            var currentLocation = $(data).find("string").text();
            if (currentLocation.indexOf(',') >= 0) {
                var storeName = currentLocation.substring(0, currentLocation.indexOf(','));
                var storeUrlName = currentLocation.substring(currentLocation.indexOf(',') + 1);
                $('.find-location .current-location-link').attr('href', '/restaurant-locations/' + storeUrlName).text(storeName);
                $('.find-location .enter-zip').hide();
                $('.find-location .current-location').show();
            }
        }, "xml");
    }
</script>

