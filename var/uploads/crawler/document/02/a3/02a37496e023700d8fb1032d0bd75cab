     function trackNavAnchorClicks (thisVar,linkPathFromHref)
            {
                var linkPath = linkPathFromHref;
                var linkPathIndex = linkPath.indexOf("/content/corporate");
                if(linkPathIndex > -1){
                    linkPath = linkPath.substring(18,linkPath.length);
                }
                //thisVar.href= linkPath;      
                /* Check if we are on home page, if so do the scroll slowly, else do the regular redirect */
				/* Also fix issue with incorrect # nav tag being shown in the url. This fix history.pushState() does not work in older browsers */				
        var pageName = getPagePathForTracking();
                var hashValues = linkPath.split("#");
                if(pageName == hashValues[0]){
                var hashVal = hashValues[1];
                 if(history.pushState){
               		 window.history.pushState( {} , '', hashValues[0] );
                    }
                $('html,body').animate({scrollTop: $("#"+hashVal).offset().top},'slow');                   
                }else{
                    thisVar.href= linkPath; 
                }		
                /* Just moving the tracking to DTM side*/
                /*  if(!(typeof CQ_Analytics === "undefined")){
					 var linkPathColon = linkPath.replace("#",":");
							CQ_Analytics.record({event: [], 
							values: { navTagLink: linkPathColon },           
							collect: false,            
							options: {},                             
							componentPath: 'nielsenglobal/components/content/topnav'}); 
                            }
                            */
            }
     function getPagePathForTracking(){

				    var pagePath = window.location.href;
                    var pageProtocol = window.location.protocol;
                    if(pageProtocol.length > 0){
                        pagePath = pagePath.substring(pageProtocol.length+3,pagePath.length);//Remove https://
                    }

                    pagePath = pagePath.substring(pagePath.indexOf("/"),pagePath.length);//Remove servername
                    var trimContentCorporate = pagePath.indexOf("content/corporate");
                    if(trimContentCorporate > -1){
                        pagePath = pagePath.substring(trimContentCorporate+17,pagePath.length);//Remove /content/corporate
                    }
                	var anchorLinks = pagePath.indexOf("#"); 
                    if(anchorLinks > -1){
                        pagePath = pagePath.substring(0,anchorLinks);//Remove #xxx from url as this is getting sent
                     }

					return pagePath;
                    }
</script>
        
                        
                         
                        

					
