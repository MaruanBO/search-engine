	    	<script>

	var mainPage = GetCookie("mainPage");
	var favorite = GetCookie("defaultPage");
	var splashSelectedCookie = GetCookie("splashSelected");
	var paramString =location.search;
	var params = {};
	var noSplash = false;
	var worldWideSelected = false;
	
	if(paramString!=""){
        var vars = paramString.substr(1).split("&");
        for (var i =0; i<vars.length; i++) {
            var pair = vars[i].split('=');
            if(pair[0] == "testX"){
                params = "testX="+pair[1];
            }else if (pair[0] == "testCC"){
                params = "testCC="+pair[1];
            }else if (pair[0] == "noSplash") {
                noSplash=true;
            }
        }
		worldWideSelected = true;
	}
	
	if (splashSelectedCookie!=null && splashSelectedCookie=="true") {
		noSplash=true;
	}
	
	if(mainPage!=null && !worldWideSelected){
		DeleteCookie("mainPage");	
	}
	else{	
		if((favorite != null) && (!worldWideSelected)){						 
			window.location.href = favorite;
			window.showSplash = false;
		} else if (noSplash == true) {
			window.showSplash = false;
			if (splashSelectedCookie==null) {
				document.cookie = "splashSelected=true; path=/";
			}
		} else {
        	var url = "/do/getregion";
        	if(params != null){
        		url = url + "?" + params;
        	}
        	$.ajax({
        		  url: url,
        		  dataType: 'json',
        		  success: function(data){
        			var code = data["countryCode"];
        			if (code != 'US' && code != '--' && !false && !false) {
        				window.showSplash = true;
        			} 
        		  },
        		async:   false
        		});
        }
	}

/*******spals overlay code*******/
$(document).ready(function(){

$('.prevented, .video-containerrr').click(function(e) {
    $('.video-containerrr').hide();
    e.preventDefault();
    e.stopPropagation();
});
$(document).click(function() {
    $('.video-containerrr').show();
});
    
});
</script>

