<script type="text/javascript">      
 var privacyCookieName = "privacycookie"+"sg"+"en";
 $('.closebtn').click(function(e)
 {
     e.preventDefault();
     var cookieValue = document.cookie.indexOf(privacyCookieName);
     if(cookieValue <0) 
     {
         var privacyCookieDate = new Date();
	     var privacyCookieMins = 0; //expire in minutes
	         	if (privacyCookieMins > 0) {
	             	privacyCookieDate.setTime(privacyCookieDate.getTime() + (privacyCookieMins * 60 * 1000));
	             	$.cookie(privacyCookieName, "yes", { expires: privacyCookieDate, path : '/' });
                }
         $(".cookiepopup").hide(); 

 		}     

 }); 

    var hidePrivacyCookie = "false";
    if(hidePrivacyCookie=="false"){
        if ((document.cookie.indexOf(privacyCookieName)) < 0) {
            var cookieInfo = "0";
            if(cookieInfo>0){
                $(".cookiepopup").show(); 
            }
        }   
        
    }




</script>  

