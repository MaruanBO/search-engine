Ext.onReady(function(){
    new ExtGallery.UploadPanel({
        renderTo: 'uploadformdiv',
        width: 380,
        padding: 5,
        listeners: {
            uploadcomplete: function(upload_id) {
                if (_gaq && _gaq.push) _gaq.push(['_trackEvent', 'JPEGbay', 'new_gallery']);
                location.href = '/manager/?uid=' + upload_id;
            }
        }
    });
});
</script>





