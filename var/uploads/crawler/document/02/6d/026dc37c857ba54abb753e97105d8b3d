function getURLParameter(name) {
    return decodeURI(
        (RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1]
    );
}
$(document).ready(function () {
	if(getURLParameter("do") == "subscribe")	{
		setTimeout(function(){$(".subscribeTab").click();},100);   
	}	 
});
