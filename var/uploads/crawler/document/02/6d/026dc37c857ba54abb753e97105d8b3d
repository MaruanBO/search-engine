<script type="text/javascript">
	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}

	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}
    $(document).ready(function () {
        $('#subscribeBox').appendTo('body');
        $('#subscribeBox .dropdown').customSelect();
        $(".subscribeTab").r2iOverlay({
			mask:{color: '#000',loadSpeed:200,opacity:0.5}, 
            top: "22%",
            onLoad: function () {
                $('.subscribeTab').animate({ width: 'toggle' });
                //return false;
            },
            onClose: function () {
                $('.subscribeTab').animate({ width: 'toggle' });
                $('#dnn_r2iSubscribe_txtAddress1').val("Address 1");
                $('#dnn_r2iSubscribe_txtAddress2').val("Address 2");
                $('#dnn_r2iSubscribe_txtCity').val("City");
                $('#dnn_r2iSubscribe_txtEmail').val("E-mail *");
                $('#dnn_r2iSubscribe_txtFirstName').val("First Name *");
                $('#dnn_r2iSubscribe_txtLastName').val("Last Name *");
                $('#dnn_r2iSubscribe_txtZip').val("Zip/Postal Code *");
                //return false;
            }
        });

        $('#dnn_r2iSubscribe_txtFirstName').focus(function () { if ($(this).val() == "First Name *") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtFirstName').blur(function () { if ($(this).val() == "") $(this).val("First Name *"); });
        $('#dnn_r2iSubscribe_txtLastName').focus(function () { if ($(this).val() == "Last Name *") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtLastName').blur(function () { if ($(this).val() == "") $(this).val("Last Name *"); });
        $('#dnn_r2iSubscribe_txtEmail').focus(function () { if ($(this).val() == "E-mail *") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtEmail').blur(function () { if ($(this).val() == "") $(this).val("E-mail *"); });
        $('#dnn_r2iSubscribe_txtAddress1').focus(function () { if ($(this).val() == "Address 1") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtAddress1').blur(function () { if ($(this).val() == "") $(this).val("Address 1"); });
        $('#dnn_r2iSubscribe_txtAddress2').focus(function () { if ($(this).val() == "Address 2") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtAddress2').blur(function () { if ($(this).val() == "") $(this).val("Address 2"); });
        $('#dnn_r2iSubscribe_txtCity').focus(function () { if ($(this).val() == "City") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtCity').blur(function () { if ($(this).val() == "") $(this).val("City"); });

        $('#dnn_r2iSubscribe_txtZip').focus(function () { if ($(this).val() == "Zip/Postal Code *") $(this).val(""); });
        $('#dnn_r2iSubscribe_txtZip').blur(function () { if ($(this).val() == "") $(this).val("Zip/Postal Code *"); });

        $('#dnn_r2iSubscribe_ddlCountry').change(function () {
            loadStates();
            return false;
        });

        $('#dnn_r2iSubscribe_lbSubmit').click(function () {
			_gaq.push(["_trackEvent", "Content Consumption", "Submit Form", "Light Box Form"]);
            if (validate()) {
                var proxy = new window.ServiceProxy("/DesktopModules/R2ISubscribeNow/service.asmx/");

                var address1 = $('#dnn_r2iSubscribe_txtAddress1').val();
                var address2 = $('#dnn_r2iSubscribe_txtAddress2').val();
                var city = $('#dnn_r2iSubscribe_txtCity').val();
                var country = $('#dnn_r2iSubscribe_ddlCountry :selected').text();
                var email = $('#dnn_r2iSubscribe_txtEmail').val();
                var firstName = $('#dnn_r2iSubscribe_txtFirstName').val();
                var lastName = $('#dnn_r2iSubscribe_txtLastName').val();
                var state = $('#dnn_r2iSubscribe_ddlState :selected').val();
                var zip = $('#dnn_r2iSubscribe_txtZip').val();
                
                //var howDidYourHear = $('#dnn_r2iSubscribe_ddlHowDidYouHear :selected').val();
				var howDidYourHear = "";

                var user = { Address1: "", Address2: "", City: "", Country: "", Email: "", FirstName: "", LastName: "", State: "", Zip: "" };
                user.Address1 = (address1 == "Address 1") ? "" : address1;
                user.Address2 = (address2 == "Address 2") ? "" : address2;
                user.Country = country;
                user.City = (city == "City") ? "" : city;
                user.Email = (email == "E-mail *") ? "" : email;
                user.FirstName = (firstName == "First Name *") ? "" : firstName;
                user.LastName = (lastName == "Last Name *") ? "" : lastName;
                user.State = state;
                user.Zip = (zip == "Zip/Postal Code *") ? "" : zip;

                var interest = new Array();
                interest.push("NEWSLTR");

                proxy.invoke("SaveInfo", { interest: user, type: interest, hear: howDidYourHear }, function () {
                    $('#subscribeBox h3').html('<h2>Confirmation</h2>');
                    $('#subscribeBox .checkboxes').remove();
                    $('#subscribeBox .fields').remove();
                    $('#subscribeBox .confirm').remove();
                    $('#subscribeBox .submit').remove();
                    $('#subscribeBox').append('<p>Thank you for partnering with In Touch Ministries. Our mission is to lead people worldwide into a growing relationship with Jesus Christ and to strengthen the local church. If you need to contact us for any reason, visit our online Customer Care page or call 1-800-789-1473. You may remove your name from this list at any time by clicking the “Unsubscribe” link at the bottom of each e-mail.</p>');
                    $('#subscribeBox').css('min-height', '210px');
                });
            }
            return false;
        });
		if(readCookie("subscribeShown") != "1")	{
			createCookie("subscribeShown","1",60);
			setTimeout(function(){$(".subscribeTab").click();},100);
				
		}
		$('body').prepend('<a href="http://www.intouch.org/betatest" target="_blank" style="text-decoration:none;"><div id="betatest" style="padding:20px;text-align:center;font-weight:normal;color:#fff;background-color:#454546;font-family:Georgia;font-size:22px;min-width: 1009px;">NEW InTouch.org coming soon! Get a first look here.</div></a>');
    });

    function validate() {

        var result = true;
        if (!$('#dnn_r2iSubscribe_chkConfirm').attr('checked')) {
            alert('Please confirm your subscription by checking the confirmation box.');
            return false;
        }

        if ($('#dnn_r2iSubscribe_txtEmail').val() == "E-mail *") {
            alert('Please enter a valid email address.');
            return false;
        }
        if ($('#dnn_r2iSubscribe_txtFirstName').val() == "First Name *") {
            alert('Please enter your First Name.');
            return false;
        }
        if ($('#dnn_r2iSubscribe_txtLastName').val() == "Last Name *") {
            alert('Please enter your Last Name');
            return false;
        }

        if ($('#dnn_r2iSubscribe_txtZip').val() == "Zip/Postal Code *") {
            alert('Please enter your Zip or Postal Code');
            return false;
        }        

        return result;
    }

    function loadStates() {
        var proxy = new window.ServiceProxy("/DesktopModules/R2ISubscribeNow/service.asmx/");
        var country = $("#dnn_r2iSubscribe_ddlCountry").val();

        proxy.invoke("GetStates", { countryId: country }, function (results) {
            $('#dnn_r2iSubscribe_ddlState').children('option').remove();
            for (var i = 0; i < results.length; i++) {
                $("#dnn_r2iSubscribe_ddlState").append("<option value='" + results[i].Abbreviation + "'>" + results[i].Name + "</option>");
            }
            $('#dnn_r2iSubscribe_ddlState').next('.customStyleSelectBox').remove();
            $('#dnn_r2iSubscribe_ddlState').customSelect();
            
        });
    }

</script>
