			<script type="text/javascript">
				$(document).ready(function() {

					$("#hp-offer-switch #tabs li").click(function() {
						$("#hp-offer-switch #tabs li").removeClass("active");
						$(this).addClass("active");
						var getid = "#" + $(this).attr("loadid");
						$("#hp-offer-switch #offer-body div.offer-tab").removeClass("active");
						$(getid).addClass("active");
					});

					if($("#hp-offer-switch #tabs li:first").css('display') == 'none')
						$("#hp-offer-switch #tabs li:first").next().click();
					else
						$("#hp-offer-switch #tabs li:first").click();

					var today = new Date();
					var dd = today.getDate();
					var mm = today.getMonth() + 1;
					var yy = today.getFullYear();
					if (mm == 1 && yy == 2013 && dd > 11 && dd < 20) {
						// this change is only effective for this week 1/12/2013 - 1/19/2013
						// activate the tab selector
						$("#hp-offer-switch #tabs li").removeClass("active");
						$('#hp-offer-switch #tabs li:last').addClass('active');

						// activate the forum tab
						$('#hp-offer-01').removeClass('active');
						$('#hp-offer-06').addClass('active');
					}

				});
			</script>
			
