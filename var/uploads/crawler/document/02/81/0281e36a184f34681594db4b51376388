<script type="text/javascript">
	var timer;
	$(function() {
		/*
		var timer;
		$("#nav-main li.show-more").hover(function() {
		var dropdown = $(this);
		if (timer) {
		clearTimeout(timer);
		timer = null;
		}
		timer = setTimeout(function() {
		dropdown.addClass("open");
		}, 300)
		}, function() {
		$("ul#nav-main li").removeClass("open");
		clearTimeout(timer);
		});
		*/
		$('#nav-main li.show-more').click(function() {
			var dropdown = $(this);
			if (dropdown.hasClass('open')) {
				dropdown.removeClass("open");
				clearTimeout(timer);
			}
			else {
				$("ul#nav-main li").removeClass("open");
				dropdown.addClass("open");
				timer = setTimeout(function() {
					dropdown.removeClass("open");
				}, 15*1000);
			}
		});


		$("a.omniture-link").click(function() {

			var reg = /;jsessionid=[a-zA-Z0-9\:\-\+\_]+/;
			var linkHref = (obj.attr("href")).replace(reg, "");
			var linkText = obj.text();
			var linkName = obj.attr("oname");

			var oaId = OmnitureAccountId();
			var s = s_gi(oaId);
			s.linkTrackVars = 'prop6,eVar11,events';
			s.linkTrackEvents = 'event2';
			s.events = linkName;

			var linkType = 'o'; // o->Custom Links ; e->Exit Links
			s.tl(this, linkType, linkName, linkHref + "|" + linkName + "|" + linkText);
		});

	});
</script>


                            
                    
                    
