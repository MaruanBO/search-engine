				function FAD_validate_form(t) {
					if (typeof t.limit_lastname != "undefined") {
						if (t.limit_lastname.value == '') {
							alert("Last Name is required.\n");
							return false;
						}
					}
					
					if (typeof t.limit_specialty != "undefined") {
						if (t.limit_specialty.selectedIndex == 0 && t.limit_withinZIP.value == '' && t.limit_within.selectedIndex == 0) {
							alert("Please specify search criteria.\n");
							return false;
						}
						if (t.limit_specialty.selectedIndex == 0) {
							alert("Specialty is required.\n");
							return false;
						}
						
					}
										
					if (typeof t.limit_gender != "undefined") {
						if (t.limit_gender[0].checked && t.limit_withinZIP.value == '' && t.limit_within.selectedIndex == 0) {
							alert("Please specify search criteria.\n");
							return false;
						}
					}
					
					if (typeof t.limit_withinZIP != "undefined") {
						if (t.limit_withinZIP.value == '' && t.limit_within.selectedIndex != 0) {
							alert("Please enter a ZIP code.\n");
							return false;
						} else if (t.limit_withinZIP.value != '') {
							var v = t.limit_withinZIP.value;
							if (v.length !=5 && v.length !=10) {
								alert("Please enter a valid ZIP Code.\n");
								return false;
							} else {
								var valid = "0123456789-";
								var hyphencount = 0;
								for (var i=0; i < v.length; i++) {
									temp = "" + v.substring(i, i+1);
									if (temp == "-") hyphencount++;
									if (valid.indexOf(temp) == "-1") {
										alert("Invalid characters in ZIP Code. Please try again.\n");
										return false;
									}
									if ((hyphencount > 1) || ((v.length==10) && ""+v.charAt(5)!="-") || (hyphencount == 1 && v.length < 10)) {
										alert("The hyphen character should be used with a properly formatted 5 digit+four zip code, like '12345-6789'. Please try again.\n");
										return false;
								   	}
								} /* for */
							}
						}
					}
					return true;
				}		
			</script>
		</head>
