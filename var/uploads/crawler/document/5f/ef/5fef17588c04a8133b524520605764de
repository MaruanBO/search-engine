							<script language="JavaScript" type="text/javascript"><!--
	function clearTextTopNavLoc(t) {
		if (t.value=='Enter Your ZIP Code') {
 	 		t.value= "";
		}
	}
	
	function validateTopNavLocZip(name){
		var field = document.getElementById(name);
		var v = field.value;
		var valid = "0123456789-";
		var hyphencount = 0;
		if (v == 'Enter Your ZIP Code' || v == '') {
			alert("Please enter a ZIP Code.");
			return false;
		}
		if (v.length!=5 && v.length!=10) {
			alert("Please enter a valid ZIP Code.");
			return false;
		}
		for (var i=0; i < v.length; i++) {
			temp = "" + v.substring(i, i+1);
			if (temp == "-") hyphencount++;
			if (valid.indexOf(temp) == "-1") {
				alert("Invalid characters in ZIP Code. Please try again.");
				return false;
			}
			if ((hyphencount > 1) || ((v.length==10) && ""+v.charAt(5)!="-") || (hyphencount == 1 && v.length < 10)) {
				alert("The hyphen character should be used with a properly formatted 5 digit+four zip code, like '12345-6789'. Please try again.");
				return false;
		   	}
		}
		return true;
	}	
//-->
</script>
