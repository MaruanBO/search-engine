	<script type="text/javascript">
		function doSubmit (f)
		{
			var searchTerm = $("#keywords").val();
			var location = "body.cfm?id=969&action=list&keywords=" + escape(searchTerm);
			if (searchTerm.toLowerCase() == "search")
				$("#keywords").val("");
			if (checkform(f, "keywords", true))
				window.location.href = location;
			return false;
		}	// doSubmit	
