										<script type="text/javascript">
												var sliderContentObject	= $('#sliderContent');
												var sliderContentCount	=	sliderContentObject.find('ul#sliderContent_i li').length; 
												var sliderContentDelay	=	7000;
												var sliderContentIndex	=	1; sliderContentGoTo(sliderContentIndex,false);
												var sliderContentSlide	=	window.setInterval('sliderContentGoToNext()',sliderContentDelay);
												sliderContentObject.find('ul#sliderContent_i li').each(function(index){
													$(this).bind('click',function(){
														sliderContentGoTo(index+1,true);
													});
												});
												sliderContentObject.find('ul#sliderContent_i li:first').addClass('first-child');
												function sliderContentGoToNext(){sliderContentIndex++;if(sliderContentIndex > sliderContentCount){sliderContentIndex = 1;}sliderContentGoTo(sliderContentIndex);}
												function sliderContentGoTo(index,isClick){
													var lastIndex = index-1;
													if(lastIndex == 0){lastIndex = sliderContentCount;}
													if(isClick){
														window.clearTimeout(sliderContentSlide);
														if(sliderContentObject.find('#sliderContent_'+index+':hidden').length){
															sliderContentObject.find('ul#sliderContent_i li').each(function(){$(this).removeClass('active');});
															sliderContentObject.find('#sliderContent_content div.sliderContent_content').each(function(){$(this).hide();$(this).find('div.sliderContent_copy').hide();});
															sliderContentObject.find('li#sliderContent_i'+index).addClass('active');
															sliderContentObject.find('#sliderContent_'+index).fadeIn(500,function(){
																sliderContentObject.find('#sliderContent_'+index+' div.sliderContent_copy').delay(0).fadeIn(250);
															});
														}
													}else{
														sliderContentObject.find('ul#sliderContent_i li').each(function(){$(this).removeClass('active');});
														sliderContentObject.find('li#sliderContent_i'+index).addClass('active');
														sliderContentObject.find('#sliderContent_'+lastIndex).fadeOut(500,function(){
															sliderContentObject.find('#sliderContent_'+index).fadeIn(500,function(){
																sliderContentObject.find('#sliderContent_'+index+' div.sliderContent_copy').delay(250).fadeIn(250).delay(4750).fadeOut(500);
															});
														});
													}
												}
										</script>
										
