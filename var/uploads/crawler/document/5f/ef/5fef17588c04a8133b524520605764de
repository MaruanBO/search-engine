		if(!sm_ValidateForm(t,false)){
			return false;
		}else{
				if($('#overlay')){
					$('#overlay').css('height',$(document).height()).show();
					if($.browser.msie){
						$("body").append('<div id="overlayContent"><img src="images/template/prototypeLogin.gif" alt=""><p>Loading, Please Wait..</p></div>');
						if($.browser.version.substr(0,1)<7){
							$('select').css('visibility','hidden');
