<script type="text/javascript">
	jQuery(document).ready(function($){
		$('.navigationTopLeft a').each(function(){
			var strText = $(this).text();
			var arrText = strText.split(' ');
			if(arrText.length == 2){
				$(this).html(arrText[0]+'<br/>'+arrText[1]);
			}else if(arrText.length == 3){
				$(this).html(arrText[0]+' '+arrText[1]+'<br/>'+arrText[2]);
			}else if(arrText.length == 4){
				if(arrText[0].toLowerCase() == "for"){
					$(this).html(arrText[0]+' '+arrText[1]+' '+arrText[2]+'<br/>'+arrText[3]);
				}else{
					$(this).html(arrText[0]+' '+arrText[1]+'<br/>'+arrText[2]+arrText[3]);
				}
			}
		});
	});
</script>



