        function processDocumentReadyFunctions() {
            for (var i = 0; i < OnDocumentReadyFunctions.length; i++) {
                if (typeof OnDocumentReadyFunctions[i] === "function") {
                    OnDocumentReadyFunctions[i]();
                }
            }
            while (OnDocumentReadyFunctions.length > 0) {
                OnDocumentReadyFunctions.pop();
            }
        }
