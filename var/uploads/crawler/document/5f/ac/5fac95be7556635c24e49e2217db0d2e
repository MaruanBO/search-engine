       $(document).ready(function () {
            $('#SearchText').on('click', function () {
               $(".helper").fadeIn(function () {
                   $('#SearchText').off('click');
               }).delay(3000).fadeOut();
            });

            if ($.browser.msie && $.browser.version == 7) {
                $('#legacyBrowser').show();
                $('#legacyBrowser').click(function () {
                    $('#legacyBrowser').hide();
                });
            }

            var isiPad = navigator.userAgent.match(/iPad/i) != null;
            var hideMobileBar = $.cookie('hideMobileBar')  != null || $.cookie('hideMobileBar') != '' ? Boolean($.cookie('hideMobileBar')) : false;
            if (isiPad && !hideMobileBar) {
                $('.mobile-bar').show().animate({ top: '0px' }, 'medium', function () {
                    setTimeout(function () {
                        $('.mobile-bar').animate({ top: '-80px', easing: 'easeOutElastic' }, 'medium');
                    }, 10000);
                    $(window).scroll(function () { $('.mobile-bar').animate({ top: '-80px', easing: 'easeOutElastic' }, 'fast'); });
                });
                $.cookie('hideMobileBar', true, { path: '/' });
                $('#noMobile').click(function () { $('.mobile-bar').animate({ top: '-80px', easing: 'easeOutElastic' }, 'medium'); });
            }

            
                m_ViewType = 'list';
                $.cookie('viewType', 'list', { path: '/' });
            
            //END: Ready
       });


    </script>

    
    <script type="text/javascript">_satellite.pageBottom();</script>
    
</body>
</html>";i:2;s:4:"8556";}