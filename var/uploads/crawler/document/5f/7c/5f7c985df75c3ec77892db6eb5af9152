<!-- front page box effect -->
<script type="text/javascript">
$('.box').mouseenter(function(){
	$(this).find('.infopopup').animate({top: "70px"}, 250);
	$(this).find('.infodropdown').animate({top: "0px"}, 250);
});
$('.box').mouseleave(function(){
	$(this).find('.infopopup').animate({top: "250px"}, 250);
	$(this).find('.infodropdown').animate({top: "-70px"}, 250);
});
</script>

<!-- Google Plus -->
<script type="text/javascript">
<!-- initialize -->
(function() {
var po = document.createElement('script');
po.type = 'text/javascript'; po.async = true;
po.src = 'https://apis.google.com/js/client:plusone.js?onload=render';
var s = document.getElementsByTagName('script')[0];
s.parentNode.insertBefore(po, s);
})();
<!-- set up helper for onSignInCallback -->
var helper = (function() {
	var BASE_API_PATH = 'plus/v1/';
	return {
		onSignInCallback: function(authResult) {
			gapi.client.load('plus','v1', function(){
			$('#authResult').html('Auth Result:<br/>');
			for (var field in authResult) {
				$('#authResult').append(' ' + field + ': ' +authResult[field] + '<br/>');
			}
			if (authResult['access_token']) {
				helper.profile();
			} else if (authResult['error']) {
				console.log('There was an error: ' + authResult['error']);
			}
		});
	},
	profile: function(){
		var request = gapi.client.plus.people.get( {'userId' : 'me'} );
			request.execute( function(profile) {
				email = profile['emails'].filter(function(v) {return v.type === 'account';})[0].value;
				$('#gplusform-email').attr('value',email);
				$('#gplusform-id').attr('value',profile.id);
				$('#gplusform').submit();
			});
		}
	};
})();
<!-- signin callback function, should return a users profile -->
function onSignInCallback(authResult) {
	helper.onSignInCallback(authResult);
}
<!-- when document is ready, render initial button and set up onclick for unauthorizing it -->
$(document).ready(function() {
	render();
	$('#disconnect').click(function(){unauthorize_gplus()});
});
<!-- when they click unauthorize -->
function unauthorize_gplus() {
	console.log('unauthorize_gplus() called');
	$.ajax({
		type: 'GET',
		url: 'https://accounts.google.com/o/oauth2/revoke?token='+gapi.auth.getToken().access_token,
		async: false,
		contentType: 'application/json',
		dataType: 'jsonp',
		success: function(result) {
			console.log('revoke response: ' + result);
		},
		error: function(e) {
			console.log(e);
		}
	});
	window.location = 'http://www.glamourpage.com'+'/logout';
}
<!-- click login button -->
$('#customBtn').click(function(){
	gapi.auth.signIn({
		'clientid' : '473562969445.apps.googleusercontent.com',
		'cookiepolicy' : 'single_host_origin',
		'callback' : 'onSignInCallback'
	});
});
<!-- render() loads the google plus button -->
function render() {
	gapi.signin.render('customBtn', {
		'clientid': '473562969445.apps.googleusercontent.com',
		'cookiepolicy': 'single_host_origin',
		'requestvisibleactions': 'http://schema.org/AddAction',
		'scope': 'https://www.googleapis.com/auth/plus.login'
	});
}
</script>

<!-- Taboola End Code -->
<script type="text/javascript">
window._taboola = window._taboola || [];
