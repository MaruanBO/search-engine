<script type="text/javascript">
  function ShowLightbox(whatPage, divWidth, divHeight){
    var documentHeight = jQuery(document).height();
    var documentWidth = jQuery(document).width();
    var windowHeight = jQuery(window).height();
    var windowWidth = jQuery(window).width();
    if (divWidth == null && divHeight == null || windowWidth < 580 || windowHeight < 330) {
      var preloaderY = 10;
      var preloaderX = (windowWidth/2) - halfdivWidth;
    } else {
      var halfdivWidth = divWidth/2;
      var halfdivHeight = divHeight/2;
      var preloaderX = (windowWidth/2) - halfdivWidth;
      var preloaderY = (windowHeight/2) - halfdivHeight;
      jQuery('#lightbox-content').css({ width: divWidth, height: divHeight});
    }

    jQuery("#lightbox").fadeIn(0).animate({ width: documentWidth, height: documentHeight, top: 0, left: 0}, 1, 'linear', function(){
      jQuery('#lightbox-content').css({ top: preloaderY, left: preloaderX }).fadeIn("fast").load(whatPage);
    });
  }

  function closeLightbox(){
    //return false;
    jQuery("#lightbox, #lightbox-content").fadeOut(300).empty();
  }

  jQuery(document).ready(function(){
    jQuery("#homepageYouTubeTrigger").on("click", function() {
      ShowLightbox('homepageYouTube.html','560','317');
    });

    (function(resizelb){
      jQuery.fn.fullSize = function(){
        var blackArea = jQuery(this);
        var divWidth = 560;
        var divHeight = 315;

        function resizeBlackArea() {
          var winwidth = jQuery(window).width();
          var winheight = jQuery(window).height();

          blackArea.css({
            width: winwidth+'px',
            height: winheight+'px'
          });

          if(winwidth < 580 || winheight < 350) {
            var preloaderY = 10;
            var preloaderX = 10;
          } else {
            var halfdivWidth = divWidth/2;
            var halfdivHeight = divHeight/2;
            var preloaderX = (winwidth/2) - halfdivWidth;
            var preloaderY = (winheight/2) - halfdivHeight;
          }
          jQuery('#lightbox-content').css({ top: preloaderY, left: preloaderX });
        }
        resizeBlackArea();
        jQuery(window).resize(function() {resizeBlackArea();});
      };
    })(jQuery)

    jQuery(function(resizelb) {jQuery("#lightbox").fullSize();});

    jQuery('#lightbox-content').click(function(event){ event.stopPropagation(); }); // prevent lightbox from closing when it's clicked w/in the lightbox
    jQuery("#lightbox").click(function (evt) { closeLightbox(); }); // close lightbox when user clicks outside the lightbox
  });
  </script>

