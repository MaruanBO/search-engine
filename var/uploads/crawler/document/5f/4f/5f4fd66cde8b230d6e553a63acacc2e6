  <script type="text/javascript">

    function newsLeteCallback(data){

        var flagElem = $('div#weekly-newsletter');
        var container = flagElem.closest('div.fourth');
        var caption = container.find('h4');

        flagElem.children().remove();
        var text = 'Thank You For Subscribing! You Will Now Receive Our Newsletter.';
        flagElem.text(text);
    }

    $( document ).ready(function() {

      jQuery('#weekly-newsletter-form').submit(function(evt) {
          evt.preventDefault();

        if ( jQuery("#weekly-newsletter-form").validationEngine('validate') ) {


                jQuery.ajax({
                  url: 'weekly-newsletter.php',
                  type: 'GET',
                  data: 'action=subscribe&name='+jQuery('#name2').val()+'&email='+jQuery('#email2').val(),
                  //async: false,
                  success: function (data) {
                      // this is executed when ajax call finished well
                      jQuery('#weekly-newsletter').html('Thank You For Subscribing! You Will Now Receive Our Newsletter.');
                      return false;
                  },
                  error: function (xhr, status, error) {
                      // executed if something went wrong during call
                      jQuery('#weekly-newsletter').html(error);
                      return false;
                  }

              });

              return false;

          } else {
              return false;
          }

          return false;

      });
  });
  </script>



