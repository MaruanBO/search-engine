setFlagByCountry();
function setFlagByCountry()
{

$.get("http://ipinfo.io", function (response) {

var country = getCountryName (response.country);
	var countryMessageImg = document.getElementById('country-message-flag');
	if( country != 'Israel')
	{
		document.getElementById("country-message").style.display = "block";
		countryMessageImg.src = 'flags/' + country.replace(" ","-") + '.png';
	}
	else
	{
		document.getElementById("local-message").style.display = "block";
	}

}, "jsonp");

}
</script>

