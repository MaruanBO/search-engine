  <script language="JavaScript" type="text/javascript">
  // we call the changeZIndex function in a setInterval every 1 second
  var int = self.setInterval(function(){ changeZIndex(); }, 1000);

  function changeZIndex() {

    $('div#adDiv0 div:nth-child(1)').css('z-index', '40');

    // we stop the setInterval when the pushdown Ad div's z-index is changed
    if($('div#adDiv0 div:nth-child(1)').css('z-index') == '1') {
    int = window.clearInterval(int);
    }
  }
