}

function toggle_game_display()
{
    currentDisplay = document.getElementById('more-games-container').style.display;

    if (currentDisplay != 'block')
    {
        document.getElementById('morelinkhref').innerHTML = 'Show Fewer Games';
        document.getElementById('more-games-container').style.display = 'block';
    }
    else
    {
        document.getElementById('morelinkhref').innerHTML = 'Show More Games';
        document.getElementById('more-games-container').style.display = 'none';
    }
}
