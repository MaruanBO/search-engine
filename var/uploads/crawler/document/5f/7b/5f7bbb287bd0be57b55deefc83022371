<script type="text/javascript">
$( window ).load(function() {
    
    function emailLightbox(){
        $('body').prepend('<div id="cdev-email-lightbox"><a id="lightbox-close"><span class="icon-close"></span></a><div class="cdev-lightbox-inner"><h2>Get on the <br/>Inside Track</h2><p>Be the first to know about <br/>new products, offers and events. </p><a id="join-email-list" class="button primary"><span>Join Our Email List</span></a></div></div>');    
        $('#cdev-email-lightbox').delay(5000).animate({ marginLeft: "0px" }, 600); // open module
        
        
        $('#lightbox-close').click(function(){     
            $('#cdev-email-lightbox').animate({ marginLeft: "-500px" }, 450); // close module
            return false;
        });
        

        $('#join-email-list').click(function(){
            showEmailForm();
        });
    }

    
    function showEmailForm(){
        $('#cdev-email-lightbox .cdev-lightbox-inner').html('<iframe src="https://shop.newbalance.com/pub/sf/ResponseForm?_ri_=X0Gzc2X%3DWQpglLjHJlYQGsrJXvozczcmFzcLYizd2E7bJzgcjOtNzc0OVXMtX%3DWQpglLjHJlYQGiCGmnlzdzg1sdY9BBpLJF9TRqf4BcgO&_ei_=EoISaEPBTX8PxGaGqQU5SSE" scrolling="no"></iframe>');

        $('#lightbox-close').click(function(){     
            $('#cdev-email-lightbox').animate({ marginLeft: "-500px" }, 450); // close module
            return false;
        });
    }
    
    
     // gets the cookie
    function getCookie(cname) {
        var name = cname + "=",
            ca = document.cookie.split(';'),
            i,
            c;
        for (i = 0; i < ca.length; i++) {
            c = ca[i].trim();
            if (c.indexOf(name) === 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }    

    // sets a cookie with an expiration date in days
    function setCookie(cname, cvalue, exdays) {
        var d = new Date(),
            expires;
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        expires = "expires=" + d.toGMTString();
        document.cookie = cname + "=" + cvalue + "; path=/; " + expires;
    }    

     // checks the cookie
    // if the cookie is not this will set the cookie
    function checkCookie() {
        if (navigator.cookieEnabled) {
            var hideEmailForm = getCookie("hideEmail");
            if (hideEmailForm !== "hide") {
                emailLightbox();
                setCookie("hideEmail", "hide", 365);
            }
        }
    }
    
    
    if (typeof lightboxOption === 'undefined') {
        // mobile check
        var windowsize = $(window).width();
        if (windowsize > 640) {
            checkCookie();      
        }
    }
});
</script>
