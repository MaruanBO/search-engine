<script type="text/javascript" src="http://demandware.edgesuite.net/aagi_prd/on/demandware.static/Sites-newbalance_us2-Site/-/default/v1424325526830/js/country_sites.js"></script>
	<script type="text/javascript" src="http://demandware.edgesuite.net/aagi_prd/on/demandware.static/Sites-newbalance_us2-Site/-/default/v1424325526830/lib/jquery/js/jquery/jquery.cookie.js"></script>
	
	<script type="text/javascript">
			function checkGeoLoc()
			{
				//Check for geolocation cookie; get geolocation
				var geoloc_cookie = $.cookie("akgeo");
				var site_country_code = "US";//parse for comma list
				if(site_country_code != "null") //don't show modal if the SiteCountryCode site preference hasn't been set
				{
					if(geoloc_cookie != "" && geoloc_cookie != null)
					{
						if(geoloc_cookie != site_country_code){
							//If site exists for country code is geo_cookie, show modal
							var new_site_url = findCountrySite(geoloc_cookie);
							showSiteRerouteModal(geoloc_cookie, new_site_url);
						}
					}
					else //call pipeline to get location from akamai
					{
						jQuery.ajax({
							//url: app.util.appendParamToURL("http://www.burton.com/on/demandware.store/Sites-Burton_US-Site/default/User-GetLocation"),
							url: app.util.appendParamToURL("https://www.newbalance.com/on/demandware.store/Sites-newbalance_us-Site/default/User-GetLocation"),
							type: "GET",
							crossDomain: true,
							dataType: "jsonp",
							async: true,
							success: function(data){
								if (data.success === true && data.data != null) {
									var json_country_code = data.data.country_code;
									if(json_country_code != site_country_code) {
										showSiteRerouteModal(json_country_code, findCountrySite(json_country_code));
									}
								}
							}
						});
					}
				}
			}
			
			function showSiteRerouteModal(cust_country_code, site_url)
			{
				// If there is another country site they could go to, show modal giving them option to redirect
				if(site_url != "") {
					// modal window
					if (jQuery("#siteRerouteModal").length == 0) {
						jQuery("<div/>").attr("id", "siteRerouteModal").appendTo(document.body);
					}
					jQuery("#siteRerouteModal").empty();

					app.createDialog({id: 'siteRerouteModal', options: {
						modal: true,
						height: 'auto',
				    	width: 650,
				    	minHeight: 370,
				    	title:""
					}});
					jQuery('#siteRerouteModal').dialog('open');
					jQuery("#siteRerouteModal").html(app.showProgress("productloader"));
					jQuery('#siteRerouteModal').parent().find('#ui-dialog-title-siteRerouteModal').css('margin','0px').css('padding-top','11px');
	
					var url = app.util.appendParamToURL('/on/demandware.store/Sites-newbalance_us2-Site/default/RedirectURL-CountryRedirect','CountryCode',cust_country_code);
					var url = app.util.appendParamToURL(url,'SiteURL',site_url);
	
					jQuery("#siteRerouteModal").load(url);	
				}
			}
			
			
	</script>


