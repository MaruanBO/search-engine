<script>
    LQcom.ready(function () {
        watermark("EMAIL_ADDR", "Enter your email address");
    });
function checkform() {
    if (document.signup.EMAIL_ADDR.value.length==0) {
        alert('Please provide your email address.');
        document.signup.EMAIL_ADDR.select();
        return false;
    }
    
    var string1=document.signup.EMAIL_ADDR.value
    if (string1.indexOf("@")==-1){
        alert("Please provide your email address."); 
        document.signup.EMAIL_ADDR.focus();
        document.signup.EMAIL_ADDR.select();
        return false ;
    }
    
    var string1=document.signup.EMAIL_ADDR.value
    if (string1.indexOf(".")==-1){
        alert("Email address requires a '.com', '.net' or similar extention.") ;
        document.signup.EMAIL_ADDR.focus();
        document.signup.EMAIL_ADDR.select();
        return false ;
    }
    
}

function doSubmit() {
    document.signup.action = document.signup.action.replace(/EMAIL_ADDR=(.*)&/, "EMAIL_ADDR=" + document.signup.EMAIL_ADDR.value + "&");
    window.open('','signup','resizable=1,scrollbars=1,width=450,height=470');
}
</script></div>
