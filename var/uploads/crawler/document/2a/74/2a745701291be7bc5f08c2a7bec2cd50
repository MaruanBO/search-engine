	<script type="text/javascript">	
        function completeFormData(latitute, longitude, range, campaingID, hotelName, hotelCountry, hotelState){
            $('#sdLocationLat').val(latitute);
            $('#sdLocationLng').val(longitude);
            $('#sdSearchRadius').val(range);
            $('#sdCid').val(campaingID);
            $('#sdSearchCity').val(hotelName);
            var stateAndCountryCode = hotelState+", "+hotelCountry;
            $('#sdstateandcountry').val(stateAndCountryCode);
            var sdForm = document.forms["specialDealsSearchForm"];
            LQcom.findHotels.updateContext(sdForm, LQcom.findHotels.codeLocationByDestination);
            sdForm.submit();
        }	
		LQcom.ready(function(){

			
            var fortnightCheckin = new Date();
			var fortnightCheckout = new Date();
			
			fortnightCheckin.setDate(fortnightCheckin.getDate()+14);
            fortnightCheckout.setDate(fortnightCheckout.getDate()+15);
            
            $('#sdCheckinDate').val($.datepicker.formatDate('mm/dd/yy', fortnightCheckin));
	        $('#sdCheckoutDate').val($.datepicker.formatDate('mm/dd/yy', fortnightCheckout));
			
			var specialDealsWithSessionID;
			var sessionDataGlobal = LQcom.Session.Global.getData();		
	  		var sessionDataFindAndBook = LQcom.Session.FindAndBook.getData(); 
			var ratesFrom = 'rates from';

			$('#specialDealsSessionId').val(sessionDataGlobal.sessionId)

			$.get("/bin/lq-com/specialDeals",
			{
				sessionId: sessionDataGlobal.sessionId
			},
			function(data){
				specialDealsWithSessionID = data;
				for(var i =0;i < specialDealsWithSessionID.length-1;i++)
				{
					var item = specialDealsWithSessionID[i];
					$('.bookEarlyScroll').append("<div class=\"bookEarlyScrollElement\">"+
													"<div class=\"beCity\">"+
														"<a onClick=\"javascript:completeFormData("+
															""+item["hotelGeoLocation"]["latitude"]+","+
															""+item["hotelGeoLocation"]["longitude"]+","+
															""+item["hotelRange"]["range"]+","+
															"'"+item["campaingID"]+"', "+
															"'"+item["hotelName"]+"','"+item["hotelCountry"]+"','"+item["hotelState"]+"')\" "+
															"href=\"javascript:void(0)\">"+
															""+item["hotelName"]+
														"</a>"+
													"</div>"+
													"<div class=\"beRates\">"+ratesFrom+
														" <strong>$"+item["hotelRate"]+"</strong>"+
													"</div>"+
													"<div class=\"clearDiv\"></div>"+
												"</div>");
				}
			}, "json");
	});
	</script>

    
	
