<script language="javascript">

    LQcom.ready(function() {
    	var globalData = LQcom.Session.Global.getData();
    	
    	$.get("/content/lq/lq-com/en/jcr:content/home-sidebar/findhotel.destinations.html?sessionId=" +  globalData.sessionId, function(data) {
        	var destinations = LQcom.parseJSON(data);
        	$('#searchCity').autocomplete({
        	    lookup: destinations,
        	    width:'auto',
        		lookupFilter: function (suggestion, originalQuery, queryLowerCase) {
        	                    return suggestion.value.toLowerCase().match("^"+queryLowerCase);
        	                },
        					formatResult:function(suggestion, currentValue){
        					return suggestion.value;
        					}
        	  });
    	});

		$("#submit").on("click",function(e) {
	            e.preventDefault();
	        	var values = $("#searchCity").val().split(",");
	        	$("#stateAndCountryCode").val(values[1]+", "+values[2]);
	            LQcom.findHotels.codeAddress(document.getElementById('findHotelsForm'),'Please specify a &quot;City&quot; & &quot;State&quot; or &quot;Zip&quot; to continue your search','No results are available for this route','The max number of searches has been exceeded','Request denied by the directions provider','The request was invalid');
		});
    });
</script>
