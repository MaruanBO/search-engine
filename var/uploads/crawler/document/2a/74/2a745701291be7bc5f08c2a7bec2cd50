    LQcom.ready(function() {
        $(".signInModuleSuccess").on("click", "#logoutlink", function(event) {
            event.preventDefault();
            if (!$("#remember").is(':checked')) {
                $("#returnsid").val("");
                watermark("returnsid", "Member #");
            }
            LQcom.ReturnsLogin.logoutUser();
            $("#loginaction").val("logout");
            $("#rcCheckAccountForm").submit();            
        });
    });
