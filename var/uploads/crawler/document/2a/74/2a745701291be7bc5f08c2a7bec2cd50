    LQcom.ready(function() {
        LQcom.googleMaps.initGeoCoder();

        var sessionData = LQcom.Session.Global.getData();
        $('#findHotelSessionId').val(sessionData.sessionId);
        
        var ccSearch = LQcom.Session.FindAndBook.getData();
        
        LQcom.datePicker.createDatePicker("checkinDate", "checkoutDate", "en", ccSearch.checkinDate, ccSearch.checkoutDate);
        if(ccSearch.checkinDate){
            $('#checkInDate').val(ccSearch.checkinDate);
        }
        if(ccSearch.checkoutDate){
            $('#checkOutDate').val(ccSearch.checkoutDate);
        }
        
		
		$('#promoCode').val(ccSearch.promoCode ? ccSearch.promoCode.trim() : ccSearch.promoCode);
		
		
		
		$('select[name=specialRates]').val(ccSearch.rateType);
		

		if("no" == "yes"){
      		var lead = '2';
		      var startdate = '11/02/2014';
		      var enddate = '11/03/2014';
		      var promocode = '';
		      var iata = '';
			var hidePromocode = 'no'
			var	overrideType = 'none'
			setDefaultConfigurations(startdate, enddate, promocode, iata, lead, hidePromocode, overrideType);
		}

		   $("#checkinDate").blur(function(){
				var dt = $("#checkinDate").val();
	            if(!LQcom.isDateValid(dt)){
	                alert("The Check-In Date and Check-Out Date must be in MM/DD/YYYY format. Please enter your dates in this format or select dates from the graphical Calendar that appears when you click into each field.");
	                if(ccSearch.checkinDate){
	                	$("#checkinDate").val(LQcom.checkPreceedingZeroForDate(ccSearch.checkinDate));
	                }
	                if(ccSearch.checkoutDate){
	                	$("#checkoutDate").val(LQcom.checkPreceedingZeroForDate(ccSearch.checkoutDate));
	                }
	            }

	        });

	        $("#checkoutDate").blur(function(){
				var dt = $("#checkoutDate").val();
	            if(!LQcom.isDateValid(dt)){
	                alert("The Check-In Date and Check-Out Date must be in MM/DD/YYYY format. Please enter your dates in this format or select dates from the graphical Calendar that appears when you click into each field.");
	                if(ccSearch.checkinDate){
	                	$("#checkinDate").val(LQcom.checkPreceedingZeroForDate(ccSearch.checkinDate));
	                }
	                if(ccSearch.checkoutDate){
	                	$("#checkoutDate").val(LQcom.checkPreceedingZeroForDate(ccSearch.checkoutDate));
	                }
	            }

	        });

	});


    function setDefaultConfigurations(startdate, enddate, promocode, iata, lead, hidePromocode, overrideType) {
    	
    	if(overrideType == "lead"){
			setLeadTime(parseInt(lead));
    	}else if(overrideType == "fixed"){
    	if(startdate!=undefined && startdate!="" && enddate!=undefined && enddate!="" ) {
	    	 $("#checkinDate").val(startdate);
	         $("#checkoutDate").val(enddate);
	         LQcom.datePicker.createDatePicker("checkinDate", "checkoutDate", "en", startdate, enddate);
    	}
    	}
    	if(hidePromocode == "no" && promocode != ""){
         $("#promoCode").val(promocode);
    	}
         
         LQcom.cobrand.setIata(iata);
    }

         function setLeadTime(lead){

		  function addDays(dateObj, numDays) {
           dateObj.setDate(dateObj.getDate() + numDays);
           return dateObj;
        }

        var now = new Date();
        var currentAhead = addDays(new Date(), lead);
        var currentTommorrowAhead = addDays(new Date(), (lead+1));
        
	        var checkinDate = LQcom.formatDate(currentAhead);
	        var checkoutDate = LQcom.formatDate(currentTommorrowAhead);
        
        $("#checkinDate").val(checkinDate);
        $("#checkoutDate").val(checkoutDate);
        }
</script>
