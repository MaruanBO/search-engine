<script>
require.config({
baseUrl: 'http://www.hopkinsmedicine.org/lib/js/',
paths: {
responsive: '',
jquery: 'jquery-1.8.3.min',
search: '../template_engine.cfm?template=searchhbs',
resjs: '../../sebin/r/b/thincrust-responsive',
ems: '../template_engine.cfm?template=responsiveems',
modernizr: 'modernizr.min',
jQueryTweet: 'jquery.tweet.js'
},
shim: { 
modernizr: { exports: 'Modernizr' },
'jquery.slideshow': ['jquery'],
'jquery.autocomplete.min':['jquery']
}
});

define('responsive', function () {

});

require(['domready!', 'jquery', 'resjs','jquery.slideshow','jquery.autocomplete.min'], function (dom, $, resjs, jqs, jqa) {
resjs.setupThincrustActions();

// Preload Images
$( '#slider1 img' ).each( function( i, ele ) {
var image = $('<img />').attr('src', $( ele ).data( 'full-image' ) );
});

//Initialize Slider
var slider = $('#slider1').jhmSlider({
auto: true,
displaySlideQty: 7,
moveSlideQty: 1,
pause: 9000,
controls: true,
nextImage: 'http://hopkinsmedicine.org/sebin/z/z/slideshow_next.png',
prevImage:'http://hopkinsmedicine.org/sebin/f/l/slideshow_prev.png',
nextSelector:'.slider-next',
prevSelector: '.slider-prev',
mode:'vertical',
onBeforeSlide: function( currentSlideNumber, totalSlideQty, currentSlideHtmlObject ) {
// Empty for now
},
onAfterSlide: function( currentSlideNumber, totalSlideQty, currentSlideHtmlObject ) {
var $selected = $( currentSlideHtmlObject );
var $thumb = $selected.find( 'img' );

// Give the new thumbnail the selected class
$selected.addClass( 'selected' ).siblings().removeClass( 'selected' );

//change the masthead image
var banner_src = $thumb.attr( 'data-full-image' );
var alt_text = $thumb.attr( 'alt' );
var title_text = $thumb.attr( 'title' );
var description = $thumb.data('description')
? $thumb.data('description')
: null;
var link = $thumb.attr('data-href')
? $thumb.attr('data-href')
: '#';
var target = $thumb.attr('data-target')
? $thumb.attr('data-target')
: '_top';

// Create a new image element and place it "under" the current
// masthead image.
var $newMasthead = $( new Image() );
$newMasthead
.addClass( 'slide' )
.attr( 'alt', alt_text )
.attr( 'src', banner_src )
.prependTo( '.banner-inner' )
;
if(link){ 
var a = $('<a/>'); a.attr('href', link); a.attr("target",target);
$newMasthead.wrap(a);
}
// Apply any caption to the image
if( title_text ) {
var title = $('<h2>' + title_text + '</h2>').wrapInner(a).get(0).outerHTML;
var caption = description && description.length
  ? $('<p>' + description +'<span></span></p>').wrapInner(a).get(0).outerHTML
  : "";
$newMasthead.parent('a').after( '<div class="caption">' + title + caption + '</div>' );
}
else {
// If no caption exists, display a placeholder in case the next slide
// does contain one. If the placeholder doesn't exist, the .caption:last
// selector below will pick up the caption we want to display next and
// remove it.
$newMasthead.after( '<div class="caption"></div>' );
}



// The :not(:first) of each element will include the one that is currently being displayed. We
// need to fade that one out so that the one beneath effectively fades in. Which will be the first elment
$( '.banner .banner-inner .slide:not(:first), .banner .banner-inner .caption:not(:first)' )
.stop( true, true )
.fadeOut( 1500, function() {
var $this = $( this );
if($this.hasClass('slide')){
$this.parent().remove();
}else{
$this.remove();
}
});
}
});

// Register a thumbnail event listener
$('#slider1').on('click', 'img', function( e ) {
var $container = $( e.delegateTarget );
var $this = $(this);

// $.index() is 0-based. Something else doesn't appear to be in case
// you find your self wondering why we're subtracting 1 below.
var i = $container.find( 'li' ).index( $this.parent() ) - 1; 
goTo = i < slider.getSlideCount()
? i
: i - slider.getSlideCount();

slider.stopShow();
slider.goToSlide( goTo, false );
slider.startShow();
});

// Get Facebook likes
$.getJSON("https://api.facebook.com/method/fql.query?query=select%20%20like_count%20from%20link_stat%20where%20url=%22https://www.facebook.com/Johns.Hopkins.Medicine%22&format=json",
function(data) {
$('.facebook-likes').prepend(addCommas(data[0].like_count));
});
// Get data from the youtube user 
$.getJSON("https://gdata.youtube.com/feeds/api/users/johnshopkinsmedicine?callback=?&v=2&alt=json",
{}, function(data) {
$('.subscriber-count').prepend(addCommas(data.entry.yt$statistics.subscriberCount));
$('.view-count').prepend(addCommas(data.entry.yt$statistics.totalUploadViews));
});
/** Add commas for stat formatting */
function addCommas(nStr)
{
nStr += '';
x = nStr.split('.');
x1 = x[0];
x2 = x.length > 1 ? '.' + x[1] : '';
var rgx = /(\d+)(\d{3})/;
while (rgx.test(x1)) {
x1 = x1.replace(rgx, '$1' + ',' + '$2');
}
return x1 + x2;
}

//Tab functionality
$('ul.tabs li.tab').hide();
$('ul.tabs li.active').show();
$('ul.tab-titles li').click(function(event) { 
var t_class = "." + $(this).attr('class'); //current tab class
var x = t_class.replace(/(<[^>]*>)/g,'').split(' '); // grab first class only
//add active to tab, remove active from other tabs
$(x[0]).addClass('active').siblings().removeClass('active'); 
//hide/show tabs
$('ul.tabs li.tab').hide();
$('ul.tabs li.active').show();
event.preventDefault(); //stop the href from redirecting
});

// remove height and width attributes from flexible images
$('.flex-img img').each(function(){
$(this).removeAttr('width');
$(this).removeAttr('height');
});

var sdcAC = $("#spec_disease").autocomplete({
serviceUrl: window.location.protocol + "//" + window.location.host + "/dataservice/query",
minChars:2,
delimiter:'',
maxHeight:240,
width:$("#spec_disease").width(),
zIndex:9999,
params: {
hch_doc: 'no',
jh_doc: 'yes',
cp_doc: 'yes',
gs_doc: 'yes',
vip_doc: 'yes',
type: 'sdc'
},
noCache:false,
onSelect: function(value,data){return true;}
});
});
</script>

