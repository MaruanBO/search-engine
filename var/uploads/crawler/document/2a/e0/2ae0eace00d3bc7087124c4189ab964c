<script>
  // set up Find n Save

  if ( typeof( mi.FindnSave ) !== 'undefined' ) {
    mi.findnSave = new mi.FindnSave();
  }

  if ( mi.findnSave.isFirstWidget() ) {
    mi.findnSave.setConf( "fns_domain", "tricityherald.com" );
    mi.findnSave.setConf( "enabled", true );
  }

  jQuery(document).ready( function() {
    var domain = "http://" + mi.findnSave.getConf( "fns_subdomain" )
      + mi.findnSave.getConf( "fns_domain" );
    jQuery(".fnsLogoLink").attr( 'href', domain + '#wgt=fnscombo' );
    console.info( "FindnSave domain is " + domain );
    jQuery("div.dsSearch form").attr( 'action', domain + '/Search' + '#wgt=fnscombo' );
    jQuery.getJSON( domain + "/api/printadvertiser.json?n=2&callback=?",
    function(json) { printadvertiser(json) } );
  } );

  function printadvertiser(data) {
    var paAdvertiser = data.result.advertisers.advertiser;
    var num_ads = paAdvertiser.length;
    if(  num_ads == 0 ) return;

    jQuery(".dsROP").css( 'display', 'block' );

    jQuery("h3.dsROP_title span.dsROP_title_text").text( 'Featured Advertisers' );
    jQuery("h3.dsROP_title span.dsROP_viewall_link a").attr( 'href',
      '/Local-Ads' + '#wgt=fnscombo' );
    mi.findnSave.insertFnSDomain();

    var content = jQuery("div.dsROP_content div.fnsPrintAd");
    
    jQuery("div.dsROP_content").each(function(index) {
      var content = $(this).find("div.fnsPrintAd");
      for (var i = 0; i < num_ads; i++) {
        content.eq(i).find("a").attr( 'href', paAdvertiser[i].adUrl + '#wgt=fnscombo' );
        content.eq(i).find("h5.paBusiness a").text( paAdvertiser[i].name );
        content.eq(i).find("a img").attr( 'src', paAdvertiser[i].adimageUrl );
        content.eq(i).css( 'display', 'inline' );
      }
    });
    
  }


</script>

<!-- *** Find n Save Traffic Driver *** -->

