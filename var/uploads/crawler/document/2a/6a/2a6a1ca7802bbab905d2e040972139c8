        <!-- ko foreach: variants() -->
          <strong data-bind="text: $data[1]"></strong>
          <!-- ko if: $index() < ($parent.variants().length - 1) -->, <!-- /ko -->
        <!-- /ko -->
        <br>
      <!-- /ko -->

      <!-- ko ifnot: editingQty -->
        <!-- ko text: quantity --><!-- /ko --> x <strong data-bind="text: priceDescription"></strong>
        <a href="#" class="edit-basket-item-qty" title="Edit Quantity" data-bind="visible: !emailFulfillment()">edit</a>
      <!-- /ko -->

      <!-- ko if: editingQty -->
        <strong>Qty:</strong>
        <select class="basket-item-qty-select" data-bind="value: selectQty, options: qtyOptions"></select>
        <a href="#" class="cancel-edit-basket-item-qty">cancel</a>
      <!-- /ko -->

      <!-- ko if: shippingSurcharge() > 0 -->
        <br><strong>Shipping Surcharge:</strong>
        <!-- ko text: shippingSurchargeDescription --><!-- /ko --> each
      <!-- /ko -->
    </div>

    <div class="clearfix"></div>

    <!-- ko if: confirmMessages().length > 0 -->
      <div class="basket-item-confirm">
        <!-- ko with: confirmMessages()[0] -->
          <!-- ko text: message --><!-- /ko -->
          <br><a class="confirm-basket-item" href="#" data-bind="text: confirmText"></a> &middot;
        <!-- /ko -->
        <!-- ko if: confirmMessages()[0].showRemoveLink -->
          <a class="remove-basket-item" href="#">Remove from basket</a>
        <!-- /ko -->
      </div>
    <!-- /ko -->

    <!-- ko if: removeMessages().length > 0 -->
      <div class="basket-item-confirm removal">
        <!-- ko text: removeMessages()[0].message --><!-- /ko -->
        <br><a class="remove-basket-item" href="#">Remove from basket</a>
      </div>
    <!-- /ko -->

    <div class="basket-item-body email-fulfillment" data-bind="with: emailFulfillment">
      <label>From:</label>
      <!-- ko text: fromName --><!-- /ko --> (<!-- ko text: fromEmail --><!-- /ko -->)
      <br>
      <label>To:</label>
      <!-- ko text: toName --><!-- /ko --> (<!-- ko text: toEmail --><!-- /ko -->)
      <br>
        <label>Delivers:</label>
        <span data-bind="text: deliveryDateDescription"></span><br>
      <!-- ko if: message -->
        <label>Message:</label>
        <!-- ko text: message --><!-- /ko -->
      <!-- /ko -->
    </div>
  </li>
</ul>

                      </li>
                    </ul>

                    <div class="basket-preview-footer">
