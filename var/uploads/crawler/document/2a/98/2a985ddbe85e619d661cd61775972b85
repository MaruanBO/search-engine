<script>
	function checkEmailAddress(email) {
		var validEmail = email.match(/^[\w-\.]{1,}\@([\da-zA-Z-]{1,}\.){1,}[\da-zA-Z-]{2,6}$/) ? true : false;
		if (validEmail) {
			return true;
		} else {
	 		alert("Please enter a valid email address!");
			return false;
		}
  	}

	jQuery(document).ready(function(){
		jQuery(".signup_button").click(function(){
			var url = "https://www.oshkosh.com/on/demandware.store/Sites-Carters-Site/default/EmailSignup-Start?ajaxusername=UUUU";
			var form = $(this).closest("form");
			var email = form.find("input[name=email_signup]").val();
			url = url.replace("UUUU", email);
			if(checkEmailAddress(email)) {
				document.location.href = url;
			}
		});
		
		jQuery('.join-content-emailsignup input').keypress(function(event){
			var keycode = (event.keyCode ? event.keyCode : event.which);
			if(keycode == '13'){
				event.preventDefault();
				var url = "https://www.oshkosh.com/on/demandware.store/Sites-Carters-Site/default/EmailSignup-Start?ajaxusername=UUUU";
				var form = $(this).closest("form");
				var email = form.find("input[name=email_signup]").val();
				url = url.replace("UUUU", email);
				if(checkEmailAddress(email)) {
					document.location.href = url;
				}
				return false;
			}
		});

  	});
</script>



       	
       	
		
			
				 	
