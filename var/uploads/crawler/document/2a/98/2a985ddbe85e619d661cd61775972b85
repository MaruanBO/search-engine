$(".onFocusClear").each(function(){
	$(this)
	.attr('defaultVal', $(this).attr('value'))
	.focus( function(){ if( $(this).attr('defaultVal') == $(this).attr('value') ){$(this).attr('value', '')} } )
	.blur( function(){ if( $(this).attr('value') == '' ){$(this).attr('value', $(this).attr('defaultVal'))} } )
