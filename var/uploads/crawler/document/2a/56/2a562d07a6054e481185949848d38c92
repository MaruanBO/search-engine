    function set_astrology()
    {
        var astro_cookie = readCookie('astro');       
        
        if (astro_cookie != "" && astro_cookie != null)
        {
            astro_cookie = astro_cookie.toLowerCase();
            if (astro_cookie != 'aries') {
                astro.astro_js(astro_cookie);
            }
        }
        
    }
