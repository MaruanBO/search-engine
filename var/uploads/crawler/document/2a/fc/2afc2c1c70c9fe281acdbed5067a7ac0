    'dojo/dom-class',
    'dojo/query',
    'dojo/topic',
    'dojo/domReady!'
], function (domClass, query, topic) {
    // Subscribe to the "update" event to react to rotator updates
    topic.subscribe('HeroRotator/rotator/update', function (action, rotator) {
        var buttons;
        var i;
        // Only pay attention to the "onAfterTransition" action
        if (action !== 'onAfterTransition') {
            return;
        }
        // Get the set of buttons (DIV elements in this case) contained in the element with ID "HeroRotator-controls" 
        buttons = query('div', 'HeroRotator-controls');
        for (i = 0; i < buttons.length; i++) {
            domClass.remove(buttons[i], 'selected'); 
        }
        domClass.add(buttons[rotator.idx], 'selected');
    });
});
</script><!-- END ContentRecommendation.jsp --><!--/top banner -->
