<script type="text/javascript">
	function updateMiniCartTitle() 
	{
		var dialog = dijit.byId("cartDialog");
		if(dojo.byId("ordRecCount") != undefined)
		{
			var count =  dojo.byId("ordRecCount").value;
			var message = count + " item(s) <span>in your cart</span>";
			dialog.set('title',message );
		}
	}
	function addToCart(entitledItem)
	{
		shoppingActionsJS.setCommonParameters('-1','10151','10151','G','$');
		shoppingActionsServicesDeclarationJS.setCommonParameters('-1','10151','10151');
		shoppingActionsJS.Add2ShopCartAjax(entitledItem,'1', false);
	}
</script>	

