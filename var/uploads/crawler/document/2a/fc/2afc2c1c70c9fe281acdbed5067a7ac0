<script type="text/javascript">
		function downloadExe(){
			var chkStatus = document.getElementById('agreeTC').checked ;
			var finalURL;
		
			if (navigator.appVersion.indexOf("Win")!=-1){
				if(document.getElementById("scannerUrlId") != null){
					finalURL =   document.getElementById("scannerUrlId").value ;
					console.debug("windows url ="+ finalURL);			
				}
			} else if (navigator.appVersion.indexOf("Mac")!=-1){
				if(document.getElementById("scannerMacUrlId") != null){
					finalURL =  document.getElementById("scannerMacUrlId").value ;
					console.debug("mac url ="+ finalURL);			
				}
			} else{
				if(document.getElementById("scannerUrlId") != null){
					finalURL =   document.getElementById("scannerUrlId").value ;
					console.debug("windows url ="+ finalURL);			
				}
			}
		
		
			if(chkStatus){
				if(finalURL != null){
					window.location.href=finalURL;
				}
				 
			}else{
				var termsCond =  document.getElementById("termsCondId").value;
				if(termsCond == null || termsCond == ''  || termsCond  == "undefined" ){
					termsCond = "Please agree to the terms and condition";
				}
				document.getElementById('errorMsg').innerHTML ='<span class="errorMessage roundborder">'+termsCond+'</span> ';
			}
		}

	</script> 
