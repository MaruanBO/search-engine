<script type="text/javascript">
	dojo.addOnLoad(function() {
	
		var modelTypeVal = document.getElementById("modelType1").value;
		var manufacturerVal = document.getElementById("manufacturer1").value;
		var modelTypeDisplayFlag = document.getElementById("modelTypeDisplay1").value;

		if(modelTypeVal.length == 0){
			if (manufacturerVal.length > 0) {
				load("MF","ALL", null, document.getElementById("modelTypeDIV1"));
				
				if (modelTypeDisplayFlag == "false") {
					document.getElementById("modelTypeTextDIV1").style.display="none";
					document.getElementById("modelTypeDIV1").style.display="none";
				} else {
					document.getElementById("modelTypeTextDIV1").style.display="block";
				}
			}
			else {
				// When modelType, manufacturer NOT passed. But modelTypeDisplay flag is passed, then this flag loses it's meaning.
				document.getElementById("modelTypeDIV1").style.display="block";
			}
		}
		else {
			document.getElementById("modelTypeDIV1").style.display="block";
			var modelTypeFacetVal = document.getElementById("modelTypeSelect1").options[document.getElementById("modelTypeSelect1").options.selectedIndex].value;
			if (modelTypeDisplayFlag == "false") {
				modelTypeFacetVal = "ALL";
			}
			load("MF",modelTypeFacetVal, null, document.getElementById("modelTypeDIV1"));
			
			document.getElementById("modelTypeText1").innerHTML=modelTypeVal;
			
			if (modelTypeDisplayFlag == "false") {
				document.getElementById("modelTypeTextDIV1").style.display="none";
				document.getElementById("modelTypeDIV1").style.display="none";
				
				
			} else {
				document.getElementById("modelTypeTextDIV1").style.display="block";
			}
		}
		parseWidget("manufacturerDisplayArea1");
		parseWidget("productLineDisplayArea1");
		parseWidget("modelDisplayArea1");
		
	});
</script>
