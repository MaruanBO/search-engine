<script type="text/javascript">
function updateMiniWLTitle() {
	var dialog = dijit.byId("wishListDialog");
	var count =  dojo.byId("wlRecCount").value;
	var itemsInWL = 'items <span>in your wish list</span>';
	var message = '';
	
	if(count != '' && Number(count) > 0) {
		message = count + ' ' + itemsInWL;
	}
	
	dialog.set('title',message ); 
}
</script>
