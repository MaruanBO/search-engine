<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {
$('#DLFeatureFlex-yw0').dlFeatureFlex({'divId':'ad-1','maxClicks':'5','rotateInterval':'60','beaconAttrNm':'dl-rotate'});
jQuery('#yw5').gameRatings({'gameId':'43386','disabled':null});
jQuery('.module.module-game-remove').gameRemove();
$('.module-game-tile').gameTile();
jQuery('#yw11').gameRatings({'gameId':'42869','disabled':null});
jQuery('#my-tray').isotopeTray({'intersectDivId':'ad-1','tabCookieName':'gHomeTray','trays':{'#recently-played':{'eventListeners':'gameRemoveEvent','promoLinkPrefix':'home - index - my tray - recently played - ','url':'/players/summary/me','class':'recently-played','queryParams':{'sort':'TIME','start':2,'limit':8},'showCompleteRows':false,'filter':function($guest, $tray){
                            if ($tray.find('.module-isotope.game.recently-played:not(.template):not(.in-transition)').length==0) {
                                return '.module-isotope.recently-played.template';
                            } else {
                                return '.module-isotope.recently-played.game:not(.template)';
                            }
                        },'getGamesData':function(data){
                            var gameData = [];
                            for(var ii = 0; ii < data.summary.length; ii++) {
