<script id="GameRatings" type="text/x-mustache-template">
<div
    id="{{moduleId}}"
    class="module module-game-rating {{starSize}} {{displayMode}}"
    data-id="{{gameInfo.id}}"
    data-readOnly="{{isReadOnly}}"
    data-name="{{gameInfo.name}}" 
    {{#includeSchema}}
    itemprop="aggregateRating" itemscope="itemscope" itemtype="http://schema.org/AggregateRating"
    {{/includeSchema}}
>

    {{#includeSchema}}
        <meta itemprop="ratingValue" content="{{gameInfo.formattedRating}}" />
        <meta itemprop="bestRating" content="5" />
        <meta itemprop="ratingCount" content="{{gameInfo.numRatings}}" />
    {{/includeSchema}}        
    
    <a
        href="javascript:void(0)"
        class="average oRating {{gameInfo.ratingValueText}} {{#gameInfo.userRating}} user{{/gameInfo.userRating}}"
        {{#gameInfo.userRating}}data-user-rating="{{gameInfo.userRating}}"{{/gameInfo.userRating}}
        data-avg-rating="{{gameInfo.avgRating}}"
        title="{{gameInfo.formattedRating}} star rating" 
        >
        {{#gameInfo.userRating}}
            {{gameInfo.userRating}}
        {{/gameInfo.userRating}}
        {{^gameInfo.userRating}}
            {{gameInfo.avgRating}}
        {{/gameInfo.userRating}}

    </a>
    <span class="votes">

    {{#gameInfo.userRating}}
        Your Vote
    {{/gameInfo.userRating}}

    {{^gameInfo.userRating}}
