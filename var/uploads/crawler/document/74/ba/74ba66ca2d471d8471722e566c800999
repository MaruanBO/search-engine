	<script>
	$(function(){
		$('.media-scroll-links a').on('click',function(){
			var id = $(this).data('type');	
			if(id=="scroll-seeall"){window.location.href="/category/what-is-new.html";return true;}
			
			$('.media-scroller').removeClass('currently-active');
			$('.media-scroll-links a').removeClass('selected');
			$(this).addClass('selected');
			$('#'+id).addClass('currently-active');
			
			return false;
		});
	});
	</script>
	
	
		
