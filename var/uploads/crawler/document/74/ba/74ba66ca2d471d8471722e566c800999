<script>
function updateNavArrows(){
        var slideW = $('.fullwidthbanner-container').width();
        var winW = $(window).width();
        var arrowW = 20;
        if(slideW > winW)arrowW = (slideW - winW)/2 + 20;

        $('.tp-rightarrow').css('right', arrowW+"px");
        $('.tp-leftarrow').css('left', arrowW + "px");
}
$(function(){

	$("#slideDownButton").click(function() {
	    $('html, body').animate({
	        scrollTop: ($(".home-container-content").offset().top - 180)
	    }, 1000);
		return false;
	});


	
});	
</script>


