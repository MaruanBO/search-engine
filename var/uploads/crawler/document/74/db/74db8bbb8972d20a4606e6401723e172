<script type="text/javascript">
$(document).ready(function(){
$('#cycleShow').cycle();
$('.jsSlideCap').mouseout(function(){ $('#cycleShow').cycle('resume'); });
$('.jsSlideCap').mouseover(function(){ $('#cycleShow').cycle('pause'); });
});
function showCaption(capId){
$('#slide_'+capId+' .jsSlideCam').hide();
$('#slide_'+capId+' .jsSlideCap').show();
}
function hideCaption(capId){
$('#slide_'+capId+' .jsSlideCap').hide();
$('#slide_'+capId+' .jsSlideCam').show();
}
</script>
