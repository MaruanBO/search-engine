   <script type='text/javascript'>
var signinLink = document.getElementById('persona-signin');

function loginViaEmail() {
  navigator.id.get(function(assertion) {
    if (assertion) {
        $$('input[name=assertion]')[0].value = assertion;

        $("persona_form").submit();
    } else {
      window.location = "#{failure_path}"
    }
  });
}


if (signinLink) {
  signinLink.onclick = function() { loginViaEmail(); return false; };
}
</script>


