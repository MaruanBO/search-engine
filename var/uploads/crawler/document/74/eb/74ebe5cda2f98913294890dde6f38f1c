    <script type="text/javascript">
    var soup_is_index = 0;
    function soup_is() {
      var soup_is = [
        'your own space on the web',
        'blogging for the rest of us',
        'a place to share cool stuff',
        'your remix of the web',
        'self-expression made simple',
        'all about you',
        'easy as pie',
        'your new bicycle',
        'easy personal publishing'
      ];
      var o = $('soup_is');
      soup_is_index++;
      if(soup_is_index > soup_is.length-1) soup_is_index = 0;
      new Fx.Style(o, 'opacity', { duration:300 })._start(.999, 0);
      setTimeout(function() {
        o.innerHTML = soup_is[soup_is_index]+'.';
        new Fx.Style(o, 'opacity', { duration:300 })._start(0, .999);
      }, 320);
    }
    Event.observe(window, 'load', function() { setInterval(soup_is, 3000) })
    </script>

