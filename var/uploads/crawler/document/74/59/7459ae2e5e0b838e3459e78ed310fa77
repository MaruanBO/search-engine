<script type="text/javascript">
  function mycarousel_initCallback(carousel) {
    $('.jcarousel-control a').bind('click', function() {
      carousel.scroll($.jcarousel.intval($(this).text()));        
      return false;
    });
    $('#mycarousel-next').bind('click', function() {
      carousel.next();
      return false;
    });
    $('#mycarousel-prev').bind('click', function() {
      carousel.prev();
      return false;
    });
    hpJCarousel = carousel;
  };

  function mycarousel_reloadCallback(carousel) {
    // if the carousel is animating then wait for the animation to end (same if locked)
    if (carousel.locked || carousel.animating) {
      window.setTimeout(mycarousel_reloadCallback.bind(this, carousel), 200);
      return;
    }

    // resize the carousel items to fit the screen
    // based on lines 153 ... 170 in /js/jq/jquery.jcarousel.js
    var di = carousel.options.visible != null ? Math.ceil(carousel.clipping() / carousel.options.visible) : null;
    var li = carousel.list.children('li');

    if (li.size() > 0) {
      var wh = 0, i = carousel.options.offset;
      li.each(function() {
        carousel.format(this, i++);
        wh += carousel.dimension(this, di);
      });

      carousel.list.css(carousel.wh, wh + 'px');
    }

    // reposition the list
    var w = carousel.get(carousel.first).width(),
        current_lt = parseInt(carousel.list.css(carousel.lt)),
        updated_lt = Math.round(current_lt / w) * w;
    if (current_lt !== updated_lt) {
      carousel.animate(updated_lt, false);
    }
  }

  $(function() {
    $('#mycarousel').jcarousel({
      scroll: 1,
      visible: 1,
      auto: 10,
      wrap: 'last',
      initCallback: mycarousel_initCallback,
      reloadCallback: mycarousel_reloadCallback,
      buttonNextHTML: null,
      buttonPrevHTML: null
    });
    $('#mycarousel ul li:first img').addClass('currImage');
    $('#mycarousel #controls a:first').addClass('selected');
  });
</script>
            
