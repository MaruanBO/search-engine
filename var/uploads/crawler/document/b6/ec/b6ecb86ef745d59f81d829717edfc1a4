       function getParameterByName(name) {
	var match = RegExp('[?&]' + name + '=([^&]*)')
	    .exec(window.location.search);
    return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
}

var mobileOffParam = getParameterByName("mobile");

var uagent = navigator.userAgent.toLowerCase();

var isMobile = uagent.match(/(ipod)|(PlayBook)|(Xoom)|(android)|(msnbot_mobile)|(googlebot-mobile\/2.1)|(googlebot-mobile)|(Blackberry)|(htc)|(webos) |(symbian)|(series60)|(series70)|(series80)|(series90)|(windows ce)|(iemobile)|(wm5 pie)|(vnd.rim)|(iphone)|(palm)|(windows mobile)|(blackberry)|(benq)|(au-mic)|(astel)|(cricket)|(docomo)|(itunes)|(j-phone)|(kddi-kc)|(lge)|(lg\/ku990i)|(lg-lg)|(lg-gt)|(lg-gs)|(2.1pre31)|(maui wap)|(mobileexplorer)|(mot)|(elaine\/3.0)|(avantgo 3.2)|(brew)|(teleca)|(polaris)|(kindle)|(j2me)|(armv5te)|(windows phone)|(pda)|(playstation)|(mobile content viewer)|(mobile safari)|(zunewp7)|(silk\/1.1.0-84)|(nintendo 3ds)|(bada)|(armv61)|(armv7l)|(nokia)|(nook browser\/1.0)|(openwave untrusted\/1.0)|(opera mobi)|(opera mini)|(opwv-sdk)|(pdxgw\/1.0)|(reqwirelessweb\/3.2)|(sam-r)|(samsung-s8003)|(samsung-sgh)|(sch-a950)|(sec-sgh)|(sonyericsson)|(sharp-tq-gx)|(sie-me45)|(sie-s)|(up.browser)|(blazer 1.0)|(Vodafone\/1.0)|(xiino)|(XV6850)|(YahooSeeker)/i);
var check_city = location.host; // Go to mobile site if site is partycity
var currentPath = window.location.pathname;
var search = window.location.search;
var protocol = window.location.protocol;


//if(isMobile != null && mobileOffParam != "off" && ${mobileEnabled}){
	//window.location.href="http://${header.host}/mobile.do";
