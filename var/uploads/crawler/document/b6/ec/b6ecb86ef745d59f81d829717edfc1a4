// function for getting MobileOptOut query parameter
function mobileOptOut(){ // right now only single value is appended as query parameter
	var href = window.location.href.split('?');
	if(typeof href[1] != 'undefined' && href[1].indexOf('&') != -1){
	href =href[1].split('&');
	}
	for(i=0;i<href.length;i++){
		if(href[i].indexOf('MobileOptOut') != -1){
		href = href[i].split('=');
			if(href[1] != ''){				
				return href[1];
			}else{
				return null;
				}
		}
	}
}

// function redirects to mobile site
function redirectToMobSite(){
if(check_city.toLowerCase().indexOf("www") == -1){
	check_city = "m." + check_city;
} else{
	check_city = check_city.replace(/www/i,"m");		
}
window.location.href = protocol + "//" + check_city + currentPath + search;
}

// function for setting the cookie
function setCookie(c_name, value, mins) {
	var exdate = new Date();
	exdate.setTime(exdate.getTime() + (mins*60000));
	var c_value = escape(value)
			+ ((mins == null) ? "" : "; expires=" + exdate.toGMTString());
	document.cookie = c_name + "=" + c_value + ";domain=." + document.domain + ";path=/";
}
// function gets the cookie
function getCookie(c_name) {
	var c_value = document.cookie;
	var c_start = c_value.indexOf(" " + c_name + "=");
	if (c_start == -1) {
		c_start = c_value.indexOf(c_name + "=");
	}
	if (c_start == -1) {
		c_value = null;
	} else {
		c_start = c_value.indexOf("=", c_start) + 1;
		var c_end = c_value.indexOf(";", c_start);
		if (c_end == -1) {
			c_end = c_value.length;
		}
		c_value = unescape(c_value.substring(c_start, c_end));
	}
	return c_value;
}
// function checks for cookie
function checkCookie() {
	if(isMobile != null && isMobileEnabled){
		var mobOptOutUrlVal = mobileOptOut(); 
		if(typeof mobOptOutUrlVal == 'undefined'){
			mobOptOutUrlVal = null;}
		var optOutCookie = getCookie("MobileOptOut");
		
		
		// user hits site for 1st time, no cookie present + mobile environment + mobOptOutUrl is null		
			if(null == optOutCookie && null == mobOptOutUrlVal){	/* no cookie, no optout var, just redirect */				
				redirectToMobSite(); 			
			} else if(mobOptOutUrlVal == 1){ 						/* if optout url param is 1, cookie set to 1 */
				setCookie("MobileOptOut", "1", 30);
			} else if(mobOptOutUrlVal == 2){ 						/* if optout url param is 0, cookie is destroyed */
				setCookie("MobileOptOut", "", -1);
			} else if(mobOptOutUrlVal == 0){ 						/* if optout url param is 0, cookie is destroyed and redirected to mobile site */
				setCookie("MobileOptOut", "", -1);
				redirectToMobSite();
			}
	}		
}
