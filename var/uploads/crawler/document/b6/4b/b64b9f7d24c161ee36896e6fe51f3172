<script type="text/javascript" language="javascript">
	// HTML CLICK TRACKING CALL SAMPLE
	function AddClickTracking(trackname, contenttype, campaign, channel) {
		MATTEL.tracker.Tracker.enableShortCuts();
		Tracker.name = trackname;
		Tracker.campaign = campaign;
		Tracker.channel = channel;
		Tracker.contenttype = contenttype;
		Tracker.action = "Click";
		Tracker.track();	        
	}
	
	function TrackAndRedirect(trackname, contenttype, campaign, channel, redirectUrl) {
		AddClickTracking(trackname, contenttype, campaign, channel);
		window.location = redirectUrl;
	}
</script>

