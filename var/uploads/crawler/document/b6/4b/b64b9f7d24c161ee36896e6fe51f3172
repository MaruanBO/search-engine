function get_random() {
  var rand = Math.floor((Math.random()*55)+1);
  
  if ($.inArray(rand, disp_nodes) == -1) {

    disp_nodes.push(rand);
    return true;
  }
  else {
    get_random();
  }
}





$(document).ready(function() {



$('.hotspot').hover(

  function(){
  
 //   $('#' + $(this).attr('id') + '-over').show();
  },
  
  function() {
//  $('#' + $(this).attr('id') + '-over').hide();
  }
);

$('.hotspot, .brand-promos-item a').click(function() {

if ($(this).hasClass('hotspot')) {
  pre = 'Button_Ornament';
}
else {
  pre = 'Button_Tile';
}

MATTEL.tracker.Tracker.enableShortCuts();
Tracker.name = "MDN Home: None: " + pre + $(this).attr('data-track-name');
Tracker.campaign = "None";
Tracker.channel = "MDN Home";
Tracker.contenttype = "Button";
Tracker.action = "See The Toys";
Tracker.track();
  
});

var gamescontent = '';
var videoscontent = '';

for (i = 0; i < 9; i++) {
  get_random();
 
  preloader(gamesnodes[disp_nodes[i]].filepath);
  preloader(videosnodes[disp_nodes[i]].filepath);


  if ((i == 0) || ((i % 3) == 0)) {
    gamescontent= gamescontent + '<div class="games-videos-row">';
    videoscontent= videoscontent+ '<div class="games-videos-row">';

 }

				
	gamescontent = gamescontent + '<div class="games-videos-row-item"><a href="' + gamesnodes[disp_nodes[i]].dst +'"><img width="135" height="85" class="imagecache imagecache-wMS" ' +
        'title="" alt="" src="' + gamesnodes[disp_nodes[i]].filepath +'" '  +
	'onclick="wtClickTrack(\'MDN Home: None: '+ gamesnodes[disp_nodes[i]].filename + '\',\'None\', \'MDN Home\', ' +   
        '\'Thumbnail\',\'Click\')"><span  class="promo-title-txt">' + gamesnodes[disp_nodes[i]].title + '</span>' + 
        '</a></div>';


       videoscontent = videoscontent+ '<div class="games-videos-row-item"><a href="' + videosnodes[disp_nodes[i]].dst +'"><img width="135" height="85" class="imagecache imagecache-wMS" ' +
        'title="" alt="" src="' + videosnodes[disp_nodes[i]].filepath +'" '  +
	'onclick="wtClickTrack(\'MDN Home: None: '+ videosnodes[disp_nodes[i]].filename + '\',\'None\', \'MDN Home\', ' +   
        '\'Thumbnail\',\'Click\')"><span  class="promo-title-txt">' + videosnodes[disp_nodes[i]].title + '</span>' + 
        '</a></div>';



  if ( ((i % 3) == 2) || (i  == 2)) {
    gamescontent = gamescontent + ' <div class="clearfix"></div></div>';
    videoscontent = videoscontent + ' <div class="clearfix"></div></div>';
  }
 

  
}

$('#videos-content').html(videoscontent);
$('#games-content').html(gamescontent);


});

</script>





