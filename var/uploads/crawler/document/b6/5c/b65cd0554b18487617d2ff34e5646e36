	(function($) {
		$('.search-holder a').click(function(event) {
			event.preventDefault();
			var $searchWrap = $('.search-wrapp');

			$searchWrap.toggleClass('search-active');
			$searchWrap.find('input').focus();
		});

		$('.search-wrapp').find('input').blur(function(event) {
			$('.search-wrapp').removeClass('search-active');
		});

		$('.www_FlowSlider_com-branding').remove();
	})(jQuery);
</script>


                                                                                        
