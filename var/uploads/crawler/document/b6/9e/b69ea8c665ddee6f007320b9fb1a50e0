<script type="text/javascript">




function is_android(){
	var ua = navigator.userAgent.toLowerCase();
	var isAndroid = ua.indexOf("android") > -1;
	return isAndroid;
}

function resizelayer() {
	var window_width = $(window).width();
	var window_height = $(window).height();

	// landscape
	if($(window).width()>$(window).height()) {
		$("#layer_droidoptimizer").css('width',window_height*0.95*1.5);
		$("#layer_droidoptimizer").css('height',window_height*0.95);
		var layer_width = $("#layer_droidoptimizer").width();
		$("#layer_droidoptimizer").css('font-size',layer_width/100);
	}
	
	// portrait
	if($(window).width()<$(window).height()) {
		$("#layer_droidoptimizer").css('width',window_width*0.95);
		$("#layer_droidoptimizer").css('height',window_width*0.95/15*10);
		var layer_width = $("#layer_droidoptimizer").width();
		$("#layer_droidoptimizer").css('font-size',layer_width/100);
	}
}

$(document).ready(function(){
	if(is_android()){
		if(typeof localStorage != 'undefined'){
			// local storage supported
			if(!localStorage.droidoptimizerLayerShown){
				resizelayer();
				$(".do_close").click(function(){
						$("#layer_droidoptimizer").fadeOut('2000');
				});                
				$(window).resize(resizelayer);    
				localStorage.droidoptimizerLayerShown = true;
				$('#layer_droidoptimizer').show();
			}
		}
	}
});


</script>    


