        <script type="text/javascript">
          jQuery(document).ready(function($) {
            $('.languages li a').click(function() {
              var locale = $(this).attr('data-locale');
              var kookLang = locale;
              var tempKookLang = kookLang.slice(3,5);
              var kookDate = new Date();

              kookLang = kookLang.slice(0,2)+"_"+tempKookLang.toUpperCase();
              kookDate.setTime(kookDate.getTime()+(14*24*60*60*1000));

              acom.createCookie({name: 'adotcomLang', value: kookLang, daysToExpire: kookDate.toGMTString(), domain: 'acrobat.com'});

              if ( !! locale && locale != '') {
                document.location = document.location.href.replace(/([a-z]{2}_[a-z]{2})/i, locale);
              }

              return false;
            });
          });
        </script>
