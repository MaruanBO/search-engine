mboxCreate("Acom_Global","product_name\x3Ddocumentsolutions","acom_language\x3Den");
// hide default mbox content; hiding just the outer div doesn't work, it will be shown again a bit later
var defaultDivs = document.getElementById("content-document-solutions-en_us-mboxIpar-start").getElementsByTagName("div");
for(var i=0; i<defaultDivs.length; i++) {
    defaultDivs[i].style.visibility="hidden";
}
// show default mbox content if still there
function showDefaultAcom_Global() {
    var defaultContent = document.getElementById("content-document-solutions-en_us-mboxIpar-start");
    if (defaultContent) {
        CQ_Analytics.TestTarget.ignoreNextUpdate('Acom_Global');
        var defaultDivs = defaultContent.getElementsByTagName("div");
        for(var i=0; i<defaultDivs.length; i++) {
            defaultDivs[i].style.visibility="visible";
        }
    }
}
// wait 2s for campaign content to arrive before showing default content
setTimeout("showDefaultAcom_Global()", 100);
</script>


