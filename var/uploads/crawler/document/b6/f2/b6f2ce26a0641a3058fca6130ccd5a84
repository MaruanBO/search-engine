<script type="text/javascript">
mboxCreate("Acom_Homepage_Marquee","acom_language\x3Den","product_name\x3Ddocumentsolutions");
// hide default mbox content; hiding just the outer div doesn't work, it will be shown again a bit later
var defaultDivs = document.getElementById("content-document-solutions-en_us-headContentPar-start").getElementsByTagName("div");
for(var i=0; i<defaultDivs.length; i++) {
    defaultDivs[i].style.visibility="hidden";
}
// show default mbox content if still there
function showDefaultAcom_Homepage_Marquee() {
    var defaultContent = document.getElementById("content-document-solutions-en_us-headContentPar-start");
    if (defaultContent) {
        CQ_Analytics.TestTarget.ignoreNextUpdate('Acom_Homepage_Marquee');
        var defaultDivs = defaultContent.getElementsByTagName("div");
        for(var i=0; i<defaultDivs.length; i++) {
            defaultDivs[i].style.visibility="visible";
        }
    }
}
// wait 2s for campaign content to arrive before showing default content
setTimeout("showDefaultAcom_Homepage_Marquee()", 100);
</script>


