// show default mbox content if still there
function showDefaultAcom_Header() {
    var defaultContent = document.getElementById("content-document-solutions-en_us-topNavIpar-start");
    if (defaultContent) {
        CQ_Analytics.TestTarget.ignoreNextUpdate('Acom_Header');
        var defaultDivs = defaultContent.getElementsByTagName("div");
        for(var i=0; i<defaultDivs.length; i++) {
            defaultDivs[i].style.visibility="visible";
        }
    }
}
