<script type="text/javascript">
mboxCreate("Acom_Header","acom_language\x3Den","product_name\x3Ddocumentsolutions");
// hide default mbox content; hiding just the outer div doesn't work, it will be shown again a bit later
var defaultDivs = document.getElementById("content-document-solutions-en_us-topNavIpar-start").getElementsByTagName("div");
for(var i=0; i<defaultDivs.length; i++) {
    defaultDivs[i].style.visibility="hidden";
