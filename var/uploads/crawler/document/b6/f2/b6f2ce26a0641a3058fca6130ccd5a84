  <script>var alternateLanguages = {"nb_no":"/nb_no.html","ja_jp":"/ja_jp.html","eu_es":"/eu_es.html","nl_nl":"/nl_nl.html","uk_ua":"/uk_ua.html","ro_ro":"/ro_ro.html","en_gb":"/en_gb.html","pt_br":"/pt_br.html","it_it":"/it_it.html","zh_tw":"/zh_tw.html","es_es":"/es_es.html","ru_ru":"/ru_ru.html","zh_cn":"/zh_cn.html","fr_fr":"/fr_fr.html","sl_si":"/sl_si.html","sk_sk":"/sk_sk.html","hr_hr":"/hr_hr.html","cs_cz":"/cs_cz.html","tr_tr":"/tr_tr.html","hu_hu":"/hu_hu.html","ca_es":"/ca_es.html","fi_fi":"/fi_fi.html","sv_se":"/sv_se.html","da_dk":"/da_dk.html","de_de":"/de_de.html","pl_pl":"/pl_pl.html","ko_kr":"/ko_kr.html"};</script>
  <script type="text/javascript">
    var acom = acom || {};

    /**
     * Creates a cookie.
     *
     * @param {Object} with keys: name, value, daysToExpire, domain.
     */
    acom.createCookie = function (cookie) {
      var date,
          expires = '',
          domain = '';

      if (cookie) {
        if (cookie.daysToExpire) {
          date = new Date();
          date.setTime(date.getTime() + (cookie.daysToExpire * 24 * 60 * 60 * 1000));
          expires = '; expires=' + date.toGMTString();
        }

        if (cookie.domain) {
          domain = '; domain=' + cookie.domain;
        }

        document.cookie = cookie.name + '=' + cookie.value + expires + domain + '; path=/';
      }
    };

    /**
     * Reads a cookie.
     *
     * @param {String} name The cookie name to retrieve.
     * @return {String} The cookie value.
     */
    acom.readCookie = function (name) {
      name += '=';
      for (var ca = document.cookie.split(/;\s*/), i = ca.length - 1; i >= 0; i--)
          if (!ca[i].indexOf(name))
              return ca[i].replace(name, '');
    };

    /**
     * Erases a cookie.
     *
     * @param {String} name The cookie name to erase.
     */
    acom.eraseCookie = function (name) {
      this.createCookie({name: name, value: '', daysToExpire: -1});
    };

    /**
     * Redirects user to proper locale version of the current page.
     *
     * Only forwards in production environment. Tries to read the
     * 'adotcomLang' cookie, and falls back to the browser's language.
     *
     * Handles acrobat.com cookies such as "en_us" as well as webapp
     * cookies such as "en".
     *
     * Checks to make sure the referrer URL and the forwarding URL
     * are not the same.
     */
    (function(){
      var url,
          isProd,
          languageCookie,
          redirectURL,
          referrerLocation,
          currentItem;

      if (typeof alternateLanguages !== 'undefined') {
        url = location.href;
        isProd = url.indexOf(".acrobat.com") !== -1 && url.indexOf('author.acrobat.com') === -1;

        if (isProd) {
          languageCookie = acom.readCookie('adotcomLang') || navigator.language || navigator.userLanguage || navigator.browserLanguage;

          if (languageCookie) {
            languageCookie = languageCookie.toLowerCase().replace(/-/gi, '_');

            if (languageCookie !== 'en_us' && languageCookie !== 'en') {
              redirectURL = alternateLanguages[languageCookie];

              /* redirectUrl not found. Test for legacy language code such as "en". */
              if (!redirectURL && languageCookie.length === 2) {
                for (currentItem in alternateLanguages) {
                  if (currentItem.indexOf(languageCookie) === 0) {
                    redirectURL = alternateLanguages[currentItem];
                    break;
                  }
                }
              }

              if (redirectURL && document.referrer.indexOf(redirectURL) === -1) {
                location.href = redirectURL + location.search + location.hash;
              }
            }
          }
        }
      }
    })();
  </script>

