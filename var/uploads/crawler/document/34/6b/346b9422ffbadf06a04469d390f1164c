<script type="text/javascript">
function build_oauth_lb(type) {

	var unity_action_href = 'http://profile.uproxx.com/signup';
	
	if(!window.thred_back){
		var thred_back = window.location.href;
	}
	
	if (type == 'fb') {
		build_lb(unity_action_href + "?action=fbauth&social=facebook&back_url=" + thred_back, 650, 290);
	}
	if (type == 'tw') {
		build_lb(unity_action_href + "?action=twauth&social=twitter&back_url=" + thred_back, 650, 290);
	}
	return false;
}
</script>
