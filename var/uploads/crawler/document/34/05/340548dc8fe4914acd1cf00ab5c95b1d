<script language="JavaScript" type="text/javascript"> 	
	bt_chatAvailable = false;//set this to false to prevent null ponter errors if bold chat doesn't respond in time.
	bc_protocol = (('https:' == document.location.protocol) ? 'https:' : 'http:');
	//this line changes per site
	bc_url = "//cbi.boldchat.com/aid/4921734055461953341/bc.cbhs?wdid=45588957257205593&amp;rdid=1397837999045566081";
	bc_noChat = "Ask us a Question";
	var boldchatRequest = $.ajax({
		type: "GET",
		url: bc_protocol+bc_url,
		dataType: "script",
		timeout: 2000,
		complete: function(){
					if(bt_chatAvailable == true){
						//$(".boldChatText").html("Chat Now");
					}
					else{
						//show chat unavailable text
						$(".boldChatText").html(bc_noChat);
					}
					
				  },
		error: function(){
					//show chat not available
					$(".boldChatText").html(bc_noChat);
					bt_chatAvailable = false;//set this to false to prevent null ponter errors if bold chat doesn't respond in time.
				}
	});
</script>
