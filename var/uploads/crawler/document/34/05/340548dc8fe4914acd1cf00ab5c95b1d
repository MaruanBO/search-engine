/*LSS 10/7/2014 added search split test*/

/*Appends to body... pass content to it*/
function randomIntFromInterval(min,max)
{
    return Math.floor(Math.random()*(max-min+1)+min);
}
var siteIdentifier = "NWFC";  //Fry site acronym
var currentExpContent = "searchsplit"; //name of creative
var currentExpContentExpiration = 360; //days before showing again
var Cookie = YAHOO.util.Cookie;
// STOP EDIT
var globalCookieExpiration = 1095;
var cookieName = siteIdentifier + "ExpCookie"; //official cookie name
var today = new Date();
var expExpirationDate = new Date();
var cookieExpirationDate = new Date();
expExpirationDate.setDate(expExpirationDate.getDate()+currentExpContentExpiration); //adjust expiration into the future
cookieExpirationDate.setDate(expExpirationDate.getDate()+globalCookieExpiration);
var ExpCookie = YAHOO.util.Cookie.getSub(cookieName,currentExpContent);
var abSplitPath = -1;
if(!ExpCookie){
	abSplitPath = randomIntFromInterval(0,1);
	YAHOO.util.Cookie.setSub(cookieName, currentExpContent, abSplitPath ,{path: "/", expires: new Date(cookieExpirationDate)});
}else{
    
	abSplitPath = YAHOO.util.Cookie.getSub(cookieName,currentExpContent);
}
if(abSplitPath == 0){
 
	$(document).ready(function() {(function abSplitDelay(){setTimeout(function(){var ab=s_gi('weddingshopnwf'); ab.linkTrackVars='eVar4'; ab.eVar4='ABSplit-Control Advanced Search Split 10_7_2014'; ab.tl(true,'o','ABSplit-Control Advanced Search Split 10_7_2014');}, 500); })();});

}else if(abSplitPath == 1){

		$('#strSearch').css({'width':'325px','height':'20px','font-size':'16px'});
		$('#headerContent').css({'margin-top':'4px'}); 
		$(document).ready(function() {(function abSplitDelay(){setTimeout(function(){var ab=s_gi('weddingshopnwf'); ab.linkTrackVars='eVar4'; ab.eVar4='ABSplit Advanced Search Split 10_7_2014'; ab.tl(true,'o','ABSplit Advanced Search Split 10_7_2014');}, 500); })();});
}
/*LSS 10/7/2014 end search split test*/
</script>
