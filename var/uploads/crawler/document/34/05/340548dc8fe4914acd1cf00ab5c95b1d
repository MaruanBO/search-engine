		/********************************************/
		/****************header slider***************/
		/********************************************/
		$(function() {
			var slideImgs = $('#slideShow .slideImg');
			var verification = false;
		    slideImgs.each(function(){
			    if($(this).has("img").length >= 1){
			    	verification = true;
			    }else{
			    	verification = false;
				}
				if(!verification){
					if($(this).has("a").length >= 1){
				    	verification = true;    
					}
					if(!verification){
				    	$(this).remove();
					}
				}
			});

		    
			if(slideImgs.length > 1){
				$('#slideShow').cycle({
			        fx: 'custom',
			        cssBefore: { top: 80 },
			        animIn:  { top: 0 },
			        animOut: { top: -80 },
			        exTimeout:6000
			    });
			}
		});
		</script>
