    <div id="SpecialIssues" class="MainSidebar Home ">
      <h2><a class="issueLink" href="http://www.eastbayexpress.com/oakland/holiday-guide-2014/SpecialReports?oid=4132149">Special Issues</a></h2>
      
      <div id="SpecialIssuesSlideshow">
        
          <a class="issueLink" href="http://www.eastbayexpress.com/oakland/holiday-guide-2014/SpecialReports?oid=4132149">
            <img src="/imager/b/sidecar/4132410/13a7/ss_COVER_3707.jpg" 
                 alt="Holiday Guide 2014" 
                 title="Holiday Guide 2014"
                 width="300" 
                 height="200" />
          </a>
        
      </div><!-- #componentSlideshow -->
      
  
  
  
  

  
  
  

  
  
  

  
  
  

      
      <div class="sliderNavWrapper">
        
          <div class="rotator-thumb-wrap">
            
            <div class="rotator-thumb-slider" id="SpecialIssuesThumbSlider"></div>
            
              <a href="javascript://rotator-thumbnail" id="Thumb1" class="rotator-thumbnail">&bull;</a>
            
              <a href="javascript://rotator-thumbnail" id="Thumb2" class="rotator-thumbnail">&bull;</a>
            
              <a href="javascript://rotator-thumbnail" id="Thumb3" class="rotator-thumbnail">&bull;</a>
            
              <a href="javascript://rotator-thumbnail" id="Thumb4" class="rotator-thumbnail">&bull;</a>
            
          </div>
        
      </div>
      <div id="SpecialIssuesSlideshowCaption">
          <h3 class="headline"><a href="http://www.eastbayexpress.com/oakland/holiday-guide-2014/SpecialReports?oid=4132149">Holiday Guide 2014</a></h3>
    <h4 class="subhead">Your guide to the best gifts, gadgets, toys, music, and books of the year; plus holiday food and drink, snow sports, winter getaways, and more.</h4>
    <p class="byline">By Robert Gammon,David Downs,Sam Levin,Ellen Cushing,Luke Tsai,Sam Lefebvre,Sarah Burke,Anna Pulley,Bert Johnson,AJ Kiyoizumi,Nastia Voynovskaya</p>
        </div>
    </div>
    
      <script type="text/javascript">
      dnLoadScript("/foundation/scripts/slideshow.js");

      Event.observe(document, "dom:loaded", function () {
        var slides = [{
  src: "\/imager\/b\/sidecar\/4132410\/13a7\/ss_COVER_3707.jpg",
  width: 300,
  height: 200,
  href: "http://www.eastbayexpress.com/oakland/holiday-guide-2014/SpecialReports?oid=4132149",
  caption: "<h3 class=\"headline\"><a href=\"http:\/\/www.eastbayexpress.com\/oakland\/holiday-guide-2014\/SpecialReports?oid=4132149\">Holiday Guide 2014<\/a><\/h3>\r\n    <h4 class=\"subhead\">Your guide to the best gifts, gadgets, toys, music, and books of the year; plus holiday food and drink, snow sports, winter getaways, and more.<\/h4>\r\n    <p class=\"byline\">By Robert Gammon,David Downs,Sam Levin,Ellen Cushing,Luke Tsai,Sam Lefebvre,Sarah Burke,Anna Pulley,Bert Johnson,AJ Kiyoizumi,Nastia Voynovskaya<\/p>"
  }, {
  src: "\/imager\/b\/sidecar\/4085482\/3a88\/CV_TASTE_ss.jpg",
  width: 300,
  height: 200,
  href: "http://www.eastbayexpress.com/oakland/taste-fall-2014/SpecialReports?oid=4085480",
  caption: "<h3 class=\"headline\"><a href=\"http:\/\/www.eastbayexpress.com\/oakland\/taste-fall-2014\/SpecialReports?oid=4085480\">Taste, Fall 2014<\/a><\/h3>\r\n    <h4 class=\"subhead\">Everything you need to know about dining in and out in the East Bay.<\/h4>\r\n    <p class=\"byline\">By Sam Levin,Luke Tsai,Sam Lefebvre,Sarah Burke,Anna Pulley<\/p>"
  }, {
  src: "\/imager\/b\/sidecar\/4054634\/a09f\/ss_cpver_3647.jpg",
  width: 300,
  height: 200,
  href: "http://www.eastbayexpress.com/oakland/fall-arts-2014/SpecialReports?oid=4054530",
  caption: "<h3 class=\"headline\"><a href=\"http:\/\/www.eastbayexpress.com\/oakland\/fall-arts-2014\/SpecialReports?oid=4054530\">Fall Arts 2014<\/a><\/h3>\r\n    <h4 class=\"subhead\">Our annual guide to arts and culture.<\/h4>\r\n    <p class=\"byline\">By Sam Levin,Sam Lefebvre,Sarah Burke,Anna Pulley<\/p>"
  }, {
  src: "\/imager\/b\/sidecar\/3950956\/cbd6\/ss_cover_3636.jpg",
  width: 300,
  height: 200,
  href: "http://www.eastbayexpress.com/oakland/local-economy-2014/SpecialReports?oid=3950865",
  caption: "<h3 class=\"headline\"><a href=\"http:\/\/www.eastbayexpress.com\/oakland\/local-economy-2014\/SpecialReports?oid=3950865\">Local Economy 2014<\/a><\/h3>\r\n    <h4 class=\"subhead\">Everything you need to know about buying local, artist collectives, local hacking movements, co-working spaces, and more.<\/h4>\r\n    <p class=\"byline\">By Robert Gammon,Sam Levin,Jean Tepperman,Madeleine Key,Julian Mark,Michael Berry,Julius Rea<\/p>"
  }];

        var SwitchSlide = function (element) {
          var slideshow,
              currentIndex,
              shiftIndex;

          slideshow = dnSlideshow.registry.SpecialIssuesSlideshow;
          currentIndex = slideshow.curSlide;
            
          if(!slideshow){ return false; }

          slideshow.pause();

          if(element.hasClass("nextSlide")){
            slideshow.next();
          } else if(element.hasClass("prevSlide")){
            slideshow.previous();
          } else {
            shiftIndex = element.attr("id").replace("Thumb","") -1;
            slideshow.jump(shiftIndex - currentIndex);
          }
          return true;
        };

        var MoveThumb = function (moveToIndex){
          var thumbWidth = 8, 
              thumbSpacing = 4, 
              newThumbPos;

          //move thumbnail
          moveToIndex = moveToIndex + 1;
          newThumbPos = moveToIndex * thumbWidth - thumbWidth + (moveToIndex - 1) * thumbSpacing;
          new Effect.Morph('SpecialIssuesThumbSlider', {style:"left:+" + newThumbPos + "px;", duration: 0.25});
        };

        var CaptionCallback = function (){
          var slideshow, caption, captionId, captionElem;

          slideshow = dnSlideshow.registry.SpecialIssuesSlideshow;
          MoveThumb(slideshow.curSlide); 

          caption = slideshow.slides[slideshow.curSlide].caption;
          if (!caption){ caption = ""; }

          captionId = slideshow.config.captionId;
          if (!captionId){ return; }

          captionElem = jQuery("#"+captionId);
          if (!captionElem){ return; }
          captionElem.html(caption);
        };

        new dnSlideshow(
          "SpecialIssuesSlideshow",
          {
            bezelImage: "/foundation/images/slideshow-bezel.png",
            bezelOffset: "-100%",
            transition: "slide",
            delay: 7,
            width: 300,
            height: 200,
            preloadWindowSize:4,
            alwaysAnimate: true,
            captionId: "SpecialIssuesSlideshowCaption",
            captionCallback: CaptionCallback
          },
          slides
        ).start();

        //
        jQuery("#SpecialIssues div.rotator-thumb-wrap a").click(function(event){
          event.preventDefault();
          SwitchSlide(jQuery(this));
        });

        //
        jQuery("#SpecialIssues a.prevSlide, #SpecialIssues a.nextSlide").click(function (element) {
          event.preventDefault();
          SwitchSlide(jQuery(this));
        });

        // Swipe handlers for touch. Requires TouchSwipe
        (new Foundation.Script("/foundation/scripts/jquery.touchSwipe.min.js", true)).then(function () {
          // Have to disable the <a> around the image manually
          jQuery('img[id^="dnSlideshow:SpecialIssuesSlideshow:slide:image:"]').parent('a').click(function(someEvent){
            someEvent.preventDefault();
          });
          // Handle the image link via the tap event in touchSwipe
          jQuery('div[id^="dnSlideshow:SpecialIssuesSlideshow:slide:"]').swipe( {
            tap:function(){
              window.location = jQuery(this).find('a').attr('href');
            },
            swipeLeft:function(){ 
              dnSlideshow.registry.SpecialIssuesSlideshow.pause();
              dnSlideshow.registry.SpecialIssuesSlideshow.next(); 
            },
            swipeRight:function(){ 
              dnSlideshow.registry.SpecialIssuesSlideshow.pause();
              dnSlideshow.registry.SpecialIssuesSlideshow.previous(); 
            },
            excludedElements:[]
          });
          // For swipe in the Caption area
          jQuery('#SpecialIssuesSlideshowCaption').swipe( {
            swipeLeft:function(){ 
              dnSlideshow.registry.SpecialIssuesSlideshow.pause();
              dnSlideshow.registry.SpecialIssuesSlideshow.next(); 
            },
            swipeRight:function(){ 
              dnSlideshow.registry.SpecialIssuesSlideshow.pause();
              dnSlideshow.registry.SpecialIssuesSlideshow.previous(); 
            },
            excludedElements:[]
          });
        });

        
      });
      </script>
    
    <!-- End #SpecialIssues -->
  





