<script>
function lhContains(a, obj) {
var i = a.length;
while (i--) {
if (a[i] === obj) {
return true;
}
}
return false;
}
function lhGetNumsWithoutActive(){
var elements = [];
var prevElements = [];
jQuery(".local_happenings_container .swap.active .local_happenings_row").each(function (){
prevElements.push(jQuery(this).attr("num"));
});
jQuery(".local_happenings_container .local_happenings_block .local_happenings_row").each(function (){
if(!lhContains(prevElements, jQuery(this).attr("num"))){
elements.push(jQuery(this).attr("num"));
}
});
var array = [];
var randNums = [];
var i = 0;
while(i < 4){
var randNumber = Math.floor(Math.random() * elements.length);
if(!lhContains(array, randNumber) && elements.length > 3){
array[i] = randNumber;
randNums[i] = elements[array[i]];
i++;
}
if(elements.length < 4){
array[i] = i;
randNums[i] = elements[array[i]];
i++;
}
}
return randNums;
}
function lhShowNext(){
var nums = lhGetNumsWithoutActive();
jQuery(".local_happenings_container .swap.hidden").empty();
jQuery(".local_happenings_container .swap").not(".active").addClass("in_progress");
for(var i = 0; i < 4; i++){
var elem = jQuery(".local_happenings_container .local_happenings_block [num=" + nums[i] + "]").clone();
jQuery(".local_happenings_container .swap").not(".active").append(elem);
}
jQuery(".local_happenings_container .swap").siblings(".active").addClass("hidden").removeClass("active");
jQuery(".local_happenings_container .swap").siblings(".in_progress").removeClass("in_progress").addClass("active");
jQuery(".local_happenings_container .swap").siblings(".active").removeClass("hidden");
jQuery(".local_happenings_container .swap.active").fadeIn(2000);
jQuery(".local_happenings_container .swap.hidden").fadeOut(3000);
jQuery(".swap.active .happenings_title").each(function (){
var originalText = jQuery(this).html();
var count = 0;
while(jQuery(this).height() > 40){
var text = originalText.substring(0, originalText.length - count) + "...";
jQuery(this).html(text);
count++;
}
});
setTimeout(lhShowNext, 20000);
}
jQuery(document).ready(function(){
setTimeout(lhShowNext);
});
</script>
</div>
</div>
    <script type="text/javascript">
      jQuery.noConflict();
