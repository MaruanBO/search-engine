<script type="text/javascript">
<!-- <![CDATA[

ninja=false;if(typeof(ninjaLog)!="undefined"){ninja=true;};

$(function(){
	simple_slideshow0sliderSizeAdjust();
});

effectsimple_slideshow0 = "scrollHorz";

function simple_slideshow0sliderSizeAdjust(){

	if(ninja){ninjaLog.Update({text:"Slider simple_slideshow-0 Initial Transition is: scrollHorz"});}

	jQuery('#simple_slideshow-0 .loading').hide();
	
	jQuery('#simple_slideshow-0 .item-list').show();

	if(((jQuery('#simple_slideshow-0').width()<301) || ("horizontal" == "vertical")) && ("horizontal" != "horizontal")){

		jQuery("#simple_slideshow-0").removeClass("horizontal").addClass("vertical");
		jQuery("#simple_slideshow-0").height(jQuery("#simple_slideshow-0").find(".item").height()*2+65+(12*2));
		jQuery("#simple_slideshow-0").find(".item-list").height(jQuery("#simple_slideshow-0").find(".item").height()*2+(12*2));


		jQuery(jQuery("#simple_slideshow-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<2){
    				jQuery(this).height("100%");
			}
		});

		if(effectsimple_slideshow0 == "scrollHorz"){ effectsimple_slideshow0 = "scrollVert";}

		if(ninja){
			ninjaLog.Update({text:"Slider is Vertical",style:""});
			ninjaLog.Update({text:"Using effect: "+effectsimple_slideshow0});
		};
	}

	if(effectsimple_slideshow0=="scrollHorz"){

		var sliderAreaWidth = jQuery("#simple_slideshow-0").width();
		var columnAdjust = 38;

		if(typeof(ninjaLog)!="undefined"){
			ninjaLog.Update({text:"Slider is Horizontal",style:""});
			ninjaLog.Update({text:"area width is: "+sliderAreaWidth});
			ninjaLog.Update({text:"item width is: "+jQuery("#simple_slideshow-0").find(".item").width()});
			ninjaLog.Update({text:"columnAdjust is: "+columnAdjust});
		}
	}

		if(2>1){


			if((sliderAreaWidth-55-(columnAdjust*2)<(jQuery("#simple_slideshow-0").find(".item").width())*2)||(true)){
				if(ninja){ninjaLog.Update({text:"Width of items require adjustment",style:""});};
				jQuery("#simple_slideshow-0").find(".item").width(Math.ceil((sliderAreaWidth-55-(columnAdjust*2))/2));
			}
		}else{
			if(typeof(ninjaLog)!="undefined"){
				ninjaLog.Update({text:"Adjusting for a single column: 70px"});
			}
			jQuery("#simple_slideshow-0").find(".item").width(sliderAreaWidth-70);
		}

		jQuery(jQuery("#simple_slideshow-0").find(".group")).each(function(){
   			if(jQuery(this).find(".item").length<2){
    				jQuery(this).width("100%");
			}
		});

		if(jQuery.browser.msie && Number(jQuery.browser.version)<7){
//alert("#"+jQuery(jQuery("#simple_slideshow-0").parents(".tncms-region")[0]).attr("id")+" width: "+jQuery(jQuery("#simple_slideshow-0").parents(".tncms-region")[0]).width());

			//jQuery("#simple_slideshow-0").find(".group .item .image").hide();
			jQuery("#simple_slideshow-0").find(".group .item .text").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#simple_slideshow-0").find(".group .item .text .description").css({"overflow":"auto","height":"auto","width":"auto","display":"block"});
			jQuery("#simple_slideshow-0").width(jQuery("#simple_slideshow-0").parent().width());

			if(jQuery("#simple_slideshow-0").hasClass("showcase")){
				jQuery('#simple_slideshow-0').find(".group").css({"padding":"0px"});

				var itemWidth = jQuery("#simple_slideshow-0").find(".item").width()-((2) * 10);

				var groupWidth = jQuery('#simple_slideshow-0').find(".group").width();
var itemWidth = (groupWidth/2)-50;
jQuery('#simple_slideshow-0').find(".item").css({"width":itemWidth});
			}else if(jQuery("#simple_slideshow-0").hasClass("playlist")){
				jQuery('#simple_slideshow-0').find(".group").css({"padding":"0px"});

				var groupWidth = jQuery('#simple_slideshow-0').find(".group").width();
				var itemWidth = (groupWidth/2)-40;

				jQuery('#simple_slideshow-0').find(".item").css({"height":"154px","width":itemWidth});
				jQuery('#simple_slideshow-0').find(".item").find(".image").css({"width":itemWidth});

			}
		}

		


	jQuery('#simple_slideshow-0 .prev').show();
	jQuery('#simple_slideshow-0 .next').show();


        // BEFORE TRANSITION
        function onBeforesimple_slideshow0(curr, next, opts){
            $(curr).removeClass("active");
    		
        }

	// AFTER TRANSITION
	function onAftersimple_slideshow0(curr, next, opts) {
            

            $(next).addClass("active");

            $('#simple_slideshow-0 .slideCounter').html((parseInt(opts.currSlide)+1)+' of '+parseInt(opts.slideCount)); // ouput current 1 of 4

            

            $(next).find(".image img").animate({'opacity':1},500);


            
	}



	$('#simple_slideshow-0 .item-list').cycle({
		fx: effectsimple_slideshow0,
		sync: true,
		fit: 1,
		
		timeout: 0,
                before: onBeforesimple_slideshow0,
		after:	onAftersimple_slideshow0,
        pause:true,
		pager:'#simple_slideshow-0 .nav',
		prev:'#simple_slideshow-0 .prev',
		next:'#simple_slideshow-0 .next'
	});
        

        

}



// ]]> -->
</script>



