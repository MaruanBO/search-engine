            }
            var $aLink = jQuery('#6a5eb40f-dac1-4a7a-9e7d-faac2d530839').find('a[target="_blank"]');
            if($aLink && $aLink.length > 0 && typeof $aLink.newWindowLink === 'function') {
                $aLink.newWindowLink();
            };

            var $richtext = jQuery('#6a5eb40f-dac1-4a7a-9e7d-faac2d530839.hiddenRte');
