<script>
function updateNewsTicker(n){
            
    var cnt = 0;
    while (true){
        var newsItem1 = document.getElementById("newsTickerItem"+cnt);
        if (null == newsItem1){
            cnt --;
            break;
        }
        cnt ++;
    }
    
    //alert("n="+n);
    var idx = 0;
    var newVisibleIdx=0;
    while (true){
        var newsItem = document.getElementById("newsTickerItem"+idx);
        if (null == newsItem){
            //alert("newItem is null");
            break;
        }
        //alert(idx+":");
        if (newsItem.style.display == ''){ 
            //alert("visible");
            newVisibleIdx = parseInt(n)+idx;
            //alert("new visibleItem idx = "+newVisibleIdx);
            if (newVisibleIdx <0){
                break;
            }
            //alert("getting " + "newsTickerItem"+newVisibleIdx);
            var visibleItem = document.getElementById("newsTickerItem"+newVisibleIdx);
            if (null != visibleItem){
                visibleItem.style.display = '';
                newsItem.style.display = 'none';
            }
            break;
        }
        else{
            //alert("hidden");
        }
            
        idx ++;
    }
    document.getElementById('previouslinkid').style.display = '';
    document.getElementById('nextlinkid').style.display = '';
    if (0 == newVisibleIdx){
        document.getElementById('previouslinkid').style.display = 'none';
    }
    if (cnt == newVisibleIdx){
        document.getElementById('nextlinkid').style.display = 'none';
    }
    
}
</script>
 
</div>
</div></div>
