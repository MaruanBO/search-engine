							var sliderBar = $('.fly');
							var footer = $(".td-footer-wrap");
							var stickyTop = sliderBar.offset().top;
							
							$(window).scroll(function() {
								var windowTop = $(window).scrollTop();
								var delta = footer.offset().top - stickyTop - sliderBar.outerHeight();
								if (windowTop > stickyTop) {
									var fixPosition = 0;
									if (windowTop > delta + stickyTop) {
										fixPosition = windowTop - delta - stickyTop;
									}
									$('.fly').css({ 
										position: 'fixed',
										top: -fixPosition
									});
									
								} else {
									$('.fly').css('position','static');
								}
							});
