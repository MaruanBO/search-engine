<!--  Must Be placed after jquery is defined -->
<script type="text/javascript">
  $(document).ready(function() {
    var parentJQ = $("#fun-rental-cars");
    var droffOffJQ = $('#fun-dropOffSearchAirportString', parentJQ);
    droffOffJQ.val("");
    
    // Apply the rule that if the pick up location is a City or POI, then the drop off should be a Airport
    $("body").bind("AUTOCOMPLETE_ITEM_SELECTED", function(event, data /* {srcJQ: , event : , ui : } */) {
      // make sure this is the autocomplete that we are interested in
      if (data.srcJQ.attr("id") != "fun-pickUpSearchAirportString") {
    	  return;
      }
      var pickUpJQ = data.srcJQ;
      var acData = pickUpJQ.data("acdata");
    								
      if (acData.type === "AIRPORT"){
    	droffOffJQ.attr('add-ac', 'airport|city|poi');
        droffOffJQ.attr('placeholder', 'Airport, City, or Point of Interest');
      }
      if (acData.type === "CITY" || acData.type === "POI"){
    	droffOffJQ.attr('add-ac', 'airport');    
        droffOffJQ.attr('placeholder', 'Airport');
        
        // clear the drop off field if it is not an airport
		var dropOffData = droffOffJQ.data("acdata");
		if (dropOffData && dropOffData.type != "AIRPORT") {
	        droffOffJQ.val('');
	        droffOffJQ.focus();
		}
      }
    });
    
    $("#fun-different-dropoff", parentJQ).on("click",
      function(event) {
        $("#fun-same-as-pickup", parentJQ).hide();
        $("#rc-fun-do-group", parentJQ).slideDown(600, function() {
          $("#fun-same-as-pickup", parentJQ).show();
          $("#fun-different-dropoff", parentJQ).hide();
        });
      });
    
    $("#fun-same-as-pickup", parentJQ).on("click",
      function(event) {
        $("#fun-different-dropoff", parentJQ).show();
        $("#fun-same-as-pickup", parentJQ).hide();
        $("#rc-fun-do-group", parentJQ).slideUp(600);
        droffOffJQ.val("");
      });
    
    $("#fun-pickUpDate").val("");
  });  
</script>

