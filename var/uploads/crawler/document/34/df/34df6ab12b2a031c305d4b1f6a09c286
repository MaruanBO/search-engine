	script.async = true;
	script.type = "text/javascript";
	script.src = "/svcs/mkt/tag/pclntms.js";
	var node = document.getElementsByTagName("script")[0];
	node.parentNode.insertBefore(script, node);
	var notloaded = true;
	script.onload = script.onreadystatechange = function() {
   	if (notloaded && (!script.readyState || script.readyState == "loaded" || script.readyState == "complete")) {
      		notloaded = false;
      		script.onload = script.onreadystatechange = null;
      		if (typeof pclntms != "undefined") {
   				pclntms.config({'productId':'16', 'pageType':'hp', 'appCode': 'DESKTOP'});
      		}
   		}
 	};
  });
})();
