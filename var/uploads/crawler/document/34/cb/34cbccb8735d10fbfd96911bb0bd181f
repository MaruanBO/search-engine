		/**
		Plugin: trackCrossSiteNav
		Desc: track when visitor crosses domains
		@param d string comma delimited list of domains to eval
		@param r string ovverride default document.referrer to eval
		@param c string override default document.domain to eval
		@return object o
		**/
		s.trackCrossSiteNav = function(d,r,c) {
			var d=d||'';
			var m,x,t;
			var r=(r||(document.referrer||'')).toLowerCase();
			var rd=(r.split('/')[2]||'').toLowerCase();
			var c=(c||(document.domain||'')).toLowerCase();
			var o={'type':'','url':r};
			if ( r&&(r!=c) ) {
				d=d.split(',');
				t='external';
				for (x=0;x<d.length;x++) {
					if(d[x]=='')continue;
					m=new RegExp(d[x].replace(/\./g,'\\.')+'$','i');
					if(rd.match(m)){
						t='internal';
						break;
					}
				}
				o.type=t;
			}
			return o;
		} // end trackCrossSiteNav
