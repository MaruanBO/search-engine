						iframe.contentWindow.postMessage( {
							'msg_type': 'poll_size',
							'frame_id': 'wpcom-iframe-7c46f9e8a6d5bee73df0b671bdba71fc-54e657b7729b8'
						}, window.location.protocol + '//wpcomwidgets.com' );
