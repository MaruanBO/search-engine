  // initial call - set up sitepage and sponsor index
  function adTargetSitePage(PHP_sitepage) {
    targets = PHP_sitepage.split("|");
    if (targets.length>1) {
      var d = new Date();
      var n = d.getMilliseconds();
      ad_target_index = n % targets.length;
      return targets[ad_target_index];
    } else {
      return PHP_sitepage;
    }
  }

  // subsequent placement calls (sponsor logo, more on box, promo box)
  function adTargetSponsorAssets(position) {
    //Saving the more on block to a variable and removing it from the DOM
    var sponsor_more_on_boxes = document.getElementById('block-tdm_ad_targeting_sponsor-more_on_box').innerHTML;
    document.getElementById('block-tdm_ad_targeting_sponsor-more_on_box').remove();
    if (typeof sponsor_logo_rights === 'undefined') {
      var sponsor_logo_rights = '';
    }
    if (typeof sponsor_logo_contents === 'undefined') {
      var sponsor_logo_contents = '';
    }
    if (typeof sponsor_more_on_boxes === 'undefined') {
      var sponsor_more_on_boxes = '';
    }
    if (typeof sponsor_promo_boxes === 'undefined') {
      var sponsor_promo_boxes = '';
    }
    switch(position) {
      case "sponsor-logo-right": html = sponsor_logo_rights; break;
      case "sponsor-logo-content": html = sponsor_logo_contents; break;
      case "sponsor-more-on-box":
        //If the more on box has children (links to show) the image will be displayed as well
        if(document.getElementById('more_on_box_ul').childNodes.length > 0) {
          html = sponsor_more_on_boxes;
        }
        break;
      case "sponsor-promo-box": html = sponsor_promo_boxes; break;
      default: html = ""; break;
    }
    targets = html.split("|");
    if (targets.length>1) {
      return targets[ad_target_index];
    } else {
      return html;
    }
  }

  var ad_target_sitepage = adTargetSitePage('tdm/home');

