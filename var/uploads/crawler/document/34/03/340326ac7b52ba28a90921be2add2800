        }
      }
  }

  /* parsePositions(jsonObj)
   *
   * Determine which ads run based on frequency cap settings.
   */
  function parsePositions(jsonObj) {
    var list = "";
    var currentPosition = "";
    //console.log("JsonObj: "+jsonObj["Middle2"]);
    if (jsonObj["Middle2"]==1) {
      list = "Middle2";
    } else {
    for (var x in jsonObj) { //for each position
        if (x.indexOf("ros") != -1) {
            currentPosition = x;
        }
        else {
            currentPosition = "content-" + x;
        }
        if(typeof(Storage)!=="undefined") { //if the browser supports local storage
            if (parseInt(jsonObj[x]) > 0) { //and if frequency cap is a non-zero number then increment local storage
                //console.log(currentPosition);
                if (localStorage[currentPosition]) {
                    localStorage[currentPosition] = Number(localStorage[currentPosition]) + 1; //add 1 to the positions view count
                }
                else {
                    localStorage[currentPosition] = 1; //make new variable and set it to 1
                }
                /*ad to the list if not capped*/
                if (parseInt(localStorage[currentPosition]) <= jsonObj[x]) {
                    list = list + x + ",";
                }
            }
            else { //otherwise delete the local storage key/value if it exists and add position to the list
                //delete page-specific value
                if (localStorage.getItem(currentPosition) != null) {
                    localStorage.removeItem(currentPosition);
                }
                //delete run-of-site value if present
                if (localStorage.getItem("ros-" + x) != null) {
                    localStorage.removeItem("ros-" + x);
                }
              list = list + x + ",";
            }
        }
        else { //on older browsers fall back to cookies.
            if (parseInt(jsonObj[x]) > 0) { //and if frequency cap is a non-zero number then increment local storage
                //console.log(currentPosition);
                if (getCookie(currentPosition)) {
                    setCookie(currentPosition, Number(getCookie(currentPosition)) + 1); //add 1 to the positions view count
                }
                else {
                    setCookie(currentPosition,1); //make new variable and set it to 1
                }
                /*ad to the list if not capped*/
                if (parseInt(getCookie(currentPosition)) <= jsonObj[x]) {
                    list = list + x + ",";
                }
            }
            else { //otherwise delete the local storage key/value if it exists and add position to the list
                //delete page-specific value
                if (getCookie(currentPosition)) {
                    deleteCookie(currentPosition);
                }
                //delete run-of-site value if present
                if (getCookie("ros-" + x)) {
                    deleteCookie("ros-" + x);
                }
              list = list + x + ",";
            }
        }
    }//for loop
    list = list.slice(0,-1); //remove trailing comma
    }
    return(list);
  }//parsePositions


  /* setCookie / getCookie / deleteCookie
   *
   *  Helper functions to set/get cookies.
   */
  function setCookie(name,value,days,hours) {
      if (!days && days != 0) {
          days = 15;
      }
      if (!hours) {
          hours = 0;
      }
      var expDate = new Date();
      expDate.setDate(expDate.getDate() + days);
      expDate.setHours(expDate.getHours() + hours);
      var expires = expDate.toUTCString();
      document.cookie = name + "=" + value + ";expires=" + expires + "; path=/";
  }

  function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(";");
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==" ") c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
  }

  function deleteCookie(name) {
      setCookie(name,"",-1);
  }

  // execute OAS post-initialization callbacks if any
  if (typeof OAS_onexecute != "undefined") {
    for (var i = 0, j = OAS_onexecute.length; i != j; i++) {
       try { OAS_onexecute[i](); } catch(err) {}
    }
  }
  OAS_onexecute = { push : function (cb) { cb() } };
