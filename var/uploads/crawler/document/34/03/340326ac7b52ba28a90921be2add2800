  // Helper functions to set/get cookies.
  function setCookie(name,value,days,hours) {
      if (!days && days != 0) {
          days = 15;
      }
      if (!hours) {
          hours = 0;
      }
      var expDate = new Date();
      expDate.setDate(expDate.getDate() + days);
      expDate.setHours(expDate.getHours() + hours);
      var expires = expDate.toUTCString();
      document.cookie = name + "=" + value + ";expires=" + expires + "; path=/";
  }

  function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(";");
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==" ") c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
  }

  function WelcomdeAdClose(){
    var body = document.getElementsByTagName('body')[0];
    body.removeChild(document.getElementById('welcome_container'));
    body.style.overflow = "visible";
    body.style.height = "auto";
    setTimeout(function(){
      jwplayer().stop();
      cookie('jwplayer.mute', false);
      jwplayer().setMute(false);
      jwplayer().play();        
    },100);
  }

  // Setup Welcome Ad Variables
  var welcome_ad = tdmAdTargetingWelcome.tdm_welcome_ad_toggle;
  var welcome_ad_frequency = tdmAdTargetingWelcome.tdm_welcome_ad_frequency;

  // Set Welcome Ad Frequency Cap
  var wa_hours,wa_minutes;
  if (welcome_ad_frequency == "frequency_three_daily") {
      wa_hours = 8;
      wa_days = 0;
  } else if (welcome_ad_frequency == "frequency_twice_daily") {
      wa_hours = 12;
      wa_days = 0;
  } else {
      wa_hours = 0;
      wa_days = 1;
  }

  // Jump to the Welcome Ad
  if (!getCookie("tdm_welcome") && welcome_ad == 1) {
    cookie('jwplayer.mute', true);
    setCookie("tdm_welcome",document.URL,wa_days,wa_hours);

    var readyStateCheckInterval = setInterval(function() {
        if (document.getElementsByTagName('body').length > 0){
            var link = "/ads/welcome.html"
            var iframe = document.createElement('iframe');
            iframe.frameBorder=0;
            iframe.id="welcome_container";
