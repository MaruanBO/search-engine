		container.appendChild(containerInner0);
		var numPages = Math.ceil(playlist.length / 2);	
		if(numPages > 1 || (mobileFlag == true && playlist.length > 1)) {
			var leftPagerHolder = document.createElement("div");
			var leftPager = document.createElement("a");
			leftPager.id="plLEFT";
			leftPager.className="btn btn-large plLeft";						
			leftPager.innerHTML = "&nbsp&nbsp<img src=\"http://media.graytvinc.com/designimages/gdm_arrow_left.png\">";
			
			var rightPagerHolder = document.createElement("div");
			var rightPager = document.createElement("a");
			rightPager.id="plRIGHT";
			rightPager.className="btn btn-large plRight";
			rightPager.innerHTML = "<img src=\"http://media.graytvinc.com/designimages/gdm_arrow_right.png\">&nbsp&nbsp";
			
			container.appendChild(leftPager);
			container.appendChild(rightPager);
		}
				var videoSpace = 206;
		jQuery("#plLEFT").click(function(){
			var plWidth = jQuery("#playListMaster").width();
			var plMoveRemainder = plWidth % 206; 
			var plMove = plWidth - plMoveRemainder;	
			var leftStop = jQuery("#holder").css("margin-left");
			if(leftStop >= "0px"){
				jQuery("#holder").animate({marginLeft: '+=10px'}, 250 );
				jQuery("#holder").animate({marginLeft: '0px'}, 250 );
			}
			else
			{
				jQuery("#holder").animate({marginLeft: '+='+plMove}, 500 );
				if(leftStop >= "60px"){
					jQuery("#holder").animate({marginLeft: '0px'}, 250 );
				}
			}
		});
		jQuery("#plRIGHT").click(function(){
			var plWidth = jQuery("#playListMaster").width();
			var plMoveRemainder = plWidth % 206; 
			var plMove = plWidth - plMoveRemainder;	
			var plSlideWidth = jQuery("#playListSlide").width();
			var plRemainder = plWidth % videoSpace; 
			var plDisplayed = (plWidth - plRemainder) / videoSpace;
			var stopSpot = plSlideWidth - (plDisplayed*videoSpace);
			var rightStop = jQuery("#holder").css("margin-left");
			var rightStopReplace=rightStop.replace("px","");
			var rightStopInt=parseInt(rightStopReplace);
			var rightStopInvert=rightStopInt*-1;
				if(rightStopInvert >= stopSpot){
				jQuery("#holder").animate({marginLeft: '-=10px'}, 250 );
				jQuery("#holder").animate({marginLeft: '-'+stopSpot+'px'}, 250 );
			}
			else{
				jQuery("#holder").animate({marginLeft: '-='+plMove}, 500 );
				if(rightStopInt >= stopSpot){
					jQuery("#holder").animate({marginLeft: '0px'}, 250 );
				}
			}
		});
		function onImageLoad() {
			this.parentNode.style.display="inline-block";
		}		
		function onItemSelected() {
			pat=/anv-item(.+)/;
			var m=pat.exec(this.id);
			var index=parseInt(m[1]);
			playerInstance.play(playlist[index]);
			console.log(index);
		}	
	}
</script>

