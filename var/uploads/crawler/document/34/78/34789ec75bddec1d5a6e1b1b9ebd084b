	function AnvStoryPlaylist(divId,playlist,playerInstance) {
		console.log("@AnvPlaylist playlist:",playlist);
				var mobileFlag = false;
				var container=document.getElementById(divId);
		container.className="storyplaylist carousel slide well";
		var containerInner=document.createElement("div");
		containerInner.className="carousel-inner";
		
		var containerItem=document.createElement("div");
		containerItem.className="item active";
		var itemRow=document.createElement("div");
		itemRow.className="row-fluid";
		if(mobileFlag == false) {
			var itemFirst=document.createElement("div");
			itemFirst.className="span1";
			itemRow.appendChild(itemFirst);
		}
		for(var i=0,len=playlist.length; i<len;	i++) {
			if(i > 0 && (i % 3 === 0 || mobileFlag == true)) {
				containerItem.appendChild(itemRow);
				containerInner.appendChild(containerItem);
				
				containerItem=document.createElement("div");
				containerItem.className="item";
				itemRow=document.createElement("div");
				itemRow.className="row-fluid";
				
				if(mobileFlag == false) {
					itemFirst=document.createElement("div");
					itemFirst.className="span1";
					itemRow.appendChild(itemFirst);
				}
			}
			var entry=playlist[i];
			var itemSpan=document.createElement("div");
			if(mobileFlag == true) {
				itemSpan.className="span14";
			} else {
				itemSpan.className="span4";
			}
			var item=document.createElement("div");
			var thumb=document.createElement("img");
			var play=document.createElement("div");
			var title=document.createElement("div");
			var header=document.createElement("div");
			header.style.position="absolute";
			header.className="anv-item-header";
			header.style.top="0";
			header.style.height="auto";
			header.style.width="100%";
			header.style.padding="0px";
			item.className="anv-item";
			item.id="anv-item"+i;
			item.style.position="relative";
			item.style.width="100%";
			item.style.margin="15px 0";
			item.style.color="white";
			item.style.display="none";
			item.onclick=onItemSelected;
			item.style.cursor="pointer";
			thumb.onload=onImageLoad;
			thumb.src=entry.src_image_url;			
			play.style.background="url('http://media.graytvinc.com/designimages/anv_2_sprite32.png') 4px -18px";
			play.style.width="36px";
			play.style.height="36px";
			play.style.backgroundSize="400%";
			play.style.display="block";
			title.style.fontSize="13px";
			title.style.letterSpacing="0.02em";
			title.style.lineHeight="1.6em";
			title.style.position="relative";
			title.style.whiteSpace="wrap";
			title.style.fontFamily="Arial, sans-serif";
			title.style.padding="4px";
			title.innerHTML=entry.def_title;
			title.style.maxHeight="40px";
			title.style.overflow="hidden";
			item.appendChild(thumb);
			header.appendChild(title);
			header.appendChild(play);
			item.appendChild(header);
			itemSpan.appendChild(item);
			itemRow.appendChild(itemSpan);
		}
		containerItem.appendChild(itemRow);
		containerInner.appendChild(containerItem);
		container.appendChild(containerInner);
		var numPages = Math.ceil(playlist.length / 3);
		if(numPages > 1 || (mobileFlag == true && playlist.length > 1)) {
			var leftPager = document.createElement("a");
			leftPager.className="left carousel-control";
			leftPager.href="#" + divId;
			leftPager.setAttribute("data-slide", "prev");
			leftPager.innerHTML = "‹";
			var rightPager = document.createElement("a");
			rightPager.className="right carousel-control";
			rightPager.href="#" + divId;
			rightPager.setAttribute("data-slide", "next");
			rightPager.innerHTML = "›";
			
			container.appendChild(leftPager);
			container.appendChild(rightPager);
		}	
		function onImageLoad() {
			this.parentNode.style.display="inline-block";
		}	
		function onItemSelected() {
			pat=/anv-item(.+)/;
			var m=pat.exec(this.id);
			var index=parseInt(m[1]);
			playerInstance.play(playlist[index]);
			console.log(index);
		}
	}
