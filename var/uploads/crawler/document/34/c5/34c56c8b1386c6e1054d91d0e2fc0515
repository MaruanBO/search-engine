		function endsWith(s, s1) {  
	        if (s1 == null || s1 == ""|| s1.length == 0 || s1.length > s.length){  
	            return false;  
	        } 	              
	        if (s.substring(s.length-s1.length) == s1) {  
	            return true;  
	        } else {  
	            return false;  
	        }  
	        return true;  
	    }  
