</div><script type="text/javascript">(function(){

var aff_def = [
  { //sports
    valid_primary: true,
    regex: /\/sports\/|\/vendor\/espn\//i
  },
  { //entertainment
    valid_primary: true,
    regex: /\/entertainment\/|\bhollyscoop\b|\bcelebritywire\b|e(?:\!|\%21)|\/player\/fiveminute\//i
  },
  { //general news
    valid_primary: true,
    regex: /\/news\/read\/category\/top(?:\%20|\+|\s)news\/|\/news\/category\/(?:us|world|political)\//i
  },
  { //top searches
    valid_primary: false,
    regex: /\bcontext\=topsearches\b/i
  },
  { //video
    valid_primary: false,
    regex: /\/tv\/3\//i
  },
  { //games
    valid_primary: true,
    regex: /\/games\//i
  },
  { //weather
    valid_primary: true,
    regex: /\/weather_channel\//i
  },
  { //search
    valid_primary: false,
    regex: /\/search\//i
  },
  { //other
    valid_primary: false,
    regex: /\/local\/|\/music_channel\/|\/offers\/|\/tv\/|\/news\//i
  }
];

var aff_interval_cur = [
  // v0
  null,
  // v1
  function ()
  {
    return Math.floor(((new Date()).getTime()/1000 - 1388880000) / (60*60*24*7));
  }
];

var aff_init = [
  // v0
  function ()
  {
    return {primary:null, affinities:[0,0,0,0,0]};
  },
  // v1
  function ()
  {
    var v1 = {
      primary: null,
      affinities: [],
      group: Math.random() < .5 ? 0 : 1,
      interval: aff_interval_cur[1]()
    };
    for (var i=0; i<aff_def.length; i++)
    {
      v1.affinities[i] = 0;
    }
    return v1;
  }
];

var aff_upgrade_steps = [
  // v0 => v1
  function (v0)
  {
    var v1 = aff_init[1]();
    var affinity_map = [0, 1, 2, 3, 4];

    if (v0.primary !== null && v0.primary < affinity_map.length)
    {
      v1.primary = affinity_map[v0.primary];
    }

    for (var i=0; i<v0.affinities.length; i++)
    {
      var new_i = affinity_map[i];
      if (new_i === null)
      {
        continue;
      }
      v1.affinities[new_i] = v0.affinities[i];
    }

    return v1;
  }
];

var aff_parse = [
  // v0
  function (synaff)
  {
    if (!(matches = /^0([0-9a-z]|X)((?:[0-9a-z]{2})+)$/.exec(synaff)))
    {
      return null;
    }
    var primary = matches[1] === 'X' ? null : parseInt(matches[1], 36);
    var affstr = matches[2];
    var affinities = [];
    for (var i=0; i<affstr.length/2; i++)
    {
      affinities[i] = parseInt(affstr.substr(i*2,2), 36);
    }
    return {primary:primary, affinities:affinities};
  },
  // v1
  function(synaff)
  {
    if (!(matches = /^1([0-9a-z]|X)([0-9a-z])([0-9a-z]{2})((?:[0-9a-z]{2})+)$/.exec(synaff)))
    {
      return null;
    }
    var primary = matches[1] === 'X' ? null : parseInt(matches[1], 36);
    var group = parseInt(matches[2], 36);
    var interval = parseInt(matches[3], 36);
    var affstr = matches[4];
    var affinities = [];
    for (var i=0; i<affstr.length/2; i++)
    {
      affinities[i] = parseInt(affstr.substr(i*2,2), 36);
    }
    return {primary:primary, group:group, interval:interval, affinities:affinities};
  }
];

function aff_read()
{
  var matches;
  if (!(matches = /(?:^|\;)\s*synaff\s*\=\s*([^\;]+)/.exec(document.cookie)))
  {
    return null;
  }
  var synaff = matches[1];
  var version = parseInt(synaff.substr(0, 1), 36);
  if (!aff_parse[version])
  {
    return false;
  }
  return aff_upgrade(version, aff_parse[version](synaff));
}

function aff_upgrade(version_start, aff)
{
  var version = version_start;
  while (version < aff_upgrade_steps.length)
  {
    aff = aff_upgrade_steps[version](aff);
    version++;
  }
  aff.version_start = version_start;
  aff.version = version;
  return aff;
}

function aff_update_primary(aff)
{
  var maxi = aff.primary;
  var maxv = aff.primary === null ? 0 : aff.affinities[aff.primary];
  for (var i=0; i<aff.affinities.length; i++)
  {
    if (aff_def[i].valid_primary && aff.affinities[i] > maxv)
    {
      maxi = i;
      maxv = aff.affinities[i];
    }
  }
  aff.primary = maxi;
}

function aff_update_halflife(aff)
{
  var cur_interval = aff_interval_cur[aff_interval_cur.length-1]();
  if (aff.interval >= cur_interval)
  {
    return false;
  }

  while (aff.interval < cur_interval)
  {
    for (var i=0; i<aff.affinities.length; i++)
    {
      if (aff.affinities[i] <= 2)
      {
        continue;
      }
      aff.affinities[i] = Math.ceil(aff.affinities[i]/2);
    }

    aff.interval++;
  }
  return true;
}

function aff_write(aff)
{
  var max_affinity = 36*36 - 1;

  function b36(v, bytes)
  {
    var str = "";
    if (v === null)
    {
      while (str.length < bytes)
      {
        str += "X";
      }
      return str;
    }

    var max = Math.pow(36, bytes) - 1;
    str = (v > max ? max : v < 0 ? 0 : v).toString(36);
    while (str.length < bytes)
    {
      str = "0" + str;
    }
    return str;
  }

  var cv = b36(1,1) + b36(aff.primary,1) + b36(aff.group,1) + b36(aff.interval,2);
  for (var i=0; i<aff.affinities.length; i++)
  {
    cv += b36(aff.affinities[i], 2);
  }

  document.cookie = "synaff="+cv+"; path=/; domain="+location.hostname+"; expires=Tue, 19 Jan 2038 03:14:07 GMT";
}

function aff_categorize(url)
{
  for (var i=0; i<aff_def.length; i++)
  {
    if (url.match(aff_def[i].regex))
    {
      return i;
    }
  }
  return null;
}

function aff_update()
{
  var aff_updated = false;
  var aff = aff_read();
  if (aff === false)
  {
    return;
  }
  else if (aff === null)
  {
    aff = aff_init[aff_init.length-1]();
  }
  else
  {
    if (aff.version_start != aff.version)
    {
      aff_updated = true;
    }
    if (aff_update_halflife(aff))
    {
      aff_updated = true;
    }
  }
  var aff_category = aff_categorize(location.href);
  if (aff_category !== null)
  {
    aff.affinities[aff_category]++;
    aff_update_primary(aff);
    aff_updated = true;
  }
  if ('Syn' in window && Syn && Syn.Tracking && Syn.Tracking.Omniture)
  {
    var omniture_affinityname = ["sports", "entertainment", "general news", "top searches", "video", "games", "weather", "search", "other"];
    Syn.Tracking.Omniture.setSValues({'prop46':aff.primary===null?"(no affinity)":omniture_affinityname[aff.primary]});
    var omniture_groupname = ["test", "control"];
    Syn.Tracking.Omniture.setSValues({'prop47':omniture_groupname[aff.group]});
  }
  if (aff_updated) aff_write(aff);
}

aff_update();

})();</script>
