<script type="text/javascript">
	var j = jQuery.noConflict();
	j(document).ready(function() {

		j('#clear').click(function() {
			j('#searchcompany01').val("Company Name");
			j('#searchCity01').val("City");
			j('#searchState01').val("");
		});

		j('#searchState01').val('');
		
		j('#searchState01').keyup(function(e) {
	          if(e.keyCode == 13) {
	        	  if (validatesearch()) {
		        	  j('#businesssearch01').submit();
	        	  }
		      }
		    });
	});

	function validatesearch()
    {
	    var ret = true;
	    
    	var company = j('#searchcompany01').val();
    	if(company.length == 0 || company == 'Company Name')
	    	ret = false;		    		

       	if(!ret)
    		alert('Please make sure you enter a company name when searching.');

		return ret;
    }
</script>
		
