          <script src="https://www.google.com/jsapi?key=ABQIAAAANIDTzgNhWOrqdIbrEgJV4RQI58ArBdmQjm5wXK-NdCkCM_GZ0hSg-VFwja-qNPwP5Q09bN_OyoQJ9w" type="text/javascript"></script><script type="text/javascript">

              var wwptagwwpinc = "wwpinc"
              var wwpamtwwpinc = 7;
              var wwpdescriptionwwpinc = 0;
              var holderwwpinc = $(".newsFeed");
              google.load("feeds", "1");

      function initializewwpinc () {
        var feedwwpinc = new google.feeds.Feed("http://www.diigo.com/rss/user/Wwpinc/"+wwptagwwpinc);
        feedwwpinc.setNumEntries(wwpamtwwpinc);
        feedwwpinc.load(function(items) {
          if (!items.error) {
            holderwwpinc.append('<ul id="engineShareList"></ul>');
            $.each(items.feed.entries, function(i,ele){
                holderwwpinc.find("ul#engineShareList").append('<li id="link_'+i+'"><a class="LinkItem" href="'+ele.link+'" target="_blank">'+ele.title+'</a></li>');
                if(wwpdescriptionwwpinc){
                  holderwwpinc.find("ul#engineShareList li#link_"+i).append('<p class="LinkDesc">'+ele.content+'</p>');
                  holderwwpinc.find("ul#engineShareList li#link_"+i+" p:has(strong:contains('Tags:'))").remove();
                }
              });
            holderwwpinc.find("ul#engineShareList").append('<li id="link_rss"><a target="_blank" href="http://www.diigo.com/rss/user/wwpinc/"><img alt="RSS" src="http://www.diigo.com/images/v2/rss.gif"></a></li>');
          }
        });
      }
    
    google.setOnLoadCallback(initializewwpinc);

     </script>
