                <script type="text/javascript">
                    (function(){
                        var takeover_tab = document.getElementById('tab-4'),
                            tab_img = takeover_tab.getElementsByTagName("img")[0],
                            isTabVisible = takeover_tab && (" " + takeover_tab.className + " ").replace(/[\t\r\n\f]/g, " ").indexOf('selected') >= 0,
                            isTraditionalTakeover = (" " + document.getElementById('home-header').className + " ").replace(/[\t\r\n\f]/g, " ").indexOf('takeover') >= 0,
                            existingId = document.getElementById('tab-tracker') || document.getElementById('tab-tracking') || document.getElementById('tab-contents-tracking'),
                            
                            
                            tab_pixel = 'http://bs.serving-sys.com/BurstingPipe/adServer.bs?cn=tf&amp;c=19&amp;mc=imp&amp;pli=12038598&amp;PluID=0&amp;ord=[timestamp]&amp;rtu=-1',
                            
                            
                            
                            takeover_pixel = 'http://bs.serving-sys.com/BurstingPipe/adServer.bs?cn=tf&amp;c=19&amp;mc=imp&amp;pli=12038599&amp;PluID=0&amp;ord=[timestamp]&amp;rtu=-1';
                            

                        if ( existingId ){
                            /* Skip if existing javascript in flatblock */
                            return false;
                        }

                        function appendTrackingPixel( id, url ) {
                            var this_pixel = document.getElementById(id);
                            var time_stamp = new Date().getTime().toString();
                            var re = /timestamp|cachebuster/gi;
                            var new_url = url.replace(re, time_stamp);
                            this_pixel.src = new_url;
                        }

                        function trackTabContents() {
                            appendTrackingPixel('tab-contents-tracking', takeover_pixel);
                        }

                        if ( tab_pixel ) {
                            /* This tracks views of the tab, not the takeover within the tab */
                            document.write('<img id="tab-tracking" class="visuallyhidden" style="position:absolute;" alt="" >');
                            appendTrackingPixel('tab-tracking', tab_pixel);
                        }

                        if ( takeover_pixel ) {
                            document.write('<img id="tab-contents-tracking" class="visuallyhidden" style="position:absolute;" alt="" >');
                            if ( isTabVisible || isTraditionalTakeover ) {
                                trackTabContents();
                            } else {
                                if ( takeover_tab && takeover_tab.addEventListener ) {
                                    takeover_tab.addEventListener('click',trackTabContents,false);
                                } else if ( takeover_tab && takeover_tab.attachEvent ) {
                                    takeover_tab.attachEvent('onclick',trackTabContents);
                                }
                            }
                        }

                    })();
                </script>
                
