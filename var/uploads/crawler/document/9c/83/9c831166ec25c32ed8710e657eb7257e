        <script>
        var BWY = BWY || {};
        BWY.userAgent = {};
        BWY.userAgent.full = navigator.userAgent;
        BWY.userAgent.isIPhone = BWY.userAgent.full.match(/iPhone/i);
        BWY.userAgent.isIPod = BWY.userAgent.full.match(/iPod/i);
        BWY.userAgent.isIPad = BWY.userAgent.full.match(/iPad/i);
        BWY.userAgent.isAndroid = BWY.userAgent.full.match(/Android/i);
        BWY.userAgent.isIOS = BWY.userAgent.isIPhone || BWY.userAgent.isIPad || BWY.userAgent.isIPod;
        BWY.userAgent.isTouchDevice = ("ontouchstart" in window);
        </script>
        
            

            <script src="http://d1069swflop3lk.cloudfront.net/js/libs/jquery-1.11.1.min.8101d596b2b8.js"></script>
            <script src="http://d1069swflop3lk.cloudfront.net/js/libs/jquery.placeholder.6e5b889042b3.js"></script>
            <script src="http://d1069swflop3lk.cloudfront.net/js/libs/jquery-plugins/jquery.validate.d5231b637884.js"></script>

            <script>
                jQuery.noConflict();
            </script>

            <script src="http://d1069swflop3lk.cloudfront.net/js/base.8ae96be647b9.js"></script>

            <script>
                jQuery(document).ready(function($){

                    // this grabs the string before the first period (the subdomain), and
                    // splits everything into an array for manipulation
                    var full = window.location.host;
                    var parts = full.split('.');
                    var sub = parts[0];

                    // If the subdomain matches a value in the language drop down, select it. If it doesn't,
                    // it will just select english by default (which is what we want)
                    language_options = [];
                    jQuery('#mySelect option').each(function(){
                        language_options[jQuery(this).val()] = true;
                    });

                    sub = language_options[sub] || 'en';
                    jQuery("#mySelect").val(sub);

                    jQuery("#mySelect").change(function(){

                        // This grabs the value that was selected
                        var language = $('#mySelect option:selected').val();
                        if (language == 'en'){
                            language = '';
                        } else {
                            language += '.';
                        }

                        var currentUrl = window.location.href;
                        var currentJoin;

                        // This grabs all of the values in the options inside of the select
                        var opts = document.getElementById('mySelect').options;
                        var vals = [];
                        for(var i = 0, j = opts.length; i < j; i++) {
                           vals.push(opts[i].value);
                        }

                        // Loop through the values stashed from all of the options, and check
                        // them against the subdomain. If there is a match, then the page is
                        // translated. If not, the page is not translated

                        var check = function(){

                            if (sub == 'www' || '0' == '2'){
                                // If the url begins with www, strip it
                                var currentUrlParts = currentUrl.split('.');
                                currentUrlParts.splice(0,1);
                                currentJoin = currentUrlParts.join(".");
                                return currentJoin;
                            } else {

                                for (i=0;i<vals.length;i++){
                                     if (sub == vals[i]){
                                     // if the url begins with any known translated page, strip it
                                          var currentUrlParts = currentUrl.split('.');
                                          currentUrlParts.splice(0,1);
                                          currentJoin = currentUrlParts.join(".");
                                          return currentJoin;
                                     }

                                 }

                                // if the url does not begin with www or a translated page, assume
                                // it's a city or an affiliate, and just strip http://
                                currentJoin = currentUrl.replace(/.*?:\/\//g, "");
                                return currentJoin;

                            }

                        };

                        window.location = "http://" + language + check();

                    })

                });
            </script>

            <script type="text/javascript">
            // For nav tracking of ads
            jQuery(document).ready(function($){
                $('.find-a-show.navli').on('mouseenter focus', '.trigger', function(e){
                    trackingTimestamp($('#nav-show'));
                });
                $('.video-portal.navli').on('mouseenter focus', '.trigger', function(e){
                    trackingTimestamp($('#nav-video'));
                });
            })
            </script>

            <script>
            var safariEvent = 'Safari-style Top Sites';
            if(window.navigator&&window.navigator.loadPurpose==="preview") {
                ga('send','event',{
                    'eventCategory' : safariEvent,
                    'eventAction'   : safariEvent,
                    'eventLabel'    : safariEvent
                });
            }
            jQuery(document).ready(function($){
                jQuery('input, textarea').placeholder();
            

                gaHashTracking('#footer', '.spaced-col:not(:last-child) a', function(){ return '#loc=f'; });

                if (BWY.userAgent.isTouchDevice){
                    var this_tab,
                        this_clicked = null;

                    $('.nav-global').on('touchstart', '.nav-wrap > ul > li .trigger', function(e){
                        // first click should activate drop-down, second should go to href or deactivate hover if no href
                        var $this = $(this).parent('li');
                        this_tab = $this.text();

                        if (this_clicked === this_tab) {
                            // has clicked on this tab before
                            this_clicked = null; // in case tab doesn't have href
                            $this.removeClass('hover');
                            return true;
                        } else {
                            // clicked on a different tab previously
                            // OR clicked on a tab with no href
                            e.preventDefault();
                            this_clicked = this_tab;
                            $this.siblings().andSelf().removeClass('hover');
                            $this.addClass('hover');
                        }
                    });
                }
            });
            </script>

        

        <script>
        (function() {
            var metas = document.getElementsByTagName('meta');
            var i;
            if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i)) {
                for (i=0; i<metas.length; i++) {
                    if (metas[i].name == "viewport") {
                        metas[i].content = "width=device-width, maximum-scale=1.0";
                    }
                }
                document.getElementsByTagName('body')[0].addEventListener("gesturestart", gestureStart, false);
            }
            function gestureStart() {
                for (i=0; i<metas.length; i++) {
                    if (metas[i].name == "viewport") {
                        metas[i].content = "width=device-width, minimum-scale=0.25, maximum-scale=1.6";
                    }
                }
            }
        }());
        </script>

        
        <script src="http://d1069swflop3lk.cloudfront.net/js/libs/jquery-plugins/typeahead.bundle.min.474b2be117d0.js"></script>
        <script>
            (function($){
                var $searchField =  $('#search-input')
                var showsEngine = new Bloodhound({
                    datumTokenizer: function(datum){
                        return Bloodhound.tokenizers.whitespace(datum.value)
                    },
                    queryTokenizer: Bloodhound.tokenizers.whitespace,
                    limit: 3,
                    prefetch: {
                        url: '/api/v1/typeahead/?domain=1',
                        ttl: 1800000,
                        filter: function(data){
                            return $.map(data.objects, function(item) {
                                var showPoster = item.show.poster;
                                if (showPoster === null){
                                    showPoster =  "http://d1069swflop3lk.cloudfront.net/img/icons/placeholders/ph-89-135.48000a0f2214.png"
                                }
                                else {
                                    showPoster = item.show.poster.medium
                                }
                                return {
                                    value: item.show.name,
                                    url: item.show.get_absolute_url,
                                    poster: showPoster,
                                    tickets: item.display_buy_tickets,
                                    id: item.id
                                }
                            })
                        }
                    }
                });

                // kicks off the loading/processing of `local` and `prefetch`
                showsEngine.initialize();
                $searchField.typeahead(
                    {
                        hint: false,
                        highlight: false,
                        minLength: 3
                    },
                    {
                    value: 'showsEngine',
                    displayKey: 'value',
                    source: showsEngine.ttAdapter(),
                    templates: {
                        suggestion: function(data){
                            return '<span class="mbn media"><a class="img" href="' + data.url + '" title="'+  data.value + '"><img src="'+ data.poster + '"/></a><span class="block"><a href="' + data.url + '">' + data.value + '</a></span>' + '<a title="Buy Tickets for '+ data.value +'" href="/tickets/show/'+ data.id +'"><span class="mvm pill-button red">Buy Tickets</span></a></span>'
                        },
                        footer: function(){
                            var searchInstance = $searchField.typeahead('val');
                            return '<span class="tt-menu-addon ptm"><a href="/search/star_files/?q='+ searchInstance + '#nav-auto-star-files'+ '"><b>"'+ searchInstance + '"</b> in <i>Stars</i><i class="icon icon-FA-angle-right mls"></i></a><a href="/search/articles/?q='+ searchInstance + '#nav-auto-buzz' + '"><b>"'+ searchInstance + '"</b> in <i>Buzz</i><i class="icon icon-FA-angle-right mls"></i></a><a href="/search/?q='+ searchInstance + '#nav-auto-all'+ '"><b>"'+ searchInstance + '"</b> All Results<i class="icon icon-FA-angle-right mls"></i></a></span>'
                        }
                    }
                })

                $searchField.on('typeahead:selected', function(obj, datum, name){
                    if(datum.url == undefined){
                        window.location = 404;
                    }
                    //send users that click with keyboard to -e
                    window.location = datum.url + '#nav-auto-e';
                });

                gaHashTracking('.twitter-typeahead', '.tt-suggestion a', function(){ return '#nav-auto'; });

                $searchField.one('focus', function(){
                    var category = 'Search';
                    var action = category + ': Global Nav Show Search focused';
                    // label is not used for this event tracking
                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'hitCallback'   : function() {
                            console.log(action);
                        }
                    });
                });
            }(jQuery));
        </script>
        


        
        <script>
            

            (function($){
                /* If bottom fixed bar's close button is clicked, it is not shown again for a week */

                var shouldDisplay = !BWY.getSessionStorageCookie('stickyfooter_closed');

                var $stickyfooter        = $('.stickyfooter');
                var $bottom_footer_ad    = $('.bottom-footer-ad');
                var $expand_stickyfooter = $('.new-expand-stickyfooter');
                var $expand_stickyfooter_old = $('.expand-stickyfooter');
                var $body                = $('body');

                if (shouldDisplay) {
                    $stickyfooter.css('bottom','0');
                    $bottom_footer_ad.css('bottom','4px');

                    $expand_stickyfooter.css({ bottom: '-40px' });
                    $expand_stickyfooter_old.css({ bottom: '-40px' });
                    $body.css({ marginBottom: '115px' });
                } else {
                    $expand_stickyfooter.css('bottom','40px');
                    $expand_stickyfooter_old.css('bottom','40px');
                    $stickyfooter.css('bottom','-40px');
                    $bottom_footer_ad.hide();
                    $('.stickyfooter-content').hide();
                    $('.stickyfooter-bg').hide();
                    $('.close-stickyfooter-js').hide();
                    $('.social-stickys').hide();
                    $('.close-stickyfooter').hide();

                }

                // When the stickyFooter is closed, log the date it was closed for future use
                $('.close-stickyfooter-js').click(function(e){
                    e.preventDefault();

                    $stickyfooter.animate({ bottom: '-40px' });
                                 $bottom_footer_ad.animate({ bottom: '-33px' }, function() {
                                    $bottom_footer_ad.hide();
                                    $('.stickyfooter-content').hide();
                                    $('.stickyfooter-bg').hide();
                                    $('.close-stickyfooter-js').hide();
                                 } );
                    $expand_stickyfooter.animate({ bottom: '40px' });
                    $body.animate({ marginBottom: '0' });

                    // When the stickyFooter is closed, log the date it was closed for future use
                    BWY.getSessionStorageCookie("stickyfooter_closed", true);

                    var category = 'Other Actions';
                    var action = category + ': Stickyfooter';
                    var label = action + ' - Collapse';

                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'eventLabel'    : label,
                        'hitCallback'   : function() {
                            console.log('stickyfooter close new');
                        }
                    });
                });

                $('.new-expand-stickyfooter').click(function(e){
                    e.preventDefault();

                    $bottom_footer_ad.show();
                    $('.stickyfooter-content').show();
                    $('.stickyfooter-bg').show();
                    $('.close-stickyfooter-js').show();
                    $stickyfooter.animate({ bottom: '0' });
                    $bottom_footer_ad.animate({ bottom: '4px' }, function(){
                    });

                    $expand_stickyfooter.animate({ bottom: '-40' });
                    $body.animate({ marginBottom: '115px' });

                    // When the stickyFooter is open, destroy the associated localStorage for it
                    BWY.removeSessionStorageCookie("stickyfooter_closed");

                    var category = 'Other Actions';
                    var action = category + ': Stickyfooter';
                    var label = action + ' - Expand';
                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'eventLabel'    : label,
                        'hitCallback'   : function() {
                            console.log('stickyfooter open new');
                        }
                    });
                });

                // LEGACY
                // When the stickyFooter is closed, log the date it was closed for future use
                $('.close-stickyfooter').click(function(e){
                    e.preventDefault();
                    $stickyfooter.animate({ bottom: '-40px' });
                                 $bottom_footer_ad.animate({ bottom: '-33px' }, function() {
                                    $bottom_footer_ad.hide();
                                    $('.social-stickys').hide();
                                    $('.stickyfooter-bg').hide();
                                    $('.close-stickyfooter').hide();
                                 } );
                    $expand_stickyfooter_old.animate({ bottom: '40px' });
                    $body.animate({ marginBottom: '0' });
                    // When the stickyFooter is closed, log the date it was closed for future use
                    BWY.getSessionStorageCookie("stickyfooter_closed", true);

                    var category = 'Other Actions';
                    var action = category + ': Stickyfooter';
                    var label = action + ' - Collapse';

                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'eventLabel'    : label,
                        'hitCallback'   : function() {
                            console.log('stickyfooter close legacy');
                        }
                    });
                });

                $('.expand-stickyfooter').click(function(e){
                    e.preventDefault();

                    $bottom_footer_ad.show();
                    $('.social-stickys').show();
                    $('.stickyfooter-bg').show();
                    $('.close-stickyfooter').show();
                    $stickyfooter.animate({ bottom: '0' });
                    $bottom_footer_ad.animate({ bottom: '4px' }, function(){
                    });

                    $expand_stickyfooter_old.animate({ bottom: '-40' });
                    $body.animate({ marginBottom: '115px' });

                    // When the stickyFooter is open, destroy the associated localStorage for it
                    BWY.removeSessionStorageCookie("stickyfooter_closed");

                    var category = 'Other Actions';
                    var action = category + ': Stickyfooter';
                    var label = action + ' - Expand';
                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'eventLabel'    : label,
                        'hitCallback'   : function() {
                            console.log('stickyfooter open legacy');
                        }
                    });
                });

            }(jQuery));
        </script>
        


    
    <script type="text/javascript">
    jQuery(document).ready(function($){
        var $selector =        $('.city-nav-li');
        var $selector_hitbox = $('.city-nav-li .nav-pill');

        if ($selector_hitbox) {
            
            $selector_hitbox.click(function(e){
                e.preventDefault();
                e.stopImmediatePropagation();
                $selector.toggleClass('hover');
            });

            
            $('body').click(function(e){
                if (!$(e.target).is('.city-nav-li, .city-nav-li *')) {
                    $selector.removeClass('hover');
                }
            });

            
            $selector.click(function(e){
                var category = 'Global City Select';
                var action   = category + ': ';
                var label = action + 'using version: default';

                if ($(e.target).is('a[href*="broadway.com"]')) {
                    ga('send','event',{
                        'eventCategory' : category,
                        'eventAction'   : action,
                        'eventLabel'    : label,
                        'hitCallback'   : function() {
                            console.log('city select sent');
                        }
                    });
                    return true;
                } else {
                    e.stopImmediatePropagation();
                }
            });
        }
    });
    </script>

        
        
