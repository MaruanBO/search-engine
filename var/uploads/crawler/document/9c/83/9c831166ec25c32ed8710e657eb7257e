    <script type="text/javascript">
        jQuery(document).ready(function($){
            homepageCarousels();

            if ($('#newsletter-signup, #eclub-signup').length) {
                
                var newsletter = $('#newsletter-signup, #eclub-signup');
                newsletter.validate({
                    debug: true,
                    rules: {
                        email: {
                            required: true,
                            email: true
                        }
                    },
                    messages: {
                        email: {
                            required: "Please enter an email address.",
                            email: "Please enter a valid email address."
                        }
                    },
                    submitHandler: function(form) {
                        var $form = $(form),
                            data = $form.serialize();
                            $.ajax({
                                url: $form.attr('action'),
                                type: 'POST',
                                data: data
                            })
                            .done(function() {
                                if(!$('input.success').length) {
                                    $form
                                        .html("<label for='newsletter-signup' generated='generated' class='success'>Thanks! You've been added to our newsletter.</label>");
                                    try {
                                        ga('send','pageview','/newsletters/success');
                                    } catch(err){}
                                }
                        });
                    }
                });
            }

            homepageTabs('Broadway.com');

            function homeCarouselHash() {
                var $this = $(this);
                var tab_index  = $this.parents('.home-carousel-ul').data('tab-number');
                var hero_index = $this.parents('.carousel-item').data('hero-number');
                var hash = '#loc=c-tab' + tab_index + '-pos' + hero_index;
                return hash;
            }

            gaHashTracking('#home #black-box', '.carousel-wrapper .carousel-item a', homeCarouselHash);
            gaHashTracking('#home .highlights .shows', 'a', function(){ return '#loc=highlights'; });

            function carouselLabel(){
                var $this = $(this);
                var show = $this.data('showname');
                var tab_index  = $this.parents('.home-carousel-ul').data('tab-number');
                var hero_index = $this.parents('.carousel-item').data('hero-number');
                var label = show + ', tab-' + tab_index + ' pos-' + hero_index;
                return label;
            }

            
            gaRedirectTracking('#home .highlights .categories', 'a', 'Popular Categories', function(){ return $(this).text(); });
            
            gaRedirectTracking('#home #black-box', '.carousel-wrapper .carousel-item a[href*="tickets/show"]', 'Carousel Buy Tickets', carouselLabel);
        });
    </script>
      
    

        <script>
             jQuery(document).ready(function($){
                 $('div[data-homepage-area-tagging]').on('click', 'a[href]', function(e) {
                     var $this = $(this);
                     var targetArea = $(e.delegateTarget).data('homepage-area-tagging');
                     var targetUrl = $this.prop('href');

                     dataLayer.push({
                         'event': 'homepage-area-tag',
                         'homepage-area': targetArea,
                         'target-url': targetUrl
                     });
                 });

                 $('.carousel-previous, .carousel-next').on('click', function(e) {
                     dataLayer.push({
                         'event': 'homepage-hero-scroll'
                     });
                });
            });
        </script>
    

