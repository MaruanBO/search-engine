            <script type="text/javascript">
    jQuery(document).ready(function($){ 
        BWY = BWY || {};

        function appendTabContentsPixel() {
            //Counterintuitive - this tracks the number of views of the large takeover image
            var tab_takeover_pixel = 'http://adfarm.mediaplex.com/ad/tr/21167-155592-3436-5?mpt=CACHEBUSTER';
            var timestamp = new Date().getTime().toString();
            var re = /timestamp|cachebuster/gi;
            var newurl = tab_takeover_pixel.replace(re, timestamp);
            var pixel = $('<img alt="">');
            pixel.attr('src', newurl);
            // putting it in the tab itself screws up alignment
            $('body').append(pixel);                        
        }
        
        if ( BWY.TrackedTabisRunning === 'true' ) {
            var tabIsVisibleOnLoad = $('#tab-contents-4').hasClass('selected');
            if ( tabIsVisibleOnLoad ){
                //append tracking pixel
                appendTabContentsPixel();
            } else {
                $('.tab').click(function(){
                    var $this = $(this);
                    
                    // if they clicked on the 4th tab
                    if ( $this.attr('id') === 'tab-4' ){
                        // append tracking pixel
                        clickie = appendTabContentsPixel();
                        console.log(clickie)                        
                    }                    
                });
            }
        }
    });
</script>
        
            
