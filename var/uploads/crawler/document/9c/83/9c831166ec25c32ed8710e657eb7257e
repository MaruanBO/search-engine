            <script type="text/javascript">
    jQuery(document).ready(function($){ 
        BWY = BWY || {};

        function appendTabContentsPixel() {
            var tab_takeover_pixel = 'http://bs.serving-sys.com/BurstingPipe/adServer.bs?cn=tf&c=19&mc=imp&pli=7595454&PluID=0&ord=[timestamp]&rtu=-1';
            var timestamp = new Date().getTime().toString();
            var re = /timestamp|cachebuster/gi;
            var newurl = tab_takeover_pixel.replace(re, timestamp);
            var pixel = $('<img alt="">');
            pixel.attr('src', newurl);
            $('#tab-contents-4').append(pixel);                        
        }
        
        if ( BWY.BoMisRunning === 'true' ) {
            var tabIsVisibleOnLoad = $('#tab-contents-4').hasClass('selected');
            if ( tabIsVisibleOnLoad ){
                //append tracking pixel
                appendTabContentsPixel();
            } else {
                $('.tab').click(function(){
                    var $this = $(this);
                    
                    // if they clicked on the 4th tab
                    if ( $this.attr('id') === 'tab-4' ){
                        // append tracking pixel
                        appendTabContentsPixel();                        
                    }                    
                });
            }
        }
    });
</script>
        
