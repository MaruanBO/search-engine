<script type="text/javascript">
    jQuery(document).ready(function(){
        var country = "", region = "", countryname = "", regionname = "";

        country = geoip_country_code();
        countryname = geoip_country_name();
        regionname = geoip_region_name();
        if(country != ""){
            jQuery("#countryIdInput").html(country);
        }
        region = geoip_region();
        if(region != ""){
            jQuery("#regionIdInput").html(region);
        }
        if(regionname!=""){
            jQuery('.region').html(regionname);
        }
    });
</script>		
