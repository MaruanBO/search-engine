<script type="text/javascript">
	if (!isBrowserSupported()) {
		alert('Your browser version is not supported.  For Netscape please use version 7 or above.');
	}
	function getParameterByName(name) {
		name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
		var regexS = "[\\?&]" + name + "=([^&#]*)";
		var regex = new RegExp(regexS);
		var results = regex.exec(window.location.search);
		if (results == null) {
			return "";
		} else {
			return decodeURIComponent(results[1].replace(/\+/g, " "));
		}
	}
	var affiliatePID = getParameterByName('PID');
	if (affiliatePID) {
		setCookie('AffiliatePID_10052', affiliatePID.toUpperCase());
	}
</script>


