	<script type="text/javascript">

	var ghMenuTimeout;
	function ghMenuSetTimeout() {
		ghMenuClearTimeout();
		ghMenuTimeout = setTimeout(ghHideAllMenus, 500);
	}
	function ghMenuClearTimeout() {
		if (ghMenuTimeout != undefined) {
			clearTimeout(ghMenuTimeout);
		}
	}
	function ghHideAllMenus() {
		amplify.publish("menu/*/close"); // de-coupled way to notify any open menus that they should close (* means "all")
	}
	function ghTrim(s) {
		return s.replace(/^\s+|\s+$/g,"");
	}

	amplify.subscribe("menu/*/close", function() {
		ghMyStoreHideMenu();
		ghMyAccountHideMenu();

		snShopByCategoryHideMenu();
		snShopByBrandHideMenu();
		snNewProductsHideMenu();
		snDealsHideMenu();
		snLearnHideMenu();

	});

	</script>

	
	
