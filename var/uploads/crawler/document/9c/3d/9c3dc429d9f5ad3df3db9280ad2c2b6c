		<script type="text/javascript">
		// Are we on home page? If true, Shop By Category menu is always visible.
		var snHomePage = false;
		$("#snVerticalMenu").hoverMenu();
		$(document).ready(function() {
			
			if (!snHomePage) {
				$("#snShopByCategory").mouseenter(function() {
					ghMenuClearTimeout();
					ghHideAllMenus();
					$("#snShopByCategoryPanel").css("visibility", "visible");
				});
				$("#snShopByCategory").mouseleave(ghMenuSetTimeout);
				$("#snShopByCategoryPanel").mouseenter(ghMenuClearTimeout);
				$("#snShopByCategoryPanel").mouseleave(ghMenuSetTimeout);
			} else {
				$("#snShopByCategory").css("height", "40px");
				$("#snShopByCategoryLabel").css("top", "10px");
				$("#snShopByCategoryPanel").css("top", "40px");				
				$("#snShopByCategoryPanel").css("visibility", "visible");
				$("#snShopByCategory").mouseenter(function() {
					ghMenuClearTimeout();
					ghHideAllMenus();
				});
				$("#snShopByCategoryPanel").mouseenter(function() {
					ghMenuClearTimeout();
					ghHideAllMenus();
				});
			}
		});
		function snShopByCategoryHideMenu() {
			if (!snHomePage) {
				$("#snShopByCategoryPanel").css("visibility", "");
			}
		}
		</script>
