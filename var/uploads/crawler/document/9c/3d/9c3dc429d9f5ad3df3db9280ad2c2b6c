<!-- STOP FooterDisplay.jsp --><!-- STOP LayoutContainerBottom.jsp --><script type="text/javascript">
	function saveSourceURLParameters() { 
		// initialize the cookie name
		var cookieName = 'SourceURL_10052';
		
		if (getCookie(cookieName) == null) {
			// original landing URL
			var originalLanding_URL = window.location.href;
			
			// referral URL and source flag
			var referral_URL = '';
			var sourceFlag = '';
			if (document.referrer != '') {
				referral_URL = unescape(document.referrer);
				sourceFlag = retrieveDomain(referral_URL);
			} else {
				sourceFlag = 'DirectLoad';
			}
			
			// sets the source URL parameters cookie (if applicable)
			var cm_mmc = '';
			
			// build the source URL parameters string
			var sourceURLParameters = '';
			if (originalLanding_URL) {
				sourceURLParameters += 'OriginalLanding_URL=' + originalLanding_URL;
			}
			if (cm_mmc) {
				sourceURLParameters += '~cm_mmc=' + cm_mmc;
			}
			if (sourceFlag) {
				sourceURLParameters += '~SourceFlag=' + sourceFlag;
			}
			if (referral_URL) {
				sourceURLParameters += '~Referral_URL=' + referral_URL;
			}
			if (sourceURLParameters) {
				setCookie(cookieName, sourceURLParameters);
			}
		}
	}
	function retrieveDomain(url) { 
		var domain = url; 
		// remove the leading and trailing spaces, if any
		domain = domain.replace(new RegExp(/^\s+/),"");
		domain = domain.replace(new RegExp(/\s+$/),""); 
		// replace backslashes with forward slashes, if any
		domain = domain.replace(new RegExp(/\\/g),"/");
		// remove 'http://', 'https://' or 'ftp://', if present
		domain = domain.replace(new RegExp(/^http\:\/\/|^https\:\/\/|^ftp\:\/\//i),"");
		// remove 'www.' from the beginning, if present
		domain = domain.replace(new RegExp(/^www\./i),"");
		// remove complete string from first forward slash onwards
		domain = domain.replace(new RegExp(/\/(.*)/),"");
		// remove '.??.??' or '.???.??' from the end - e.g. '.co.in', '.com.au'
		if (domain.match(new RegExp(/\.[a-z]{2,3}\.[a-z]{2}$/i))) {
	      	domain = domain.replace(new RegExp(/\.[a-z]{2,3}\.[a-z]{2}$/i),"");
		// remove '.??' or '.???' or '.????' from the end - e.g. '.us', '.com', '.info'
		} else if (domain.match(new RegExp(/\.[a-z]{2,4}$/i))) {
	      domain = domain.replace(new RegExp(/\.[a-z]{2,4}$/i),"");
		}
		// return domain name including subdomain(s)
		return domain; 
	} 
	saveSourceURLParameters();
</script>

