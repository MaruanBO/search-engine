<script type="text/javascript">

function postGeoCallback(){
	$.ajax({
		type: 'GET',
		url: 'MiniShopCartView?langId=-1&storeId=10052&catalogId=10551',
		success: function(response) {
			
			var $pencilBannerTopResponseObj = $(response).filter("#pencil-banner-top-response");
			var $pencilBannerTopResponseHtml = $pencilBannerTopResponseObj.html();
			$pencilBannerTopResponseObj.remove();

			var $shValuePropReponseObj = $(response).filter("#shValueProp-response");
			var $shValuePropResponseHtml = $shValuePropReponseObj.html();
			$shValuePropReponseObj.remove();
			
			$("#ghCartContainer").html(response);

			var shValuePropObj = $('#shValueProp');
			if(shValueProp && $shValuePropResponseHtml) {
				shValuePropObj.html($shValuePropResponseHtml);
			}

			var pencilBannerTopObj = $('#pencil-banner-top');
			if(pencilBannerTopObj && $pencilBannerTopResponseHtml) {
				pencilBannerTopObj.html($pencilBannerTopResponseHtml);
			}
			
			Utils.refreshArea("ghMyStoreContainer");
		}
	});
}

$(document).ready(function() {
	if ( $( "#logonPasswordOld" ).length == 0 && $( "#logonPasswordVerify" ).length == 0) {

		$.ajax({
			type: 'GET',
			url: 'GeoLocationView?langId=-1&storeId=10052&catalogId=10551&type=ajax',
			success: function(response) {
				postGeoCallback();
			}, 
			error:function (xhr, ajaxOptions, thrownError) {
				postGeoCallback();
			}
		});
	}
			
	// note: using "on" allows these handlers to persist across AJAX refreshes
	$("#globalHeader").on("mouseenter", "#ghMyStore", function() {
		ghMenuClearTimeout();
		ghHideAllMenus();
		$("#ghMyStore").addClass("ghMyStoreBackground");
		$("#ghMyStorePanel").css("visibility", "visible");
	});
	$("#globalHeader").on("mouseleave", "#ghMyStore", ghMenuSetTimeout);
	$("#globalHeader").on("mouseenter", "#ghMyStorePanel", ghMenuClearTimeout);
	$("#globalHeader").on("mouseleave", "#ghMyStorePanel", ghMenuSetTimeout);

	amplify.subscribe("menu/*/close", function() {
		ghMyStoreHideMenu();
		ghMyAccountHideMenu();
		
		snShopByCategoryHideMenu();
		snShopByBrandHideMenu();
		snNewProductsHideMenu();
		snDealsHideMenu();
		snLearnHideMenu();
		
	});
});

function ghMyStoreHideMenu() {
	$("#ghMyStore").removeClass("ghMyStoreBackground");
	$("#ghMyStorePanel").css("visibility", "");
}
</script>


