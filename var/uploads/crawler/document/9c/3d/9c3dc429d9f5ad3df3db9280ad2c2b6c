	<script type="text/javascript">
	$(document).ready(function() {
		$("#ghMyAccount").mouseenter(function() {
			ghMenuClearTimeout();
			ghHideAllMenus();
			$(this).addClass("ghMyAccount_on");
			$("#ghMyAccountPanel").show();
		});
		$("#ghMyAccount").mouseleave(ghMenuSetTimeout);
		$("#ghMyAccountPanel").mouseenter(ghMenuClearTimeout);
		$("#ghMyAccountPanel").mouseleave(ghMenuSetTimeout);
	});
	function ghMyAccountHideMenu() {
		$("#ghMyAccount").removeClass("ghMyAccount_on");
		$("#ghMyAccountPanel").hide();
	}
	</script>
	
	
	
