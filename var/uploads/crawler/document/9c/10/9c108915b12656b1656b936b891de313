<div id="online-checkin-tab-panel" class="online-checkin tab-panel" data-tab-panel="online-checkin" role="tabpanel" aria-labelledby="online-checkin-tab" aria-hidden="true">

    <fieldset class="membership-selector">
      <div class="membership-display">
        <input type="radio" class="ui-radio" name="user" id="ocNonMembers" checked="checked" data-id="has-no-membership" value="0">
                <label for="ocNonMembers">Non-members</label>
                <input type="radio" class="ui-radio" name="user" id="ocMembers" data-id="has-membership" value="1">
                <label for="ocMembers">Members</label>
