<script type="text/javascript">
  $(function(){


    $('#popular-widget .nav .show').click(function(evt){
      evt.preventDefault();
      var show = $(this).attr('rel');
      $(this).parent().find('li.show').removeClass('on');
      $(this).addClass('on');
      $('div.show-popular').each(function(){
        $(this).hide();
        if( $(this).hasClass(show) ) $(this).show();
      });
    });

    

  });
</script>

