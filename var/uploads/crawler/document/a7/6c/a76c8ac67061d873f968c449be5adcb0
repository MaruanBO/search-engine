		<script type='text/javascript'>
		var iAdvizeCallbacks = {
			onStatusChanged: function(obj){
				if (obj.offline == 1 || obj.busy == 1) {
					$('.idz-chat-online').hide();
					$('.idz-contact').show();
				} else {
					$('.idz-chat-online').show();
					$('.idz-online').show();
					$('.idz-contact').hide();
				}
			},
			onCallStatusChanged: function(obj) {
				if (obj.offline == 1) {
					$('.idz-call-online').hide();
				} else {
					$('.idz-call-online').show();
					$('.idz-online').show();
				}
			}
		};
/*
