function switchFrancais(){
  document.getElementById('EnglishLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'none';
  document.getElementById('FrenchLetter').style.display = 'block';
  document.getElementById('EnglishEmail').style.display = 'none';
  document.getElementById('CanEmail').style.display = 'none';
  document.getElementById('FreEmail').style.display = 'none';
}
function switchEnglish(){
  document.getElementById('FrenchLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'none';
  document.getElementById('EnglishLetter').style.display = 'block';
  document.getElementById('EnglishEmail').style.display = 'none';
  document.getElementById('CanEmail').style.display = 'none';
  document.getElementById('FreEmail').style.display = 'none';
}
function switchCanada(){
  document.getElementById('FrenchLetter').style.display = 'none';
    document.getElementById('EnglishLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'block';
  document.getElementById('EnglishEmail').style.display = 'none';
  document.getElementById('CanEmail').style.display = 'none';
  document.getElementById('FreEmail').style.display = 'none';
}
function switchEngEmail(){
  document.getElementById('EnglishLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'none';
  document.getElementById('FrenchLetter').style.display = 'none';
  document.getElementById('EnglishEmail').style.display = 'block';
  document.getElementById('CanEmail').style.display = 'none';
  document.getElementById('FreEmail').style.display = 'none';
}
function switchCanEmail(){
  document.getElementById('EnglishLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'none';
  document.getElementById('FrenchLetter').style.display = 'none';
  document.getElementById('EnglishEmail').style.display = 'none';
  document.getElementById('CanEmail').style.display = 'block';
  document.getElementById('FreEmail').style.display = 'none';
}
function switchFreEmail(){
  document.getElementById('EnglishLetter').style.display = 'none';
  document.getElementById('CanadianLetter').style.display = 'none';
  document.getElementById('FrenchLetter').style.display = 'none';
  document.getElementById('EnglishEmail').style.display = 'none';
  document.getElementById('CanEmail').style.display = 'none';
  document.getElementById('FreEmail').style.display = 'block';
}
