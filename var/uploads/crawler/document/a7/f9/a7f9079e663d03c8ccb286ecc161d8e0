   if (document.referrer)
      scriptsrc += "&amp;referer=" + escape(document.referrer);
   scriptsrc += "&amp;elem=" + adselems[i].id;
   scriptsrc += "&amp;elemix=" + i + "'";
   scriptsrc += "><" + "/script>";
   document.write(scriptsrc);
  }
}
