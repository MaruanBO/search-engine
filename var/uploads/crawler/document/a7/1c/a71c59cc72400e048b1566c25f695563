<script type="text/javascript">
    var ShoppingCartStatusUpdateControl = function (sandbox) {
        return {
            init: function () {
                sandbox.listen("update-shopping-cart-item-count", this.UpdateItemCount, this);
            },

            UpdateItemCount: function (msg) {
                var itemCount = msg.data.ItemCount;
                if (itemCount > 1) {
                    $("#ctl00_header_shoppingcart_status_ShoppingCartLinkButton").text("Shopping Cart (" + itemCount + " items)");
                }
                else if (itemCount == 1) {
                    $("#ctl00_header_shoppingcart_status_ShoppingCartLinkButton").text("Shopping Cart (" + itemCount + " item)");
                }
                else {
                    $("#ctl00_header_shoppingcart_status_ShoppingCartLinkButton").text("Shopping Cart");
                }
            }
        };
    }

</script>
