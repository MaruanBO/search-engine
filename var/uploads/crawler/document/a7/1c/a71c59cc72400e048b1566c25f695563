    <script type="text/javascript">
        function KeyCheck(event) {
            var keynum;

            keynum = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;

            if (keynum == 13) {
                var btnSignIn = document.getElementById('ctl00_Content_btnSignIn');
                if (typeof (btnSignIn) != 'undefined') {
                    btnSignIn.click();
                    return false;
                }
            }
            return true;
        }

        $(document).ready(function () {
            if ($("#ctl00_Content_tabber_pnlInvitations").is(':visible')) {

