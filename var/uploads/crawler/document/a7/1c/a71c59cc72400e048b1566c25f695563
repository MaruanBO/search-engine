                //Set Order Status tab image to deselected
                $('#ctl00_Content_tabber_btnOrderStatus').attr("src", '/images/home/tab_login_order_statusNew.png');


            }
            else {

                //Set Order Status tab to selected image
                $('#ctl00_Content_tabber_btnOrderStatus').attr("src", '/images/home/tab_login_order_statusSelNew.png');

                //Set Shared Images tab to deselected image
                $('#ctl00_Content_tabber_btnInbox').attr("src", '/images/home/tab_login_inboxSelNew.png');
