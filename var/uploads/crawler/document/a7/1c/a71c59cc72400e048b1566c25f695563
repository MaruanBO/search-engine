<script type="text/javascript">

    function OpenEmaiOffers() {
        window.open('https://secure.ed4.net/CVS/enroll/global/signup.cfm?b=photo&WT.mc_id=PhotoFooter_2010_OptIn', 'gp', 'scrollbars=yes,resizable=yes,width=1024,height=760,status=no,location=no,toolbar=no,menubar=no,top=150,left=350,screenY=0,screenX=0');
    }

    function ShowPromoPopup() {
        document.getElementById('UserPromoPopup').style.display = 'block';

        if (navigator.userAgent.indexOf('MSIE') > 0)
            document.getElementById('UserPromoPopupIFrame').style.display = 'block';
    }

    function HidePromoPopup() {
        document.getElementById('UserPromoPopup').style.display = 'none';

        if (navigator.userAgent.indexOf('MSIE') > 0)
            document.getElementById('UserPromoPopupIFrame').style.display = 'none';
    }

    var eDialogPopupShow = 'false';
    if (eDialogPopupShow == "true") {
        ShowPromoPopup();
    }
    
    
</script>
