<script type="text/javascript">

    $(function () {
        toggle();
        addcss();   
        uploadNavigationTracking();
    });
    
    // ----------------------------------------------------------------------------------- toggle //
    function toggle() {
        jQuery("ul.main_nav li a, ul.main_sub_nav li a").click(function () {
            jQuery(this).parent("ul.main_nav li, ul.main_sub_nav li").addClass("active").siblings().removeClass("active");
            //return false;
        });
    };

    // ----------------------------------------------------------------------------------- last li css //
    function addcss() {
        //jQuery('ul.main_nav li').first().addClass('first');
        jQuery('ul.main_nav li').last().addClass('last');
        jQuery('ul.main_sub_nav li.projects ul li').last().addClass('last');
        jQuery('ul.main_sub_nav li.projects ul li').first().addClass('first');
        //jQuery('ul.main_sub_nav li').first().addClass('first');
    };

    // ----------------------------------------------------------------------------------- uploadNavigationTracking //
    function uploadNavigationTracking() {
        $("#navUploadFromComputer").live('click', function () {
            UploadWebTrend.navigationUploadFromComputer();
        });

        $("#navUploadFromFacebook").live('click', function () {
            UploadWebTrend.navigationUploadFromFacebook();
        });

        $("#navUploadFromPicasa").live('click', function () {
            UploadWebTrend.navigationUploadFromPicasa();
        });
    };

</script>

