<script type="text/javascript">
$(document).ready(function() {

  if (!(!!('ontouchstart' in window) || !!('msmaxtouchpoints' in window.navigator))) {

      $('#brand-panel .shop-links li').hover(
        function () {
            $(this).addClass('mouseover');
            $('#brand-panel .brand-headline .headline').hide();
            var layerDivClass = '.hl' + $(this).attr('id').replace('layer', '');
            $('#brand-panel .brand-headline ' + layerDivClass).show();
            var greyImgClass = '.grey' + $(this).attr('id').replace('layer', '');
            $(greyImgClass).hide();
            var colrImgClass = '.colr' + $(this).attr('id').replace('layer', '');
            $(colrImgClass).show();
        },
        function () {
            $(this).removeClass('mouseover'); 
            $('#brand-panel .brand-headline .headline').hide();
            $('.colrbrand').hide();
            $('.greybrand').show();
        }
      );
  
  }
  
});
</script>

