                $.elementReady('helplinks-start', function() {
                    var browser   = $.ua.browser.name;
                    if (   browser != 'chrome'
                        && browser != 'firefox'
                        || location.search.indexOf('status=404') != -1 ) {
                        $('#helplinks-start').html('&nbsp;');
                    }
                });
                BodyEnded.add(function(){
                    var ico_start = $('#helplinks-start');
                    var browser   = $.ua.browser.name;

                    ico_start.attr('data-browser', browser);
                    $('a.tooltip', ico_start).attr('data-tooltip-query-global','#helplinks-start .infobox[data-browser='+browser+']');

                    $('> a', ico_start).on('click', function() {
                        $('html').addClass('hideSpecialAd');
                    });

                    $(ico_start).on('tooltip-hide', function() {
                        $('html').removeClass('hideSpecialAd');
                    });
                });
            
/*]]>*/
</script>
