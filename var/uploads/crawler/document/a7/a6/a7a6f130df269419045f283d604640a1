            function dynId() {
                for (var newId; !newId || ids[newId]; newId = ((Math.random()*26|0)+10).toString(36) + (Math.random()*1E16).toString(36).replace(/\./g, ''));
                ids[newId] = true;
                return newId;
            }
            function dynTag() {
