 <script type="text/javascript">
//<![CDATA[
	window.addEvent('domready', function() {
		var value;

		// Set the input value if not already set.
		if (!document.id('searchfieldID').getProperty('value')) {
			document.id('searchfieldID').setProperty('value', '');
		}

		// Get the current value.
		value = document.id('searchfieldID').getProperty('value');

		// If the current value equals the default value, clear it.
		document.id('searchfieldID').addEvent('focus', function() {
			if (this.getProperty('value') == '') {
				this.setProperty('value', '');
			}
		});

		// If the current value is empty, set the previous value.
		document.id('searchfieldID').addEvent('blur', function() {
			if (!this.getProperty('value')) {
				this.setProperty('value', value);
			}
		});

		document.id('kovelsearch').addEvent('submit', function(e){
			e = new Event(e);
			e.stop();

			// Disable select boxes with no value selected.
			/*if (document.id('mod-finder-advanced') != null) {
				document.id('mod-finder-advanced').getElements('select').each(function(s){
					if (!s.getProperty('value')) {
						s.setProperty('disabled', 'disabled');
					}
				});
			}*/

			document.id('kovelsearch').submit();
		});

		/*
		 * This segment of code sets up the autocompleter.
		 */
								var url = '/finder?Itemid=435&format=json&task=suggestions.display&tmpl=component';
			var ModCompleter = new Autocompleter.Request.JSON(document.id('searchfieldID'), url, {'postVar': 'q'});
			});
//]]>
</script>
<script language="javascript">

function enable_radio_header()
{
 var val; 	
 var len = document.kovelsearch.pattern.length;
  if(document.getElementById('relevance_header').checked==true)
  val=false;
 else
  val=true;
  for(var cntr=0;cntr<len;cntr++)
   {
	  if(val==true)
	   document.kovelsearch.pattern[cntr].checked=false;
	 document.kovelsearch.pattern[cntr].disabled=val;               		      
   }
   if(val==false)
   document.kovelsearch.pattern[0].checked=true;
}

  function setAction(){
	 
  	  var frm  = document.kovelsearch;
  	  var len = document.kovelsearch.areas.length;
  	  var counter = 0;
  	  var actionstr = '';
  	  
   	  for(var i=0;i<len;i++)
  	     {
  	         if(document.kovelsearch.areas[i].checked)
  	         {
  	              counter++;
  	              var actionstr  =  document.kovelsearch.areas[i].value+','+actionstr;  	              		      
  	         } 	
  	     }
  	    
		if(counter>3)
		{
			document.kovelsearch.areas[0].checked=true;
			//disabled_all();
		}
if(counter==1)
   {		 
   	  for(var i=0;i<len;i++)
  	     {
  	         if(document.kovelsearch.areas[i].checked)
  	         {
  	              var actionname = document.kovelsearch.areas[i].value;
                  break;			      
  	         } 	
  	     }	
   	  if (actionname == 'All') 
  	  	{
				document.getElementById('view').value ="search_result_all";
				document.getElementById('option').value ="com_datalists";
				document.getElementById('table_name').value = 'all' ;
				document.getElementById('searchfieldID').name = 'sobi2Search' ;
				
		} 
		else  if ( actionname == 'datalist_price_guide' || actionname == 'datalist_marks') 
		{
				document.getElementById('view').value ="search_result";
				document.getElementById('option').value ="com_datalists";
				document.getElementById('table_name').value = actionname ;
				document.getElementById('searchfieldID').name = 'sobi2Search' ;
								
		} 
		else  if (actionname == 'Directory') 
		{
				document.getElementById('sobi2Task').value ="axSearch";
				document.getElementById('option').value ="com_sobi2";
				document.getElementById('SobiSearchPage').value ="0";
				document.getElementById('searchfieldID').name = 'sobi2Search' ;
		}
		else  if (actionname == 'Store') 
		{
				
				//document.getElementById('option').value ="com_virtuemart";
				//document.getElementById('Itemid').value ="48";
				//document.getElementById('searchfieldID').name = 'keyword' ;
				//document.getElementById('page').value ="storeSearch";
				//document.getElementById('view').value ="wrapper";
				//document.getElementById('option').value ="com_wrapper";
				//document.getElementById('Itemid').value ="182";
				document.getElementById('searchfieldID').name = 'keywords' ;
				document.kovelsearch.action ="store/advanced_search_result.php";
												
		}
		else  if (actionname == 'Article') 
		{
			
			document.getElementById('searchfieldID').name = 'searchword' ;
			document.getElementById('option').value ="com_search";
			
		}
		
	}
	
	else
	{
			document.getElementById('view').value ="search_result_all";
			document.getElementById('option').value ="com_datalists";
			document.getElementById('table_name').value = actionstr ;
			document.getElementById('searchfieldID').name = 'sobi2Search' ;		
	}	  
		  	 
	}
	function showoption()
	 {
	    document.getElementById('search_option').style.display='block';	
	    document.getElementById('general_search').style.display='block';
	    document.getElementById('advance_search').innerHTML='';
	 }
	 function hideoption()
	 {
	    document.getElementById('search_option').style.display='none';	
	    document.getElementById('general_search').style.display='none';
	    document.getElementById('advance_search').innerHTML='[+]Advanced Search';
	 }
	 function disabled_all()
	 {
	 	return;
	 	var len = document.kovelsearch.areas.length;
	 	if(document.kovelsearch.areas[0].checked==true)
	 	{
			for(var cntr=1;cntr<len;cntr++)
			 {
				  document.kovelsearch.areas[cntr].disabled=true;               		      
					
			 }
		}
		else
		{
			for(var cntr=1;cntr<len;cntr++)
			 {
				  document.kovelsearch.areas[cntr].disabled=false;               		      
					
			 }
			
		}	 
	 	
	 }
	 function check_all()
	 {
	 	var len = document.kovelsearch.areas.length;
	 	var val;
	 	if(document.kovelsearch.areas[0].checked==true)
	 		val=true;
	 	else
	 		val=false;
		for(var cntr=1;cntr<len;cntr++)
		 {
			  document.kovelsearch.areas[cntr].checked=val;               		      

		 }
		
	 }
	 
	 function anchortag(){
	 	document.getElementById('searchfieldID').focus();
	 }
	 
</script>

