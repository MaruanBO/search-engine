        $("#ModelNumber").autocomplete({
            source: function (req, responseFn) {
                var re = $.ui.autocomplete.escapeRegex(req.term);
                var matcher = new RegExp("^" + re, "i");
                var a = $.grep(wordlist, function (item, index) {
                    return matcher.test(item);
                });
                responseFn(a);
            }
        });
    });


    $('#btnStep1').removeClass('button-off').addClass('button-on');
    $('#tdBackButton').hide();

    $(document).ready(function () {
        $('div.hover-box').hover(
            function () { $(this).addClass('product-lookup-button-hover').removeClass('product-lookup-button') },
            function () { $(this).addClass('product-lookup-button').removeClass('product-lookup-button-hover') }
        );

        $('#btnBox1').click(function () {
            SetDisplayMode('1');
        });
        $('#btnBox2').click(function () {
            SetDisplayMode('2');
        });
        $('#btnBox3').click(function () {
            SetDisplayMode('3');
        });

                    
                    //$('#btnBox3, .box-3').hide();
                    

        SetDisplayMode($('#ProductLookupType').val());
    });

    function ChooseProduct() {
        var selectedProductId = $("input[name='selectedProd']:checked").val();
        $('#ProductId').val(selectedProductId);
        $('#ModelId').val('');
        if (selectedProductId != '' && selectedProductId != null) {
            $('#SubmittedButton').val('prodContinue');
            document.forms[1].submit();
        }
        else {
            $('#productOverlayError').show();
        }
    }

    function SetDisplayMode(pProductLookupType) {
        ResetDisplay();
        $('#ProductLookupType').val(pProductLookupType);
        if (pProductLookupType == '1') {
            $('.box-1').show();
        }
        else if (pProductLookupType == '2') {
            $('.box-2').show();
        }
        else if (pProductLookupType == '3') {
            $('.box-3').show();
        }
        if (pProductLookupType != null && pProductLookupType != '') {
            $('.box-right').show();
            $('#PrevNextButtons').show();
            $('#btnBack').attr('href', 'https://prod-registration.com/en-us/Philips/RegisterProduct');
        }
    }

    function ResetDisplay() {
        $('.box-right').hide();
        $('.box-1').hide();
        $('.box-2').hide();
        $('.box-3').hide();
        $('#btnBack').hide();
        $('#PrevNextButtons').hide();
    }

    function CheckRadio(pID) {
        $(pID).attr("checked", true);
    }
</script>

