<script>
			function toggleFanBox()	{
				var fanBox=document.getElementById('fbFanBox');

				if(fanBox.style.display=='none')
				{
					fanBox.style.display='';
				}
				else
				{
					fanBox.style.display='none';
				}
			}
		</script>

