			<script type="text/javascript">
				/*
				 * Create the global edonor object. Use this object to store
				 * future edonor objects (to prevent over-populating the global
				 * variable scope).
				 */
				if ( !this.edonor )
				{
					var edonor = {};
				}
				
				//Define the current module for the request
				edonor.module = "donor";

				//Create the object for storing localization data
				if ( !this.edonor.i18n )
				{
					edonor.i18n = {};
				}
				
				/*
				 * Only attempt to create an instance of the CurrentLocale class
				 * if it is available. This is used in case the external JS
				 * files are disabled in the CSR (we don't want to cause a JS
				 * error).
				 */
				if ( this.CurrentLocale ) {
					edonor.i18n.CurrentLocale = new CurrentLocale( "en_US" );
				}
			</script>
		

			
			<script type="text/javascript">
				// Create the tooltips only on document load
				// Documentation http://craigsworks.com/projects/qtip/
				$(document).ready(function()
				{
				
				var titleTextColor = utiljs.getCSS('.tooltipTitle','color');
				var titleBGColor = utiljs.getCSS('.tooltipTitle','backgroundColor');
				var outlineColor = utiljs.getCSS('.tooltipBody','borderTopColor');
				var bodyTextColor = utiljs.getCSS('.tooltipBody','color');
				var bodyBGColor = utiljs.getCSS('.tooltipBody','backgroundColor');
				
		
		
		 			$.fn.qtip.styles.thestyle = { // Last part is the name of the style
						 width: { min: 350, max: 350 }
						, background: bodyBGColor
						, color: bodyTextColor
						, border: { width: 1, radius: 0, color: outlineColor }
						, title: { background: titleBGColor, color: titleTextColor, nowrap:'nowrap' }
						
							// Now an object instead of a string
							, tip: { corner: 'leftMiddle' // We declare our corner within the object using the corner sub-option
									, color: outlineColor
									, size: { x: 10 // Be careful that the x and y values refer to coordinates on screen, not height or width.
											, y : 10 // Depending on which corner your tooltip is at, x and y could mean either height or width!
											} 
									}
						
						, overflow: 'auto'
					} 
		 
				      $('#info_did').qtip(
				      {
				         content: {     	
				         		
					         	// Set the text to an image HTML string with the correct src URL to the loading image you want to use
					         	  	url: '/assets/html/remember_me_text.html'
					         	
							, title: {
								  text: 'Remember Me'
								 // text: $('#elementID').attr('title') // Give the tooltip a title using each elements text 
								, button: '<img onclick="return false;" style="border:0;" src="/Assets/images/dhtmlPopups/closebutton.gif" alt="Close" />'
				            }
				         }
				        , position: {
				        
							corner: { target: 'rightMiddle', tooltip:'leftMiddle'}
							, adjust: {screen: false}
				         }
		
				        , show: {
				     		  delay: 30
				         	, when: { event: 'click' }
				         	, effect: { type: 'fade', length: 100 }
				        	, solo: true // Only show one tooltip at a time
				         }
				        , hide: {
							  when: { event: 'unfocus' }
							, effect: { type: 'fade', length: 300 }
				         }
						, style:  'thestyle'
						, api: {
							beforeShow: function(){ this.updatePosition() }
		                   }
					})
				  });
			</script>
			

