<script type="text/javascript">
    require(['jquery'], (function ($) {

        maxHeight = 0;
        $(".fixed-height").each(function () {
            if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
        });

        $(".fixed-height").height(maxHeight + 50);

        maxHeight = 0;
        $(".description").each(function () {
            if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
        });

        $(".description").height(maxHeight);

        var maxHeight = 0;

        $(".pricingCols").each(function () {
            if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
        });

        $(".pricingCols").height(maxHeight + 15);
    }));
</script>
