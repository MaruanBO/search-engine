function SelectTab(selectid,showdiv,showheader,tabcount,isselectbg,isnotselectbg){
	for(i=0;i<tabcount;i++){
		if(i==selectid) {
			document.getElementById(showheader[i]).style.backgroundImage="url("+isselectbg+")";
			document.getElementById(showdiv[i]).style.display="";
		}
		else{
			document.getElementById(showheader[i]).style.backgroundImage="url("+isnotselectbg+")";
			document.getElementById(showdiv[i]).style.display="none";
 
		}
	}
}  
function ChangeTab(selectid,n)
{
    var showdiv=new Array("rdcxtable_a_"+n,"rdcxtable_b_"+n,"rdcxtable_c_"+n,"rdcxtable_d_"+n,"rdcxtable_e_"+n);
    var showheader=new Array("rdcxtd_a_"+n,"rdcxtd_b_"+n,"rdcxtd_c_"+n,"rdcxtd_d_"+n,"rdcxtd_e_"+n);
    var isselectbg="none";
    var isnotselectbg="none";
    var tabcount=5;
    SelectTab(selectid,showdiv,showheader,tabcount,isselectbg,isnotselectbg);
}
</SCRIPT>

