      <script>
        $('#hero-demo-btn').click(function(e){
          $('#request-name').focus();
          return false;
        });

        var requestingDemo = false;
        $('#request-demo-form button').click(function(e){
          e.preventDefault();

          if (requestingDemo) return;

          $r = $('#request-demo-form');
          $r.find('.msg').hide();

          var iName = $('#request-name').val();
          var iCompany = $('#request-company').val();
          var iEmail = $('#request-email').val();
          var iPhone = $('#request-phone').val();

          if (!iName || !iCompany || !(iEmail || iPhone)){
            $r.find('.msg').hide();
            $('#request-a-demo-error').text('Please enter all fields.').show();
            return;
          }

          $('#request-a-demo-doing').show();
          requestingDemo = true;

          $xhr = $.ajax({
            url: 'http://www.fanplayr.com/wp-content/themes/fanplayr3/demo-email.php',
            data: {
              name: iName,
              company: iCompany,
              email: iEmail,
              phone: iPhone
            }
          });
          $xhr.done(function(){
            $r.find('.msg').hide();
            $('#request-a-demo-done').show();
            requestingDemo = false;
            $('#request-demo-form input').val('');
          });
          $xhr.fail(function(){
            $r.find('.msg').hide();
            $('#request-a-demo-error').text('Sorry there was an error sending your details. Please try again.').show();
            requestingDemo = false;
          });

        });

        var sourceSwap = function(){
          var $this = $(this);
          var newSource = $this.data('alt-src');
          $this.data('alt-src', $this.attr('src'));
          $this.attr('src', newSource);
        };
        $('#cart-wrap img').hover(sourceSwap, sourceSwap);

        var formatNumber = function(n) {
          var parts = n.toString().split(".");
          parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
          return parts.join(".");
        };

		var mil = 1000000;
		var msADay = 86.4 * mil;
		var dataPerSession = 6.1 * 42;
		var sessionsAtStart = 163 * mil;
		var sessionsPerDay = 1 * mil;
		var couponsAtStart = 17.8 * mil;
		var couponsPerDay = sessionsPerDay * .11;
		var daysSinceStart = (Date.now() - 1423522303363)/msADay;
		
        var counterPoints = sessionsAtStart * dataPerSession + Math.floor(daysSinceStart * sessionsPerDay * dataPerSession);
        var counterOffers = couponsAtStart + Math.floor(daysSinceStart * couponsPerDay);
		
		var msUpdate = 250;
		var dataPerUpdate = msUpdate / msADay * sessionsPerDay * dataPerSession;
		var couponsPerUpdate = msUpdate / msADay * couponsPerDay;
		
        setInterval(function(){
          if (Math.random() > .5) return;
          counterPoints += Math.random()*(dataPerUpdate/2) + (dataPerUpdate*1.5);
          counterOffers += Math.random()*(couponsPerUpdate/2) + (couponsPerUpdate*1.5);
          $('#counter-points').text(formatNumber(Math.floor(counterPoints)));
          $('#counter-offers').text(formatNumber(Math.floor(counterOffers)));
        }, msUpdate);
      </script>
