<script type="text/javascript">
var calendar_oneway = jQuery.noConflict();
calendar_oneway(function() {
	var dates = calendar_oneway('#fdt, #tdt').datepicker({
		defaultDate: "+0",
		changeMonth: false,
		numberOfMonths: 1,
		onSelect: function(selectedDate) {
			var option = this.id == "fdt" ? "minDate" : "maxDate";
			var instance = calendar_oneway(this).data("datepicker");
			var date = calendar_oneway.datepicker.parseDate(instance.settings.dateFormat || calendar_oneway.datepicker._defaults.dateFormat, selectedDate, instance.settings);
			dates.not(this).datepicker("option", option, date);
		}
	});
});
</script>

