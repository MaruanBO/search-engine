<script language="javascript">

function expMonthChange(year)
{
	var str;
	var mnth=document.getElementById('exp_month').value;
	
	if(year == 2015)
	{
		str="<select name='exp_month' id='exp_month' class='dropdown dpwidth1'><option value='-'>-</option> <option value='2'>2</option><option value='3'>3</option><option value='4'>4</option><option value='5'>5</option><option value='6'>6</option><option value='7'>7</option><option value='8'>8</option><option value='9'>9</option><option value='10'>10</option><option value='11'>11</option><option value='12'>12</option> </select>";

		old_mnth = mnth;
		mnth = 02;

		if(old_mnth > mnth){
			mnth = old_mnth;
		
		}else{
			
			mnth = '-';

		}
		
	}
	else
	{
		str="<select name='exp_month' id='exp_month' class='dropdown dpwidth1'><option value='-'>-</option><option value='1'>1</option><option value='2'>2</option><option value='3'>3</option><option value='4'>4</option><option value='5'>5</option><option value='6'>6</option><option value='7'>7</option><option value='8'>8</option><option value='9'>9</option><option value='10'>10</option><option value='11'>11</option><option value='12'>12</option></select>";
	}
	document.getElementById('expiryDate').innerHTML = str;
	document.getElementById('exp_month').value=mnth;
}

</script>


