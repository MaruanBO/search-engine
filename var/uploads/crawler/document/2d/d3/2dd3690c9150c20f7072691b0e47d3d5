var load_cmc = function(){linktracker_init(15272774,0,2);};
if ( typeof addLoadEvent != 'undefined' ) addLoadEvent(load_cmc);
else load_cmc();
</script>


<script type="text/javascript">
$(document).ready(function() {
	AD_NAV = {
	    changed : false,
	    change_page: function( inc ){
	        var path = window.location.pathname;
	        if (p=path.match(/\/page\/(\d+)\/?$/)) {
	            var p = parseInt(p[1], 10) + inc;
	            var s = p > 1 ? '/page/'+p+'/' : '';
	            window.location.pathname = path.replace(/\/page\/(\d+)\/?$/, s);
	            $('#navkeyLoader').fadeIn('slow');
	        } else if (inc > 0)
	            window.location.pathname = path + '/page/2/';
	            $('#navkeyLoader').fadeIn('slow');
	    },
		o: function(){
	        var dest = jQuery('.postSelected > h3 a').attr('href');
	        if(typeof(dest)!="undefined"){
	            window.location = dest;
	            // site_ad.trackevent('navkey','key','o');
	        }
	    },
		j: function(){
	        AD_NAV.changed = false;
	        jQuery('.mpost').each( function(i,el) {
	            jQuery(el).removeClass('postSelected');
	            var doc_top = jQuery(document).scrollTop();
	            var elem_top= jQuery(el).offset().top;
	            if( !AD_NAV.changed && parseInt( elem_top,10) > parseInt( doc_top,10)) {
	                if(doc_top + jQuery(window).height() >= jQuery(document).height()){
	                    AD_NAV.change_page(1);
	                }
	                jQuery('html,body').animate({scrollTop: elem_top}, 'fast');
	                jQuery(el).addClass('postSelected');
	                AD_NAV.changed = true;
	                // site_ad.trackevent('navkey','key','j');
	            }
	        });
	        if ( !AD_NAV.changed ) {
	            AD_NAV.change_page(1);
	        }
	    },
		k: function(){
	        AD_NAV.changed = false;
	        jQuery('.mpost').each( function(i,el) {
	            jQuery(el).removeClass('postSelected');
	            if( !AD_NAV.changed && parseInt(jQuery(el).offset().top,10) >= parseInt(jQuery(document).scrollTop(),10)) {
	                if (i > 0) {
	                    var d = jQuery('.mpost').get(i-1);
	                    	jQuery('html,body').animate({scrollTop: jQuery(d).offset().top}, 'fast');
	                    jQuery(d).addClass('postSelected');
	                    AD_NAV.changed = true;
	                } else {
	                    AD_NAV.change_page(-1);
	                }
	            }
	        });
	        if ( !AD_NAV.changed ) {
	            AD_NAV.change_page(-1);
	        }
	    }
	};
	jQuery(document).ready(function() {
	    jQuery().navkey({
	        j: function(){ AD_NAV.j(); },
	        k: function(){ AD_NAV.k(); },
	        o: function(){ AD_NAV.o(); }
	    });
	});
});
</script>


<!-- Twitter  -->
<script type="text/javascript">
	window.twttr = (function (d,s,id) {
		var t, js, fjs = d.getElementsByTagName(s)[0];
		if (d.getElementById(id)) return; js=d.createElement(s); js.id=id;
		js.src="//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs);
		return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f) } });
	}(document, "script", "twitter-wjs"));
</script>

<!-- Facebook -->
<div id="fb-root"></div>
<script type="text/javascript">
	// Callback function
	function showface(){
		jQuery('.showface').fadeIn();
	};
	/* Facebook Api */
	window.fbAsyncInit = function(){
		FB.init({appId: '161604560536078', status: true, cookie: true, xfbml: true});
		FB.Event.subscribe('xfbml.render', function(){showface();});
	};
	(function() {
		var e = document.createElement('script'); e.async = true;
		e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
		document.getElementById('fb-root').appendChild(e);
	}());
</script>






