function setChanHeaderWidth() 
{
    var wWidth;
    if(navigator.appName == "Netscape") { wWidth = window.innerWidth; }
        else if(navigator.appName == "Microsoft Internet Explorer") { wWidth = document.body.clientWidth; }
    if (wWidth < 968)  { document.getElementById("headerContainer").style.width = 968+"px";document.getElementById("bannerContainer").style.width = 968+"px"; } 
        else { document.getElementById("headerContainer").style.width = "100%"; document.getElementById("bannerContainer").style.width = "100%"; }
}
var netHostUrl = "https://client.schwab.com";
window.CHANCONF = {
    CMSServer: "https://www.schwab.com:443",
    NETServer: "https://client.schwab.com"
};
