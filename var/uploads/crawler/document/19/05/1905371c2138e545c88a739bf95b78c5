    <script type="text/javascript">

        $(document).ready(function () {

            $("#SelectedRegion").change(function () {
                updateCountries();
              
            });
            countriesOnChange();            
        });

        function countriesOnChange() {
            $("#SelectedCountry").change(function () {
                updateRegion();
                updateTripType();
            });

        }

        function findTrip() {
            var selectedYear = document.getElementById("SelectedYear");
            var yearValue = selectedYear.options[selectedYear.selectedIndex].value;

            var selectedRegion = document.getElementById("SelectedRegion");
            var regionValue = selectedRegion.options[selectedRegion.selectedIndex].value;

            var selectedCountry = document.getElementById("SelectedCountry");
            var countryValue = selectedCountry.options[selectedCountry.selectedIndex].value;
            
            var selectedTripType = document.getElementById("SelectedTripType");
            var tripTypeValue = selectedTripType.options[selectedTripType.selectedIndex].value;

            $.ajax({
                url: '/trips/home-search',
                type: 'post',
                data:
                {
                    year: yearValue,
                    region: regionValue,
                    country: countryValue,
                    tripType: tripTypeValue
                },
                success: function (response) {

                    window.location = response;
                }
            });

            return false;
        };
        
        function updateRegion() {
            var selectedCountry = document.getElementById("SelectedCountry");
            var countryValue = selectedCountry.options[selectedCountry.selectedIndex].value;

            $.ajax({
                url: '/trips/get-region',
                type: 'get',
                data:
                {
                    country: countryValue
                },
                success: function (response) {
                    var regionValue = response;
                    $('#SelectedRegion option:contains(' + regionValue + ')').prop({ selected: true });
                }
            });

            return false;
        };
        
        function updateCountries() {
            var selectedRegion = document.getElementById("SelectedRegion");
            var regionValue = selectedRegion.options[selectedRegion.selectedIndex].value;

            $.ajax({
                url: '/trips/get-countries',
                type: 'get',
                data:
                {
                    region: regionValue
                },
                success: function (response) {
                    $("#countries-field").html(response);
                    countriesOnChange();
                }
            });

            return false;
        }

        function updateTripType() {            
            var selectedCountry = document.getElementById("SelectedCountry");           
            var countryValue = selectedCountry.options[selectedCountry.selectedIndex].value;
           

            $.ajax({
                url: '/trips/get-triptype',
                type: 'get',
                data:
                    {                  
                        country: countryValue                       
                    },
                success: function (response) {
                    $("#triptypes-field").html(response);
                }


            });
            return false;
        }
        
    </script>
</div>

    </div>
</section>

