<script>

    //Microsoft jQuery Unobtrusive Validation brokes when the item is added dynamically to the UI
    //this problem can be solved by manually loading validation
    function loadValidation(selector) {
        if ($(selector).length > 0) {
            var form = $(selector).removeData("validator").removeData("unobtrusiveValidation");

            if ($.validator && $.validator.unobtrusive) {
                $.validator.unobtrusive.parse(form);
            }
        }
    }

    $("#global-rac-header").on('click', function () {
        loadValidation(".sales-form");
    });
</script>
