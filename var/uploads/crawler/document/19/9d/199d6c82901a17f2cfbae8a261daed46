     <script type="text/javascript">
         $.noConflict();
         // Create the tooltips only on document load
         jQuery(document).ready(function($) {
         
         
             // Use the each() method to gain access to each elements attributes
         jQuery('.mtbpopup').each(function() {
                 var popupWidth = 200; // min popup width set to 200
                 var vPos = 0;
                 var hPos = 0;
                 var targetPosition = 'bottomRight';
                 var popupHtml = $(this).attr("title").split('[<')[1];
                 if (popupHtml == undefined) {
                     popupHtml = 'div width=80 targetPosition=topLeft vPos=0 hPos=0>Error!</div> ]]>';
                 }
                 popupHtml = popupHtml.replace(']]>', '');
                 popupHtml = '<' + popupHtml;               
                 popupWidth = parseInt($(popupHtml).attr("width").replace('px', ''), 10);
                 vPos = parseInt($(popupHtml).attr("vPos").replace('px', ''), 10);
                 hPos = parseInt($(popupHtml).attr("hPos").replace('px', ''), 10);
                 targetPosition = $(popupHtml).attr("targetPosition");

                 var tooldata = targetPosition.split(";");
                 var cor1 = tooldata[0];
                 var Ttip = tooldata[1];
                 
		         if (cor1 == "TopLeft" || cor1 == "BottomLeft") {
			        hPos = (hPos * -1);
		         }
		         if ($(this).is("img")) {
			        cor1 = "leftMiddle";
		         }

                 $(this).attr("popupHtml", $(this).attr("title"));
                 $(this).attr("title", '');
                 $(this).qtip(
                    {
                        content:
                      {
                          text: popupHtml  // Add .clone() if you don't want the matched elements to be removed, but simply copied
                      },
                        position: {
                            corner: {
                                target: cor1, // Position the tooltip above the link
                                tooltip: Ttip

                            },
                            adjust: {
                                x: hPos,
                                screen: true // Keep the tooltip on-screen at all times
                            }
                        },
                        // Only show one tooltip at a time
                        show: {
                            effect: { type: 'slide' },
                            when: 'mouseenter',
                            solo: true
                        },
                        hide: { when: 'mouseout', fixed: true, delay:100 },
                        style: {
                            classes: '',
                            widget: false,
                            width: popupWidth,
                            background: 'white',
                            "word-wrap": "break-word",
                            //background: '#e6ebef', blue background
                            //height: popupWidth,
                            tip: {
                                corner: true,
                                mimic: false,
                                //width: popupWidth,
                                height: 500,
                                border: true,
                                offset: 0,
                                size:
                                {
                                    x: hPos,
                                    y: vPos
                                }
                            },
                            border: {
                                width: 1,
                                radius: 1,
                                color: '#007856'
                                //color:'#003359' blue border
                            },

                            name: 'light'
                        }


                    })

             });
         
         });
