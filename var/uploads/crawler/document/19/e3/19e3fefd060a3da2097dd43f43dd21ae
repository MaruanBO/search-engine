<script type="text/javascript">
(function($){
$(document).ready(function() {
$('#submit').click(function(){
var email = $('#email').val();
if(validateEmail(email)==false){
$('#errors').html('Please enter a valid email address');
return false;
}
else if (validateEmail(email)==true){
$('#errors').html('<span style="color:#4fb000;">Thank you! We look forward to telling you about our special offers!</span>');
}
});
});
function validateEmail(test_email) {
var emailReg = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
if( !emailReg.test( test_email ) ) {
return false;
} else {
return true;
}
}
})(jQuery);
</script></div>
