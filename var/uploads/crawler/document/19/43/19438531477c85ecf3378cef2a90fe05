<script type="text/javascript">

	var submitFormComponent = ( function() {

	    var esCookieName = "emailSubmit";
	    var emailRegex = /^[_A-Za-z0-9-\+]+(\.[_A-Za-z0-9-]*)*@[A-Za-z0-9-]+(\.[A-Za-z0-9]+)*(\.[A-Za-z]{2,})$/;
	    var zipCodeRegex = /(^[ABCEGHJKLMNPRSTVXY]\d[ABCEGHJKLMNPRSTVWXYZ]( )?\d[ABCEGHJKLMNPRSTVWXYZ]\d$)|(^\d{5}$)/;

	    function throwEmailExistError () {
            cc$('#email-subscribe-popup .email-exist-error').show();
            cc$('#email-subscribe-popup input[name="email"]')
                .focus()
                .addClass('error');
        }
        function throwResponsysError () {
	        cc$('#email-subscribe-popup .service-unavailable-error').show();
	    }
	    function throwEmailInvalidError () {
	        cc$('#email-subscribe-popup .email-invalid-error').show();
	        cc$('#email-subscribe-popup input[name="email"]')
                .focus()
                .addClass('error');
	    }
	    function throwZipCodeError () {
	        cc$('#email-subscribe-popup .zipcode-invalid-error').show();
	        cc$('#email-subscribe-popup input[name="zip"]')
                .focus()
                .addClass('error');
	    }
	    function throwDestinationError () {
	        cc$('#email-subscribe-popup .destination-invalid-error').show();
            cc$('#main-email-form .select-container').addClass('error');
	    }
	    function hideErrors () {
            cc$(
                '#email-subscribe-popup input[name="email"], ' +
                '#email-subscribe-popup input[name="zip"], ' +
                '#main-email-form .select-container'
            ).removeClass('error');

	        cc$(
	            '#email-subscribe-popup .email-exist-error, ' +
	            '#email-subscribe-popup .email-invalid-error, ' +
	            '#email-subscribe-popup .zipcode-invalid-error, ' +
	            '#email-subscribe-popup .destination-invalid-error'
	        ).hide();
	    }

	    function validateEmailForm () {
	        var $form = cc$('#main-email-form');
	        var email = $form.find('input[name="email"]').val();
	        var zipCode = $form.find('input[name="zip"]').val();
	        var destination = $form.find('select[name="destination"]').val();
            var destinationIsSelected = $form.find('select[name="destination"]').hasClass('selected-value');

	        if (!emailRegex.test(email)) {
	            throwEmailInvalidError();
	            return false;
	        }

	        if (!zipCodeRegex.test(zipCode)) {
	            throwZipCodeError();
	            return false;
	        }

	        if (!destinationIsSelected || !destination || destination == -1) {
	            throwDestinationError();
	            return false;
	        }

	        return true;
	    }

	    function saveCookie(emailSubmitJson){
	        var expirationDate = new Date();
	        expirationDate.setDate(expirationDate.getDate() + 30);
	        cc$.cookie(esCookieName, JSON.stringify(emailSubmitJson), {expires: expirationDate, path: "/"});
	    }

	    function notShowPopup(){
	        var emailSubmitJson = JSON.parse(cc$.cookie(esCookieName));
	        emailSubmitJson.show = false;
	        saveCookie(emailSubmitJson);
	    }

	    function sendFormData () {
	        var data = cc$('#main-email-form').serialize();

	        cc$.get('/mvc/responsys/signup?' + data, function (res) {
                if (typeof res != 'object') {
                    try {
                        res = JSON.parse(res);
                    }
                    catch (parseException) {
                        return throwResponsysError();
                    }

                }

                if (res.error) {
                    throwResponsysError();
                }
                else if (res.exist) {
                    throwEmailExistError();
                }
                else if (res.validationError == 'email') {
                    throwEmailInvalidError();
                }
                else if (res.validationError == 'zip') {
                    throwZipCodeError();
                }
                else {
                    toggleFormSubmitedState();
                    notShowPopup();
                }
            }).onerror = function (err) {
                console.error("error retrieving responsys");
                console.error(err);
                throwResponsysError();
            };
	    }

	    function resizeHandler () {
	        var newTopOffset = Math.max(0, ((cc$(window).height() - 400) / 2));
	        cc$('#email-subscribe-popup .email-container').css('top', newTopOffset);
	    }

        function toDefaultView () {
            cc$('#email-subscribe-popup .email-container').removeClass('hidden-container');
            cc$('#email-subscribe-popup .email-container.email-thanks').addClass('hidden-container');
        }

        function fieldsToDefaults () {
            cc$('#email-subscribe-popup input[name="email"], #email-subscribe-popup input[name="zip"]').val('').change().blur();
            cc$('#email-subscribe-popup select[name="destination"] option[value="-1"]').attr('selected', true);
            cc$('#email-subscribe-popup select[name="destination"]').change().removeClass('selected-value');
        }

	    function hideEmailDealPopup (event) {
	        cc$('#email-subscribe-popup').fadeOut(1000, function () {
                toDefaultView();
                fieldsToDefaults();
            });
            notShowPopup();
	    }

	    function showEmailSubmitPopup () {
	        cc$('#email-subscribe-popup').fadeIn(1000);
	    }

	    function toggleFormSubmitedState () {
	        cc$('#email-subscribe-popup .email-container').toggleClass('hidden-container');
	    }

	    function formSubmit (e) {
	        e.preventDefault();
	        if (!validateEmailForm()) {
	            return false;
	        }
	        sendFormData();
	        return false;
	    }

        function selectPlaceholderFix() {
            cc$('#email-subscribe-popup .ie-fixSelect>select').each(function(){
                cc$(this)
                    .change(function(){
                        var val = cc$(this).val();
                        var text = cc$('option:selected',this).text();
                        if ((val.length == 0 || val == -1) && text.length == 0) {
                            text = cc$('option:selected',this).attr('placeholder');
                        }
                        cc$(this).next().text(text);
                    })
            });

            var isIE = tripSearchSharedObject.isIE();
            if (isIE) {
                cc$('#email-subscribe-popup select[name="destination"] option[value="-1"]').attr('disabled', true);
            }
        }

        function inputPlaceholderFix() {
            var isIE = tripSearchSharedObject.isIE();
            if (!isIE || isIE.version > 9) {
                return;
            }

            cc$('#email-subscribe-popup [placeholder]').focus(function() {
              var input = cc$(this);
              if (input.val() == input.attr('placeholder')) {
                input.val('');
                input.removeClass('placeholder');
              }
            }).blur(function() {
              var input = cc$(this);
              if (input.val() == '' || input.val() == input.attr('placeholder')) {
                input.addClass('placeholder');
                input.val(input.attr('placeholder'));
              }
            }).blur();
        }

        function initializeComponent () {
            selectPlaceholderFix();
            inputPlaceholderFix();
        }

        function initializeCookie () {
            var origin;
            if (!location.origin) {
                origin = location.hostname;
            }
            else {
                origin = location.origin;
            }

            var cookieValue = cc$.cookie(esCookieName);
            var emailSubmitJson = null;
            if (cookieValue) {
            	emailSubmitJson = JSON.parse(cookieValue);
            }
            var currentUri = origin.concat(location.pathname);

            if (emailSubmitJson !== null){
                if (emailSubmitJson.uri !== currentUri && emailSubmitJson.show){
                    setTimeout(showEmailSubmitPopup, 10000);
                }
            } else {
                emailSubmitJson = {
                    uri: currentUri,
                    show: true
                };
                saveCookie(emailSubmitJson);
            }
        }

		return {
			showPopup: showEmailSubmitPopup,
            resizeHandler: resizeHandler,
            hidePopup: hideEmailDealPopup,
            hideErrors: hideErrors,
            initializeComponent: initializeComponent,
            formSubmit: formSubmit,
            initializeCookie: initializeCookie
		};
	})();

    cc$(function () {
        submitFormComponent.resizeHandler();
        cc$(window).resize(submitFormComponent.resizeHandler);
        cc$('#email-subscribe-popup .email-container .email-header>.icon').click(function(event){
            submitFormComponent.hidePopup(event);
        });
        cc$('#main-email-form').submit(submitFormComponent.formSubmit);
        cc$('#email-subscribe-popup input, #email-subscribe-popup select').change(submitFormComponent.hideErrors);
        cc$('#email-subscribe-popup select').change(function () {
            cc$(this).addClass('selected-value');
        });
        cc$('#email-subscribe-popup input[name="zip"]').keydown(function (e) {
            if (cc$(this).val().length >= 7 && (/[a-zA-Z0-9 ]/).test(String.fromCharCode(e.keyCode)) && (this.selectionEnd - this.selectionStart) == 0) {
                e.preventDefault();
                return false;
            }
        });

        submitFormComponent.initializeComponent();
        submitFormComponent.initializeCookie();

    });

</script>

        </div>

        <script type="text/javascript">

            var ieBrowser = tripSearchSharedObject.isIE();
            if ((cc$.browser && cc$.browser.msie == true && parseInt(cc$.browser.version) <= 8) || (ieBrowser && ieBrowser.version <= 8)){
                cc$( document ).ready( function(){
                    if (cc$.cookie("ieUpgradeMessageShown") == null){
                        cc$.cookie("ieUpgradeMessageShown","1", { path: '/' });
                        var dialogDiv = cc$(
                        	'<div id="over" title="Outdated Browser Detected - Please Upgrade">' +
                        		'<div class="">To get the best possible experience using our website we recommend that you upgrade to a newer version of Internet Explorer or use another web browser</div>' +
