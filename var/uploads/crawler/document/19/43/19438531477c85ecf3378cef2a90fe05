<script type="text/javascript" charset="utf-8">

var departureUriPrefix = "/deals/by-airport/";
var departureAirportParam = "departureAirport";

function updateDepartureAirportInCookieAndForward(id) {
 var airportCode = $("#"+id).val();
    if (airportCode == '')
        return;
    storeSearchCookie();
 updateCookie('departure', airportCode);
 window.location = departureUriPrefix + makeNatural($("#"+id+" option:selected").text()) + ".html?" + departureAirportParam+"=" +  airportCode;
}

var destinationUriPrefix = "/deals/by-destination/";

function updateDestinationAirportInCookieAndForward(id) {
 var destCode =  $("#"+id).val();
    if (destCode == '')
        return;
    var destName =  makeNatural($("#"+id+" option:selected").text());
 storeSearchCookie();
 updateCookie('destination',destCode);
 window.location = destinationUriPrefix + destName + "/" + destCode+ ".html";
}
</script>

<div class="oneColumn" gaTrackItem="Quick Search">
    <div class="header orange large"><h2>Quick Search</h2></div>
