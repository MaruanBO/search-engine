  function toTopClick(e) {
    e.preventDefault();
    $("html, body").animate({
        scrollTop : 0
    }, 1e3);
  }

  $(window).scroll(function() {
    if ($(this).scrollTop() > 50) {
      $("#pfToTop").fadeIn("slow");
    } else {
      $("#pfToTop").fadeOut("slow");
    }
  });

  $("#pfToTop").click(toTopClick);

