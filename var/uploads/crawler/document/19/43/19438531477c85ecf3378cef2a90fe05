    var tripSearchFormToSubmit;

    function trackTripSearchCTA() {
        var valid;
        if (tripSearchFormToSubmit[0] == $('#osbForm')[0]) {
            valid = validateOsbForm();
        } else {
            // new osb form was already validated.
            valid = true;
        }
        if (!valid) {
            return;
        }

        var sc = s_gi('ccacheapcaribbeandev');
        sc.linkTrackVars = 'events';
        sc.linkTrackEvents = 'event57';
        sc.events = "event57";
        sc.tl(true, 'o', 'tripsearch_cta', null, function () {
            tripSearchFormToSubmit.submit();
        });
    }
</script>
