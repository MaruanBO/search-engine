<!-- JAVASCRIPT SECTION -->

<script type="text/javascript">

    var minicart_open = false;
    var is_dressbar = 0;
    var content = '';

    function closeMiniCart() {
        if (YAHOO.speedfc.cart.add.cart_overlay !== null) {
            YAHOO.speedfc.cart.add.cart_overlay.hide();
            YAHOO.speedfc.cart.add.cart_overlay.setBody('');
            minicart_open = false;
        }
    }

    loadMiniCart();
    //jQuery(window).load(
    function loadMiniCart(){
        if (!minicart_open) {
            var loading = "<div style='text-align: center'><img src='/images/spinner.gif' alt='loading...' /></div>";
            YAHOO.speedfc.cart.add.prepareBody(loading, false, false);
            minicart_open = false;

        }

        jQuery.ajax({
            type: "POST",
            url: "/cart/headerCart",
            data: {is_dressbar: 0},
            success: function (data, textStatus, jqXHR) {
                YAHOO.speedfc.cart.add.cart_overlay.setBody(data);
                content = data;
            }
        })
    }
    //);

    jQuery("#minicart-header-link").hover(
        function (e) {
            if (!minicart_open) {
                var loading = "<div style='text-align: center'><img src='/images/spinner.gif' alt='loading...' /></div>";
                YAHOO.speedfc.cart.add.showCart(content, false, true);
                minicart_open = true;

            }
        },
        function (e) {
            //closeMiniCart();
        }
    );

    /*
    jQuery("#minicart-header-link").click(function (e) {
        e.preventDefault();
        e.stopImmediatePropagation();
    });
    */

    function cartHeaderMouseOut(event) {
        closeMiniCart();
    }

    function cartHeaderClick(event){
        event.stopImmediatePropagation();
    }

    jQuery("body").click(function(e){

        if(YAHOO.speedfc.cart.add.cart_overlay !== null) {
            YAHOO.speedfc.cart.add.cart_overlay.hide();
            YAHOO.speedfc.cart.add.cart_overlay.setBody('');
            minicart_open = false;
        }
    });

    //jQuery(document).ready(function isExplorer() {
            var ua     = window.navigator.userAgent;
            var msie   = ua.indexOf("MSIE ");
            var gecko  = ua.indexOf("like Gecko");
            var chrome = ua.indexOf("Chrome");

            if (msie > 0)   {   // If Internet Explorer, return version number
                jQuery("#category_nav a").css("font-size","12px");
            }
            else if (gecko > 0 && chrome < 0) {
                jQuery("#category_nav a").css("font-size","12px");
            }
        //}
    //);
</script>        </div>
