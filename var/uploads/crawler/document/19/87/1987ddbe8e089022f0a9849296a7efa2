        <script type="text/javascript"> 
          function FooterEmailSignup(){ 

			_gaq.push(['_trackEvent','Email',document.getElementById('signThisEmail').value]);    			
            // remove previous message in blocks 
            document.getElementById('email_signup_messages').style.display = "none"; 
            document.getElementById('email_signup_messages').innerHTML = "none"; 

            // retrieve email value 
            var email_value = document.getElementById('signThisEmail').value; 

            // post data to server 
            YAHOO.util.Connect.asyncRequest('POST', '/customer-service/dmailSignup', { 
                success:function(o) { 
                        var r = YAHOO.lang.JSON.parse(o.responseText); 
                  if(r.success != undefined){ 
                        document.getElementById('email_signup_messages').style.display = "block"; 
                        for(var e in r.messages){ 
                      document.getElementById('email_signup_messages').innerHTML = r.messages[e]; 
                    } 
                    if(r.success == true){ 
                        document.getElementById('email_footer_signup_form').style.display = "none"; 
                        if(typeof(s) != 'undefined') {
                            var sObjm = new Object(); 
                            sObjm.events = 'event21';
                            s.t(sObjm);
                        }
                    } 
                  } 
                }, 
                failure:function(o) { 
                } 
            }, '&email=' + email_value + '&email_conf=' + email_value + '&rand=' + Math.random()*1000 ); 

          } 
        </script>

