<!-- HOVER NAV CONTENTS ENDS -->

<script type="text/javascript">
    jQuery(document).ready(function(){
        $(window).load(function(){

            var navigation_to = setTimeout(function(){}, 0);

            jQuery("#navigation #category_nav .navigation-left li").each(function(){
                jQuery("#navigation .replicable-hover").append("<li><div class=\"triangle-up\"></div></li>");
                var current = jQuery("#navigation .replicable-hover li:last");
                current.width(jQuery(this).width());

                jQuery(this).hover(
                    function(){
                        mouseOver(this,current);
                    },
                    function(){
                        mouseOut(this,current);
                    }
                );

            });

            jQuery("#navigation #category_nav .navigation-right li").each(function(){
                jQuery("#navigation .replicable-hover").append("<li class='hover-right'><div class=\"triangle-up\"></div></li>");
                var current = jQuery("#navigation .replicable-hover li:last");
                current.width(jQuery(this).width());

                jQuery(this).hover(
                    function(){
                        mouseOver(this,current);
                    },
                    function(){
                        mouseOut(this,current);
                    }
                );

            });

            function mouseOver(element,navHover){

                jQuery(".hierarchy-hover-nav > ul").hide();

                clearTimeout(navigation_to);

                jQuery("#navigation .replicable-hover .triangle-up").hide();
                jQuery(navHover).find('.triangle-up').show();
                jQuery(".hierarchy-hover-nav-container").show();
                jQuery(".hierarchy-hover-nav > ul."+jQuery(element).find("a:first").attr("rel")+"-content").show();

                // make all submenu items same height
                jQuery("#navigation .hierarchy-hover-nav > ul > li > ul")
                    .height("")
                    .height("180px");

                //Remove the height for the titles
                jQuery("#navigation .hierarchy-hover-nav .hover-content-titles ul").css("height", "auto");
            }

            function mouseOut(element,navHover){

                navigation_to = setTimeout(function(){
                    jQuery(".hierarchy-hover-nav-container").hide();
                },500);

            }

            jQuery(".hierarchy-hover-nav-container").hover(function() {

                clearTimeout(navigation_to);

            }, function() {

                navigation_to = setTimeout(function(){
                    jQuery(".hierarchy-hover-nav-container").hide();
                },500);

            });

        });

    });
</script>
<script type="text/javascript">
    YAHOO.util.Event.onContentReady("category_nav_container", YAHOO.speedfc.utilities.navigation.getCatNavContainer);
</script>                    </div>

