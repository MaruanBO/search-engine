<script>
// asynchro iframe API load (for YT video tracking)
var tag = document.createElement('script');
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

// video event tracking
function onPlayerReady(event) {}
function onPlayerStateChange(event) {
	if (event.data == YT.PlayerState.PLAYING) {
		dataLayer.push(['_trackEvent', 'Videos', 'Play', 'Intro page (en_US)']);
	}
	if (event.data == YT.PlayerState.ENDED) {
		dataLayer.push(['_trackEvent', 'Videos', 'Finished', 'Intro page (en_US)']);
	}
}

jQuery(document).ready(function($)
{
	// SW API init
	$.sw.init({url: "http://www.smallworlds.com/api/", surl: "https://www.smallworlds.com/api/"});

	// redirect to root if user session exists
	$.sw.user.me(true).success(function(user)
	{
		window.location = "/";
	});
	
	// check user's country and redirect if necessary
	$.sw.get('country/current', true).success(function(country)
	{
		// SW -> DE
		if ('en_US' == 'en_US' && (country.code == 'DE' || country.code == 'AT' || country.code == 'CH' || country.code == 'BE'))
		{
			// cookie already set?
			if ($.cookie('redirect-to-de'))
			{
				window.location = "https://www.smallworldsonline.de";
			}
			// open redirect lightbox
			else
			{
				$.fancybox({
					href: "/redirect/de/?countryName=" + encodeURIComponent(country.name),
					type: "iframe",
					autoSize : false,
					width : 700,
					height : 450,
					padding: 2,
				});
			}
		}
		// SW -> ES
		else if ('en_US' == 'en_US' && (country.code == 'AR' || country.code == 'BO' || country.code == 'CL' || country.code == 'CO' || country.code == 'CR' || country.code == 'CU' || country.code == 'DO' || country.code == 'EC' || country.code == 'SV' || country.code == 'GT' || country.code == 'HN' || country.code == 'MX' || country.code == 'NI' || country.code == 'PA' || country.code == 'PY' || country.code == 'PE' || country.code == 'PR' || country.code == 'UY' || country.code == 'VE'))
		{
			// cookie already set?
			if ($.cookie('redirect-to-es'))
			{
				window.location = "http://minimundos.axeso5.com";
			}
			// open redirect lightbox
			else
			{
				$.fancybox({
					href: "/redirect/es/?countryName=" + encodeURIComponent(country.name),
					type: "iframe",
					autoSize : false,
					width : 800,
					height : 450,
					padding: 2,
				});
			}
		}
	});

    $("a.iframe").click(function(e){
        e.preventDefault();
        $.fancybox({
            href: $(this).attr("href"),
            type: "iframe",
            autoSize : false,
            width : $(this).attr("iframe-width"),
            height : $(this).attr("iframe-height"),
            padding: 2,
            afterClose: function(){ dataLayer.push({'event': '_trackVirtualview', 'category': 'Iframe', 'action': 'Close'}); }
        });
    });

	$('a#intro-header-video-link').click(function(e){
	    e.preventDefault();
		$.fancybox(
		{
			href: "https://www.youtube.com/embed/qgm5kd5PT9w?rel=0&autoplay=1&enablejsapi=1&origin=https://internal.smallworlds.com&eurl=internal.smallworlds.com",
			type: "iframe",
            autoSize: false,
            width: $(this).attr("iframe-width"),
            height: $(this).attr("iframe-height"),
			tpl:
			{
				'iframe': '<iframe id="iframe-vid" name="fancybox-frame{rnd}" class="fancybox-iframe" frameborder="0" vspace="0" hspace="0" webkitallowfullscreen="" mozallowfullscreen="" allowfullscreen=""' + ($.browser.msie ? ' allowtransparency="true"' : '') + '></iframe>'
			},
			beforeShow: function() {
				player = new YT.Player('iframe-vid', {
					events: {
						'onReady': onPlayerReady,
						'onStateChange': onPlayerStateChange
					}
				});
			}
		});
	});

	$('div#carousel-backgrounds').carousel(
	{
		interval: 5000
	});

	// carousel imgs prefetch (for FF and other browsers that don't prefetch imgs with 'display: none' css value)
	var preloads = [];
	preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b1.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b2.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b3.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b4.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b5.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b6.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b7.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b8.jpg');preloads.push('https://www.smallworlds.com/content/webcontent-v4016/website/img/intro/header_background_vid_b9.jpg');	$(preloads).each(function(){$('<img/>')[0].src = this;});
});
</script>

