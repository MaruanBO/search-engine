<script>
jQuery(document).ready(function($) {
	$('#frm_login_inline').submit(function() {
		clearErrors();

		if(!$.sw_login.validateNotEmpty($("#email").val())){ $.sw_login.showInvalidateMessage($("#email"), "Please enter a valid email address.", "bottom"); return false;}
        if(!$.sw_login.validateEmail($("#email").val())){ $.sw_login.showInvalidateMessage($("#email"), "Please enter a valid email address.", "bottom"); return false;}
        if(!$.sw_login.validateNotEmpty($("#password").val())){ $.sw_login.showInvalidateMessage($("#password"), "Please enter a password.", "bottom"); return false;}
		
       	$("#loading_spinner_inline").css("display", "block");
        $("#login_registration").find(".error").remove();
        
        dataLayer.push({'event': '_trackVirtualview', 'category': 'Login', 'action': 'normalLogin'});
        
        $.sw_login.doLogin($('#email').val(), $('#password').val(), $('#rememberMe').is(':checked'), onLoginSuccess, onLoginError);
        
        return false;
	});
    
    $("#btn_loginwithfb").click(function(){
    	clearErrors();
    	
       	$("#loading_spinner_inline").css("display", "block");
       	
        dataLayer.push({'event': '_trackVirtualview', 'category': 'Login', 'action': 'fbLogin'});

        facebookLogin(facebookLoginCallback);
    	
        return false;
    });

    function facebookLoginCallback(userId, shortLivedToken) {
        
		if (userId == null || shortLivedToken == null) {
			onLoginError("0");
			return;
		}
		
		$.sw.facebook.login({userId: userId, shortLivedToken: shortLivedToken})

		.success(function(data, textStatus, jqXHR) {
			$("#loading_spinner_inline").css("display", "none");

			if (data.newUser)
				window.top.jQuery.fancybox({
					href: "/register/facebook/?firstName=" + data.firstName + "&aToken=" + data.longLivedToken,
					type: "iframe",
					autoSize : false,
					padding : 0,
					width : 405,
					height : 635
				});  
			else
				window.location.reload();
		})

		.error(function(data) {
			$("#loading_spinner_inline").css("display", "none");

			onLoginError(data.errorCode);
		});
    }

    $("#btn_forgot_password").click(function(event) {
    	event.preventDefault();
    	switchToForgotPwd();
    });

    $("#btn_forgot_password_back").click(function(event) {
    	event.preventDefault();
    	switchToLogin();
    });

    $("#frm_recover_password").submit(function() {

    	clearErrors();

    	if(!$.sw_login.validateNotEmpty($("#forgot_email").val())){ $.sw_login.showInvalidateMessage($("#forgot_email"), "Please enter a valid email address.", "bottom"); return false;}
        if(!$.sw_login.validateEmail($("#forgot_email").val())){ $.sw_login.showInvalidateMessage($("#forgot_email"), "Please enter a valid email address.", "bottom"); return false;}

       	$("#loading_spinner_inline").css("display", "block");
        
        $.sw.auth.forgotPassword($("#forgot_email").val())
        .success(function() {            
        	$("#loading_spinner_inline").css("display", "none");
        	showForgotPswdErrorMessage("Your password has been recovered and sent to your email address. <a href=\"#\" onclick=\"switchToLogin()\">Try Logging In Again</a>");
        })
        .error(function(data) {
        	$("#loading_spinner_inline").css("display", "none");
            switch(data.errorCode){
           		case $.sw.errorCode.LOGIN_EMAIL_NOT_FOUND:
           			$.sw_login.showInvalidateMessage($("#forgot_email"), "We cannot find an account with that email address.", "bottom");
                    break;
                default:
                	showForgotPswdErrorMessage("Oops, something went wrong. Please try again.");
                    break;
            }
        });
        
        return false;
    });

    function onLoginSuccess() {
    	window.location.reload();
    }
    
    function onLoginError(errorCode) {
    	$("#loading_spinner_inline").css("display", "none");

        switch(errorCode) {
            case $.sw.errorCode.INTERNAL_ERROR:
            	showLoginErrorMessage("Oops, something went wrong. It has been logged and we will attend to it as soon as possible.");
				break;
            case $.sw.errorCode.SESSION_EXPIRED:
            	showLoginErrorMessage("Oops, something went wrong, please refresh and<br />try again.");
				break;
            case $.sw.errorCode.DO_NOT_HAVE_PERMISSION:
            	showLoginErrorMessage("You do not have permissions to complete this action.");
            	break;
            case $.sw.errorCode.INVALID_PARAM:
            	$.sw_login.showInvalidateMessage($("#email"), "Oops!", "bottom");
				break;
            case $.sw.errorCode.LOGIN_EMAIL_NOT_FOUND:
            	$.sw_login.showInvalidateMessage($("#email"), "The account for this email was not found.", "bottom");
				break;
            case $.sw.errorCode.LOGIN_INVALID_PASSWORD:
            	$.sw_login.showInvalidateMessage($("#password"), "You have entered an incorrect password.", "bottom");
				break;
            case $.sw.errorCode.LOGIN_ACCOUNT_NOT_ACTIVE:
            	$.sw_login.showInvalidateMessage($("#email"), "The account for this email was not found.", "bottom");
				break;
            case $.sw.errorCode.LOGIN_ACCOUNT_BANNED:
            	$.sw_login.showInvalidateMessage($("#email"), "Your account has been locked, check your email for details.", "bottom");
				break;
            case $.sw.errorCode.USER_NOT_FOUND:
            	showLoginErrorMessage("The currently logged in Facebook account does not have a SmallWorlds account associated with it.");
                break;
            default:
            	showLoginErrorMessage("An error has occured. Code: " + errorCode);
        }
    }

    function showLoginErrorMessage(msg) {
    	$("#frm_login_inline").after("<div class='validation_error' style='top: 41px; left: 86px'><div class='box'>" 
    			+ msg + "</div></div>");
    }

    function showForgotPswdErrorMessage(msg) {
    	$("#frm_recover_password").after("<div class='validation_error' style='top: 41px; left: 186px'><div class='box'>" 
    			+ msg + "</div></div>");
    }
});

function switchToForgotPwd() {
	clearErrors();
	jQuery.placeholder.shim();
	jQuery("#login_inline_section").fadeOut(200, function() {
		jQuery("#recover_password_section").fadeIn(200);
	});
}

function switchToLogin() {
	clearErrors();
	jQuery("#recover_password_section").fadeOut(200, function() {
		jQuery("#login_inline_section").fadeIn(200);
	});
}

function clearErrors() {
	jQuery("#login_registration").find(".validation_error").remove();
	jQuery("#login_registration").find("input.error").removeClass("error");
}
</script>

