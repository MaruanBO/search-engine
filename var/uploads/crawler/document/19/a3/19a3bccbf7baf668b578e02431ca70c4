	<div id="login_inline_section">
		<form id="frm_login_inline" novalidate>
			<div class="input pull-left">
				<input class="email padding-moderate-left" type="email" id="email" name="email" placeholder="Email address..." onfocus="clearErrors();" tabindex="1" />
				<div style="margin-top: 5px;"><input type="checkbox" checked id="rememberMe" name="rememberMe" /><label for="rememberMe" style="text-transform: none; font-family: font-body, 'Helvetica Neue', Helvetica, Arial, sans-serif;">Keep me logged in</label></div>
			</div>
			<div class="input pull-left gap-mini-left">
				<input class="password padding-moderate-left" type="password" id="password" name="password" placeholder="Password..." onfocus="clearErrors()" tabindex="2" />
				<div style="margin-top: 5px;"><strong><a href="#" id="btn_forgot_password" class="link">Forgot Password?</a></strong></div>
			</div>
			<input type="image" class="pull-left gap-mini-left" src="https://www.smallworlds.com/content/webcontent-v4016/shared/img/login/button_login.gif" value="Login" tabindex="3" />
			<div id="btn_loginwithfb" class="btn_fb_signin pull-left gap-mini-left"><div></div></div>
		</form>
	</div>
