		<script language="javascript">
function OpenTieredRate(arg_ID)
{
 window.open
    ("Popup_MerchantTieredRate.aspx?ID=" + arg_ID,
     "TieredRate",
     "location=no,menubar=no,scrollbars=yes,status=no,directories=no,toolbar=no," +
     "resizable=yes,width=550,height=305");     
}

function CountryPosition()
{


var isNS = (navigator.appName == "Netscape"); 
var isOpera = (navigator.userAgent.indexOf("Opera") != -1); 
var isFireFox = (navigator.userAgent.indexOf("Firefox") != -1); 
var leftValue = 0;
var tableWidth = 743;
var img_pixelwidth = 211;
var new_position = 0;

   if (isFireFox)
   {
      leftValue = (Math.round((document.body.clientWidth - tableWidth)/2));
      new_position = document.body.clientWidth - leftValue - img_pixelwidth;
      document.all.international_sites.style.left = new_position;   
   }

   else if (isNS || isOpera)
   {
      leftValue = (Math.round((innerWidth - tableWidth)/2));
      new_position = innerWidth - leftValue - img_pixelwidth;
      document.getElementById('international_sites').style.left = new_position; 
   }
   
   else //By default the remaining browser isIE
   {
      leftValue = (Math.round((document.body.clientWidth - tableWidth)/2));
      new_position = document.body.clientWidth - leftValue - img_pixelwidth;
      document.all.international_sites.style.left = new_position;
   }
   if (leftValue < 0) //This will make sure that the leftValue is never negative.
   {
      leftValue = 0;
   }


}

	</script>
";i:2;s:4:"6316";}