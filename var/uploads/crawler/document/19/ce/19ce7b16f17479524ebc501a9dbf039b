function SetInternational(type){

  if(type=='on'){
  
    if (document.getElementById)
    {
    document.getElementById('international_sites').style.visibility="visible";    
    }
    else
    {  
      document.all.international_sites.style.visibility="visible";
    }
  }else  {
  
    if (document.getElementById)
    {
    document.getElementById('international_sites').style.visibility="hidden";    
    }
    else
    {  
      document.all.international_sites.style.visibility="hidden";
    }
  }

  return false;
}

