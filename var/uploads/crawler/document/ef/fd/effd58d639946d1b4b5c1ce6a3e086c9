<script>
var mobileRedirectConfig = [
    {
        "userAgent": "iphone|ipod|mobile|blackberry|palm|silk",
        "not": {
            "userAgent": "tablet|ipad|playstation|iemobile|msie",
            "mediaQuery": "(min-width: 768px)"
        }
    }
];

if (mobileRedirectConfig){
    try {
        var defaultRedirect = 'http://m.vw.com';
        var passed = false;

        for (var i=mobileRedirectConfig.length; i--;){
            // check to see if thisConfig passes all tests.
            var thisConfig = mobileRedirectConfig[i];
            var configPass = true;

            // userAgent
            var userAgentMatch = navigator.userAgent.toLowerCase().match(new RegExp(thisConfig.userAgent, 'i'));

            //if match is null, then it's a no pass.  if not null, it passed; set to true
            configPass = configPass && userAgentMatch;

            // media query
            if (typeof window.matchMedia === 'function' && thisConfig.mediaQuery){
                var mqMatch = window.matchMedia(thisConfig.mediaQuery);
                configPass = configPass && mqMatch.matches;
            }

            //not rules
            if (typeof thisConfig.not === 'object') {

                //not user agent
                if (thisConfig.not.userAgent){
                    var notUAMatch = navigator.userAgent.toLowerCase().match(new RegExp(thisConfig.not.userAgent, 'i'));
                    configPass = notUAMatch ? false : configPass;
                }

                //not media query
                if (typeof window.matchMedia ==='function' && thisConfig.not.mediaQuery){
                    var notMediaQuery = window.matchMedia(thisConfig.not.mediaQuery);
                    configPass = (notMediaQuery.matches) ? false: configPass;
                }
            }

            passed = passed || configPass;
        }


        if (window.overrideMobileRedirect === "true") {
            passed = false;
        }

        if (passed) {
            //redirect
            window.location = window.mobileRedirectUrl || defaultRedirect;
        }
    } catch (ex) {
        // error
        debugger;
    }
}
</script>
