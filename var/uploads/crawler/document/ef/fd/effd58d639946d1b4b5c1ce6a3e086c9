<script>
    $(document).ready(function(){
        var browser = VW.utilities.isIE();
        var tag = $('html');
        var classToAdd = '';

        switch(browser){
            case 8:
                classToAdd ='ie8';
                break;
            case 9:
                classToAdd ='ie9';
                break;
            case 10:
                classToAdd ='ie10';
                break;
            case 11:
                classToAdd ='ie11';
                break;

            default:
                if (navigator.userAgent.indexOf('Windows NT 6.1') > 0 ) {
                    classToAdd = 'windowsSeven';
                }
                break;
        }

        if (classToAdd) tag.addClass(classToAdd);
    });
</script>

