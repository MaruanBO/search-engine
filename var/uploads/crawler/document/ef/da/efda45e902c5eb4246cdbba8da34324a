//***** Breaking News Alert Cookie *****//
    if(!$.cookie('DFM_BNA')){$('#alerts').animate({'height':'62'},'fast');}
    function DFM_BNA_COOKIE(){var date=new Date();date.setTime(date.getTime()+(3600*1000));$.cookie('DFM_BNA','set',{expires: date});}
    $('.weather-toggle').click(function(e){ e.preventDefault(); activateTopBar($(this), $('.weather-bar')); $('.traffic-toggle').removeClass('active'); });
    $('.traffic-toggle').click(function(e){ e.preventDefault(); activateTopBar($(this), $('.traffic-bar')); $('.weather-toggle').removeClass('active'); });
    function activateTopBar($btn, $target) {
        if($('#topbar').is(':visible')) {
            if($btn.hasClass('active')) { $('#topbar').slideUp('fast', function(){ $('.weather-bar, .traffic-bar').hide();});}
            else {
                $('#topbar').slideUp('fast', function(){
                    $('.weather-bar, .traffic-bar').hide();
                    $target.show();
                    $('#topbar').slideDown('fast');
                });
            }
        } else {
            $('.weather-bar, .traffic-bar').hide();
            $target.addClass('visible').show();
            $('#topbar').slideDown('fast');
        }
        $btn.toggleClass('active');
    }
