<script>

            
            $(document).on('keydown', '.vf-content', function () {
                $('#commentLoginForm').modal('show');
            });
        
    
    

    (function (v, s) {
        v.type = 'text/javascript';
        v.async = !0;
        v.src = '//cdn.viafoura.net/vf.js';
        s.parentNode.insertBefore(v, s);
    }(document.createElement('script'), document.getElementsByTagName('script')[0]));

    window.vfAsyncInit = function(){
        window.viafoura.ready(function(){
            window.viafoura.subscribe('onLoginRequired', function () {
                window.viafoura.ui.login.hide();
                $('#commentLoginForm').modal('show');
            });
        });
    };

    function logUserOut()
    {
        document.cookie = "csg-access=; expires=Thu, 01 Jan 1970 00:00:00 UTC";

        window.viafoura.ready(function ()
        {
            window.viafoura.session.logout();
        });
        localStorage.removeItem('troyId');
        localStorage.removeItem('utm_t');
        localStorage.removeItem('troyEndTime');
        localStorage.removeItem('troyFname');
        localStorage.removeItem('troySname');

        document.getElementById('logmeout').submit();
    }

    $(function() {
        $('.roadblock-link').on('click', function() {
            var id = $(this).attr('id');
            ga('send', 'event', 'ask-register', 'click', id);
        });
    
            
            var troy_redirect = location.href;
            var expire = '{"+1 days"|date_format:"%a, %d %b %Y %H:%M:%S UTC"}';
            document.cookie='troy_redirect='+troy_redirect+'; expires='+expire+'; path=/; domain=gazette.com';
        });
    
</script>


<script type="text/javascript">

	if (window.location.href.indexOf('debug') > -1) {
		console.log('CSG InsidersClub Init');
	}
	CSGInsidersClub = {
		_contests: {},
		_email: '',

		submitEntries: function () {
			var entries = [];
			jQuery('a.insidersClubNotEntered.insidersInlineButton').each(function(i, e){
				entries.push($(e).attr('id').match(/[\d]+/)[0]);
			});

			var s = '';

			for (var i in entries)
			{
				s = (s=='') ? entries[i] : s+','+ entries[i];
			}

			CSGInsidersClub.doSendEntry({email: CSGInsidersClub._email, bulk: s});
		},

		enterContest: function(cID) {
			CSGInsidersClub.doSendEntry({email: CSGInsidersClub._email, entryID: cID})
		},

		doSendEntry: function(data, times) {
			times = (typeof(times) == 'undefined') ? 1 : times + 1;

			$.ajax({
				url: 'http://www3.gazette.com/insider/api/',
				data: data,
				method: 'POST'
			}).done(function(ret){
						if (ret.status == 200)
						{
							CSGInsidersClub.doCheckForEntries();
						} else {
							if (window.location.href.indexOf('debug') > -1) {
								console.log('API error in doSendEntry');
								console.debug(data);
								console.debug(ret);
							}
						}
					}
			).fail(function (ret) {
						if (window.location.href.indexOf('debug') > -1) {
							console.log('API error in doSendEntry');
							console.debug(data);
							console.debug(ret);
						}

						if (times <= 5)
						{
							setTimeout(function(){CSGInsidersClub.doSendEntry(data, times)}, 200);
						} else {
							console.log('Reached limit of retries for API.  Giving up');
						}
					}
			);
		},

		doCheckForEntries: function() {
			var btn;
			var cid;
			var post = {email: CSGInsidersClub._email};
			var span;

			$.ajax({
				url: 'http://www3.gazette.com/insider/api/entries/',
				data: post,
				method: 'POST'
			}).done(function(ret) {
						for (var i in ret.entries)
						{
							btn = jQuery('#contest-button-'+ret.entries[i]);

							if (btn !== null)
							{
								$(btn).removeClass('insidersClubDisabled').removeClass('insidersClubNotEntered').removeAttr('onClick').addClass('insidersClubEntered');

								span = $(btn).children('span')[0];
								if (typeof(span) == 'undefined') {
									$(btn).html('ENTERED!');
								} else {
									$(span).html('ENTERED!');
								}
							}
						}

						// Now update any remaining buttons
						jQuery('a.insidersClubDisabled.insidersInlineButton').each(function(index, e){
							cid = $(e).attr('id').match(/[\d]+/);
							$(e).removeClass('insidersClubDisabled').addClass('insidersClubNotEntered').attr('onClick', 'CSGInsidersClub.enterContest('+cid+')');

							span = $(e).children('span')[0];
							if (typeof(span) == 'undefined') {
								$(e).html('ENTER');
							} else {
								$(span).html('ENTER');
							}
						});

						setTimeout(function(){
							if (jQuery('a.insidersClubNotEntered.insidersInlineButton').length == 0) {
								$('a.insidersClubBottomButton').removeClass('insidersClubDisabled').removeClass('insidersClubNotEntered').addClass('insidersClubEntered').removeAttr('onClick').html('ENTERED IN ALL AVAILABLE CONTESTS!');
							} else {
								$('a.insidersClubBottomButton').removeClass('insidersClubDisabled').removeClass('insidersClubEntered').addClass('insidersClubNotEntered').html('ENTER ALL AVAILABLE CONTESTS').attr('onClick', 'CSGInsidersClub.submitEntries()');
							}
						}, 100);
					}
			).fail(function(ret) {
						if (window.location.href.indexOf('debug') > -1) {
							console.log('API error in doCheckForEntries()');
							console.debug(post);
							console.debug(ret);
						}
					}
			);
		},

		addToContestPool: function (contestID, contestField) {
			console.log('Adding in contest [' + contestID + ']');
			this._contests[contestID] = contestField;
		}
	};
	// For articles, we populate a div telling users the must be logged in in order to enter contests
	if (window.location.href.indexOf('/insider') == -1)
	{
		var box = document.createElement('div');
		box.className = 'insidersClubLoginBox';
		var c = jQuery('div#region2 article');

		box.innerHTML = 'You must be <a href="http://http://secure.gazette.com/activate">registered</a> and <a href="http://http://secure.gazette.com/login">logged in</a> in order to enter contests and offers';

		$(c).prepend(box);
	}
</script>



