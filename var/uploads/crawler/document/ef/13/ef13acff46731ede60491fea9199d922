var runDate = new Date();
var month = runDate.getMonth()+1;
var day = runDate.getDate();	
var year = runDate.getFullYear() 
var output = 
((''+month).length<2 ? '0' : '') + month + '/' +
((''+day).length<2 ? '0' : '') + day+ '/' +
((''+year).length<4 ? '0' : '') + year;
fetchData('0Av5yv13N4o_QdFB6c0xGLXptNGU2WWo1MmhUSGFmLUE');
function fetchData(key) {
Tabletop.init({
key: key,
callback: showInfo,
simpleSheet: true,
endpoint: 'http://cdn.www3.gazette.com/info/libraries/cache.php?v=1&q=',
singleton: true,
query: 'date='+output
});
};
// Compile Handlebars template
function showInfo(data, tabletop) {
//templates
var source = $("#deal-list").html();
var template = Handlebars.compile(source);
// Create a templated item for every row in the spreadsheet
$.each( tabletop.sheets("deal-data").all(), function(i, cat) {
var html = template(cat);
$(".deal-main").append(html);
});
}
