.relative {position:relative;}
.gallery-overlay {
	position:fixed;
	width:100%;
	height:100%;
	top:0;
	left:0;
    background:url(http://s3.amazonaws.com/content.newsok.com/newsok/images/oklahoman.com/dialog-overlay-bg.png);
	background:rgba(0,0,0,.7);
	z-index:49999;
}
.gallery-frame-nextgen {
	position:relative;
	z-index:50002;
	background:#FFF;
}
.closeit {
	display:block;
	width:28px;
	height:29px;
	background:url(http://s3.amazonaws.com/content.newsok.com/newsok/images/closeit.png) center top no-repeat;
	text-indent:-70000px;
	position:absolute;
	right:-360px;
	top:-40px;
	z-index:50003;
}
</style>
<script>
$(document).ready(function() {

	var clicked = false;
	function iframe() {
		$('#gallery-frame-nextgen_105515').contents().find('body').mousedown(function() {
			if (clicked == false && !$('.gallery-overlay').is(':visible')) {
                $('.galleryZ').css('z-index', '4000');
                $('.galleryZ.widget_105515').css('z-index', '50000');
				$('#gallery-frame-nextgen_105515').animate({
					width: '940px',
					padding: '25px',
					marginTop: '-25px',
					marginLeft: '-25px'
				}, 300, function() {
					$(this).addClass('active');
					$('#nextGen_widget_105515 .closeit').fadeIn(700);
					// load ad code
                    ord=Math.random()*10000000000000000;
					var adblock = 'http://ad.doubleclick.net/N5578/adi/cdg.gazette/photos/gallery;sz=300x250;pos=1;ord='+ord;
					$(this).contents().find('iframe').attr('src', adblock);
				}).addClass('active');
				$('#nextGen_widget_105515 .gallery-overlay').fadeIn(300);
				clicked = true;
				// hide leaderboard
				if ($('.leaderboard').length > 0) {
					$('.leaderboard').css('z-index', '0');
				}
				// hide anchored leaderboard
				if ($('#anchored-leaderboard').length > 0) {
					$('#anchored-leaderboard').animate({
						height: 'toggle'
					},100)
				}
			}
		});
	}

	iframe();
	setTimeout(function() {
		iframe();
	}, 4500);

	$('#nextGen_widget_105515 .gallery-overlay, .closeit').click(function() {
		$('#gallery-frame-nextgen_105515').animate({
			width: '620px',
			padding: 0,
			margin: 0
		}, 300).removeClass('active');
		$('#nextGen_widget_105515 .gallery-overlay, .closeit').fadeOut(300);
		clicked = false;
		iframe();
		// hide leaderboard
		if ($('.leaderboard').length > 0) {
			$('.leaderboard').css('z-index', '1');
		}
		// bring leaderboard ad back
		if ($('#anchored-leaderboard').length > 0) {
			$('#anchored-leaderboard').animate({
				height: 'toggle'
			},300)
		}
	});


});
</script>

