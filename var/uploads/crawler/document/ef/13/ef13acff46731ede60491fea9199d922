                                                            <script>
jQuery(document).ready(function () {
    var pageRegion2 = jQuery('#region2');
    var pageRegion6Section1 = jQuery('#R6Wrapper1');

    if (jQuery(pageRegion2).height() > jQuery(pageRegion6Section1).height()) {
        jQuery(pageRegion6Section1).height(jQuery(pageRegion2).height() + 10);
    } else {
        jQuery(pageRegion2).height(jQuery(pageRegion6Section1).height());
    }

    // Check on the DotD as well

    setTimeout(
        function () {
            var dotd = jQuery('#dealOfTheDayContainer');
            var advert = jQuery('#region4 div.GoogleActiveViewClass');

            if (jQuery(dotd).height() > jQuery(advert).height()) {
                jQuery(advert).height(jQuery(dotd).height());
            } else {
                jQuery(dotd).height(jQuery(advert).height());
            }

            CSGazetteRescaleSection3(false);
        }, 1000);

    setTimeout(
        function () {
            var dotd = jQuery('#dealOfTheDayContainer');
            var advert = jQuery('#region4 div.GoogleActiveViewClass');

            if (jQuery(dotd).height() > jQuery(advert).height()) {
                jQuery(advert).height(jQuery(dotd).height() - 21);
            } else {
                jQuery(dotd).height(jQuery(advert).height());
            }

            CSGazetteRescaleSection3(false);
        }, 1500);

    setTimeout(
        function () {
            var dotd = jQuery('#dealOfTheDayContainer');
            var advert = jQuery('#region4 div.GoogleActiveViewClass');

            if (jQuery(dotd).height() > jQuery(advert).height()) {
                jQuery(advert).height(jQuery(dotd).height() - 21);
            } else {
                jQuery(dotd).height(jQuery(advert).height());
            }

            CSGazetteRescaleSection3(true);
        }, 2000);

    // Now get the headlines with advert
    function CSGazetteRescaleSection3(add) {
        var sec1 = jQuery('#region3 .widget.headline-list')[0];
        var sec2 = jQuery('#R4Wrapper1');
        var sec3 = jQuery('#R6Wrapper2');

        if (jQuery(sec1).height() != jQuery(sec2).height()) {
            if (jQuery(sec1).height() > jQuery(sec2).height()) {
                // sec1 greater than all
                if (jQuery(sec1).height() > jQuery(sec3).height()) {
                    jQuery(sec2).height(jQuery(sec1).height());
                    jQuery(sec3).height(jQuery(sec1).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height() - 20);
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            } else {
                if (jQuery(sec2).height() > jQuery(sec3).height()) {
                    jQuery(sec1).height(jQuery(sec2).height() - 20);
                    jQuery(sec3).height(jQuery(sec2).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height() - 20);
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            }
        } else {
            if (jQuery(sec2).height() > jQuery(sec3).height()) {
                jQuery(sec3).height(jQuery(sec2).height());
            } else {
                jQuery(sec1).height(jQuery(sec3).height() - 20);
                jQuery(sec2).height(jQuery(sec3).height());
            }
        }

    if (add)
    {
         jQuery(sec3).height(jQuery(sec3).height() + 20);
    }

        CSGazetteRescaleSection4(add);
    }

    function CSGazetteRescaleSection4(add) {
        var sec1 = jQuery('#region3 div.widget.lead-story')[1];
        var sec2 = jQuery('#region4 div.widget.lead-story')[1];
        var sec3 = jQuery('#R6Wrapper3');

        if (jQuery(sec1).height() != jQuery(sec2).height()) {
            if (jQuery(sec1).height() > jQuery(sec2).height()) {
                // sec1 greater than all
                if (jQuery(sec1).height() > jQuery(sec3).height()) {
                    jQuery(sec2).height(jQuery(sec1).height());
                    jQuery(sec3).height(jQuery(sec1).height());
                } else {
                    jQuery(sec1).css('cssText', 'margin-bottom: 10px !important');
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            } else {
                if (jQuery(sec2).height() > jQuery(sec3).height()) {
                    jQuery(sec1).css('cssText', 'margin-bottom: 10px !important');
                    jQuery(sec1).height(jQuery(sec2).height());
                    jQuery(sec3).height(jQuery(sec2).height());
                } else {
                    jQuery(sec1).css('cssText', 'margin-bottom: 10px !important');
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            }
        } else {
            if (jQuery(sec2).height() > jQuery(sec3).height()) {
                jQuery(sec3).height(jQuery(sec2).height());
            } else {
                jQuery(sec1).css('cssText', 'margin-bottom: 10px !important');
                jQuery(sec1).height(jQuery(sec3).height());
                jQuery(sec2).height(jQuery(sec3).height());
            }
        }

        CSGazetteRescaleSection5(add);
    }

    function CSGazetteRescaleSection5(add) {

        var sec1 = jQuery('#region3 div.widget.lead-story')[1];
        var sec2 = jQuery('#region4 div.widget.lead-story')[1];
        var sec3 = jQuery('#R6Wrapper3');

        if (jQuery(sec1).height() != jQuery(sec2).height()) {
            if (jQuery(sec1).height() > jQuery(sec2).height()) {
                // sec1 greater than all
                if (jQuery(sec1).height() > jQuery(sec3).height()) {
                    jQuery(sec2).height(jQuery(sec1).height());
                    jQuery(sec3).height(jQuery(sec1).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            } else {
                if (jQuery(sec2).height() > jQuery(sec3).height()) {
                    jQuery(sec1).height(jQuery(sec2).height());
                    jQuery(sec3).height(jQuery(sec2).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            }
        } else {
            if (jQuery(sec2).height() > jQuery(sec3).height()) {
                jQuery(sec3).height(jQuery(sec2).height());
            } else {
                jQuery(sec1).height(jQuery(sec3).height());
                jQuery(sec2).height(jQuery(sec3).height());
            }
        }

        CSGazetteRescaleSection6(add);
    }

    function CSGazetteRescaleSection6(add) {

        var sec1 = jQuery('#region3 div.widget.lead-story')[2];
        var sec2 = jQuery('#region4 div.widget.lead-story')[2];
        var sec3 = jQuery('#R6Wrapper4');

        if (jQuery(sec1).height() != jQuery(sec2).height()) {
            if (jQuery(sec1).height() > jQuery(sec2).height()) {
                // sec1 greater than all
                if (jQuery(sec1).height() > jQuery(sec3).height()) {
                    jQuery(sec2).height(jQuery(sec1).height());
                    jQuery(sec3).height(jQuery(sec1).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            } else {
                if (jQuery(sec2).height() > jQuery(sec3).height()) {
                    jQuery(sec1).height(jQuery(sec2).height());
                    jQuery(sec3).height(jQuery(sec2).height());
                } else {
                    jQuery(sec1).height(jQuery(sec3).height());
                    jQuery(sec2).height(jQuery(sec3).height());
                }
            }
        } else {
            if (jQuery(sec2).height() > jQuery(sec3).height()) {
                jQuery(sec3).height(jQuery(sec2).height());
            } else {
                jQuery(sec1).height(jQuery(sec3).height());
                jQuery(sec2).height(jQuery(sec3).height());
            }
        }
    }
});
</script>
                                                                                </div>
                    </div>
                </footer>
            </div><!-- /role=main -->
<!--++++++++++++++++++++++++++
    END MAIN CONTENT
++++++++++++++++++++++++++-->

            </div>
    <!-- / pg-wrapper -->

