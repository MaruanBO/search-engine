    <script type="text/javascript">
                
        /* Desktop Navigation hover state */
        $(document).ready(function () {
            var navTimer;
            $('#mainNav li').mouseover(function () {
                if (navTimer) {
                    clearTimeout(navTimer);
                    navTimer = null
                }
                var thisId = $(this).attr('id');
                var thisSubnav = '#subnav_' + thisId;
                navTimer = setTimeout(function () {
                    $('#' + thisId).addClass('hover').siblings().removeClass('hover');
                    $('.sub-nav').hide();
                    $(thisSubnav).show();
                    $('#subnavWrap').show();
                    // Load the subnav ad
                    var subnavAd = $(thisSubnav).find('.subnav-ad').attr('id');
                    if (subnavAd) {
                        googletag.cmd.push(function () {
                            googletag.display(subnavAd);
                        });
                    }
                }, 300)
            });
            $('#mainNav li').mouseout(function () {
                clearTimeout(navTimer);
                navTimer = null
            });

            var hideSub;
            var hideHoverState;
            $('#masthead').bind("mouseleave", function () {
                hideSub = setTimeout("$('#subnavWrap').hide()", 500);
                hideHoverState = setTimeout("$('#mainNav li.dropdown').removeClass('hover')", 500);
            });
            $('#masthead').bind("mouseenter", function () {
                clearTimeout(hideSub);
                clearTimeout(hideHoverState);
            });
        });
        
                
        /* Google Site Search */
        (function () {
            var cx = '009187632804568954601:gzwwza35lwe';
