                        $("#donate-click").click();
                });

                var loadMoreClick=1; //Track number of times 'Load More' is clicked
                $( ".home #loadMore" ).on( "click", function() { 
                    
                    _gaq.push(['_trackEvent', 'Homepage', 'Load More', 'Click '+loadMoreClick.toString()]);
                    loadMoreClick=loadMoreClick+1;
                    
                    $("body").css("cursor","wait");
                    var content;
                    $.get("/ajax-retrieve-posts/?offset="+artOffset+" ul#thumbwrap > li", function(data){
                        content=$(data).find("ul.thumbwrap");

                        $('ul.thumbwrap').append(content.prevObject[0].children);
                        $("body").css("cursor","auto");
                        artOffset = artOffset + 6;
                    });
                });  //end "load more stories"
                
                var clicked = false;
                function autoClose() {
                    if (!clicked) {
                        jQuery('#homeAlertContent').slideToggle("slow",showstate);
                    }
                }
                
                function showstate(){
                    if (jQuery('#homeAlertContent').is(':hidden')) {
                        jQuery('img#homeAlertOpen').attr('src', '//s3.amazonaws.com/static.samaritanspurse.org/sp-genesis-assets/global/uber-menu/more_icon.png');
                        _gaq.push(['_trackEvent', 'Homepage', 'Home Alert', 'Close']);
                    }
                    else {
                        jQuery('img#homeAlertOpen').attr('src', '//s3.amazonaws.com/static.samaritanspurse.org/sp-genesis-assets/global/uber-menu/close_icon.png');
                        _gaq.push(['_trackEvent', 'Homepage', 'Home Alert', 'Open']);
                    }
                }  
                
                //register close/open alert button
                jQuery("div#homeMiniWrapRight").on("click", function() {
                    clicked=true;
                    if (!hi_open_no_close) {
                        jQuery('#homeAlertContent').slideToggle("slow",showstate);
                    }
                });

                if (homeAlert===true && !hi_open) {  
                    setTimeout(autoClose, hi_open_secs * 1000);
                }
                if (hi_open_no_close) {
                    jQuery('div#homeMiniWrapRight img#homeAlertOpen').remove();
                    jQuery('div#homeMiniWrapRight').css('cursor','default');
                }
                
            
