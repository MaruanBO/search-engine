<script type="text/javascript">

var memGiving=false;
function getCookie(name) {
    if (document.cookie.length > 0) {
        var start = document.cookie.indexOf(name + "=");
        if (start != -1) {
            start = start + name.length + 1;
            var end = document.cookie.indexOf(";", start);
            if (end == -1) {
                end = document.cookie.length;
            }
            return unescape(document.cookie.substring(start, end));
        }
    }
    return "";
}

function setCookie(name, value, days) {
    var expires;
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires=" + date.toGMTString();
    } else {
        expires = "";
    }
    document.cookie = name + "=" + value + expires + "; path=/";
}


function setMegaMenuDonateFromCookies() { //Set the check-out button in mega-menu if cookies show items in cart
    //jQuery('<span class="cart-count"></span>').appendTo('#megaUber .megamenu-donate');
    if (cartId.length>0 && cartCookieCount.length>0) {
        jQuery('.show-with-items').removeClass('hidden');
        jQuery('div#site-wrapper').removeClass('cart-no');
        jQuery('div#site-wrapper').addClass('cart-active');
        jQuery('span.cart-count').text(cartCookieCount);
        jQuery('span.cart-display').text(cartDisp);
        jQuery('span.cart-total-amount').text(cartTotalAmt);
        jQuery('div#gblCart').css("display","block");
    }
    else {
        jQuery("div#site-wrapper").removeClass("cart-active");
        jQuery("div#site-wrapper").addClass("cart-no");
        setCookie('_cart-count', '', -1);
        setCookie('_cart-display', '', -1);
        setCookie('_cart-total-amount', '', -1);
    }
}

function updateCartAmt() {
    //This function only serves to present a total amount on pages that display the total cart amount
    var cartAmt = 0;
    jQuery.each(cart.data.Items, function() {
        cartAmt+=this.Amount;
    });
    cart.Display = "Item(s): " + cart.data.Items.length + " Total: $" + cartAmt.toFixed(2);
    cart.DisplayAmt = cartAmt.toFixed(2);
}

function updateCookiesFromCart() { //update cookies once cart is loaded
    updateCartAmt();
    if (cart.data.Items.length>0) {
        setCookie('_cart-count', cart.data.Items.length, 1);
        setCookie('_cart-display', cart.Display, 1);
        setCookie('_cart-total-amount', cart.DisplayAmt, 1);
        jQuery(".show-with-items").removeClass("hidden");
    }
    else {
        setCookie('_cart-count', '', -365);
        setCookie('_cart-display', '', -365);
        setCookie('_cart-total-amount', '', -365);
        jQuery(".show-with-items").addClass("hidden");
    }
    jQuery('span.cart-count').text(cart.data.Items.length);
    jQuery('span.cart-display').text(cart.Display);
    jQuery('span.cart-total-amount').text(cart.DisplayAmt);

}

//go through the cart object and set appropriate item values for donation items (non-checkout pages only)
function cartLoadedOK() {
    var items = cart.data.Items;
    if (items && items.length>0 && donate) { //loop through items and set qty and amt if on a donation/merchandise page

        jQuery.each(items, function() {
            if (!this.IsOrder) {
                var $this = this;
                var size;
                jQuery.each(this.EntityAttributes, function(i){
                    if(this.Key == 'RecurringDate' && this.Value) {
                        jQuery('[data-sku='+$this.Designation+']').data('recurringdate',this.Value).change();
                    }
                });
                jQuery('#qty_'+this.Designation).val(this.Quantity).change();
                jQuery('#amt_'+this.Designation).val(this.Amount).change();
                if (this.Quantity>0) 
                        jQuery('#add_'+this.Designation).text("Checkout");
            }

        });
    }

    //check to see if motivation/source code has been updated.  Cookie value trumps cart value.
    if (cart.data.Referrer != cartRef || cart.data.SourceCode != cartSrc) {
      cart.data.Referrer = cartRef;
      cart.data.SourceCode = cartSrc;
    }
    updateCookiesFromCart();
}

function getDonationInfo($this) { //new function - Walt
    var item = new Object();
    item.desig = jQuery($this).attr('id').substring(4,13);
    item.qty = jQuery("#qty_"+item.desig).val(); //get user selected quantity

    if (!item.qty) item.qty = jQuery("input#qty_"+item.desig).val(); // try for an input just in case
    if (!item.qty) item.qty=1; //regular donation items have no quantity


    item.amt = jQuery("input#amt_"+item.desig).val(); //get user entered amount
    if (!item.amt) {
        item.amt="0.00";
    }
    item.amt = item.amt.replace("$","");
    var dataAttr = jQuery("div#donateDiv" + item.desig).data(); //get donation item details from parent div
    item.desc = dataAttr.itemDescription;
    item.recurringDate = dataAttr.recurringdate;
    if(parseInt(item.recurringdate) > 0 && item.desc.indexOf(' (Monthly)') == -1) item.desc = item.desc + ' (Monthly)';
    item.maxQty = dataAttr.maxQty;
    if (dataAttr.itemValue2) var arrVal2 = dataAttr.itemValue2.split(":");
    if (arrVal2) {
        item.val2ID = arrVal2[0];
        item.val2Val = arrVal2[1];
    }
    item.itemValue = dataAttr.itemValue;
    item.honorMem = dataAttr.allowResponse;
    item.responseLimit = dataAttr.responseLimit;
    item.isOrder = dataAttr.isOrder;
    item.authOnly = dataAttr.authorizeOnly;
    /* 6/18/14-Walt - adding extra entity attribute */
    if (dataAttr.entityAttributes) var arrExtraEntAttr = dataAttr.entityAttributes.split(":");
    if (arrExtraEntAttr) {
        item.extraKey = arrExtraEntAttr[0];
        item.extraVal = arrExtraEntAttr[1];
    }

    return item;
}

function checkGiftCatalog(pc) {
    if(jQuery("div#donateDiv"+pc).hasClass("gc2014")){ //if GC item is detected, check motv & referrer
        if (typeof cart.data.Referrer == 'undefined')
            cart.data.Referrer = "gcDefaultReferrer";
        if (typeof cart.data.SourceCode == 'undefined')
            cart.data.SourceCode = "YS4G-015W";
        if (cart.data.Referrer == '')
            cart.data.Referrer = "gcDefaultReferrer";
        if (cart.data.SourceCode == '')
            cart.data.SourceCode = "YS4G-015W";

        cartRef = cart.data.Referrer;
        cartSrc = cart.data.SourceCode;

        setCookie('_motivation', cart.data.SourceCode, 1);
        setCookie('_referrer', cart.data.Referrer, 1);

    }
}

function updateItemInCart($this) { //The title says it all
    var item=getDonationInfo($this);
    var cartItems = cart.data.Items;
    jQuery.each(cartItems, function() {         //find existing item to update
        if (item.desig==this.Designation) {
            this.Quantity = item.qty;
            this.Amount = item.amt;

            jQuery.each(this.EntityAttributes, function(i){
                if(this.Key == 'RecurringDate') {
                    this.Value = item.recurringDate;
                }
            });
        }
    });
}

function addItemToCart($this) { //Add item to cart
    var itemInfo=getDonationInfo($this), ret, item;
    // setup the item object
    item = {
        "Description": itemInfo.desc,
        "Designation": itemInfo.desig,
        "Quantity": itemInfo.qty,
        "Amount": itemInfo.amt,
        "ItemValue": itemInfo.itemValue,
        "ResponseAllowed": itemInfo.honorMem,
        "AuthorizeOnly": itemInfo.authOnly,
        "IsOrder": itemInfo.isOrder,
        "EntityAttributes": [
            {"Key":"MaxQuantity", "Value":itemInfo.maxQty},
            {"Key":"RecurringDate", "Value":itemInfo.recurringDate},
            {"Key":"ResponseLimit", "Value":itemInfo.responseLimit}
        ]
    };

    if (itemInfo.extraKey && itemInfo.extraVal) {
        item.EntityAttributes.push({"Key":itemInfo.extraKey, "Value":itemInfo.extraVal});
    }

    if (memGiving) {
        cart.data.EntityAttributes[0]={
                           "Key": "Memorial.Id",
                           "Value": attr0val
                       }
        cart.data.EntityAttributes[1]={
                           "Key": "Memorial.Name",
                           "Value": attr1val
                       }
        cart.data.EntityAttributes[2]={
                           "Key": "Memorial.City",
                           "Value": attr2val
                       }
        cart.data.EntityAttributes[3]={
                           "Key": "Memorial.Region",
                           "Value": attr3val
                       }
    }

    checkGiftCatalog(itemInfo.desig);
    // trigger any bound functions before adding item to the cart
    item = ((ret=jQuery(window).triggerHandler('onCartAddItem', [item])) ? ret : item);
    cart.addItem(item);

}

function addUpdateItem($this, directCheckout) {
      var selId = jQuery($this).attr('id');
      var desig = selId.substring(4,13); //get item designation #
      checkGiftCatalog(desig);
      if (selId.substring(0,3)=="add" || selId.substring(0,3)=="amt") {
            var qty = jQuery("select#qty_"+desig).val(); //get user selected quantity
            if (!qty) qty=1; //regular donation items have no quantity
            var amt = jQuery("input#amt_"+desig).val(); //get user entered amount
            if (!amt) {
                amt="0.00";
            }
            amt = amt.replace("$","");
            if (!amt) amt=0; //books have no amount
            var dataAttr = jQuery("div#donateDiv" + desig).data();
            var desc = dataAttr.itemDescription;

            if(parseInt(dataAttr.recurringdate) > 0 && desc.indexOf(' (Monthly)') == -1) desc = desc + ' (Monthly)';

            var maxQty = dataAttr.maxQty;
            if (dataAttr.itemValue2) var arrVal2 = dataAttr.itemValue2.split(":");
            if (arrVal2) {
                var val2ID = arrVal2[0];
                var val2Val = arrVal2[1];
            }
            var itemValue = dataAttr.itemValue;
            var honorMem = dataAttr.allowResponse;
            var isOrder = dataAttr.isOrder;
            var authOnly = dataAttr.authorizeOnly;
            var items = cart.data.Items
            var respLimit = dataAttr.responseLimit;
            /* 6/18/14-Walt - adding extra entity attribute */
            if (dataAttr.entityAttributes) var arrExtraEntAttr = dataAttr.entityAttributes.split(":");
            if (arrExtraEntAttr) {
                var extraKey = arrExtraEntAttr[0];
                var extraVal = arrExtraEntAttr[1];
            }
            var updt = jQuery($this).text(); //determine whether button clicked was add, update, or checkout
            if (updt.length==0) { //should only occur with sidebar accordion donation items
                 if (items) {
                    jQuery.each(items, function(index) {
                        if (desig==this.Designation) {
                            updt = "Update";
                            return false
                        }
                    });
                }
                //if (updt!="Update") updt="Add";
            }
            if ((updt=="Update" || updt=="Checkout") && !directCheckout) { //update existing cart item (not allowed on merchandise items except books)
               if (items) {
                    jQuery.each(items, function(index) {
                        if (desig==this.Designation) {
                            this.Quantity = qty;
                            this.Amount = amt;
                            this.Description = desc;

                            if ((isOrder && qty==0) || (!isOrder && amt==0)) { //remove item if it's updated to zero qty or amt
                                cart.removeItem(index);
                                jQuery('#add_'+this.Designation).text("Add");
                            }

                                        jQuery.each(this.EntityAttributes, function(i){
                                                if(this.Key == 'RecurringDate') {
                                                        this.Value = dataAttr.recurringdate;
                                                }
                                        });

                            cart.save({success: function (data){
                                cartLoadedOK();
                                if (directCheckout || updt=="Checkout")
                                    window.location="/our-ministry/check-out/";
                              }, error: function(){
                                console.log('error updating quantity/amount');
                              }});
                        }
                    });
                } 


            }
            //change button to "Checkout" 12-3-13
            //else if (updt=="Checkout") {
            //    window.location = "http://www.samaritanspurse.org/our-ministry/check-out/";
            //}
            //end change button to "Checkout" 12-3-13
            else {   //add new cart item
               if (!qty) qty=1;
                if ((isOrder && qty>0) || (!isOrder && amt>0)) { //don't update cart if qty is zero
                    var size=jQuery("select#size_"+desig).val();
                    if (size) {
                        desig = desig + size;
                        desc = desc + " (" + size + ")";
                        if (size==val2ID) itemValue = val2Val; //if size is XXL, use higher $ value
                    }

                    var ret,item;

                    // setup the item object
                    item = {
                        "Description": desc,
                        "Designation": desig,
                        "Quantity": qty,
                        "Amount": amt,
                        "ItemValue": itemValue,
                        "ResponseAllowed": honorMem,
                        "AuthorizeOnly": authOnly,
                        "IsOrder": isOrder,
                        "EntityAttributes": [
                            {"Key":"MaxQuantity", "Value":maxQty},
                            {"Key":"RecurringDate", "Value":dataAttr.recurringdate},
                            {"Key":"Size", "Value":size},
                            {"Key":"ResponseLimit", "Value":respLimit}
                        ]
                    }

                    if (extraKey && extraVal) {
                        item.EntityAttributes.push({"Key":extraKey, "Value":extraVal});
                    }

                    // trigger any bound functions before adding item to the cart
                    item = ((ret=jQuery(window).triggerHandler('onCartAddItem', [item])) ? ret : item);

                    cart.addItem(item);

                    if (memGiving) {
                        cart.data.EntityAttributes[0]={
                                           "Key": "Memorial.Id",
                                           "Value": attr0val
                                       }
                        cart.data.EntityAttributes[1]={
                                           "Key": "Memorial.Name",
                                           "Value": attr1val
                                       }
                        cart.data.EntityAttributes[2]={
                                           "Key": "Memorial.City",
                                           "Value": attr2val
                                       }
                        cart.data.EntityAttributes[3]={
                                           "Key": "Memorial.Region",
                                           "Value": attr3val
                                       }
                    }
                    cart.save({success: function (data){
                         cartLoadedOK();
                        if (directCheckout)
                            window.location="/our-ministry/check-out/";
                        else {
                            if (size)
                                jQuery('#add_'+desig.replace(size,"")).text("Checkout");
                            else
                                jQuery('#add_'+desig).text("Checkout");
                        }
                      }, error: function(){
                        console.log('error adding item');
                      }});
                }
            } //end 'else add'
        }
}

//set var to determine if we're on the checkout page
var checkOut = false;
//set var to determine if donation items exist on page
if (jQuery(":input.donationitem").length>0 || jQuery(".donationitem-group").length>0)
    var donate = true;
else
    var donate = false;


//set cart vars if cookies exist
var cartId=getCookie("_cartid"),cartCookieCount=getCookie("_cart-count"),cartDisp=getCookie("_cart-display");
var cartTotalAmt=getCookie("_cart-total-amount"), cartSrc=getCookie("_motivation"), cartRef=getCookie("_referrer");

//set mega-menu checkout button based on cookies
setMegaMenuDonateFromCookies();

if (donate) { //if on a page with donation items...load the cart
    if (!cart) { //if cart does not exist, create it
      var cart = new Cart({Referrer: cartRef, SourceCode: cartSrc}, '');
    }
    if (cartId) { // if _cartId cookie exists, load the cart
      cart.load({success: function(){
            cartLoadedOK();
        }, error: function(){
            console.log('Problem occurred with cart load');
        }});       
    }

   jQuery("button.donationitem-add").live("click", function() { //update cart if quantity is changed
        if (this.innerHTML == 'Checkout')
            window.location='/our-ministry/check-out';
        else
            addUpdateItem(jQuery(this));
   });  

   jQuery("div.megamenu-donate-button").live("click", function() { //hijack the Checkout button and make sure there's an item in the cart
        if (cart.data.Items.length<1){
            alert("You must enter an amount before clicking the Checkout button.  If you are still having trouble and would like to donate by phone, please call 1-800-528-1980."); 
            return false;
        }

   });

    jQuery("a#sideSubBtn, button.directCheckout").live("click", function() { //add items if sidebar "Complete your gift" btn is clicked
        var temp = jQuery(this).prev();
        if (typeof temp != 'undefined' && temp.length>0 && temp[0].value<1) {
            alert('You must enter an amount before clicking the "Checkout" button.  If you are still having trouble and would like to donate by phone, please call 1-800-528-1980.');
            return false;
        }
        jQuery("html").addClass("busy");
        var itemLen = jQuery(":input.donationitem").length; //how many donation items exist on page
        for (var i=0;i<itemLen;i++)                         //loop through all query items on page
        { 
            if (jQuery(":input.donationitem")[i].value.length>0) { //if query item has no amount, skip it
                if (cart && cart.data.Items.length>0) {     //loop through the cart see if item already exists in cart (don't add dupe)
                    var updated=false;
                    var items = cart.data.Items;
                    var tempDesig = jQuery(":input.donationitem")[i].id;
                    tempDesig = tempDesig.replace("amt_","");
                    jQuery.each(items, function() {         //compare input item desig with each cart item desig
                        if (tempDesig==this.Designation) {
                            updateItemInCart(jQuery(":input.donationitem")[i]);
                            updated=true;
                        }
                    });
                    if (!updated)
                        addItemToCart(jQuery(":input.donationitem")[i]);
                }
                else        //no items in cart, so just add the item
                    addItemToCart(jQuery(":input.donationitem")[i]);
            }
        }          
        cart.save({success: function (data){
             updateCookiesFromCart();
             window.location="/giving/check-out/";
          }, error: function(){
            console.log('error adding item to cart');
        }}); 
        jQuery("html").removeClass("busy");     //change cursor back to normal status if needed
   });  //end sideSubBtn click
   
   
   
   //if merchandise items are present, check to see if any are out of stock
   var merchItems = jQuery('*[data-is-order="true"]');
   if (merchItems.length>0) { //only run this if merch items are on the page
       
       jQuery.ajaxSetup({ cache: false });
       //retrieve backordered list
        jQuery.getJSON( "/api/proxy_json_version2.php/svc/helper/sp/inventory/backordered.json", function( data ) {
            jQuery.each(data.items, function(key, obj) { //loop through returned out of stock items
                var outSKU = obj.sku.replace(/\D+/, '');
                var outSize = obj.sku.replace(/\d+/, '');
                jQuery.each(merchItems, function(key, value) { //loop through items on the page looking for matches
                    var sku=jQuery(value).data('sku');
                    if (sku==outSKU) { //merch item on the page matches an out of stock item
                        if (outSize) { //merch item has sizes
                            var sizeSelect=jQuery(value).find("select#size_"+sku+" > option");
                            jQuery(sizeSelect).each(function() {
                                if (jQuery(this).val() == outSize) {
                                    jQuery(this).attr('disabled','disabled');
                                    jQuery(this).text(jQuery(this).text() + " *Out of stock");
                                }
                            });
                        } else { //merch item does not have sizes
                            jQuery(value).find("div.di-full-payment").html("<div class='merchOOS'>Temporarily out of stock.<br>Please check back later.</div>");
                        }

                    }

               });
            });

            
            
         });
       
      

        //Create the XCRM item (run this on pages w/ new merch items
        /*jQuery.each(merchItems, function(key, value) {
            var sku=jQuery(value).data('sku');
            var select=jQuery(value).find("select#size_"+sku+" > option");
            var desc=encodeURI(jQuery(value).find("span.di-full-title").html());
            if (select.length>0) {
                jQuery(select).each(function() {
                     jQuery.ajax({
                         url: "/api/proxy_json_version2.php/svc/helper/sp/inventory/check/"+ sku+jQuery(this).val() +".json?name="+desc
                     });

                 });
             } else {
                 jQuery.ajax({
                     url: "/api/proxy_json_version2.php/svc/helper/sp/inventory/check/"+ sku +".json?name="+desc
                 });

             }
        }); */
   
   }
} //end "if donate"

if (donate || checkOut) {
  jQuery(function(){  //set cursor 'wait' state when ajax is called
      jQuery("html").bind("ajaxStart", function(){  
         jQuery("html").addClass('busy');  
       }).bind("ajaxStop", function(){  
         jQuery("html").removeClass('busy');  
       });  
  });
}


