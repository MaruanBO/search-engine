    require(["loader"], function() {
        require(['jquery', 'angular', 'angular-ui-bootstrap', 'signin', 'dashboard-nav'], function ($, ng) {
            'use strict';
            var userMenu = ng.module("userMenu", ["ui.bootstrap"]);

            userMenu.controller("UserMenuCtrl", ["$scope", function ($scope) {
            }]);

            ng.bootstrap(document.getElementById('user_menu'), ['userMenu']);

            // Register AngularJS mini-app
            // avoid conflicts with javascript mvc
            if (window.location.pathname.indexOf('client/invitation') === -1) {
                ng.bootstrap(document.getElementById('signin'), ['signin']);
            }

            // main_nav handler
            if( window.location.pathname.toLowerCase() === "/dashboard" || window.location.pathname.toLowerCase() === "/activities" ) {
                // We are in the dashboard or the activities page, so let's help manage the navigation
                ng.bootstrap(document.getElementById('main_nav'), ['dashNav']);
            }
        });
    });
