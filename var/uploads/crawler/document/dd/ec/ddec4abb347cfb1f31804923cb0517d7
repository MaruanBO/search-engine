<script>
    $('.email-aw').watermark('Email Address');
    $('.password-aw').watermark('Password');

    $.ajax({
        url: "/validateEdb/channel-8498/",
        async: false
    }).done(function(premiumAllowed) {
        premiumAllowed = +premiumAllowed;
        if (edbTokens.productToken == null || !premiumAllowed) {
            $('ul.signed-in').remove();
            $('ul.signed-out').show();
            if ( edbTokens.errorMessage && edbTokens.signInError=='header' ){
                $('#signIn').prepend('<div class="error-wrap error-back-light" style="display: block; "><span class="error-text error-text-light">'+
                edbTokens.errorMessage+
                '</span><img src="/img/utils/error_dark.png"/></div>');
                if(edbTokens.emailForPrompt){
                    $("input.useremail").val(edbTokens.emailForPrompt);
                }
            }
        } else {
            $('ul.signed-out').remove();
            $('ul.signed-in').show();
        }
    });
</script>

