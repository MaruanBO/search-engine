<script>
$(document).ready(function(e) {
    $('.tabs').each(function(index, element){
		var tab = $(this);
		var LIs = $(this).children('ul').children('li');
		var DIVs = $(this).children('div')
		LIs.on('click', 'a', function(e){
			e.preventDefault();
			var self = $(this);
			LIs.removeClass('active');
            LIs.find('img').each(function(){
                var img = $(this);
                img[0].src = img[0].src.replace('images/active', 'images');
            });
			DIVs.removeClass('active');
			self.parent('li').addClass('active');
            self.children('img').each(function(){
                var img = $(this);
                img[0].src = img[0].src.replace('images', 'images/active');
            });
			$(self.attr('href')).addClass('active');
		});
	});
	$('.navbar-toggle').on('click', function(e){
        e.preventDefault();
        console.log($('.header-nav ul'));
        $('.header-nav ul').toggleClass('open');
    });
});
</script>
