<script type="text/javascript">
    function roll_over(img_name, img_src) {
        $('img[name=' + img_name + ']').attr('src', img_src);
    }
</script>
<script type="text/javascript">
    $(function () {
        (function () {
            var largePromo_rotator = $("#largePromo_rotator").scrollable({
                circular: true,
                keyboard: false,
                speed: 1700,
				onSeek:function(){
					seekPause();
				}
            });

            var imgs = largePromo_rotator.find('.largePromo-item').not('.cloned').find('img.feature'),

    nav = $('#navigatorDiv'),
    navWidth = null,
    collapseBtn = $('<div class="collapse-btn"></div>').prependTo(nav);
            if (imgs.length > 1) {
                largePromo_rotator.autoscroll({ autopause: true, autoplay: true, interval: 5000 }).navigator({ naviItem: 'a' })
                nav.css("display", "block");
                nav.find('a').each(function (i) {
                    $img = $('<img/>').attr({
                        src: imgs[i].src,
                        width: 95,
                        height: 45
                    });
                    $(this).html($img);
                });
            }
            navWidth = nav.width();
            collapseBtn.toggle(
      function () {
          nav.stop().animate({ 'right': -(navWidth - 30) }, 500);
          $(this).addClass('collapsed');
      },
      function () {
          nav.stop().animate({ 'right': 0 }, 500);
          $(this).removeClass('collapsed');
      }
      );
	  
	   largePromo_rotatorAPI = largePromo_rotator.data("scrollable");
	   
	   function seekPause()	{
		largePromo_rotatorAPI.pause();
		var timer = setTimeout(function(){largePromo_rotatorAPI.play();clearTimeout(timer);},5000); 
	 }
        })();
    })
</script>


        
        
        
