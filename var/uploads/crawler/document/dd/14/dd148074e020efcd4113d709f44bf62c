      <script type="text/javascript">

  function buildAndSend(prefix) {
    slide = $('#topstory > div').filter(function() { return $(this).css("display") == "block" })[0];
    label = "";
    label += prefix + " - ";
    
    
    label += "Slide " + slide.id.slice(-1) + " - "; 
    if ( $(slide).children(".mask:first").hasClass('video') ) {
      label += $(slide).children().children().children('iframe').attr("src");
    } else {
      label += $(slide).children().children().children('a').attr('href');
    }
    
    
    //console.log(label);
    _gaq.push(['_trackEvent', 'button', 'click', label]);
    return label;    
  }

  $(document).ready(function() {
    // Hero
    $('#controls li a').click(function() {
      pre = "Hero Tab";
      pre += this.href.slice(-1)
      buildAndSend(pre);
    });
    
    $('#topstory div').click(function(event) {
      buildAndSend("Hero Click");
    });
    
    // Feed
    $('#feed-tabs li a').click(function() {
      label = "Feed ";
      label += $(this).html() + " Tab";
      //console.log(label);
      _gaq.push(['_trackEvent', 'button', 'click', label]);
    });

    $('#feed div.teaser').find('a').click(function(event) {
      label = "Feed Post - " + $(this).attr('href');
      //console.log(label);
      _gaq.push(['_trackEvent', 'button', 'click', label]);
    });
    
  });

</script>

<script type="text/javascript">
