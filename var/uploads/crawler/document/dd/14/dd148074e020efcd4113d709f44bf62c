<script>
$(document).ready(function(){
   $('#pac-contribute-form').validate({
	  onkeyup: false,
	  rules: {
			amount_other: {
		  	required: true,
		  	number: true
			}
	  },
	  messages: {
			amount_other: {
		  	required: '*Required',
		  	number: 'Add #'
			}
	  },
	  errorClass: "error",
	  errorPlacement: function(error, element) {
			var msg = $(error).html();
			$(element).val(msg);
	  }
	});
});
</script>



