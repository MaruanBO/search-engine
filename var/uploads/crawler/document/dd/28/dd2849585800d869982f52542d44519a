<script type="text/javascript">
$(document).ready(function() {

	$("#launch_time").countdown({
		date: "October 28, 2011 10:48:01", /* Satellite Launch Date. The date is off by an additional 5 hours. */
		offset: 1,
		onComplete: function( event ){
		
			$(this).html("<div class=\"countdown_complete\">LAUNCHED!</div>");
		},
		onPause: function( event, timer ){

			$(this).html("Pause");
		},
		onResume: function( event ){
		
			$(this).html("Resumed");
		},
		leadingZero: true
	});
	
	//$("#time2").countdown('pause');
	
	//$("#time2").countdown('resume');

});
</script>
