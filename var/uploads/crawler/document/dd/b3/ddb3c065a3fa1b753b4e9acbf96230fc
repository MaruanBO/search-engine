<script type="text/javascript">
    jQuery.noConflict();

    (function ($) {
        var cookieEnabled;
        var choiceValue;

        $(document).ready(function () {
            isBrowserCookiesEnabled();

            $('input[name="poll_1e79c49c-9341-4788-8b49-f8bab433cfcd"]').click(function () {
                if ($('#vote_1e79c49c-9341-4788-8b49-f8bab433cfcd').is(':disabled') && cookieEnabled) {
                    $('#vote_1e79c49c-9341-4788-8b49-f8bab433cfcd').prop('disabled', false);
                }

                choiceValue = $(this).val();
            });

            $('#vote_1e79c49c-9341-4788-8b49-f8bab433cfcd').click(function () {
                $(this).prop('disabled', true);
                $('#showPollResults_1e79c49c-9341-4788-8b49-f8bab433cfcd').hide();

                $.ajax({
                    url: 'http://cms.teamusa.org/api/poll/vote/1e79c49c-9341-4788-8b49-f8bab433cfcd/' + choiceValue,
                    type: 'POST'
                })
                .done(function (poll) {
                    if (poll != undefined && poll.Choices != undefined) {
                        if (poll.Error) {
                            alert(poll.Error);
                        }
                        else {
                            writeCookie('Poll_1e79c49c-9341-4788-8b49-f8bab433cfcd', choiceValue, 365);
                            updatePoll(poll);
                        }
                    }
                    else {
                        alert('An error has occurred, please try again later.');
                    }
                })
                .fail(function () { alert('Updating the poll failed, please try again later.') });
            });

            $('#showPollResults_1e79c49c-9341-4788-8b49-f8bab433cfcd').click(function () {
                $('#pollContent_1e79c49c-9341-4788-8b49-f8bab433cfcd').find('div[id^="choice_"]').each(function () {
                    $(this).hide();
                });

                $('#pollContent_1e79c49c-9341-4788-8b49-f8bab433cfcd').find('p.hidden').each(function () {
                    $(this).removeClass('hidden');
                });

                $('#pollContent_1e79c49c-9341-4788-8b49-f8bab433cfcd').find('.progress').each(function () {
                    $(this).show();
                });

                $('#vote_1e79c49c-9341-4788-8b49-f8bab433cfcd').hide();
                $(this).hide();
                $('#totalVotesSection_1e79c49c-9341-4788-8b49-f8bab433cfcd').show();

                return false;
            });
        });

        function isBrowserCookiesEnabled() {
            cookieEnabled = (navigator.cookieEnabled) ? true : false

            if (!cookieEnabled) {
                alert("Cookies are required for the polls. Please enable cookies in your browser settings and reload page.");
            }
        }

        function updatePoll(poll) {
            $.each(poll.Choices, function (i, item) {
                $('#choice_' + item.ChoiceId).hide();
                $('#progressbar_' + item.ChoiceId).attr('aria-valuenow', item.Percent);
                $('#progressbar_' + item.ChoiceId).width(item.Percent + '%');
                $('#votes_' + item.ChoiceId).html(item.Percent + '%');
            });

            $('#progressbar_' + choiceValue).addClass('progress-bar-danger');

            $('#pollContent_1e79c49c-9341-4788-8b49-f8bab433cfcd').find('p.hidden').each(function () {
                $(this).removeClass('hidden');
            });

            $('#pollContent_1e79c49c-9341-4788-8b49-f8bab433cfcd').find('.progress').each(function () {
                $(this).show();
            });

            $('#vote_1e79c49c-9341-4788-8b49-f8bab433cfcd').hide();

            $('#totalVotes_1e79c49c-9341-4788-8b49-f8bab433cfcd').html(poll.TotalVotes);
            $('#totalVotesSection_1e79c49c-9341-4788-8b49-f8bab433cfcd').show();
        }

        function writeCookie(name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = "; expires=" + date.toGMTString();
                document.cookie = name + "=" + value + expires + "; path=/;";
            }
            else {
                var expires = "";
                document.cookie = name + "=" + value + expires + "; path=/;";
            }
        }
    })(jQuery);
</script><div class='ad ad-300x250' style="overflow: hidden; width: 100%;">
