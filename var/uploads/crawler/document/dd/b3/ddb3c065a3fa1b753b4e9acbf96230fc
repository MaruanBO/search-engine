    (function ($) {
        

        $('.bxslider').bxSlider({
     
            buildPager: function(slideIndex){
					switch(slideIndex){
							
								case 0:
								    return '<img src="http://www.teamusa.org/~/media/TeamUSA/BlogsWinter/ElanaMeyers/Blog12_021915_800x500.jpg?as=0&mw=150&mh=85&ignoreCopyrightOverlay=true" />';
							
							
								case 1:
								    return '<img src="http://www.teamusa.org/~/media/TeamUSA/Bobsled/Williams_Lauryn_021915_800x500.jpg?as=0&mw=150&mh=85&ignoreCopyrightOverlay=true" />';
							
							
								case 2:
								    return '<img src="http://www.teamusa.org/~/media/TeamUSA/Fencing/Kiefer_Lee_072412_800x500.jpg?as=0&mw=150&mh=85&ignoreCopyrightOverlay=true" />';
							
							
								case 3:
								    return '<img src="http://www.teamusa.org/~/media/TeamUSA/Ski Jumping/Englund_Nita_012515_800x500.jpg?as=0&mw=150&mh=85&ignoreCopyrightOverlay=true" />';
							
							
								case 4:
								    return '<img src="http://www.teamusa.org/~/media/TeamUSA/BlogsWinter/MadisonChock/Blog2_021715_800x500.jpg?as=0&mw=150&mh=85&ignoreCopyrightOverlay=true" />';
							
					}

                 },
            
            auto: true,
            mode: 'horizontal',
            controls: false,
            pause: 6500
        });

    })(jQuery);

</script>
