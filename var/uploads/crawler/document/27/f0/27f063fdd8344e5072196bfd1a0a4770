            <script id="AllEventsTemplate" type="text/x-jQuery-tmpl">
                {{if isArticle !== null}}
                    <div class="tile editorial ${all_class} ${eventFilter}">
                        <div class="eventboxdate" style="display: none;">${eventBoxDate}</div>
                        <!-- Article source name -->
                        {{if articleSourceText !== null}}
                                <span class="source ${articleSourceClass}">${articleSourceText}</span>
                        {{/if}}
                        {{if articleTitle !== null}}
                            <!-- Article title -->
                        <h1 class="title">${articleTitle}</h1>
                        {{/if}}
                        <!-- Aticle image.
                        Usually the main media image or the main inline image -->
                        {{if articleImage !== null}}
                            <span class="articleUrl" url="${articleUrl}">
                                <img class="articleImage" src="${articleImage}" alt="" /></span>
                        {{/if}}
                        <!-- A brief txt from the beginning of the article.
                            Usually this is the text before the article-break, or we should set a limit on the number of words -->
                        {{if articleDescription !== null}}
                            <div class="brief">
                                <p>
                                    ${articleDescription}
                                </p>
                            </div>
                        {{/if}}
                        <!-- Link to source article -->
                        <a href="#" class="articleUrl priceFrom" url="${articleUrl}">continue reading <i class="fa-angle-right"></i></a>
                    </div>
                {{else}}  
