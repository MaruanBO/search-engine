			function setlimit() {
				var sel = document.getElementById('speed');
				var limit = sel.options[sel.selectedIndex].value;
				var exdate=new Date();
				exdate.setDate(exdate.getDate()+30);
				document.cookie='throttlespeed'+'='+escape(limit)+';expires='+exdate.toGMTString()+';path=/;domain=abcmouse.com;';
			}

			function PopupCover() {
				//PopupCover.coverCount++;
				if(PopupCover.activeCover != null) return;

				PopupCover.activeCover = this;
				//if(PopupCover.activeCover != null) return;
				var cover = document.createElement("div");
				cover.className = "popupcover";

				this.close = function() {
					cover.parentNode.removeChild(cover);
					PopupCover.activeCover = null;
				};

				document.body.appendChild(cover);
			}

			PopupCover.activeCover = null;
			PopupCover.close = function() {
				if(PopupCover.activeCover != null) {
					PopupCover.activeCover.close();
				}
			};

