				}
			}

			function showDivPopup(divId, width, height) {
				var position = getCenteredPosition(width, height);
				var popup = document.getElementById(divId);
				if(popup == null) return;

				popup.style.top = position.y + "px";
				popup.style.left = position.x + "px";

				showElement(divId);
				new PopupCover();
			}

			function closeDivPopup(divId) {
				hideElement(divId);
				PopupCover.close();
			}

			function inArray(value, array) {
				for(var x=0; x < array.length; x++) {
					if(array[x] == value)
						return true;
				}

				return false;
			}
			
			function showShortCut(langtype) {
				switch (langtype) {
					case "zhs":
						ABCmouseUtils.showIframePopup({url: '/shortcut_zhs', width:737, height:562});  //pl08042014
						break;
					case "com":
						ABCmouseUtils.showIframePopup({url: '/shortcut', width:737, height:562});  //pl08042014
						break;
					
				}
			}
			
			function closeShortCut() {
				ABCmouseUtils.closeIframePopup(); //pl08042014
			}
			
