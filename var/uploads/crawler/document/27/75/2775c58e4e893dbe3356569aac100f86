			function showWaitPixel() {
				var pixel = document.getElementById("wait_pixel");
				if(pixel != null && getCookie("wait-popup") == "")
					pixel.style.display = "block";
			}

			function removeWaitPixel() {
				var pixel = document.getElementById("wait_pixel");
				if(pixel != null) {
					pixel.parentNode.removeChild(pixel);
					setCookie("wait-popup",1,365);
					if(homepageVersion == 379)
						setCookie("sweepstakes_popup",1,365);
				}
			}

			var firstMonthPopupTimer = null;
			function showFirstMonthPopup(width, height) {
				var popup = null;
				if(homepageVersion == 2201) {
					new ABCmouseUtils.ABCmousePopup({
						id: "mini_reg_popup",
						title: "Thank You for Watching Our Video",
						className: "mini_reg_popup blue_popup_002",
						script: "/popups/mini_reg_popup.php"
					}).show();	
					
					return;
				} else if(homepageVersion >= 2801) {
					new ABCmouseUtils.ABCmousePopup({
						id: "mini_reg_popup",
						title: "Thank You for Watching Our Video!",
						className: "mini_reg_popup yellow_popup_001",
						script: "/popups/mini_reg_popup.php"
					}).show();
					
					return;
				}
				
				if(getCookie("elog") == "") {
					
					showPopup("/email_special/2/multi/211/1424381799.5724", width, height);
					firstMonthPopupTimer = setTimeout(function() {
						//closePopup();
					}, 3000);
					
				} else  {
					width = width || 576;
					height = height || 335;

					var popup = document.getElementById("getYourFirstMonthPopup");
					if(popup == null) return;

					var pos = getCenteredPosition(width, height);
					var topPos = pos.y;
					var leftPos = pos.x;
					
						
					popup.style.top = topPos + "px";
					popup.style.left = leftPos + "px";
					showElement("getYourFirstMonthPopup");

					new PopupCover();
				}
			}

			function closeFirstMonthPopup() {
				hideElement("getYourFirstMonthPopup");
				PopupCover.close();
			}

