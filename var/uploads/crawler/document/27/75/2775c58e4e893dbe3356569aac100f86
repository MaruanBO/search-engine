		function swf(id) {
			if (navigator.appName.indexOf("Microsoft") != -1) return window[id];
			else return document[id];
		}

		 var soundPlayed = false;
		function playSound() {
			if(soundPlayed)
				return;
			if(swf("rolloverplayer") != null && (typeof swf("rolloverplayer").startSound == "function")) {
				soundPlayed = true;
				swf("rolloverplayer").startSound('https://cdni.abcmouse.com/home/homepage/free-site-42/clickonanybuttontogetstarted.mp3');
				setCookie("freeSiteAudio",1,1);
			} else {
				setTimeout(function(){playSound();},10);
			}
		}

		var filePreloaded = false;
		function preloadPopup() {
			if(filePreloaded)
				return;

			if(swf("rolloverplayer") != null && (typeof swf("rolloverplayer").cacheFile == "function")) {
				filePreloaded = true;
				swf("rolloverplayer").cacheFile('https://cdni.abcmouse.com/windows/free_content_minireg_pop.swf');
			} else {
				setTimeout(function(){preloadPopup();},10);
			}
		}

		var videoPreloaded = false;
		function preloadVideo() {
			if(videoPreloaded)
				return;

			if(swf("rolloverplayer") != null && (typeof swf("rolloverplayer").cacheFile == "function")) {
				videoPreloaded = true;
				swf("rolloverplayer").cacheFile('https://cdni.abcmouse.com/home/homepage/video/siteoverview13_720x405.f4v');
			} else {
				setTimeout(function(){preloadVideo();},10);
			}
		}

		function executeTracking()
		{
			var kvpTemp     =   'action=OldHomePageLoadDetails|browserTypeVersion=Default--Browser|deviceTypeScreenSize=Desktop--xxx|platformTypeVersion=unknown--|homepageDetails=a-__-4382:_:b-__-yyy:_:c-__-en-en:_:d-__-zzz:_:e-__-oldHPLd';
			var kvpTemp1    =	kvpTemp.replace("xxx"  , "w"+screen.width     +"x"+screen.height     +"h");
			var kvpTemp2    =	kvpTemp1.replace("yyy" , "w"+screen.availWidth+"x"+screen.availHeight+"h");
			var kvpTemp3    =	kvpTemp2.replace("zzz" , screen.colorDepth);
			var kvp         =   kvpTemp3;

			track('homepageloadInfo', kvp);
		}

		function showDelayedPopup() {
			if(!VideoPlayer.isOpen && !FlowVideoPlayer.isOpen && homepageVersion == 158)
				showSurveyPopup(-1);
		}

			function showLoginBox() {
				//popup = document.getElementById('loginBox');
				document.getElementById('loginBox').src = '/loginbox';
				document.getElementById('loginBox').style.display = "";
				//document.body.appendChild(popup);
				new PopupCover();
			}
			function closeLoginBox() {
				document.getElementById('loginBox').style.display = 'none';
				//document.getElementById('loginBox').innerHTML = '';
				PopupCover.close();
			}

			function showTestimonialBox() {
				track("Testimonial_Home");
				ABCmouseUtils.showTestimonialPopup();
				return; //s.h. 20130717

				new PopupCover();

				var popupIframe = document.getElementById('testimonialBox');
				popupIframe.src = '/testimonialbox';
				popupIframe.style.display = "block";
				popupIframe.style.position = "fixed";
				if(window.innerHeight) {
					var marginTop = (window.innerHeight - 610)/2;
					var marginLeft = (window.innerWidth - 750)/2;
				}
				else {
					var marginTop = (document.documentElement.clientHeight - 610)/2;
					var marginLeft = (document.documentElement.clientWidth - 750)/2;
				}
				popupIframe.style.position = "fixed";
				if(marginTop < 0) { marginTop = 4; }
				popupIframe.style.top = marginTop + "px";
				popupIframe.style.left = marginLeft + "px";

				document.body.appendChild(popupIframe);

			}
			function closeTestimonialBox() {
				iframe = document.getElementById("testimonialBox");
				iframe.style.display = 'none';
				PopupCover.close();
				if('contentWindow' in iframe) { iframeDoc = iframe.contentWindow; }
				else if('contentDocument' in iframe) { iframeDoc = iframe.contentDocument; }
				iframeDoc.document.head.innerHTML = "";
				iframeDoc.document.body.innerHTML = "";
			}
