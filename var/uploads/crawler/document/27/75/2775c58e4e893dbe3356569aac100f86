			var mobile_device = "";
			
			var is_android_device = (mobile_device == "android" || mobile_device.indexOf("kindle") != -1) ? true : false;
			
			if(is_android_device) {
				var html5VideoPlayer = new VideoPlayer(IMGHOST + "/home/homepage/video/wia_android.mp4", "html5_video_holder", true, "", 720, 405, true, false, "none", "NMHP");
