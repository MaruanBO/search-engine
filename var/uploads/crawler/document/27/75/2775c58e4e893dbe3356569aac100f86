			function resizeIframe(height) {
				var browser = "Default Browser 0";

				if(browser == "Internet Explorer 7.0") {
					height -= 10;
				}
				document.getElementById("subPage").style.height = height + "px";
			}

			function showSurveyPopup(videoPlayerId) {
				var source = "unknown";
				var forceSurvey = (videoPlayerId == -100) ? true : false;

				if(VideoPlayer.isOpen && videoPlayerId >= 0) {
					VideoPlayer.close(videoPlayerId);
				}

				if(getCookie("survey-completed") == "" && ((homepageVersion != 88 && homepageVersion != 89 && homepageVersion != 134 && homepageVersion != 151) || forceSurvey)) {
					if(getCookie("survey-popup") != "02.19.15" || videoPlayerId == -30 || forceSurvey) {

						if(videoPlayerId >= 0) {
							source = "videoClose-Other";
						} else if(videoPlayerId == -1) {
							source = "autopop";
						} else if(videoPlayerId == -30) {
							source = "video-seal";
						} else {
							source = "videoClose";
						}

						showPopup('/survey-popup/' + source,591,1300, "absolute");
						setCookie("survey-popup","02.19.15",365);
						new PopupCover();
						return;

					}
				}

				if(getCookie("survey-popup") == "") {
					showPopup('/survey-popup/videoClose',591,1300, "absolute");
					setCookie("survey-popup",1,30);
					new PopupCover();
				}
			}

			function closeSurveyPopup() {
				closePopup();
				PopupCover.close();
			}

			function showSurveyErrorMessage() {
				if(document.getElementById("errorsPopup") != null)
					return;

				var errorMessage = '<div id="errorsPopup">' +
					'<div id="top">' +
						'<div id="title">' +
							'Oops!' +
						'</div>' +
						'<div id="closeIcon" onclick="closeSurveyErrorMessage();"></div>' +
					'</div>' +

					'<div id="popupContent">' +
						'<div class="message">Please answer all 3 survey questions to continue.</div>' +
						'<div class="okBtn">' +
							'<img src="https://cdni.abcmouse.com/home/survey-popup/ok-btn.png" onclick="closeSurveyErrorMessage();" />' +
						'</div>' +
					'</div>' +
					'<div id="bottom"></div>' +
				'</div>';

				var errorsDiv = document.createElement("div");
				errorsDiv.innerHTML = errorMessage;

				var position = getCenteredPosition(560, 233);

				errorsDiv.style.position = "fixed";
				errorsDiv.style.zIndex = "101";
				errorsDiv.style.left = position.x + "px";
				errorsDiv.style.top = position.y + "px";

				document.body.appendChild(errorsDiv);
			}

			function closeSurveyErrorMessage() {
				var error = document.getElementById("errorsPopup");

				if(error)
					error.parentNode.removeChild(error);
			}

