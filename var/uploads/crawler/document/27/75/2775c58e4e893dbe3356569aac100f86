			function alertVideoClose() {
				if(document.getElementById('seeallvideos').src.search(/videopanel/) == -1) return;
				document.getElementById('seeallvideos').style.display = "block";
				new PopupCover();
			}

			function getPos(obj) {
				var winW = "innerWidth" in window ? window.innerWidth : document.body.offsetWidth;
				var pos = {x: 0, y: 0};
				while(obj) {
					pos.x += obj.offsetLeft;
					pos.y += obj.offsetTop;
					obj = obj.offsetParent;
				}
				if(pos.y != 0)
					pos.y -= "pageYOffset" in window ? window.pageYOffset : document.documentElement.scrollTop;
				if(pos.x != 0)
					pos.x -= "pageXOffset" in window ? window.pageXOffset : document.documentElement.scrollLeft;
				if(pos.y < 10) pos.y = 10;
				if(pos.x + VideoPlayer.META_DATA.width > winW)
					pos.x = winW - VideoPlayer.META_DATA.width;
				return pos;
			}

			function videoPos(file) {
				var pos = {x: 0, y: 0};
				switch(file) {
					case "siteoverview2.f4v":
					//case "artandcolor_overview.f4v":
					case "artandcolor_overview_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionright_doublesection1"));
						break;
					//case "coloring_options.f4v":
					case "coloringoptions_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionright_doublesection1"));
						break;
					//case "books_overview.f4v":
					case "books-overview_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionleft_doublesection1"));
						break;
					//case "music_overview.f4v":
     				case "musicoverview_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionright_doublesection2"));
						break;
					//case "puzzles_overview.f4v":
					case "puzzles_overview_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionleft_doublesection2"));
						break;
					//case "games_overview.f4v":
					case "gamesoverview_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionright_doublesection3"));
						break;
					//case "printables_overview.f4v":
					case "printables_720x405.f4v":
						pos = getPos(document.getElementById("doublesectionleft_doublesection3"));
						break;
				}
				return pos;
			}

