			window.onload = function() {
												executeTracking();
				
				
				initializePopups();
				loadImages();

							};

			function loadImages() {
				var images = document.getElementsByTagName("img");
				var image, preloaderCounter = 0, imgCounter = 0;;

				for(var x=0; x < images.length; x++) {
					image = images[x];
					if(image.getAttribute("data-source") != null) {
						image.onload = function() {
							doneLoading();
						};
						image.src = image.getAttribute("data-source");
						imgCounter++;
					}
				}

				function doneLoading() {
					preloaderCounter++;
					if(preloaderCounter == imgCounter) {
												track('homepagefullload', 'src=homepagefullload');
											}
				}
			}

