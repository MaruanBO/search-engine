			function playHTML5Video(file) {
				var position 						= getCenteredPosition(720,405);

				ActiveVideoDiv                      =   document.getElementById("html5_video_holder");
				ActiveVideoDiv.style.zIndex         =   '103';
				ActiveVideoDiv.style.top            =   position.y + "px";
				ActiveVideoDiv.style.left           =   position.x + 'px';
				ActiveVideoDiv.style.visibility     =   'visible';
				ActiveVideoDiv.style.display     	=   'block';
				html5VideoPlayer.setPosition(0);
				
				if(html5VideoPlayer.mediaplayer.canPlayType && html5VideoPlayer.mediaplayer.canPlayType('video/mp4')) {
					html5VideoPlayer.mediaplayer.src	= IMGHOST + "/home/homepage/video/" + file;
				} else {
					html5VideoPlayer.mediaplayer.src	= IMGHOST + "/home/homepage/video/" + file.replace('.mp4','.ogv');
				}
				
				if(is_android_device) {
					document.getElementById("vp_totaltime_1").style.display = "none";
					document.getElementById("vp_progressbarbackground_1").style.width = "478px";
					document.getElementById("vp_currenttime_1").style.left = "578px";
				}
				
				html5VideoPlayer.play();
				
				html5VideoPlayer.addEventListener("closed", function onclose() {
					if(file == "wia_android.mp4") {
						closePopup();
						//showFirstMonthPopup(576,345);
						showFirstMonthPopup(864,518);
					}
					document.getElementById("vp_background_1").style.visibility = "hidden";
					html5VideoPlayer.removeEventListener("closed", onclose);
				});
				
				if(mobile_device.indexOf("kindle") != -1) {
					html5VideoPlayer.addEventListener("pause", function() {
						if(!document.webkitIsFullScreen) {
							html5VideoPlayer.close_video();
						}
					});
				}
				
				html5VideoPlayer.addEventListener("completed", function oncomplete() {
					if(file == "wia_android.mp4") {
						if(!document.webkitFullscreenElement || document.webkitFullscreenElement == null || document.webkitFullscreenElement == '') { html5VideoPlayer.close_video(); }
						else {
							html5VideoPlayer.mediaplayer.webkitExitFullScreen();
							html5VideoPlayer.close_video();
						}
						//closePopup();
						//var position = (getCookie("elog") != "") ? {x:344, y: 110} : {x:346, y: 52};
						//showPopup("/email_special/3/multi/211/1424381799.5723", 335, 308, "absolute", {position: position, dimScreen: false, parentElement: ActiveVideoDiv});
					} else {
						var sign_up_btn = document.createElement("div");
						sign_up_btn.id = "end_video_sign_up_btn";
						sign_up_btn.style.position = "absolute";
						sign_up_btn.style.top = "36%";
						sign_up_btn.style.left = "34%";
						sign_up_btn.innerHTML = "\n\t\t\t\t\t\t\t<a href=\"\/subscribe\">\n\t\t\t\t\t\t\t\t<img src=\"https:\/\/cdni.abcmouse.com\/home\/sign-up-btn.png\" \/>\n\t\t\t\t\t\t\t<\/a>\n\t\t\t\t\t\t";
						
						ActiveVideoDiv.appendChild(sign_up_btn);
					}
					
					html5VideoPlayer.removeEventListener("completed", oncomplete);
				});
				
				html5VideoPlayer.addEventListener("play", function onplay() {
					closePopup();
					var sign_up_btn = document.getElementById("end_video_sign_up_btn");
					if(sign_up_btn) {
						sign_up_btn.parentNode.removeChild(sign_up_btn);
					}
					
					html5VideoPlayer.removeEventListener("play", onplay);
				});
				
				window.addEventListener("orientationchange", function(event) {
										var leftPos = (wWidth - 720)/2;
					var topPos = (wHeight - 404)/2;
					var ActiveVideoDiv                = document.getElementById("html5_video_holder");
					ActiveVideoDiv.style.top            =   topPos + "px";
					ActiveVideoDiv.style.left           =   leftPos + 'px';
				});
			}

			function onVideoLinkClick(getPos, file, hideSignup) {
				
				if(is_android_device) {
					playHTML5Video(file);
					return;
				}

				var forceSurvey = false && /siteoverview/.test(file);

				var config = {showSurvey: true, forceSurveyPeriod: forceSurvey};

				var signUpVids = ["david_simkins_oct23.f4v","glenna_gambrell_oct23.f4v","honeylyn_barnett_oct23.f4v","wia_parents_12213_1280x720.f4v","results_montage_siteintro_12813.f4v","wia_parentsupfront_12913_1280x720.f4v","wia_simkinsfront_reneeback_13013_1280x720.f4v","wia_parentsatend_2113_1280x720.f4v","siteovervie19.f4v","wia_ana_and_traci.f4v","wia_parents_995_pricing.f4v"];

				if((/siteoverview/.test(file) || inArray(file, signUpVids)) && (homepageVersion == 211 || homepageVersion == 213 ||  homepageVersion >= 215)) {
					if(homepageVersion == 683) {
						config.signUpBtn = "https://cdni.abcmouse.com/home/homepage/683/enroll.png";
					} else {
						config.signUpBtn = "https://cdni.abcmouse.com/home/homepage/211/enroll.png";
					}
					config.signUpURL = "/subscribe/get_your_first_month";
					config.signUpPosition = { width: 320, height: 190, top: 120, left: 350};
					config.showBubble = true;

					config.bubbleConfig = {
						top: 0, left: 120, width: 265, height: 140, image: "https://cdni.abcmouse.com/home/homepage/211/join.png", url: "/subscribe/get_your_first_month_bubble"
					};

					config.showSurvey = false;

					config.onclose = function() {
						var width = (homepageVersion == 215) ? 667 : 576;
						var height = (homepageVersion == 215) ? 376 : 345;

						if(homepageVersion == 252 || homepageVersion == 253 || homepageVersion == 254)
							height = 480;
						
						setTimeout(function() { showFirstMonthPopup(width, height);}, 1); //Fix for IE 9	

						if(homepageVersion >= 222)
							track("holidayOfferShow", "offer=multi|source=homepage-video-close");
					};

					if(homepageVersion == 561 && false) {
						config.signUpBtn = "https://cdni.abcmouse.com/home/homepage/561/enroll_btn.png";
						config.signUpPosition = { width: 320, height: 200, top: 120, left: 350};
					}

					if(homepageVersion >= 222 && getCookie("elog") == "") {
						var time = new Date().getTime();
						config.showSignUpBtn = false;

						if(homepageVersion == 252 || homepageVersion == 253 || homepageVersion == 254) {
							config.endVideoPopupCondfig = { popup: '<div id="end_video_popup" style="display: none;"><iframe src="/email_special/3/multi/211/' + time + '" width="335" height="308" marginwidth="0" marginheight="0" frameborder="0" scrolling="no" allowtransparency="true" style="background-color:transparent; margin: 0px; position: absolute; top: 53px; right: 28px;"></iframe></div>',
														popupId: "end_video_popup"};
						} else {
							config.endVideoPopupCondfig = { popup: '<div id="end_video_popup" style="display: none;"><iframe src="/email_special/3/multi/211/' + time + '" width="335" height="247" marginwidth="0" marginheight="0" frameborder="0" scrolling="no" allowtransparency="true" style="background-color:transparent; margin: 0px; position: absolute; top: 53px; right: 28px;"></iframe></div>',
														popupId: "end_video_popup"};
						}
					}
				} else if(/siteoverview/.test(file) && homepageVersion >= 208) {
					config.showSurvey = false;
					config.signUpBtn = "https://cdni.abcmouse.com/home/first-month-free-button.png";
					config.signUpURL = "/subscribe/get_your_first_month";
					config.onclose = function() {
						showFirstMonthPopup();
					};
				}
				
				
				if(/siteoverview/.test(file) && homepageVersion == 207) {
					config.showFacebookBtn = true;
				}

				if(file == "kidsspeak_bug_1280x720.f4v" || file == "david_simkins_story.f4v" || file == "honeylyn_barnett_oct23.f4v") {
					config.showSignUpBtn = false;
					config.onclose = function() {
						showFirstMonthPopup(576, 345);
					};
				}

				if(homepageVersion == 202 || homepageVersion == 205) {
					if(getCookie("firstMonthFree") == "" && homepageVersion == 202) {
						setCookie("firstMonthFree", "1", 3);
						setCookie("survey-completed", "2", 3);
					}

					if(/siteoverview/.test(file) && (getCookie("firstMonthFree") != "" || homepageVersion == 205)) {
						config.showSurvey = false;
						config.signUpBtn = "https://cdni.abcmouse.com/home/first-month-free-button.png";
						config.signUpURL = "/subscribe/get_your_first_month";
						config.onclose = function() {
							showFirstMonthPopup();
						};
					}
				}

				if(homepageVersion == 565) {
					document.getElementById("teacher_blink_gif").style.display = "none";
				}

				if(swf("learn-more-btn") != null && (typeof swf("learn-more-btn").stopAnimation == "function")) {
					swf("learn-more-btn").stopAnimation();
				}

				if(file == 'teacher_feedback_2_home.f4v') {
					config.autoclose = 'autoClose';
				}

				new FlowVideoPlayer().play(file, config);

			}

			function showElement(element) {
				element = document.getElementById(element);
				if(element == null)
					return;

				element.style.display = "block";
				element.style.visibility = "visible";
			}

			function hideElement(element, useVisibility) {
				element = document.getElementById(element);
				if(element == null)
					return;

				useVisibility = (useVisibility) ? useVisibility : false;

				if(useVisibility) {
					element.style.visibility = "hidden";
				} else {
					element.style.display = "none";
				}
			}

			function track(tag, kvp, redirectUrl) {
                var vars = {tag: tag, kvp: kvp};
				if(redirectUrl != null)
					ajax('/xml/track.php', vars, function(){ location.href = redirectUrl; });
				else
					ajax('/xml/track.php', vars, function(){});
			}

