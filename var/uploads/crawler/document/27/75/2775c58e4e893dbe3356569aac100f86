			function showFacebookPopup() {
				track("facebookClick","src=non-member-homepage");
				popupFB = document.getElementById("facebookPopup");
				if(window.innerHeight) {
					var marginTop = (window.innerHeight - 221)/2;
					var marginLeft = (window.innerWidth - 531)/2;
				}
				else {
					var marginTop = (document.documentElement.clientHeight - 221)/2;
					var marginLeft = (document.documentElement.clientWidth - 531)/2;
				}
				popupFB.style.display = "block";
				popupFB.style.position = "fixed";
				if(marginTop < 5) { marginTop = 5; }
				popupFB.style.left = marginLeft + "px";
				popupFB.style.top = marginTop + "px";
				new PopupCover();

			}

			function closeFacebookPopup() {
