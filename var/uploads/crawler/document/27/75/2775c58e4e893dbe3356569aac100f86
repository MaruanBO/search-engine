			}

			function trackClick(element, evt, src, url) {
				var clickEvt = window.event || evt;
				track("NMfreeSiteHomepageClick","btn=" + src);

				if(typeof CE2 != "undefined") {
					if(typeof CE2.click == "function") {
						CE2.click(element, clickEvt);
						setTimeout("location.href = '" + url + "'", 100);
					} else {
						location.href = url;
					}
				} else {
					location.href = url;
				}
			}

			function bookmarkPage() {
				track("bookmarkThisPage","src=non-member-homepage");
				window.external.AddFavorite(location.href, 'ABCmouse.com - Early Learning Academy');
			}

			function closeDemoPopup() {
				document.getElementById("cover").style.display = "none";
				document.getElementById("demoAccountPopup").style.display = "none";
			}


function showPopup(url, width, height, stylePosition, config) {
	
	var position = getCenteredPosition(width, height, (stylePosition) ? stylePosition : "");
	position = (config && config.position) ? config.position : position;
	var parentElement = (config && config.parentElement) ? config.parentElement : document.body;
	var dimScreen = (config != null && config.dimScreen != null) ? config.dimScreen : true;

	var popupIframe = createElement("div", {id: "abcmouse-popup-iframe"});
	popupIframe.innerHTML = '<iframe src="' + url + '" width="' + width + '" height="' + height + '" marginwidth="0" marginheight="0" frameborder="0" scrolling="no" allowtransparency="true" style="background-color: transparent;"></iframe>';

	popupIframe.style.zIndex = "105";

	popupIframe.width = width + "px";
	popupIframe.height = height + "px";
	popupIframe.style.position = (stylePosition) ? stylePosition : "fixed";
	popupIframe.style.top = position.y + "px";
	popupIframe.style.left = position.x + "px";
	popupIframe.style.display = "block";

	parentElement.appendChild(popupIframe);

	if(dimScreen) {
		new PopupCover();
	}

	return popupIframe;
}

addOnResizeFunction(function() {
	var popupIframe = document.getElementById("abcmouse-popup-iframe");

	if(popupIframe == null)
		return;

	var position = getCenteredPosition(popupIframe.clientWidth, popupIframe.clientHeight);
	var topPos = position.y;
	var leftPos = position.x;
	
	
	popupIframe.style.top = topPos + "px";
	popupIframe.style.left = leftPos + "px";
});

function closePopup() {
	var popupIframe = document.getElementById("abcmouse-popup-iframe");
	//var popupIframe = document.getElementById("popup");

	if(popupIframe == null)
		return;

	//popupIframe.src = "";
	popupIframe.innerHTML = "";
	popupIframe.style.display = "none";
	popupIframe.parentNode.removeChild(popupIframe);

	PopupCover.close();
}

function gotoURL(url) {
	window.location.replace(url);
}

