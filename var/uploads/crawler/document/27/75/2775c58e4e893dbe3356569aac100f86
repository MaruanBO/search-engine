			function gotoRegistration(type, position) {
				if(type == "videoPlayerSignUpButton") {
					track("regclick", "src=" + type + "|position=" + position, "/subscribe/video_sign_up_button");
				} else {
					var extraTracking = "";
					if(/video:/.test(type))
						extraTracking = "/" + type.slice("video:".length);

					track("regclick", "src=" + type + "|position=" + position, "/subscribe" + extraTracking);
				}
			}
