<script>
$(document).ready(function() {
    
    /* Remove letter to the editor from user admin - jk */
    $("ul#admin-module-links li.category ul li:contains('Add Letter to Editor')").css("display", "none");
    
    /* Replace the h2 with the "ui-widget-header" class. - Per BMT-2235 by Robert Dundon (RMD) */
    header_text = $("#user-gateway h2.ui-widget-header").eq(0).text();
    $("#user-gateway h2.ui-widget-header").replaceWith("<div id='index-section-title'><h1 class='index-section-flag'>"+header_text+"</h1></div>");
    
    
    /* Removes linked accounts from login widget header - mg targeted for deletion */
/*    $("#user-gateway h2.ui-widget-header").text("Create an account"); */

    /* Changes "Login to "Log in" - Per BMT-2235 by Robert Dundon (RMD) */
    jQuery('#login-form-submit-panel').attr('value','Log in');    
    /* Marks the Zip code visually as required - Per BMT-2235 */
    $('label[for="postcode"]').addClass('required');       

    /* #blox-user-panel change -mg */
    $("#blox-user-panel.minimal dd#uUser").text("");
    
    
     /* IMPORTANT: Tacks referer url onto the create link at the bottom of the box  -mg */
    var sNewUrl = $("#login-box-create-link-panel").attr("href") + "";
    $("#login-box-create-link-panel").attr("href", sNewUrl);
    $("p.blox-alert-alternative a.signup-link").attr("href", sNewUrl);
    $(".unauthorized-user a.signup-link").hide();
    $("#blox-user-panel.minimal a.signup-link").attr("href", "/all-access/");
    $("#blox-user-panel.minimal #uNavOut form fieldset").eq(1).find("a").eq(1).attr("href", "/subscriber-services/");
    /* $("#blox-user-panel.minimal #uNavReauth a.login-link").attr("href", "javascript:void(0);").addClass("open-modal-login-panel").addClass("login-link"); */
    $("#blox-user-panel.minimal #uNavReauth .panel-delimiter").eq(0).hide();

    $("#blox-user-panel-minimal-loading").hide().removeClass("blox-loading");
    $("#blox-user-panel.minimal").show();

    
    
 //   $("#user-gateway #index-section-title .index-section-flag").eq(0).text('Create a Richmond.com user account');
    sWidgetHeader = $("#user-gateway #index-section-title .index-section-flag").eq(0).text();
//   console.log("Widget header: " + sWidgetHeader); *** Got ya!
    if(sWidgetHeader.indexOf("Incorrect Screen name/Email") >= 0) {
        var sErrorMsg = $(".page .blox-alert .error").text();
        sErrorMsg = sErrorMsg.replace("screen name/", "");
        $(".page .blox-alert .error").text(sErrorMsg); 
        $("#user-gateway #index-section-title .index-section-flag").text("Incorrect Email"); 
        $(".page .blox-alert").eq(1).hide();
        $(".page .providers").hide();
        $(".blox-alert-alternative").hide();
    }
    $('.login-column-right.login-column.providers').hide();
    $('#calAdvancedOptions').hide();
    $("#calSearchMore").text("Advanced Search")
    
    
    
 
    
});

