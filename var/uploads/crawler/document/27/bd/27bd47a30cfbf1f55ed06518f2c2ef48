        <div id="email-signup-container">
            <script>
    var hasBeenClicked = false;
    $(document).ready(function() {
        $("#Email").addClass("light-text").val("Enter your email address").click(function() {
            if (!hasBeenClicked) {
                $(this).removeClass("light-text").val("");
                hasBeenClicked = true;
            }
        });
        
        $("#email-signup-form").bind("jqv.form.result", function (event, errorFound) {
            if (errorFound) {
                $("#email-status").hide();
            }
        });
    });

    $("#email-signup-form").live("submit", function(e) {
        $("#email-status .login-alert, #emailRequestForm .formErrorContent").hide();
        if ($(this).validationEngine('validate')) {
            $.ajax({
                type: "post",
                url: '/EmailSignUp/EmailSignup',
                data: $('#emailRequestForm form').serialize(),
                success: function(data) {
                    $("#email-status").hide();
                    $("#email-status").html($(data).find("#email-status").html());
                    $("#email-status").fadeIn();
                    $("#email-status .login-alert").fadeIn();
                    $(document).trigger('event-trigger', [{ category: 'email-signup', action: 'submit', label: 'home page'  }]);
                    $(document).trigger('event-trigger', [{ category: 'Lead form', action: 'submit', properties: { 'Lead-form-name': 'email-signup' } }]);
                }
            });
        }
        e.stopPropagation();
        return false;
    });

</script>
<div id="emailRequestForm">
