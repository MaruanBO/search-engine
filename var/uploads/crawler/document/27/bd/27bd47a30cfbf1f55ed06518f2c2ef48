<script>
    $(document).ready(function () {
        $(".select2-input").css("width", "100%").addClass("dark-text");
        $("#search-section .select2-input").focus();
        $("#search-text-overlay").show();

        $(document).keyup(function (e) {
            if (e.keyCode == 27) { $('#main-layout').click(); }
        });
        $("#search-text-overlay").click(function () {
            $(this).hide();
            $("#search-section .select2-input").focus();
        });
        $('#main-layout').click(function () {
            $("#search-text-overlay").show();
            $("#visible-modal-overlay").fadeOut();
        });
        $('#autocomplete-container, #search-section').click(function (event) {
            event.stopPropagation();
        });
        $(".slide-nav-arrow").click(function() {
            $('#responsive-full-slides').superslides('stop');
        });
        $("#prev-slide").click(function () {
            $(document).trigger('event-trigger', [{ category: 'HomePage', action: 'Search Box', properties: { clicklocation: 'scroll' } }]);
            LoadSlideImage(-1);
            $('#responsive-full-slides').superslides('animate', 'prev');
            return false;
        });
        $("#next-slide").click(function () {
            $(document).trigger('event-trigger', [{ category: 'HomePage', action: 'Search Box', properties: { clicklocation: 'scroll'} }]);
            LoadSlideImage(2);
            $('#responsive-full-slides').superslides('animate', 'next');
            return false;
        });
        $(".select2-input").change(function () {
            if ($(this).val() != "") {
                $("#search-text-overlay").hide();
            }
        });
        $(".select2-input").keyup(function () {
            if ($(this).val() != "") {
                $("#search-text-overlay").hide();
                $("#visible-modal-overlay").fadeIn();
            }
        });
        
        function LoadSlideImage(increment) {
            if ($(".slides-container").find('[lazy-src]').length > 0) {
                var currentSlide = $('#responsive-full-slides').superslides('current');
                var slides = $(".slides-container").find(".header-slide");
                var slideIndexToLoad = (currentSlide + increment) < 0 ? (slides.length - 1) - (increment + 1) : (currentSlide + increment);
                var slideToLoad = $($('#responsive-full-slides').find(".header-slide")[slideIndexToLoad]).find('[lazy-src]');
                if (slideToLoad.length) {
                    slideToLoad.attr('src', slideToLoad.attr('lazy-src')).removeAttr('lazy-src');
                }
            }
        }

        $(document).on('animated.slides', function () {
            var currentSlide = $('#responsive-full-slides').superslides('current');
            var nextSlide = parseInt(currentSlide) + 1;
            $("#current-slide").text(nextSlide); 
            LoadSlideImage(1);
        });
        $(document).trigger('event-trigger', [{ category: 'search', action: 'open-search-box' }]);
    });

</script>
