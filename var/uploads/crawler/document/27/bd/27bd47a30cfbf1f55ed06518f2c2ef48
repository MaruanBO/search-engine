<script type="text/javascript">
    $("#Email").live('blur', function () {
        $("#email-status .login-alert").hide();
        $("#emailRequestForm .formErrorContent").text('');
    });
    
    $($(".top-link")[0]).mouseover(function () {
        $(".select2-input").blur();
    });
    $(document).ready(function() {
        $("#topbar").onScrollTo(function() {
            $(document).trigger('event-trigger', [{ category: 'Scroll Depth', action: 'homepage_scroll', label: 'header' }]);
        }).check();
        $("#featured-tours").onScrollTo(function() {
            $(document).trigger('event-trigger', [{ category: 'Scroll Depth', action: 'homepage_scroll', label: 'featured-destinations' }]);
        }, { offset: "50%" }).check();
        $(".tour-slider").onScrollTo(function() {
            $(document).trigger('event-trigger', [{ category: 'Scroll Depth', action: 'homepage_scroll', label: 'rotating-banner' }]);
        }, { offset: "50%" }).check();
        $("#homepage-widget-container").onScrollTo(function() {
            $(document).trigger('event-trigger', [{ category: 'Scroll Depth', action: 'homepage_scroll', label: 'bottom_widget_container' }]);
        }, { offset: "50%" }).check();
        $("#footer").onScrollTo(function() {
            $(document).trigger('event-trigger', [{ category: 'Scroll Depth', action: 'homepage_scroll', label: 'footer' }]);
        }, { offset: "50%" }).check();

        function tourSliderTriggerEvent() {
            $(document).trigger("event-trigger-tourSliderSwiper");
        };
        $(".tour-slider .swiper-wrapper").onScrollTo(tourSliderTriggerEvent, {
            fireOnce: true
        }).check();
    });
</script>

