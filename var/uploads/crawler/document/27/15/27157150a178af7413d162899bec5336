<script src="/javascript/jquery.validate.js" type="text/javascript"></script>
<script type="text/javascript">
  $(document).ready(function() {
	$("#subscribe").validate({
      rules: {
          fname: "required",
          lname: "required",
          pass1: {
              required: true,
              minlength: 5
          },
          pass2: {
              required: true,
              minlength: 5,
              equalTo: "#subscribe_password"
          },
          email: {
              required: true,
              email: true
          },
		  zipcode: {
              required: true,
              postalcode: true
			  
          },
          subscribecheck3: "required"				
      },
      messages: {
          fname: "Please enter your firstname",
          lname: "Please enter your lastname",
          pass1: {
              required: "Please provide a password",
              minlength: "Your password must be at least 5 characters long"
          },
          pass2: {
              required: "Please provide a password",
              minlength: "Your password must be at least 5 characters long",
              equalTo: "Please enter the same password"
          },
          email: "Please enter a valid email address",
		  zipcode: "Please enter a valid zip code",
          subscribecheck3: "This field is required"
      }
  });
  
	$("#getDealAlertBtn").click(function(){
	  $('#overlay').show();
	  $('.subscribeBox').slideDown();
	  $('html, body').animate({scrollTop:0}, 500);
	});
  
	$("div.subscribeBox .close").click(function(){
	  $('.subscribeBox').slideUp(); 
	  $('#overlay').hide();
	});
  });

function subscribe234(){
  if($("#subscribe").valid()){
   $.ajax({
	 type: "POST",
	 url: "/extra/subscribe.php",
	 data: "email="+$('#subscribe_email').val()+"&zipcode="+$('#subscribe_zip').val()+"&gender="+$('[name="gender"]:checked').val()+"&fname="+$('#subscribe_first_name').val()+"&lname="+$('#subscribe_last_name').val()+"&pass1="+$('#subscribe_password').val()+"&pass2="+$('#subscribe_confirm_password').val()+"",
	 success: function(msg){
		if(msg == "confirm"){
		  $('#alert_message').show();
		  $('#alert_holder').hide(); 
		} else {
		  $('#alert_error').html(msg);
		}
	  }});
   }
}
</script>
