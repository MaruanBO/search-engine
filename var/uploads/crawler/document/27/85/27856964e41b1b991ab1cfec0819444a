<script type="text/javascript">


jQuery(function ($) {
	function searchQuerySubmit(sq){
		if (sq != ''){
			window.location.href='/search-results.html?q='+sq;
		}
	};
	
	$('#searchStickySubmit').click(function(){
		searchQuerySubmit($('#searchStickyText').val());
	});
    
	$('#searchMobileSubmit').click(function(){
		searchQuerySubmit($('#searchMobileText').val());
	});
    
	$('#searchPrimarySubmit').click(function(){
		searchQuerySubmit($('#searchPrimaryText').val());
	});
    
	/*Generic enter submit*/
	$('.gsText').keydown(function (e){
		var sq = $(this).val();
		if(e.keyCode === 13){
			searchQuerySubmit(sq);
		}
	});

});

$(function(){

    $('div.module-1').on('click', function (){
	$('.module-1 .panel').each( function(index, val) {
	    $(this).removeClass('green');
	});
	$(this).find('.panel').addClass('green');
	$('#radio-'+$(this).attr('id')).prop('checked', true);
    });

    var clearTopProductSelection = function() {
	$('.module-1 .panel').each( function(index, val) {
	    $(this).removeClass('green');
	});
	$('input[id*="radio-"]').each(function() {
	    $(this).prop('checked', false);
	});
    };

    $('#servicerModal .btnContinue').bind('click', function(e){
	var th = $(this).attr('href');	
	var data = $(this).attr('data');
	var datasplit = data.split('~');
	var linkdata = "srvmod:"+$("#select-type-of-product_id option:selected" ).text()+">"+datasplit[0];
	var selectPortal = datasplit[1];
	var evt="event46";	
    $(this).attr('data',linkdata+"~"+selectPortal+"~"+evt);
    trackFunction($(this));
    
	if( th === '' || th === '#'){
		e.preventDefault();
	}	
    });
    
    updateProdForm = function(linkUrl,data) {
	$('#servicerModal .btnContinue').attr('href', linkUrl);
	$('#servicerModal .btnContinue').attr('data', data);
    };
	
});</script></div>

