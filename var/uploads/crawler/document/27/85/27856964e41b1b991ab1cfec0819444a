   function sendContentModData( modData ){
          s=s_gi(s_account);          
          if(modData=='service module popup')
          {       
            s.pageName=s.pageName+'>service module popup';
          }
          else
          {
            s.pageName=modData;  //INCLUDE THE 'modData' VARIABLE IN THE CALL TO IDENTIFY THE CONTENT MODULE THAT TRIGGERED IT
          }
          s.linkTrackEvents='event1';
          s.tl(this,'o',s.pageName);       
        } 

    jQuery(function ($) {      
         
    
     
     s.prop40=s.eVar40
     
         
     
        ="en-US"     
       
     
    
     
     s.channel=s.eVar41
     
         
     
        ="asurion>>home"     
       
     
    
     
     s.prop2=s.eVar3
     
         
     
        ="asurion"     
       
     
    
     
     s.prop32=s.eVar55
     
         
     
        ="asurion>>home"     
       
     
    
     
     s.pageName
     
         
     
        ="asurion>>home>site index"     
       
     
    
     
     s.prop33=s.eVar56
     
         
     
        ="asurion>>home"     
       
     
    
    
       
  /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
if('Y'=='Y'){
   var s_code=s.t();if(s_code)document.write(s_code)
}

if(s.linkTrackVars) var tempTrackVars=s.linkTrackVars
if(s.linkTrackEvents)var tempTrackEvents=s.linkTrackEvents


trackFunction = function (jItem,linkType){
       var label = jItem.text();
       if(label=="" & jItem.attr('title')!=""){    // If label is not specified title of link is specified as label
                    label=jItem.attr('title');
                }  
       var eventVal = 'event33'; 
       var dynamicValue = jItem.attr('data'); 
       var dynaValue=dynamicValue.split('~');   
       if(dynamicValue.search('event')>0){
         var len=dynaValue.length-1;
         eventVal=dynaValue[len];
         if(dynaValue[len] == 'event0')
         {
            eventVal="";           
         }
       }
       
       var appStoreOrSocial="";
       var carrier="";
       
        if(dynaValue.length>1){
            appStoreOrSocial=dynaValue[0];
            carrier = dynaValue[1];             
       } else {
            appStoreOrSocial=dynaValue;
       }
       
            
       var pName = s.pageName;  
       var pchannel = s.channel;
       
       var siteidentifier= 'asurion';
               var sl=s_gi(s_account);
               sl.linkTrackVars='prop1,eVar2,prop2,eVar3,prop12,eVar12,events,eVar41,channel,eVar1,prop41,pageName';
               sl.linkTrackEvents = 'event33,event45,event46,event11';               
               sl.eVar2 = sl.prop1 = carrier;   
               sl.events=eventVal;           
               sl.eVar41 = sl.channel = pchannel; 
               sl.prop12 = sl.eVar12 = appStoreOrSocial;
               sl.pageName = sl.prop41 = sl.eVar1 = pName;
               sl.prop2 = sl.eVar3 = siteidentifier;    
               if(sl.prop12!=""){
                 label=sl.prop12;
               }                          
               sl.tl(this,linkType,label);    
               if(tempTrackVars) sl.linkTrackVars = tempTrackVars
               if(tempTrackEvents) sl.linkTrackEvents = tempTrackEvents

}

 trackLink = function(){
    $('.siteCatalystTracker').bind("click", function() {
        trackFunction($(this),'o');
    }); 
}
   

trackLinkforPage = function(){
    $('.siteCatalystTrackerPg').bind("click", function() {
        pageTabLinks($(this).attr('data'));
    }); 
}

 $('.siteCatalystTrackerForDownload').bind("click", function() {
        trackFunction($(this),'d');
    });
    
    $('.siteCatalystTrackerForExitLinks').bind("click", function() {
        trackFunction($(this),'e');
    });
  
  $('.marker').bind("click", function() {
        trackFunction($(this));
    }); 
    
    $('.trackTabs').bind("click", function() {
       tabContentModData($(this).attr('data'));
    });    
    
    pageTabLinks = function(data){ 
  
        sc=s_gi(s_account);
        sc.pageName = data;
        sc.events = "event1";
        sc.prop32 = sc.eVar55 = sc.prop33 = sc.eVar56 = s.prop32;
        sc.prop40 = sc.eVar40 = s.prop40;
        sc.channel = sc.eVar41 = s.channel;

		sc.tl(this,'o',sc.pageName); 
   
   }
    
    function tabContentModData( modData ){
      
       var dynaValue=modData.split('~');  
       sc=s_gi(s_account);
       sc.pageName=dynaValue[0]+dynaValue[1]+dynaValue[2];
       sc.prop32 = sc.eVar55 = dynaValue[0];
       sc.prop33 = sc.eVar56 = dynaValue[0]+dynaValue[1];
       sc.events = "event1";
       sc.eVar41 = sc.channel = s.channel;
       sc.prop40 = sc.eVar40 = s.prop40;
       sc.prop2 = sc.eVar3 = s.prop2;
       sc.linkTrackEvents='event1';
       sc.eVar12=sc.prop12="";
       sc.prop35=sc.prop41="";

	   sc.tl();if(s_code)document.write(s_code) ; 

    }



  customerFormLink = function(data){ 

            var dynaValue=data.split('~'); 
            var carrier = dynaValue[1];
            var prop32all = dynaValue[0];
            var pname = dynaValue[2];
            var pchannel = s.channel;
            var eventval= "event11";
            var siteidentifier= 'asurion';
            sc=s_gi(s_account);

            sc.linkTrackEvents = 'event11'; 
            sc.eVar41 = sc.channel = pchannel;               
            sc.prop2 = sc.eVar3 = siteidentifier;            
            sc.events = eventval;            
            sc.prop32 = sc.eVar55 = sc.prop33 = sc.eVar56 = prop32all+carrier;

            if(carrier === 'retail') {
                sc.eVar2 = sc.prop1 =  '';
                sc.pageName = prop32all+carrier+pname;
            } else if (carrier === 'mobile') {
                sc.eVar2 = sc.prop1 =  dynaValue[3];
                sc.pageName = prop32all+carrier+pname+" - "+dynaValue[4];
            } else if(dynaValue[5] != ''){
                sc.pageName = prop32all+carrier+pname+" - "+dynaValue[5];
                sc.prop36 = sc.eVar61 = "request type:"+dynaValue[5];                
                sc.eVar2 = sc.prop1 =  dynaValue[6];
            } else {
                sc.eVar2 = sc.prop1 =  carrier;
                sc.pageName = prop32all+carrier+pname;
            } 
                       
            sc.t();if(s_code)document.write(s_code)               
    }    

whitepaperFormLink = function(data){


 var dynaValue=data.split('~'); 
 var pname = dynaValue[0];
 var svar32n55 = dynaValue[1];
 var svar33n56 = dynaValue[2];
 var svarEvent = dynaValue[3];
 var svar2n3 = dynaValue[4];
 var svarchannel = "asurion>>marketing lp";

	sc=s_gi(s_account);

	sc.linkTrackEvents = svarEvent;
    sc.pageName = pname;
	sc.events = svarEvent;
	sc.eVar41 = sc.channel = svarchannel;               
	sc.prop2 = sc.eVar3 = svar2n3;
 	sc.prop32 = sc.eVar55 = svar32n55;
	sc.prop33 = sc.eVar56 = svar33n56;
    sc.prop40= sc.eVar40 = 'en-us';
	sc.t();if(s_code)document.write(s_code) ;

}
      
      //select all DOM elements with a 'data-tracking-id' attribute on scroll event
      $('[data-tracking-id]').bind('inview', function(e, isInView, visiblePartX, visiblePartY) {
          var elem = $(this),
              trackingID = elem.attr('data-tracking-id') //hold data-tracking-id value in var 'trackingID'

          if (elem.data('inviewtimer')) {
            clearTimeout(elem.data('inviewtimer'));
            elem.removeData('inviewtimer');
          }

          if (isInView) {
            elem.data('inviewtimer', setTimeout(function() {
              if (visiblePartY == 'top') {
                elem.data('seenTop', true);
              } else if (visiblePartY == 'bottom') {
                elem.data('seenBottom', true);
              } else {
                elem.data('seenTop', true);
                elem.data('seenBottom', true);
              }

              if (elem.data('seenTop')) {
                elem.unbind('inview');                
                //call the sendContentModData function to send data and pass the trackingID variable
                sendContentModData( trackingID )
              }
            }, 2000//set the time 
            ));
          }
        });
        
        trackLink();
        
        $('.trackOnFocus').bind("focus", function() {
        sendContentModData($(this).attr('data'));
    });
        
        
 });
 
              
            </script>


