				$eao.test = 'LOCAL';
				$eao.imgScrub();
			});
		},
		rotate : function(quote){
			var obj = $eao.dataClean.opinions[$eao.count],
			formattedvalue = $.datepicker.formatDate("M d, yy", $.datepicker.parseDate( "yy-mm-dd", obj.submissionDate.substring(0,10))),
			quoteStr = '';
			
			if(obj.answers.rating.intValue==5) {
				$eao.ratingWord='five';
			}
			if(obj.answers.rating.intValue==4) {
				$eao.ratingWord='four';
			}
			if(obj.answers.rating.intValue==3) {
				$eao.ratingWord='three';
			}
			if(obj.answers.rating.intValue==2) {
				$eao.ratingWord='two';
			}
			if(obj.answers.rating.intValue==1) {
				$eao.ratingWord='one';
			}
			quote.find('em').attr('class',$eao.ratingWord + ' col-sm-6');
			
			if (typeof obj.customer.imageUrl != 'undefined') {
				quote.find('img').attr('src',obj.customer.imageUrl);
				quote.find('.customer-quote-content').removeClass('noImg');
			} else {
				quote.find('.customer-quote-content').addClass('noImg');
			}
			
			if(obj.answers.opinion.stringValue.length > $eao.maxStrLength) {
				quoteStr = obj.answers.opinion.stringValue.substring(0, $eao.maxStrLength) + "...";				
			} else {
				quoteStr = obj.answers.opinion.stringValue;
			}
			quote.find('p.quote').html(quoteStr);
			quote.find('p.quote-date').html(formattedvalue);
			quote.find('span.quote-name').html(obj.customer.name);
		},
		imgScrub : function() {
			var dcIdx = 0;
			for (i=0; i < $eao.data.opinions.length; i++ ){
				var cust = $eao.data.opinions[i].customer;
				if(typeof cust.imageUrl == 'string') {
					$eao.dataClean.opinions[dcIdx] = $eao.data.opinions[i];
					dcIdx++;
				}
			};
			$eao.cleanIdx = dcIdx;
			$eao.initRotate();
		},
		initRotate : function() {
			//Rotate through each
			$.each( $eao.comments, function( indexInArray ){
			var quote = $(this),
			quoteContainer = quote.parent('.customer-quote-container');
			
			$eao.rotate(quote);
			
			++$eao.count;

				setTimeout(function( ){

					setInterval(function(  ){

						quote.transition({
							rotateY: '+=180deg'
						}, $eao.speed, 'snap');

						quoteContainer.transition({
							rotateY: '+=180deg'
						}, $eao.speed, 'snap');
							
						$eao.rotate(quote);

						if( $eao.count == $eao.cleanIdx - 1) {
							$eao.count = 0;
						} else {
							++$eao.count;
						}

					}, $eao.interval );
					
				}, indexInArray * $eao.delay);

			});//end each
		},
		init : function() {
			$eao.doAjax();
		}		
	};

    //Init
    (function(){
		$eao.init();		
    })();
	
</script>
