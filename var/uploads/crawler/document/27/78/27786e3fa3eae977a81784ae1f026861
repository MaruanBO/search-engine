<script type='text/javascript'>
jQuery(document).ready(function($){
	var $fb_commentobject = $(".entry-comments");
	$fb_commentobject.each( 
		function(commentIndex){
			var $commString=$(this).children('a').html();
			var $countPos=$commString.indexOf(' ');
			if($countPos==7){
				var $localTotal=0;
			}else{
				var $localTotal=$commString.substring(0, $countPos);
				$localTotal = parseInt($localTotal, 10);
			}
			var $childRef=$(this).children('a');
			var $theURL= $(this).children('a').attr('href');
			if($theURL){
				var $commentsPos= $theURL.lastIndexOf('#comments');
			}else{
				return false;
			}
			var $cleanURL = $theURL.substring(0, $commentsPos);
			$.ajax({
				url: 'http://graph.facebook.com/'+$cleanURL,
				dataType: 'jsonp',
				success: function(data) {
					var $comVar=data.comments;
					if(!$comVar){
						$comVar=0;
					}
					var $finalTotal=parseInt($comVar+$localTotal);
					var $newCommentTotal=' Comment'
					if($finalTotal>0){
						$newCommentTotal=' 1 Comment';
					}
					if($finalTotal>1){
						$newCommentTotal=' ' + $finalTotal + ' Comments';
					}
					$childRef.html($newCommentTotal);
			   }
			});
		}
	);
 })
</script>

