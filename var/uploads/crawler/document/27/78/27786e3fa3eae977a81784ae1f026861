	<script type="text/javascript">
	jQuery(function ($) {
		emailDefault=$('#subscriberPopEmail').val();
		$('#spChooseGender').addClass('spLabelInactive');
		$('#subscriberPopEmail').on('focus',function(){
			if(this.value==emailDefault){
				this.value='';
				$(this).addClass('enteringEmail');
			}
		});
		$('#subscriberPopEmail').on('blur',function(){
			if(this.value==''){
				this.value=emailDefault;
				$(this).removeClass('enteringEmail');
			}else{
				$('#spEmailLabel').addClass('spLabelInactive');
				$('#spChooseGender').removeClass('spLabelInactive');
			}
		});
		$('.genderButton').on('click',function(){
			$('.genderButton').removeClass('genderSelected');
			$(this).addClass('genderSelected');
			//Super basic email field check
			if(($('#subscriberPopEmail').val()!=(''))&&($('#subscriberPopEmail').val()!=emailDefault)){
			
			//Set appropriate gender info.
				if($(this).attr('id')=='popGenderF'){
					//Women's Button
					$('#popGender').val('f');
					$('#popIemlistid').val('2');
					var redirection='http://digitalromanceinc.com/subscribed/?gen=f&email=';
					redirection+=$('#subscriberPopEmail').val();
					$('#popRedirect').val(redirection);
				}else{
					//Men's Button
					$('#popGender').val('m');
					$('#popIemlistid').val('17');
					var redirection='http://digitalromanceinc.com/subscribed/?gen=m&email=';
					redirection+=$('#subscriberPopEmail').val();
					$('#popRedirect').val(redirection);
				}
			//Submit
				$.cookie('newsletter', 'dontshow', { expires: 3650, path: '/' });
				$('#theSubscriberPopForm').submit();
				$.magnificPopup.close();
			}
		});
		//Popup Display after 15 seconds
		popTimeoutID = window.setTimeout(displayPopupNow, 15000);
		function displayPopupNow() {
			$.magnificPopup.open({
			  items: {
				src: '#TheSubPop',
				type: 'inline',
			  },
			  
			  callbacks: {
				close: function() {
					$.cookie('newsletter', 'dontshow', { expires: 1, path: '/' });
				}
			  }
			});

			if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
				var magnificPopup = $.magnificPopup.instance;
				magnificPopup.bgOverlay.css('position', 'fixed');
				$( window ).scroll(function() {
					var PopScrollLeft=$( window ).scrollLeft();
					$('.mfp-container').css('left',PopScrollLeft);
				});
			}
		}

	});
	</script>
	
