<script>

//Time of day message script- by javascriptkit.com
//Visit JavaScript Kit (http://javascriptkit.com) for script
//Credit must stay intact for use

var Digital=new Date()
var hours=Digital.getHours()

//To Set Date Add One Extra Day
var date = new Date("May 10, 2014");
var description = "Down for Maintenance";
var now = new Date();
var diff = date.getTime() - now.getTime();
var days = Math.floor(diff / (1000 * 60 * 60 * 24));
document.write("")


//Configure message below to your own.
if (hours>=5&&hours<=11&&days == 0) //MESSAGE FOR AFTERNOON (5am-Noon)
document.write('')
else if (hours==12&&days == 0) //MESSAGE FOR NOON
document.write('')
else if (hours>=13&&hours<=18&&days == 0) //MESSAGE FOR AFTERNOON (1pm-6pm)
document.write('')
else if (hours>=13&&hours<=18&&days<=1) //MESSAGE FOR AFTERNOON (1pm-6pm Next Day)
document.write('')
else if (hours>=19&&hours<=24&&days == 0) //MESSAGE FOR EVENING (7pm-12pm)
document.write('<font color="#CC3333"><b>Please Note:</b></font> Starting at 7:00 p.m. (CST) some parts of our site may not be functioning, most notably our online ordering system and search functions. We will be back up and running as soon as possible. Thank you for your patience.')
else if (hours>=1&&hours<=2&&days<=1) //MESSAGE FOR NIGHT (1am-2am Next Day)
document.write('<font color="#CC3333"><b>Please Note:</b></font> Some parts of our site may not be functioning, most notably our online ordering system and search functions. We will be back up and running as soon as possible. Thank you for your patience.')
else //MESSAGE FOR LATE NIGHT, EARLY MORNING (12pm-4am)
document.write('')

</script>
</div>
