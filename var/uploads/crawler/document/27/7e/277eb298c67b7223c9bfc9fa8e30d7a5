<!-- Start Christmas Countdown
<div id="christmasDelivery">
<SCRIPT LANGUAGE="JavaScript">
var date = new Date("December 21, 2013");
var description = "Christmas";
var now = new Date();
var diff = date.getTime() - now.getTime();
var days = Math.floor(diff / (1000 * 60 * 60 * 24));
document.write("")
if (days > 1) {
document.write(" Only " + (days+1) + " days to order for " + description + " delivery! ");
}
else if (days == 1) {
document.write("Only 2 days to order for " + description + " delivery! ");
}
else if (days == 0) {
document.write("Only 1 day to order for " + description + " delivery! ");
}
else if (days < -1) {
document.write("Merry Christmas from Tower Hobbies!");
}
else {
document.write("Today is the last day to order for " + description + " delivery! ");
}
document.write("");
// End
</script>
