			function assignListItemMousing(obj){
				$(obj).bind('mouseenter', function(){
					$(obj).addClass("user_list_item_ovr");
	        		$(obj).children('a').children('div').css("color","#e74e1e");
		      		$(obj).children('#watchit_btn').children('a').children('img').show();

				});
				$(obj).bind('mouseleave', function(){
				  $(obj).removeClass("user_list_item_ovr");
				  $(obj).children('#watchit_btn').children('img').hide();
		      	
		      	
	        $(obj).children('a').children('div').css("color","#e7e7e7");
				});
				$(obj).bind('click', function(event){
					if(event.target.id == "alerts"){
						if($(event.target).prop('checked') == true){
							$(event.target).next().html('Alerts on');
						}else{
							$(event.target).next().html('Alerts off');
						}
					}
				});
			}
			//
			function assignSearchItemMousing(obj){
				$(obj).bind('mouseenter', function(){
					$(obj).addClass("search_list_item_ovr");
				  $(obj).addClass("search_list_item_ovr");
			$(obj).children('a').children('div').css("color","#e74e1e");
				});
				$(obj).bind('mouseleave', function(){
				  $(obj).removeClass("search_list_item_ovr");
			$(obj).children('a').children('div').css("color","#e7e7e7");
				});
				$(obj).bind('click', function(event){
					if(event.target.id == "alerts"){
						if($(event.target).prop('checked') == true){
							$(event.target).next().html('Alerts on');
						}else{
							$(event.target).next().html('Alerts off');
						}
					}
				});
			}
			function assignWatchMousing(obj){
