  </script>



<script type="text/javascript">
  var offsetX = -65;
  var offsetY = -50;
  var xPos = 0;
  var yPos = 0;
  var movie_popup_id;
  var movie_popup;
  var carrotY;
  var offsetCarrotY = -57;//-85
  var windowPadding = 220;
  var reverseArrow = false;
  $(function() {
    $('.movie_title').on('mouseover',function(e){

      movie_popup_id = $(this).parents("li").attr('rel')
      movie_popup = $(".movie_popup") //$('#'+movie_popup_id);

      $.ajax({
      url: '/home/popup/' + movie_popup_id,
      dataType: 'html',
      success: function(response) {
        $('#popup_content').html(response);  
      }
    })  

      $('.movie_movie_popup').hide()
      moviePopupPositionX(this);
      moviePopupPositionY(this);
      movie_popup.show();
    })

    $('.movie_title').on('mouseout',function(e){
      $('.movie_popup').hide()
      $('#popup_content').html("<div class=\'popup_loading\'><img alt=\"Alt_loading_animation\" src=\"http://d2a0bnlkc0czvp.cloudfront.net/assets/controls/alt_loading_animation.gif\" /><\/div>");  
    })
  })

  function moviePopupPositionX(object){
    xPos = $(object).offset().left + $(object).width() + 20 - $(object).parents('.carousel_interface').offset().left;
    container_width = $(".site_content_container").width();
    reverseArrow = false
   if(container_width < xPos + movie_popup.innerWidth()) {
      xPos = xPos - movie_popup.innerWidth() - $(object).innerWidth() - 40;
      reverseArrow = true;
    }
    movie_popup.css('left', xPos);
  }

  function moviePopupPositionY(object){
    yPos = $(object).offset().top
    yPos = yPos - $(object).parents('li').innerHeight() ;

    var c = $(document).height()  - (yPos + movie_popup.innerHeight());
    if (c < 0){
      yPos = $(document).height() - movie_popup.innerHeight() ;
      yPos = yPos + offsetY + 5
    }

    if (null!= (document.getElementById('homepage_header_image')))
    {
      yPos = yPos - (document.getElementById('homepage_header_image').offsetHeight);
    }
    else if (null!= (document.getElementById('homepage_header_image2')))
    {
      yPos = yPos - (document.getElementById('homepage_header_image2').offsetHeight)
    }
    
    movie_popup.css('top', yPos);
    carrotPositionY(movie_popup);

  }
  function carrotPositionY(object){
    carrotY = ($(object).innerHeight() / 2) + 15;
    movie_popup.children('.movie_popup_arrow').css('top', carrotY)
    if(reverseArrow) {
      movie_popup.children('.movie_popup_arrow').addClass('hidden');
      movie_popup.children('.movie_popup_arrow.revert').removeClass('hidden');
    } else {
      movie_popup.children('.movie_popup_arrow').removeClass('hidden');
      movie_popup.children('.movie_popup_arrow.revert').addClass('hidden');
    }
  }
</script>

<script type="text/javascript">
  var movieIds = [267644,267905,267962,269345,269438,269339,268782,283265,267363,269341,269808,269342,269952,267884,268542,269599,269857,268023,267884,269817,268746,266632,283289,269290,266395,268657,269251,269471,269078,266811,269852,268008,268131,269585,269341,268434,268552,267625,268622,269343,269338,269342,269339,266001,268551,267644,267363,269438,267905,265666,266807,268817,269345,269456,268599,269646,269952,269799,269455,269943,268560,269808,268542,269943,267059,277278,268870,269591,269762,269241,270005,269231,269699,269041,269799,269586,269215,269344,269242,268757,269457,269757,266964,269857,267894,277252,269819,268653,266632,269186];

  function pluralize(count, noun){
    return count == 1 ? "1 " + noun : count.asCommaSeparated() + " " + noun + "s"
  }

  String.prototype.reverse = function(){ return this.split("").reverse().join("") }
  Number.prototype.asCommaSeparated = function(){
    if (this >= 1000){
      return this.toString().reverse().match(/.{1,3}/g).join(",").reverse()
    } else {
      return this.toString();
    }
  }

  Number.parse = function(str){
    return parseInt(str.replace(/,/g, ""), 10)
  }

  function updateMovieCounters(movieId, commentsCount, queuesCount){
    setMovieQueuesCount(movieId, queuesCount);
    setMovieCommentsCount(movieId, commentsCount);
  }

  function updateMovieCountersFunc(movieId, commentsCount, queuesCount){
    return function(){
      updateMovieCounters(movieId, commentsCount, queuesCount)
    }
  }

  function setMovieQueuesCount(movieId, queuesCount){
    var movieDiv = $('[data-carousel-movie-id=' + movieId + ']');
    movieDiv.find('.queue.count .number').text(queuesCount.asCommaSeparated())
    movieDiv.find('.queue.count').data("title.poshytip", "In " + pluralize(queuesCount, "queue"));
    movieDiv.find('.queue.count .number').setVisible(queuesCount > 0)
  }

  function setMovieCommentsCount(movieId, commentsCount){
    var movieDiv = $('[data-carousel-movie-id=' + movieId + ']');
    movieDiv.find('.facebook_comment.count .number').text(commentsCount.asCommaSeparated())
    movieDiv.find('.facebook_comment.count').data("title.poshytip", pluralize(commentsCount, "comment") + " added");
    movieDiv.find('.facebook_comment.count .number').setVisible(commentsCount > 0)
  }

  function increaseMovieQueuesCounter(movieId){
    var movieDiv = $('[data-carousel-movie-id=' + movieId + ']:first');
    var queuesCount = Number.parse(movieDiv.find('.queue.count .number').text(), 10)
    setMovieQueuesCount(movieId, queuesCount + 1);
  }

  function updateAllMoviesCounter(){
    $.ajax({
      type: 'post',
      dataType: 'json',
      url: '/movies/counter',
      data: {
        movie_ids: movieIds
      },
      success: function(counters){
        for(var i = 0; i < counters.length; i++){
          var counter = counters[i];
          setTimeout(updateMovieCountersFunc(counter.movie_id, counter.comments_count, counter.queues_count), 10)
        }
      }
    })
  }
</script>

<div class="clear"></div>
<!-- CAROUSEL END -->

<script type="text/javascript">
  function setQueueStatus(movieId, status){
    if (status == "loading"){
      imageUrl = "http://gowatchit.com/assets/controls/carousel_loading_animation.gif"
      $('[data-movie-id='+movieId+']').html('<img src="'+imageUrl+'" style="cursor:default"/>');
    } else {
      imageUrl = "http://gowatchit.com/assets/controls/carousel_in_queue.gif"
      $('[data-movie-id='+movieId+']').html('<img src="'+imageUrl+'" class="in_queue"/>');
    }
  }

  function setQueueStatusFunc(movieId, status){
    return function(){ setQueueStatus(movieId, status) }
  }

</script>

