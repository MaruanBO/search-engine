				});
			
				//Assign Mousing to search items
				$("#search_list li").each(function(index) {
					assignSearchItemMousing($(this));
				});
			});

				//MUST BE AFTER CAROUSEL!!!!!!!
				$(".carousel_movie_buttons div").each(function(index) {
