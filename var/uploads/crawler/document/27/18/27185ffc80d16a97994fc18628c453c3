  <script type="text/javascript">
    //
    $(document).ready(function(){
      $("#carousel_interface_6").jCarouselLite({
        btnNext: "#btn_next_6",
        btnPrev: "#btn_prev_6",
        scroll: 5,
        visible: 5
      });
    })
    
    $(document).ready(function(){
      $("#channel_carousel_interface_6").jCarouselLite({
        btnNext: "#btn_next_6",
        btnPrev: "#btn_prev_6",
        scroll: 3,
        visible: 3
      });
