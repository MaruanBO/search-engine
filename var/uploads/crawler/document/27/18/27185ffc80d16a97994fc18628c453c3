<script type="text/javascript">
  function showNotice(notice){
    $('#notice .content').html(notice);
    $('#notice').fadeIn(350).delay(2500).fadeOut(350);
  }

  function showPermanentNotice(notice) {
    $('#notice .content').html(notice);
    $('#notice').fadeIn(350);
