      $(document).ready(function () {
        Ember.Handlebars.bootstrap(Ember.$(document));

        user_hash = null;

        if (user_hash == null) {
          GoWatchIt.user = null
        } else {
          GoWatchIt.user = GoWatchIt.User.create(user_hash);
        }

        $(function () {
          //GoWatchIt.initialize(); // we us auto initialize so this should not be needed
          aContainer = Ember.ContainerView.create({
            childViews: [GoWatchIt.ShowHeaderView.create()]
          });
          aContainer.appendTo($('#ember_header'));
        });
      });
