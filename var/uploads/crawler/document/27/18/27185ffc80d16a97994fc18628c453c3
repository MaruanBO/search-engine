  <div id="oauth_target" class="oauth_box">
    <script type="text/javascript">
      function setEmbeddedAction(name, param, channel_id){
        $('input[name="embedded_action[name]"]').val(name);
        $('input[name="embedded_action[param]"]').val(param);
        $('input[name="embedded_action[channel_id]"]').val(channel_id);
      }

      function resetEmbeddedAction(){
        setEmbeddedAction('', '', '')
      }

      function getEmbeddedAction(){
        return {
          name: $('input[name="embedded_action[name]"]').val(),
          param: $('input[name="embedded_action[param]"]').val(),
          channel_id: $('input[name="embedded_action[channel_id]"]').val()
        }
      }

      function getActionEmbeddedUrl(url){
        if (getEmbeddedAction().name){
          fullUrl = url + '?' + $.param({ embedded_action: getEmbeddedAction() });
            fullUrl = fullUrl + '&' + $.param({"modality":"main"});
          return fullUrl;
        } else {
          return url;
        }
      }

      function postEmbeddedAction(redirect) {
        embedded_data = getEmbeddedAction();
        $.ajax({
          url:getActionEmbeddedUrl('/postEmbeddedAction')
        }).success(function (){
          if (redirect != null)
            window.location = redirect;
        })
      }

    </script>

    <script type="text/javascript">
    //form manipulation
    $('.login_form_header').bind("ajax:error", function(evt, xhr, status, error){
      $("#login_error_msg").text("Invalid Email or Password");
    });

    $('.login_form_header').bind("ajax:success", function(evt, xhr, status, error){
        closePopup();
        $.ajax({
              url: '/me',
             context: this,
             beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
               dataType: 'json',
             success: function(response) {
              GoWatchIt.set('user', GoWatchIt.User.create(response));
            //this.set('isLoggedIn', true);
             }
            })
    });
    // Enter on login-password submits form
    $('#user_password_login').keypress(function (e) {
      if (e.which == 13) {
        $('form.login_form_header').submit();
        e.preventDefault();
      }
    });



        var thewinPopup,
            popup = $('#login_popup'),
            loginCarrot = $('#login_popup_arrow'),
            loginCarrotBottom = $('#login_popup_arrow_bottom'),
            carrotWidth = 21,
            carrotHeight = 12,
            minCarrotHorizontalPadding = 10,
            windowPadding = 5,
            login_only = $('.login_only'),
            login_and_queue = $('.login_and_queue'),
            movie_id = 0,
            logged_in = false,
            currentButton = 'null',
            aboveOrBelow = 'below',
            ember = false;

        // Login link
        function showLoginPopup(forElement, forQueue){
          showLoginPopupFull(forElement, forQueue, false);
        }
        function showLoginPopupFull(forElement, forQueue, forEmber){
          resetPopup(true);
          var ember = forEmber;
          if (forQueue) {
            login_only.hide()
            login_and_queue.show()
          } else {
            login_and_queue.hide()
            login_only.show()
          }
          popup.show()
          setTimeout(function(){ adjustPopupPosition(forElement) }, 0)
        }

        function adjustPopupPosition(forElement) {
          popupPositionX(forElement);
          popupPositionY(forElement);
        }

        $('.login_link, #write_review').bind('click',function(e){
          e.preventDefault();
          setEmbeddedAction($(this).data('embedded-action-name'), $(this).data('embedded-action-param'), '');
          showLoginPopup(this, $(this).data('queue-action'));
        });

        $('#add_to_queue_button, .queue_btn').bind('click',function(e){
          e.preventDefault()
          if (!logged_in){
            setEmbeddedAction("queue_movie", $(this).attr("data-movie-id"), '');
            showLoginPopup(this, true);
          }
        })

        function carrotPositionX(carrotAnchorX){
          var carrotX = carrotAnchorX - carrotWidth / 2
          loginCarrot.css('left', carrotX)
          loginCarrotBottom.css('left', carrotX)
        }

        function popupPositionX(anchorElement){
          anchorElement = $(anchorElement)
          var carrotPadding = minCarrotHorizontalPadding + carrotWidth / 2 // carrot will be far to both the left and right of the login popup at least 10px
          var carrotAnchorX = anchorElement.offset().left + anchorElement.width() / 2;
          var minPopupLeft = carrotAnchorX - popup.width() + carrotPadding
          var maxPopupLeft = carrotAnchorX - carrotPadding
          var popupLeft = carrotAnchorX - popup.width() / 2 // try to have popup align in the same symmetric axis
          if (popupLeft < $(window).scrollLeft() + windowPadding) popupLeft = $(window).scrollLeft() + windowPadding
          if (popupLeft + popup.width() > $(window).width() + $(window).scrollLeft() - windowPadding){
            popupLeft = $(window).width() + $(window).scrollLeft() - windowPadding - popup.width()
          }
          if (popupLeft < minPopupLeft) popupLeft = minPopupLeft
          if (popupLeft > maxPopupLeft) popupLeft = maxPopupLeft
          popup.css('left', popupLeft)
          carrotPositionX(carrotAnchorX - popupLeft)
        }

        function setPopupPositionYAboveAnchor(anchorElement){
          setAboveOrBelow('above')
          anchorElement = $(anchorElement)
          var carrotAnchorY = anchorElement.offset().top
          var popupTop = carrotAnchorY - carrotHeight - popup.outerHeight()
          popup.css('top', popupTop)
          return popupTop > $(window).scrollTop()
        }

        function setPopupPositionYBelowAnchor(anchorElement){
          setAboveOrBelow('below')
          anchorElement = $(anchorElement)
          var carrotAnchorY = anchorElement.offset().top + anchorElement.height()
          var popupTop = carrotAnchorY + carrotHeight
          popup.css('top', popupTop)
          return popupTop < $(window).height() + $(window).scrollTop()
        }

        function popupPositionY(anchorElement){
          if (!setPopupPositionYAboveAnchor(anchorElement)){
            setPopupPositionYBelowAnchor(anchorElement);
          }
        }

        function setAboveOrBelow(aob){
          aboveOrBelow = aob;
          if (aob == 'above'){
            loginCarrotBottom.show();
            loginCarrot.hide();
            } else {
            loginCarrotBottom.hide();
            loginCarrot.show();
          }
        }

        $("#popup_close").bind('click',function(){
          closePopup();
        })

        function closePopup(){
          movie_id = 0;
          popup.hide();
          resetPopup(true);
        }

        function resetButton(){
          if (currentButton != 'null'){
            if (isLargeButton(currentButton)){
              currentButton.attr('src','/assets/controls/add_to_queue_lrg_off.gif');
              } else {
              currentButton.attr('src','/assets/controls/carousel_queueit.gif');
            }
          }
        }

        function isLargeButton(button){
          if(button.parent().attr('id') == 'add_to_queue_button'){
            return true
          } else {
            return false
          }
        }

        function growLoginPopup(adjustment){
          if (aboveOrBelow == 'above'){
            // We need to move the popup up
            var currentPosition = popup.offset().top
            popup.css('top', currentPosition + adjustment + 'px')
          }
        }

        function resetPopup(closing){
          $('#login_with_email_form').hide();
          $('#create_account_form').hide();
          $('#alternatives').show();
          if (closing){
            resetButton();
          }
        }


      $('#no_facebook').bind('click',function(){
        var initialHeight = popup.height();
        $('#alternatives').hide();
        $('#login_with_email_form').show();

        growLoginPopup(initialHeight - popup.height());
      })

      $('#no_account').bind('click', function(){
        var embeddedAction = getEmbeddedAction();
        redirectPage(getActionEmbeddedUrl('/users/sign_up'));
      })

      function popupCenterPopup(url, width, height, name) {
        var left = (screen.width/2)-(width/2);
        var top = (screen.height/2)-(height/2);
        return window.open(url, name, "menubar=no,toolbar=no,status=no,width="+width+",height="+height+",toolbar=no,left="+left+",top="+top);
      }

      function watchWindowPopup() {
        if (thewinPopup.closed) {
          if (!ember) {
            $.post(getActionEmbeddedUrl('http://gowatchit.com/popup/login'));
          } else {
            $.ajax({
              url: '/me',
              beforeSend: function(xhr) {
                var token = $('meta[name="csrf-token"]').attr('content');
                xhr.setRequestHeader('X-CSRF-Token', token);
              },
              dataType: 'json',
              success: function(response) {
                $.post(getActionEmbeddedUrl('http://gowatchit.com/popup/login'));
                GoWatchIt.set('user', GoWatchIt.User.create(response));
             },
             error: function(response, msg, error) {
               $.post(getActionEmbeddedUrl('http://gowatchit.com/popup/login')); // TODO this shoudl be an articulate error
             }
            });
            closePopup();
          }
        } else {
          setTimeout("watchWindowPopup()",2000);
        }
      }

      function facebook_login_url() {
        return getActionEmbeddedUrl('/users/auth/facebook?redirect_to=http%3A%2F%2Fgowatchit.com%2Fpopup%2Flogin%3Fmodality%3Dmain');
      }

      function openFacebookLoginPopup(){
          thewinPopup = popupCenterPopup(facebook_login_url(), 600, 400, 'authPopup');
          watchWindowPopup();
      }

    </script>
  </div>

  <!-- facebook auth -->
  <script>


    $('#facebook_login_popup').click(function() {
      openFacebookLoginPopup();
      return false;
    });


    function field_action(field,value){
      field.focus(function() {
        if ($(this).attr("value") === value) {
          $(this).attr("value", "");
          if (value === 'Password') {
            this.type = 'password';
          }
        }
      });
      field.blur(function() {
        if ($(this).attr("value") === "") {
          $(this).attr("value", value);
          if (value === 'Password') {
            this.type = 'text';
          }
        }
      });
    };
    field_action( $("#user_email_login") ,'Email');
    field_action( $("#user_password_login") ,'Password');
    field_action( $("#user_email_create") ,'Email');
    field_action( $("#user_password_create") ,'Password');

  </script>


