		<script type="text/javascript">
		function purdifier (longString) {
			//does not capture &nbsp; as space

		  var cleanString = longString.replace(/&nbsp;/g, ' ');
		  cleanString = cleanString.replace(/ /g,"-");
		  cleanString = cleanString.replace(/&-/g,'');
		  return cleanString;
		}
		jQ(document).ready(function() {
			jQ('#navi>li').each(function(index, value){
			  if( jQ(this).attr('class').indexOf("only-mobile") >= 0 ) {
			  	//do nothing for now
			  } else {    
			    //var dropDown = (index+1)+'--'+jQ(this).find('>span>a').text();
			    var ddName = '&lid=GlobalHeader_'+purdifier(jQ(this).find('>span>a').text());
			  	//capture top nav links
			   	jQ(this).find('>span>a').each(function(index, value) {
			   		var lidString = purdifier(ddName);
			   		var oldAnc = this;
			   		oldAnc.setAttribute('name', lidString);
			   	});
			   	//capture images and ipad close clicks
			    jQ(this).find('div.fiveColMenu>span>a').each(function(index, value) {
			    	if(jQ(this).text() == 'x') {
			    		var lidString = purdifier(ddName+'_ipad close');
			    	} else {
			    		var lidString = purdifier(ddName+'_'+jQ(this).find('img').attr('id'));	
			    	}			    	
				   	var oldAnc = this;
				    oldAnc.setAttribute('name', lidString);
			    });
			    jQ(this).find('div.fiveColMenu>span h3').each(function(index, value) {
			      var Group = ddName+'_Sub_';
			    //capture h3 group links
			      jQuery(this).find(">a").each(function(index, value){
			    	Group += purdifier(jQ(this).attr('title'));
				   	var oldAnc = this;
				    oldAnc.setAttribute('name', Group);
			      });

			    //capture ul links
			      jQ(this).find('+ul a').each(function(index, value) {
			        var lidString = Group + purdifier('_'+jQ(this).attr('title'));
			        var oldAnc = this;
			        oldAnc.setAttribute('name', lidString);
			      })

			    });
			  }
			});
		});
		</script>
		
			 
