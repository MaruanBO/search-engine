    $('.gv_filmstripWrap').css({ 'width':'100%' });
    $('.gv_navWrap').css({ 'height':'35px','width':'40px' });
    $('.gv_overlay').css({ 'background-color' : '#000000', 'opacity' : '.8' });
    $('.gv_navPrev').css({ 'position' : 'relative', 'left' : '-935px','top':'8px' });
    $('.gv_navNext').css({ 'position' : 'relative', 'left' : '5px','top':'-27px' });
    $('.gv_panelNavPrev').css({ 'background-image' : 'url("/imagesTSC/galleryview-large-left-arrow-white.png")', 'opacity' : '1', 'width' : '50px', 'height' : '58px', 'left' : '0px', 'display' : 'block' });
    $('.gv_panelNavNext').css({ 'background-image' : 'url("/imagesTSC/galleryview-large-right-arrow-white.png")', 'opacity' : '1', 'width' : '50px', 'height' : '58px', 'right' : '0px', 'display' : 'block' });
    $('.gv_navPrev').css({ 'background-image' : 'url("/imagesTSC/galleryview-small-left-arrow-white.png")', 'opacity' : '1', 'width' : '30px', 'height' : '35px' });
    $('.gv_navNext').css({ 'background-image' : 'url("/imagesTSC/galleryview-small-right-arrow-white.png")', 'opacity' : '1', 'width' : '30px', 'height' : '35px' });

    $('.gv_panelNavPrev').hover(function(){
        $('.gv_panelNavPrev').css({ 'background-image' : 'url("/imagesTSC/galleryview-large-left-arrow-blue.png")', 'opacity' : '1', 'display' : 'block' });
    },function(){
