    },function(){
        $('.gv_panelNavNext').css({ 'background-image' : 'url("/imagesTSC/galleryview-large-right-arrow-white.png")', 'opacity' : '1', 'display' : 'block' });
    });

    $('.gv_navPrev').hover(function(){
        $('.gv_navPrev').css({ 'background-image' : 'url("/imagesTSC/galleryview-small-left-arrow-blue.png")', 'opacity' : '1' });
    },function(){
