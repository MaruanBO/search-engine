        <script type="text/javascript">
            $(function () {
                $("#slides").slides({
                    generatePagination: true,
                    effect: 'fade',
                    fadeSpeed: 100,
                    hoverPause: true

                });

                var titleItems = $("#VideoTitles").children('p');

                var paginationDivItems = $("ul.pagination").children('li');
                //alert(paginationDivItems.length);
                var counter = 0;
                paginationDivItems.each(function () {
                    $(this).children('a').text(titleItems.eq(counter).text());
                    counter++;
                });

                paginationDivItems.children('a').click(function () {

                    var allVideoPlayers = $("iframe.vim");

                    for (var i = 0; i < allVideoPlayers.length; i++) {
                        vidPlayer = $f(allVideoPlayers[i]);
                        vidPlayer.api('unload');
                    }
                });

                paginationDivItems.hover(function () {
                    jQuery(this).addClass('hover');
                }, function () {
                    jQuery(this).removeClass('hover');
                });

                paginationDivItems.last().addClass('lastVideoTitle');

            });
        </script>

