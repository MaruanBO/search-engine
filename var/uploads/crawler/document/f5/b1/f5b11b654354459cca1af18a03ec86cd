    <script type="text/javascript">
        $(document).ready(function () {

            var usersBrowser = $.browser;
            if (usersBrowser.msie && usersBrowser.version == '7.0') {
                $(".downservices").click(function () {
                    $(".servicesdropped").toggle();
                    //var servicesDropdown = $.get(".downservices");
                    $("li.downservices").toggleClass("troynew");
					if($('li.downservices').hasClass('troynew')){
					$('#gallery').css('zIndex',-1);
					} else {
					$('#gallery').css('zIndex',1);
					}
                });

                $("#countries").click(function () {
                    var servicesDropdown = $.get(".downservices");
                    $(".servicesdropped").toggle();
                    $("li.downservices").toggleClass("troynew");
					if($('li.downservices').hasClass('troynew')){
					$('#gallery').css('zIndex',-1);
					} else {
					$('#gallery').css('zIndex',1);
					}
                });
            } else {
                $(".downservices").click(function () {
                    $(".servicesdropped").toggle();
                    $(this).toggleClass("troynew");
                });
            }
        });
    </script>
