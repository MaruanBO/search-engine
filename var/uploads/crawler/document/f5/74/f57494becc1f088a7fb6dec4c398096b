<script type="text/javascript" language="javascript">
  function menu()
	{
    var arrayTD = document.getElementsByTagName("td");
    
    for ( var i=0; i<arrayTD.length; i++ ) {

      if ( /top\-menu\-l/i.test(arrayTD[i].className) ) {
        arrayTD[i].onmouseover = function() {
          this.className = ( /top\-menu\-l\-n/i.test(this.className) )? "top-menu-l-o" : "top-menu-l-n";
        }
        arrayTD[i].onmouseout = function() {
          this.className = ( /top\-menu\-l\-o/i.test(this.className) )? "top-menu-l-n" : "top-menu-l-o";
        }
      }
			
      if ( /top\-menu\-m/i.test(arrayTD[i].className) ) {
        arrayTD[i].onmouseover = function() {
          this.className = ( /top\-menu\-m\-n/i.test(this.className) )? "top-menu-m-o" : "top-menu-m-n";
        }
        arrayTD[i].onmouseout = function() {
          this.className = ( /top\-menu\-m\-o/i.test(this.className) )? "top-menu-m-n" : "top-menu-m-o";
        }
      }
			
      if ( /top\-menu\-r/i.test(arrayTD[i].className) ) {
        arrayTD[i].onmouseover = function() {
          this.className = ( /top\-menu\-r\-n/i.test(this.className) )? "top-menu-r-o" : "top-menu-r-n";
        }
        arrayTD[i].onmouseout = function() {
          this.className = ( /top\-menu\-r\-o/i.test(this.className) )? "top-menu-r-n" : "top-menu-r-o";
        }
      }
			
    }
  }
</script>

