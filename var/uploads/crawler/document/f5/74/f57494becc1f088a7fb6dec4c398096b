  <script type="text/javascript" language="javascript">
    function expandRegion( id, bShow )
    {
      if ( id ) {
        var obj = document.getElementById(id).getElementsByTagName("div")[0];
  
        if ( /dest\-country/i.test(obj.className) )
          if ( bShow ) {
            obj.style.display = ( bShow == "block" )? "block" : "none";
            document.getElementById("trigger-"+id).innerHTML = ( bShow == "block" )? "-" : "+";
						var req = Spry.Utils.loadURL("GET", "/inc/setExpandState.cfm?Region=" + id + "&State=" + bShow, true);
          }
          else {
            obj.style.display = ( obj.style.display != "block" )? "block" : "none";
            document.getElementById("trigger-"+id).innerHTML = ( document.getElementById("trigger-"+id).innerHTML == "+" )? "-" : "+";
						var req = Spry.Utils.loadURL("GET", "/inc/setExpandState.cfm?Region=" + id + "&State=" + obj.style.display, true);
          }
				
      }
    }
  
    function expandAllRegions()
    {
      var arrayA = document.getElementsByTagName("a");
      var bShow = ( document.getElementById("trigger-show-all").innerHTML == "+" )? "block" : "none";
      
			//var req = Spry.Utils.loadURL("GET", "/inc/setExpandState.cfm?Region=All&State=" + bShow, true);
			
      for ( var i=0; i<arrayA.length; i++ ) {
        if ( /trigger\-/i.test(arrayA[i].id) &&  document.getElementById(arrayA[i].id.replace(/trigger\-/i, "")) )
          expandRegion( arrayA[i].id.replace(/trigger\-/i, ""), bShow );
      }
      
      document.getElementById("trigger-show-all").innerHTML = ( document.getElementById("trigger-show-all").innerHTML == "+" )? "-" : "+";
      document.getElementById("header-show-all").innerHTML = ( document.getElementById("header-show-all").innerHTML == "Show All..." )? "Hide All..." : "Show All...";
			
			if ( document.getElementById("trigger-show-all").innerHTML == "+" ) {
				var req = Spry.Utils.loadURL("GET", "/inc/setExpandState.cfm?Region=All&State=none", true);
			}
			
    }
  </script>
	
	
  



  
