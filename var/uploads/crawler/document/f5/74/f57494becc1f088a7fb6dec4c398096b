<script type="text/javascript" language="javascript">
  function openGuarantee()
  {
    var LeftPosition = ( screen.width )? ( screen.width - 600 ) / 2 : 0;
    var TopPosition = ( screen.height )? ( screen.height - 500 ) / 2 : 0;
    var WinSettings = "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=600,height=500" +
                      "top=" + TopPosition +
											",left=" + LeftPosition;
    window.open( "/guarantee.cfm", "Guarantee", WinSettings );
  }
</script>
