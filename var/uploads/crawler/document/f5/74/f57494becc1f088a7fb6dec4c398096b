<script type="text/javascript">
	window.onload=function() {
		//alert(document.getElementById('CountryID2').value);
		// Check to see if any multiple countries are selected
		//if(document.getElementById('CountryID2').value != 0 || document.getElementById('CountryID3').value != 0 || document.getElementById('CountryID4').value != 0) {
		var objCtry = document.getElementById('CountryID1');
		selIdx = objCtry.selectedIndex;
		if(objCtry[selIdx].value == 'M') {
			document.getElementById('CountryTR2').style.display='table-row';
			document.getElementById('CountryTR3').style.display='table-row';
			document.getElementById('CountryTR4').style.display='table-row';
		}
		menu();
		fabUpdate('All');
	}
	
</script>


