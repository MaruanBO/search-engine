                KI.featureFlipperDomain,
                KI.locale,
                'MxV1BqyRQGKSP_fqvVYQIw'
            ),
            $normalButton = $('.promo-recipes-growing .sbtn.normal'),
            $fbButton = $('.promo-recipes-growing .sbtn.facebook'),
            showFacebookButton = function() {
                $normalButton.addClass('visible-xs');
                $fbButton.addClass('hidden-xs');
            },
            facebookButtonExperiment;

        /**
         * Experiment for displaying facebook button on welcome banner.
         */
        if (facebookButtonFF === 1) {
            showFacebookButton();
        } else if (facebookButtonFF === null) {
            facebookButtonExperiment = new KI.GoogleExperiment('MxV1BqyRQGKSP_fqvVYQIw');
            facebookButtonExperiment.getVariation(function(variation) {
                if (variation === 1) {
                    showFacebookButton();
                }
            });
