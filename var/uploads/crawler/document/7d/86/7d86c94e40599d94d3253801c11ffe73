<script type="text/javascript" charset="utf-8">
//<![CDATA[

    var vimeoPlayers = [];
    function vimeo_player_loaded(playerId) {
        var videoId = playerId.replace('player-', '');
        var elem1 = document.getElementById('video-object-' + videoId + '-1');
        var elem2 = document.getElementById('video-object-' + videoId + '-2');
        var playerElem = null;
        if(elem1 && elem2) {
            playerElem = elem2;
        } else if(elem1) {
            playerElem = elem1;
        }
        if(playerElem) {
            var player = Object.create(oyster.vimeoPlayer);
            player.init(playerElem, {'isFlash':true});
            vimeoPlayers.push(player);
        }
    }

    (function($){
        var HpSlideshow = {
            init: function(options, elem) {
                this.options = $.extend({}, this.options, options);
                this.ndx = 0;
                this.$elem = $(elem);
                this.$content = this.$elem.find('#hpPhotos-content');
                this.$nextButton = $('.slideNext', this.$elem);
                this.$prevButton = $('.slidePrev', this.$elem);
                this.$counter = $('.slideCurrent', this.$elem);
                this.numItems = this.$content.find('li').length;
                this.changing = false;
                this.clicked = false;
                return this;
            },
            getNextIndex: function(index, direction) {
                if(direction == 'next')
                    if(index + 1 == this.numItems)
                        index = 0;
                    else
                        index++;
                else if(direction == 'prev')
                    if(index == 0)
                        index = this.numItems - 1;
                    else
                        index--;
                return index;
            },
            changeItem: function(direction, manual) {
                //pause all videos
                if(this.changing)
                    return;
                this.changing = true;
                var numPlayers = vimeoPlayers.length;
                for(var i = 0, len = numPlayers; i < len; i++) {
                    try {
                        vimeoPlayers[i].api('pause');
                    } catch(err) {}
                }

                var oldNdx = this.ndx;
                this.ndx = this.getNextIndex(this.ndx, direction);
                var $oldItem = this.$content.find('li').eq(oldNdx);
                var $newItem = this.$content.find('li').eq(this.ndx);
                var obj = this;

                if(direction == 'next') {
                    var newStart = '652px';
                    var oldEnd = '-652px';
                } else {
                    var newStart = '-652px';
                    var oldEnd = '652px';
                }

                $newItem.css({
                    left: newStart,
                    opacity: 0.3
                });
                $oldItem.animate({
                    left: oldEnd,
                    opacity: 0.3
                }, 600);
                $newItem.animate({
                    left: '0px',
                    opacity: 1.0
                }, 600, function () {
                    obj.$counter.html(obj.ndx + 1);
                    obj.changing = false;
                });
                
                if(manual) {
                    var $visibleItem = this.$content.find('li').eq(this.ndx);
                    if($visibleItem.find('div[data-video]').length)
                        var trackLabel = 'Video: ' + $visibleItem.find('div:first').data('video');
                    else
                        var trackLabel = $visibleItem.find('a').attr('href');
                }
            },
            autoplay: function() {
                if(!this.clicked) {
                    this.changeItem('next', false);
                    var obj = this;
                    var time = (this.ndx == 0) ? 7000 : 4000;
                    setTimeout(function() { obj.autoplay() }, time);
                }
            }
        }
        
        $.fn.hpSlideshow = function(options) {
            if(this.length) {
                return this.each(function() {
                    var instance = Object.create(HpSlideshow);
                    instance.init(options, this);
                    
                    instance.$nextButton.bind('click', function() {
                        instance.clicked = true;
                        instance.changeItem('next', true); 
                    });
                    instance.$prevButton.bind('click', function() {
                        instance.clicked = true;
                        instance.changeItem('prev', true); 
                    });
                    setTimeout(function() { instance.autoplay(); }, 7000);
                });
            }
        }
    })(jQuery);

    $(document).ready(function(){
        $('#autofillSearch2').oysterAutofill(autofillOptions);
        $('.autofill-field').focus();

        $('#hpPhotos').hpSlideshow();

        $('iframe.vimeoPlayer').each( function() {
            var player = Object.create(oyster.vimeoPlayer);
            player.init($(this).get(0));
            vimeoPlayers.push(player);
        });
    });
//]]>
</script>




