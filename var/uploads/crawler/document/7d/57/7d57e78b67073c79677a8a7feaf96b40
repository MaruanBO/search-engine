	<script type="text/javascript" language="Javascript">		
	
	  carouActive    = 1;
          carouTimer	 = true; // don't switch product while the user is looking at them
		
          var carouArrMargin = { 
            1: {id:4358, margin: 1},
            2: {id:4359, margin: 111},
            3: {id:4360, margin: 224},
            4: {id:4361, margin: 338},
            5: {id:4362, margin: 448}            
          };			
	
          function carouMouseOver(elem,num)	
          {
          	for( var i in carouArrMargin)	
		{	
			document.getElementById('car'+i).style.color='#2a363b';	
			document.getElementById('zone'+carouArrMargin[i].id).style.display = "none";	
		}		
	
		elem.style.color='#2a363b';	
		document.getElementById('carou_menu_active').style.marginLeft = carouArrMargin[num].margin + "px";	
		$('#zone'+carouArrMargin[num].id).show();	
		carouActive = num;
	
	  }////////mouseOver
	
	  
          function carouMouseOut(elem,num)
	  {		
		return;		
	  }////////mouseOut	
	
		
	  function switchProducts()
	  {		
		if (carouTimer == true)
		{
			if (carouActive + 1 == carouArrMargin.length)
			{
				carouActive = 1;
			}
			else
			{
				carouActive = carouActive + 1;
			}
	
			carouMouseOver(document.getElementById('car'+carouActive), carouActive);
		}
	
	  }		
	
          //          window.setInterval("switchProducts()", 6000);
	
	</script>
	
	
	
