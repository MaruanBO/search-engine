<script type="text/javascript">
jQuery(document).ready(function() {
    $('a#clickurl').click(function (b) {
        var offset = $(this).offset();
        var bottomx = offset.left + $(this).width();
        var bottomy = offset.top + $(this).height();
        if( !$(this).attr('href').match(/oye=/) ) {
            $(this).attr('href', $(this).attr('href') + '&cx=' + b.pageX + '&cy=' + b.pageY + '&oxs=' + offset.left + '&oys=' + parseInt(offset.top) + '&oxe=' + bottomx + '&oye=' + parseInt(bottomy));
        }
    });
});
</script>

