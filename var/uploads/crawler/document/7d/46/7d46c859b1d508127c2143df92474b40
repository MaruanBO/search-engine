$(document).ready(function(){
$("a.inline").fancybox({
		'hideOnContentClick': false,
		'autoDimensions'	: false,
		'width': 750,
		'height' : 500,
		'hideOnOverlayClick' : false,
		'onComplete' : function(){
			$.fancybox.resize();
		},
		
		'onClosed'	: function(){
			$('.msgbox').hide();
			parent.document.location.reload();
		}
	});	
	
var url = window.location.pathname;
var filename = url.substring(url.lastIndexOf('/')+1);
    if (filename=="find.php" ){
	var moreans;		
		for(i=0;i<10;i++)
		{
		  moreans="more-ans"+i;
		 if (document.getElementById(moreans))
            hidediv(moreans);	
		}
   }
  }); 
  
