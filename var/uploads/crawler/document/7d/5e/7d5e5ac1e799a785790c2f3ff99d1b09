			$('#fixeddiv').animate({ height: '176px' });
			open = true;
		}
		setTimeout(function() {
			var totContent = $('.firstmoddiv').find("div.scrollimgs").length;
			if(totContent > 5 ){ 
				totContent = totContent * 154;
				totContent = totContent+'px';
				$('.firstmoddiv').animate({width:totContent});
			}
		},500);
		CloseColorBox();
	}
