<script type="text/javascript">
    function submitEnterTextBox( e, callBack) {
        var key;
        try {
            if (window.event || !e.which) {
                key = e.keyCode;
            }
            else { key = e.which; }

            if (key == 13) {
                callBack();
                return false;
            }
            else return true;
        }
        catch (err) {
            return true;
        }
    }

    function subscribeNow() {
        var email = $('#TextBoxEmail').val();
        if (email == "") {
            return false;
        }
        else {           
            var emailURL = 'http://health.lef.org/LandingPages/NewsSubscription.aspx' + '?email=' + email;            
            window.location = emailURL;
            return false;
        }
    }
</script>


