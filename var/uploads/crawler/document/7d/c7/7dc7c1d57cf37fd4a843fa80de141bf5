<script type="text/javascript">

  function ShowAddModal(itemNumber, sourceCode) {
    var qty = $("input[name='Qty" + itemNumber + "']:checked").val();
    $("#hidQty").val(qty);
    $("#hidItemNumber").val(itemNumber);
    $("#hidSourceCode").val(sourceCode);
    AddItemToCart(itemNumber);
  }

  function viewCart() {
    var itemNumber = $("#hidItemNumber").val();
    var qty = "1";
    var dataString = 'https://mycart.lef.org/';
    var sourceCode = $('#hidSourceCode').val();

    if (sourceCode != '') {
      dataString = dataString + "?item=" + itemNumber + "&qty=" + qty + "&sourcecode=" + sourceCode + "&querytermadd=false";
    }
    else {
      dataString = dataString + "?item=" + itemNumber + "&qty=" + qty + "&querytermadd=false";
    }

    window.location = dataString;
  }

  function continueShopping() {
    $("#addToCart").modal('hide');
  }

  function AddToCartHandler(result) {
    if (result.Success) {
      $("#addToCartHeading").text("Success! Item(s) added to cart");
      if (result.Messages.length > 0) {
        $("#addToCartMessage").text(result.Messages[0].Message);
      } else {
        $("#addToCartMessage").text("You have added your selected item(s) to your cart. What do you want to do next?");
      }

    } else {
      $("#addToCartHeading").text("Failed to add to cart").css("color", "#ff0000");
      $("#addToCartMessage").text(result.Messages[0].Message);
    }
    $("#addToCart").modal('show');
  }

  function AddItemToCart() {
    var itemNumber = $("#hidItemNumber").val();
    var qty = $("#hidQty").val();
    var url = "https://mycart.lef.org/Cart/AddToCartExternal?item=" + itemNumber + "" + "&qty=" + qty + "&callback=AddToCartHandler";

    var script = document.createElement('script');
    script.src = url;
    document.body.appendChild(script);
  }

</script>
        
