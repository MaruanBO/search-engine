                                    <div class="input-group">
                                        <input id="Query" name="Query" type="text" class="form-control input-sm" />
                                        <span class="input-group-btn">
                                            <button onclick="return Search();" id="SearchButton" class="btn btn-warning btn-sm"><i class="fa fa-search"></i></button>
                                        </span>
                                    </div>
                                    <script type="text/javascript" language="javascript">

                                        $('#Query').keyup(function (e) {
                                            if (e.keyCode == 13) {
                                                // Do something
                                                Search();
                                            }
                                        });

                                        function Search() {
                                            if (!emptySearch1()) {
                                                return false;
                                            }

                                            var queryTerm = $("#Query").val();

                                            if (queryTerm) {
                                                queryTerm = queryTerm.replace("&", "");

                                                // Trim function
                                                if (!String.prototype.trim) {
                                                    String.prototype.trim = function () {
                                                        return this.replace(/^\s+|\s+$/g, '');
                                                    };
                                                }

                                                // Format function
                                                String.prototype.format = function () {
                                                    var args = arguments;
                                                    return this.replace(/{(\d+)}/g, function (match, number) {
                                                        return typeof args[number] != 'undefined'
                                                                ? args[number]
                                                                : match
                                                            ;
                                                    });
                                                };

                                                // Redirect to search
                                                queryTerm = queryTerm.trim();
                                                window.location.href = 'http://search.lef.org' + '/?q=' + queryTerm;
                                                return false;
                                            }
                                        }

                                        function emptySearch1() {
                                            var q;
                                            q = $("#Query").val();
                                            if (q == 'Search: coq10, 00335, weight loss' || q == '') {
                                                return false;
                                            }
                                            else {
                                                return true;
                                            }
                                        }
                                    </script>
