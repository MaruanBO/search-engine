                            <input type="hidden" name="ctl00$g_ea4403c5_4ce8_444e_ae25_b809d9ac35f0$ctl00$mainMenu$searchBoxMini$hfSearchHomeCat" id="ctl00_g_ea4403c5_4ce8_444e_ae25_b809d9ac35f0_ctl00_mainMenu_searchBoxMini_hfSearchHomeCat" value="/search/?q={0}" />
                            <script type="text/javascript" language="javascript">
                                function performSearch() {
                                    if (!emptySearch()) {
                                        return false;
                                    }

                                    var queryTerm = $("#Query2").val();

                                    if (queryTerm) {
                                        queryTerm = queryTerm.replace("&", "");

                                        // Trim function
                                        if (!String.prototype.trim) {
                                            String.prototype.trim = function () {
                                                return this.replace(/^\s+|\s+$/g, '');
                                            };
                                        }

                                        // Format function
                                        String.prototype.format = function () {
                                            var args = arguments;
                                            return this.replace(/{(\d+)}/g, function (match, number) {
                                                return typeof args[number] != 'undefined'
                                                        ? args[number]
                                                        : match
                                                    ;
                                            });
                                        };

                                        // Redirect to search
                                        queryTerm = queryTerm.trim();
                                        window.location.href = 'http://search.lef.org' + '/?q=' + queryTerm;
                                        return false;
                                    }
                                }

                                function emptySearch() {
                                    var q;
                                    q = $("#Query2").val();
                                    if (q == 'Search: coq10, 00335, weight loss' || q == '') {
                                        return false;
                                    }
                                    else {
                                        return true;
                                    }
                                }
                            </script>
