			var dev = "dev";
			var mobile = "fsh-mobile-dev";
			var ppInternal = "fshppinternal";
			var ppExternal = "fshppexternal";

			temp_s_fsh_account = "fshglobalinternal";

			if (isPPInternal(s_fsh_account)) {
				temp_s_fsh_account = ppInternal + "," + temp_s_fsh_account;
			} else if (isPPExternal(s_fsh_account)) {
				temp_s_fsh_account = ppExternal + "," + temp_s_fsh_account;
			}

			if (isDev(s_fsh_account)) {
				if (isMobile(s_fsh_account)) {
					temp_s_fsh_account = mobile;
				} else {
					temp_s_fsh_account += dev;
				}
			}

			s_fsh_account = temp_s_fsh_account;
		}

		function isInternalIP() {
			var c = jQuery.cookie("sitecatalyst-blacklist");
			
			if (c != undefined) {
				return true;
			}
			
			return false;
		}

		function startsWith(word, start) {
			return word.indexOf(start, 0) === 0;
		}

		function endsWith(word, end) {
			return word.indexOf(end, word.length - end.length) !== -1;
		}

		function isDev(data) {
			return endsWith(data, dev);   
		}

		function isMobile(data) {
			return data === mobile;
		}

		function isPPInternal(data) {
			return startsWith(data, ppInternal);
		}

		function isPPExternal(data) {
			return startsWith(data, ppExternal);
		}
	</script>
    
    
    	
    	
    		
