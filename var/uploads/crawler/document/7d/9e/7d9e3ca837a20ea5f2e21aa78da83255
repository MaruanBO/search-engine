<script type="text/javascript" src="http://cf.ydcdn.net/1.0.1.28/js/jquery-ui-1.8.custom.min.js"></script>
<script type="text/javascript">
var slider = null;
$m(function(){
if($m('.bxslider').length > 0)
{
	ControlBoxes();
	$m(window).on("orientationchange",function(){
		$m(".bx-default-pager").hide();
		setTimeout(function(){
			slider.destroySlider();
			ControlBoxes();
		}, 300);
	});
	$m(window).resize(function() {
		$m(".bx-default-pager").hide();
		setTimeout(function(){
			slider.destroySlider();
			ControlBoxes();
		}, 300);
	});
}


function ControlBoxes()
{
	if($m('.bxslider').length > 0)
	{
		var singleItemWidth = 100;
		var partialAdjust = 80;
		var ShowTotal = Math.round((($m(".definitions_slider").width())-($m(".bx-prev").width()*2))/singleItemWidth);
		ShowTotal = (ShowTotal == 1) ? 1 : (ShowTotal-1);
		slider = $m('.bxslider').bxSlider({
			infiniteLoop: false,
			responsive: true,
			maxSlides: ShowTotal,
			slideWidth: singleItemWidth,
			hideControlOnEnd: true,
			moveSlides: ShowTotal,
			onSliderLoad: function(currentIndex){
					$m(".slider_item").css("visibility","visible");
					$m(".bxSliderLoader").remove();
					if($m(".slider_item").length <= ShowTotal)
					{
						$m(".bx-controls").hide();
						$m(".bx-wrapper").css("margin-bottom","0");
					}
					
					var totalDot = $m(".bx-pager-item").length;
					var removeIndex = 0;
					var lastFilled=2;
					var breakLoop=false;
					for(var i=1; i <= totalDot; i++)
					{
						lastFilled=(lastFilled-1);
						for(var j=1; j <= ShowTotal; j++)
						{
							if(lastFilled == $m(".slider_item").length)
							{
								breakLoop=true;
							}
							
							if(breakLoop)
							{
								break;
							}
							
							lastFilled++;
						}
						
						if(breakLoop)
						{
							removeIndex=(i+1);
							break;
						}
						
					}
					
		
					if(removeIndex > 1)
					{
						$m(".bx-pager-item:nth-child("+removeIndex+")").remove();
					}
			},
			onSlideBefore: function($slideElement, oldIndex, newIndex){
					$m(".slider_item").css("visibility","visible");
					$m(".slider_item").removeClass('hidden');
					$m(".slider_item").removeClass('visible');
					if ( !$m('.bx-clone').is(":visible") ) {
						$m('.bx-clone').show();
					}
			},
			onSlideAfter: function($slideElement, oldIndex, newIndex){
				var mainOffsetLeft = $m(".bx-viewport").offset().left;
				var mainOffsetRight = mainOffsetLeft+$m(".bx-viewport").width();
				$m(".slider_item").css("visibility","visible");
				$m(".slider_item").each(function(){
					var currItemOffsetLeft = $m(this).offset().left;
					var currItemOffsetRight = currItemOffsetLeft+partialAdjust;
					if(((currItemOffsetLeft) < mainOffsetLeft) || (currItemOffsetRight > mainOffsetRight))
					{
						$m(this).addClass('hidden');
						$m(this).css("visibility","hidden");
					}
					else
					{
						$m(this).addClass('visible');
					}
				});
				
				
				var lastItem = $m('.slider_item.visible:first').offset().left;
				var nextItem = $m(".bx-viewport").offset().left;
				var setTransform= ((lastItem-mainOffsetLeft));
				
				// Replace with transform, if supported
				if('WebkitTransform' in document.body.style) 
				{
					var CurrentTransformValue = eval($m('.bxslider').css('transform'));
					var transformVal= "translate3d(" + (CurrentTransformValue-setTransform) + "px, 0, 0)";
					$m('.bxslider').css('-webkit-transform',transformVal);
				}
				else if('transform' in document.body.style) 
				{
					var CurrentTransformValue = eval($m('.bxslider').css('transform'));
					var transformVal= "translate3d(" + (CurrentTransformValue-setTransform) + "px, 0, 0)";
					$m('.bxslider').css('transform',transformVal);
					$m('.bxslider').css('-ms-transform',transformVal);
					$m('.bxslider').css('-moz-transform',transformVal);
				}
				else
				{
					var CurrentTransformValue = $m('.bxslider').css('left').replace("px","");
					if($m('.slider_item.visible:first').offset().left != undefined)
					{
						$m('.bxslider').css('left',(CurrentTransformValue)+"px");
					}
				}
			}
		});
	}
	BrowseBoxCounter();
}

$m(document).on('click','.bx-pager-item:last',function(){
	$m('.bx-next').addClass('disabled');
});
	
$m(".search-clear-icon").click(function(){
	$m("#yd_search_bar").val("");
	$m(".search-clear-icon").hide();
});

function BrowseBoxCounter()
{
	$m(".bx-default-pager").show();
	var totalBoxes= $m(".bx-pager-item").length;
	var boxesToFit = Math.round($m(".bx-controls").width()/$m(".bx-pager-item").width());
	var lastItem=0;
	if(totalBoxes > boxesToFit)
	{
		boxesToFit=(boxesToFit-2)
		var Seprator= Math.round(totalBoxes/boxesToFit);
		Seprator = (Seprator == 1) ? 2 : Seprator;
		var bx_counter=0;
		$m(".bx-pager-item").hide();
		$m(".bx-pager-item").each(function(){
		   if((bx_counter%Seprator) == 0 || (bx_counter == ($m(".bx-pager-item").length-1)))
		   {
			   if(bx_counter == ($m(".bx-pager-item").length-1))
				{
					$m(this).show();
					if($m(".bx-pager-item:visible").length > boxesToFit)
					{
						lastItem.hide();
					}

				}
				else
				{
					$m(this).show();
					lastItem=$m(this);
				}
		   }
		   bx_counter++;
		});
	}

	$m('.bx-default-pager').css('margin-left','-22px');
	$m('.bx-default-pager').css('width',$m('.bx-wrapper').width()+40+'px');
	if($m(".bx-pager-item").length <= 1)
	{
		$m(".bx-pager-item").hide();
		$m(".bx-wrapper").css("margin-bottom","0");
	}
}


$m(".has-dropdown").click(function(){
	if($m(window).width() < 700)
	{
		setTimeout(function(){
			if($m(".top-bar li.moved").length > 0)
			{
				$m(".top-bar-section ul").addClass("expandHeight");
			}
			else
			{
				$m(".top-bar-section ul").removeClass("expandHeight");
			}
		}, 500);
	}
});
if($m(window).width() < 685)
{
	$m(".has-dropdown").removeClass("active");
	var dataToMove = $m("#ydHomePageSocialSection").clone();
	$m("#ydHomePageSocialSection").remove();
	dataToMove.insertAfter($m("#ydHomePageAd"));
}
else
{
	$m(".top-bar").removeClass("expanded");
}
$m("#yd_search_bar").autocomplete({
				source: function(request,response){

                var searchedItem=$m("#yd_search_bar").val();
				if(searchedItem.length>2)
				{
					$(".search-clear-icon").show();
					$m.getJSON("/keywordhelp/"+searchedItem+"/", function(data) {
					var suggestions = [];
					response( $m.map( data, function( item ) {
					return {
						label: item,
						value: item
							}
						}));
					});
				}
		},

        minLength: 3,
        delay: 100,
        select :function(e,ui) {
							$m("#yd_search_bar").val(ui.item.value);
				$m('#searchbox').submit();
            },

        change: function() {
              //$m("#yd_search_bar").val("").css("top", 2);
            }
        });
		$m('input').placeholder();
});

