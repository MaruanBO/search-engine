        <ul class="tabs-container four-tabs clearfix">
			
				
					
						<li class="active"><span>Bronze</span></li>
					
					
				
			
				
					
					
                        
                        	
                            
								<li class=""><span>Silver</span></li>
                            
                        
					
				
			
				
					
					
                        
                        	
                            
