				if(response1.status == "connected")
				{
					fbid = response1.authResponse.userID;
					Profile.createProxyCookieWithCheck(response1);
					izlesene.accessToken = response1.authResponse.accessToken;
					if(typeof izlesene.autoLoginFacebook != 'undefined') {
						izlesene.autoLoginFacebook(fbid);
					}

				} else if (response1.status === 'not_authorized') {
                    if(window.location.pathname == '/' && getCookie('scroll_fb') != "1" && $('.modal_outer').length>0 == 0 )
