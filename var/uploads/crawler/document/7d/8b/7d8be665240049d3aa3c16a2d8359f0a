		if (typeof response.zones[2113] != 'undefined') {
			var seedHtml = '';
			seedHtml += '<li>';
			seedHtml += '<a href="'+response.zones[2113].link+'" title="'+response.zones[2113].title.replace("<![CDATA[", "").replace("]]>", "")+'" onclick="track_event(\'new_index\',\'sponsor_1\');">';
