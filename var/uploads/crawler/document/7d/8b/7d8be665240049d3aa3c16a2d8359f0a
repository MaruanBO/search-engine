		}

	}

	function pageskinSwf() {

        if(typeof left == 'undefined')
        	left = '660';

        /* for premiums */
        $(".container").css("position","relative");
        $(".container").css("z-index","10");

        $(".wrapper").addClass("wrapper_pageskin");

        var psclicktag = encodeURIComponent(virgulContent.zones[7].click);

        $("#videopage_pageskin").html('<object width="1920" height="1137" type="application/x-shockwave-flash" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,45,0" id="pageskinSwf"><param name="allowScriptAccess" value="always"><param name="allowFullScreen" value="true"><param name="quality" value="best"><param name="bgcolor" value="#000000"><param name="wmode" value="opaque"><param name="scale" value="noscale"><param name="movie" value="'+virgulContent.zones[7].background+'?clicktag='+psclicktag+'&clickTag='+psclicktag+'&clickTAG='+psclicktag+'"><param name="salign" value="tc"><param name="flashvars" value="clicktag='+psclicktag+'&clickTag='+psclicktag+'&clickTAG='+psclicktag+'"><embed src="'+virgulContent.zones[7].background+'?clicktag='+psclicktag+'&clickTag='+psclicktag+'&clickTAG='+psclicktag+'" wmode="opaque" bgcolor="#000000" allowscriptaccess ="always" name="pageskinSwf" allowfullscreen="true" scale="noScale" flashvars="clicktag='+psclicktag+'&clickTag='+psclicktag+'&clickTAG='+psclicktag+'" quality="best" salign="tl" width="1920" height="1137" type="application/x-shockwave-flash"></embed></object>');

        setPageskinSwfPosition();

    	$(window).resize(setPageskinSwfPosition);

    	$("html").css("background-color","#"+virgulContent.zones[7].color);

        $("#videopage_pageskin").css("width","100%");
        $("#videopage_pageskin").css("height","100%");
