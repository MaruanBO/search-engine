		$("#roadblock_show").remove();
		$("#showad").hide();
		setCookie('roadblock_closed',0);
	}


	function openRoadBlockStatic() {
		var content = '<div id="vrgdiv53565f146e12927b78814446" style="width: 970px; height: 250px; margin: 0px auto;"><div style="position: relative;"><div id="VIRGUL_EXPANDABLE_CONTAINER" style="position: absolute; overflow: visible; top: 0px; width: 970px; height: 500px; z-index: 199; clip: rect(0px 970px 250px 0px);"><object width="970" height="500" id="banner_expand" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,45,0"><param name="allowfullscreen" value="true"><param name="allowscriptaccess" value="always"><param name="wmode" value="transparent"><param name="movie" value="http://www.izlesene.com/player_others/vodafone_live.swf&amp;clicktag=&amp;clickTAG=&amp;"><param name="flashvars" value=""><embed name="banner_expand" src="http://www.izlesene.com/player_others/vodafone_live.swf?clickTag=&amp;clicktag=&amp;clickTAG=" wmode="transparent" allowfullscreen="true" allowscriptaccess="always" menu="false" type="application/x-shockwave-flash" width="970" height="500" flashvars=""></object></div></div></div>';

		//$("section.featured_area").css('margin-top','100px');

        $("body").css('overflow-x','hidden');
        $("#adContainer").html('<div id="roadblock" class="roadblock" style="position:relative">'+content+'</div>');
		$(".nb_default").hide();
		$("#roadblock_show").remove();
		$("#showad").hide();
		setCookie('roadblock_closed',0);
	}


	function closeRoadBlock(){
		try{
			collapse_banner();
		}
		catch(err)
  		{
  		}
		$("#roadblock").remove();
		$("#adContainer").append('<div id="showad" style="width:970px; margin: 0 auto;"><a href="javascript:;" class="show_ad" onclick="openRoadBlock(\'click\');" id="roadblock_show" style="float:right;	font-size:11px; color: #aaa; margin-top: 10px;">Reklamı Göster</a><div class="clear"></div></div>');
		setCookie('roadblock_closed',1);
	}

