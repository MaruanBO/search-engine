function expand_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.pause()}catch(e){};document.getElementById('VIRGUL_EXPANDABLE_CONTAINER').style.clip='rect(0px,'+970+'px,'+500+'px,0px)';};function collapse_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.resume();}catch(e){}document.getElementById('VIRGUL_EXPANDABLE_CONTAINER').style.clip='rect(0px,'+970+'px,250px,'+0+'px)';};function close_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.unmute();}catch(e){};closeRoadBlockStatic()};</script>

