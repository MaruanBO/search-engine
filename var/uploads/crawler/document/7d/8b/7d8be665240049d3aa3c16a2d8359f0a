<script type="text/javascript">

function trackHomepageClick(event, category, params, params2) {
	cookieValue = String(readCookie('homepage_click'));
	if(cookieValue.indexOf(params) == -1) {
		iStat.track(event, 'm', category + ':' + params, params2);
		cookieValue = cookieValue + '|'+params;
		createCookie('homepage_click',cookieValue, 0.04);
	}
}


</script>


