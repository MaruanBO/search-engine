			<script type='text/javascript'>
jQuery(document).ready(function() {

function addCommas(nStr) {
    nStr += '';
    var x = nStr.split('.');
    var x1 = x[0];
    var x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
      
    while (rgx.test(x1)) {
        x1 = x1.replace(rgx, '$1' + ',' + '$2');
    }
    return x1 + x2;
}

$(".scoreValue").each(function(i){
    var num = addCommas(Math.floor(parseInt(Number($(this).text()))));
    if (i == 0) {
    	$(this).text("$"+num);
    } else {
        $(this).text(num);
    }
});
});
			</script>
