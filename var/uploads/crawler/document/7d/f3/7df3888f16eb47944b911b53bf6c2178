		<script type='text/javascript'>
		
			magicinterval = 8000;
		
			magictimer = setInterval(function(){
					magicsliderGoTo('next');
			}, magicinterval);
		
			jQuery(document).ready(function(){
			
				curframe=jQuery('#magicslider_currentframe').val();
				maxframe=jQuery('#magicslider_maxframe').val();
				
				jQuery('.magicslider_img').css('top', function(index,height){
		   			if (jQuery(this).height() > 370){
		   				var offset = jQuery(this).height() - 370;
		   				offset = '-' + offset / 2+'px';
		   				return offset;
		   			}
           			});
           			
           			jQuery('.magicslider_root').find('ul').bind('mouseenter', function(){
           				window.magicslider_pause = true;
           			}).end().bind('mouseout', function(){
           				window.magicslider_pause = false;
           			});
			
				jQuery('#magicsliderf-nav-next').children('.nav-clicktarget').bind('click',function(){
					magicsliderGoTo('next');
				});
				
				jQuery('#magicsliderf-nav-prev').children('.nav-clicktarget').bind('click',function(){
					magicsliderGoTo('prev');
					
				});
				
				jQuery('.magicinnerbutton').bind('click',function(){
					var tarframe=jQuery(this).children('input').val();
					magicsliderGoTo(tarframe);
					
				});
				
			
			});
			
			function magicsliderGoTo(tarframe){
				if (tarframe == 'next'){
					if (curframe < (maxframe-1)){
						tarframe = parseInt(curframe) + 1;
					} else {
						tarframe = 1;
					}
				} else if (tarframe == 'prev'){
					if (curframe != 1){
						tarframe = parseInt(curframe) -1;
					} else {
						tarframe = parseInt(maxframe) -1;
					}
				}
				
				clearInterval(magictimer);
				jQuery('.magicselected').removeClass('magicselected');
				//var curframe=jQuery('#magicslider_currentframe').val();
				jQuery('#magicslider_currentframe').val(tarframe)
				
				//jQuery('#magicslider-'+curframe).fadeOut();
				//jQuery('#magicslider-'+tarframe).fadeIn();
								
				jQuery('#magicslider-'+curframe).hide();
				jQuery('#magicslider-'+tarframe).show();
				
				jQuery('#magicbutton-'+tarframe).addClass('magicselected');
				
				curframe = tarframe;
				
				magictimer = setInterval(function(){
					magicsliderGoTo('next');
				}, magicinterval);
				
			}
			
			
		</script>
		<div class='magicslider_root'>
		
