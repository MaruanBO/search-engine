<script src="//www.hlserve.com/Delivery/ClientPaths/Library/hook.js?apiKey=db1d4221-1c70-46f9-8f5e-26d0b09a40c7" type="text/javascript"></script>
<script type="text/javascript">

var pageType = 'home';
var allowedCUserId = '';
var c_userid = '';
		if(c_userid == '')
		allowedCUserId = null;
		else
		allowedCUserId = c_userid;

var allowedRegId = '';
var c_regid = '';
		if( c_regid == '')
		allowedRegId = null;
		else
		allowedRegId = c_regid;
if(pageType == 'product')   
	{  
		var skuList  = '';
		var allowedParentSku = '';
		var sku = '';
		if(skuList == '')
		{
			allowedParentSku = null;
			sku = '';
		}
		else
		{
			allowedParentSku = '';
			sku = skuList;
		}
		
		HLLibrary.newUpdate()
		.setProperty("hlPageType", "P")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "product")
		.setProperty("availability", "")
		.setProperty("inStock", "")
        .setProperty("sku", sku)
		.setProperty("parentSku", allowedParentSku)
		.setProperty("price", "")
		.setProperty("regularPrice", "")
		.submit();
	}
else if(pageType == 'search')
	{	
		var e = document.getElementById("ppg");
		var pgSize = e.options[e.selectedIndex].text;
		var resCount = '0';
		var pgNum = '1';
		if(resCount > pgSize*pgNum)
		{
			resCount = pgSize;
		}
		else if((resCount < pgSize*pgNum) && pgNum != '1')
		{
			resCount = resCount - (pgSize*(pgNum-1));
		}

		var e1 = document.getElementById("s");
		var sortFilter = e1.options[e1.selectedIndex].text;
		var sortFilterHL = '';
		if(sortFilter == 'Best Selling' || sortFilter == 'Recently Added')
		sortFilterHL = 'default';
		else if(sortFilter == 'Price: Low to High')
		sortFilterHL = 'price-asc';
		else if(sortFilter == 'Price: High to Low')
		sortFilterHL = 'price-desc';
		else if(sortFilter == 'Top Rated')
		sortFilterHL = 'rating-desc';
		var min_price = '';
		var max_price = '';
		var brandFilter = '';
		var genderFilter = '';
		var ageFilter = '';
		var htgiFilter = '';
		var catFilter = '';
		var charFilter = '';
		var priceFilter = '';
		if(min_price == '')
		var allowedMinPrice = null;
		else
		var allowedMinPrice = min_price;
		if(max_price == '')
		var allowedMaxPrice = null;
		else
		var allowedMaxPrice = max_price;
		if(brandFilter == '')
		var allowedBrandFilter = null;
		else
		var allowedBrandFilter = brandFilter;
		if(genderFilter == '')
		var allowedGenderFilter = null;
		else
		var allowedGenderFilter = genderFilter;
		if(ageFilter == '')
		var allowedAgeFilter = null;
		else
		var allowedAgeFilter = ageFilter;
		if(htgiFilter == '')
		var allowedHtgiFilter = null;
		else
		var allowedHtgiFilter = htgiFilter;
		if(catFilter == '')
		var allowedCatFilter = null;
		else
		var allowedCatFilter = catFilter;
		if(charFilter == '')
		var allowedCharFilter = null;
		else
		var allowedCharFilter = charFilter;
		if(priceFilter == '')
		var allowedPriceFilter = null;
		else
		var allowedPriceFilter = priceFilter;
		HLLibrary.newRequest()
		.setTaxonomy(null)
		.setProperty("hlPageType","S")
		.setKeyword("")
		.setModule("172x3892_T-R-IG_TI_1-8_InGrid2")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "search")
		.setProperty("minOrganic ", "3")
		.setProperty("minAds", "1")
		.setProperty("maxAds ", "8")
		.setProperty("pCount", resCount)
		.setProperty("pgSize", pgSize)
		.setProperty("pgN", "1")
		.setProperty("organicSKUs", "")
		.setProperty("sort", sortFilterHL)
		.setProperty("minPrice",allowedMinPrice)
		.setProperty("maxPrice",allowedMaxPrice)
		.setProperty("minRating",null)
		.setProperty("brand",allowedBrandFilter)
		.setFilter("gender",allowedGenderFilter)
		.setFilter("age",allowedAgeFilter)
		.setFilter("howtogetit",allowedHtgiFilter)
		.setFilter("category",allowedCatFilter)
		.setFilter("Character/Theme",allowedCharFilter)
		.setFilter("price",allowedPriceFilter)
		.setAttribute("site", "bru")
		.submit();
	}
else if(pageType == 'home')
	{
		HLLibrary.newRequest()
		.setTaxonomy("home")
		.setModule("172x3892_T-R-IG_TI_1-8_InGrid2")
		.setProperty("hlPageType", "H")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "home")
		.setProperty("productid", "")
		.setProperty("price", "")
		.setProperty("quantity", "")
		.setAttribute("site", "bru")
		.submit();
	}

else if(pageType == 'category')
	{
		HLLibrary.newRequest()
		.setTaxonomy("2255957")
		.setModule("172x3892_T-R-IG_TI_1-8_InGrid2")
		.setProperty("hlPageType", "M")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "category")
		.setAttribute("site", "bru")
		.submit();
	}
else if(pageType == 'cmsTemplates')
	{
		HLLibrary.newRequest()
		.setTaxonomy("2255957")
		.setModule("172x3892_T-R-IG_TI_1-8_InGrid2")
		.setProperty("hlPageType", "M")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "shop")
		.setAttribute("site", "bru")
		.submit();
	}
else if(pageType == 'thanks')
	{
		HLLibrary.newOrder()
		.setProperty("hlPageType", "C")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("sku", "")
		.setProperty("price", "")
		.setProperty("regularPrice", "")
		.setProperty("quantity", "")
		.setProperty("orderId", "")
		.setProperty("orderSubTotal", "")
		.setProperty("pubPageType", "thanks")
		.setProperty("addId", allowedRegId)
		.submit();
	}
else if(pageType == 'family')
	{
		var e = document.getElementById("ppg");
		var pgSize = e.options[e.selectedIndex].text;
		var resCount = '0';
		var pgNum = '1';
		if(resCount > pgSize*pgNum)
		{
			resCount = pgSize;
		}
		else if((resCount < pgSize*pgNum) && pgNum != '1')
		{
			resCount = resCount - (pgSize*pgNum);
		}
		var e1 = document.getElementById("s");
		var sortFilter = e1.options[e1.selectedIndex].text;
		var sortFilterHL = '';
		if(sortFilter == 'Best Selling' || sortFilter == 'Recently Added')
		sortFilterHL = 'default';
		else if(sortFilter == 'Price: Low to High')
		sortFilterHL = 'price-asc';
		else if(sortFilter == 'Price: High to Low')
		sortFilterHL = 'price-desc';
		else if(sortFilter == 'Top Rated')
		sortFilterHL = 'rating-desc';
		var min_price = '';
		var max_price = '';
		var brandFilter = '';
		var genderFilter = '';
		var ageFilter = '';
		var htgiFilter = '';
		var catFilter = '';
		var charFilter = '';
		var priceFilter = '';
		if(min_price == '')
		var allowedMinPrice = null;
		else
		var allowedMinPrice = min_price;
		if(max_price == '')
		var allowedMaxPrice = null;
		else
		var allowedMaxPrice = max_price;
		if(brandFilter == '')
		var allowedBrandFilter = null;
		else
		var allowedBrandFilter = brandFilter;
		if(genderFilter == '')
		var allowedGenderFilter = null;
		else
		var allowedGenderFilter = genderFilter;
		if(ageFilter == '')
		var allowedAgeFilter = null;
		else
		var allowedAgeFilter = ageFilter;
		if(htgiFilter == '')
		var allowedHtgiFilter = null;
		else
		var allowedHtgiFilter = htgiFilter;
		if(catFilter == '')
		var allowedCatFilter = null;
		else
		var allowedCatFilter = catFilter;
		if(charFilter == '')
		var allowedCharFilter = null;
		else
		var allowedCharFilter = charFilter;
		if(priceFilter == '')
		var allowedPriceFilter = null;
		else
		var allowedPriceFilter = priceFilter;
		HLLibrary.newRequest()
		.setTaxonomy("2255957")
		.setProperty("hlPageType","B")
		.setModule("172x3892_T-R-IG_TI_1-8_InGrid2")
		.setProperty("cUserId", allowedCUserId)
		.setProperty("pubPageType", "family")
		.setProperty("minOrganic ", "3")
		.setProperty("minAds", "1")
		.setProperty("maxAds ", "8")
		.setProperty("pCount", resCount)
		.setProperty("pgSize", pgSize)
		.setProperty("pgN", "1")
		.setProperty("sort", sortFilterHL)
        .setProperty("organicSKUs", "")
		.setProperty("minPrice",allowedMinPrice)
		.setProperty("maxPrice",allowedMaxPrice)
		.setProperty("minRating",null)
		.setProperty("brand",allowedBrandFilter)
		.setFilter("gender",allowedGenderFilter)
		.setFilter("age",allowedAgeFilter)
		.setFilter("howtogetit",allowedHtgiFilter)
		.setFilter("category",allowedCatFilter)
		.setFilter("Character/Theme",allowedCharFilter)
		.setFilter("price",allowedPriceFilter)
		.setAttribute("site", "bru")		
		.submit();
	}

</script>











