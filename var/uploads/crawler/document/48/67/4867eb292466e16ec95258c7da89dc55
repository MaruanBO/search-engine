<script type='text/javascript'>var date=new Date();var month=date.getMonth();var day=date.getDate();var dayOfWeek=date.getDay();var hours={start:new Date(date.getFullYear(),month,day),end:new Date(date.getFullYear(),month,day)};if(dayOfWeek>=1&&dayOfWeek<=5){hours.start.setHours(6);hours.end.setHours(20);}
else if(dayOfWeek==6){hours.start.setHours(8);hours.end.setHours(20);}
else{hours.start.setHours(9);hours.end.setHours(18);}
function updateCountDown(){var date=new Date();if(date.getHours()<hours.start.getHours()||date.getHours()>hours.end.getHours()){$('.attention').fadeOut();}
var hourDiff=hours.end.getHours()-date.getHours();var minDiff=59-date.getMinutes();if(minDiff<=9){minDiff=minDiff.toString();minDiff='0'+minDiff;}
var secDiff=59-date.getSeconds();if(secDiff<=9){secDiff=secDiff.toString();secDiff='0'+secDiff;}
$('span.hours').html(hourDiff);$('span.minutes').html(minDiff);$('span.seconds').html(secDiff);}
function setColumns(col){if($(window).width()>767){var cols=$('.desc');var highest=0;$.each(cols,function(){if($(this).height()>highest){highest=$(this).height();}});$.each(cols,function(){$(this).css('height',highest+'px');});}}
onloadCol=$('.desc').eq(0);$(window).resize(function(){var w=$(window).width();if(w>767&&$('nav:hidden'))
