	<script>
		
		$(document).ready(function () {
		
            ko.applyBindings(bag, $('#bagContainer')[0]);
            var cartData = {"itemsContainer":{"itemCount":0,"itemsQuantity":0}}
            if (cartData.itemsContainer.itemCount > 0){
            	bag.refresh(cartData.itemsContainer.subtotal, cartData.itemsContainer.items, cartData);
            
			 	for(i=0; i<cartData.itemsContainer.items.length; i++){		
				 	if(cartData.itemsContainer.items[i] && cartData.itemsContainer.items[i].commerceItemType == "STANDARD") {
						for(j=0;j<cartData.itemsContainer.items[i].properties.length;j++){
							if(cartData.itemsContainer.items[i].properties[j].name == "Size"){
								$(".bagProductContainer").eq(i).find(".size").append("<strong>Size:</strong> "+cartData.itemsContainer.items[i].properties[j].value);
							}
							if(cartData.itemsContainer.items[i].properties[j].name == "Color"){
								$(".bagProductContainer").eq(i).find(".color").append("<strong>Color:</strong> "+cartData.itemsContainer.items[i].properties[j].value);
							}
						}
			  		} 
				 	if(cartData.itemsContainer.items[i] && cartData.itemsContainer.items[i].commerceItemType == "eGiftCard"){
				 		$(".bagProductContainer").eq(i).find('a').eq(1).text("E-Gift Card");
		            }
				 	if(cartData.itemsContainer.items[i] && cartData.itemsContainer.items[i].commerceItemType == "physicalGiftCard"){
				 		$(".bagProductContainer").eq(i).find('a').eq(1).text("Gift Card");
		            }
				 	
				 	
            	}
            }        
            
            if (cartData.itemsContainer.itemCount <= 0){
            	$(".ItemContainer").css({"display":"none"});
            	$( ".itemNumber" ).append( "0" );
            }
            
            if (cartData.itemsContainer.itemCount == 1){
            	$(".ItemContainer").css({"height":"180px"});
            	$("#bagTitleContainer").html("Item in your bag");
            }
          
            $("#searchGlass").click(function(){
            	var inp = $("#searchBox");

            	if(inp.val() == "")
            	{
            		$("#serarchError").removeClass("hidden");
            		$("#serarchError").html("Please enter a search term");
            		
            		return false;
            	}
            	
            });
             $("#searchBox").focus(function(){
            	 $("#serarchError").html("&nbsp;");
             }).on('keydown',function(e){
                 
             });
            
           updateBagCount();
		
        });
		</script>
