<script language="JavaScript" type="text/javascript">
  var adjustCarouselWidth = function(element){
    var newWidth = window.innerWidth,curWidth = element.parentNode.offsetWidth,marginLeft;
    if(newWidth > 1140) {
      newWidth = newWidth <=1280 ? newWidth : 1280;
      marginLeft = (curWidth - newWidth)/2;
    } else {
      newWidth = curWidth;
      marginLeft = 0;
    }
    element.style.width = newWidth + "px";
    element.style.marginLeft = marginLeft+"px";
  }
  var carousel = document.getElementById('owl_carousel');
  adjustCarouselWidth(carousel);
  $(document).ready(function(){
    $(window).on('resize',function(){
      adjustCarouselWidth(carousel);  
    });    
  });  
  if(window.innerWidth < 768) {
      var shippingLabel = document.getElementById('shippingLabel');
      shippingLabel.parentNode.removeChild(shippingLabel);
      shippingLabel.classList.add('image-stretch');
      carousel.parentNode.insertBefore(shippingLabel,carousel);
  }
</script><div class="container">
