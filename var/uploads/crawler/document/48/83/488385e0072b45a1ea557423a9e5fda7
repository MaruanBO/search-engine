	<script type="text/javascript">
          var lpMTagConfig = { 'lpServer': "server.iad.liveperson.net", 'lpNumber': "55346166", 'lpProtocol': document.location.protocol.replace(":", "") };
          function lpAddMonitorTag(src) {
              if (typeof (src) == 'undefined' || typeof (src) == 'object') {
                  src = lpMTagConfig.lpMTagSrc ? lpMTagConfig.lpMTagSrc : '/hcp/html/mTag.js';
              }
              if (src.indexOf('http') != 0) {
                  src = lpMTagConfig.lpProtocol + "://" + lpMTagConfig.lpServer + src + '?site=' + lpMTagConfig.lpNumber;
              }
              else {
                  if (src.indexOf('site=') < 0) {
                      if (src.indexOf('?') < 0)
                          src = src + '?';
                      else
                          src = src + '&';
                      src = src + 'site=' + lpMTagConfig.lpNumber;
                  }
              };
              var s = document.createElement('script');
              s.setAttribute('type', 'text/javascript');
              s.setAttribute('charset', 'iso-8859-1');
              s.setAttribute('src', src);
              document.getElementsByTagName('head').item(0).appendChild(s);
          }
          if (window.attachEvent) window.attachEvent('onload', lpAddMonitorTag);
          else window.addEventListener("load", lpAddMonitorTag, false);
      </script>

<script type="text/javascript">

    var _riTrack=null;

    var _riAccountCode = '10054950';
    var _ricategoryName = '';
    var _riproductId = '';
    var _riCustomerId = '';
    var _ripageView='';

    var riTrackPage='true';
    var riBrandTrackPage='';
    var riCatTrackPage='';
    var riViewProduct='';
    var riTrackGiftCard='';
    var riTrackCartPurchase='';
    var rTag='';
    var mtw='';
    var prdId='';
    var _riCustomerId='';
    var trackCartCheckout='';
    var setCustomer='';


    if(riTrackPage == 'true') {
    	_ripageView='Home';
    }

    if(riBrandTrackPage == 'true') {
    	_ripageView='';
    	_ricategoryName='';
    }

    if(riCatTrackPage == 'true') {
    	_ripageView='';
    	_ricategoryName='';
    }

    if(riViewProduct == 'true') {
    	_riproductId = '';
    }

    if(riTrackGiftCard == 'true') {
    	_riproductId = '';
    }


    function _riInit() {
       /* Do not change the code within this block */
         _riTrack = responsys.WRS.init(_riAccountCode);


        if(rTag == 'true' && _riCustomerId !='' && setCustomer == 'true') {
         	if(_riTrack != undefined) _riTrack.setCustomer(_riCustomerId);
     	}
         
        if (_ripageView != '') {
       		if( _riTrack != undefined) _riTrack.trackPageView(_ripageView);
       	}

        if (_ricategoryName != '') {
       		if( _riTrack != undefined)  _riTrack.trackViewProductCategory(_ricategoryName);
       	}

        if (_riproductId != '') {
        	if(_riTrack != undefined)  _riTrack.trackViewProduct(_riproductId);
        }

        if(riTrackCartPurchase == 'true') {
        	if(_riTrack != undefined) _riTrack.trackCartPurchased(null);
        }

        if(rTag == 'true' && trackCartCheckout == 'true') {
        	if(_riTrack != undefined) _riTrack.trackCartCheckout(null);
    	}

        if(mtw == 'true' && prdId !='') {
        	if(_riTrack != undefined) _riTrack.trackRemoveProductFromCart(prdId);
    	}

   	};
</script>
