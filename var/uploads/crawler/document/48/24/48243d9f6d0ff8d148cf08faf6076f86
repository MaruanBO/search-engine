<script type="text/javascript">
$(document).ready(function() {
		 var indus = [];
          var base_url = window.location.protocol+"//"+window.location.host;
 			function comSep(arr){
 				var ret = "";
 					for(var i=0; i< indus.length; i++)
 					{
 						if(indus[i] == undefined )
 							continue;
 						ret+=indus[i].name+", ";
 					}
 						return ret;
 					}
	$("#industry").tokenInput(base_url+"/users/getIndustries", {
		searchDelay:0,
		minChars:2,
		tokenLimit:10,
		hintText:"Start typing to find an industry",
		noResultsText:"No industries match your search query",
		searchingText:"Searching available industries...",
		propertyToSearch:"name",
		//resultsFormatter:function(item){ return "<li>" + "<img src='" + item.url + "' title='" + item.first_name + " " + item.last_name + "' height='25px' width='25px' />" + "<div style='display: inline-block; padding-left: 10px;'><div class='full_name'>" + item.first_name + " " + item.last_name + "</div><div class='email'>" + item.email + "</div></div></li>" },
		resultsFormatter:function(item){
		 return "<li>" + "<div><div class='full_name'>" + item.name + "</div></div></li>"
		},
		tokenFormatter:function(item) {
			return "<li>" + item.name + "</li>"
			 },
		preventDuplicates:true,
		tokenValue:name,
		deleteText: "<i style='padding:3px;' class='fa fa-times-circle fa-lg'></i>",
	    onAdd: function (item) {
	    	//alert("Added " + item.name);
	    	indus.push(item);
 			$("#industry").val(comSep(indus));
	    },
	    onDelete: function (item) {
	    	//alert("Deleted " + item.name);
	    	var ind = indus.indexOf(item);
			if(ind != -1)
				delete indus[ind];
 			$("#industry").val(comSep(indus));

	    }
}); });

</script>
