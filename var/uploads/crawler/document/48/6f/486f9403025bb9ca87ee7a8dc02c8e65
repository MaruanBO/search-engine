            <script>
                function displayOptions(type)
                {
                    if(type =='paypal')
                    {
                        $("#bank_data").hide();
                        $("#payoneer_data").hide();
                        $("#paypal_data").fadeIn(500);
                        $("#mayzus_data").hide();
                    }
                    else if(type =='payoneer')
                    {
                        $("#bank_data").hide();
                        $("#paypal_data").hide();
                        $("#payoneer_data").fadeIn(500);
                        $("#mayzus_data").hide();
                    }
                    else if(type == 'wire')
                    {
                        $("#paypal_data").hide();
                        $("#payoneer_data").hide();
                        $("#bank_data").fadeIn(500);
                        $("#mayzus_data").hide();
                    }
                    else if(type == 'mayzus')
                    {
                        $("#paypal_data").hide();
                        $("#payoneer_data").hide();
                        $("#bank_data").hide();
                        $("#mayzus_data").fadeIn(500);
                    }
                }
                function change_bank_id(country)
                {
                    if(country == 'US')
                    {
                        $('#bank_id').html("ABA Routing # ");
                    }
                    if(country == 'CA')
                    {
                        $('#bank_id').html("Bank # ");
                    }
                    if(country != 'US' && country != 'CA')
                    {
                        $('#bank_id').html("Swift Code ");
                    }
                }
                
                function showLocation()
                {
                    var add1= document.getElementById('loc_address1');
                    var add2= document.getElementById('loc_address2');
                    var addcity= document.getElementById('loc_city');
                    var addprovince= document.getElementById('loc_prov');
                    var addcode= document.getElementById('loc_postal_code');
                    //var pmnt_type = signup_form.sel_pmnt_type.options[signup_form.sel_pmnt_type.selectedIndex].value;
                    var pmnt_type_ff = document.getElementById('sel_pmnt_type').value;
                    
                    if(pmnt_type_ff=='Mailed Cheques' )
                    {    
                        //check if location textboxes are empty
                        var loc_ad =  new Array();
                        loc_ad[0] = add1;
                        loc_ad[1] = add2;
                        loc_ad[2] = addcity;
                        loc_ad[3] = addprovince
                        loc_ad[4] = addcode;
                        for( var i=0; i<= loc_ad.length; i++)
                        {
                            if(trim(loc_ad[i].value).length < 1)
                            {
                                $("#message_location").fadeIn(500);
                                $(".req").fadeIn(500);
                                loc_ad[i].focus();
                                break;
                            }
                        }
                    }
                    else
                    {
                        $("#message_location").hide();
                        $(".req").hide();
                    }                        
                    
                }
                
                function trim(str)
                 {
                    str=str.replace(/^\s*/,"").replace(/\s*$/,"");
                    return str;
                 }
                 
                function promoteSite()
                {
                    var prom_lbl = document.getElementById('lbl-how-to-promote');                
                    var lbl_explain = document.getElementById('explain');
                    
                    if(document.signup_form.chk_nosite.checked == true)
                    {                            
                        $("#txta").fadeIn(500);          
                    }
                    else
                    {                           
                        $("#txta").hide();                                     
                    }
                }                
            </script>
