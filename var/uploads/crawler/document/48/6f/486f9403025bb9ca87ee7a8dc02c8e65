<script type="text/javascript">

$('#pay-wire').click(function(){
    $('.slider').animate({height:'1785px'}, 800);
});

$('#mayzus').click(function(){
    $('.slider').animate({height:'1785px'}, 800);
});

$('#pay-paypal').click(function(){
    $('.slider').animate({height:'1100px'}, 800);
});

function signup()
{   
    var email = $('#email').val();
    var password = $('#password').val();
    var confirm_password = $('#confirm_password').val();
    var first_name = $('#first_name').val();
    var last_name = $('#last_name').val();
    var phone1 = $('#phone1').val();
    var phone2 = $('#phone2').val();
    var payment_type = $("input[name='payment_type']:checked").val();
    var aim = $('#aim').val();
    var gmail = $('#gmail').val();
    var company = $('#company').val();
    var website = $('#website').val();
    var txta = $('#txta').val();
    var post_data = 'email='+email+'&password='+password+'&confirm_password='+confirm_password+'&first_name='+first_name+'&last_name='+last_name+'&phone1='+phone1+'&phone2='+phone2;
    post_data += '&payment_type='+payment_type+'&aim='+aim+'&gmail='+gmail+'&company='+company+'&website='+website+'&txta='+txta;
    
    if (payment_type == 'paypal')
    {
        var payment_username = $('#payment_username').val();
        post_data += '&payment_username='+payment_username;
    }
    else if (payment_type == 'payoneer')
    {
        var payment_payoneer = $('#payment_payoneer').val();
        post_data += '&payment_payoneer='+payment_payoneer;
    }
    else if (payment_type == 'wire')
    {
        var bank_name = $('#bank_name').val();
        var bank_address1 = $('#bank_address1').val();
        var bank_address2 = $('#bank_address2').val();
        var bank_city = $('#bank_city').val();
        var bank_state = $('#bank_state').val();
        var bank_zip_code = $('#bank_zip_code').val();
        var bank_country_code = $('#bank_country_code').val();
        var bank_swift = $('#bank_swift').val();
        var bank_account = $('#bank_account').val();
        var bank_routing = $('#bank_routing').val();
        var bank_details = $('#bank_details').val();
        
        var int_bank_name = $('#int_bank_name').val();
        var int_bank_address1 = $('#int_bank_address1').val();
        var int_bank_address2 = $('#int_bank_address2').val();
        var int_bank_city = $('#int_bank_city').val();
        var int_bank_state = $('#int_bank_state').val();
        var int_bank_zip_code = $('#int_bank_zip_code').val();
        var int_bank_country_code = $('#int_bank_country_code').val();
        var int_bank_swift = $('#int_bank_swift').val();
        var int_bank_account = $('#int_bank_account').val();
        var int_bank_routing = $('#int_bank_routing').val();

        var holder_name = $('#holder_name').val();
        var holder_address1 = $('#holder_address1').val();
        var holder_address2 = $('#holder_address2').val();
        var holder_city = $('#holder_city').val();
        var holder_state = $('#holder_state').val();
        var holder_zip_code = $('#holder_zip_code').val();
        var holder_country_code = $('#holder_country_code').val();
        
        post_data += '&bank_name='+bank_name+'&bank_address1='+bank_address1+'&bank_address2='+bank_address2+'&bank_city='+bank_city+'&bank_state='+bank_state+'&bank_zip_code='+bank_zip_code+'&bank_country_code='+bank_country_code;  
        post_data += '&bank_swift='+bank_swift+'&bank_account='+bank_account+'&bank_routing='+bank_routing+'&bank_details='+bank_details;
        post_data += '&holder_name='+holder_name+'&holder_address1='+holder_address1+'&holder_address2='+holder_address2+'&holder_city='+holder_city+'&holder_state='+holder_state+'&holder_zip_code='+holder_zip_code+'&holder_country_code='+holder_country_code;

        post_data += '&int_bank_name='+int_bank_name+'&int_bank_address1='+int_bank_address1+'&int_bank_address2='+int_bank_address2+'&int_bank_city='+int_bank_city+'&int_bank_state='+int_bank_state+'&int_bank_zip_code='+int_bank_zip_code+'&int_bank_country_code='+int_bank_country_code; 
        post_data += '&int_bank_swift='+int_bank_swift+'&int_bank_account='+int_bank_account+'&int_bank_routing='+int_bank_routing;
    }
    else if (payment_type == 'mayzus')
    {
        var mayzus_name = $('#mayzus_name').val();
        var mayzus_address1 = $('#mayzus_address1').val();
        var mayzus_address2 = $('#mayzus_address2').val();
        var mayzus_city = $('#mayzus_city').val();
        var mayzus_state = $('#mayzus_state').val();
        var mayzus_zip_code = $('#mayzus_zip_code').val();
        var mayzus_country_code = $('#mayzus_country_code').val();
        var mayzus_swift = $('#mayzus_swift').val();
        var mayzus_account = $('#mayzus_account').val();
        var mayzus_routing = $('#mayzus_routing').val();
        var mayzus_details = $('#mayzus_details').val();
        
        var int_mayzus_name = $('#int_mayzus_name').val();
        var int_mayzus_address1 = $('#int_mayzus_address1').val();
        var int_mayzus_address2 = $('#int_mayzus_address2').val();
        var int_mayzus_city = $('#int_mayzus_city').val();
        var int_mayzus_state = $('#int_mayzus_state').val();
        var int_mayzus_zip_code = $('#int_mayzus_zip_code').val();
        var int_mayzus_country_code = $('#int_mayzus_country_code').val();
        var int_mayzus_swift = $('#int_mayzus_swift').val();
        var int_mayzus_account = $('#int_mayzus_account').val();
        var int_mayzus_routing = $('#int_mayzus_routing').val();

        var mayzus_holder_name = $('#mayzus_holder_name').val();
        var mayzus_holder_address1 = $('#mayzus_holder_address1').val();
        var mayzus_holder_address2 = $('#mayzus_holder_address2').val();
        var mayzus_holder_city = $('#mayzus_holder_city').val();
        var mayzus_holder_state = $('#mayzus_holder_state').val();
        var mayzus_holder_zip_code = $('#mayzus_holder_zip_code').val();
        var mayzus_holder_country_code = $('#mayzus_holder_country_code').val();
        
        post_data += '&mayzus_name='+mayzus_name+'&mayzus_address1='+mayzus_address1+'&mayzus_address2='+mayzus_address2+'&mayzus_city='+mayzus_city+'&mayzus_state='+mayzus_state+'&mayzus_zip_code='+mayzus_zip_code+'&mayzus_country_code='+mayzus_country_code;  
        post_data += '&mayzus_swift='+mayzus_swift+'&mayzus_account='+mayzus_account+'&mayzus_routing='+mayzus_routing+'&mayzus_details='+mayzus_details;
        post_data += '&mayzus_holder_name='+mayzus_holder_name+'&mayzus_holder_address1='+mayzus_holder_address1+'&mayzus_holder_address2='+mayzus_holder_address2+'&mayzus_holder_city='+mayzus_holder_city+'&mayzus_holder_state='+mayzus_holder_state+'&mayzus_holder_zip_code='+mayzus_holder_zip_code+'&mayzus_holder_country_code='+mayzus_holder_country_code;

        post_data += '&int_mayzus_name='+int_mayzus_name+'&int_mayzus_address1='+int_mayzus_address1+'&int_mayzus_address2='+int_mayzus_address2+'&int_mayzus_city='+int_mayzus_city+'&int_mayzus_state='+int_mayzus_state+'&int_mayzus_zip_code='+int_mayzus_zip_code+'&int_mayzus_country_code='+int_mayzus_country_code; 
        post_data += '&int_mayzus_swift='+int_mayzus_swift+'&int_mayzus_account='+int_mayzus_account+'&int_mayzus_routing='+int_mayzus_routing;
    }

     var  invitation_code = $('#invitation_code').val();
      post_data +='&invitation_code='+invitation_code;
      var employee_id = $('#account_executives').val();

      if(employee_id != 'None') {
        post_data +='&employee_id='+employee_id;
      }
      
     
   $.ajax({
           type: "POST",
           url: "http://www.ad-center.com/front/signup",
           data: post_data,
           success: function(html){
              $("#signup form input").removeAttr("disabled").css("opacity","1"); 
              if(html.indexOf("THANK YOU FOR JOINING AD CENTER") != -1){
                $('#signup').html(html);
                $('.slider').animate({height:'750px'}, 800); 
              }
              else
                $('#signup .feedback').html(html);                                    
          }}); 
}
 function forgot_password()
 {
     var affiliate_email = $('#affiliate_email').val();
     $('#forgot .feedback').animate({opacity: '.2'},{duration: 500});
     $.ajax({
           type: "POST",
           url: "http://www.ad-center.com/front/forgot_password",
           data: "affiliate_email="+affiliate_email,
           success: function(html){
              setTimeout(function(){
                  $("#forgot form input").removeAttr("disabled").css("opacity","1"); 
              },500);
              $('#forgot .feedback').html(html);              
              $('#forgot .feedback ').animate({opacity: '1'},{duration: 500});                     
           }
           });
 }
 function contact_us()
 {
     $('#contact .feedback').animate({opacity: '.2'},{duration: 500});
     var c_name = $('#c_name').val();
     var c_company = $('#c_company').val();
     var c_email = $('#c_email').val();
     var c_message = $('#c_message').val();
     
     $.ajax({
           type: "POST",
           url: "http://www.ad-center.com/front/support",
           data: "c_name="+c_name+'&c_company='+c_company+'&c_email='+c_email+'&c_message='+c_message,
           success: function(html){
              $("#contact form input").removeAttr("disabled").css("opacity","1"); 
              $('#contact .feedback').html(html);              
              $('#contact .feedback').animate({opacity: '1'},{duration: 500});                     
           }
     });
 } 
 
 function login()
 {
     
     var username = $('.username').val();
     var password = $('.password').val();
     
     $.ajax({
           type: "POST",
           url: "http://www.ad-center.com/front/login",
           data: "username="+username+'&password='+password,
           success: function(html){
               
              if(html.indexOf("<meta") != -1) 
                window.location.href = "http://www.ad-center.com/front/login"+window.location.search;
              else
              {
                $("#loading_front").hide();
                $('.login_feedback').html(html).fadeIn(500);
              }
                                     
           }
     });
 }
</script><!-- FOOTER -->
