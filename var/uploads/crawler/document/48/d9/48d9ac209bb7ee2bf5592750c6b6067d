<script type="text/javascript">
        /* ger pid passed in through URL */
        function getQueryParam(parameterName) {
            var param = null;
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            param = vars[parameterName];
            return param;
        }
        
        var pdk_videoID = getQueryParam("pid");
        if ((pdk_videoID != "null") && (pdk_videoID != undefined) && (pdk_videoID != "")) {
             document.write('<link rel="alternate" href="http://link.theplatform.com/s/6eFMJC/' + pdk_videoID + '?feed=Outdoor Channel Previews Feed&player=Outdoor%20Channel%20-%20Single Video Player No Ads - &Affiliate=Outdoor%20Channel&AdService=AdsAdTechVast&MBR=True&format=SMIL&Tracking=true&Embedded=true" type="application/smil+xml" />');
        }
</script>
<script type="text/javascript">
    /* when media starts playing populates companion div*/
    function populateAdBanners(pdkEvent) {
        if (pdkEvent.data.baseClip.isAd) {
            for (var i = 0; i < pdkEvent.data.baseClip.banners.length; i++) {
                var bannerRegionName = pdkEvent.data.baseClip.banners[i].region;
                var bannerURL = pdkEvent.data.baseClip.banners[i].src;
                var bannerHREF = pdkEvent.data.baseClip.banners[i].href;
                if ((!(bannerURL == null || bannerURL.length == 0)) && (bannerRegionName == 'companion_left')) {
                    setTimeout(function () {
                        var companionDiv = getElement('companionDiv');
                        companionDiv.innerHTML = "<a href='" + bannerHREF + "' target='_blank'><img src='" + bannerURL + "'/></a>";
                        showCompanionDiv();
                    }, 1);
                }
            }
        }
    }
    function showCompanionDiv() {
        var companionDiv = getElement('companionDiv');
        var videoCompanionSpace = getElement('videoCompanionSpace');
        companionDiv.style.display = "block";
        videoCompanionSpace.style.display = "none";
    }
    function hideCompanionDiv() {
        var companionDiv = getElement('companionDiv');
        var videoCompanionSpace = getElement('videoCompanionSpace');
        if (!companionDiv) {
            // The div may not be ready at onLoad,
            // so we'll keep trying until it is.
            window.setTimeout(hideCompanionDiv, 50);
            return;
        } else {
            companionDiv.style.display = "none";
            videoCompanionSpace.style.display = "block";
        }
    }

    setupListener();
    function setupListener() {
        if (!$pdk.controller) {
            // The controller may not be ready at onLoad,
            // so we'll keep trying until it is.
            window.setTimeout(setupListener, 50);
            return;
        }
        $pdk.controller.addEventListener("OnMediaStart", "populateAdBanners");
        $pdk.controller.addEventListener("OnReleaseEnd", "hideCompanionDiv");

        $pdk.controller.addEventListener("OnGetReleaseStarted", "whichListener");
        $pdk.controller.addEventListener("OnLoadRelease", "whichListener");
        $pdk.controller.addEventListener("OnLoadReleaseUrl", "whichListener");
        $pdk.controller.addEventListener("OnLoadSmil", "whichListener");
        $pdk.controller.addEventListener("OnMediaComplete", "whichListener");
        $pdk.controller.addEventListener("OnMediaEnd", "whichListener");
        $pdk.controller.addEventListener("OnMediaLoadStart", "whichListener");
        $pdk.controller.addEventListener("OnMediaPause", "whichListener");
        $pdk.controller.addEventListener("OnMediaPlay", "whichListener");
        $pdk.controller.addEventListener("OnMediaStart", "whichListener");
        $pdk.controller.addEventListener("OnPlayerLoaded", "whichListener");
        $pdk.controller.addEventListener("OnReleaseEnd", "whichListener");
        $pdk.controller.addEventListener("OnReleaseStart", "whichListener");
        $pdk.controller.addEventListener("OnSetRelease", "whichListener");
        $pdk.controller.addEventListener("OnSetReleaseUrl", "whichListener");


    }
    function whichListener(pdkEvent) {
        var type = "Video";
        if (pdkEvent.data.baseClip) {
            if (pdkEvent.data.baseClip.isAd) {
                type = "Ad";
            }
        }
        _gaq.push(['_trackEvent', 'Videos', pdkEvent.type + "_" + type, window.location, 0, true]);
        //_gaq.push(['_trackEvent', 'Videos', 'Play', 'Baby\'s First Birthday']);
    }
    /* combines the pid with other needed info to pass to the player */
    function getReleaseURL(pid) {
        var releaseURLText = "http://link.theplatform.com/s/6eFMJC/" + pid + "?feed=Outdoor%20Channel%20Previews%20Feed&playall=false&autoPlay=false";
        //ad zone passed in from CMS
        releaseURLText += "&player=Single Video Player No Ads";
        //reports affiliate passed in from CMS
        releaseURLText += "&Affiliate=Outdoor%20Channel";
        //ad service passed in from CMS
        releaseURLText += "&AdService=AdsAdTechVast";
        releaseURLText += "&MBR=True&format=SMIL&Tracking=true&Embedded=true";
        
        return releaseURLText;
    }
    /* passes release url including pid and meta data to player */
    function setRelease(pid) {
        //$pdk.controller.pause(true);
        //$pdk.controler.autoplay(false);
        $pdk.controller.useDefaultPlayOverlay(true);
        $pdk.controller.setReleaseURL(getReleaseURL(pid));
        
    }
</script>
