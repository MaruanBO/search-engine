<script type="text/javascript">
    $(document).ready(function () {
        $(".localeChangeBox").change(function() {
            var origSearch = location.href.split("?")[1];

            if(origSearch != null && origSearch.length > 0){
                var search = origSearch.split("&");
                var newSearch = "";

                for(i = 0; i < search.length; ++i){
                    if(search[i].substring(0, 7) == "locale="){
                        newSearch += "locale=" + $(this).val();
                        if(i < search.length - 1)
                        	newSearch += '&';
                    } else if(search[i].length > 0) {
                        newSearch += search[i];
                        if(i < search.length - 1)
                        	newSearch += '&';
                    }
                }
                if(newSearch.indexOf("locale") == -1) {
                	newSearch += "&locale=" + $(this).val();
                }
                window.location = window.location.pathname + "?" + newSearch;
            } else {
                window.location = window.location.pathname + "?locale=" + $(this).val();
            }
        });
    });
</script>
