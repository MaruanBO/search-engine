{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Testing the banner</a>"; }*/ 
if (day >= 1 && day <= 5) { // Monday through Friday
if (currtime >= 500 && currtime <= 700) //5:00 - 7:00 m-far-left
{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Click here to watch NBC Montana Today</a>"; }
else if (currtime >= 1700 && currtime <= 1730) //today
{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Click here to watch News at 5</a>"; }
else if (currtime >= 1800 && currtime <= 1830) //noon
{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Click here to watch News at 6</a>"; }
else if (currtime >= 2200 && currtime <= 2230)
{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Click here to watch News at 10</a>"; }
}

if (day == 6 || day == 0) { //Saturday & Sunday
if (currtime >= 1700 && currtime <= 1730) //4:30 - 5:30 m-far-left
{ bannerAdd.innerHTML = "<a href='/keci-kcfw-live-stream/29417526'><span class='liveNow'>Live Now</span> Click here to watch News at 5</a>"; }
