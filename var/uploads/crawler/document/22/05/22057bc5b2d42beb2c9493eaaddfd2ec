        <div class="titlebar">
            <h1>
                Irresistible</h1>
        </div>
    <ul>
            <li class="first item-0">
                    <div class="teaser imageOnTop-true imageWidth-138" itemscope itemtype="http://schema.org/CreativeWork">
        <meta itemprop="url" content="http://www.nbcmontana.com/woman-catches-46-wedding-bouquets/31336912"/>
<figure  class="ib-figure-image" itemscope itemtype="http://www.schema.org/ImageObject" itemprop="associatedMedia">
        <meta itemprop="url" content="http://www.nbcmontana.com/wedding/91860"/>
