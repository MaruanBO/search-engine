		}		
	}
	function hideSignUp() {
		document.getElementById("fadeCover").style.display = "none";
		document.getElementById("signUp").style.display = "none";
	}   
	function getAjaxObjectTB() { 
        var objXMLHttp=null
            if (window.XMLHttpRequest) {
                objXMLHttp=new XMLHttpRequest()
            }
            else if (window.ActiveXObject) {
                objXMLHttp=new ActiveXObject("Microsoft.XMLHTTP")
            }
        return objXMLHttp
    } 
    
    function signup() {
        eml = document.getElementById("emailNews").value;
        mb = 0;
        if (eml == "") {
            alert ("Please enter your Email Address");
            return;
        }
        xmlHttp=getAjaxObjectTB()
        if (xmlHttp==null) {
            alert ("Your browser does not support HTTP Request, we are unable to process your your request at this time.\n\n" + 
            'To signup contact us at info@prodege.com.');                
            return;
        }
        url="/?cmd=gn-news-letter&cl=22&cp=1&mb=" + mb + "&eml=" + eml;
        xmlHttp.onreadystatechange=checkSignup;
        xmlHttp.open("GET",url,true);
        xmlHttp.send(null);
        hideSignUp();
    }
    function checkSignup() {
        if (xmlHttp.readyState==4 || xmlHttp.readyState=="complete") {
            result = xmlHttp.responseText.substr(0,1);
            if (result == 1) {
                alert ("Success!\n\n" + 
                'Your email address has been added.');                
            } 
            else if (result == 9) {
                alert ("Invalid email address!\n\n" + 
                'Please try again.');                
            }
            else if (result == "0") {
                alert ("Did not add!\n\n" + 
                'Email address already exists in our database.');                
            }
            else {
               alert("There was an unexpected error.\n\nPlease try again, or contact us at info@prodege.com");
            }
        }
    }
    function signUpEnter(evt) {
        evt = (evt) ? evt : event;
        var charCode = (evt.charCode) ? evt.charCode : ((evt.keyCode) ? evt.keyCode : ((evt.which) ? evt.which : 0));
        if(charCode == 13) {
            signup();
            return false;
        }  
    }  
