        <script>
            $(document).ready(function () {
                var path = $(location).attr('pathname');
                var URL = document.URL;
                var hashName = window.location.hash;
				var scrollhash = hashName;

                hashName = hashName.replace('#', '');
				scrollhash = scrollhash.replace('#', '');

                path = path.split("/");
                var strName = path[1];
                strName = strName.split(".");
				
				//if (hashName == "anchor") {
//					strName[0] = "clients";
//					hashName = "";
//				}

                if (hashName == "") {
                    if (strName[0] == "default") {
                        $('#mob_menu').val('home');
                    }
                    else if (strName[0] == "about") {
                        $('#mob_menu').val('about');
                    }
                    else if (strName[0] == "clients") {
                        $('#mob_menu').val('clients');
                    }
                    else if (strName[0] == "services") {
                        $('#mob_menu').val('services');
                    }
                    else if (strName[0] == "solutions") {
                        $('#mob_menu').val('solutions');
                    }
					else {
						$('#mob_menu').val('');
					}
                }
                else {
                    switch (hashName) {
                        case "contact":
                            $('#mob_menu').val('contact');
                            break;
                        case "ratings":
                            $('#mob_menu').val('services');
                            break;
                        case "status":
                            $('#mob_menu').val('services');
                            break;
						case "clients":
                            $('#mob_menu').val('clients');
                            break;
                    }
                }
            });

            function showIconDiv(thisDiv) {
                switch (thisDiv) {
                    case "rec1":

                }
            }
            function pageURL(pagelink) {

                switch (pagelink) {
                    case "home":
                        window.location = 'default.aspx';
                        break;
                    case "about":
                        window.location = 'about.aspx';
                        break;
                    case "clients":
                        window.location = 'clients.aspx';
                        break;
                    case "services":
                        window.location = 'services.aspx';
                        break;
                    case "solutions":
                        window.location = 'solutions.aspx';
                        break;
                    case "contact":
                        window.location = 'about.aspx#contact';
                        break;
                }
            }
		</script>
