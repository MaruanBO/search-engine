		<script>
			$(window).load(function () {
				$('#bg-maincontent').css('position', 'fixed');
				
				var hashName = window.location.hash;
				
				if (hashName == "#play") {
					showVid('open');	
				}
				
				if (hashName == "#clients") {
					window.location.href = "clients.aspx#clients";	
				}
				
				if (hashName == "#ratings") {
					window.location.href = "services.aspx#ratings";	
				}
				
				if (hashName == "#hrfiles") {
					window.location.href = "solutions.aspx#hrfiles";	
				}
				
				if (hashName == "#recruiting") {
					window.location.href = "solutions.aspx#recruiting";	
				}
				
				if (hashName == "#selection") {
					window.location.href = "solutions.aspx#selection";	
				}
			});
		
			function showVid(item) {
				if (item == "open") {
					$('#demo_vid').css('left', '0');
					$('#demo_vid').css('right', '0');
					$('#demo_vid').css('opacity', '1');
					$('#bg-maincontent').css('position', 'relative');
					$('#bg-main-content').css('overflow', 'scroll');	
					scrollHere('anchor');
				}
				else {
					$('#demo_vid').css('left', '-100%');
					$('#demo_vid').css('right', '100%');
					$('#demo_vid').css('opacity', '0');	
				}
			}
		</script>
		
        
