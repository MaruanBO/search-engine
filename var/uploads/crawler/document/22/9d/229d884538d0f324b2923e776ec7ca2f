		<script>
			resizeDiv();
		
			function resizeDiv() {
                vpw = $(window).width();
                vph = $(window).height();
				
				//$('.buffer').css({'margin-top': vph + 'px'});
				$('.buffer').css({ 'margin-top': vph *.90 + 'px' });
				$('.buffer_home').css({ 'margin-top': vph * .65 + 'px' });
                
                $('.left').css({ 'min-height': vph - 50 + 'px' });
                $('.center').css({ 'min-height': vph - 50 + 'px' });
                $('.right').css({ 'min-height': vph - 50 + 'px' });
				$('.dark').css({ 'min-height': vph - 50 + 'px' });
                $('.lastpage').css({ 'min-height': vph + 300 + 'px' });
				$('.endBuffer').css({ 'min-height': vph - 75 + 'px' });
				$('.error_background').css({ 'min-height': vph + 'px' });
				$('#bg-maincontent').css({ 'min-height': vph + 'px' });
				
				setWidth();
            }
			
			function setWidth() {
				var winWidth = ((parseInt($(window).width())));
				$('.photo').css('width', winWidth);
				$('.single').css('width', winWidth);
				$('.bar').css('width', winWidth);
				$('.wrapper').css('width', winWidth);
				
			}
		</script>
        
