		<script type="text/javascript">
            $(window).load(function () {
                var d=300;
                $('#navigation .drawer').each(function(){
                    $(this).stop().animate({
                        'marginBottom':'0px'
                    },d+=150);
                });
				
				$('#navigation > li').on('click',
                function () {
                    var myMargin = $('.drawer').css('marginBottom');
					
					  if (myMargin == "300px") {
						 $('.drawer',$(this)).stop().animate({
                        'marginBottom':'0px'
                    	},200); 
					  }
					  else {
					  $('.drawer',$(this)).stop().animate({
                        'marginBottom':'300px'
                    },200);
					  }
                }
				);
				
            });

        </script>
		
