<script>
    $(document).ready(function() {
		$('.submenu a[href*="'+location.pathname+'"]').css({'text-decoration': 'underline'});
        if ("" == 'pro 30 day trial') {
			$('.appnav .breadcrumbs').addClass('cut');
			//$('.sub-nav ul li.green a').html(calculcateTimeLeft("") + ' days left in trial');
		}else if("" == "suspended"){
			msg = new jsMiniMsg('_minimsgarea');
				msg.AddMsg('_new_reg'+new Date().getTime(), 'warning', 'Account is suspended , go to <a href="/cincopamanager/paypal/plans.aspx">plans page </a> to continue.');
                msg.Show();
		}
    });
    function calculcateTimeLeft(validUntil) {
        var Today = new Date();
        var TYear = Today.getFullYear();
		var TDay = new Date(validUntil);
        TDay.getFullYear(TYear);
        var DayCount = (TDay - Today) / (1000 * 60 * 60 * 24);
        DayCount = Math.round(DayCount);
        return(DayCount);
    }
	function BuyNow(){
		try{
		_gaq.push(['_trackEvent', 'counter buy now button', 'buy now', '30day pro']);
		_gaq.push(function(){ location.href = '/cincopamanager/paypal/plans.aspx' });
		}catch(ex){ location.href = '/cincopamanager/paypal/plans.aspx' ;  }
	}
</script>
