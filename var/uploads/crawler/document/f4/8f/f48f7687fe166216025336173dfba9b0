<script>
$('.action').bind('click', function(){
	var userInfo = '' ;
	if('False' == 'True' ){ // get user info if logged in 
		userInfo  = 'Email: ; ID: ;';
	}
	_gaq.push(['_trackEvent', 'feedback',  $(this).attr('data-action')+' '+userInfo]);
	$(".help").fadeOut(function(){
		$(".feedbackForm").fadeIn();
	})
});

$('.feedbackAction').bind('click', function(){
	if($(this).attr('data-action') == 'submit'){
		var userInfo = '' ;
		if('False' == 'True' ){ // get user info if logged in 
			userInfo  = 'Email: ; ID: ;';
		}
		_gaq.push(['_trackEvent', 'feedback', 'comment' , $('#feedbacktext').val()+' '+userInfo ]);
	}
	$(".feedbackForm").fadeOut(function(){
		$(this).html( "<span class='feedbackTitle'>Thank you.</span><span class='titleSecondRow'>We appreciate your feedback.</span>").fadeIn();
	});
});

if (location.pathname == "/" || location.pathname == "/Default.aspx")
	$(".wasthispagehelpful").hide();

