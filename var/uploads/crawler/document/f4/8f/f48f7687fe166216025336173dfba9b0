<!-- lucky orange control -->
<script>
	var cinco_startLO = false;
	if ( getCookie('lovideo1') == 'True' )
		cinco_startLO  = true; // this is a video user lets lo him
	else if ( getCookie('lovideo1') == 'False' )
		cinco_startLO  = false; // this is not a video user, no need to lo him
	else // new user - lets see if this is a video page and set cookie for a month to lo him (or not) in the future
	{
		if(checkVideoPage()) {
			setCookie('lovideo1', 'True',30); 
			cinco_startLO  = true;
		} else {
			setCookie('lovideo1', 'False',30); 
			cinco_startLO  = false;
		}
	}
	
	function checkVideoPage(){
	   if ( location.href.toLowerCase().indexOf('video') > -1 ) 
		   return true;
		else
		   return false;
	}
</script>


