<!-- redirect to plans page -->
<script>
	if("False"  == "True"){
		if ("" == 'pro 30 day trial' ||  "" == "suspended" ) {
			setCookie('trial_', 'start');	
			msg = new jsMiniMsg('_minimsgarea'); 
			var dayLeft = calculcateTimeLeft("");
			var storageUsage = Number('');
			var trafficUsage = Number('');
			
			var sizeText = '', msgPrefix='';
			if(trafficUsage >= 200* Math.pow(1024,2)){
				msgPrefix = '_trafficWarning';
				sizeText +='200MB traffic'
			}
			if(storageUsage >= 400* Math.pow(1024,2)){
				if(sizeText == ''){
					msgPrefix = '_storageWarning';
					sizeText = '400MB storage';
				}
				else {
					msgPrefix = '_trafficStorageWarning';
					sizeText = '400MB storage limit  / 200MB traffic limit';
				}
			}
			if(sizeText != ''){					
				warningText = 'Friendly Reminder: You have exceeded the '+sizeText+' of the FREE account.\
							You can continue using your PRO account for '+dayLeft+' more days or <a href="/cincopamanager/paypal/plans.aspx">UPGRADE right away to one of our premium plans</a>.'
				 msg.AddMsg(msgPrefix, 'warning' , warningText);
				 if(dayLeft > 3)
				   msg.Show();
			}
			if(dayLeft <= 3){
				if(dayLeft <= 0){
					msg.AddMsg('_datWarning', 'warning' , 'Your trial has ended, <a href="/cincopamanager/paypal/plans.aspx">check our other plans</a>');
				}else{
					msg.AddMsg('_datWarning', 'warning' , 'Your trial will end in '+dayLeft+' days and will be set to free plan, <a href="/cincopamanager/paypal/plans.aspx">check our other plans</a>');
				}			 
			 msg.Show();	
			}
			 if( ("" =="suspended" || dayLeft <= 5) && getCookie('trialPage') != 'True'){
				
			 	setCookie('trialPage', 'True',10); 
				location.href =  '/cincopamanager/paypal/plans.aspx';
			 }
		}
	}else{
		setCookie('trialPage', 'False',10); 
	}
</script>


