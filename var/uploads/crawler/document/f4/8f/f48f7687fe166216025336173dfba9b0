	function cincomLoad(p) {
		$.ajax({
			url: "//analytics.orenshmu.com/ou.aspx",
			method: "POST",
			cache: false,
			dataType: 'json',
			data: "t=l&d=" + JSON.stringify(p)
		});
	}

	function cincomEvent(n, e, p) {
		$.ajax({
			url: "//analytics.orenshmu.com/ou.aspx",
			method: "POST",
			cache: false,
			dataType: 'json',
			data: "t=e&n="+n+"&d="+JSON.stringify(p)+"&e="+JSON.stringify(e)
		});
	}
	if (window.cincomSettings.email == "") window.cincomSettings.email = "notlogged";
	cincomLoad(window.cincomSettings);
</script>

