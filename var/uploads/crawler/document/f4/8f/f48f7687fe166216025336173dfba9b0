    <!-- fix for hover on droid -->
	<script>
	 $(document).ready(function(){
		$('.sub-nav ul  li').bind('touchstart',function(e){
			if(!$(this).hasClass('hover')){
				$('.sub-nav ul  li.hover').removeClass('hover');
				$(this).addClass('hover');	
				if( $(this).find('.pointer').length > 0){
					e.preventDefault();
					e.stopPropagation();
					return false;				
				}else{
				  location.href= $(this).find('a').attr('href');
				}
			}else{
				location.href= $(this).children('a').attr('href');
			}	
		});
		 
	}); 
</script>
