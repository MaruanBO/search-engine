		var __user_level = "error";
		if("False"  == "True" && __user_level == 'suspended' && location.pathname !='/suspended' ){
			location.href = '/suspended';
		}
		var __edit_mode = false;
		if("False"  == "True"){
			$('body').addClass(__user_level);
			if ("" == "mcc"){
				$('.header .row1').append('<div class="sessionBox"><span>mcc: </span></div>');
			}
			if(__user_level == "owner" || __user_level =="admin" || __user_level =="editor" ){
