<script type="text/javascript">
    // A/B Testing
    var abTestingLoader = function(testName) {
        var random        = Math.random(),
            carouselDelay;

        var abCreateCookie = function (name,value,days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime()+(days*24*60*60*1000));
                var expires = '; expires='+date.toGMTString();
            }
            else var expires = '';
            document.cookie = name+'='+value+expires+'; path=/';
        };

        var abReadCookie = function (name) {
            var nameEQ = name + '=';
            var ca = document.cookie.split(';');
            for(var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1,c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
            }
            return null;
        };
        var abEraseCookie = function (name) {
            self.createCookie(name,'',-1);
        };

        var abTestingCookie = function(testId) {
            setTimeout(function() {
                abCreateCookie('mkABTesting'+testId, 1, 365);
            },500);
        };

        var testOption = function(testName, testId, var14Value) {
            try {
                MAKERS_CONFIG.eVar14 = var14Value;
                MAKERS_CONFIG.cobrand = testId;
            } catch(e) {
            }
        }

        if (abReadCookie('mkABTesting0')) {
            testOption(testName, 'control', 'makers|dlrot|control');
            MAKERS_CONFIG.carouselDelay = 5;
        } else if (abReadCookie('mkABTesting1')) {
            testOption(testName, '8sec', 'makers|dlrot|8sec');
            MAKERS_CONFIG.carouselDelay = 8;
