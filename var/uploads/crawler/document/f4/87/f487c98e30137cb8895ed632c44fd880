    $(document).ready(function () {
        //$(".gobtn").attr("onclick",$(".gobtn").attr("onclick").replace("/Search","/Search.aspx?search_sumbitted=yes"));
        /*var currOnclick = document.getElementById($(".gobtn").attr("id")).getAttribute("onclick");
        currOnclick = currOnclick.replace("/Search", "/Search.aspx?search_sumbitted=yes");
        document.getElementById($(".gobtn").attr("id")).setAttribute("onclick", currOnclick);*/
    });
    window.GotoSearchUrl = function GotoSearchUrl(searchUrl, value) {
        // check to see if a value, in this case value may contain a url, so that gets priority
        if (true != LOCATION_SET) {
            //if (null != value && 'undefined' != value && "" != value) {
            //    window.location.href = value;
            //}
            //else {
                searchUrl = searchUrl.replace(/\/\/\/search/i, '/search');
                searchUrl = searchUrl.replace(/\/\/search/i, '/99999/search');
                searchUrl = searchUrl.replace(/\/\/(\w+)\/search/i, '/$1/search');
                window.location.href = searchUrl + "?search_submitted=yes&kw=" + $(".search").val() + "&?ab=Header_Search";
            //}
            //LOCATION_SET = true;
        }
    }
</script>

