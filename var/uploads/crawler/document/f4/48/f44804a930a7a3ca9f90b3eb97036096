<script type="text/javascript">
    // Global Unique User ID, was used for preventing pubnub from acting on itself
    // Also feel free to use for other purposes to identify all clients
    var UUID = "";

    function generateUUID() {
        var S4 = function() {
           return (((1+Math.random())*0x10000)|0).toString(16).substring(1);
        };
        return (S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4());
    }
    
    UUID = generateUUID();
    
    if(typeof HM === 'undefined'){
        HM = {};
    }
    HM.UUID = UUID;
</script>


