 function subscribeNewsLetter() {
    var url = '/version2.0.5/inc_newsletter_signup.cfm?inc=1';

    source = urlParam('source');
    if (source != null) {
        url = url + '&source=' + source;

        subsource = urlParam('subsource');
        if (subsource != null) {
            url = url + '&subsource=' + subsource;
        }
    }

    tb.jQuery.facebox({ajax: url});

setTimeout(function(){
var subscribeDiv = tb.jQuery('.signupLayout');
subscribeDiv.attr('id', 'signupLayout');
//console.log(tb.jQuery('#signupLayout'));
}, 2000);
}

