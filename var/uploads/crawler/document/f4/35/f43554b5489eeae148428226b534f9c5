 		 			function homepageTruncate() { //console.log('findAndReduce started');
  					var articleExpandable = tb.jQuery('.expandable:not(.completed)'); //console.log(articleExpandable.length + ' found to reduce');
  						articleExpandable.each(function() {
  						var preview = $(this).find('.article_preview'), new_id = randomInt(1000, 10000);
  								if (preview.length) {
  									var preview_node = preview.clone();
  									$(this).addClass('full_' + new_id);
  									preview_node.addClass('partial_' + new_id);
  									$(this).before(preview_node);
  									$('.full_' + new_id).hide(); //var mybutton = $('#js_more_' + new_id);
