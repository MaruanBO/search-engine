  									var my_id = dealID.text();
  									$('.partial_' + new_id).append('<div id="js_more_' + new_id + '" class="article-read-more" onclick="showPopNewsDisplayItem(\'' + my_id + '\', \'f_news\')"></div>');
  							//mybutton.on("click", function() {setShowMore(new_id);showPopNewsDisplayItem(my_id, 'f_news');});
  							        $(this).addClass('completed truncated');
  								} else {
  									$(this).addClass('completed didnottruncate');
  								}
  								/* else { // No Preview
