<script type="text/javascript">

if (!window.console) console = {log: function() {}}; 

function showCouponPopup(couponId) {
	
	var newWindowPopup=window.open('/inc_coupon_popup.cfm/'+couponId,'name','height=400,width=496');
     newWindowPopup.focus();
}

function revealCoupon( couponID, linkType, myUrl, aId, params, windowType ) {

	if (navigator.userAgent.toLowerCase().indexOf("firefox")>=0 ) {
		openReferrer( linkType, myUrl, aId, params, windowType);
		showCouponPopup(couponID);
		unWrapCouponCode(couponID);
		return false;
	}
	else if (navigator.userAgent.toLowerCase().indexOf("chrome")>=0) {
		openReferrer( linkType, myUrl, aId, params, 'current');
		showCouponPopup(couponID);
		unWrapCouponCode(couponID);
		return true;
	}
	else {
		openReferrer( linkType, myUrl, aId, params, 'current');
		showCouponPopup(couponID);
		unWrapCouponCode(couponID);
		return false;
	}
		
}

function openReferrer(linkType,myUrl,aId,params,windowType){

	
	
	var url="";
	if(linkType =="cjump"){
		url="/cjump.cfm?"+myUrl;
	}else if(linkType == "jump"){
		url="/jump.cfm?"+myUrl;
	}else{
		url=myUrl;
	}
	url=url+params;

	if(windowType == 'new'){
		var newWindow= window.open(url,'_blank');
		newWindow.focus();
	}else if(windowType == 'current'){
		document.location=url;
	}else if(windowType == 'blank'){
		var newWindow= window.open(url,'_blank');
		newWindow.focus();
	}

	return false;
}

function unPeelCouponCodeV2(myid) {

	var peel = tb.jQuery(myid);
	peel.find('.codeSpan').attr('style','display: block;');
	peel.find(".codePrefixSpan").hide();
	peel.find(".couponcodewrapper").hide();
	
}

function unPeelThisCouponCode(myid) {

	var n = myid.split("_");
	var back = n[1];
	var peel = tb.jQuery('#couponinc_' + back);
	if(peel.length == 0) {
		peel = tb.jQuery('#coupon_' + back);
	}
	if(peel.length != 0) {
		var myclass = peel.attr('class');
		peel.find('.codeSpan').attr('style','display: block;');
		peel.find(".codePrefixSpan").hide();
		peel.find(".couponcodewrapper").hide();
		unPeelOtherCoupons(myclass);
	}
	
}

function unPeelOtherCoupons(mystore_id) {
	//<!--
	var storeClasses = mystore_id.split(" ");
	var storeClass;
	for(var i = 0; i<storeClasses.length; i++) {
		if(storeClasses[i].indexOf("store_") !=-1){
			storeClass = storeClasses[i];
		}
	}
	var otherCoupons = tb.jQuery('.' + storeClass);
	otherCoupons.each(function() {

		$(this).find('.codeSpan').attr('style','display: block;');
		$(this).find(".codePrefixSpan").hide();
		$(this).find(".couponcodewrapper").hide();

	});
	// -->
}

function clickCouponCodeV3(code, store_id){
	var myid = '#' + code;
	unPeelCouponCodeV2(myid);
	unPeelOtherCoupons(store_id);
}

function unWrapCouponCode(code){
	var myid = '#couponinc_' + code;
	unPeelThisCouponCode(myid);
}


</script>

		</div>
		</div>

	<script type="text/javascript">
	var scrollingIdsArray = new Array('4','251','203','15','124','14','63','142','13','252','');

	scrollingIdsArray.pop();

	var prevScrollingSelectedDivIndex = 0;
	var currentScrollingSelectedDivIndex = 2;
	//var currentScrollingSelectedDivIndex = Math.floor(Math.random() * scrollingIdsArray.length) + 1;
	var isScrollingAjaxRequestUnderProcess = false, xmlhttp;
	
	var currentDivId = 'tab-' + currentScrollingSelectedDivIndex;

	tb.jQuery('#topDealsByPC').ready(function(){

		var currentDiv = document.getElementById(currentDivId);

		if (currentDiv != null) {currentDiv.className += " selected";}

		else {
			console.log(currentDivId);
			var setDefaultTabCounter = 0,
			setDefaultTab =  setInterval(function(){
				setDefaultTabCounter++;
				if (setDefaultTabCounter > 10) {clearInterval(setDefaultTab)}
				var currentDiv2 = document.getElementById(currentDivId);
				if (currentDiv2 != null) {
				currentDiv2.className += " selected";
				clearInterval(setDefaultTab);
				}
			}, 500);

		}
		
	});
	
	function getAjaxScrollingDealsById(position, itemId, numberOfElementstoShow, event) {

  	//console.log(position, divIdClassName, itemId, numberOfElementstoShow, event);

	if(isScrollingAjaxRequestUnderProcess) {	//	An ajax request is currently being processed		
		return;
	}

	if(position == currentScrollingSelectedDivIndex) {		// they clicked on the currently selected tab		
		return;
	}

	loadAjaxDiv(event);

	isScrollingAjaxRequestUnderProcess = true;

	prevScrollingSelectedDivIndex = currentScrollingSelectedDivIndex;

	currentScrollingSelectedDivIndex = position;

	var divIdIndex = position - 1; // zero based array

	if (window.XMLHttpRequest) {
		xmlhttp=new XMLHttpRequest();
	} else if (window.ActiveXObject) {
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	} else {
		alert("Your browser does not support XMLHTTP!");
	}

	xmlhttp.onreadystatechange = function() {
		updateDealScrollingIdsAjaxCallback(numberOfElementstoShow);
	}

	var this_id = parseInt(scrollingIdsArray[divIdIndex]);

	xmlhttp.open("GET","/deal/dealScrolling?categoryId=" + this_id, true);

  	console.log("/deal/dealScrolling?categoryId=" + this_id );

	xmlhttp.send(null);

	}	

	function updateDealScrollingIdsAjaxCallback(numberOfElementstoShow) {
	if(xmlhttp.readyState == 4) {
		hideAjaxDiv();
		if (xmlhttp.status == 200) {
			if (versionNumber == 2 ){	
				  deals = xmlhttp.responseText;
			} else {
				  deals.splice(0, deals.length);
				  deals = xmlhttp.responseText.split("<a></a>");
			}
			startIndex = 0;
			showElements(0, getEndIndex(0));
			showHideDivIdScrolling();
		} else {
			console.error('There was an error processing your request. Please try again');
		}
		isScrollingAjaxRequestUnderProcess = false;
		carouselScroll(2, numberOfElementstoShow);
		}
	}

	function showHideDivIdScrolling() {
		var previousTab = document.getElementById('tab-' + prevScrollingSelectedDivIndex)
		var currentTab = document.getElementById('tab-' + currentScrollingSelectedDivIndex);
		
		if (previousTab != null) {
			previousTab.style.cursor = "pointer";
			previousTab.className = "m" + prevScrollingSelectedDivIndex;
		} // else { console.log(previousTab); }
		
		if (currentTab != null) {
		currentTab.style.cursor = "text";
		currentTab.className = "m" + currentScrollingSelectedDivIndex + " selected";
		} // else { console.log(currentTab); }

	}

	function showHideDivScrolling() {
			var prevObj = document.getElementById(scrollingDealIdsArray[prevScrollingSelectedDivIndex]);
			console.log(prevObj);
			var currentObj = document.getElementById(scrollingDealIdsArray[currentScrollingSelectedDivIndex]);
			console.log(currentObj);
			prevObj.className = "m" + prevScrollingSelectedDivIndex;
			currentObj.className = "m" + currentScrollingSelectedDivIndex + " selected";
			prevObj.style.cursor = "pointer";
			currentObj.style.cursor = "text";
	}
		carouselScroll(2, 4);
		//console.log('carouselScroll(2, 4), currentScrollingSelectedDivIndex = 1');
		function getScrollingCoupons (divIdIndex, event, numberOfElementstoShow) {

			if(isScrollingAjaxRequestUnderProcess){
			return;
			}

			if(divIdIndex == currentScrollingSelectedDivIndex) {
			return;
			}

			prevScrollingSelectedDivIndex = currentScrollingSelectedDivIndex;
			currentScrollingSelectedDivIndex = divIdIndex;

			var coupontiles = tb.jQuery('#coupontiles');

			var tiles = coupontiles.find('ul').clone();

			//console.log(tiles);

			var carousel = tb.jQuery('.caroufredsel_wrapper');

			//console.log(carousel);

			var currenttiles = carousel.find('ul');

			//console.log(currenttiles);

			currenttiles.remove();

			carousel.prepend(tiles);

			showHideDivScrolling();

			carouselScrollFour(2, numberOfElementstoShow);

		}

		function carouselScrollFour(i, j){
		tb.jQuery(document).ready(function($) {
		categoryElement = '#category'+i;
		prevElement = '#prev'+i;
		nextElement = '#next'+i;

	   $(categoryElement).carouFredSel({
	       items: { visible: j },
	       prev: prevElement,
	       next: nextElement,
	       circular: false,
	       auto: false,
	       height: 'auto',
	       scroll: {items: 4} });
    	});
		}

	</script>

	<script language="JavaScript">
		var isScrollingDealExpanded = 'true';
		//displayScrollingDivSection(isScrollingDealExpanded);
		showHideDivIdScrolling();
	</script>

	<script language="JavaScript">

	    versionNumber = '2.0.5';

	</script>
