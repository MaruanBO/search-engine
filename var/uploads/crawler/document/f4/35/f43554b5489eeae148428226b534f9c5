  									$(this).prepend(pn);
  									var preview_node = $(this).find('.article_preview')
  								} */
  						});
					}

					function randomInt(min,max){return Math.floor(Math.random()*(max-(min+1))+(min+1));}

					function findToTruncate() {
						homepageTruncate();
					}

 	 function populateLastHomeVisitTime(result) {
 		homeVisitTime = Date.parse(result.time);
 		lastHomeVisitTimeUpdated = true;
 		updateNewDeal();

  	 }

  	 function updateLastHomeVisitTime(userId) {
  		var scriptTag = document.createElement('script');
 		scriptTag.type = 'text/javascript';
 		scriptTag.src = "http://int.api.techbargains.us.com/last_time/" + userId + "?callback=populateLastHomeVisitTime";
 		document.body.appendChild(scriptTag);
  	 }

  	function showHideExpireDeals(value) {
		toggleShowExpiredDeals(value);
		updateShowExpiredDealsCss(value);
	}

