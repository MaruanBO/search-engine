<script>
var _tbLastScrollPosition;
var _tbIdleMinutes = 60;
var _tbIdleCheck;
function reloadIdle() {
  var currentPosition = jQuery(window).scrollTop();
  if (_tbLastScrollPosition=== undefined || currentPosition != _tbLastScrollPosition) {
    _tbLastScrollPosition = currentPosition;
  }
  else {
    window.scrollTo(0,0);
    location.reload();
  }

  _tbIdleCheck = setTimeout("reloadIdle();", _tbIdleMinutes * 60000);
}
jQuery(document).ready(reloadIdle);
</script>

