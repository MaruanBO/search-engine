function shareOnGPlus(dealId, isCoupon){
    var url = 'https://plus.google.com/share?url=';
    if(isCoupon){
        url = url + encodeURI(domainUrlForShare + '/coupon/') + dealId + encodeURI('?source=gplusshare');
    }else {
        url = url + encodeURI(domainUrlForShare + '/news_displayItem.cfm/') + dealId + encodeURI('?source=gplusshare');
    }
    var tbgp = window.open( url, 'TB_'+dealId);
    if(tbgp){
        tbgp.focus();
    }
}
</script>

