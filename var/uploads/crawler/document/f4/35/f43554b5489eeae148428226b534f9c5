<script>
//var currentLUTimeStamp = tb.jQuery('#lastUpdatedTime').find('div').text();
//console.log('currentLastUpdatedTimeStamp: ' + currentLUTimeStamp);
var last_n, newDealsCount = 0, newDealDate = new Date(), originalTimeStamp = newDealDate.getTime(), newDealsValue = 0, checkForNewDeals = null;
window._tb_newDealPass = 0;
window._tb_newDealCount = 0;

function queryNewDeals() {
//console.log('queryNewDeals ' + newDealsCount + ' :' + currentLastUpdatedTimeStamp);
  if ("currentLastUpdatedTimeStamp" in window) {
    if ("currentLastUpdatedTimeStamp_" in window) {
      //console.log('currentLastUpdatedTimeStamp_ ' + currentLastUpdatedTimeStamp_);
    } else {
    currentLastUpdatedTimeStamp_ = currentLastUpdatedTimeStamp;
    //console.log('created currentLastUpdatedTimeStamp_ ' + currentLastUpdatedTimeStamp_);
  }
var roundedLastUpdatedTimeStamp = Math.floor(currentLastUpdatedTimeStamp_ / 6000) * 6000;
window._tb_newDealPass = window._tb_newDealPass + 1;
//var newDealQuery='/deal/newDealsAvailableCount?date=' + currentLastUpdatedTimeStamp_;
var newDealQuery='/deal/newDealsAvailableCount?date=' + roundedLastUpdatedTimeStamp + '&pass=' + window._tb_newDealPass;
//console.log(newDealQuery);
  var jqxhr = $.ajax({url:newDealQuery})
  .done(function(data) {
    //console.log('New Deals is ' + data.count + ', Count is ' + newDealsCount);
//console.log(data);
    if (data.count > 0) { // || newDealsCount % 10 == 0
	  // Skip doing anything if the new deal count is the same as what we have already seen.
	  if (data.count == window._tb_newDealCount) {
	    //console.log('new deal count is the same: ' + data.count);
	  }
	  else {
	    // Remove the div if it already exists.
	    tb.jQuery('#newDealsAdded').remove();
	    var count_plural = (data.count == 1 ? "" : "s");
	    var newDealsAddedDiv = tb.jQuery('<div id="newDealsAdded" class="tbbkgradient" style="position: fixed; width: 180px; height: 40px; z-index: 1001; left: 50%; top: 10%; text-align: center; color: #fff; font-weight: 700; margin-left: -75px; margin-top: -25px;  border: 1px solid #fff; border-radius: 9px; padding-top: 5px; cursor: pointer;"> ' + data.count + ' New Deal' + count_plural + ' Added<br><a onClick="newDealsRefresh();" style="text-decoration: underline; color: #fff" class="newDealRefresh">REFRESH</a><div id="closeNewDealAlert" style="background: url(http://cdn1.techbargains.us.com/tbimageweb/version2/images/close-button.png) left top no-repeat; width: 15px; height: 15px; background-size: 99%; position: absolute; top: 1px; right: 1px;" onClick="removeNewDealsAdded();"></div></div>');

	    tb.jQuery('body').append(newDealsAddedDiv);
	  }
    }
  })
  .fail(function() {
    //console.error();
  })
  .always(function() {
    //last_n = n;
  });
  }
}

function removeNewDealsAdded() {

tb.jQuery('#newDealsAdded').remove();
// We do not want to update the TS when the alert is closed since it will just mess with caching and its ok to say "6 new deals" when there
// are 6 new deals, even if there were 5 when the alert was closed.
//updateCurrentLastUpdatedTS();
}

function newDealsRefresh () {
tb.jQuery('html, body').animate({
scrollTop: tb.jQuery('.homepage-deals').offset().top
}, 600, function() {
     tb.jQuery('.newDealRefresh').text('RELOADING');
     window.location.reload();
});
}

function updateCurrentLastUpdatedTS () {
var currentNewDealDate = new Date(), currentTimeStamp = currentNewDealDate.getTime();
var timeElapsed = (currentTimeStamp - originalTimeStamp);
if ("currentLastUpdatedTimeStamp_" in window) {
  //console.log('last:', currentLastUpdatedTimeStamp_);
  currentLastUpdatedTimeStamp_ = Number(currentLastUpdatedTimeStamp_) + Number(timeElapsed);
  //console.log('current:', currentLastUpdatedTimeStamp_);
}

}

function setCheckForNewDeals() {

checkForNewDeals = setInterval(function () {
  queryNewDeals();
  newDealsCount++;
  //console.log('New Deals Count: ' + newDealsCount);
    if (newDealsCount > 1000) {
        clearInterval(checkForNewDeals);
    }
}, 60000);

}

if (document.location.pathname == '/') {
  setCheckForNewDeals();
  //console.log('setIntervalCheckForNewDeals executed');
}

