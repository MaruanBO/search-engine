<script type="text/javascript">
if (window.tb && window.tb.jQuery && window._tb_expired_str && window.dealId && window._tb_expired_str[window.dealId]) {
  var sse = Date.parse(window._tb_expired_str[window.dealId]) / 1000;

  var ajax_url = 'http://int.api.techbargains.us.com/expired_deal/hit/' + window.dealId + '?expired_at=' + sse + '&title=' + encodeURIComponent(document.title);

  tb.jQuery.ajax({
    type : "GET",
    url: ajax_url,
	data: "",
	async: true,
	success: function (data, textStatus, jqXHR) {
      //console.log('success callback for expired hit');
    }
  });
}

</script>
<script>
function showPopNewsDisplayItem(dealId, cname) {

  var site = '';
	if(document.location.hostname.indexOf('planetbargains.com') < 0){
    site = 'pb'
  }

  if (cname) {
    tb.jQuery.facebox({ajax: '/pop_news_displayItem' + site + '.cfm/' + dealId}, cname);
    //console.log('/pop_news_displayItem' + site + '.cfm/' + dealId + ', ' + cname);
  }
  else {
    tb.jQuery.facebox({ajax: '/pop_news_displayItem' + site + '.cfm/' + dealId});
  }

var $ga_ajaxed_deal =  tb.jQuery('#facebox');
                    setTimeout(function() {
                    runDealGAEvents($ga_ajaxed_deal, 'Homepage');
                    }, 5000);

}
</script>
