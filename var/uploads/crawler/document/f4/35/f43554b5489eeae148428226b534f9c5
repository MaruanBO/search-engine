	<!--
	function doBodyOnLoadTasks() {
			// Lazy load images
			lazyLoadImages("img.lazy");

			// Show popup based on request param value
			var paramValue = getParameterByName('popup');

			showPopUpForValue(paramValue);
		}

		function showPopUpForValue(paramValue) {
			if(paramValue == 'newsletter') {
				subscribeNewsLetter();
			} else if (paramValue == 'contactus') {
				showCommentDiv(0, null, 'contact');
			} else if(paramValue == 'dealalert') {
				showAlertDiv(null);
			}
		}

