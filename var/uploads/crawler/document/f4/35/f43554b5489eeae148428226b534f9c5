<script>
function showMoreStores()
{
	if($('#moreStores').hasClass('is_hidden'))
	{
		$('#moreStores').removeClass('is_hidden');
		$("#mL").html("Less");
		//tb.jQuery(document).scrollTop( tb.jQuery("#tA").offset().top );
	} else {
		$('#moreStores').addClass('is_hidden');
		$("#mL").html("More");
		//tb.jQuery(document).scrollTop( tb.jQuery("#lA").offset().top );
	}
}

tb.jQuery(document).ready(function($) {

	$('.popular-stores').css('visibility', 'visible');
	
});

</script>
