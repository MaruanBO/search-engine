function gaDeals() {
    var vddeals = $('#ajaxDeals');
    var vd_deals_list = vddeals.find('.CBoxTDealStyling');
    vd_deals_list.each(function() {
        var deal_title = $(this).find('.upperrightSort a').text().replace(/(\r\n|\n|\r|')/gm,"");
        var deal_links = $(this).find('.contentrightSort a');
        deal_links.each(function () {
            var link_onclick = $(this).attr('onclick');
            if (link_onclick == undefined) {link_onclick = '';}
            $(this).attr('onclick', link_onclick + '_gaq.push([\'_trackEvent\', \'Deal\', \'Clickout\', \'' + location.href + ': ' + deal_title + '\']);');
        });
    });
}
if (location.href.indexOf('/stores/') > -1 ) {
var $store_deals = $('.generalNews');
if ($store_deals.length) {
runDealGAEvents($store_deals, 'Deal');
}
}
if ((location.href.indexOf('/catsearch.cfm/') > -1) || (location.href.indexOf('/laptopcomputernotebookreview.cfm') > -1) ) {
var $catdeals = $('#generalNews');
if ($catdeals.length) {
runDealGAEvents($catdeals, 'Deal');
}
}

setTimeout(function() {findCTA();},6000);
function findCTA() {
var cta_thumbs = $('.thumbnailPriceColumn.ctaadded');
if (cta_thumbs.length) {
cta_thumbs.each(function() {
    var cta_title = $(this).parent().find('.upperrightSort a').text().replace(/(\r\n|\n|\r|')/gm,"");
    var cta_link = $(this).find('a.btn_get_deal');
    cta_link.attr('onclick', '_gaq.push([\'_trackEvent\', \'Homepage\', \'Clickout\', \'CTA Link: ' + cta_title + '\']);');
});
}
}
});

function runDealGAEvents ($obj, subsource) {
//console.log('runDealGAEvents');

var ga_outbound_links = $obj.find('.contentrightSort:not(.ga_out_links), .article_preview:not(.ga_out_links)');

ga_outbound_links.each(function() {
$(this).addClass('ga_out_links');
var gaout_this_deal = $(this).parent().find('.upperrightSort a').text();
if (gaout_this_deal == '') {
// Check if this was called within a preview instead of in a non-readmore block
gaout_this_deal = $(this).parent().parent().parent().find('.upperrightSort a').text();
}
if (gaout_this_deal == '') {
var gaout_this_deal_h1 = $(this).parent().find('.upperrightSort h1').text();
} else { var gaout_this_deal_h1 = gaout_this_deal;
}
if (gaout_this_deal_h1 == '') {gaout_this_deal = $(this).parent().find('.upperRightSortNew a').text();} else {gaout_this_deal = gaout_this_deal_h1;
}
gaout_this_deal = gaout_this_deal.replace(/(\r\n|\n|\r|')/gm,"");
var these_links = $(this).find('a');
these_links.each(function() {
if ($(this).hasClass('ga_out_link')) {
  // We have already added the handler once for this link
}
else {
  $(this).addClass('ga_out_link');
  var this_onclick = $(this).attr('onclick'), thisOnclick = '';
  if (this_onclick) {thisOnclick = this_onclick;}
  $(this).attr('onclick', '_gaq.push([\'_trackEvent\', \'' + subsource + '\', \'Clickout\', \'' + gaout_this_deal + '\']);' + thisOnclick);
}
});
});

}
