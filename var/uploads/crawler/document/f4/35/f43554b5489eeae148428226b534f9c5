	tb.jQuery(document).ready(hideExpireCacheDiv);

	function hideExpireCacheDiv(){
		if(document.URL.search("qa.techbargains") > 0){
				 document.getElementById("expireCacheDiv").style.display = 'block';
		}
	}
	 
	function clearMemCache() {
		var xmlhttp;
	  		if (window.XMLHttpRequest) {
	  			xmlhttp=new XMLHttpRequest();
	 		} else if (window.ActiveXObject) {
	 			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	 		} else {
	 			alert("Your browser does not support XMLHTTP!");
	 		}
			
	  		
	 		var currentURL=document.URL;
	  		xmlhttp.onreadystatechange = clearMemCacheCallBack;
			if(false){
					var Id=''; // for catsearch page
					xmlhttp.open("GET","/cachedobjects/clearCacheForPage?currURL="+currentURL+"&catId="+Id, true);
			}else if(false){ //for event pages
				var eventId='';
				xmlhttp.open("GET","/cachedobjects/clearCacheForPage?currURL="+currentURL+"&eventId="+eventId, true);
			}else{
				xmlhttp.open("GET","/cachedobjects/clearCacheForPage?currURL="+currentURL, true);
			}
			xmlhttp.send(null);
	  		
	 	}
	  	
	 function clearMemCacheCallBack() {
	 		if(xmlhttp.readyState == 4) {
	 			if (xmlhttp.status == 200) {
					 location.reload();
				} else {
					console.error('There was an error processing your request. Please try again');
				}
			}
 	 }

 	function viewOnProdLink(){
			var cuurUrl=document.URL;
			var prodUrl=cuurUrl.replace("qa","www");
			window.location=prodUrl;
	}
    </script>
