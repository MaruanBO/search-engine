<script>
	    (function() {
		    var lastTime = 0;
		    var vendors = ['ms', 'moz', 'webkit', 'o'];
		    for(var x = 0; x < vendors.length && !window.requestAnimationFrame; ++x)
		    { window.requestAnimationFrame = window[vendors[x]+'RequestAnimationFrame']; window.cancelAnimationFrame = window[vendors[x]+'CancelAnimationFrame'] || window[vendors[x]+'CancelRequestAnimationFrame']; }
		    if (!window.requestAnimationFrame)
			    window.requestAnimationFrame = function(callback, element) {
				    var currTime = new Date().getTime();
				    var timeToCall = Math.max(0, 16 - (currTime - lastTime));
				    var id = window.setTimeout(function()
				    { callback(currTime + timeToCall); }
				    ,
				    timeToCall);
				    lastTime = currTime + timeToCall;
			       return id;
			    };
		    if (!window.cancelAnimationFrame)
		       window.cancelAnimationFrame = function(id) { clearTimeout(id); };
	    }());

 if (!Array.prototype.indexOf) {
	    Array.prototype.indexOf = function (searchElement /*, fromIndex */ ) {
	            "use strict";
	            if (this == null) {
	                throw new TypeError();
	            }
	            var t = Object(this);
	            var len = t.length >>> 0;
	            if (len === 0) {
	                return -1;
	            }
	            var n = 0;
	            if (arguments.length > 0) {
	                n = Number(arguments[1]);
	                if (n != n) { // shortcut for verifying if it's NaN
	                    n = 0;
	                } else if (n != 0 && n != Infinity && n != -Infinity) {
	                    n = (n > 0 || -1) * Math.floor(Math.abs(n));
	                }
	            }
	            if (n >= len) {
	                return -1;
	            }
	            var k = n >= 0 ? n : Math.max(len - Math.abs(n), 0);
	            for (; k < len; k++) {
	                if (k in t && t[k] === searchElement) {
	                    return k;
	                }
	            }
	            return -1;
	    }
	}


tb.jQuery(document).ready(function($) {

if (jQuery('#afterDeals').length > 0) {

var ajax_count = 0, scroll_top = 0, my_target_position = 0, ajaxQueue = [], noAjaxProcessUnderway = true, currentDealsPage, d_page = 0;

var prevNewsTestRecursion = function(){

	    scroll_top = window.pageYOffset || document.documentElement.scrollTop;

	    my_target_position = $('#afterDeals').offset();

	    //console.log(scroll_top, (my_target_position.top - Math.floor(my_target_position.top*threshVal(d_page))), d_page);

	    if (scroll_top > (my_target_position.top - Math.floor(my_target_position.top*threshVal(d_page)))) {

	        if (noAjaxProcessUnderway) {

	            getPrevAjaxDealsCS(d_page);

	        }

	    } else {

	        if (d_page < 14) {

	            //setTimeout(prevNewsTestRecursion, 3000);

	        } else {

	                    clearTimeout(scrollThresholdValueInterval);

	            }

	    }

        // recursion
        scrollThresholdValueInterval = setTimeout(prevNewsTestRecursion, 3000);
};

var scrollThresholdValueInterval = setTimeout(prevNewsTestRecursion, 1500);

//console.log(scrollThresholdValueInterval);


function threshVal(count) {
	    var x = 0;
	    if (count == 0) { x = 0.75; }
	    else if (count == 1) { x = 0.5; }
	    else if (count == 2) { x = 0.3; }
	    else { x = 0.25; }
	    return x
}


function getPrevAjaxDealsCS(page) {
	    if(isAjaxRequestUnderProcess){return;}
	    if(d_page === currentDealsPage) {return;}
	    isAjaxRequestUnderProcess = true;
	    currentDealsPage = d_page;
	    //var ajax_url = "/deal/dealSection?newsType=" + newsTypeArray[currNewsTypeIndex] + "&sortBy=" + sortByArray[currCatIndex] + "&view=" + view[1] + "&deal_ids=" + lastDealId;
	    var ajax_url = '/deal/dealSectionTwentyFive?newsType=0&sortBy=0&view=' + view[1] + '&deal_ids=&paging=' + page;
if ("currentLastUpdatedTimeStampCS" in window) {
ajax_url = ajax_url + '&datetime=' + currentLastUpdatedTimeStampCS;
}
	    //console.log(ajax_url);
	    //console.time('timer_ajax');
	    noAjaxProcessUnderway = false;

	    $.ajax({
		    type : "GET",
		    url: ajax_url,
		    data: "",
		    async: true,
		    success: function (data, textStatus, jqXHR) {
		        //console.log('success callback');
		        ////console.log("Received: '%o'", data);
		        var ajax_deals = new Array();
		        ajax_deals = data.split('<a>DEALANDTIMESEPERATOR</a>');
		        tb.jQuery("#prevList").remove();
		        //document.getElementById("generalNews").innerHTML += removeDealsOnPage2dot0(values[1]);
var timestampRaw = ajax_deals[0];
if ("currentLastUpdatedTimeStampCS" in window) {
} else {
var currentLastUpdatedTimeStampArray = timestampRaw.split('<div style="display:none;">');
currentLastUpdatedTimeStamp = currentLastUpdatedTimeStampArray[1].replace("</div>", "");
currentLastUpdatedTimeStampCS = currentLastUpdatedTimeStamp;
//console.log(currentLastUpdatedTimeStampCS);
}
		        addAjaxedDeals(ajax_deals[1]);
		        showExpiredDeals(showExpired);
		        ajax_deals = null;
		        //setLastDealId();
		        isAjaxRequestUnderProcess = false;
		        lazyLoadImages("img.lazy");
		        updatePage(page);
		            //dealsPage++;
		    }
	    })
	    .always(function(){
		    noAjaxProcessUnderway = true;
		    //console.timeEnd('timer_ajax');
	    });

}



function updatePage(page) {
    //console.log('dealsPage val', d_page);
    d_page = d_page + 1;
    //console.log('dealsPage val', d_page);
    currentDealsPage = page;
    //console.log('updatePage: ' + d_page + ', ' + currentDealsPage);
}


function addAjaxedDeals(deals) {

	    var dealsHtml = tb.jQuery.parseHTML(deals);
	    var dealIds = [];
	    //console.time('timer_paint');
	    tb.jQuery(".hidden.dealID").each(function(){
	        dealIds.push(tb.jQuery(this).text());
	    });

	    //console.timeEnd('timer_paint');

	    /* if(tb.jQuery(dealsHtml).length <= 0)
	    { tb.jQuery("#pn").remove(); }
	    */

	    tb.jQuery(dealsHtml).find('.CBoxTDealStyling').each(function(){
		    ////console.log(tb.jQuery(this));
		    var this_id = tb.jQuery(this).find('.hidden.dealID').text();
		    ////console.log(this_id);
		    if (dealIds.indexOf(this_id) > -1) {
		      //console.log(this_id + ' skipped: position ' + dealIds.indexOf(this_id));
		    } else {
			    ajaxQueue.push(tb.jQuery(this));
			    //console.log('Pushed ', this_id);
		    }
	    });

	    //console.log('ajaxQueue: ', ajaxQueue.length);
	    //tb.jQuery('#generalNews').append(dealsHtml);
	    //return '<a id="prevList"></a>' + tb.jQuery(dealsHtml)[0].outerHTML;
	    if (ajaxQueue.length > 0) {
		    tb.jQuery('#generalNews').append('<div class="CBoxTDeal container_' + ajax_count + '"></div>');
		    dealdiv = tb.jQuery('#generalNews').find('.CBoxTDeal').last();
		    processQueue();
	    }
}

    function dealsCount() {}

    var requestId;

    function processQueue() {
	    requestId = window.requestAnimationFrame(processQueue);
	    var dealstoadd = ajaxQueue.splice(0,5);
	    //console.log('ajaxQueue: ' + ajaxQueue.length);
	    for (var i = 0; i < dealstoadd.length; i++) {
	        dealdiv.append(dealstoadd[i]);

	        if (ajaxQueue.length < 1) {
	            window.cancelAnimationFrame(requestId); requestId = undefined;
                    var $ga_new_deals =  tb.jQuery('.homepage-deals');
                    setTimeout(function() {
                    runDealGAEvents($ga_new_deals, 'Homepage');
                    }, 5000);

	        }
	    }
    }


	// Call this one time initially
	getPrevAjaxDealsCS(d_page);
}

});
</script>
