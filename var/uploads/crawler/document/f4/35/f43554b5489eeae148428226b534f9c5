  <script language="Javascript">
			function showDomainGroupDiv() {
				var Xcordinate =  getBrowserWidth();
			 	<!--
			 		if(Xcordinate < 1000) {
				 		Xcordinate = 1000;
			 		}
				// -->
	
				document.getElementById("tboverlay").style.height= getPageHeightWithScroll()+ "px";
				document.getElementById("tboverlay").style.width = Xcordinate+"px";
				document.getElementById("tboverlay").style.display = "block";
				document.getElementById("showDomainGroup").style.display = "block";
				
				var scrollFromTop = 0;

				// for chrome
				if(scrollFromTop == 0 && document.body) {
					scrollFromTop = document.body.scrollTop;
				}

				// for others
				if(scrollFromTop == 0 && document.documentElement) {
					scrollFromTop = document.documentElement.scrollTop;
				}

				document.getElementById("showDomainGroup").style.top = (scrollFromTop + 200) + "px";
			}

			function closeCommentDiv(){
				document.getElementById("showDomainGroup").style.display = 'none';
				document.getElementById("tboverlay").style.height= "100px";
				document.getElementById("tboverlay").style.display = "none";
			}

			function rediredToDomain(selectedDomain){
				<!-- 
