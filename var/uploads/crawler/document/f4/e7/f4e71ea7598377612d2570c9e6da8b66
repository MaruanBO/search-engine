}

function cc(c) {
  for(var i = 0; i <= document.getElementById('cat').length; i++) {
    if(document.getElementById('cat')[i].text.indexOf(c) == 0) {   
      document.getElementById('cat')[i].selected = true;
      document.sq.submit();
      return false;
    }
  }  
}    

var pageLoaded = false;

function stopKeys(e) {
  var e = (e) ? e : ((event) ? event : null);
  var node = (e.target) ? e.target : ((e.srcElement) ? e.srcElement : null);
  var name = node.getAttribute('id');
  if(name) {
    // Begin trap(s) for upload
    if(name.indexOf('photo') == 0) {
      if(node.getAttribute('type') == 'file') return false;
      if(e.keyCode == 13) {
        if(document.getElementById('option2').checked == true) fileChange(document.getElementById(name));
        return false;
      }
    }
    // End trap(s) for upload
  }
}

window.onload = function() {
  uf();

  pageLoaded = true;

  var preLoad = ['userf1.gif', 'userf2.gif', 'passf1.gif', 'passf2.gif'];
  var preImg  = new Array();
  for(var i = 0; i < preLoad.length; i++) {
    preImg[i] = new Image();
    preImg[i].src = 'images/' + preLoad[i];
  }

  var el = [document.getElementById('username'), document.getElementById('password')];
  var t  = ['user', 'pass'];
  for(var i = 0; i < el.length; i++) if(isAuthed) el[i].value = '';
  for(var i = 0; i < el.length; i++) bl(el[i], t[i]);
  al2();
  for(var i = 0; i < el.length; i++) el[i].style.color = '#0966A4';
  
  if(isAuthed) {
    setTimeout('al1()', 280);
    se('hdr1');
    se('div0');
  }
  else se('hdr2');

  XMLHttpReq = getXMLHTTPRequest();

  if(navigator.appName.indexOf('Internet Explorer') != -1) ieVersion = parseInt((navigator.appVersion.split('MSIE'))[1], 10);

  ulPrep();

  document.onkeypress = stopKeys;

  setTimeout('showAlert()', 1000);
}

function urlDecodeCharacter(str, c) {
  return String.fromCharCode(parseInt(c, 16));
}

function showAlert() {
  var query = window.location.search.substring(1);
  var vars = query.split('&'); 
  for(var i = 0; i < vars.length; i++) {
    var pair = vars[i].split('=');
    if (pair[0] == 'alert') {
      alert(decodeURIComponent(pair[1].replace(/\+/g, '%20')).replace(/\%([0-9a-f]{2})/g, urlDecodeCharacter));   
      window.location.href= '/';
    }
  }
}

var userFocus = false;
var passFocus = false;

function al1() {
  var f = ['username', 'password'];
  for(var i = 0; i < f.length; i++) {
    var el  = document.getElementById(f[i]);
    el.value = '';
	el.style.backgroundImage = 'url(\'images/' + f[i].substr(0, 4) + 'f1.gif\')';
  }
}

function al2(t) {
  var f = ['username', 'password'];
  for(var i = 0; i < f.length; i++) {
    var el  = document.getElementById(f[i]);
    var hasFocus = false;
    if(f[i] == 'username' && userFocus) hasFocus = true;
    if(f[i] == 'password' && passFocus) hasFocus = true;
    if(!hasFocus && el.value != '' && el.style.backgroundImage != '') el.style.backgroundImage = '';
  }
  setTimeout('al2()', 60);
}

function cl(el, s) {
  if(el.id == 'username') userFocus = true;
  if(el.id == 'password') passFocus = true;
  if(s == '' || el.value != '') el.style.backgroundImage = ''
  else el.style.backgroundImage = 'url(\'images/' + s + 'f2.gif\')';
}

function bl(el, s) {
  if(el.id == 'username') userFocus = false;
  if(el.id == 'password') passFocus = false;
  if(s == '' || el.value != '') el.style.backgroundImage = ''
  else el.style.backgroundImage = 'url(\'images/' + s + 'f1.gif\')';
}

// ------[ Upload Functions ]------------------------------------------------------------------

var XMLHttpReq = false;
var subLock = false;

function getXMLHTTPRequest() {
  var req = false;
  try {
    req = new XMLHttpRequest();
  }
  catch(err1) {
    try {
      req = new ActiveXObject("Msxml2.XMLHTTP");
    }
    catch(err2) {
      try {
        req = new ActiveXObject("Microsoft.XMLHTTP");
      }
      catch(err3) {
        req = false;
      }
    }
  }
  return req;
}

function getAbsolutePosition(element) {
  var ret = new Point();
  for(; element && element != document.body; ret.translate(element.offsetLeft, element.offsetTop), element = element.offsetParent);
  return ret;
}

function Point(x,y) {
  this.x = x || 0;
  this.y = y || 0;
  this.toString = function() {
    return '('+this.x+', '+this.y+')';
  }
  this.translate = function(dx, dy) {
    this.x += dx || 0;
    this.y += dy || 0;
  }
  this.getX = function() { return this.x; }
  this.getY = function() { return this.y; }
  this.equals = function(anotherpoint) {
    return anotherpoint.x == this.x && anotherpoint.y == this.y;
  }
}

function clearState() {
  for(var i = 1; i <= maxUploads; i++) fileState[i] = false;
}

function se(id) {
  var el = document.getElementById(id);
  if(el == null) return false;
  el.style.display = 'block';
  el.style.visibility = 'visible';
}

function he(id) {
  var el = document.getElementById(id);
  if(el == null) return false;
  el.style.display = 'none';
  el.style.visibility = 'hidden';
}

var lastCnt = 1;

function uc() {
  var i;
  for(i = 1; i <= document.getElementById('photocnt').value; i++) {
    se('div' + (i + 2));
    if(i > lastCnt) {
      if(document.getElementById('option1').checked == true) changeSrc(i, 0)
      else if(document.getElementById('option2').checked == true) changeSrc(i, 1);
    }
  }
  for(i; i <= maxUploads; i++) he('div' + (i + 2));
  lastCnt = document.getElementById('photocnt').value;
}

function eo(el) {
  var photoCnt = document.getElementById('photocnt').value;

  clearState();

  if(el.value == '1') { // Video
    he('div1');
    se('div2');
    he('pdivs');
    lastCnt = 2;
    document.getElementById('photocnt').value = lastCnt;
//  document.getElementById('set1').checked = true; // Default Individual
    document.getElementById('set0').checked = true; // Default Set
    for(var i = 1; i <= photoCnt; i++) {
      he('tblk' + i);
      changeSrc(i, 0);
      document.getElementById('delphoto' + i).innerHTML = '<img src="images/tran.gif" width="16" height="16" class="filedel">';
    }
    se('sinf');
  }
  if(el.value == '2') {  // Photo
    if(isAuthed) se('div1');
    he('div2');
    se('pdivs');
    lastCnt = 2;
    document.getElementById('photocnt').value = lastCnt;
    uc();
    if(!document.getElementById('set0').checked) he('sinf')
    else se('sinf');
    for(var i = 1; i <= photoCnt; i++) {
      he('tblk' + i);
      changeSrc(i, 0);
      document.getElementById('delphoto' + i).innerHTML = '<img src="images/tran.gif" width="16" height="16" class="filedel">';
    }
  }
  if(el.value == '3') { // Web
    if(isAuthed) se('div1');
    he('div2');
    se('pdivs');
    lastCnt = 2;
    document.getElementById('photocnt').value = lastCnt;
    uc();
    if(!document.getElementById('set0').checked) he('sinf')
    else se('sinf');
    for(var i = 1; i <= photoCnt; i++) {
      he('tblk' + i);
      changeSrc(i, 1);
      document.getElementById('delphoto' + i).innerHTML = '<img src="images/tran.gif" width="16" height="16" class="filedel">';
    }
  }
}

function so(el) {
  var photoCnt = document.getElementById('photocnt').value;

  if(el.value == '0') { // Individual
    for(var i = 1; i <= photoCnt; i++) {
      var fieldTxt = document.getElementById('photo' + i).value;
      if(fieldTxt != '' && fieldTxt != 'Paste web URL here') {
        document.getElementById('filename' + i).style.border = '#fff 1px solid';
        se('tblk' + i);
      }
      else {
        he('tblk' + i);
        document.getElementById('filename' + i).style.border = '#96a6c5 1px solid';
      }
    }
    he('sinf');
  }
  else if(el.value == '1') { // Set
    for(var i = 1; i <= photoCnt; i++) {
      he('tblk' + i);
      document.getElementById('filename' + i).style.border = '#96a6c5 1px solid';
    }
    se('sinf');
  }
}

function cm(el, s) {
  if(el.value == s) el.value = '';
}

function bm(el, s) {
  if(el.value == '') el.value = s;
}

function fileChange(n) {
  var photoCnt = parseInt(document.getElementById('photocnt').value, 10);
  
  if(document.getElementById('option2').checked && document.getElementById('filename' + n).value != '' && document.getElementById('filename' + n).value != 'Paste web URL here') {
    document.getElementById('filename' + n).readOnly = true;
    document.getElementById('photo' + n).value = document.getElementById('filename' + n).value;
  }
  
  var el = (n > 0) ? document.getElementById('photo' + n) : document.getElementById('video');
  var s  = el.value.replace(/^\s+|\s+$/g, '');
  if(!fileState[n] && s != '' && s != 'Paste web URL here') {
//  fileState[n] = true;

    var p = s.lastIndexOf('/')
    if(p == -1) p = s.lastIndexOf('\\');
    if(p == -1) p = s.lastIndexOf(':');
    if(p > -1)  s = s.substr(p + 1);

    document.getElementById('filename' + n).value = s;

    if(document.getElementById('set1').checked) {
      var h = '';

      h += '<table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
      h += '  <tr>\n';
      h += '    <td><input type="text" id="title' + n + '" name="title' + n + '" class="textbox" value="Add title here" maxlength="50" onClick="cm(this, \'Add title here\')" onFocus="cm(this, \'Add title here\')" onBlur="bm(this, \'Add title here\')"></td>\n';
      h += '  </tr>\n';
      h += '  <tr>\n';
      h += '    <td><span style="text-align: right; padding-right: 5px; padding-top: 10px; postion: relative; margin-left: -50px; display: block; width: 45px; float: left; font-size: 14px; font-weight: bold; vertical-align: center;">Tags:</span><input style="margin-top: 5px;" type="text" id="tags' + n + '" name="tags' + n + '" class="textbox" placeholder="Use spaces to separate tags" maxlength="100"></td>\n';
      h += '  </tr>\n';
      h += '</table>\n';

      document.getElementById('filename' + n).style.border = '#fff 1px solid';
      document.getElementById('tblk' + n).innerHTML = h;
      se('tblk' + n);
      se('tags' + n);
    }
    else {
      if(n > 0) {
        he('tblk' + n);
        se('tags' + n);
        document.getElementById('filename' + n).style.border = '#96a6c5 1px solid';
      }
    }
    document.getElementById('delphoto' + n).innerHTML = '<a href="#" onClick="removeFile(' + n + '); return false"><img src="images/filedel1.gif" alt="Remove" title="Remove" width="16" height="16" border="0"></a>'

    if(photoCnt < maxUploads) {
      for(var i = photoCnt; i >= 0; i--) {
        if(i == 0) break;
        else {
         el = document.getElementById('photo' + i);
         if(el.value != '' && el.value != 'Paste web URL here') break;
        }
      }
      if(i > 0) {
        var freeCnt = photoCnt - i;
        // With the following logic, we always have two extra fields up until photoCnt.
        // It serves as a visual queue that you can add more!
        if(freeCnt == 0) photoCnt += 2;
        else if(freeCnt == 1) photoCnt++;
        if(photoCnt > maxUploads) photoCnt = maxUploads;
        document.getElementById('photocnt').value = photoCnt;
        if(photoCnt != lastCnt) uc();
      }
    }

    if(document.getElementById('option2').checked) {
      for(; n <= photoCnt; n++) {
        el = document.getElementById('photo' + n);
        if(el.value == '' || el.value == 'Paste web URL here') {
          el.focus(); el.select();
          break;
        }
      }
    }
  }
  return false;
}

function removeFile(n) {
  document.getElementById('delphoto' + n).innerHTML = '<img src="images/tran.gif" width="16" height="16">';
  if(n < 1) {
    document.getElementById('filespn' + n).innerHTML = '<input type="file" id="video" name="video" onChange="fileChange(' + n + ')">';
    document.getElementById('filename' + n).value     = '';
  }
  else if(document.getElementById('option1').checked) {
    document.getElementById('filespn' + n).innerHTML = '<input type="file" accept="' + accept_filter + '" id="photo' + n + '" name="photo' + n + '" onChange="fileChange(' + n + ')">';
    document.getElementById('filename' + n).value     = '';
  }
  else {
    document.getElementById('filespn' + n).innerHTML = '<input type="text" id="filename' + n + '" name="filename' + n + '" value="Paste web URL here" onClick="cm(this, \'Paste web URL here\')" onFocus="cm(this, \'Paste web URL here\')" onBlur="bm(this, \'Paste web URL here\')" class="textbox" maxlength="255"><input type="hidden" id="photo' + n + '" name="photo' + n +'" value="">';
    document.getElementById('filename' + n).value     = 'Paste web URL here';
  }
  document.getElementById('delphoto' + n).innerHTML = '<img src="images/tran.gif" width="16" height="16" class="filedel">';
  document.getElementById('filename' + n).style.border = '#96a6c5 1px solid';

  if(n > 0) {
    var h = '';
  
    h += '<table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
    h += '  <tr>\n';
    h += '    <td><input type="text" id="title' + n + '" name="title' + n + '" class="textbox" value="Add title here" maxlength="50" onClick="cm(this, \'Add title here\')" onFocus="cm(this, \'Add title here\')" onBlur="bm(this, \'Add title here\')"></td>\n';
    h += '  </tr>\n';
    h += '</table>\n';

    document.getElementById('tblk' + n).innerHTML = h;
    he('tblk' + n);
  }

  fileState[n] = false;
}

function changeSrc(n, t) {
  var h = '';

  if(t == 0) {
    h += '<table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
    h += '  <tr>\n';
    h += '    <td><input type="text" id="filename' + n + '" name="filename' + n + '" class="textbox" maxlength="255" readonly></td>\n';
    h += '    <td class="leftpad8">\n';
    h += '      <div class="filediv"><span id="filespn' + n + '" class="filespn"><input type="file" accept="' + accept_filter + '" id="photo' + n + '" name="photo' + n + '" onChange="fileChange(' + n + ')"></span></div>\n';
    h += '      <div class="filebdiv"><input type="button" value="Browse" class="filebinp"></div>\n';
    h += '    </td>\n';
    h += '    <td class="leftpad8"><div id="delphoto' + n + '"><img src="images/tran.gif" width="16" height="16"></div></td>\n';
    h += '  </tr>\n';
    h += '</table>\n';

    document.getElementById('file' + n).innerHTML  = h;
    document.getElementById('label' + n).innerHTML = '<span class="uplabel">Photo #' + n + ':</span>';
  }
  else {
    h += '<table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
    h += '  <tr>\n';
    h += '    <td><span id="filespn' + n + '"><input type="text" id="filename' + n + '" name="filename' + n + '" value="Paste web URL here" onClick="cm(this, \'Paste web URL here\')" onFocus="cm(this, \'Paste web URL here\')" onBlur="bm(this, \'Paste web URL here\')" class="textbox" maxlength="255"><input type="hidden" id="photo' + n + '" name="photo' + n +'" value=""></span></td>\n';
    h += '    <td class="leftpad8"><input type="button" value="Submit" onClick="fileChange(' + n + ')" class="filebinp"></td>\n';
    h += '    <td class="leftpad8"><div id="delphoto' + n + '"><img src="images/tran.gif" width="16" height="16"></div></td>\n';
    h += '  </tr>\n';
    h += '</table>\n';

    document.getElementById('file' + n).innerHTML  = h;
    document.getElementById('label' + n).innerHTML = '<span class="uplabel">URL #' + n + ':</span>';
  }
  document.getElementById('delphoto' + n).innerHTML = '<img src="images/tran.gif" width="16" height="16" class="filedel">';
}

var ulPrepped = false;

function ulPrep() {
  if(!ulPrepped) {
    ulPrepped = true;

    var h = '';
    for(var i = 1; i <= maxUploads; i++) {
      h += '<div id="div' + (i + 2) + '" class="hidden">\n';
      h += '  <table border="0" cellpadding="2" cellspacing="0" width="100%" class="form">\n';
      h += '    <tr>\n';
      h += '      <td width="88" align="right" valign="top"><div name="label' + i + '" id="label' + i + '" style="padding-top: 6px"><span class="uplabel">Photo #' + i + ':</span></div></td>\n';
      h += '      <td width="334">\n';
      h += '        <div id="file' + i + '">';
      h += '          <table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
      h += '            <tr>\n';
      h += '              <td><input type="text" id="filename' + i + '" name="filename' + i + '" class="textbox" maxlength="255" readonly></td>\n';
      h += '              <td class="leftpad8">\n';
      h += '                <div class="filediv"><span id="filespn' + i + '" class="filespn"><input type="file" accept="' + accept_filter + '" id="photo' + i + '" name="photo' + i + '" onChange="fileChange(' + i + ')"></span></div>\n';
      h += '                <div class="filebdiv"><input type="button" value="Browse" class="filebinp"></div>\n';
      h += '              </td>\n';
      h += '              <td class="leftpad8"><div id="delphoto' + i + '"><img src="images/tran.gif" width="16" height="16"></div></td>\n';
      h += '            </tr>\n';
      h += '          </table>\n';
      h += '        </div>\n';
      h += '        <div id="tblk' + i + '" class="hidden">\n';
      h += '          <table border="0" cellpadding="0" cellspacing="0" class="filespace">\n';
      h += '            <tr>\n';
      h += '              <td><input type="text" id="title' + i + '" name="title' + i + '" class="textbox" value="Add title here" maxlength="50" onClick="cm(this, \'Add title here\')" onFocus="cm(this, \'Add title here\')" onBlur="bm(this, \'Add title here\')"></td>\n';
      h += '            </tr>\n';
      h += '  <tr>\n';
      h += '    <td><span style="text-align: right; padding-right: 5px; padding-top: 10px; postion: relative; margin-left: -50px; display: block; width: 45px; float: left; font-size: 14px; font-weight: bold; vertical-align: center;">Tags:</span><input style="margin-top: 5px; display: block; visibility: visible;" type="text" id="tags' + i + '" name="tags' + i + '" class="textbox" placeholder="Use spaces to separate tags" maxlength="100"></td>\n';
      h += '  </tr>\n';
      h += '          </table>\n';
      h += '        </div>';
      h += '      </td>\n';
      h += '    </tr>\n';
      h += '  </table>\n';
      h += '</div>\n';
    }

    document.getElementById('pdivs').innerHTML = h;
    clearState();
    lastCnt = 2;
  
//  document.getElementById('set1').checked = true; // Default Individual
    document.getElementById('set0').checked = true; // Default Set

    var hidePriv = false;
    if(isAuthed) {
      if(!hidePriv) se('privopt');
      se('add_text');
      se('autopost_radio');
    }
    for(var i = 0; i < document.uploadform.elements.length; i++) document.uploadform.elements[i].disabled = false;
    for(var i = 0; i < document.loginform.elements.length; i++) document.loginform.elements[i].disabled = false;
    
    var defOption = 'option1'; // Photo selected by default
    document.getElementById(defOption).checked = true;
    eo(document.getElementById(defOption));
  }
}

function XMLHttpRes() {
  if(XMLHttpReq.readyState == 4) {
    if(XMLHttpReq.status == 200) {
      var servRes = XMLHttpReq.responseText.split(':');
      if(servRes[0] == 'cookie') {
        // Had problems setting document.cookie in some versions of IE
        // so we'll send the cookie data with the upload form and update
        // the cookie on refresh.
        // document.cookie = servRes[1];
        document.getElementById('cookie').value = servRes[1];
        startUpload();
      }
      else if(servRes[0] == 'error') {
        alert(servRes[1]);
      }
      else if(servRes[0] == 'redirect') {
        document.location.href = servRes[1];
      }
//    else {
//      alert(servRes[0] + ':' + servRes[1]);
//    }
      subLock = false;
    }
  }
}

function uploadNow() {
  var fileCnt = 0;
  if(document.getElementById('video').value != '') fileCnt++;
  for(var i = 1; i <= maxUploads; i++) {
    var f = document.getElementById('photo' + i).value;
    if(f != '' && f != 'Paste web URL here') fileCnt++;
  }
  if(fileCnt < 1) {
    alert('You haven\'t chosen anything to upload!');
    return false;
  }

  ta = document.getElementById('textarea_with_default_text');
  if (ta) {
      if (ta.value.length > 10000) {
          alert("Text cannot exceed 10000 characters");
          return false;
      }
  }


  // Video extention check
  var t = new RegExp(/(.avi|.asf|.wmv|.mov|.mpg|.mpeg|.mp4|.flv)$/i);
  var f = document.getElementById('video').value;
  if(f != '') {
    if(t.exec(f) == null) {
      alert('The selected video file, "' + f + '" is not an acceptable file type!');
      return false;
    }
  }

  // Photo extention check
  var t = new RegExp(/(.tif|.tiff|.jpg|.jpeg|.png|.gif|.bmp)$/i);
  for(var i = 1; i <= maxUploads; i++) {
    var f = document.getElementById('photo' + i).value;
    if(f != '' && f != 'Paste web URL here') {
      if(t.exec(f) == null) {
        alert('File #' + i + ', "' + f + '" is not an acceptable file type!');
        return false;
      }
    }
  }

//if(document.getElementById('category').value == '0' && document.getElementById('set0').checked == true) {
//  alert('Please select the most appropriate category for the file(s) you\'re uploading.');
//  return false;
//}

// Always start upload and skip over interim div.
  startUpload();
  return;
  
  if(isAuthed) {
    startUpload();
  }
  else {
    he('sub1');
    for(var i = 0; i <= maxUploads; i++) {
      var fieldTxt = document.getElementById('filename' + i).value;
      if(fieldTxt != '' && fieldTxt != 'Paste web URL here') {
        document.getElementById('delphoto' + i).innerHTML = '<img src="images/filedel2.gif" width="16" height="16">';
      }
    }
    for(var i = 0; i < document.uploadform.elements.length; i++) document.uploadform.elements[i].disabled = true;
    for(var i = 0; i < document.loginform.elements.length; i++) document.loginform.elements[i].disabled = true;
    se('sub2');
    document.getElementById('uploadY').disabled = false;
    document.getElementById('uploadN').disabled = false;
    document.getElementById('register').disabled = false;
  }
}

function uploadPref(f) {
  if(f) { // Log in or sign up
    he('layer1');
    // if(f == 1) se('layer2a');
    // else se('layer2b');
    se('layer2');
  }
  else {
    // Just give links
    startUpload();
  }
}

function userLogin(f) {
  if(subLock) return false;
  subLock = true;

  var p = new Array();
  var url = '';
  if(f) { // Sign Up
    for(var i = 1; i <= 7; i++)   p[i] = document.getElementById('signup' + i).value;
    for(var i = 12; i <= 20; i++) p[i] = document.getElementById('signup' + i).value;
    var nullFields = 0;
    for(var i = 1; i <= 6; i++) if(p[i] == '') nullFields++;
	if(p[19] == '') nullfields++;
	if(p[20] == '') nullfields++;
    if(nullFields > 0) {
      alert('All fields are required for signup!');
      subLock = false;
      return false;
    }
    if(p[1] != p[2]) {
      alert('The verification email does not match the original email you entered.');
      subLock = false;
      return false;
    }
    if(p[4] != p[5]) {
      alert('The confirmation password does not match the original password you entered.');
      subLock = false;
      return false;
    }
    if((p[6] == 'US' || p[6] == 'CA' || p[6] == 'UK') && p[7] == '') {
      alert('Please enter a valid postal code.');
      subLock = false;
      return false
    }
    if(p[12] != '1') {
      alert('You must read and agree to the terms of service.');
      subLock = false;
      return false;
    }
    url = 'signup.pl?email=' + p[1] + '&username=' + p[3] + '&password=' + p[4] + '&country=' + p[6] + '&zip=' + p[7] + '&phone1=' + p[15] + '&phone2=' + p[16] + '&phone3=' + p[17] + '&phone4=' + p[18] + '&firstname=' + p[19] + '&lastname=' + p[20] + '&readterms=' + p[12] + '&sharerights=1&promocode=' + p[14] + '&simple=1&ajax=true';
  }
  else {  // Log In
    for(var i = 1; i <= 2; i++) p[i] = document.getElementById('login' + i).value;
    if(p[1] == '' || p[2] == '') {
      alert('All fields are required for login!');
      return false;
    }
    url = 'login.pl?username=' + p[1] + '&password=' + p[2] + '&ajax=true&rand=' + myRand;
  }
  if(XMLHttpReq) {
    var myRand = parseInt(Math.random() * 999999999999, 10);
    XMLHttpReq.open('GET', url + '&rand=' + myRand, true);
    XMLHttpReq.onreadystatechange = XMLHttpRes;
    XMLHttpReq.send(null);
  }
  return false;
}

function startUpload() {
  // he('layer2a'); he('layer2b'); se('layer1');
  he('layer2'); se('layer1');
  var winTop = window.pageYOffset || document.body.scrollTop || document.documentElement.scrollTop;
  var winHeight;
  var docHeight;
  if (typeof document.height != 'undefined') {
    docHeight = document.height;
    winHeight = window.innerHeight;
  }
  else if (document.compatMode && document.compatMode != 'BackCompat') {
    docHeight = document.documentElement.scrollHeight;
    winHeight = document.documentElement.clientHeight;
  }
  else if (document.body && typeof document.body.scrollHeight != 'undefined') {
    docHeight = document.body.scrollHeight;
    winHeight = document.body.clientHeight;
  }
  document.getElementById('layer3').style.height = docHeight;
  document.getElementById('layer4').style.top = (winTop + (winHeight / 2) - 63) + 'px';
  se('layer3'); se('layer4');
  for(var i = 0; i < document.uploadform.elements.length; i++) document.uploadform.elements[i].disabled = false;
  setTimeout('document.uploadform.submit()', 1000);
}

function popOpen(url, width, height) {
  var win = window.open(url, '', 'width=' + width + ', height=' + height + 'location=no, menubar=no, status=no, toolbar=no, scrollbars=no, resizable=no');
  win.resizeTo(width, height);
  //win.focus();
}
