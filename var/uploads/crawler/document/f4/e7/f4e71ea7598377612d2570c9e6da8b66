<script type="text/javascript" language="javascript">
var post = 0;
var w_ap = 0;
var no_soc = 1;
var autopost = '';
var def_text_area='Create a blog or simply include information about your images';
w_ap = autopost == 'no' || no_soc ? 0 : 1;

function postToAutopostSettings(makePost) {
  jQuery.fancybox({
    href: "http:///social-settings",
    type: "iframe",
    scrolling: 'auto',
    height: 500,
    width: 620
  });
  return false;
}

function setSocialContent(html) {
  jQuery("#wer").html(html);
}

function p_ac_noth() {
  jQuery("#make_autopost").val('no');
  jQuery.fancybox.close();
}

function p_ac_dont() {
  w_ap = 1;
  var s = jQuery("<script>").attr('src', "http://.use.com/social-apwindow-dont");
  jQuery(s).appendTo(jQuery("body"));
  jQuery("#make_autopost").val('yes');
  jQuery.fancybox.close();
}

jQuery(document).ready(function() {
  jQuery('#textarea_with_default_text').val(def_text_area);
  jQuery('#autopost').click(function() {
    postToAutopostSettings();
    return false;
  });

  jQuery('#autopost_settigns_link').click(function() {
    postToAutopostSettings();
    return false;
  });

  jQuery("#autopost_yes").change(function() {
    if((jQuery(this).attr('checked')) && (jQuery('#haveSocial').val() == 0)) {
      postToAutopostSettings();
    }
  });

  jQuery('#textarea_with_default_text').bind('focus',function() {
    if(jQuery(this).val() == def_text_area) {
      jQuery(this).val('');
    }
  });

  jQuery('#textarea_with_default_text').bind('blur',function() {
    if(jQuery(this).val() == '') {
      jQuery(this).val(def_text_area);
    }
  });
});
</script>
