	//<![CDATA
	function search(text) {
		if (text !== "") {
			executeLinkButton('header_btnSearch_btn');
			return false;
		}
	}

	function lsdModule_Click() {
		
		//Get lsdModule elements
		var lsdMenu = document.getElementById("lsdMenu");
		var hideMenu = document.getElementById("hideMenu");
		var img = document.getElementById("imgArrow");

		//Get the size/postions of elements
		var menuHeight = lsdMenu.offsetHeight;
		var startX = lsdMenu.offsetTop;
		var animTime = 1.0;

		//Set animation variables
		var openMenu = new Tween(lsdMenu.style, 'top', Tween.strongEaseInOut, startX, 0, animTime, 'px');
		var closeMenu = new Tween(lsdMenu.style, 'top', Tween.strongEaseInOut, 0, -menuHeight + 29, animTime, 'px');

		if (img.className.toString() == "imgArrowDown") {
			//Open menu animation
			cmCreateElementTag("LSD Dropdown Module", "LSD", "Open");
			openMenu.start();
			hideMenu.style.height = menuHeight + 'px';			
			img.className = "imgArrowUp";
		}
		else {
			//Close menu animation
			cmCreateElementTag("LSD Dropdown Module", "LSD", "Close");
			closeMenu.start();
			img.className = "imgArrowDown";	

			closeMenu.onMotionFinished = function () { menuReset(); }
		}
	}

	function menuReset() {
		//Reset the hideMenu height to only show the last row after closing the lsdMenu
		document.getElementById("hideMenu").style.height = "29px";
	}

	function SubmitToSearch() {
		var searchTerm = $("[id$='searchText_txtBox']").val();
		var searchsite = '/Search/Search.aspx?search=' + encodeURI(searchTerm);
    	window.location = searchsite;
    	return false;
	}
		// ]]>
