				success: function( getCatsData ) {
					//a.next('.subMenu').empty().append( getCatsData 
					t.append('<div class="subMenu">'+getCatsData+'</div>');
					Tabs2();
				}
			}).done ( function () {	
				$('img.lazy-drop').lazyload();
				console.log('dropping now........');
			});
		});
		
		$( '.nav-menu #menu-primary-menu > li.menu-item-object-category' ).hover(function(){
			$(this).find('.subMenu').slideDown(function(){
				$('img.lazy-drop').lazyload();
			});
			$(this).addClass('active');
		},function(){
			$(this).find('.subMenu').hide();
			$(this).removeClass('active');
		});
	
		$('.main-menu > li:not(:first)').addClass('page_item');
		
