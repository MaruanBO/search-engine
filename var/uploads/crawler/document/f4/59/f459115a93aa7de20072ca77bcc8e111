		jQ(window).scroll(function(){
		if (jQ('.flipbox').is(':in-viewport') ==  true) {
			jQ('.flipTrigger').not('.dontFlip').trigger('click');
			
		} else {
			jQ('.flipTrigger').removeClass('dontFlip');
			
			
		}
	});
		
	
	var wh = jQ(window).width();
	
	jQ(window).resize(function(){
		wh = jQ(window).width();
		
		if( jQ(window).width()<760 ){
			jQ('.home-sticky').eq(1).hide();
		}
	});
		
	jQ(window).load(function(){
	
		jQ('.count-heart a').wrapInner('<span></span>');
		jQ('.count-heart a').append(' <i class="icon-comments"></i>');
		jQ('.count-single a').append(' <strong>Comment &raquo;</strong>');


		if( (jQ('body.home').length>0 || jQ('body.category').length>0) && wh>1030 ){
			if (!!jQ('.home-sticky').offset()) { // make sure ".sticky" element exists

				var stickyTop = jQ('.home-sticky').offset().top; // returns number 
				
				jQ(window).scroll(function(){ // scroll event

					var windowTop = jQ(window).scrollTop(); // returns number 
					var footerTop = jQ('footer').offset().top;
					
					//console.log(stickyTop+ ' / ' +windowTop+' / '+footerTop+' / '+wh);
					
					if ((footerTop-1100) < windowTop){
						jQ('.home-sticky').css('position','static');
					}else if (stickyTop < windowTop){
						jQ('.home-sticky').css({ position: 'fixed', top: 0 });
					} else {
						jQ('.home-sticky').css('position','static');
					}

				});

			}
		} 


		if( jQ('.sticky-inner').length>0 ){
			if (!!jQ('.sticky-inner').offset()) { // make sure ".sticky" element exists

				var stickyTop = jQ('.sticky-inner').offset().top; // returns number 
				
				jQ(window).scroll(function(){ // scroll event

					var windowTop = jQ(window).scrollTop(); // returns number 

					if (stickyTop < windowTop){
						jQ('.sticky-inner').css({ position: 'fixed', top: 107 });
					} else {
						jQ('.sticky-inner').css({ position: 'static', top: 0 });
					}
 
				});

			}
		} 


		if( jQ('.count-single').length>0 ){
			if (!!jQ('.count-single').offset()) { // make sure ".sticky" element exists
				var fontsize = '14';
				var stickyTop = jQ('.count-single').offset().top + 100; // returns number 
				
				jQ(window).scroll(function(){ // scroll event

					var windowTop = jQ(window).scrollTop(); // returns number 
					

						fontsize = (fontsize*1) + 1;
						if( fontsize < 81 ){
							jQ('.count-single a').css({ fontSize: fontsize+'px' });
						}
			
 
				});

			}
		} 
		
		if( jQ(window).width()<760 ){
			jQ('.home-sticky').eq(1).hide();
		}
		
	});
	function Tabs2(){
		jQ('.tabBlock:not(.galleryBlock, .widget, .trending-galleries)').each(function() {
			jQ(this).find('.tab').first().addClass('tab-active');
			jQ(this).find('.tab-content .content').hide();
			jQ(this).find('.tab-content .content').first().show();
			jQ('.tabBlock:not(.galleryBlock, .widget, .trending-galleries)').find('.tab a').bind('click',function () {
				window.location.href = jQ(this).attr('data-url');
				return false;
			});
			
			jQ('.tabBlock:not(.galleryBlock, .widget, .trending-galleries)').find('.tab a').hover(function () { 
				jQ(this).closest('li').siblings().removeClass('tab-active')
				jQ(this).closest('li').addClass('tab-active');
				var currentTab = jQ(this).attr('href');
				jQ(this).closest('.tabBlock:not(.galleryBlock, .widget, .trending-galleries)').find('.tab-content .content').hide();
				jQ(currentTab).show();
				return false;
			});	 
		});
	}
</script>

